{
  "chapter": {
    "id": "37dcea6d-6044-4aaf-a403-45a1624958ee",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Ai%20Ca%201&version=BD2011",
    "createdAt": "2024-09-09T04:13:05.482Z",
    "updatedAt": "2024-09-09T04:13:05.482Z",
    "bookId": "86840a15-95ec-46a2-8174-0b2bb385d174"
  },
  "verses": [
    {
      "id": "946f8e94-faf0-4421-b7f4-8cdcfc3ed516",
      "number": 1,
      "content": "Kìa thành phố một thời đông người nhộn nhịp,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.802Z",
      "updatedAt": "2024-09-09T05:02:28.802Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "cdd74608-0663-4c1b-9f35-bb4793ee3a55",
      "number": 1,
      "content": "Nay thưa người đơn chiếc đìu hiu!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.814Z",
      "updatedAt": "2024-09-09T05:02:28.814Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "648798fb-db1e-4f2f-8e60-2ebb97b1e2ac",
      "number": 1,
      "content": "Kìa nàng vốn đại đô các nước,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.816Z",
      "updatedAt": "2024-09-09T05:02:28.816Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "d1f4f77d-4cc4-4d61-9e98-fb678714c4b6",
      "number": 1,
      "content": "Giờ trở thành góa phụ cô đơn!",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.818Z",
      "updatedAt": "2024-09-09T05:02:28.818Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "994b4ad5-7997-4c5e-b398-cba595bb5e9d",
      "number": 1,
      "content": "Kìa nàng vốn nữ hoàng các tiểu quốc,",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.820Z",
      "updatedAt": "2024-09-09T05:02:28.820Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "43798996-dd07-4f17-809b-939f57cfca1a",
      "number": 1,
      "content": "Nay biến thành nữ nô lệ phục tùng!",
      "order": 5,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.822Z",
      "updatedAt": "2024-09-09T05:02:28.822Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "d6908159-f9e5-407e-98d9-1ca299c119b1",
      "number": 2,
      "content": "Giữa đêm khuya nàng khóc than cay đắng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.824Z",
      "updatedAt": "2024-09-09T05:02:28.824Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "c4478abc-46db-47ce-a9e6-e084da25b499",
      "number": 2,
      "content": "Lệ tuôn tràn trên đôi má gầy gò;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.826Z",
      "updatedAt": "2024-09-09T05:02:28.826Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "a245e6f3-be96-4bc3-89b4-a63249bc4d56",
      "number": 2,
      "content": "Trong số tất cả tình nhân của nàng, không ai an ủi nàng;",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.828Z",
      "updatedAt": "2024-09-09T05:02:28.828Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "d9d2881e-9986-4695-bd0c-3901ac48d174",
      "number": 2,
      "content": "Tất cả các bạn nàng đã phụ rẫy nàng,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.829Z",
      "updatedAt": "2024-09-09T05:02:28.829Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "76d01034-536d-4b62-b3d2-95e521bc65e6",
      "number": 2,
      "content": "Thậm chí chúng còn trở thành kẻ thù của nàng.",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.832Z",
      "updatedAt": "2024-09-09T05:02:28.832Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "6b930186-f887-4373-806a-a96811eb9202",
      "number": 3,
      "content": "Giu-đa đã bị lưu đày,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.834Z",
      "updatedAt": "2024-09-09T05:02:28.834Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "a2747836-ef41-4684-89b3-d56e614e8d05",
      "number": 3,
      "content": "Nàng phải chịu biết bao đau đớn và phải làm lao động khổ sai;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.836Z",
      "updatedAt": "2024-09-09T05:02:28.836Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "2e93e104-31a5-4ca4-9d73-5f16e4dae9de",
      "number": 3,
      "content": "Bây giờ nàng phải sống rải rác giữa các nước,",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.838Z",
      "updatedAt": "2024-09-09T05:02:28.838Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "8f309b5c-0d4f-45de-91cc-6374b4550f7c",
      "number": 3,
      "content": "Nàng chẳng tìm được nơi nào để nghỉ ngơi;",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.841Z",
      "updatedAt": "2024-09-09T05:02:28.841Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "36beadb4-b677-4a81-a4e8-e0fcdb87da60",
      "number": 3,
      "content": "Mọi kẻ truy đuổi nàng đều bắt kịp nàng trong lúc nàng cùng đường.",
      "order": 4,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.845Z",
      "updatedAt": "2024-09-09T05:02:28.845Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "9a46d407-e8d5-41d8-80dc-63beb6172c2e",
      "number": 4,
      "content": "Những con đường dẫn đến Si-ôn âu sầu ảm đạm,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.849Z",
      "updatedAt": "2024-09-09T05:02:28.849Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "fdd4dab1-a5e7-4d40-94db-28fcd334f8d9",
      "number": 4,
      "content": "Vì không ai đến dự các đại lễ như xưa;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.854Z",
      "updatedAt": "2024-09-09T05:02:28.854Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "88487ff4-87c4-4525-92c8-531574310229",
      "number": 4,
      "content": "Tất cả các cổng thành đều đìu hiu quạnh quẽ,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.858Z",
      "updatedAt": "2024-09-09T05:02:28.858Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "c139796c-c416-43bb-9001-d2a758d6c374",
      "number": 4,
      "content": "Các tư tế đều rầu rĩ thở than,",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.863Z",
      "updatedAt": "2024-09-09T05:02:28.863Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "3093038a-aefe-4a1b-9d59-3c05e4139b62",
      "number": 4,
      "content": "Các thiếu nữ của nàng buồn da diết,",
      "order": 4,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.868Z",
      "updatedAt": "2024-09-09T05:02:28.868Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "0db80595-9427-482e-ad5d-70e1e6b901ad",
      "number": 4,
      "content": "Số phận nàng thật đau khổ đắng cay.",
      "order": 5,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.872Z",
      "updatedAt": "2024-09-09T05:02:28.872Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "edcd327e-0a66-420d-9cd2-f0e459a9dd97",
      "number": 5,
      "content": "Những kẻ thù ghét nàng đã trở thành các chủ nhân nàng,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.877Z",
      "updatedAt": "2024-09-09T05:02:28.877Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "9a53c7a8-d1e7-493f-ae85-1f96fa2c9215",
      "number": 5,
      "content": "Những kẻ thù của nàng đã phát đạt.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.882Z",
      "updatedAt": "2024-09-09T05:02:28.882Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "7befac48-3b9d-49ca-a3c1-c2607a55c5c6",
      "number": 5,
      "content": "Vì vô số tội lỗi của nàng,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.887Z",
      "updatedAt": "2024-09-09T05:02:28.887Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "b10e2407-653e-430c-8aa2-e2c1be3d36cf",
      "number": 5,
      "content": "Chúa bắt nàng phải chịu đau khổ;",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.891Z",
      "updatedAt": "2024-09-09T05:02:28.891Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "d9aa1c74-ebea-4b64-a9bd-f0c90f9d27be",
      "number": 5,
      "content": "Con cái nàng phải sống tha phương,",
      "order": 4,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.895Z",
      "updatedAt": "2024-09-09T05:02:28.895Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "8c9f3ed4-3080-4173-8033-e8bc1fec4167",
      "number": 5,
      "content": "Vì bị quân thù đem lưu đày biệt xứ.",
      "order": 5,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.899Z",
      "updatedAt": "2024-09-09T05:02:28.899Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "f295982d-23b6-47cb-9110-825e7d8e9e76",
      "number": 6,
      "content": "Ái Nữ của Si-ôn đã hết thời,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.903Z",
      "updatedAt": "2024-09-09T05:02:28.903Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "16a4950c-5aba-4b4f-ba6f-49f1e8dffec3",
      "number": 6,
      "content": "Vẻ cao sang quý phái của nàng không còn nữa;",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.908Z",
      "updatedAt": "2024-09-09T05:02:28.908Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "5eff0c82-c750-422f-9011-6b5b4ec5e44c",
      "number": 6,
      "content": "Những kẻ cầm quyền của nàng như nai đói không tìm ra đồng cỏ;",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.915Z",
      "updatedAt": "2024-09-09T05:02:28.915Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "9f1c504c-dff0-4549-834f-515d7955cea0",
      "number": 6,
      "content": "Họ bạc nhược bỏ chạy trước mặt kẻ truy kích họ.",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.920Z",
      "updatedAt": "2024-09-09T05:02:28.920Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "25c01a7d-ea8f-44e0-ad06-2606ef56c1c7",
      "number": 7,
      "content": "Giữa những ngày nàng đau khổ và lang thang,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.924Z",
      "updatedAt": "2024-09-09T05:02:28.924Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "cb30cf31-7ce6-4a77-8d99-f97ed8a536e9",
      "number": 7,
      "content": "Giê-ru-sa-lem nhớ lại mọi phước hạnh nàng đã hưởng ngày xưa.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.928Z",
      "updatedAt": "2024-09-09T05:02:28.928Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "c5c8d10c-1e7c-461c-aaf5-3a4be7b66b04",
      "number": 7,
      "content": "Khi dân nàng sa vào tay quân thù, chẳng ai đến tiếp cứu nàng;",
      "order": 2,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.932Z",
      "updatedAt": "2024-09-09T05:02:28.932Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "4f62a512-4fe2-413d-9cc7-cd358552c0c6",
      "number": 7,
      "content": "Thấy tình cảnh của nàng cô đơn như thế, kẻ thù của nàng cười hả hê trước sự sụp đổ của nàng.",
      "order": 3,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.937Z",
      "updatedAt": "2024-09-09T05:02:28.937Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "a09a6103-bec1-449e-944d-b7a8a90bfd81",
      "number": 8,
      "content": "Giê-ru-sa-lem đã phạm quá nhiều tội lỗi,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.941Z",
      "updatedAt": "2024-09-09T05:02:28.941Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "5af457ab-2034-4e46-9da4-4733f2f49319",
      "number": 8,
      "content": "Nên nàng thành cớ thiên hạ cười chê;",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.945Z",
      "updatedAt": "2024-09-09T05:02:28.945Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "7e2bbf59-2126-4f63-9763-bda7709f05c5",
      "number": 8,
      "content": "Tất cả những ai từng kính nể nàng đều khinh bỉ nàng,",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.950Z",
      "updatedAt": "2024-09-09T05:02:28.950Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "17080c77-d2e8-4c8c-9b2f-276327233139",
      "number": 8,
      "content": "Vì chúng đã thấy nàng bị trần trụi lõa lồ.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.954Z",
      "updatedAt": "2024-09-09T05:02:28.954Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "e70b2254-57a9-4a26-86b0-d98fe30e061c",
      "number": 8,
      "content": "Phải, chính nàng cũng rên rỉ thở than,",
      "order": 4,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.958Z",
      "updatedAt": "2024-09-09T05:02:28.958Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "8abbf25d-edc1-479e-ad3a-5d901419b4dc",
      "number": 8,
      "content": "Và quay mặt đi để khỏi bị ai nhìn thấy.",
      "order": 5,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.963Z",
      "updatedAt": "2024-09-09T05:02:28.963Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "75e3a0ef-bafd-43d0-85f9-cff5425e1ec4",
      "number": 9,
      "content": "Vết ô uế vẫn còn dính trên váy nàng,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.967Z",
      "updatedAt": "2024-09-09T05:02:28.967Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "dfc082f4-b173-4646-8659-914f80ce662b",
      "number": 9,
      "content": "Khi phạm tội nàng không nghĩ đến hậu quả về sau;",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.972Z",
      "updatedAt": "2024-09-09T05:02:28.972Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "d7bcac10-e91e-4ee2-83c6-445d880f95cf",
      "number": 9,
      "content": "Vì thế nàng bị sụp đổ kinh hoàng;",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.977Z",
      "updatedAt": "2024-09-09T05:02:28.977Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "285c7d6f-cbb2-45af-a604-10b13c22eba4",
      "number": 9,
      "content": "Không ai an ủi nàng nửa lời.",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.981Z",
      "updatedAt": "2024-09-09T05:02:28.981Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "d1375d72-dca3-43cb-a364-038ff6eb8464",
      "number": 9,
      "content": "“Lạy Chúa, xin đoái xem nỗi đau khổ của con,",
      "order": 4,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.986Z",
      "updatedAt": "2024-09-09T05:02:28.986Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "db5e5f2a-09ea-41d9-ab69-7ff1a4def7e3",
      "number": 9,
      "content": "Vì quân thù của con thắng hơn con.”",
      "order": 5,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.990Z",
      "updatedAt": "2024-09-09T05:02:28.990Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "214c2475-86bf-4b48-ba28-087c307c28c4",
      "number": 10,
      "content": "Quân thù đã đưa tay hốt sạch của cải quý báu của nàng;",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:28.995Z",
      "updatedAt": "2024-09-09T05:02:28.995Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "5255daf0-c69b-425e-a718-074423b9a6ea",
      "number": 10,
      "content": "Nàng đã thấy các dân ngoại nghênh ngang vào đền thánh,",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29Z",
      "updatedAt": "2024-09-09T05:02:29Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "afc0eba5-a155-4bcb-a981-2d29e5f825fc",
      "number": 10,
      "content": "Ðó là những kẻ Ngài cấm vào nơi nhóm họp của dân Ngài.",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.005Z",
      "updatedAt": "2024-09-09T05:02:29.005Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "efd800b5-798c-4092-a600-f04b1c03a1ba",
      "number": 11,
      "content": "Toàn dân nàng than thở khi tìm thực phẩm đỡ lòng;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.009Z",
      "updatedAt": "2024-09-09T05:02:29.009Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "105ee410-5c73-4523-8ac6-abb389998ca3",
      "number": 11,
      "content": "Họ đánh đổi của báu cho thức ăn đạm bạc,",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.014Z",
      "updatedAt": "2024-09-09T05:02:29.014Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "55d3ba78-c8f8-4995-a16d-eb8089aeb95c",
      "number": 11,
      "content": "Ðể mong được phục sức.",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.018Z",
      "updatedAt": "2024-09-09T05:02:29.018Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "a92b3fac-74a5-437f-bde2-c6c48cf8d7c6",
      "number": 11,
      "content": "“Lạy Chúa, xin đoái xem,",
      "order": 3,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.022Z",
      "updatedAt": "2024-09-09T05:02:29.022Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "0990bacc-a312-4eb8-b890-a060c31353c2",
      "number": 11,
      "content": "Con bị khinh bỉ biết bao!”",
      "order": 4,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.027Z",
      "updatedAt": "2024-09-09T05:02:29.027Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "469e13f2-c079-43e2-a9f6-224e352010d4",
      "number": 12,
      "content": "Hỡi tất cả những ai qua lại,",
      "order": 0,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.032Z",
      "updatedAt": "2024-09-09T05:02:29.032Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "376dfb33-b827-47b6-8ffc-a951630b6b0c",
      "number": 12,
      "content": "Quý vị thấy sự kiện này chẳng có ý nghĩa gì sao?",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.036Z",
      "updatedAt": "2024-09-09T05:02:29.036Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "bfd6b746-0437-44f7-9d66-839eaacb2142",
      "number": 12,
      "content": "Hãy nhìn và xem,",
      "order": 2,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.041Z",
      "updatedAt": "2024-09-09T05:02:29.041Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "473d76fa-b3e3-457f-b180-c339a31e1922",
      "number": 12,
      "content": "Có nỗi sầu khổ nào sánh bằng nỗi sầu khổ của tôi chăng?",
      "order": 3,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.046Z",
      "updatedAt": "2024-09-09T05:02:29.046Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "b92095af-41d5-49ce-bcda-9469ee403f6f",
      "number": 12,
      "content": "Ðó là điều đã xảy đến với tôi,",
      "order": 4,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.050Z",
      "updatedAt": "2024-09-09T05:02:29.050Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "3ee76a6f-f2e5-476a-8138-b93873ebe0e3",
      "number": 12,
      "content": "Mà Chúa đã giáng trên tôi trong ngày Ngài bừng bừng nổi giận.",
      "order": 5,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.055Z",
      "updatedAt": "2024-09-09T05:02:29.055Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "a9ab4a04-c4a5-4929-81f0-f21e845c6d09",
      "number": 13,
      "content": "Từ trời cao Ngài phóng lửa hừng vụt xuống,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.060Z",
      "updatedAt": "2024-09-09T05:02:29.060Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "ca464f99-6712-4f45-a794-b6e662cfb022",
      "number": 13,
      "content": "Khiến lửa hừng thiêu đốt xương tôi;",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.064Z",
      "updatedAt": "2024-09-09T05:02:29.064Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "9509838b-3ec8-473f-a12f-77ccdb2eb02b",
      "number": 13,
      "content": "Ngài giăng lưới để chân tôi vướng mắc,",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.069Z",
      "updatedAt": "2024-09-09T05:02:29.069Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "f9dfc6de-8904-4192-a004-918028e2e63f",
      "number": 13,
      "content": "Ngài bắt tôi phải quay trở lại;",
      "order": 3,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.074Z",
      "updatedAt": "2024-09-09T05:02:29.074Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "4f2f500b-ce30-401e-96c9-d8ca7a35188b",
      "number": 13,
      "content": "Ngài để tôi quạnh hiu và ngất xỉu suốt ngày.",
      "order": 4,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.078Z",
      "updatedAt": "2024-09-09T05:02:29.078Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "ecad5245-04f7-42d2-bd01-a0cdb3710402",
      "number": 14,
      "content": "Ngài buộc những vi phạm của tôi vào một cái ách,",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.083Z",
      "updatedAt": "2024-09-09T05:02:29.083Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "fd6a9eeb-50fa-4304-8042-eec5933419ef",
      "number": 14,
      "content": "Ngài dùng tay Ngài cột chặt chúng lại với nhau,",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.087Z",
      "updatedAt": "2024-09-09T05:02:29.087Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "c99b4ab6-a944-43b2-b42b-acc105ac20ea",
      "number": 14,
      "content": "Và đặt chúng đè nặng trên cổ tôi,",
      "order": 2,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.091Z",
      "updatedAt": "2024-09-09T05:02:29.091Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "69ee37eb-a321-42f3-bf07-5fe01371fc2b",
      "number": 14,
      "content": "Khiến sức lực tôi tiêu hao cạn kiệt;",
      "order": 3,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.096Z",
      "updatedAt": "2024-09-09T05:02:29.096Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "b98fc995-859b-48d4-b2da-c5936bd3393b",
      "number": 14,
      "content": "Chúa đã trao tôi vào tay những kẻ tôi không có khả năng chống cự.",
      "order": 4,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.101Z",
      "updatedAt": "2024-09-09T05:02:29.101Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "50fb4a1e-953e-4df1-816f-72eb722a8093",
      "number": 15,
      "content": "Chúa loại bỏ tất cả các dũng sĩ ở giữa tôi;",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.105Z",
      "updatedAt": "2024-09-09T05:02:29.105Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "3d48d421-f97c-42ec-8995-486feb24daa6",
      "number": 15,
      "content": "Ngài triệu tập một đội quân đông đảo chống lại tôi,",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.109Z",
      "updatedAt": "2024-09-09T05:02:29.109Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "c34e6622-0e6d-428e-a0c5-da2a76d5ab65",
      "number": 15,
      "content": "Ðể tiêu diệt các chiến sĩ trẻ của tôi;",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.114Z",
      "updatedAt": "2024-09-09T05:02:29.114Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "d4424cb7-c6cb-448e-9be2-16ca49357a19",
      "number": 15,
      "content": "Chúa chà đạp tôi như người ta đạp nho trong bồn ép nho làm rượu;",
      "order": 3,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.119Z",
      "updatedAt": "2024-09-09T05:02:29.119Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "c5e19104-555f-4bfa-a8c1-d916814d9ce5",
      "number": 15,
      "content": "Ngài chà đạp người trinh nữ là Ái Nữ của Giu-đa.",
      "order": 4,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.124Z",
      "updatedAt": "2024-09-09T05:02:29.124Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "73d6cf7b-3410-4140-916b-f9a3034c19aa",
      "number": 16,
      "content": "Vì những điều ấy nên tôi khóc,",
      "order": 0,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.128Z",
      "updatedAt": "2024-09-09T05:02:29.128Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "9087182d-5cca-4556-bda6-b2d0dd78b8a4",
      "number": 16,
      "content": "Mắt tôi đổ lệ dầm dề;",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.132Z",
      "updatedAt": "2024-09-09T05:02:29.132Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "da7cf7c4-9c72-426f-af8a-1fffd66aa49f",
      "number": 16,
      "content": "Vì Ðấng an ủi, Ðấng làm cho tinh thần tôi phục hồi, đã lìa xa tôi;",
      "order": 2,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.137Z",
      "updatedAt": "2024-09-09T05:02:29.137Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "6350343e-aad5-425a-a978-095bd27bc5b7",
      "number": 16,
      "content": "Các con cái tôi bơ vơ đói khổ,",
      "order": 3,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.141Z",
      "updatedAt": "2024-09-09T05:02:29.141Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "8c3f454c-78e7-42c1-84f3-6f5343a5c9e3",
      "number": 16,
      "content": "Vì quân thù đã thắng hơn tôi.",
      "order": 4,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.145Z",
      "updatedAt": "2024-09-09T05:02:29.145Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "3f3257e6-859e-4847-9667-aeaae9866188",
      "number": 17,
      "content": "Si-ôn đưa tay ra van xin giúp đỡ,",
      "order": 0,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.150Z",
      "updatedAt": "2024-09-09T05:02:29.150Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "b8603e75-cf7a-4827-af72-0fce15a0b1c2",
      "number": 17,
      "content": "Nhưng không ai an ủi nàng;",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.154Z",
      "updatedAt": "2024-09-09T05:02:29.154Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "ca18dfc6-9dd4-4661-a83e-b02324c100d0",
      "number": 17,
      "content": "Chúa đã ra lịnh cho các láng giềng của Gia-cốp rằng,",
      "order": 2,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.158Z",
      "updatedAt": "2024-09-09T05:02:29.158Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "e15386d3-4fc9-4e62-a998-266e869c3e6b",
      "number": 17,
      "content": "Chúng phải trở nên kẻ thù của ông;",
      "order": 3,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.163Z",
      "updatedAt": "2024-09-09T05:02:29.163Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "54ddbc0a-1d14-4695-8759-b4d74771f665",
      "number": 17,
      "content": "Giê-ru-sa-lem đã trở thành một vật ô uế giữa các láng giềng của nàng.",
      "order": 4,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.167Z",
      "updatedAt": "2024-09-09T05:02:29.167Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "dd138ea0-28e4-4cad-9830-efa5ce6d932f",
      "number": 18,
      "content": "Chúa thật đúng, vì tôi đã nổi loạn chống lại mạng lịnh Ngài;",
      "order": 0,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.171Z",
      "updatedAt": "2024-09-09T05:02:29.171Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "e119c531-cf64-4c1f-9141-5f3cff9a1f3f",
      "number": 18,
      "content": "Hỡi muôn dân, hãy lắng nghe,",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.176Z",
      "updatedAt": "2024-09-09T05:02:29.176Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "3265192c-a6c8-402f-a59b-c8ed491878d8",
      "number": 18,
      "content": "Và hãy nhìn xem những sầu thảm tôi đang phải chịu;",
      "order": 2,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.180Z",
      "updatedAt": "2024-09-09T05:02:29.180Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "18675e07-66b1-4266-8b00-4bda92a6404f",
      "number": 18,
      "content": "Các thiếu nữ và các thanh niên của tôi đã bị bắt đem lưu đày.",
      "order": 3,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.184Z",
      "updatedAt": "2024-09-09T05:02:29.184Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "693692b6-2cce-4091-b9e1-1bb0755bc284",
      "number": 19,
      "content": "Tôi gọi những người yêu của tôi, nhưng họ đã gạt tôi;",
      "order": 0,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.188Z",
      "updatedAt": "2024-09-09T05:02:29.188Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "048f711f-5c28-4280-a285-ccde18e1b5ed",
      "number": 19,
      "content": "Các tư tế và các trưởng lão của tôi chết thảm trong thành,",
      "order": 1,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.196Z",
      "updatedAt": "2024-09-09T05:02:29.196Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "05d4f4c4-e4bb-49c5-b97b-ab4ad769a993",
      "number": 19,
      "content": "Khi họ cố tìm chút gì để ăn đỡ đói, hầu phục hồi sinh lực của họ.",
      "order": 2,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.200Z",
      "updatedAt": "2024-09-09T05:02:29.200Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "7b84d297-4370-4d77-88b9-14668210ec69",
      "number": 20,
      "content": "Lạy Chúa, xin Ngài xem, con tuyệt vọng biết bao!",
      "order": 0,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.204Z",
      "updatedAt": "2024-09-09T05:02:29.204Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "0e273dfb-0c3d-4d85-ba3d-00bb75435bdf",
      "number": 20,
      "content": "Bụng con phập phồng lo sợ,",
      "order": 1,
      "parNumber": 95,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.208Z",
      "updatedAt": "2024-09-09T05:02:29.208Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "bdf23dca-92b8-4163-8b7f-b9b9d56ef0f6",
      "number": 20,
      "content": "Tim con hồi hộp âu lo.",
      "order": 2,
      "parNumber": 96,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.213Z",
      "updatedAt": "2024-09-09T05:02:29.213Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "6e9479ea-fd3e-4fb0-93b1-91cafe03a127",
      "number": 20,
      "content": "Vì con đã quyết tâm chống lại Ngài,",
      "order": 3,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.217Z",
      "updatedAt": "2024-09-09T05:02:29.217Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "9b7ba445-2079-4f77-9dce-5024d9d3f738",
      "number": 20,
      "content": "Nên ngoài đường gươm đao đua nhau lấy mạng,",
      "order": 4,
      "parNumber": 98,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.221Z",
      "updatedAt": "2024-09-09T05:02:29.221Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "33e419df-b3a4-4248-b31b-068567750259",
      "number": 20,
      "content": "Còn trong nhà thần chết hoành hành.",
      "order": 5,
      "parNumber": 99,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.225Z",
      "updatedAt": "2024-09-09T05:02:29.225Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "1ec1f344-8d4d-4423-b191-b78952695ad2",
      "number": 21,
      "content": "Người ta nghe tiếng con rên rỉ, nhưng chẳng ai an ủi con.",
      "order": 0,
      "parNumber": 100,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.229Z",
      "updatedAt": "2024-09-09T05:02:29.229Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "7602b6bb-b9ee-4e35-a955-612f04778651",
      "number": 21,
      "content": "Tất cả kẻ thù của con nghe tin con gặp nạn,",
      "order": 1,
      "parNumber": 101,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.233Z",
      "updatedAt": "2024-09-09T05:02:29.233Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "4ea3cda6-1e3a-49cc-a163-d0b1319e100d",
      "number": 21,
      "content": "Chúng vui mừng vì Ngài làm việc ấy cho con.",
      "order": 2,
      "parNumber": 102,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.237Z",
      "updatedAt": "2024-09-09T05:02:29.237Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "ba7a44ce-74ee-4c45-9e2a-2c8236118500",
      "number": 21,
      "content": "Xin Ngài sớm thực hiện ngày đoán phạt mà Ngài đã phán,",
      "order": 3,
      "parNumber": 103,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.241Z",
      "updatedAt": "2024-09-09T05:02:29.241Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "0d3f15d6-5a73-4e03-b04b-b4690ee1cd4e",
      "number": 21,
      "content": "Ðể chúng biết mùi đau khổ như con.",
      "order": 4,
      "parNumber": 104,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.245Z",
      "updatedAt": "2024-09-09T05:02:29.245Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "62fa3f03-5a61-4452-bb76-fb3eb3f4d1f3",
      "number": 22,
      "content": "Nguyện các việc ác của chúng bị phơi bày trước thánh nhan Ngài;",
      "order": 0,
      "parNumber": 105,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.250Z",
      "updatedAt": "2024-09-09T05:02:29.250Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "eaa8fe57-e7ef-4d39-b73d-5cebf6f1b581",
      "number": 22,
      "content": "Xin đối xử với chúng như Ngài đã đối xử với con,",
      "order": 1,
      "parNumber": 106,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.254Z",
      "updatedAt": "2024-09-09T05:02:29.254Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "c16c9e25-7cf5-43c1-887f-d68d57324c4a",
      "number": 22,
      "content": "Vì mọi vi phạm của con,",
      "order": 2,
      "parNumber": 107,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.258Z",
      "updatedAt": "2024-09-09T05:02:29.258Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "995b1af1-d02a-47e5-9285-26c56fbf6bf9",
      "number": 22,
      "content": "Vì con than thở quá nhiều,",
      "order": 3,
      "parNumber": 108,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.262Z",
      "updatedAt": "2024-09-09T05:02:29.262Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    },
    {
      "id": "64093fff-36c4-4d6c-a6f6-46a402663bf5",
      "number": 22,
      "content": "Và lòng con giờ đã nao sờn.",
      "order": 4,
      "parNumber": 109,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:29.267Z",
      "updatedAt": "2024-09-09T05:02:29.267Z",
      "chapterId": "37dcea6d-6044-4aaf-a403-45a1624958ee"
    }
  ],
