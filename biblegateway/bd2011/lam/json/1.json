{
  "chapter": {
    "id": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Ai%20Ca%201&version=BD2011",
    "createdAt": "2024-07-12T04:03:52.193Z",
    "updatedAt": "2024-07-24T15:07:52.741Z",
    "bookId": "6b281373-14aa-49aa-98f5-676fdd51956b"
  },
  "verses": [
    {
      "id": "a62e8870-148f-48c6-a216-eed89eead8b6",
      "number": 1,
      "content": "Kìa thành phố một thời đông người nhộn nhịp,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.142Z",
      "updatedAt": "2024-07-15T03:47:21.142Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "6c6de96c-0284-43ce-9c7d-43347af7d1f5",
      "number": 1,
      "content": "Nay thưa người đơn chiếc đìu hiu!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.142Z",
      "updatedAt": "2024-07-15T03:47:21.142Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "42c6961d-d0e5-409f-88a2-553cd192c73e",
      "number": 1,
      "content": "Kìa nàng vốn đại đô các nước,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.142Z",
      "updatedAt": "2024-07-15T03:47:21.142Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "f74cbf49-d110-4e4e-8b0e-0d661767e077",
      "number": 1,
      "content": "Giờ trở thành góa phụ cô đơn!",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.142Z",
      "updatedAt": "2024-07-15T03:47:21.142Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "5829c4fb-4806-411b-930e-f3e97288ae27",
      "number": 1,
      "content": "Kìa nàng vốn nữ hoàng các tiểu quốc,",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.142Z",
      "updatedAt": "2024-07-15T03:47:21.142Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "135e1028-5747-4275-b71f-e2d97d02679e",
      "number": 1,
      "content": "Nay biến thành nữ nô lệ phục tùng!",
      "order": 5,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.142Z",
      "updatedAt": "2024-07-15T03:47:21.142Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "8a2a7cc4-0e22-4b0e-b5b8-d948b892327c",
      "number": 2,
      "content": "Giữa đêm khuya nàng khóc than cay đắng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.152Z",
      "updatedAt": "2024-07-15T03:47:21.152Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "6cdcb069-c02e-473c-8a53-362250edd93a",
      "number": 2,
      "content": "Lệ tuôn tràn trên đôi má gầy gò;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.152Z",
      "updatedAt": "2024-07-15T03:47:21.152Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "b5f8bc9a-f23c-4475-bb66-9220245d8c00",
      "number": 2,
      "content": "Trong số tất cả tình nhân của nàng, không ai an ủi nàng;",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.152Z",
      "updatedAt": "2024-07-15T03:47:21.152Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "9b92f790-66b3-4b4e-9588-6de263289cdc",
      "number": 2,
      "content": "Tất cả các bạn nàng đã phụ rẫy nàng,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.152Z",
      "updatedAt": "2024-07-15T03:47:21.152Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "3987270b-f2c0-4493-b45c-552261ae2c1d",
      "number": 2,
      "content": "Thậm chí chúng còn trở thành kẻ thù của nàng.",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.152Z",
      "updatedAt": "2024-07-15T03:47:21.152Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "92873f0e-6cc9-43b7-9d4a-ce163b58d131",
      "number": 3,
      "content": "Giu-đa đã bị lưu đày,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.155Z",
      "updatedAt": "2024-07-15T03:47:21.155Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "fdd1ce9b-e287-4861-b9e6-d885ffe2b400",
      "number": 3,
      "content": "Nàng phải chịu biết bao đau đớn và phải làm lao động khổ sai;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.155Z",
      "updatedAt": "2024-07-15T03:47:21.155Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "0d4c131e-8e5f-48a8-b950-5c1eca599cac",
      "number": 3,
      "content": "Bây giờ nàng phải sống rải rác giữa các nước,",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.155Z",
      "updatedAt": "2024-07-15T03:47:21.155Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "f8457f11-9ae8-4528-9929-eeb166cbde9a",
      "number": 3,
      "content": "Nàng chẳng tìm được nơi nào để nghỉ ngơi;",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.155Z",
      "updatedAt": "2024-07-15T03:47:21.155Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "f0391337-0636-4429-bed1-102a49c439af",
      "number": 3,
      "content": "Mọi kẻ truy đuổi nàng đều bắt kịp nàng trong lúc nàng cùng đường.",
      "order": 4,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.155Z",
      "updatedAt": "2024-07-15T03:47:21.155Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "e9e9f988-0028-42fd-98fb-cc3398b155f8",
      "number": 4,
      "content": "Những con đường dẫn đến Si-ôn âu sầu ảm đạm,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.158Z",
      "updatedAt": "2024-07-15T03:47:21.158Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "799e5ca8-bf45-4b73-8eb1-89b217bf21d9",
      "number": 4,
      "content": "Vì không ai đến dự các đại lễ như xưa;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.158Z",
      "updatedAt": "2024-07-15T03:47:21.158Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "945098c8-01a7-4531-9fc7-d8f5d7421a23",
      "number": 4,
      "content": "Tất cả các cổng thành đều đìu hiu quạnh quẽ,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.158Z",
      "updatedAt": "2024-07-15T03:47:21.158Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "eeb22b48-693c-4962-abfb-d9bf29315b81",
      "number": 4,
      "content": "Các tư tế đều rầu rĩ thở than,",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.158Z",
      "updatedAt": "2024-07-15T03:47:21.158Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "89a555f7-b05a-45ff-bb59-7c0fa58b1b78",
      "number": 4,
      "content": "Các thiếu nữ của nàng buồn da diết,",
      "order": 4,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.158Z",
      "updatedAt": "2024-07-15T03:47:21.158Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "4323fd99-fa4b-4f37-a975-1d9749ee38b9",
      "number": 4,
      "content": "Số phận nàng thật đau khổ đắng cay.",
      "order": 5,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.158Z",
      "updatedAt": "2024-07-15T03:47:21.158Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "874c8154-31d3-4e9f-a504-1ad2ffde4ffe",
      "number": 5,
      "content": "Những kẻ thù ghét nàng đã trở thành các chủ nhân nàng,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.161Z",
      "updatedAt": "2024-07-15T03:47:21.161Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "1c345da6-d22d-4fd6-93ed-686e5b220a92",
      "number": 5,
      "content": "Những kẻ thù của nàng đã phát đạt.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.161Z",
      "updatedAt": "2024-07-15T03:47:21.161Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "9cf07ed3-e333-48ae-a3f9-6b46d5a14d1d",
      "number": 5,
      "content": "Vì vô số tội lỗi của nàng,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.161Z",
      "updatedAt": "2024-07-15T03:47:21.161Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "e208145c-b0a8-480e-aee9-97f8c1f2901c",
      "number": 5,
      "content": "Chúa bắt nàng phải chịu đau khổ;",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.161Z",
      "updatedAt": "2024-07-15T03:47:21.161Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "b0988e64-031b-4037-8cf9-fc8edf84a34c",
      "number": 5,
      "content": "Con cái nàng phải sống tha phương,",
      "order": 4,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.161Z",
      "updatedAt": "2024-07-15T03:47:21.161Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "6d0447be-0454-4fea-9245-97d1cf1fd188",
      "number": 5,
      "content": "Vì bị quân thù đem lưu đày biệt xứ.",
      "order": 5,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.161Z",
      "updatedAt": "2024-07-15T03:47:21.161Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "31481c56-777b-486f-a3cf-d5a697b5c873",
      "number": 6,
      "content": "Ái Nữ của Si-ôn đã hết thời,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.163Z",
      "updatedAt": "2024-07-15T03:47:21.163Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "8beb06c5-95ab-4d6b-8fd8-5c0f602b9f7d",
      "number": 6,
      "content": "Vẻ cao sang quý phái của nàng không còn nữa;",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.163Z",
      "updatedAt": "2024-07-15T03:47:21.163Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "c646851e-16ba-45d6-8f67-f0224a0626d1",
      "number": 6,
      "content": "Những kẻ cầm quyền của nàng như nai đói không tìm ra đồng cỏ;",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.163Z",
      "updatedAt": "2024-07-15T03:47:21.163Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "1d5ee31d-41c2-4b93-8116-e029e12eab64",
      "number": 6,
      "content": "Họ bạc nhược bỏ chạy trước mặt kẻ truy kích họ.",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.163Z",
      "updatedAt": "2024-07-15T03:47:21.163Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "d8e94dd2-b8d7-45a8-93ed-b8f5ef63f50e",
      "number": 7,
      "content": "Giữa những ngày nàng đau khổ và lang thang,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.166Z",
      "updatedAt": "2024-07-15T03:47:21.166Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "2753ba45-51d4-4667-a4d8-0dc61ad97e03",
      "number": 7,
      "content": "Giê-ru-sa-lem nhớ lại mọi phước hạnh nàng đã hưởng ngày xưa.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.166Z",
      "updatedAt": "2024-07-15T03:47:21.166Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "b55755dc-dece-4760-90c4-6b77ac28a522",
      "number": 7,
      "content": "Khi dân nàng sa vào tay quân thù, chẳng ai đến tiếp cứu nàng;",
      "order": 2,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.166Z",
      "updatedAt": "2024-07-15T03:47:21.166Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "9241cd99-c6cb-4ef7-96c7-cab852235c7b",
      "number": 7,
      "content": "Thấy tình cảnh của nàng cô đơn như thế, kẻ thù của nàng cười hả hê trước sự sụp đổ của nàng.",
      "order": 3,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.166Z",
      "updatedAt": "2024-07-15T03:47:21.166Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "65abbba8-ce39-444c-a4f7-43e6c5890a01",
      "number": 8,
      "content": "Giê-ru-sa-lem đã phạm quá nhiều tội lỗi,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.169Z",
      "updatedAt": "2024-07-15T03:47:21.169Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "47acec6d-8be6-4c44-8fed-79ecfb9f254c",
      "number": 8,
      "content": "Nên nàng thành cớ thiên hạ cười chê;",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.169Z",
      "updatedAt": "2024-07-15T03:47:21.169Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "ffbc276a-9828-41a3-96b4-3bfadeeb3853",
      "number": 8,
      "content": "Tất cả những ai từng kính nể nàng đều khinh bỉ nàng,",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.169Z",
      "updatedAt": "2024-07-15T03:47:21.169Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "3134bcf4-6af3-4a29-9703-020076f2b1b9",
      "number": 8,
      "content": "Vì chúng đã thấy nàng bị trần trụi lõa lồ.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.169Z",
      "updatedAt": "2024-07-15T03:47:21.169Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "4bb0a356-3f50-45df-bea1-4354cc95c542",
      "number": 8,
      "content": "Phải, chính nàng cũng rên rỉ thở than,",
      "order": 4,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.169Z",
      "updatedAt": "2024-07-15T03:47:21.169Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "61e86938-2dca-4868-8f42-bd2d7c3b16be",
      "number": 8,
      "content": "Và quay mặt đi để khỏi bị ai nhìn thấy.",
      "order": 5,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.169Z",
      "updatedAt": "2024-07-15T03:47:21.169Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "9788e3ac-4f18-4a2d-a380-5440fe01f9de",
      "number": 9,
      "content": "Vết ô uế vẫn còn dính trên váy nàng,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.171Z",
      "updatedAt": "2024-07-15T03:47:21.171Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "03e3fccb-7791-4b52-b1bc-4809bfd42b69",
      "number": 9,
      "content": "Khi phạm tội nàng không nghĩ đến hậu quả về sau;",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.171Z",
      "updatedAt": "2024-07-15T03:47:21.171Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "29bf68b2-1bda-45bf-b8f5-19479e8d100c",
      "number": 9,
      "content": "Vì thế nàng bị sụp đổ kinh hoàng;",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.171Z",
      "updatedAt": "2024-07-15T03:47:21.171Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "87839718-f61d-4f63-988c-bb214ba0a589",
      "number": 9,
      "content": "Không ai an ủi nàng nửa lời.",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.171Z",
      "updatedAt": "2024-07-15T03:47:21.171Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "36fe8532-3df3-425e-9e8e-4139f7fce673",
      "number": 9,
      "content": "“Lạy Chúa, xin đoái xem nỗi đau khổ của con,",
      "order": 4,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.171Z",
      "updatedAt": "2024-07-15T03:47:21.171Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "6e892d50-1dee-49d6-be86-7b4bdea3d6bb",
      "number": 9,
      "content": "Vì quân thù của con thắng hơn con.”",
      "order": 5,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.171Z",
      "updatedAt": "2024-07-15T03:47:21.171Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "5e3338db-6016-481a-8d29-ab0070e38659",
      "number": 10,
      "content": "Quân thù đã đưa tay hốt sạch của cải quý báu của nàng;",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.174Z",
      "updatedAt": "2024-07-15T03:47:21.174Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "debbbe77-64b3-4e39-b12b-28402369ec59",
      "number": 10,
      "content": "Nàng đã thấy các dân ngoại nghênh ngang vào đền thánh,",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.174Z",
      "updatedAt": "2024-07-15T03:47:21.174Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "430f7f72-14db-48f2-91d1-f6db7a5248e4",
      "number": 10,
      "content": "Ðó là những kẻ Ngài cấm vào nơi nhóm họp của dân Ngài.",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.174Z",
      "updatedAt": "2024-07-15T03:47:21.174Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "0c42e9cf-3b11-4086-8cc7-96a60fe5f2fb",
      "number": 11,
      "content": "Toàn dân nàng than thở khi tìm thực phẩm đỡ lòng;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.176Z",
      "updatedAt": "2024-07-15T03:47:21.176Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "c87813f4-1344-4ec7-b27d-aacb5030a4fb",
      "number": 11,
      "content": "Họ đánh đổi của báu cho thức ăn đạm bạc,",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.176Z",
      "updatedAt": "2024-07-15T03:47:21.176Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "0e9fedef-e493-4aee-9d52-c65e92522d5b",
      "number": 11,
      "content": "Ðể mong được phục sức.",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.176Z",
      "updatedAt": "2024-07-15T03:47:21.176Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "d7791075-e69f-4705-98c1-1c2f486fe9e4",
      "number": 11,
      "content": "“Lạy Chúa, xin đoái xem,",
      "order": 3,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.176Z",
      "updatedAt": "2024-07-15T03:47:21.176Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "ba221d57-dce9-4bfe-ba4c-09d0ea14c371",
      "number": 11,
      "content": "Con bị khinh bỉ biết bao!”",
      "order": 4,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.176Z",
      "updatedAt": "2024-07-15T03:47:21.176Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "fa239cdc-b980-4f18-a6bd-576324617240",
      "number": 12,
      "content": "Hỡi tất cả những ai qua lại,",
      "order": 0,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.179Z",
      "updatedAt": "2024-07-15T03:47:21.179Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "16e8e06c-4a7f-4c40-896e-52ed271a04bc",
      "number": 12,
      "content": "Quý vị thấy sự kiện này chẳng có ý nghĩa gì sao?",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.179Z",
      "updatedAt": "2024-07-15T03:47:21.179Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "f6b8ee2c-9fd4-471f-b13e-2603bf71d3fc",
      "number": 12,
      "content": "Hãy nhìn và xem,",
      "order": 2,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.179Z",
      "updatedAt": "2024-07-15T03:47:21.179Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "57fdd3f1-ce12-42f6-a1fe-862b38fa9834",
      "number": 12,
      "content": "Có nỗi sầu khổ nào sánh bằng nỗi sầu khổ của tôi chăng?",
      "order": 3,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.179Z",
      "updatedAt": "2024-07-15T03:47:21.179Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "9ecaa611-58d0-47be-8e2a-e184d30ecea2",
      "number": 12,
      "content": "Ðó là điều đã xảy đến với tôi,",
      "order": 4,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.179Z",
      "updatedAt": "2024-07-15T03:47:21.179Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "a432d91c-6757-4a55-b5d6-9b55b083ab86",
      "number": 12,
      "content": "Mà Chúa đã giáng trên tôi trong ngày Ngài bừng bừng nổi giận.",
      "order": 5,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.179Z",
      "updatedAt": "2024-07-15T03:47:21.179Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "3640650a-d7c2-4f98-bde9-5530da8f6436",
      "number": 13,
      "content": "Từ trời cao Ngài phóng lửa hừng vụt xuống,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.188Z",
      "updatedAt": "2024-07-15T03:47:21.188Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "8fe701bb-45df-403e-8f8b-e0f1b9dd8810",
      "number": 13,
      "content": "Khiến lửa hừng thiêu đốt xương tôi;",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.188Z",
      "updatedAt": "2024-07-15T03:47:21.188Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "ce40be30-8632-470b-b30d-075895896134",
      "number": 13,
      "content": "Ngài giăng lưới để chân tôi vướng mắc,",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.188Z",
      "updatedAt": "2024-07-15T03:47:21.188Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "81c2b59c-936d-4016-9a68-51ddfcdc3bb6",
      "number": 13,
      "content": "Ngài bắt tôi phải quay trở lại;",
      "order": 3,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.188Z",
      "updatedAt": "2024-07-15T03:47:21.188Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "0defd977-032e-4113-9962-b82b2dfd9ef9",
      "number": 13,
      "content": "Ngài để tôi quạnh hiu và ngất xỉu suốt ngày.",
      "order": 4,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.188Z",
      "updatedAt": "2024-07-15T03:47:21.188Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "b7e2b5ea-5e9a-4266-97e3-9d735c73c389",
      "number": 14,
      "content": "Ngài buộc những vi phạm của tôi vào một cái ách,",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.224Z",
      "updatedAt": "2024-07-15T03:47:21.224Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "e448927c-5833-4bc2-ae8a-229faebb6829",
      "number": 14,
      "content": "Ngài dùng tay Ngài cột chặt chúng lại với nhau,",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.224Z",
      "updatedAt": "2024-07-15T03:47:21.224Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "7376dd29-863d-42c1-876c-c6eb828489dc",
      "number": 14,
      "content": "Và đặt chúng đè nặng trên cổ tôi,",
      "order": 2,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.224Z",
      "updatedAt": "2024-07-15T03:47:21.224Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "01d2861f-bdc4-4c3c-86eb-d25c7099e0db",
      "number": 14,
      "content": "Khiến sức lực tôi tiêu hao cạn kiệt;",
      "order": 3,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.224Z",
      "updatedAt": "2024-07-15T03:47:21.224Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "309610fe-85e5-4a6e-a677-15917b79c371",
      "number": 14,
      "content": "Chúa đã trao tôi vào tay những kẻ tôi không có khả năng chống cự.",
      "order": 4,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.224Z",
      "updatedAt": "2024-07-15T03:47:21.224Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "69212ed3-4398-4c72-9c6c-00bb2ab0b4d7",
      "number": 15,
      "content": "Chúa loại bỏ tất cả các dũng sĩ ở giữa tôi;",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.272Z",
      "updatedAt": "2024-07-15T03:47:21.272Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "abfe0cdd-13c1-447b-9b20-5f8d97247704",
      "number": 15,
      "content": "Ngài triệu tập một đội quân đông đảo chống lại tôi,",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.272Z",
      "updatedAt": "2024-07-15T03:47:21.272Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "ea497c6c-64f4-4bc9-90fc-91cd6cee90a9",
      "number": 15,
      "content": "Ðể tiêu diệt các chiến sĩ trẻ của tôi;",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.272Z",
      "updatedAt": "2024-07-15T03:47:21.272Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "2e00a375-8176-40bb-89d1-90c3e59dd75c",
      "number": 15,
      "content": "Chúa chà đạp tôi như người ta đạp nho trong bồn ép nho làm rượu;",
      "order": 3,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.272Z",
      "updatedAt": "2024-07-15T03:47:21.272Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "e43f76b4-deff-4172-bbde-c36d78cc58f1",
      "number": 15,
      "content": "Ngài chà đạp người trinh nữ là Ái Nữ của Giu-đa.",
      "order": 4,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.272Z",
      "updatedAt": "2024-07-15T03:47:21.272Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "80857a8f-bcbc-4523-8d59-773be3f0e169",
      "number": 16,
      "content": "Vì những điều ấy nên tôi khóc,",
      "order": 0,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.313Z",
      "updatedAt": "2024-07-15T03:47:21.313Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "c0510d4a-a142-4eb3-87dd-efe7ad382d96",
      "number": 16,
      "content": "Mắt tôi đổ lệ dầm dề;",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.313Z",
      "updatedAt": "2024-07-15T03:47:21.313Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "7811aee5-60a0-4958-a2c1-3ca846828b00",
      "number": 16,
      "content": "Vì Ðấng an ủi, Ðấng làm cho tinh thần tôi phục hồi, đã lìa xa tôi;",
      "order": 2,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.313Z",
      "updatedAt": "2024-07-15T03:47:21.313Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "f58bcefe-c5d2-4589-9251-666abf4cb66b",
      "number": 16,
      "content": "Các con cái tôi bơ vơ đói khổ,",
      "order": 3,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.313Z",
      "updatedAt": "2024-07-15T03:47:21.313Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "e58b1615-ff0a-4d6b-90ac-da40a06ac5cd",
      "number": 16,
      "content": "Vì quân thù đã thắng hơn tôi.",
      "order": 4,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.313Z",
      "updatedAt": "2024-07-15T03:47:21.313Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "ca07174e-e8eb-4f00-b53e-ea4188b3e83b",
      "number": 17,
      "content": "Si-ôn đưa tay ra van xin giúp đỡ,",
      "order": 0,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.320Z",
      "updatedAt": "2024-07-15T03:47:21.320Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "189fe1ff-211b-463f-b589-aae079d1ad53",
      "number": 17,
      "content": "Nhưng không ai an ủi nàng;",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.320Z",
      "updatedAt": "2024-07-15T03:47:21.320Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "3fa6fb05-1129-4d4d-a2b0-30ffcda2cdec",
      "number": 17,
      "content": "Chúa đã ra lịnh cho các láng giềng của Gia-cốp rằng,",
      "order": 2,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.320Z",
      "updatedAt": "2024-07-15T03:47:21.320Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "511198bf-c215-496e-a767-0ec7c947b7af",
      "number": 17,
      "content": "Chúng phải trở nên kẻ thù của ông;",
      "order": 3,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.320Z",
      "updatedAt": "2024-07-15T03:47:21.320Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "45c0897b-6d65-4299-a31b-bcee78eae23e",
      "number": 17,
      "content": "Giê-ru-sa-lem đã trở thành một vật ô uế giữa các láng giềng của nàng.",
      "order": 4,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.320Z",
      "updatedAt": "2024-07-15T03:47:21.320Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "36f2bf92-d099-4e61-bdfa-e616a9cf66b7",
      "number": 18,
      "content": "Chúa thật đúng, vì tôi đã nổi loạn chống lại mạng lịnh Ngài;",
      "order": 0,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.323Z",
      "updatedAt": "2024-07-15T03:47:21.323Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "01087ca4-5767-4150-9b53-823e1beb59ae",
      "number": 18,
      "content": "Hỡi muôn dân, hãy lắng nghe,",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.323Z",
      "updatedAt": "2024-07-15T03:47:21.323Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "fc11a1e8-e460-4f75-8696-fc38ca318e13",
      "number": 18,
      "content": "Và hãy nhìn xem những sầu thảm tôi đang phải chịu;",
      "order": 2,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.323Z",
      "updatedAt": "2024-07-15T03:47:21.323Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "8e3d2e42-b957-408d-8668-0523aac69386",
      "number": 18,
      "content": "Các thiếu nữ và các thanh niên của tôi đã bị bắt đem lưu đày.",
      "order": 3,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.323Z",
      "updatedAt": "2024-07-15T03:47:21.323Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "94bf137c-d123-454f-a783-0cdb29ac9819",
      "number": 19,
      "content": "Tôi gọi những người yêu của tôi, nhưng họ đã gạt tôi;",
      "order": 0,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.325Z",
      "updatedAt": "2024-07-15T03:47:21.325Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "2e31e6e0-c64b-482f-9865-2be46307483b",
      "number": 19,
      "content": "Các tư tế và các trưởng lão của tôi chết thảm trong thành,",
      "order": 1,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.325Z",
      "updatedAt": "2024-07-15T03:47:21.325Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "d2e538e2-cfa3-4833-b1b0-00e76d2ed35c",
      "number": 19,
      "content": "Khi họ cố tìm chút gì để ăn đỡ đói, hầu phục hồi sinh lực của họ.",
      "order": 2,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.325Z",
      "updatedAt": "2024-07-15T03:47:21.325Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "fbed133e-ee99-4a5a-aa3b-9c49d5782d59",
      "number": 20,
      "content": "Lạy Chúa, xin Ngài xem, con tuyệt vọng biết bao!",
      "order": 0,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.328Z",
      "updatedAt": "2024-07-15T03:47:21.328Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "4f895641-3efa-4898-bc12-f7993c12f3ae",
      "number": 20,
      "content": "Bụng con phập phồng lo sợ,",
      "order": 1,
      "parNumber": 95,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.328Z",
      "updatedAt": "2024-07-15T03:47:21.328Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "9bcb09b0-833f-4e13-a6d2-96bd3a269f94",
      "number": 20,
      "content": "Tim con hồi hộp âu lo.",
      "order": 2,
      "parNumber": 96,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.328Z",
      "updatedAt": "2024-07-15T03:47:21.328Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "5b84599c-7e83-4778-aacb-b55987430391",
      "number": 20,
      "content": "Vì con đã quyết tâm chống lại Ngài,",
      "order": 3,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.328Z",
      "updatedAt": "2024-07-15T03:47:21.328Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "d1ae7a94-3a4b-4916-b0a2-cf0d04fe17da",
      "number": 20,
      "content": "Nên ngoài đường gươm đao đua nhau lấy mạng,",
      "order": 4,
      "parNumber": 98,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.328Z",
      "updatedAt": "2024-07-15T03:47:21.328Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "5305da08-8226-458a-be5e-8a5a0502372f",
      "number": 20,
      "content": "Còn trong nhà thần chết hoành hành.",
      "order": 5,
      "parNumber": 99,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.328Z",
      "updatedAt": "2024-07-15T03:47:21.328Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "26921e51-c3ed-464f-a441-d312af42b9da",
      "number": 21,
      "content": "Người ta nghe tiếng con rên rỉ, nhưng chẳng ai an ủi con.",
      "order": 0,
      "parNumber": 100,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.331Z",
      "updatedAt": "2024-07-15T03:47:21.331Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "d09ca91f-9d4a-4e74-88f9-e4e5c248baf2",
      "number": 21,
      "content": "Tất cả kẻ thù của con nghe tin con gặp nạn,",
      "order": 1,
      "parNumber": 101,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.331Z",
      "updatedAt": "2024-07-15T03:47:21.331Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "a1c49229-e970-4bd3-a749-0ae097dda1cb",
      "number": 21,
      "content": "Chúng vui mừng vì Ngài làm việc ấy cho con.",
      "order": 2,
      "parNumber": 102,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.331Z",
      "updatedAt": "2024-07-15T03:47:21.331Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "2d166f7d-62ce-420d-962e-b9c2e64e9d17",
      "number": 21,
      "content": "Xin Ngài sớm thực hiện ngày đoán phạt mà Ngài đã phán,",
      "order": 3,
      "parNumber": 103,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.331Z",
      "updatedAt": "2024-07-15T03:47:21.331Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "f377cbc7-a5bc-46ae-b1a5-15e821a1f0a5",
      "number": 21,
      "content": "Ðể chúng biết mùi đau khổ như con.",
      "order": 4,
      "parNumber": 104,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.331Z",
      "updatedAt": "2024-07-15T03:47:21.331Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "baae7b85-c440-47da-84a4-d9ed576b7fa6",
      "number": 22,
      "content": "Nguyện các việc ác của chúng bị phơi bày trước thánh nhan Ngài;",
      "order": 0,
      "parNumber": 105,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.334Z",
      "updatedAt": "2024-07-15T03:47:21.334Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "be24e1ce-9bcf-44ab-b604-1b327ad72c14",
      "number": 22,
      "content": "Xin đối xử với chúng như Ngài đã đối xử với con,",
      "order": 1,
      "parNumber": 106,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.334Z",
      "updatedAt": "2024-07-15T03:47:21.334Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "0129655b-9c67-4cee-9e88-964e1461ca39",
      "number": 22,
      "content": "Vì mọi vi phạm của con,",
      "order": 2,
      "parNumber": 107,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.334Z",
      "updatedAt": "2024-07-15T03:47:21.334Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "c3e44c2a-6ecd-4164-bb26-2a2193bbca8a",
      "number": 22,
      "content": "Vì con than thở quá nhiều,",
      "order": 3,
      "parNumber": 108,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.334Z",
      "updatedAt": "2024-07-15T03:47:21.334Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    },
    {
      "id": "647b102e-d9fc-4659-89be-8a6c48a5ddb1",
      "number": 22,
      "content": "Và lòng con giờ đã nao sờn.",
      "order": 4,
      "parNumber": 109,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:21.334Z",
      "updatedAt": "2024-07-15T03:47:21.334Z",
      "chapterId": "e6ae04c6-acaa-4472-9c7d-ee64cbc39221"
    }
  ],
