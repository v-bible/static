    {
      "id": "1c81aa5a-7a7d-4321-bea3-bd30cb1f3173",
      "content": "Hình Phạt của Si-ôn",
      "order": 0,
      "createdAt": "2024-09-24T02:31:52.020Z",
      "updatedAt": "2024-09-24T02:31:52.020Z",
      "verseId": "d003348f-0f4b-43ea-a38f-2b9014117057",
      "chapterId": "65e00834-ef5a-4f6c-ac2c-0ac52a5a547f"
    }
  ],
