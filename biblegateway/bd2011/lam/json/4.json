    {
      "id": "c8cca57f-6893-4fa7-8d9d-4c4cf92a670c",
      "content": "Hình Phạt của Si-ôn",
      "order": 0,
      "createdAt": "2024-09-09T05:02:46.722Z",
      "updatedAt": "2024-09-09T05:02:46.722Z",
      "verseId": "8f7a9a97-3a71-4202-9503-f61b52346cf4",
      "chapterId": "f319c9d7-d11d-4c9e-b012-6c93e73f649e"
    }
  ],
