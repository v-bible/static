  "headings": [
    {
      "id": "96317b53-aaeb-4d57-9e05-f87c1d013baf",
      "content": "Hình Phạt của Si-ôn",
      "order": 0,
      "createdAt": "2024-07-15T03:48:16.670Z",
      "updatedAt": "2024-07-15T03:48:16.670Z",
      "verseId": "3e3bd61b-89be-4b73-ba7c-be03a6f16d18",
      "chapterId": "545e01a3-3e96-45a7-ad7b-acfc7bd98308"
    }
  ],
