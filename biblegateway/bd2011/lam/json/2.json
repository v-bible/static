{
  "chapter": {
    "id": "912d7078-5409-45b5-bded-7bf8790ebca7",
    "number": 2,
    "ref": "https://www.biblegateway.com/passage/?search=Ai%20Ca%202&version=BD2011",
    "createdAt": "2024-09-09T04:13:05.498Z",
    "updatedAt": "2024-09-09T04:13:05.498Z",
    "bookId": "86840a15-95ec-46a2-8174-0b2bb385d174"
  },
  "verses": [
    {
      "id": "08536607-68ed-4981-826f-b0006a9b8fab",
      "number": 1,
      "content": "Sao Chúa nỡ bao phủ Ái Nữ của Si-ôn bằng cơn giận?",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.941Z",
      "updatedAt": "2024-09-09T05:02:34.941Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "ff331944-6baf-4413-9bab-5f08e6d40d38",
      "number": 1,
      "content": "Từ trời cao Ngài ném xuống đất vẻ đẹp của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.953Z",
      "updatedAt": "2024-09-09T05:02:34.953Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "6eb518a7-b776-40be-ae3d-6467d0cba3e2",
      "number": 1,
      "content": "Và không nhớ đến bệ chân Ngài trong ngày Ngài nổi giận.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.955Z",
      "updatedAt": "2024-09-09T05:02:34.955Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "c696d494-7c6b-4bf3-a53a-de0f04fc1605",
      "number": 2,
      "content": "Chúa tiêu diệt không chút xót thương mọi nơi cư ngụ của Gia-cốp;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.957Z",
      "updatedAt": "2024-09-09T05:02:34.957Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "020e1f65-5956-409e-a48e-d0da65a30ad5",
      "number": 2,
      "content": "Trong cơn giận Ngài phá tan các thành lũy Ái Nữ của Giu-đa;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.959Z",
      "updatedAt": "2024-09-09T05:02:34.959Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "455fe21e-4531-498e-9c50-173a9feaf082",
      "number": 2,
      "content": "Ngài khiến chúng đổ xuống đất;",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.961Z",
      "updatedAt": "2024-09-09T05:02:34.961Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "81aab716-6bbd-45ac-ad2f-e66e3e35ea09",
      "number": 2,
      "content": "Ðể làm nhục vương quốc và những người lãnh đạo của nó.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.963Z",
      "updatedAt": "2024-09-09T05:02:34.963Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "ccaf1a71-7668-4964-8167-0e3286d650eb",
      "number": 3,
      "content": "Trong cơn cực kỳ phẫn nộ Ngài chặt đứt tất cả các sừng của I-sơ-ra-ên;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.965Z",
      "updatedAt": "2024-09-09T05:02:34.965Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "8bf150fd-d3a4-41f6-8335-eed224d48bdb",
      "number": 3,
      "content": "Ngài rút lại cánh tay phải Ngài trước mặt quân thù của họ;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.967Z",
      "updatedAt": "2024-09-09T05:02:34.967Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "dbbefdf6-6dfd-4138-bfa2-5b7d4b967805",
      "number": 3,
      "content": "Ngài đốt rụi trong xứ của Gia-cốp,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.968Z",
      "updatedAt": "2024-09-09T05:02:34.968Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "3085d3e9-fea5-4086-8e23-d113f1d5974e",
      "number": 3,
      "content": "Như ngọn lửa hừng thiêu rụi mọi thứ xung quanh.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.970Z",
      "updatedAt": "2024-09-09T05:02:34.970Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "a84db2c1-da04-419c-b7b8-b93ea8ccb303",
      "number": 4,
      "content": "Ngài đã giương cung nhắm bắn như một xạ thủ của quân thù,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.972Z",
      "updatedAt": "2024-09-09T05:02:34.972Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "06a15d6d-5fc3-4a48-89ae-e1e409ed31c1",
      "number": 4,
      "content": "Tay phải Ngài sẵn sàng buông tên;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.975Z",
      "updatedAt": "2024-09-09T05:02:34.975Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "503eac18-47ce-4580-a998-2617e3d4e47b",
      "number": 4,
      "content": "Như kẻ thù, Ngài giết sạch kẻ nào mắt Ngài muốn giết,",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.977Z",
      "updatedAt": "2024-09-09T05:02:34.977Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "96ab4fc9-9471-47d6-8fcd-6554f0b2d9e3",
      "number": 5,
      "content": "Chúa đã trở thành như kẻ thù;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.978Z",
      "updatedAt": "2024-09-09T05:02:34.978Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "d1d50664-a5b9-446d-b7bc-9f45bf2cbf40",
      "number": 5,
      "content": "Ngài nuốt chửng I-sơ-ra-ên;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.981Z",
      "updatedAt": "2024-09-09T05:02:34.981Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "85a983b5-582a-4d45-b646-25546ddc7f0d",
      "number": 5,
      "content": "Ngài nuốt trộng tất cả cung điện nó,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.985Z",
      "updatedAt": "2024-09-09T05:02:34.985Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "7f5212c3-a894-4336-9ae4-ac58235e3c07",
      "number": 5,
      "content": "Ngài biến các thành trì nó ra hoang phế,",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.989Z",
      "updatedAt": "2024-09-09T05:02:34.989Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "4e6c879c-8ac9-4128-b7aa-2f0e3b60ec13",
      "number": 5,
      "content": "Ngài làm gia tăng nỗi sầu thảm và bi ai cho Ái Nữ của Giu-đa.",
      "order": 4,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.994Z",
      "updatedAt": "2024-09-09T05:02:34.994Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "3d9e6902-b864-48e6-9d49-cc6f2f6c9086",
      "number": 6,
      "content": "Ngài phá tan Lều Tạm của Ngài như người ta phá bỏ một mảnh vườn;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:34.998Z",
      "updatedAt": "2024-09-09T05:02:34.998Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "c2da7eeb-5378-427d-ac25-3432eae19869",
      "number": 6,
      "content": "Ngài phá hủy nơi nhóm lại của Ngài.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.002Z",
      "updatedAt": "2024-09-09T05:02:35.002Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "d0bc19d7-54b4-4597-b648-71cdb14976ec",
      "number": 6,
      "content": "Chúa làm cho những ngày đại lễ và những ngày Sa-bát ở Si-ôn không được nhớ đến nữa;",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.006Z",
      "updatedAt": "2024-09-09T05:02:35.006Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "75d5fc73-594b-4d58-a685-9f91b7bf6fcf",
      "number": 6,
      "content": "Trong cơn cực kỳ phẫn nộ Ngài ghê tởm cả vua lẫn tư tế ở đó.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.010Z",
      "updatedAt": "2024-09-09T05:02:35.010Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "c7444ee7-5368-4d52-a68b-ee055dd37c29",
      "number": 7,
      "content": "Chúa loại bỏ bàn thờ của Ngài;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.014Z",
      "updatedAt": "2024-09-09T05:02:35.014Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "2eee5f7a-c061-4d59-842e-f1ed324d4db7",
      "number": 7,
      "content": "Ngài ghê tởm nơi thánh của Ngài;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.019Z",
      "updatedAt": "2024-09-09T05:02:35.019Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "916969b3-66a2-4d82-80de-04cff7d03b22",
      "number": 7,
      "content": "Ngài trao vào tay quân thù các bức tường cung điện của nàng;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.023Z",
      "updatedAt": "2024-09-09T05:02:35.023Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "97204115-5d40-492d-b27c-ccbbce3cd993",
      "number": 7,
      "content": "Chúng làm ồn trong nhà Chúa như tiếng ồn trong ngày đại lễ.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.027Z",
      "updatedAt": "2024-09-09T05:02:35.027Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "accb1994-1f2a-46ca-967b-ba13c3ac8e8c",
      "number": 8,
      "content": "Chúa quyết định phá đổ tường thành Ái Nữ của Si-ôn;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.031Z",
      "updatedAt": "2024-09-09T05:02:35.031Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "fb98f225-167c-4da0-a267-ed7fef0c60ee",
      "number": 8,
      "content": "Ngài giăng dây ra đo và không rút tay Ngài lại khi hủy diệt;",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.036Z",
      "updatedAt": "2024-09-09T05:02:35.036Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "0d5915f9-0557-434e-b14b-28988d2ca85e",
      "number": 8,
      "content": "Ngài khiến các chiến lũy và tường thành đều buồn thảm;",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.041Z",
      "updatedAt": "2024-09-09T05:02:35.041Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "b004fa51-c559-498a-93ba-37e638cf3ee7",
      "number": 8,
      "content": "Chúng cùng nhau suy tàn.",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.045Z",
      "updatedAt": "2024-09-09T05:02:35.045Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "257df05c-0d2c-435c-8c2f-e4fe9da0d86d",
      "number": 9,
      "content": "Các cổng thành của nàng lún sâu dưới đất;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.050Z",
      "updatedAt": "2024-09-09T05:02:35.050Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "90dc5c36-9523-412b-9fdb-a5e9e18f71ac",
      "number": 9,
      "content": "Ngài phá tan và bẻ gãy các thanh ngang;",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.054Z",
      "updatedAt": "2024-09-09T05:02:35.054Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "256cf9ad-85e1-49f9-80a4-5485ee37560f",
      "number": 9,
      "content": "Vua và các quan nàng bị lưu đày nơi xứ lạ,",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.059Z",
      "updatedAt": "2024-09-09T05:02:35.059Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "9bab2b27-d514-49c6-a50d-09a2481dad71",
      "number": 9,
      "content": "Luật Pháp không còn nữa,",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.063Z",
      "updatedAt": "2024-09-09T05:02:35.063Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "7aa1e58f-e707-464d-b37f-f44a6d5e5ae7",
      "number": 9,
      "content": "Các tiên tri nàng chẳng thấy khải tượng nào từ Chúa cả.",
      "order": 4,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.067Z",
      "updatedAt": "2024-09-09T05:02:35.067Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "5ecd2bca-a78e-4696-8fac-22be811210e8",
      "number": 10,
      "content": "Các trưởng lão Ái Nữ của Si-ôn ngồi im lìm dưới đất,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.072Z",
      "updatedAt": "2024-09-09T05:02:35.072Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "1126a785-31fe-48fc-a1cb-63b508e0c57b",
      "number": 10,
      "content": "Họ hốt bụi rải trên đầu,",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.077Z",
      "updatedAt": "2024-09-09T05:02:35.077Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "cdec1b0e-a3f3-4b5a-afef-f74f9a0986b3",
      "number": 10,
      "content": "Họ mặc vải thô;",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.082Z",
      "updatedAt": "2024-09-09T05:02:35.082Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "753a5d89-3c56-453f-8752-6b1730560a7e",
      "number": 10,
      "content": "Các trinh nữ của Giê-ru-sa-lem cúi gục đầu xuống đất.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.086Z",
      "updatedAt": "2024-09-09T05:02:35.086Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "bfc8e05c-a915-4cf9-9eff-b45fd77248b7",
      "number": 11,
      "content": "Mắt tôi hốc hác vì khóc khô dòng lệ;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.091Z",
      "updatedAt": "2024-09-09T05:02:35.091Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "7f768738-0c75-4914-b6fe-49c956bb0a6b",
      "number": 11,
      "content": "Lòng dạ tôi tan nát từng cơn,",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.095Z",
      "updatedAt": "2024-09-09T05:02:35.095Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "83d2e473-95e2-485a-ad82-4ada2e94d70b",
      "number": 11,
      "content": "Mật tôi đổ ra trên đất vì sự hủy diệt Ái Nữ của dân tôi,",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.100Z",
      "updatedAt": "2024-09-09T05:02:35.100Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "c6bdcbec-b7be-4859-b5ff-a869d58cb8de",
      "number": 11,
      "content": "Bởi vì các trẻ thơ và các ấu nhi ngất xỉu trên các đường phố.",
      "order": 3,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.104Z",
      "updatedAt": "2024-09-09T05:02:35.104Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "a78a2064-5ecb-4b62-a5d6-3310cbe8ad5c",
      "number": 12,
      "content": "Chúng khóc xin với mẹ chúng,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.108Z",
      "updatedAt": "2024-09-09T05:02:35.108Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "5f3941c4-9219-4dcc-9172-ed78361ef890",
      "number": 12,
      "content": "“Cơm bánh và thức uống đâu?”",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.112Z",
      "updatedAt": "2024-09-09T05:02:35.112Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "b4e6aaa1-7c2e-4526-aa95-0214750948b3",
      "number": 12,
      "content": "Chúng ngất xỉu như người bị tử thương trên các đường phố,",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.117Z",
      "updatedAt": "2024-09-09T05:02:35.117Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "c1e80bd3-e7a2-4af5-b570-53dbaa280d2a",
      "number": 12,
      "content": "Rồi chúng từ từ tắt thở trong vòng tay bên lòng mẹ.",
      "order": 3,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.122Z",
      "updatedAt": "2024-09-09T05:02:35.122Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "48c40139-da3c-4ddb-89d5-268d56f6313e",
      "number": 13,
      "content": "Hỡi Ái Nữ của Giê-ru-sa-lem, ta có thể nói gì cho ngươi nữa?",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.126Z",
      "updatedAt": "2024-09-09T05:02:35.126Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "132e46ee-2dc2-4d76-a2ff-ce13c18ddc4d",
      "number": 13,
      "content": "Ta có thể lấy gì so sánh với ngươi?",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.131Z",
      "updatedAt": "2024-09-09T05:02:35.131Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "8dc2edec-6582-442b-9e94-ea8140ba2632",
      "number": 13,
      "content": "Hỡi nàng trinh nữ, Ái Nữ của Si-ôn, ta có thể so sánh ngươi với ai,",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.136Z",
      "updatedAt": "2024-09-09T05:02:35.136Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "c0f36e2e-c96d-4350-b277-39087f62325f",
      "number": 13,
      "content": "Ðể ta có thể an ủi ngươi?",
      "order": 3,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.140Z",
      "updatedAt": "2024-09-09T05:02:35.140Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "a808bd1d-9db1-43cf-9301-0ab5bfdecd1b",
      "number": 13,
      "content": "Vì nỗi đau thương của ngươi lớn như biển cả,",
      "order": 4,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.145Z",
      "updatedAt": "2024-09-09T05:02:35.145Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "22ed58fd-c5a2-412f-a186-15f81258deec",
      "number": 13,
      "content": "Ai có thể chữa cho ngươi được lành?",
      "order": 5,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.149Z",
      "updatedAt": "2024-09-09T05:02:35.149Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "745ce1fa-dbde-4b45-bf70-fae8498afaaf",
      "number": 14,
      "content": "Các tiên tri của ngươi đã thấy toàn những điều giả dối và tầm phào;",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.153Z",
      "updatedAt": "2024-09-09T05:02:35.153Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "739006a5-3d5f-436f-9216-6ba4e2a357ef",
      "number": 14,
      "content": "Chúng không vạch rõ các tội của ngươi ra,",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.157Z",
      "updatedAt": "2024-09-09T05:02:35.157Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "3609604c-62d4-4896-ac4f-62acb608b54c",
      "number": 14,
      "content": "Hầu mong đảo ngược vận mạng của ngươi,",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.162Z",
      "updatedAt": "2024-09-09T05:02:35.162Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "18e968ad-c237-48f0-a406-825a10609a87",
      "number": 14,
      "content": "Nhưng chúng chỉ thấy và bảo ngươi toàn những sứ điệp giả dối và ảo giác hư không.",
      "order": 3,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.170Z",
      "updatedAt": "2024-09-09T05:02:35.170Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "75bb6c34-0029-4ff8-a243-50571ccfafaf",
      "number": 15,
      "content": "Tất cả những kẻ đi ngang qua thấy ngươi đều đập tay tiếc rẻ,",
      "order": 0,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.174Z",
      "updatedAt": "2024-09-09T05:02:35.174Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "59b4355a-753e-4400-a3ca-74c14f88b183",
      "number": 15,
      "content": "Chúng xầm xì chế nhạo và lắc đầu khinh bỉ Ái Nữ của Giê-ru-sa-lem,",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.179Z",
      "updatedAt": "2024-09-09T05:02:35.179Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "1cba9b21-fdab-42f7-ab2b-693db1c51716",
      "number": 15,
      "content": "“Phải chăng đây vốn là thành mang danh tuyệt mỹ,",
      "order": 2,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.183Z",
      "updatedAt": "2024-09-09T05:02:35.183Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "3fbe5273-f676-4793-aa23-71efb04c99be",
      "number": 15,
      "content": "Và niềm vui của cả thế gian?”",
      "order": 3,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.187Z",
      "updatedAt": "2024-09-09T05:02:35.187Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "403a8391-0cd1-4ccc-91c7-1dce6e37b995",
      "number": 16,
      "content": "Tất cả kẻ thù của ngươi mở miệng chê bai ngươi;",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.191Z",
      "updatedAt": "2024-09-09T05:02:35.191Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "540d8c45-00e7-414f-a489-a7db6c3adb27",
      "number": 16,
      "content": "Chúng xầm xì chế nhạo, nghiến răng, và đắc ý nói vang,",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.195Z",
      "updatedAt": "2024-09-09T05:02:35.195Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "e91711f9-7b50-4f50-b657-5d34ed87e787",
      "number": 16,
      "content": "“Phen này chúng ta nuốt trộng nó được rồi!",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.199Z",
      "updatedAt": "2024-09-09T05:02:35.199Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "9d38da0f-196b-47e7-b3bc-6161be68855a",
      "number": 16,
      "content": "Chúng ta đã đợi ngày này lâu lắm rồi;",
      "order": 3,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.203Z",
      "updatedAt": "2024-09-09T05:02:35.203Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "befe202c-43f5-44ae-87d8-9eecba279bc8",
      "number": 16,
      "content": "Cuối cùng chúng ta đã thấy được tận mắt!”",
      "order": 4,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.208Z",
      "updatedAt": "2024-09-09T05:02:35.208Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "ced96c81-bcf9-4953-a13a-6410104139ec",
      "number": 17,
      "content": "Chúa thực hiện những gì Ngài đã quyết định;",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.212Z",
      "updatedAt": "2024-09-09T05:02:35.212Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "3d8f7880-0c76-4294-8c4e-e6a2e21bb40d",
      "number": 17,
      "content": "Ngài làm ứng nghiệm lời Ngài đã cảnh cáo từ lâu.",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.216Z",
      "updatedAt": "2024-09-09T05:02:35.216Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "037be0f8-e775-42d2-b9ed-a9702e974157",
      "number": 17,
      "content": "Ngài lật đổ chẳng mảy may thương xót;",
      "order": 2,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.220Z",
      "updatedAt": "2024-09-09T05:02:35.220Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "0020c604-7662-4387-8be7-7fcf7f215dc5",
      "number": 17,
      "content": "Ngài làm cho kẻ thù của ngươi vui mừng vì ngươi,",
      "order": 3,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.225Z",
      "updatedAt": "2024-09-09T05:02:35.225Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "aa145709-a793-4a3b-82ec-41b9c5a4a88b",
      "number": 17,
      "content": "Và làm cho sừng của kẻ thù ngươi thêm cứng mạnh.",
      "order": 4,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.229Z",
      "updatedAt": "2024-09-09T05:02:35.229Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "c44c2fd1-6f72-4023-aa95-ea45a1d3a3fa",
      "number": 18,
      "content": "Hỡi tường thành Ái Nữ của Si-ôn, hãy dốc đổ lòng ngươi kêu cầu với Chúa;",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.233Z",
      "updatedAt": "2024-09-09T05:02:35.233Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "164111b7-ea6f-4e12-a506-96baf85d8d2e",
      "number": 18,
      "content": "Hãy đổ lệ ngươi như suối suốt ngày đêm.",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.237Z",
      "updatedAt": "2024-09-09T05:02:35.237Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "ea07d204-2acb-4b37-93e8-c5426366a43e",
      "number": 18,
      "content": "Chớ để cho ngươi được nghỉ ngơi thơi thả.",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.242Z",
      "updatedAt": "2024-09-09T05:02:35.242Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "1cad6d96-2376-4ba1-b763-7fcf4e2bcadd",
      "number": 18,
      "content": "Ðừng để cho mắt ngươi ngưng đổ lệ phút nào.",
      "order": 3,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.246Z",
      "updatedAt": "2024-09-09T05:02:35.246Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "11cd9101-cc02-4852-9799-ccff2ea638f1",
      "number": 19,
      "content": "Hãy thức dậy giữa đêm khuya để kêu cầu thống thiết,",
      "order": 0,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.251Z",
      "updatedAt": "2024-09-09T05:02:35.251Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "844f62ce-a501-4933-ae64-ff2b2c3d9754",
      "number": 19,
      "content": "Mỗi đầu canh hãy cất tiếng van nài;",
      "order": 1,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.255Z",
      "updatedAt": "2024-09-09T05:02:35.255Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "85023ab0-e4f3-4d29-a50e-181738a64672",
      "number": 19,
      "content": "Hãy dốc đổ lòng ngươi trước mặt Chúa như nước tuôn lai láng,",
      "order": 2,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.260Z",
      "updatedAt": "2024-09-09T05:02:35.260Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "f2627d2d-d054-45f8-9267-9000e65614b4",
      "number": 19,
      "content": "Hãy đưa đôi tay ngươi lên hướng về Ngài cầu nguyện cho sinh mạng đàn con dại của ngươi,",
      "order": 3,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.264Z",
      "updatedAt": "2024-09-09T05:02:35.264Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "9fe86954-d7de-4935-b100-80f5a9701128",
      "number": 19,
      "content": "Vì chúng đói khát đến ngất xỉu nơi mỗi đầu đường góc phố.",
      "order": 4,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.268Z",
      "updatedAt": "2024-09-09T05:02:35.268Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "1c0d2075-770f-4559-a01e-d4924b452429",
      "number": 20,
      "content": "Lạy Chúa, xin Ngài đoái xem và nhìn thấy!",
      "order": 0,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.274Z",
      "updatedAt": "2024-09-09T05:02:35.274Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "72d740cc-ad1e-4755-8c74-0f024c4e1bf0",
      "number": 20,
      "content": "Ðể phạt ai mà Ngài làm những điều này?",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.278Z",
      "updatedAt": "2024-09-09T05:02:35.278Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "f0d2d5af-0144-4b11-ae30-2901ff799a96",
      "number": 20,
      "content": "Chẳng lẽ những người mẹ phải ăn thịt con mình để sống,",
      "order": 2,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.282Z",
      "updatedAt": "2024-09-09T05:02:35.282Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "b431ce4a-5e02-4e01-a5ee-c912a085edff",
      "number": 20,
      "content": "Những đứa con mà họ mang nặng đẻ đau?",
      "order": 3,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.287Z",
      "updatedAt": "2024-09-09T05:02:35.287Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "e3607e21-c092-4d2e-991f-32c4668bf42f",
      "number": 20,
      "content": "Chẳng lẽ tư tế và tiên tri phải bị giết trong nơi thánh của Chúa sao?",
      "order": 4,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.292Z",
      "updatedAt": "2024-09-09T05:02:35.292Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "4f3bcd72-3cda-458b-998c-b7f4c4452c1d",
      "number": 21,
      "content": "Xác trai trẻ và người già nằm la liệt trên đất khắp các đường phố;",
      "order": 0,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.296Z",
      "updatedAt": "2024-09-09T05:02:35.296Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "34014b54-f975-4437-b462-23f27568b8e0",
      "number": 21,
      "content": "Các thiếu nữ và các thanh niên bị ngã chết vì gươm.",
      "order": 1,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.300Z",
      "updatedAt": "2024-09-09T05:02:35.300Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "6a6840a0-1a37-4b82-b70c-26e69f9b28f1",
      "number": 21,
      "content": "Ngài giết họ trong ngày Ngài nổi giận,",
      "order": 2,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.304Z",
      "updatedAt": "2024-09-09T05:02:35.304Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "cb2af0e2-6524-4a77-82c5-a19064b4e8db",
      "number": 21,
      "content": "Ngài tàn sát họ không một chút xót thương.",
      "order": 3,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.308Z",
      "updatedAt": "2024-09-09T05:02:35.308Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "f6673325-aaa5-4603-ba5d-b8c3ca2da7ee",
      "number": 22,
      "content": "Ngài mời quân thù của con từ khắp nơi kéo đến,",
      "order": 0,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.313Z",
      "updatedAt": "2024-09-09T05:02:35.313Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "3864b8a1-dd68-49f4-8308-ff250347ed42",
      "number": 22,
      "content": "Như thể chúng được mời đến dự một ngày đại lễ tưng bừng,",
      "order": 1,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.317Z",
      "updatedAt": "2024-09-09T05:02:35.317Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "54188a98-3a3f-4053-8ae6-33be4fce4c09",
      "number": 22,
      "content": "Ðể trong ngày thịnh nộ của Chúa không người nào trốn thoát hoặc sống sót,",
      "order": 2,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.321Z",
      "updatedAt": "2024-09-09T05:02:35.321Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    },
    {
      "id": "d2dde88d-7c6e-429c-9a04-50a8bc95a1b1",
      "number": 22,
      "content": "Quân thù giết cả những con thơ mà con bồng ẵm và dưỡng nuôi.",
      "order": 3,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-09T05:02:35.325Z",
      "updatedAt": "2024-09-09T05:02:35.325Z",
      "chapterId": "912d7078-5409-45b5-bded-7bf8790ebca7"
    }
  ],
