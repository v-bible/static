{
  "chapter": {
    "id": "8d03152d-c7ff-45c3-8d39-6077fb7158b1",
    "number": 2,
    "ref": "https://www.biblegateway.com/passage/?search=Ai%20Ca%202&version=BD2011",
    "createdAt": "2024-07-12T04:03:52.209Z",
    "updatedAt": "2024-07-24T15:07:52.757Z",
    "bookId": "6b281373-14aa-49aa-98f5-676fdd51956b"
  },
  "verses": [
    {
      "id": "0bc0d5d3-3214-4be2-aaf6-76847cd56fc2",
      "number": 1,
      "content": "Sao Chúa nỡ bao phủ Ái Nữ của Si-ôn bằng cơn giận?",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.066Z",
      "updatedAt": "2024-07-15T03:47:43.066Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "ef15338f-0d9f-4920-933b-43a8627b2b5e",
      "number": 1,
      "content": "Từ trời cao Ngài ném xuống đất vẻ đẹp của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.066Z",
      "updatedAt": "2024-07-15T03:47:43.066Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "f14b7749-fcb0-4796-a41d-15da4ba8923d",
      "number": 1,
      "content": "Và không nhớ đến bệ chân Ngài trong ngày Ngài nổi giận.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.066Z",
      "updatedAt": "2024-07-15T03:47:43.066Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "fff3120d-be02-4c0f-8409-ed9a31cfd6f3",
      "number": 2,
      "content": "Chúa tiêu diệt không chút xót thương mọi nơi cư ngụ của Gia-cốp;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.076Z",
      "updatedAt": "2024-07-15T03:47:43.076Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "6849378d-25ce-4abc-a30b-a2ff6cb976fb",
      "number": 2,
      "content": "Trong cơn giận Ngài phá tan các thành lũy Ái Nữ của Giu-đa;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.076Z",
      "updatedAt": "2024-07-15T03:47:43.076Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "1697755f-b9a6-46b5-bf12-1cf37068eeb7",
      "number": 2,
      "content": "Ngài khiến chúng đổ xuống đất;",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.076Z",
      "updatedAt": "2024-07-15T03:47:43.076Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "2cc9b93f-fa63-468c-8aed-103328db6dcb",
      "number": 2,
      "content": "Ðể làm nhục vương quốc và những người lãnh đạo của nó.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.076Z",
      "updatedAt": "2024-07-15T03:47:43.076Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "f782331c-ad88-4cb5-a620-53e84c1626cf",
      "number": 3,
      "content": "Trong cơn cực kỳ phẫn nộ Ngài chặt đứt tất cả các sừng của I-sơ-ra-ên;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.079Z",
      "updatedAt": "2024-07-15T03:47:43.079Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "af9233af-d496-4cd7-b084-d59ca10fb952",
      "number": 3,
      "content": "Ngài rút lại cánh tay phải Ngài trước mặt quân thù của họ;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.079Z",
      "updatedAt": "2024-07-15T03:47:43.079Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "3e8127b0-c3e3-42da-bfc7-aa321fb1cdb2",
      "number": 3,
      "content": "Ngài đốt rụi trong xứ của Gia-cốp,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.079Z",
      "updatedAt": "2024-07-15T03:47:43.079Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "2d849c7d-5639-4a0a-b249-8e73520c2a5d",
      "number": 3,
      "content": "Như ngọn lửa hừng thiêu rụi mọi thứ xung quanh.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.079Z",
      "updatedAt": "2024-07-15T03:47:43.079Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "b639c831-4639-4423-bdf5-b36baff693de",
      "number": 4,
      "content": "Ngài đã giương cung nhắm bắn như một xạ thủ của quân thù,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.081Z",
      "updatedAt": "2024-07-15T03:47:43.081Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "cd4df389-db65-4822-8bb4-bb9b4021c6f9",
      "number": 4,
      "content": "Tay phải Ngài sẵn sàng buông tên;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.081Z",
      "updatedAt": "2024-07-15T03:47:43.081Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "c0a89481-a216-4cfd-9c6e-9a1c9966f6bd",
      "number": 4,
      "content": "Như kẻ thù, Ngài giết sạch kẻ nào mắt Ngài muốn giết,",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.081Z",
      "updatedAt": "2024-07-15T03:47:43.081Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "e33e8cac-e0c9-467e-a88b-a4403f34a0a7",
      "number": 5,
      "content": "Chúa đã trở thành như kẻ thù;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.084Z",
      "updatedAt": "2024-07-15T03:47:43.084Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "1dd28cc5-37b9-40c4-b28b-7a61678f3b97",
      "number": 5,
      "content": "Ngài nuốt chửng I-sơ-ra-ên;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.084Z",
      "updatedAt": "2024-07-15T03:47:43.084Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "951d4f59-e443-4813-b5fa-fc3de192a33e",
      "number": 5,
      "content": "Ngài nuốt trộng tất cả cung điện nó,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.084Z",
      "updatedAt": "2024-07-15T03:47:43.084Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "fb41421f-1c13-45b4-9b2b-5f1bccb15891",
      "number": 5,
      "content": "Ngài biến các thành trì nó ra hoang phế,",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.084Z",
      "updatedAt": "2024-07-15T03:47:43.084Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "2bf34b6d-6bf1-4c74-9e97-8b010f8f6789",
      "number": 5,
      "content": "Ngài làm gia tăng nỗi sầu thảm và bi ai cho Ái Nữ của Giu-đa.",
      "order": 4,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.084Z",
      "updatedAt": "2024-07-15T03:47:43.084Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "631f45b7-dda1-4ddf-abab-ca5eed2ca4a6",
      "number": 6,
      "content": "Ngài phá tan Lều Tạm của Ngài như người ta phá bỏ một mảnh vườn;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.086Z",
      "updatedAt": "2024-07-15T03:47:43.086Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "0b96ec19-de15-4e99-bba3-8b51169e99a0",
      "number": 6,
      "content": "Ngài phá hủy nơi nhóm lại của Ngài.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.086Z",
      "updatedAt": "2024-07-15T03:47:43.086Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "293fcf82-3623-4139-87bf-5b644d6f2d90",
      "number": 6,
      "content": "Chúa làm cho những ngày đại lễ và những ngày Sa-bát ở Si-ôn không được nhớ đến nữa;",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.086Z",
      "updatedAt": "2024-07-15T03:47:43.086Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "0fe1b0d0-4c73-43d5-b3ee-b6998d86fa0c",
      "number": 6,
      "content": "Trong cơn cực kỳ phẫn nộ Ngài ghê tởm cả vua lẫn tư tế ở đó.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.086Z",
      "updatedAt": "2024-07-15T03:47:43.086Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "72c7f884-dd13-4c3d-95e9-8f93dfd2181f",
      "number": 7,
      "content": "Chúa loại bỏ bàn thờ của Ngài;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.089Z",
      "updatedAt": "2024-07-15T03:47:43.089Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "7eaedd7f-8b96-4ec9-b85c-2086ba826099",
      "number": 7,
      "content": "Ngài ghê tởm nơi thánh của Ngài;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.089Z",
      "updatedAt": "2024-07-15T03:47:43.089Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "c233e812-4af6-463a-8773-b585a3238edb",
      "number": 7,
      "content": "Ngài trao vào tay quân thù các bức tường cung điện của nàng;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.089Z",
      "updatedAt": "2024-07-15T03:47:43.089Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "48a7e30b-de74-4333-9c95-4f2061c5d459",
      "number": 7,
      "content": "Chúng làm ồn trong nhà Chúa như tiếng ồn trong ngày đại lễ.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.089Z",
      "updatedAt": "2024-07-15T03:47:43.089Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "f67ac111-5e16-4b7d-b943-04dbb70de779",
      "number": 8,
      "content": "Chúa quyết định phá đổ tường thành Ái Nữ của Si-ôn;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.092Z",
      "updatedAt": "2024-07-15T03:47:43.092Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "8c2424ba-c61c-4d49-9707-4173ec08f2bd",
      "number": 8,
      "content": "Ngài giăng dây ra đo và không rút tay Ngài lại khi hủy diệt;",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.092Z",
      "updatedAt": "2024-07-15T03:47:43.092Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "3b1cb899-a125-41c1-a32c-438a2c3406c2",
      "number": 8,
      "content": "Ngài khiến các chiến lũy và tường thành đều buồn thảm;",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.092Z",
      "updatedAt": "2024-07-15T03:47:43.092Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "7701ba0b-13af-47c8-8473-21c541ffad71",
      "number": 8,
      "content": "Chúng cùng nhau suy tàn.",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.092Z",
      "updatedAt": "2024-07-15T03:47:43.092Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "4a8442cd-4b17-4464-9f90-bd5f0bc231d2",
      "number": 9,
      "content": "Các cổng thành của nàng lún sâu dưới đất;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.094Z",
      "updatedAt": "2024-07-15T03:47:43.094Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "1e4727a2-f3d7-4751-ad02-2094d10e9130",
      "number": 9,
      "content": "Ngài phá tan và bẻ gãy các thanh ngang;",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.094Z",
      "updatedAt": "2024-07-15T03:47:43.094Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "95f66ddf-67d7-4476-b784-52681fd86ccf",
      "number": 9,
      "content": "Vua và các quan nàng bị lưu đày nơi xứ lạ,",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.094Z",
      "updatedAt": "2024-07-15T03:47:43.094Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "52fda496-6e07-49d1-9138-f118a1e5c0dc",
      "number": 9,
      "content": "Luật Pháp không còn nữa,",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.094Z",
      "updatedAt": "2024-07-15T03:47:43.094Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "aa97d016-ec12-4887-b250-312de63671fa",
      "number": 9,
      "content": "Các tiên tri nàng chẳng thấy khải tượng nào từ Chúa cả.",
      "order": 4,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.094Z",
      "updatedAt": "2024-07-15T03:47:43.094Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "8bf4be16-b7b6-486e-a6d2-ead8b5581797",
      "number": 10,
      "content": "Các trưởng lão Ái Nữ của Si-ôn ngồi im lìm dưới đất,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.097Z",
      "updatedAt": "2024-07-15T03:47:43.097Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "5b76cd72-8e72-484e-9c3c-def0176cb375",
      "number": 10,
      "content": "Họ hốt bụi rải trên đầu,",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.097Z",
      "updatedAt": "2024-07-15T03:47:43.097Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "a9347d80-3fc9-4f33-b761-b2e5b43755fd",
      "number": 10,
      "content": "Họ mặc vải thô;",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.097Z",
      "updatedAt": "2024-07-15T03:47:43.097Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "9df7921e-5c88-47b7-ab80-95f3ac704c69",
      "number": 10,
      "content": "Các trinh nữ của Giê-ru-sa-lem cúi gục đầu xuống đất.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.097Z",
      "updatedAt": "2024-07-15T03:47:43.097Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "417f9f2d-f1ae-41e7-891a-9fed93d91493",
      "number": 11,
      "content": "Mắt tôi hốc hác vì khóc khô dòng lệ;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.103Z",
      "updatedAt": "2024-07-15T03:47:43.103Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "c063d440-844c-498d-9952-c7a41dea55e2",
      "number": 11,
      "content": "Lòng dạ tôi tan nát từng cơn,",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.103Z",
      "updatedAt": "2024-07-15T03:47:43.103Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "85295a0e-8e6e-41ef-be8a-5075b8fde4c4",
      "number": 11,
      "content": "Mật tôi đổ ra trên đất vì sự hủy diệt Ái Nữ của dân tôi,",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.103Z",
      "updatedAt": "2024-07-15T03:47:43.103Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "e80bb1aa-d60a-498c-8609-fbfd8e80d359",
      "number": 11,
      "content": "Bởi vì các trẻ thơ và các ấu nhi ngất xỉu trên các đường phố.",
      "order": 3,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.103Z",
      "updatedAt": "2024-07-15T03:47:43.103Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "dfd32a72-c6ba-4b9b-b551-fa55019d9ade",
      "number": 12,
      "content": "Chúng khóc xin với mẹ chúng,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.105Z",
      "updatedAt": "2024-07-15T03:47:43.105Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "03dbfa2b-e68c-423e-9fd2-e8886c72a167",
      "number": 12,
      "content": "“Cơm bánh và thức uống đâu?”",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.105Z",
      "updatedAt": "2024-07-15T03:47:43.105Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "10e9cff5-16f4-4d33-9b1d-10e0b9284894",
      "number": 12,
      "content": "Chúng ngất xỉu như người bị tử thương trên các đường phố,",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.105Z",
      "updatedAt": "2024-07-15T03:47:43.105Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "8aae64cb-71d9-4442-a62c-4fef056d3d10",
      "number": 12,
      "content": "Rồi chúng từ từ tắt thở trong vòng tay bên lòng mẹ.",
      "order": 3,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.105Z",
      "updatedAt": "2024-07-15T03:47:43.105Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "71cff37e-d8f3-4116-81a1-bf8eda4b8775",
      "number": 13,
      "content": "Hỡi Ái Nữ của Giê-ru-sa-lem, ta có thể nói gì cho ngươi nữa?",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.112Z",
      "updatedAt": "2024-07-15T03:47:43.112Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "1041a3b3-f32f-4fd4-8804-5046f1af7398",
      "number": 13,
      "content": "Ta có thể lấy gì so sánh với ngươi?",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.112Z",
      "updatedAt": "2024-07-15T03:47:43.112Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "ffbcff3d-ddff-4ac1-b703-441eed2d0fe2",
      "number": 13,
      "content": "Hỡi nàng trinh nữ, Ái Nữ của Si-ôn, ta có thể so sánh ngươi với ai,",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.112Z",
      "updatedAt": "2024-07-15T03:47:43.112Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "d4005c92-de3d-47f6-96a3-73dac06f155f",
      "number": 13,
      "content": "Ðể ta có thể an ủi ngươi?",
      "order": 3,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.112Z",
      "updatedAt": "2024-07-15T03:47:43.112Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "ab9efd3f-3cba-48ff-b298-2f3f7d709163",
      "number": 13,
      "content": "Vì nỗi đau thương của ngươi lớn như biển cả,",
      "order": 4,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.112Z",
      "updatedAt": "2024-07-15T03:47:43.112Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "1b893fcf-64b5-4ad9-9b7f-87dd2018fbaa",
      "number": 13,
      "content": "Ai có thể chữa cho ngươi được lành?",
      "order": 5,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.112Z",
      "updatedAt": "2024-07-15T03:47:43.112Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "7f994207-eb9a-4e35-aed4-2ce7df4fb308",
      "number": 14,
      "content": "Các tiên tri của ngươi đã thấy toàn những điều giả dối và tầm phào;",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.121Z",
      "updatedAt": "2024-07-15T03:47:43.121Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "75fa4cff-b55e-4033-bdb1-8f4f346c8946",
      "number": 14,
      "content": "Chúng không vạch rõ các tội của ngươi ra,",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.121Z",
      "updatedAt": "2024-07-15T03:47:43.121Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "a4798032-0bda-4b1c-9acf-a1691f3e7767",
      "number": 14,
      "content": "Hầu mong đảo ngược vận mạng của ngươi,",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.121Z",
      "updatedAt": "2024-07-15T03:47:43.121Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "6be2e764-f6cb-4e82-a61a-3fab5c9985c0",
      "number": 14,
      "content": "Nhưng chúng chỉ thấy và bảo ngươi toàn những sứ điệp giả dối và ảo giác hư không.",
      "order": 3,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.121Z",
      "updatedAt": "2024-07-15T03:47:43.121Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "310794f5-8337-4126-bb54-b38abd15297d",
      "number": 15,
      "content": "Tất cả những kẻ đi ngang qua thấy ngươi đều đập tay tiếc rẻ,",
      "order": 0,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.221Z",
      "updatedAt": "2024-07-15T03:47:43.221Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "cdc24478-a32b-4f5c-81fd-0d5d4a2e2e33",
      "number": 15,
      "content": "Chúng xầm xì chế nhạo và lắc đầu khinh bỉ Ái Nữ của Giê-ru-sa-lem,",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.221Z",
      "updatedAt": "2024-07-15T03:47:43.221Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "1bc512c4-e69a-4a23-9b45-f094ac67b817",
      "number": 15,
      "content": "“Phải chăng đây vốn là thành mang danh tuyệt mỹ,",
      "order": 2,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.221Z",
      "updatedAt": "2024-07-15T03:47:43.221Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "4e4cbe42-b3df-482b-9ae6-beb4ec5bf573",
      "number": 15,
      "content": "Và niềm vui của cả thế gian?”",
      "order": 3,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.221Z",
      "updatedAt": "2024-07-15T03:47:43.221Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "ec149f65-831c-4cfb-ac13-7d272ea861fd",
      "number": 16,
      "content": "Tất cả kẻ thù của ngươi mở miệng chê bai ngươi;",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.234Z",
      "updatedAt": "2024-07-15T03:47:43.234Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "5a6ffbaa-2111-427a-ad14-ab0ef1504a77",
      "number": 16,
      "content": "Chúng xầm xì chế nhạo, nghiến răng, và đắc ý nói vang,",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.234Z",
      "updatedAt": "2024-07-15T03:47:43.234Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "25d13377-72d2-4124-824b-555313592752",
      "number": 16,
      "content": "“Phen này chúng ta nuốt trộng nó được rồi!",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.234Z",
      "updatedAt": "2024-07-15T03:47:43.234Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "5eab3c59-1cb6-4324-80cc-0f1d42aeadbb",
      "number": 16,
      "content": "Chúng ta đã đợi ngày này lâu lắm rồi;",
      "order": 3,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.234Z",
      "updatedAt": "2024-07-15T03:47:43.234Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "f7de6dc5-226e-49eb-ad44-9266ea87650f",
      "number": 16,
      "content": "Cuối cùng chúng ta đã thấy được tận mắt!”",
      "order": 4,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.234Z",
      "updatedAt": "2024-07-15T03:47:43.234Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "823ab435-9c3c-449d-9d7d-61dba8969027",
      "number": 17,
      "content": "Chúa thực hiện những gì Ngài đã quyết định;",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.237Z",
      "updatedAt": "2024-07-15T03:47:43.237Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "9740ce5c-957c-4976-b8d5-d0b6e98e6f3c",
      "number": 17,
      "content": "Ngài làm ứng nghiệm lời Ngài đã cảnh cáo từ lâu.",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.237Z",
      "updatedAt": "2024-07-15T03:47:43.237Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "73a85c31-72e0-40b5-89f2-b5950894fb38",
      "number": 17,
      "content": "Ngài lật đổ chẳng mảy may thương xót;",
      "order": 2,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.237Z",
      "updatedAt": "2024-07-15T03:47:43.237Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "bb1faec3-68af-47bc-b8e0-6fd9e38b9253",
      "number": 17,
      "content": "Ngài làm cho kẻ thù của ngươi vui mừng vì ngươi,",
      "order": 3,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.237Z",
      "updatedAt": "2024-07-15T03:47:43.237Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "fff1cbf8-5d00-4f08-add4-714068fcce6a",
      "number": 17,
      "content": "Và làm cho sừng của kẻ thù ngươi thêm cứng mạnh.",
      "order": 4,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.237Z",
      "updatedAt": "2024-07-15T03:47:43.237Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "987b0033-f4b9-47fb-81e5-3b0e2c33ccc7",
      "number": 18,
      "content": "Hỡi tường thành Ái Nữ của Si-ôn, hãy dốc đổ lòng ngươi kêu cầu với Chúa;",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.239Z",
      "updatedAt": "2024-07-15T03:47:43.239Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "33ed4988-5a9f-422d-ba44-e8efa1e3978d",
      "number": 18,
      "content": "Hãy đổ lệ ngươi như suối suốt ngày đêm.",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.239Z",
      "updatedAt": "2024-07-15T03:47:43.239Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "fd0ff118-450a-49a0-a970-1399feeb5a6c",
      "number": 18,
      "content": "Chớ để cho ngươi được nghỉ ngơi thơi thả.",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.239Z",
      "updatedAt": "2024-07-15T03:47:43.239Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "452f58fe-9180-4110-840c-73f86e5d72df",
      "number": 18,
      "content": "Ðừng để cho mắt ngươi ngưng đổ lệ phút nào.",
      "order": 3,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.239Z",
      "updatedAt": "2024-07-15T03:47:43.239Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "69a18f9a-5af1-4c9e-8c21-fd9e3e9659bd",
      "number": 19,
      "content": "Hãy thức dậy giữa đêm khuya để kêu cầu thống thiết,",
      "order": 0,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.242Z",
      "updatedAt": "2024-07-15T03:47:43.242Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "d4fbfc51-cf18-4a72-ada8-96fd4dd44a89",
      "number": 19,
      "content": "Mỗi đầu canh hãy cất tiếng van nài;",
      "order": 1,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.242Z",
      "updatedAt": "2024-07-15T03:47:43.242Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "538d1fe7-eebc-4534-a646-b1dd6544c5e9",
      "number": 19,
      "content": "Hãy dốc đổ lòng ngươi trước mặt Chúa như nước tuôn lai láng,",
      "order": 2,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.242Z",
      "updatedAt": "2024-07-15T03:47:43.242Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "8e3e72cb-6fde-4f5b-986a-4436a46deffc",
      "number": 19,
      "content": "Hãy đưa đôi tay ngươi lên hướng về Ngài cầu nguyện cho sinh mạng đàn con dại của ngươi,",
      "order": 3,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.242Z",
      "updatedAt": "2024-07-15T03:47:43.242Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "3074be44-e010-4de4-a8bd-ee462555fe62",
      "number": 19,
      "content": "Vì chúng đói khát đến ngất xỉu nơi mỗi đầu đường góc phố.",
      "order": 4,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.242Z",
      "updatedAt": "2024-07-15T03:47:43.242Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "b8562e94-694b-4041-8d28-1c2af0a5524f",
      "number": 20,
      "content": "Lạy Chúa, xin Ngài đoái xem và nhìn thấy!",
      "order": 0,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.244Z",
      "updatedAt": "2024-07-15T03:47:43.244Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "585a23b4-03b2-4608-b7dd-d20a61d895d7",
      "number": 20,
      "content": "Ðể phạt ai mà Ngài làm những điều này?",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.244Z",
      "updatedAt": "2024-07-15T03:47:43.244Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "52d622d2-509b-4f3b-9eca-265dce85d59b",
      "number": 20,
      "content": "Chẳng lẽ những người mẹ phải ăn thịt con mình để sống,",
      "order": 2,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.244Z",
      "updatedAt": "2024-07-15T03:47:43.244Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "1e41a832-3997-4a18-bb1a-16a582fb21c1",
      "number": 20,
      "content": "Những đứa con mà họ mang nặng đẻ đau?",
      "order": 3,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.244Z",
      "updatedAt": "2024-07-15T03:47:43.244Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "85ef45a7-e983-47a2-aa40-21a34b349a55",
      "number": 20,
      "content": "Chẳng lẽ tư tế và tiên tri phải bị giết trong nơi thánh của Chúa sao?",
      "order": 4,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.244Z",
      "updatedAt": "2024-07-15T03:47:43.244Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "85b2fc11-8159-4915-97d8-cb7acf1e93cd",
      "number": 21,
      "content": "Xác trai trẻ và người già nằm la liệt trên đất khắp các đường phố;",
      "order": 0,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.247Z",
      "updatedAt": "2024-07-15T03:47:43.247Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "4b4c5724-23a6-48d0-9bfb-2df41c00e330",
      "number": 21,
      "content": "Các thiếu nữ và các thanh niên bị ngã chết vì gươm.",
      "order": 1,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.247Z",
      "updatedAt": "2024-07-15T03:47:43.247Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "28941293-c79a-4a25-a30c-a1714aefed51",
      "number": 21,
      "content": "Ngài giết họ trong ngày Ngài nổi giận,",
      "order": 2,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.247Z",
      "updatedAt": "2024-07-15T03:47:43.247Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "e3876b91-c9f7-4c89-9672-c1a108ad5513",
      "number": 21,
      "content": "Ngài tàn sát họ không một chút xót thương.",
      "order": 3,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.247Z",
      "updatedAt": "2024-07-15T03:47:43.247Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "38486584-0f11-446f-9cc6-9b153c7ddaf2",
      "number": 22,
      "content": "Ngài mời quân thù của con từ khắp nơi kéo đến,",
      "order": 0,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.249Z",
      "updatedAt": "2024-07-15T03:47:43.249Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "7f7404ae-df10-4b8b-8679-d4a955f5ad02",
      "number": 22,
      "content": "Như thể chúng được mời đến dự một ngày đại lễ tưng bừng,",
      "order": 1,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.249Z",
      "updatedAt": "2024-07-15T03:47:43.249Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "4265d764-0e38-4a17-93b2-83ab2a46e73b",
      "number": 22,
      "content": "Ðể trong ngày thịnh nộ của Chúa không người nào trốn thoát hoặc sống sót,",
      "order": 2,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.249Z",
      "updatedAt": "2024-07-15T03:47:43.249Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    },
    {
      "id": "896fa3a3-4ff7-45d1-97f0-84940583acae",
      "number": 22,
      "content": "Quân thù giết cả những con thơ mà con bồng ẵm và dưỡng nuôi.",
      "order": 3,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:47:43.249Z",
      "updatedAt": "2024-07-15T03:47:43.249Z",
      "chapterId": "8d03152d-c7ff-45c3-8d39-6077fb7158b1"
    }
  ],
