    }
  ],
  "headings": [
    {
      "id": "5218d615-35f6-4926-b9e7-53bb53c54b59",
      "content": "Than Khóc vì Tội của I-sơ-ra-ên",
      "order": 0,
      "createdAt": "2024-07-15T04:05:55.265Z",
      "updatedAt": "2024-07-15T04:05:55.265Z",
      "verseId": "72443968-702c-48e8-945b-4f6d4481570e",
      "chapterId": "a14dcb38-36d9-4e54-a5f9-6ffc941172cd"
    },
    {
      "id": "1012396c-ec79-4f5a-ac06-d98ac5b98a6e",
      "content": "Ngày của Chúa Thật Ðen Tối",
      "order": 0,
      "createdAt": "2024-07-15T04:05:55.265Z",
      "updatedAt": "2024-07-15T04:05:55.265Z",
      "verseId": "cacc9233-7940-45af-8af7-7fcf79be4eb4",
      "chapterId": "a14dcb38-36d9-4e54-a5f9-6ffc941172cd"
    },
    {
      "id": "5d4939ed-0e3e-4e03-8b30-67486cef5ece",
      "content": "Chúa Không Hài Lòng về Sự Thờ Phượng Chỉ Bằng Hình Thức",
      "order": 0,
      "createdAt": "2024-07-15T04:05:55.265Z",
      "updatedAt": "2024-07-15T04:05:55.265Z",
      "verseId": "4e743acd-1d1d-4feb-a84a-064dce2df11e",
      "chapterId": "a14dcb38-36d9-4e54-a5f9-6ffc941172cd"
    }
  ],
  "references": []
}