{
  "chapter": {
    "id": "5dc3455d-f195-4533-a02d-58e633251761",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Na-hum%201&version=BD2011",
    "createdAt": "2024-07-12T04:03:54.421Z",
    "updatedAt": "2024-07-24T15:07:54.651Z",
    "bookId": "688a1140-d3d1-44c6-8931-ce78310324fe"
  },
  "verses": [
    {
      "id": "ea26c826-7042-4bd5-a57a-7983b9ca3d49",
      "number": 1,
      "content": "Lời tiên tri về Ni-ni-ve.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.600Z",
      "updatedAt": "2024-07-15T04:09:41.600Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "362acc60-2640-4227-926f-507c3851046b",
      "number": 1,
      "content": "Sách ghi lại khải tượng của Na-hum người Ên-cốc.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.600Z",
      "updatedAt": "2024-07-15T04:09:41.600Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "63d8c20d-8668-4b8c-b040-e741601299c0",
      "number": 2,
      "content": "Chúa là Ðức Chúa Trời ghen tương và báo trả,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.602Z",
      "updatedAt": "2024-07-15T04:09:41.653Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "a618846f-276c-417a-a455-2fc788f2b340",
      "number": 2,
      "content": "Chúa nổi giận và báo trả;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.602Z",
      "updatedAt": "2024-07-15T04:09:41.662Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "8a0914c6-0b17-4097-aed9-2d5e90ef33c8",
      "number": 2,
      "content": "Chúa báo trả những kẻ chống nghịch Ngài,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.602Z",
      "updatedAt": "2024-07-15T04:09:41.671Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "91ea3bf5-8103-41a4-a0b1-290cfcb8cedd",
      "number": 2,
      "content": "Ngài dành cơn thịnh nộ cho những kẻ thù ghét Ngài.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.602Z",
      "updatedAt": "2024-07-15T04:09:41.680Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "5760946b-0be4-4db5-bd62-b4a449441bf0",
      "number": 3,
      "content": "Chúa chậm giận, nhưng quyền năng Ngài rất lớn lao;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.606Z",
      "updatedAt": "2024-07-15T04:09:41.689Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "7d46b54a-1c03-46f8-8169-704a65560d7f",
      "number": 3,
      "content": "Chúa sẽ không bỏ qua tội lỗi của kẻ ác bao giờ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.606Z",
      "updatedAt": "2024-07-15T04:09:41.698Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "e0aff851-29b3-4467-8f0b-643efdcfeac2",
      "number": 3,
      "content": "Ðường lối Ngài thể hiện qua cuồng phong bão tố,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.606Z",
      "updatedAt": "2024-07-15T04:09:41.709Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "89fb8ea3-9f6a-49b8-96ae-b37e965af81b",
      "number": 3,
      "content": "Các áng mây đen là bụi dưới chân Ngài.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.606Z",
      "updatedAt": "2024-07-15T04:09:41.719Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "1d8df8cc-39a2-49fd-a362-f3f45589e60b",
      "number": 4,
      "content": "Ngài quở trách biển, nó trở thành khô cạn,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.609Z",
      "updatedAt": "2024-07-15T04:09:41.730Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "083f5eef-ab56-4ab4-a73b-ada7eb4047f5",
      "number": 4,
      "content": "Các dòng sông không còn nước, khô khan.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.609Z",
      "updatedAt": "2024-07-15T04:09:41.738Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "db9c9805-b73e-4c94-b824-50a4b0845837",
      "number": 4,
      "content": "Ba-san và Cạt-mên khô cằn,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.609Z",
      "updatedAt": "2024-07-15T04:09:41.747Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "54f6d202-8997-495b-90ba-b92f45551a9e",
      "number": 4,
      "content": "Hoa lá ở Li-băng nhạt phai.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.609Z",
      "updatedAt": "2024-07-15T04:09:41.755Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "3e3e3a68-808b-4e86-afd7-13797f3fbda9",
      "number": 5,
      "content": "Trước mặt Ngài các núi lắc lư, các đồi tan chảy,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.612Z",
      "updatedAt": "2024-07-15T04:09:41.779Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "89b082a8-75a7-464c-8551-8b3c88e0ff09",
      "number": 5,
      "content": "Trước thánh nhan Ngài trái đất lung lay,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.612Z",
      "updatedAt": "2024-07-15T04:09:41.804Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "60debed9-7ff1-4c98-b8fe-2b43e7db4008",
      "number": 5,
      "content": "Phải, trái đất và mọi vật trên đất run lên cầm cập.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.612Z",
      "updatedAt": "2024-07-15T04:09:41.829Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "4569e1e5-8aaf-40c8-98f3-f5b7d3651cf1",
      "number": 6,
      "content": "Ai có thể đứng nổi trước cơn thịnh nộ của Ngài?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.615Z",
      "updatedAt": "2024-07-15T04:09:41.853Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "fa7f7c54-8975-4d94-af13-5d4fe1724a9f",
      "number": 6,
      "content": "Ai đủ sức chịu nổi cơn nóng giận của Ngài?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.615Z",
      "updatedAt": "2024-07-15T04:09:42.034Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "94ceb9e0-fca4-4fcd-ae21-9203603c7665",
      "number": 6,
      "content": "Cơn nóng giận của Ngài đổ xuống như lửa,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.615Z",
      "updatedAt": "2024-07-15T04:09:42.063Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "0e765179-5411-4f13-b6be-eca9b5cb2c8a",
      "number": 6,
      "content": "Trước cơn thịnh nộ của Ngài đá khối cũng phải vỡ nát tan tành.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.615Z",
      "updatedAt": "2024-07-15T04:09:42.080Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "a8165c49-7abb-4d3b-86e0-14f7c014f7ef",
      "number": 7,
      "content": "Chúa rất tốt,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.618Z",
      "updatedAt": "2024-07-15T04:09:41.618Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "04e799c9-f528-4193-9e19-a2d0dc01e3cf",
      "number": 7,
      "content": "Ngài là thành trì bảo vệ trong ngày khốn khó;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.618Z",
      "updatedAt": "2024-07-15T04:09:41.618Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "e9e1385c-d44c-4009-968b-1e3fa8e6bb03",
      "number": 7,
      "content": "Ngài che chở những ai nương náu nơi Ngài.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.618Z",
      "updatedAt": "2024-07-15T04:09:41.618Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "d6e4061f-358d-4660-b6cb-b21562fe31ea",
      "number": 8,
      "content": "Ngài quét sạch bọn chống nghịch Ngài như nước lũ cuồn cuộn đùa chúng đi khỏi chỗ của chúng;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.620Z",
      "updatedAt": "2024-07-15T04:09:41.620Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "43bedc1c-b6ce-4157-96c7-eee45280f441",
      "number": 8,
      "content": "Ngài truy đuổi quân thù của Ngài lui vào bóng tối.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.620Z",
      "updatedAt": "2024-07-15T04:09:41.620Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "4121381e-12a7-42e5-bb37-69dfbf11f8e3",
      "number": 9,
      "content": "Sao các ngươi dám lập mưu chống lại Chúa?",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.622Z",
      "updatedAt": "2024-07-15T04:09:41.622Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "fec134e5-2d5f-4d06-9c95-c4b740d891ff",
      "number": 9,
      "content": "Ngài sẽ làm các ngươi bị tuyệt diệt;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.622Z",
      "updatedAt": "2024-07-15T04:09:41.622Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "5dfb30f0-85b9-4a25-9f4a-ff883122a2ca",
      "number": 9,
      "content": "Không kẻ thù nào của Ngài cần bị đánh lại lần thứ hai.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.622Z",
      "updatedAt": "2024-07-15T04:09:41.622Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "5f384c05-af45-4791-a18f-9971329bc360",
      "number": 10,
      "content": "Chúng bị rối loạn như một lùm gai chằng chịt;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.625Z",
      "updatedAt": "2024-07-15T04:09:41.625Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "df3ffd3e-a67b-4bcd-85ec-184c44150ba4",
      "number": 10,
      "content": "Chúng lờ đờ như một lũ say rượu;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.625Z",
      "updatedAt": "2024-07-15T04:09:41.625Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "33f4759d-e950-49d8-881b-48e4ef411140",
      "number": 10,
      "content": "Chúng bị thiêu rụi như rơm khô gặp lửa.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.625Z",
      "updatedAt": "2024-07-15T04:09:41.625Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "3f0b2727-cfa9-4cf6-8d68-9433ce1adbe4",
      "number": 11,
      "content": "Từ giữa ngươi có một kẻ đi ra lập mưu chống lại Chúa;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.627Z",
      "updatedAt": "2024-07-15T04:09:41.627Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "d5da8e77-6948-4df9-97f6-4821d4f7bf56",
      "number": 11,
      "content": "Nó là kẻ bày mưu hiểm độc.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.627Z",
      "updatedAt": "2024-07-15T04:09:41.627Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "99c26ddc-171a-4d17-8a06-28601f9c5df2",
      "number": 12,
      "content": "Chúa phán thế nầy,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.630Z",
      "updatedAt": "2024-07-15T04:09:42.096Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "efc0782a-ce07-49c4-b433-4737b107e5b5",
      "number": 12,
      "content": "“Dù chúng hùng cường và đông đúc,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.630Z",
      "updatedAt": "2024-07-15T04:09:42.108Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "eb154f2f-98ed-40cb-9032-066c9bd9ba76",
      "number": 12,
      "content": "Chúng sẽ bị tiêu diệt và biến mất.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.630Z",
      "updatedAt": "2024-07-15T04:09:42.118Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "bb5a600d-7540-4324-83fd-076130505324",
      "number": 12,
      "content": "Dù Ta đã phạt ngươi phải đau đớn,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.630Z",
      "updatedAt": "2024-07-15T04:09:42.129Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "b9927a74-bc7c-4607-8dad-71d29f8fb8fb",
      "number": 12,
      "content": "Ta sẽ không làm ngươi đau đớn nữa.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.630Z",
      "updatedAt": "2024-07-15T04:09:42.141Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "9dc73ba8-cda7-4e70-9d74-36c1a0a3ae3d",
      "number": 13,
      "content": "Bây giờ Ta sẽ bẻ gãy gông cùm khỏi cổ ngươi,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.632Z",
      "updatedAt": "2024-07-15T04:09:42.150Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "8eb11688-9671-47d9-be9d-20887911c82e",
      "number": 13,
      "content": "Ta sẽ bứt đứt xiềng xích trói buộc ngươi.”",
      "order": 1,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T04:09:41.632Z",
      "updatedAt": "2024-07-15T04:09:42.163Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "e4371c6f-70d0-4f79-a36c-1682136476a4",
      "number": 14,
      "content": "Chúa đã truyền lịnh về ngươi thế nầy,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.635Z",
      "updatedAt": "2024-07-15T04:09:41.635Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "cba9ed28-8177-45a5-a2b2-f3b947060b22",
      "number": 14,
      "content": "“Ngươi sẽ không còn con cháu nối dõi để mang danh ngươi nữa;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.635Z",
      "updatedAt": "2024-07-15T04:09:41.635Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "f438550a-71c4-42a0-b560-78f2012c593c",
      "number": 14,
      "content": "Ta sẽ tận diệt dòng dõi ngươi ngay tại đền thờ ngươi thờ lạy các thần tượng,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.635Z",
      "updatedAt": "2024-07-15T04:09:41.635Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "91f1ca64-284e-40ed-9336-daa1f7cef1e1",
      "number": 14,
      "content": "Tức các thần tượng ngươi đã chạm hình và đúc tượng để thờ;",
      "order": 3,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.635Z",
      "updatedAt": "2024-07-15T04:09:41.635Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "f04784c5-9bd3-4a31-a559-b344e0fc3acb",
      "number": 14,
      "content": "Ta sẽ biến phần mộ của ngươi ra nơi bẩn thỉu, vì ngươi là kẻ bị gớm ghiếc.",
      "order": 4,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.635Z",
      "updatedAt": "2024-07-15T04:09:41.635Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "6f6046a6-a856-4f2f-8335-09053c1d905a",
      "number": 15,
      "content": "Hãy xem, này bàn chân của người mang tin mừng,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.637Z",
      "updatedAt": "2024-07-15T04:09:41.637Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "9e3aa449-39ef-40ab-a826-8788f6f1b4b3",
      "number": 15,
      "content": "Người rao báo tin hòa bình thịnh vượng trên các núi đồi!",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.637Z",
      "updatedAt": "2024-07-15T04:09:41.637Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "96a6520f-bf84-47e1-b247-c065ba427f47",
      "number": 15,
      "content": "Hỡi Giu-đa, hãy vui mừng cử hành các thánh lễ của ngươi,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.637Z",
      "updatedAt": "2024-07-15T04:09:41.637Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "d7dea706-2986-4fa7-aa91-35285936ff37",
      "number": 15,
      "content": "Hãy làm trọn những gì ngươi đã hứa nguyện,",
      "order": 3,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.637Z",
      "updatedAt": "2024-07-15T04:09:41.637Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "f653d2f5-eae2-4551-9a7c-4240e0d1f223",
      "number": 15,
      "content": "Vì quân gian ác sẽ không bao giờ xâm lăng ngươi nữa;",
      "order": 4,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.637Z",
      "updatedAt": "2024-07-15T04:09:41.637Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    },
    {
      "id": "182c10d5-42a3-4ab1-87fd-4911d9f0f67b",
      "number": 15,
      "content": "Chúng đã hoàn toàn bị tận diệt.",
      "order": 5,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T04:09:41.637Z",
      "updatedAt": "2024-07-15T04:09:41.637Z",
      "chapterId": "5dc3455d-f195-4533-a02d-58e633251761"
    }
  ],
