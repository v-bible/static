{
  "chapter": {
    "id": "b518961f-bcae-47c9-b21d-45cece07d0f8",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Na-hum%201&version=BD2011",
    "createdAt": "2024-09-24T01:00:50.644Z",
    "updatedAt": "2024-09-24T01:00:50.644Z",
    "bookId": "a36188a4-31ee-4c82-9895-ef97cf7073ce"
  },
  "verses": [
    {
      "id": "1d5cf285-1ca0-4bc1-bb0a-b423fa77a495",
      "number": 1,
      "content": "Lời tiên tri về Ni-ni-ve.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:42:26.915Z",
      "updatedAt": "2024-09-24T02:42:26.915Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "527d14d9-59c3-45d5-988e-3ac2fc3725f6",
      "number": 1,
      "content": "Sách ghi lại khải tượng của Na-hum người Ên-cốc.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:42:26.936Z",
      "updatedAt": "2024-09-24T02:42:26.936Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "3aafd6b5-fc20-4b94-8144-50c97b7bb23d",
      "number": 2,
      "content": "Chúa là Ðức Chúa Trời ghen tương và báo trả,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.941Z",
      "updatedAt": "2024-09-24T02:42:26.941Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "ac9c807e-930c-4ba1-a806-a94b3bc67dcd",
      "number": 2,
      "content": "Chúa nổi giận và báo trả;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.950Z",
      "updatedAt": "2024-09-24T02:42:26.950Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "12ad6205-d636-412c-947b-71586b84dee9",
      "number": 2,
      "content": "Chúa báo trả những kẻ chống nghịch Ngài,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.955Z",
      "updatedAt": "2024-09-24T02:42:26.955Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "667a7a25-f05b-4441-8dba-5a5155288d88",
      "number": 2,
      "content": "Ngài dành cơn thịnh nộ cho những kẻ thù ghét Ngài.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.960Z",
      "updatedAt": "2024-09-24T02:42:26.960Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "a2b3c678-1126-43c4-bbb9-6d0956578fed",
      "number": 3,
      "content": "Chúa chậm giận, nhưng quyền năng Ngài rất lớn lao;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.965Z",
      "updatedAt": "2024-09-24T02:42:26.965Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "6c101452-0cc6-4c43-81d9-6e77f58a911c",
      "number": 3,
      "content": "Chúa sẽ không bỏ qua tội lỗi của kẻ ác bao giờ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.970Z",
      "updatedAt": "2024-09-24T02:42:26.970Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "bb63d1b1-2c21-4532-8b20-12bcb46d6888",
      "number": 3,
      "content": "Ðường lối Ngài thể hiện qua cuồng phong bão tố,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.975Z",
      "updatedAt": "2024-09-24T02:42:26.975Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "7d3f7804-0863-4a70-955a-06048fefc88d",
      "number": 3,
      "content": "Các áng mây đen là bụi dưới chân Ngài.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.980Z",
      "updatedAt": "2024-09-24T02:42:26.980Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "e46167c3-73dc-4850-8bf6-1445824a1eac",
      "number": 4,
      "content": "Ngài quở trách biển, nó trở thành khô cạn,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.985Z",
      "updatedAt": "2024-09-24T02:42:26.985Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "5880a7af-e117-478c-8f40-9e59353ea1f4",
      "number": 4,
      "content": "Các dòng sông không còn nước, khô khan.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.990Z",
      "updatedAt": "2024-09-24T02:42:26.990Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "1131ea4d-ac33-4240-b4fa-ae7c09fb97f7",
      "number": 4,
      "content": "Ba-san và Cạt-mên khô cằn,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.994Z",
      "updatedAt": "2024-09-24T02:42:26.994Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "0008e14e-230c-4481-9ab7-a3d2bcff8214",
      "number": 4,
      "content": "Hoa lá ở Li-băng nhạt phai.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:26.999Z",
      "updatedAt": "2024-09-24T02:42:26.999Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "3fad76b3-57d0-4670-a64e-b1bd78de93f7",
      "number": 5,
      "content": "Trước mặt Ngài các núi lắc lư, các đồi tan chảy,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.004Z",
      "updatedAt": "2024-09-24T02:42:27.004Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "cd878736-4541-4606-96ff-04cfc228e40a",
      "number": 5,
      "content": "Trước thánh nhan Ngài trái đất lung lay,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.009Z",
      "updatedAt": "2024-09-24T02:42:27.009Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "e4047a57-0572-4ff8-b4ac-4af569ea40a9",
      "number": 5,
      "content": "Phải, trái đất và mọi vật trên đất run lên cầm cập.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.014Z",
      "updatedAt": "2024-09-24T02:42:27.014Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "abc1c09d-62c1-402b-8c33-489970d6df5e",
      "number": 6,
      "content": "Ai có thể đứng nổi trước cơn thịnh nộ của Ngài?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.020Z",
      "updatedAt": "2024-09-24T02:42:27.020Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "3e40eec5-d30b-4874-8806-2815ebc20298",
      "number": 6,
      "content": "Ai đủ sức chịu nổi cơn nóng giận của Ngài?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.025Z",
      "updatedAt": "2024-09-24T02:42:27.025Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "0c618728-79c5-4e3e-bebc-5b016649815a",
      "number": 6,
      "content": "Cơn nóng giận của Ngài đổ xuống như lửa,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.030Z",
      "updatedAt": "2024-09-24T02:42:27.030Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "f57cd3da-48ce-41a3-aa79-bea4c9d2109d",
      "number": 6,
      "content": "Trước cơn thịnh nộ của Ngài đá khối cũng phải vỡ nát tan tành.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.035Z",
      "updatedAt": "2024-09-24T02:42:27.035Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "96295bd2-5606-4b16-bf15-3172eae066a6",
      "number": 7,
      "content": "Chúa rất tốt,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.040Z",
      "updatedAt": "2024-09-24T02:42:27.040Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "dbabb771-5b99-4818-afd7-7cccbff79bdb",
      "number": 7,
      "content": "Ngài là thành trì bảo vệ trong ngày khốn khó;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.045Z",
      "updatedAt": "2024-09-24T02:42:27.045Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "0e9a0957-ee33-4f89-9d9e-d726e85bad6d",
      "number": 7,
      "content": "Ngài che chở những ai nương náu nơi Ngài.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.050Z",
      "updatedAt": "2024-09-24T02:42:27.050Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "75fb8367-cc19-4a63-9cd0-0e4a47f324a8",
      "number": 8,
      "content": "Ngài quét sạch bọn chống nghịch Ngài như nước lũ cuồn cuộn đùa chúng đi khỏi chỗ của chúng;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.054Z",
      "updatedAt": "2024-09-24T02:42:27.054Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "f5536d19-5a60-4c7f-878a-0d05c3c9c7cc",
      "number": 8,
      "content": "Ngài truy đuổi quân thù của Ngài lui vào bóng tối.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.059Z",
      "updatedAt": "2024-09-24T02:42:27.059Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "c2feff26-6b17-482d-a642-a2d5d64e329a",
      "number": 9,
      "content": "Sao các ngươi dám lập mưu chống lại Chúa?",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.064Z",
      "updatedAt": "2024-09-24T02:42:27.064Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "e6579844-a52f-42cc-bd30-577f0d5b560e",
      "number": 9,
      "content": "Ngài sẽ làm các ngươi bị tuyệt diệt;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.069Z",
      "updatedAt": "2024-09-24T02:42:27.069Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "5c46265d-cda6-4e98-ae31-55feb377264b",
      "number": 9,
      "content": "Không kẻ thù nào của Ngài cần bị đánh lại lần thứ hai.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.074Z",
      "updatedAt": "2024-09-24T02:42:27.074Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "a75cb6c0-269e-411e-b05f-d845a7e0c7ce",
      "number": 10,
      "content": "Chúng bị rối loạn như một lùm gai chằng chịt;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.079Z",
      "updatedAt": "2024-09-24T02:42:27.079Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "d0576579-f4b7-4d9f-81f0-d3661cc06384",
      "number": 10,
      "content": "Chúng lờ đờ như một lũ say rượu;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.084Z",
      "updatedAt": "2024-09-24T02:42:27.084Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "6932d7a8-946c-4d26-9e8f-7bbb4aac61f8",
      "number": 10,
      "content": "Chúng bị thiêu rụi như rơm khô gặp lửa.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.089Z",
      "updatedAt": "2024-09-24T02:42:27.089Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "e3966cfc-6cdc-4f27-b0e0-a53c48c19e33",
      "number": 11,
      "content": "Từ giữa ngươi có một kẻ đi ra lập mưu chống lại Chúa;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.094Z",
      "updatedAt": "2024-09-24T02:42:27.094Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "a782dc76-db84-4f6b-bce3-fa937db39738",
      "number": 11,
      "content": "Nó là kẻ bày mưu hiểm độc.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.104Z",
      "updatedAt": "2024-09-24T02:42:27.104Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "39c727ab-9f91-4f06-8586-d2b74d3a425a",
      "number": 12,
      "content": "Chúa phán thế nầy,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.109Z",
      "updatedAt": "2024-09-24T02:42:27.109Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "e59dcc74-37a0-4527-95ad-b6b71e895fa4",
      "number": 12,
      "content": "“Dù chúng hùng cường và đông đúc,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.118Z",
      "updatedAt": "2024-09-24T02:42:27.118Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "b1121bb0-50cf-4a86-a3d2-54d22a61c7a7",
      "number": 12,
      "content": "Chúng sẽ bị tiêu diệt và biến mất.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.125Z",
      "updatedAt": "2024-09-24T02:42:27.125Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "6c7b1f24-7da2-4c39-9fd2-f344e7f68e8b",
      "number": 12,
      "content": "Dù Ta đã phạt ngươi phải đau đớn,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.130Z",
      "updatedAt": "2024-09-24T02:42:27.130Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "19b40de3-dfe1-4db0-807b-49997f38e695",
      "number": 12,
      "content": "Ta sẽ không làm ngươi đau đớn nữa.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.137Z",
      "updatedAt": "2024-09-24T02:42:27.137Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "c3cd7dd7-5769-460c-a6a9-73bd04bb294d",
      "number": 13,
      "content": "Bây giờ Ta sẽ bẻ gãy gông cùm khỏi cổ ngươi,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.139Z",
      "updatedAt": "2024-09-24T02:42:27.139Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "280019ca-aad3-47ef-affd-81f4ea9fe151",
      "number": 13,
      "content": "Ta sẽ bứt đứt xiềng xích trói buộc ngươi.”",
      "order": 1,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.142Z",
      "updatedAt": "2024-09-24T02:42:27.142Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "f7b5a1de-6b24-4640-b306-758d0c9d1e93",
      "number": 14,
      "content": "Chúa đã truyền lịnh về ngươi thế nầy,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.144Z",
      "updatedAt": "2024-09-24T02:42:27.144Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "c5681138-ec9f-4e65-abf0-b31c274bc3f5",
      "number": 14,
      "content": "“Ngươi sẽ không còn con cháu nối dõi để mang danh ngươi nữa;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.148Z",
      "updatedAt": "2024-09-24T02:42:27.148Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "b1d4a3da-aeab-4914-b4ce-54565e7155a1",
      "number": 14,
      "content": "Ta sẽ tận diệt dòng dõi ngươi ngay tại đền thờ ngươi thờ lạy các thần tượng,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.150Z",
      "updatedAt": "2024-09-24T02:42:27.150Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "0c396e1b-4fb5-44c4-89e4-da94d225681b",
      "number": 14,
      "content": "Tức các thần tượng ngươi đã chạm hình và đúc tượng để thờ;",
      "order": 3,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.152Z",
      "updatedAt": "2024-09-24T02:42:27.152Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "c0f9970a-c4a7-4d55-a02c-acb558499566",
      "number": 14,
      "content": "Ta sẽ biến phần mộ của ngươi ra nơi bẩn thỉu, vì ngươi là kẻ bị gớm ghiếc.",
      "order": 4,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.154Z",
      "updatedAt": "2024-09-24T02:42:27.154Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "c16d5074-ed3b-4ef0-89f7-14b63734b7a3",
      "number": 15,
      "content": "Hãy xem, này bàn chân của người mang tin mừng,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.156Z",
      "updatedAt": "2024-09-24T02:42:27.156Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "acb61601-8df1-4da8-bafc-d3d0d4605207",
      "number": 15,
      "content": "Người rao báo tin hòa bình thịnh vượng trên các núi đồi!",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.158Z",
      "updatedAt": "2024-09-24T02:42:27.158Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "5ba35a3b-d201-4943-a9d7-8732b72c0fb1",
      "number": 15,
      "content": "Hỡi Giu-đa, hãy vui mừng cử hành các thánh lễ của ngươi,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.160Z",
      "updatedAt": "2024-09-24T02:42:27.160Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "f551ab84-6ae9-4a54-a028-c35170fd2fca",
      "number": 15,
      "content": "Hãy làm trọn những gì ngươi đã hứa nguyện,",
      "order": 3,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.162Z",
      "updatedAt": "2024-09-24T02:42:27.162Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "0965b38a-aefd-441e-8128-b14a0ba254fc",
      "number": 15,
      "content": "Vì quân gian ác sẽ không bao giờ xâm lăng ngươi nữa;",
      "order": 4,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.164Z",
      "updatedAt": "2024-09-24T02:42:27.164Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    },
    {
      "id": "b2cbb220-20ed-4f34-a3c1-4d86bd64f522",
      "number": 15,
      "content": "Chúng đã hoàn toàn bị tận diệt.",
      "order": 5,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:42:27.166Z",
      "updatedAt": "2024-09-24T02:42:27.166Z",
      "chapterId": "b518961f-bcae-47c9-b21d-45cece07d0f8"
    }
  ],
