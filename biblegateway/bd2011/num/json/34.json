      "chapterId": "a0afedda-45f5-4ed8-8eea-d2fc211baf51"
    },
    {
      "id": "165ae24d-3701-410f-815f-79bef0fd0c72",
      "content": "Biển Ðịa Trung Hải",
      "position": 48,
      "order": 2,
      "createdAt": "2024-09-09T04:22:37.282Z",
