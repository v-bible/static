      "chapterId": "2af488d1-7c3f-4a70-8e20-1abf5d1672b6"
    },
    {
      "id": "8f1d80c4-8463-4853-aa38-c8cbb1af044f",
      "content": "nt: của lễ của sự công chính",
      "position": 50,
      "order": 4,
      "createdAt": "2024-09-09T04:24:38.910Z",
      "updatedAt": "2024-09-09T04:24:38.910Z",
      "verseId": "ee955d00-70da-4830-8823-94152fda073f",
      "chapterId": "2af488d1-7c3f-4a70-8e20-1abf5d1672b6"
    },
