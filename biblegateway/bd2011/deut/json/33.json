      "chapterId": "2af488d1-7c3f-4a70-8e20-1abf5d1672b6"
    },
    {
      "id": "82778379-e135-4aab-8b47-b9fc5a195a08",
      "content": "ctd: Ngài ngự giữa hai vai (núi đồi) của người ấy",
      "position": 31,
      "order": 2,
      "createdAt": "2024-09-09T04:24:38.850Z",
      "updatedAt": "2024-09-09T04:24:38.850Z",
