  },
  "verses": [
    {
      "id": "b747a2f1-0aa0-4a04-b646-d27f06d9f850",
      "number": 1,
      "content": "Kìa, tốt đẹp thay và phước hạnh thay khi anh chị em sống hòa thuận với nhau!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:36.938Z",
      "updatedAt": "2024-07-31T11:52:52.896Z",
      "chapterId": "bdb80521-bd6c-4bee-b807-717b35d8c66d"
    },
    {
      "id": "09d21bce-c1f5-47ab-8f46-60629169693d",
      "number": 2,
      "content": "Ðiều ấy chẳng khác gì dầu quý báu từ trên đầu chảy xuống,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:36.940Z",
      "updatedAt": "2024-07-31T11:52:52.903Z",
      "chapterId": "bdb80521-bd6c-4bee-b807-717b35d8c66d"
    },
    {
      "id": "01e2d844-7c8f-4e7e-ba95-bf7117b74f57",
      "number": 2,
      "content": "Chảy xuống râu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:36.940Z",
      "updatedAt": "2024-07-31T11:52:52.911Z",
      "chapterId": "bdb80521-bd6c-4bee-b807-717b35d8c66d"
    },
    {
      "id": "db989841-2911-43ef-841d-87fab0f7ec82",
      "number": 2,
      "content": "Chảy xuống râu của A-rôn,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:36.940Z",
      "updatedAt": "2024-07-31T11:52:52.919Z",
      "chapterId": "bdb80521-bd6c-4bee-b807-717b35d8c66d"
    },
    {
      "id": "65ae6adf-6e4a-4912-b982-0fc5bc1bfaa8",
      "number": 2,
      "content": "Rồi tiếp tục chảy xuống vạt áo ông.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:36.940Z",
      "updatedAt": "2024-07-31T11:52:52.926Z",
      "chapterId": "bdb80521-bd6c-4bee-b807-717b35d8c66d"
    },
    {
      "id": "fc1fcebc-94e3-4b14-9f16-37c52708736e",
      "number": 3,
      "content": "Ðiều ấy giống như sương móc từ Núi Hẹt-môn sa xuống các rặng núi Si-ôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:36.943Z",
      "updatedAt": "2024-07-31T11:52:52.934Z",
      "chapterId": "bdb80521-bd6c-4bee-b807-717b35d8c66d"
    },
    {
      "id": "34977ef7-bb64-4397-aea1-3fe4f6060c8c",
      "number": 3,
      "content": "Vì tại đó Chúa ban phước,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:36.943Z",
      "updatedAt": "2024-07-31T11:52:52.945Z",
      "chapterId": "bdb80521-bd6c-4bee-b807-717b35d8c66d"
    },
    {
      "id": "1672ce15-7f35-4530-8035-a63756f632f5",
      "number": 3,
      "content": "Phước của sự sống đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:36.943Z",
      "updatedAt": "2024-07-31T11:52:52.953Z",
      "chapterId": "bdb80521-bd6c-4bee-b807-717b35d8c66d"
    }
  ],
