{
  "chapter": {
    "id": "e94c9447-8bbe-487f-aabd-20a38b225667",
    "number": 142,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20142&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.252Z",
    "updatedAt": "2024-09-24T01:00:45.252Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "1291bb9c-f2dc-426f-bde9-7245e3ea0439",
      "number": 1,
      "content": "Tôi hướng về Chúa và cất tiếng kêu cầu;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.281Z",
      "updatedAt": "2024-09-24T02:12:42.281Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "a25e8f4e-a486-45f2-a9e0-e2d3fb613d87",
      "number": 1,
      "content": "Tôi cất tiếng dâng lên Chúa lời cầu nguyện của tôi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.286Z",
      "updatedAt": "2024-09-24T02:12:42.286Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "df11f10b-7ea7-4711-85dc-12e91e0010ff",
      "number": 2,
      "content": "Tôi dốc đổ phiền muộn của lòng tôi ra trước mặt Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.288Z",
      "updatedAt": "2024-09-24T02:12:42.288Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "5f049ee0-72ce-46da-8355-fd4775e85e6d",
      "number": 2,
      "content": "Tôi trình dâng nỗi khổ của tôi lên trước thánh nhan Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.290Z",
      "updatedAt": "2024-09-24T02:12:42.290Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "93ee56a4-6443-4084-9dba-3db420b4c8a5",
      "number": 3,
      "content": "Khi tâm linh con nao sờn ở trong con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.292Z",
      "updatedAt": "2024-09-24T02:12:42.292Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "b36d17ed-d367-42c2-b319-496b1c3567c9",
      "number": 3,
      "content": "Thì chính Ngài biết rõ lối con đi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.294Z",
      "updatedAt": "2024-09-24T02:12:42.294Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "77c336c0-341c-41f2-ad0f-3bf65111686a",
      "number": 3,
      "content": "Trong con đường con tiến bước,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.296Z",
      "updatedAt": "2024-09-24T02:12:42.296Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "bfd05267-d144-44ce-98ca-4fd4209fcc79",
      "number": 3,
      "content": "Chúng đã âm thầm gài bẫy hại con.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.298Z",
      "updatedAt": "2024-09-24T02:12:42.298Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "7f3af518-a0f5-494c-8eb2-ca7dffd702d0",
      "number": 4,
      "content": "Xin Ngài nhìn bên phải con và xem;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.300Z",
      "updatedAt": "2024-09-24T02:12:42.300Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "36b19bbd-55d8-407a-ad2e-1d2a1d07515e",
      "number": 4,
      "content": "Chẳng ai ngó ngàng gì đến con cả.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.302Z",
      "updatedAt": "2024-09-24T02:12:42.302Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "c56f5997-b1f5-4c1c-889d-8b04c241c277",
      "number": 4,
      "content": "Con chẳng còn một lối thoát nào;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.305Z",
      "updatedAt": "2024-09-24T02:12:42.305Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "3b3668bf-7c66-4b46-82b3-9be92d635190",
      "number": 4,
      "content": "Thật chẳng ai quan tâm gì đến linh hồn con cả.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.307Z",
      "updatedAt": "2024-09-24T02:12:42.307Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "23451700-38e3-4aec-ae82-fa9d224ba19e",
      "number": 5,
      "content": "Chúa ôi, con kêu cầu Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.309Z",
      "updatedAt": "2024-09-24T02:12:42.309Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "23780e8d-4cea-4040-b332-c378d246b344",
      "number": 5,
      "content": "Con nói, “Ngài là nơi ẩn náu của con;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.311Z",
      "updatedAt": "2024-09-24T02:12:42.311Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "f56a41cc-15b5-473a-b491-38a61d4c7b95",
      "number": 5,
      "content": "Ngài là phần của con trong cõi nhân sinh.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.315Z",
      "updatedAt": "2024-09-24T02:12:42.315Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "275dfb8e-e385-41b7-bce4-44f65adf8471",
      "number": 6,
      "content": "Xin đoái đến tiếng kêu cầu của con,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.320Z",
      "updatedAt": "2024-09-24T02:12:42.320Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "45927ee3-c20e-4cfc-bdc3-6e3c7a202712",
      "number": 6,
      "content": "Vì con khốn khổ vô cùng;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.325Z",
      "updatedAt": "2024-09-24T02:12:42.325Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "b5670a26-40d1-4c51-831e-7efd4e0db43f",
      "number": 6,
      "content": "Xin cứu con khỏi những kẻ bách hại con,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.330Z",
      "updatedAt": "2024-09-24T02:12:42.330Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "3a852085-9410-452c-ae24-14fe145bda54",
      "number": 6,
      "content": "Vì chúng mạnh hơn con.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.335Z",
      "updatedAt": "2024-09-24T02:12:42.335Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "c2d94647-e210-4ddc-9080-9f4cae47ea11",
      "number": 7,
      "content": "Xin đem linh hồn con ra khỏi chốn ngục tù,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.341Z",
      "updatedAt": "2024-09-24T02:12:42.341Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "6761d87d-dd85-438f-b133-550c7f2f7813",
      "number": 7,
      "content": "Ðể con có thể ca ngợi danh Ngài;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.346Z",
      "updatedAt": "2024-09-24T02:12:42.346Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "4ccde772-22f1-402e-8d7c-0d7fa7fc9bfe",
      "number": 7,
      "content": "Rồi những người ngay lành sẽ tụ lại quanh con,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.351Z",
      "updatedAt": "2024-09-24T02:12:42.351Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    },
    {
      "id": "626730ea-e97e-4893-ba13-ddad9bdf5ae2",
      "number": 7,
      "content": "Vì Ngài hậu đãi con.”",
      "order": 3,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:12:42.357Z",
      "updatedAt": "2024-09-24T02:12:42.357Z",
      "chapterId": "e94c9447-8bbe-487f-aabd-20a38b225667"
    }
  ],
