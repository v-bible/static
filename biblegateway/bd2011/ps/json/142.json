{
  "chapter": {
    "id": "7eeb932e-e160-4c4e-847f-99cfeefa7750",
    "number": 142,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20142&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.624Z",
    "updatedAt": "2024-07-24T15:07:49.464Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "35c7d1c6-b2f0-4560-9837-b700dcdb4b23",
      "number": 1,
      "content": "Tôi hướng về Chúa và cất tiếng kêu cầu;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.807Z",
      "updatedAt": "2024-07-31T11:54:37.195Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "2f6c6a4d-2c9b-4a3f-aba0-7525e4393284",
      "number": 1,
      "content": "Tôi cất tiếng dâng lên Chúa lời cầu nguyện của tôi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.807Z",
      "updatedAt": "2024-07-31T11:54:37.202Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "452d2e70-6e5e-4dde-b743-951055891c75",
      "number": 2,
      "content": "Tôi dốc đổ phiền muộn của lòng tôi ra trước mặt Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.817Z",
      "updatedAt": "2024-07-31T11:54:37.209Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "661d7e6a-ce4f-4c5b-ae7e-9b92b8e1ae6d",
      "number": 2,
      "content": "Tôi trình dâng nỗi khổ của tôi lên trước thánh nhan Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.817Z",
      "updatedAt": "2024-07-31T11:54:37.216Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "70277b74-02d4-49ea-b5e3-823cdd3fb430",
      "number": 3,
      "content": "Khi tâm linh con nao sờn ở trong con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.821Z",
      "updatedAt": "2024-07-31T11:54:37.226Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "3bc9d5af-ffb6-4792-a125-e2901f9c3f22",
      "number": 3,
      "content": "Thì chính Ngài biết rõ lối con đi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.821Z",
      "updatedAt": "2024-07-31T11:54:37.234Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "3741a2ea-4382-40c6-9a27-46802f9e4726",
      "number": 3,
      "content": "Trong con đường con tiến bước,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.821Z",
      "updatedAt": "2024-07-31T11:54:37.255Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "383236e8-de37-4f5a-a613-c4e1a20f44c9",
      "number": 3,
      "content": "Chúng đã âm thầm gài bẫy hại con.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.821Z",
      "updatedAt": "2024-07-31T11:54:37.277Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "a0b9ca49-683f-436b-a1ff-21d332ff4998",
      "number": 4,
      "content": "Xin Ngài nhìn bên phải con và xem;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.825Z",
      "updatedAt": "2024-07-31T11:54:37.299Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "1f9726cb-c02b-45c4-95c8-7532dcd951a3",
      "number": 4,
      "content": "Chẳng ai ngó ngàng gì đến con cả.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.825Z",
      "updatedAt": "2024-07-31T11:54:37.321Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "6da9bd8b-98b6-407b-b693-3464082a2787",
      "number": 4,
      "content": "Con chẳng còn một lối thoát nào;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.825Z",
      "updatedAt": "2024-07-31T11:54:37.343Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "9d8a09c9-57ac-4d92-beb2-278bbcf97c1e",
      "number": 4,
      "content": "Thật chẳng ai quan tâm gì đến linh hồn con cả.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.825Z",
      "updatedAt": "2024-07-31T11:54:37.366Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "0971c127-b944-4f97-ac58-3c1876800094",
      "number": 5,
      "content": "Chúa ôi, con kêu cầu Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.829Z",
      "updatedAt": "2024-07-31T11:54:37.397Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "ad750fd4-350e-4e4b-b50c-066e06aa6d7d",
      "number": 5,
      "content": "Con nói, “Ngài là nơi ẩn náu của con;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.829Z",
      "updatedAt": "2024-07-31T11:54:37.420Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "bebf59c5-722b-4bc6-aa12-5ec0d39deab5",
      "number": 5,
      "content": "Ngài là phần của con trong cõi nhân sinh.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.829Z",
      "updatedAt": "2024-07-31T11:54:37.447Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "445b3bd6-a3d2-49df-beab-d29d8ad507c4",
      "number": 6,
      "content": "Xin đoái đến tiếng kêu cầu của con,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.833Z",
      "updatedAt": "2024-07-31T11:54:37.458Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "ab8cd2d8-d5cc-444a-898e-ab318dc3aba7",
      "number": 6,
      "content": "Vì con khốn khổ vô cùng;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.833Z",
      "updatedAt": "2024-07-31T11:54:37.468Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "bdcc4943-d503-4d2d-942a-396f36f33633",
      "number": 6,
      "content": "Xin cứu con khỏi những kẻ bách hại con,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.833Z",
      "updatedAt": "2024-07-31T11:54:37.476Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "aefc604b-26d9-4082-9506-93616f96e608",
      "number": 6,
      "content": "Vì chúng mạnh hơn con.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.833Z",
      "updatedAt": "2024-07-31T11:54:37.483Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "62c04356-da45-46db-847d-5161096dd256",
      "number": 7,
      "content": "Xin đem linh hồn con ra khỏi chốn ngục tù,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.838Z",
      "updatedAt": "2024-07-31T11:54:37.554Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "84c027e6-139f-4931-852c-d71658f5edfb",
      "number": 7,
      "content": "Ðể con có thể ca ngợi danh Ngài;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.838Z",
      "updatedAt": "2024-07-31T11:54:37.603Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "560bdde0-00e1-4859-8064-60eff53d7268",
      "number": 7,
      "content": "Rồi những người ngay lành sẽ tụ lại quanh con,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.838Z",
      "updatedAt": "2024-07-31T11:54:37.619Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    },
    {
      "id": "ccbffb86-2523-4915-9a0f-06dba31578fd",
      "number": 7,
      "content": "Vì Ngài hậu đãi con.”",
      "order": 3,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:13:34.838Z",
      "updatedAt": "2024-07-31T11:54:37.629Z",
      "chapterId": "7eeb932e-e160-4c4e-847f-99cfeefa7750"
    }
  ],
