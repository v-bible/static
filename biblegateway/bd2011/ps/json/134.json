  "verses": [
    {
      "id": "cdc345ab-3e7d-49ec-ab42-855e9af27a4f",
      "number": 1,
      "content": "Hỡi tất cả tôi tớ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:48.927Z",
      "updatedAt": "2024-07-31T11:53:05.041Z",
      "chapterId": "0ceeb09f-5919-4846-9324-fd8be59890c5"
    },
    {
      "id": "c7c1a8b6-11ac-4086-ac02-2540043d2887",
      "number": 1,
      "content": "Những người phục vụ trong nhà Chúa ban đêm,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:48.927Z",
      "updatedAt": "2024-07-31T11:53:05.056Z",
      "chapterId": "0ceeb09f-5919-4846-9324-fd8be59890c5"
    },
    {
      "id": "c95e86f1-3dc1-4740-b774-b9a4be0e043b",
      "number": 1,
      "content": "Xin hãy chúc tụng Chúa.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:48.927Z",
      "updatedAt": "2024-07-31T11:53:05.063Z",
      "chapterId": "0ceeb09f-5919-4846-9324-fd8be59890c5"
    },
    {
      "id": "d61ad2e0-4838-4f77-aab8-262c78827854",
      "number": 2,
      "content": "Xin đưa cao đôi tay quý vị lên trong nơi thánh và chúc tụng Chúa.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:48.938Z",
      "updatedAt": "2024-07-31T11:53:05.071Z",
      "chapterId": "0ceeb09f-5919-4846-9324-fd8be59890c5"
    },
    {
      "id": "2859a3bc-b808-41c7-8089-aa3c0adce49a",
      "number": 3,
      "content": "Nguyện Chúa, Ðấng dựng nên trời và đất, ban phước cho quý vị từ Si-ôn.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:48.943Z",
      "updatedAt": "2024-07-31T11:53:05.081Z",
      "chapterId": "0ceeb09f-5919-4846-9324-fd8be59890c5"
    }
  ],
