  "verses": [
    {
      "id": "2438db00-f0f2-4620-8560-44e91e451ab1",
      "number": 1,
      "content": "Hỡi tất cả tôi tớ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:06.933Z",
      "updatedAt": "2024-09-09T04:52:06.933Z",
      "chapterId": "dfd69d6b-3518-4f96-8416-8ec4d9e9adaf"
    },
    {
      "id": "0fe5b0be-091d-4247-b3c9-ea189b1afb9e",
      "number": 1,
      "content": "Những người phục vụ trong nhà Chúa ban đêm,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:06.944Z",
      "updatedAt": "2024-09-09T04:52:06.944Z",
      "chapterId": "dfd69d6b-3518-4f96-8416-8ec4d9e9adaf"
    },
    {
      "id": "2a3bbb3d-ffed-4417-9566-c9cd6674a1d9",
      "number": 1,
      "content": "Xin hãy chúc tụng Chúa.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:06.946Z",
      "updatedAt": "2024-09-09T04:52:06.946Z",
      "chapterId": "dfd69d6b-3518-4f96-8416-8ec4d9e9adaf"
    },
    {
      "id": "17e55ff4-397b-4876-9134-27a064cee8f4",
      "number": 2,
      "content": "Xin đưa cao đôi tay quý vị lên trong nơi thánh và chúc tụng Chúa.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:06.948Z",
      "updatedAt": "2024-09-09T04:52:06.948Z",
      "chapterId": "dfd69d6b-3518-4f96-8416-8ec4d9e9adaf"
    },
    {
      "id": "e8e24f47-44a2-4e3b-97f0-c587f468d668",
      "number": 3,
      "content": "Nguyện Chúa, Ðấng dựng nên trời và đất, ban phước cho quý vị từ Si-ôn.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:06.950Z",
      "updatedAt": "2024-09-09T04:52:06.950Z",
      "chapterId": "dfd69d6b-3518-4f96-8416-8ec4d9e9adaf"
    }
  ],
