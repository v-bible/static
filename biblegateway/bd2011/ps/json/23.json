{
  "chapter": {
    "id": "8e452855-6ac5-4e5b-9a17-cecdae7419f5",
    "number": 23,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2023&version=BD2011",
    "createdAt": "2024-09-09T04:12:59.608Z",
    "updatedAt": "2024-09-09T04:12:59.608Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "2489c38d-72da-49fd-b5d2-321d0f36e406",
      "number": 1,
      "content": "Chúa là Ðấng chăn giữ tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.578Z",
      "updatedAt": "2024-09-09T04:42:54.578Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "28697e52-cc46-4931-92e4-4e40ab3b335b",
      "number": 1,
      "content": "Tôi sẽ chẳng thiếu thốn gì.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.589Z",
      "updatedAt": "2024-09-09T04:42:54.589Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "b8147e1b-4a61-47c8-abe1-db48e7e18664",
      "number": 2,
      "content": "Ngài cho tôi nằm nghỉ trong đồng cỏ xanh tươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.592Z",
      "updatedAt": "2024-09-09T04:42:54.592Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "e0f79a34-8a10-4016-83df-eaf60e7eb26d",
      "number": 2,
      "content": "Ngài dẫn tôi đến mé nước bình tịnh.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.594Z",
      "updatedAt": "2024-09-09T04:42:54.594Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "98ea7c20-5863-4e31-9ccd-3c7cbe60ccea",
      "number": 3,
      "content": "Ngài phục hồi sức lực cho linh hồn tôi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.596Z",
      "updatedAt": "2024-09-09T04:42:54.596Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "9c191eec-0fc8-4295-b92a-44888b249e61",
      "number": 3,
      "content": "Ngài dẫn tôi vào các lối công chính, vì cớ danh Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.598Z",
      "updatedAt": "2024-09-09T04:42:54.598Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "83c8c8c8-528f-4dc4-adef-151bf94f100d",
      "number": 4,
      "content": "Thật vậy, dù khi con đi qua thung lũng rợp bóng tử thần,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.600Z",
      "updatedAt": "2024-09-09T04:42:54.600Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "406fb552-cee9-4172-83a1-3f676165eafa",
      "number": 4,
      "content": "Con sẽ chẳng sợ tai họa nào,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.601Z",
      "updatedAt": "2024-09-09T04:42:54.601Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "0b7fc8e2-48da-482f-ac38-acba388152b8",
      "number": 4,
      "content": "Vì Ngài ở với con;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.603Z",
      "updatedAt": "2024-09-09T04:42:54.603Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "7a987dca-58a5-45f9-9c0f-631f1cdf61bd",
      "number": 4,
      "content": "Cây trượng và cây gậy của Ngài an ủi con.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.605Z",
      "updatedAt": "2024-09-09T04:42:54.605Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "3990db97-ac3c-43d4-8dca-051643c8b8c3",
      "number": 5,
      "content": "Ngài dọn bàn cho con trước mặt những kẻ thù của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.607Z",
      "updatedAt": "2024-09-09T04:42:54.607Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "4182eb42-4b34-40c0-ac2d-9578a346399b",
      "number": 5,
      "content": "Ngài xức dầu cho đầu con;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.609Z",
      "updatedAt": "2024-09-09T04:42:54.609Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "4b1d4fac-d1b9-4813-97bd-0ee42fb31401",
      "number": 5,
      "content": "Chén con đầy tràn.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.611Z",
      "updatedAt": "2024-09-09T04:42:54.611Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "9a4e817c-bdc7-425f-afc0-0404f8d544e9",
      "number": 6,
      "content": "Quả thật, trọn đời tôi, phước hạnh và ơn thương xót sẽ theo tôi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.613Z",
      "updatedAt": "2024-09-09T04:42:54.613Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    },
    {
      "id": "4f709a38-c374-40d8-81a9-f2894eb32ae7",
      "number": 6,
      "content": "Tôi sẽ ở trong nhà của Chúa cho đến đời đời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:42:54.615Z",
      "updatedAt": "2024-09-09T04:42:54.615Z",
      "chapterId": "8e452855-6ac5-4e5b-9a17-cecdae7419f5"
    }
  ],
