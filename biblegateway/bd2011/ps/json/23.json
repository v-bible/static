{
  "chapter": {
    "id": "61d16f9f-6406-43ab-9398-82013e9aa45d",
    "number": 23,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2023&version=BD2011",
    "createdAt": "2024-07-12T04:03:45.994Z",
    "updatedAt": "2024-07-24T15:07:46.981Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "e4913a76-2968-4576-9a65-fb0c017968d1",
      "number": 1,
      "content": "Chúa là Ðấng chăn giữ tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.065Z",
      "updatedAt": "2024-07-31T11:23:36.593Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "feb1f5d6-f8b0-44e5-b16c-af99bc20d875",
      "number": 1,
      "content": "Tôi sẽ chẳng thiếu thốn gì.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.065Z",
      "updatedAt": "2024-07-31T11:23:36.602Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "49b45eac-a22a-46c1-94aa-a0e4c23ff2b8",
      "number": 2,
      "content": "Ngài cho tôi nằm nghỉ trong đồng cỏ xanh tươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.074Z",
      "updatedAt": "2024-07-31T11:23:36.621Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "c5ffc10e-3f82-4cca-9adc-ebdf69929fb0",
      "number": 2,
      "content": "Ngài dẫn tôi đến mé nước bình tịnh.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.074Z",
      "updatedAt": "2024-07-31T11:23:36.630Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "41533123-9a90-4b38-8cf5-634dded8244f",
      "number": 3,
      "content": "Ngài phục hồi sức lực cho linh hồn tôi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.077Z",
      "updatedAt": "2024-07-31T11:23:36.638Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "7f436a33-cb4a-4e30-ab07-85b58dd3ff84",
      "number": 3,
      "content": "Ngài dẫn tôi vào các lối công chính, vì cớ danh Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.077Z",
      "updatedAt": "2024-07-31T11:23:36.647Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "50d8f4d1-fd01-40bd-9f05-2037c252790b",
      "number": 4,
      "content": "Thật vậy, dù khi con đi qua thung lũng rợp bóng tử thần,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.079Z",
      "updatedAt": "2024-07-31T11:23:36.657Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "91961afc-fa01-4bb3-a5ca-95affca08b53",
      "number": 4,
      "content": "Con sẽ chẳng sợ tai họa nào,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.079Z",
      "updatedAt": "2024-07-31T11:23:36.667Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "f2884547-94c4-4403-9537-151456158d0d",
      "number": 4,
      "content": "Vì Ngài ở với con;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.079Z",
      "updatedAt": "2024-07-31T11:23:36.676Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "bafca1cf-13b3-4b57-b120-92b4bcc2c0a8",
      "number": 4,
      "content": "Cây trượng và cây gậy của Ngài an ủi con.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.079Z",
      "updatedAt": "2024-07-31T11:23:36.687Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "19aa6cd6-3c34-48bc-ad5b-c64a7bde3589",
      "number": 5,
      "content": "Ngài dọn bàn cho con trước mặt những kẻ thù của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.085Z",
      "updatedAt": "2024-07-31T11:23:36.702Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "593b0f8e-6bf7-476f-ae53-364270445fba",
      "number": 5,
      "content": "Ngài xức dầu cho đầu con;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.085Z",
      "updatedAt": "2024-07-31T11:23:36.712Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "dc0a7eea-e959-4940-b616-7200459c9f46",
      "number": 5,
      "content": "Chén con đầy tràn.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.085Z",
      "updatedAt": "2024-07-31T11:23:36.720Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "4768f567-9f8e-40ca-93ce-4648019787d7",
      "number": 6,
      "content": "Quả thật, trọn đời tôi, phước hạnh và ơn thương xót sẽ theo tôi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.090Z",
      "updatedAt": "2024-07-31T11:23:36.734Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    },
    {
      "id": "a1e36253-2156-4cfe-a308-b38246eb7cbb",
      "number": 6,
      "content": "Tôi sẽ ở trong nhà của Chúa cho đến đời đời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:48:12.090Z",
      "updatedAt": "2024-07-31T11:23:36.743Z",
      "chapterId": "61d16f9f-6406-43ab-9398-82013e9aa45d"
    }
  ],
