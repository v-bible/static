{
  "chapter": {
    "id": "5b7b294e-40f5-472a-9291-5ed4004aec3e",
    "number": 104,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20104&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.295Z",
    "updatedAt": "2024-09-09T04:13:01.295Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "db8895f6-d83f-4594-ae8b-bbbfe3c95d6b",
      "number": 1,
      "content": "Hãy ca ngợi Chúa, hỡi linh hồn ta!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.076Z",
      "updatedAt": "2024-09-09T04:49:37.076Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "6d5241e0-d37d-4c3c-92e1-7b9b7b4142ed",
      "number": 1,
      "content": "Lạy Chúa, Ðức Chúa Trời của con, Ngài thật vĩ đại thay!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.085Z",
      "updatedAt": "2024-09-09T04:49:37.085Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "55ae5e3e-0cce-4d72-839e-3e9a1be87baf",
      "number": 1,
      "content": "Ngài mặc bằng sự huy hoàng và sự uy nghi.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.088Z",
      "updatedAt": "2024-09-09T04:49:37.088Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "3f394cfb-9674-40bf-a6b3-2a2ae16c2089",
      "number": 2,
      "content": "Ngài bao phủ mình bằng ánh sáng như chiếc áo choàng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.090Z",
      "updatedAt": "2024-09-09T04:49:37.090Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "47a26de7-4433-4529-9e14-79270396ac90",
      "number": 2,
      "content": "Ngài giăng các tầng trời ra như căng một cái trại.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.091Z",
      "updatedAt": "2024-09-09T04:49:37.091Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "0a44fc65-cf1c-43eb-8014-093c442fbd97",
      "number": 3,
      "content": "Ngài đặt các đà ngang cho các thiên cung của Ngài trên nước;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.093Z",
      "updatedAt": "2024-09-09T04:49:37.093Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "03195f94-ece8-4ead-973a-773dc4037b3e",
      "number": 3,
      "content": "Ngài dùng mây làm xa giá;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.095Z",
      "updatedAt": "2024-09-09T04:49:37.095Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "9a9bef1b-7af7-49c7-ab69-7eb418292ed7",
      "number": 3,
      "content": "Ngài bước đi trên cánh gió;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.097Z",
      "updatedAt": "2024-09-09T04:49:37.097Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "3df02aff-c5ed-49cf-b3a9-eabd7b74f40c",
      "number": 4,
      "content": "Ngài dùng gió làm các sứ giả Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.099Z",
      "updatedAt": "2024-09-09T04:49:37.099Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "ee6acdee-3ee8-40f3-9716-56c64a068016",
      "number": 4,
      "content": "Lửa hừng làm bầy tôi hầu hạ Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.101Z",
      "updatedAt": "2024-09-09T04:49:37.101Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "066e31b4-1545-4dd8-9544-2f98631a5422",
      "number": 5,
      "content": "Ngài đặt trái đất trên nền của nó,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.103Z",
      "updatedAt": "2024-09-09T04:49:37.103Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "e442cc13-2722-4178-b721-0ac4579808ab",
      "number": 5,
      "content": "Ðể đến đời đời nó chẳng chuyển lay.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.105Z",
      "updatedAt": "2024-09-09T04:49:37.105Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "459198cc-791b-4ad3-8d4e-373f2794d0b0",
      "number": 6,
      "content": "Ngài bao phủ đất bằng biển sâu như phủ một chiếc áo;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.107Z",
      "updatedAt": "2024-09-09T04:49:37.107Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "0fb97e62-7159-4882-a62a-60f7e27e0485",
      "number": 6,
      "content": "Các nguồn nước lại đứng trên các ngọn núi cao.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.109Z",
      "updatedAt": "2024-09-09T04:49:37.109Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "0b757b0a-486e-424f-b466-53346b20bb72",
      "number": 7,
      "content": "Khi Ngài quở trách, chúng thất kinh chạy trốn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.111Z",
      "updatedAt": "2024-09-09T04:49:37.111Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "60cd40ea-fe64-4870-8f5e-3c89ae879867",
      "number": 7,
      "content": "Nghe tiếng sấm sét của Ngài, chúng vụt chạy cho xa.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.113Z",
      "updatedAt": "2024-09-09T04:49:37.113Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "ca41e411-c61f-4c24-b0d6-a9034108e808",
      "number": 8,
      "content": "Núi đồi trồi lên, thung lũng sụp xuống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.115Z",
      "updatedAt": "2024-09-09T04:49:37.115Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "ace54801-fc80-462c-9737-ce9ef3e94938",
      "number": 8,
      "content": "Ðúng vào nơi Ngài định cho chúng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.117Z",
      "updatedAt": "2024-09-09T04:49:37.117Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "fd880dd5-814a-44d2-ae8c-f4eb4de32134",
      "number": 9,
      "content": "Ngài định giới hạn để nước không thể vượt qua,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.119Z",
      "updatedAt": "2024-09-09T04:49:37.119Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "f6cfe1cf-d337-4b11-8dff-e0c4d5e4de9e",
      "number": 9,
      "content": "Hầu không được phủ ngập mặt đất nữa.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.121Z",
      "updatedAt": "2024-09-09T04:49:37.121Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "e9ed0e1e-2610-4661-951d-5db6bf19ba55",
      "number": 10,
      "content": "Trong các thung lũng Ngài ban các dòng nước ngọt;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.124Z",
      "updatedAt": "2024-09-09T04:49:37.124Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "10f19156-9da1-41a5-a982-fbd1d1ce5e66",
      "number": 10,
      "content": "Chúng lượn quanh tuôn chảy giữa núi đồi;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.129Z",
      "updatedAt": "2024-09-09T04:49:37.129Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "1c92b1e7-4200-4c45-8f66-b5c2c8d444ee",
      "number": 11,
      "content": "Chúng cung cấp thức uống cho mọi sinh vật trong đồng;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.135Z",
      "updatedAt": "2024-09-09T04:49:37.135Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "1054d604-e23f-4ab4-a88e-474e409466ef",
      "number": 11,
      "content": "Nhờ chúng những lừa rừng được thỏa mãn cơn khát.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.140Z",
      "updatedAt": "2024-09-09T04:49:37.140Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "5592a9a7-e34a-4e24-b393-fdce999fa819",
      "number": 12,
      "content": "Bên cạnh chúng chim trời làm tổ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.145Z",
      "updatedAt": "2024-09-09T04:49:37.145Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "f561d9fd-929e-453c-8db1-e6f44b29a4ac",
      "number": 12,
      "content": "Cất tiếng hót ca giữa các cành cây.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.151Z",
      "updatedAt": "2024-09-09T04:49:37.151Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "bdb4117c-2de5-47aa-b073-050c85bfb0ae",
      "number": 13,
      "content": "Từ thiên cung Ngài đổ nước xuống tưới các núi rừng;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.156Z",
      "updatedAt": "2024-09-09T04:49:37.156Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "12a16680-7253-4af3-8733-cd7bdff2b74a",
      "number": 13,
      "content": "Trái đất được no nê nhờ bông trái của công việc Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.161Z",
      "updatedAt": "2024-09-09T04:49:37.161Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "0c1a378c-5ed3-4a90-8fbb-4852c3efd163",
      "number": 14,
      "content": "Ngài khiến cỏ xanh mọc lên cho súc vật,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.166Z",
      "updatedAt": "2024-09-09T04:49:37.166Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "c99ff6af-c7fd-4c44-8244-79fef2204a3f",
      "number": 14,
      "content": "Và rau trái sinh ra để phục vụ loài người;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.171Z",
      "updatedAt": "2024-09-09T04:49:37.171Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "c2a9f244-0748-47a3-b249-7394161b0711",
      "number": 14,
      "content": "Từ ruộng đất người ta có thể đem về cho mình thực phẩm,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.176Z",
      "updatedAt": "2024-09-09T04:49:37.176Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "a6e1d8d2-35f6-49fe-ba2d-fbe4faa5fdec",
      "number": 15,
      "content": "Rồi chế thành rượu để uống cho lòng vui vẻ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.180Z",
      "updatedAt": "2024-09-09T04:49:37.180Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "0464eb45-623c-464f-ad87-8b40abda1613",
      "number": 15,
      "content": "Chế thành dầu để xức cho mặt mày tươi sáng,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.185Z",
      "updatedAt": "2024-09-09T04:49:37.185Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "21d7251a-9bf7-462a-9609-4bdbd19399cd",
      "number": 15,
      "content": "Và làm cơm bánh để ăn cho vững mạnh lòng người.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.191Z",
      "updatedAt": "2024-09-09T04:49:37.191Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "c18360cb-c98c-493f-a72b-8041c7adfc45",
      "number": 16,
      "content": "Những cây cối của Chúa đều được nước uống no nê,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.196Z",
      "updatedAt": "2024-09-09T04:49:37.196Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "27280210-022a-45f3-b84e-236e383c435b",
      "number": 16,
      "content": "Kể cả những cây bá hương Ngài đã trồng ở Li-băng,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.201Z",
      "updatedAt": "2024-09-09T04:49:37.201Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "a70dbd9a-66ea-4a91-bc7a-951a9e90df01",
      "number": 17,
      "content": "Cùng những cây cối khác, nơi các chim trời làm tổ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.207Z",
      "updatedAt": "2024-09-09T04:49:37.207Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "df536dc2-71ca-4a1d-80d4-043f1196ab07",
      "number": 17,
      "content": "Luôn cả những cây bách, nơi chim hạc chọn làm nhà.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.211Z",
      "updatedAt": "2024-09-09T04:49:37.211Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "d0054ae5-898c-4a52-9ff4-de8029ac2b31",
      "number": 18,
      "content": "Ngài dựng nên những núi cao cho các dê rừng làm chỗ ở;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.217Z",
      "updatedAt": "2024-09-09T04:49:37.217Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "1d18d94a-ffe0-40f4-83b1-495e797d4b58",
      "number": 18,
      "content": "Các gành đá cheo leo cho các chuột núi nương thân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.222Z",
      "updatedAt": "2024-09-09T04:49:37.222Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "e7eba71e-25ba-49aa-836e-7c09fbb1810f",
      "number": 19,
      "content": "Ngài dựng nên mặt trăng để ấn định các mùa;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.227Z",
      "updatedAt": "2024-09-09T04:49:37.227Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "1826a23e-a73b-4431-ac41-dbf5234be0d0",
      "number": 19,
      "content": "Còn mặt trời tự biết khi nào lặn xuống.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.233Z",
      "updatedAt": "2024-09-09T04:49:37.233Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "68cfbd9c-9830-4f0e-b8c8-15e3a8b59b2d",
      "number": 20,
      "content": "Ngài dựng nên bóng tối, và nó trở thành ban đêm;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.237Z",
      "updatedAt": "2024-09-09T04:49:37.237Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "d7162879-99b0-492d-bf5e-43f0b80f3be0",
      "number": 20,
      "content": "Ấy là khi các thú rừng đi ra săn mồi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.242Z",
      "updatedAt": "2024-09-09T04:49:37.242Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "9d203ae4-6635-436d-a09d-a486d49cbdd8",
      "number": 21,
      "content": "Các sư tử tơ gầm rống tìm mồi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.247Z",
      "updatedAt": "2024-09-09T04:49:37.247Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "88b324b9-e090-491a-b382-6efabfa9edf2",
      "number": 21,
      "content": "Và tìm kiếm thức ăn từ Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.253Z",
      "updatedAt": "2024-09-09T04:49:37.253Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "a3dcda62-d14b-4ffb-a7cb-5e2aebc2a2db",
      "number": 22,
      "content": "Ðến khi mặt trời mọc lên chúng lại rút về,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.258Z",
      "updatedAt": "2024-09-09T04:49:37.258Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "4752dcd8-570a-4ac2-b249-8334e292f52f",
      "number": 22,
      "content": "Và nằm xuống trong hang.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.263Z",
      "updatedAt": "2024-09-09T04:49:37.263Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "19f6ee3e-c537-404e-9e5b-a64925def74f",
      "number": 23,
      "content": "Người ta đi ra làm việc,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.267Z",
      "updatedAt": "2024-09-09T04:49:37.267Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "235805cc-a5c9-4f8a-8750-b82a30bd16c9",
      "number": 23,
      "content": "Và làm lụng cho đến hoàng hôn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.272Z",
      "updatedAt": "2024-09-09T04:49:37.272Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "f3993360-711b-46d7-b786-64ff7558be0e",
      "number": 24,
      "content": "Chúa ôi, công việc của Ngài thật nhiều thay!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.276Z",
      "updatedAt": "2024-09-09T04:49:37.276Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "b6b75260-9f7d-45d4-9cdc-954cd1c84e55",
      "number": 24,
      "content": "Ngài đã dựng nên tất cả bằng sự khôn ngoan của Ngài;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.281Z",
      "updatedAt": "2024-09-09T04:49:37.281Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "f13408b8-a653-4028-b3ab-d79a5ccddd53",
      "number": 24,
      "content": "Trái đất đầy dẫy muôn vật do Ngài dựng nên.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.286Z",
      "updatedAt": "2024-09-09T04:49:37.286Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "a920e24d-5d75-4cca-9e8c-c9c0b7cbbbb6",
      "number": 25,
      "content": "Kìa là đại dương sâu rộng mênh mông,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.291Z",
      "updatedAt": "2024-09-09T04:49:37.291Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "aa262a8f-c19c-4bae-8681-7cd91098808a",
      "number": 25,
      "content": "Trong đó chứa nhung nhúc biết bao sinh vật;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.297Z",
      "updatedAt": "2024-09-09T04:49:37.297Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "332ce943-82ee-4019-a7d4-6915498e11dd",
      "number": 25,
      "content": "Thật là vô sốsinh vật lớn và nhỏ.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.302Z",
      "updatedAt": "2024-09-09T04:49:37.302Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "ddebd178-0417-4d2b-b6b6-97655b29d6b6",
      "number": 26,
      "content": "Rồi cũng có các ghe thuyền tàu bè qua lại,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.307Z",
      "updatedAt": "2024-09-09T04:49:37.307Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "7d376631-3010-4431-9ba2-464301c4f08f",
      "number": 26,
      "content": "Và con quái vật Ngài đã dựng nên đùa chơi trong đó.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.312Z",
      "updatedAt": "2024-09-09T04:49:37.312Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "1b94af43-0ee9-485d-b313-163335243be8",
      "number": 27,
      "content": "Tất cả đều trông chờ nơi Ngài,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.322Z",
      "updatedAt": "2024-09-09T04:49:37.322Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "2c879ba4-01ef-4b81-bf65-ef3d5ef66cc2",
      "number": 27,
      "content": "Ðể Ngài ban cho chúng vật thực phải thì.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.327Z",
      "updatedAt": "2024-09-09T04:49:37.327Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "d1f02982-ea76-4e98-b5f4-007344ee8603",
      "number": 28,
      "content": "Khi Ngài cho ăn, chúng liền tụ lại;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.332Z",
      "updatedAt": "2024-09-09T04:49:37.332Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "338dd56d-c880-4c74-932d-550b5d8578fb",
      "number": 28,
      "content": "Khi Ngài sè tay ra, chúng được ăn uống no nê.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.337Z",
      "updatedAt": "2024-09-09T04:49:37.337Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "e2618452-55f6-45b0-94b0-91e7cab4e31b",
      "number": 29,
      "content": "Khi Ngài ẩn mặt, chúng hoảng kinh bối rối;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.343Z",
      "updatedAt": "2024-09-09T04:49:37.343Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "1e6c8e20-811e-4ca1-819f-5a9bff011540",
      "number": 29,
      "content": "Khi Ngài lấy hơi thở chúng đi, chúng liền chết và trở về cát bụi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.348Z",
      "updatedAt": "2024-09-09T04:49:37.348Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "63dd4eb3-5aac-43c7-91ca-710c91bab799",
      "number": 30,
      "content": "Khi Ngài ban Thần Khí Ngài ra, chúng được dựng nên,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.352Z",
      "updatedAt": "2024-09-09T04:49:37.352Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "1a574f9f-0c5d-437a-a278-654af29069ad",
      "number": 30,
      "content": "Và Ngài làm cho mặt đất mới lại.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.357Z",
      "updatedAt": "2024-09-09T04:49:37.357Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "7e62eb4f-c31c-4b22-80c4-e55b261d2e41",
      "number": 31,
      "content": "Nguyện vinh hiển của Chúa còn đến đời đời.",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.361Z",
      "updatedAt": "2024-09-09T04:49:37.361Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "dd23ee9c-8f73-41f2-bd34-edb2ee6d9872",
      "number": 31,
      "content": "Nguyện Chúa vui thích về các công việc Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.366Z",
      "updatedAt": "2024-09-09T04:49:37.366Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "c4af400d-e31d-43d2-8ffa-2a8f20fabad0",
      "number": 32,
      "content": "Ngài nhìn vào trái đất, nó run lên lẩy bẩy;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.371Z",
      "updatedAt": "2024-09-09T04:49:37.371Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "64c94b90-6b43-4f98-8181-78cb66a53624",
      "number": 32,
      "content": "Ngài đụng đến núi non, chúng bốc khói mịt mù.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.378Z",
      "updatedAt": "2024-09-09T04:49:37.378Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "2fe87986-b214-4df9-85af-289acfe0a9a6",
      "number": 33,
      "content": "Hễ tôi còn sống bao lâu, tôi sẽ ca tụng Chúa bấy lâu;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.383Z",
      "updatedAt": "2024-09-09T04:49:37.383Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "6e713edc-8555-4949-9b98-7ecd783a11ec",
      "number": 33,
      "content": "Hễ tôi còn sống chừng nào, tôi sẽ ca ngợi Ðức Chúa Trời tôi chừng nấy.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.388Z",
      "updatedAt": "2024-09-09T04:49:37.388Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "8a374c29-b982-469d-b0f4-c7b275596c58",
      "number": 34,
      "content": "Nguyện những suy nghĩ của tôi được đẹp lòng Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.393Z",
      "updatedAt": "2024-09-09T04:49:37.393Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "f2e0f554-3087-465b-b0c0-8f34a7a59e1d",
      "number": 34,
      "content": "Về phần tôi, tôi sẽ cứ vui mừng trong Chúa.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.398Z",
      "updatedAt": "2024-09-09T04:49:37.398Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "9f8ad0f1-6caf-4002-8b75-533049adb1f2",
      "number": 35,
      "content": "Nguyện những kẻ tội lỗi bị diệt trừ khỏi mặt đất;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.403Z",
      "updatedAt": "2024-09-09T04:49:37.403Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "fa39d86d-a3f5-44d2-bcdb-257dc26e66fc",
      "number": 35,
      "content": "Nguyện những kẻ ác không còn nữa.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.407Z",
      "updatedAt": "2024-09-09T04:49:37.407Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "a2d70df2-e183-49b1-87c9-fa291baaae71",
      "number": 35,
      "content": "Hãy chúc tụng Chúa, hỡi linh hồn ta,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.412Z",
      "updatedAt": "2024-09-09T04:49:37.412Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    },
    {
      "id": "5b7e5897-89cd-439c-ac87-555384ef36d0",
      "number": 35,
      "content": "Hãy ca ngợi Chúa!",
      "order": 3,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:37.417Z",
      "updatedAt": "2024-09-09T04:49:37.417Z",
      "chapterId": "5b7b294e-40f5-472a-9291-5ed4004aec3e"
    }
  ],
