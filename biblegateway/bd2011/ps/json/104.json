{
  "chapter": {
    "id": "8567b500-b44f-4402-8efd-265b09b35eda",
    "number": 104,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20104&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.778Z",
    "updatedAt": "2024-07-24T15:07:48.679Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "bd580408-ef01-4c8a-ad93-76e1cfdd8fdf",
      "number": 1,
      "content": "Hãy ca ngợi Chúa, hỡi linh hồn ta!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.782Z",
      "updatedAt": "2024-07-31T11:46:10.898Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "71e4e535-8b67-43b2-b7fa-8673b26c2be1",
      "number": 1,
      "content": "Lạy Chúa, Ðức Chúa Trời của con, Ngài thật vĩ đại thay!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.782Z",
      "updatedAt": "2024-07-31T11:46:10.925Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "bc5dc103-8762-4624-b30a-f2c410fb45f1",
      "number": 1,
      "content": "Ngài mặc bằng sự huy hoàng và sự uy nghi.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.782Z",
      "updatedAt": "2024-07-31T11:46:10.943Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "987fbcf4-f683-414e-a981-141d7021e3b6",
      "number": 2,
      "content": "Ngài bao phủ mình bằng ánh sáng như chiếc áo choàng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.792Z",
      "updatedAt": "2024-07-31T11:46:10.956Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "87a1fa9c-afa3-4905-89ba-94f11602672c",
      "number": 2,
      "content": "Ngài giăng các tầng trời ra như căng một cái trại.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.792Z",
      "updatedAt": "2024-07-31T11:46:10.967Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "f4efd5f2-fe67-429a-b1d2-0f6f14a9462f",
      "number": 3,
      "content": "Ngài đặt các đà ngang cho các thiên cung của Ngài trên nước;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.796Z",
      "updatedAt": "2024-07-31T11:46:10.977Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "1e646afe-c4fb-410f-bb64-6f3b2f0af628",
      "number": 3,
      "content": "Ngài dùng mây làm xa giá;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.796Z",
      "updatedAt": "2024-07-31T11:46:10.989Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "e9015ff8-cffd-4130-bccc-c538432e9e57",
      "number": 3,
      "content": "Ngài bước đi trên cánh gió;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.796Z",
      "updatedAt": "2024-07-31T11:46:10.998Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "54da9b61-78bd-434d-94c6-14072a55a834",
      "number": 4,
      "content": "Ngài dùng gió làm các sứ giả Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.802Z",
      "updatedAt": "2024-07-31T11:46:11.007Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "7d71afee-b072-4a3f-9c90-f638de2ac544",
      "number": 4,
      "content": "Lửa hừng làm bầy tôi hầu hạ Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.802Z",
      "updatedAt": "2024-07-31T11:46:11.015Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "5027532a-fddd-41b4-b443-1d46b2c76f30",
      "number": 5,
      "content": "Ngài đặt trái đất trên nền của nó,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.808Z",
      "updatedAt": "2024-07-31T11:46:11.027Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "cb1390b6-bc6c-4be9-ab6f-6eaa3b783d03",
      "number": 5,
      "content": "Ðể đến đời đời nó chẳng chuyển lay.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.808Z",
      "updatedAt": "2024-07-31T11:46:11.036Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "a2e83ac5-d288-4019-b3e0-b936cf30a9c0",
      "number": 6,
      "content": "Ngài bao phủ đất bằng biển sâu như phủ một chiếc áo;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.814Z",
      "updatedAt": "2024-07-31T11:46:11.047Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "4811691d-15af-43bb-8827-a0e4dea2c110",
      "number": 6,
      "content": "Các nguồn nước lại đứng trên các ngọn núi cao.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.814Z",
      "updatedAt": "2024-07-31T11:46:11.056Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "be350ac9-2dbe-4c18-8611-b2c354ca6c92",
      "number": 7,
      "content": "Khi Ngài quở trách, chúng thất kinh chạy trốn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.820Z",
      "updatedAt": "2024-07-31T11:46:11.066Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "70dd2209-5059-4f8c-ad4a-ee8be3f5105a",
      "number": 7,
      "content": "Nghe tiếng sấm sét của Ngài, chúng vụt chạy cho xa.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.820Z",
      "updatedAt": "2024-07-31T11:46:11.075Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "e6a1daa5-70c0-4d22-8991-d476be40816a",
      "number": 8,
      "content": "Núi đồi trồi lên, thung lũng sụp xuống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.826Z",
      "updatedAt": "2024-07-31T11:46:11.084Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "4c72114b-8384-4e40-a6ad-58590f6a9b51",
      "number": 8,
      "content": "Ðúng vào nơi Ngài định cho chúng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.826Z",
      "updatedAt": "2024-07-31T11:46:11.093Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "e98c8422-f051-4b7d-a8f8-565a94d2d169",
      "number": 9,
      "content": "Ngài định giới hạn để nước không thể vượt qua,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.833Z",
      "updatedAt": "2024-07-31T11:46:11.102Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "6abf063e-88f5-4c4e-9c13-2f7dc6cb273b",
      "number": 9,
      "content": "Hầu không được phủ ngập mặt đất nữa.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.833Z",
      "updatedAt": "2024-07-31T11:46:11.111Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "e17972d0-1923-4569-9024-6d3adb0b81d7",
      "number": 10,
      "content": "Trong các thung lũng Ngài ban các dòng nước ngọt;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.839Z",
      "updatedAt": "2024-07-31T11:46:11.123Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "94bcb073-81c3-4286-be1e-06e49c194573",
      "number": 10,
      "content": "Chúng lượn quanh tuôn chảy giữa núi đồi;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.839Z",
      "updatedAt": "2024-07-31T11:46:11.132Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "2df4512f-0581-4c03-a218-d26f10905950",
      "number": 11,
      "content": "Chúng cung cấp thức uống cho mọi sinh vật trong đồng;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.845Z",
      "updatedAt": "2024-07-31T11:46:11.140Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "72549b8f-d947-461b-9e3d-84d693210684",
      "number": 11,
      "content": "Nhờ chúng những lừa rừng được thỏa mãn cơn khát.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.845Z",
      "updatedAt": "2024-07-31T11:46:11.149Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "969c0189-a664-4f1a-a6d0-133fb976c12e",
      "number": 12,
      "content": "Bên cạnh chúng chim trời làm tổ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.850Z",
      "updatedAt": "2024-07-31T11:46:11.158Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "e2c26ad0-9e8f-42f1-95d5-6dbe1d132194",
      "number": 12,
      "content": "Cất tiếng hót ca giữa các cành cây.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.850Z",
      "updatedAt": "2024-07-31T11:46:11.167Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "a54ce8dd-4921-46d1-b4cc-6cea5cf70a2a",
      "number": 13,
      "content": "Từ thiên cung Ngài đổ nước xuống tưới các núi rừng;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.856Z",
      "updatedAt": "2024-07-31T11:46:11.176Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "9160e39f-e693-447e-b35c-d26428952d9f",
      "number": 13,
      "content": "Trái đất được no nê nhờ bông trái của công việc Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.856Z",
      "updatedAt": "2024-07-31T11:46:11.186Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "deb23e08-6326-4676-b54e-30b66bc9e4bf",
      "number": 14,
      "content": "Ngài khiến cỏ xanh mọc lên cho súc vật,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.862Z",
      "updatedAt": "2024-07-31T11:46:11.197Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "a36ed845-8cd2-4620-80bc-343c9657a6d2",
      "number": 14,
      "content": "Và rau trái sinh ra để phục vụ loài người;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.862Z",
      "updatedAt": "2024-07-31T11:46:11.206Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "0b8701cf-4e4e-421b-afad-251c4e9a7cb7",
      "number": 14,
      "content": "Từ ruộng đất người ta có thể đem về cho mình thực phẩm,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.862Z",
      "updatedAt": "2024-07-31T11:46:11.217Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "190b6105-8403-4938-9228-833905b2589a",
      "number": 15,
      "content": "Rồi chế thành rượu để uống cho lòng vui vẻ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.867Z",
      "updatedAt": "2024-07-31T11:46:11.226Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "51cc302b-0003-43d8-8663-945f931682ec",
      "number": 15,
      "content": "Chế thành dầu để xức cho mặt mày tươi sáng,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.867Z",
      "updatedAt": "2024-07-31T11:46:11.235Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "3296815b-f4fa-46f7-bd5d-2db4821f801a",
      "number": 15,
      "content": "Và làm cơm bánh để ăn cho vững mạnh lòng người.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.867Z",
      "updatedAt": "2024-07-31T11:46:11.244Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "5b712dd5-0680-47b2-9ed4-516654ed72cb",
      "number": 16,
      "content": "Những cây cối của Chúa đều được nước uống no nê,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.873Z",
      "updatedAt": "2024-07-31T11:46:11.255Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "82d45210-2a98-4ab1-a00d-c66ea5065d99",
      "number": 16,
      "content": "Kể cả những cây bá hương Ngài đã trồng ở Li-băng,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.873Z",
      "updatedAt": "2024-07-31T11:46:11.266Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "3689534a-9c6a-46e5-bd17-6555fad76efc",
      "number": 17,
      "content": "Cùng những cây cối khác, nơi các chim trời làm tổ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.878Z",
      "updatedAt": "2024-07-31T11:46:11.276Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "bcad7de6-b1dd-4f3f-a07f-af48fd255f41",
      "number": 17,
      "content": "Luôn cả những cây bách, nơi chim hạc chọn làm nhà.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.878Z",
      "updatedAt": "2024-07-31T11:46:11.286Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "54ab7f42-33a7-492a-90c1-bd712fb5559c",
      "number": 18,
      "content": "Ngài dựng nên những núi cao cho các dê rừng làm chỗ ở;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.884Z",
      "updatedAt": "2024-07-31T11:46:11.297Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "2e95d997-2fcc-49da-9356-30c03887368b",
      "number": 18,
      "content": "Các gành đá cheo leo cho các chuột núi nương thân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.884Z",
      "updatedAt": "2024-07-31T11:46:11.307Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "2e039e7f-e016-4087-bc3d-a1c9f9bdeda1",
      "number": 19,
      "content": "Ngài dựng nên mặt trăng để ấn định các mùa;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.889Z",
      "updatedAt": "2024-07-31T11:46:11.316Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "8543fbfd-a372-4385-a0f2-a326ffe91291",
      "number": 19,
      "content": "Còn mặt trời tự biết khi nào lặn xuống.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.889Z",
      "updatedAt": "2024-07-31T11:46:11.325Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "63979538-c044-43d4-8a9a-7be4f4d2dcc3",
      "number": 20,
      "content": "Ngài dựng nên bóng tối, và nó trở thành ban đêm;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.894Z",
      "updatedAt": "2024-07-31T11:46:11.333Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "b0ecfaab-973c-4d28-ad83-c0e6793eca8e",
      "number": 20,
      "content": "Ấy là khi các thú rừng đi ra săn mồi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.894Z",
      "updatedAt": "2024-07-31T11:46:11.342Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "d751b855-c2e4-49b1-bfc3-f41454134df1",
      "number": 21,
      "content": "Các sư tử tơ gầm rống tìm mồi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.900Z",
      "updatedAt": "2024-07-31T11:46:11.350Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "276767c3-1235-4380-ae3c-696c66eb0ab1",
      "number": 21,
      "content": "Và tìm kiếm thức ăn từ Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.900Z",
      "updatedAt": "2024-07-31T11:46:11.359Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "02ec2295-05e3-4315-837b-946dd33bc12b",
      "number": 22,
      "content": "Ðến khi mặt trời mọc lên chúng lại rút về,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.905Z",
      "updatedAt": "2024-07-31T11:46:11.368Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "b1b48326-557e-47cf-b892-577bdd8eb04d",
      "number": 22,
      "content": "Và nằm xuống trong hang.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.905Z",
      "updatedAt": "2024-07-31T11:46:11.380Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "e6e0f502-d628-4119-bfe9-6dba9890c8ab",
      "number": 23,
      "content": "Người ta đi ra làm việc,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.910Z",
      "updatedAt": "2024-07-31T11:46:11.388Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "f814258b-5517-4d01-8c08-2ba6d58d3113",
      "number": 23,
      "content": "Và làm lụng cho đến hoàng hôn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.910Z",
      "updatedAt": "2024-07-31T11:46:11.397Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "f120bdcd-873f-4cae-8ef6-26fba6103eea",
      "number": 24,
      "content": "Chúa ôi, công việc của Ngài thật nhiều thay!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.916Z",
      "updatedAt": "2024-07-31T11:46:11.410Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "42afd958-7734-48e7-a99a-a14d9d29e1e1",
      "number": 24,
      "content": "Ngài đã dựng nên tất cả bằng sự khôn ngoan của Ngài;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.916Z",
      "updatedAt": "2024-07-31T11:46:11.420Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "aa00bf5d-aa97-47a5-94fe-602e15e6f1a3",
      "number": 24,
      "content": "Trái đất đầy dẫy muôn vật do Ngài dựng nên.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.916Z",
      "updatedAt": "2024-07-31T11:46:11.429Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "50cb7a26-3047-492c-8f1d-f5f116f15a90",
      "number": 25,
      "content": "Kìa là đại dương sâu rộng mênh mông,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.922Z",
      "updatedAt": "2024-07-31T11:46:11.437Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "4e0677fc-3ece-43a7-92d1-4bfa356fa2b2",
      "number": 25,
      "content": "Trong đó chứa nhung nhúc biết bao sinh vật;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.922Z",
      "updatedAt": "2024-07-31T11:46:11.446Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "edba2549-d612-411c-9035-c54adc9d0617",
      "number": 25,
      "content": "Thật là vô sốsinh vật lớn và nhỏ.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.922Z",
      "updatedAt": "2024-07-31T11:46:11.456Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "e56afeb7-a823-4cae-a8d6-4f11f823868b",
      "number": 26,
      "content": "Rồi cũng có các ghe thuyền tàu bè qua lại,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.927Z",
      "updatedAt": "2024-07-31T11:46:11.467Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "d8a67de8-d052-4272-906c-c39e0dc4f379",
      "number": 26,
      "content": "Và con quái vật Ngài đã dựng nên đùa chơi trong đó.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.927Z",
      "updatedAt": "2024-07-31T11:46:11.477Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "953c3f08-78c7-4450-9b42-cbad452ff1da",
      "number": 27,
      "content": "Tất cả đều trông chờ nơi Ngài,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.933Z",
      "updatedAt": "2024-07-31T11:46:11.489Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "0cb42251-fef5-4921-9354-00d41e9f5a45",
      "number": 27,
      "content": "Ðể Ngài ban cho chúng vật thực phải thì.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.933Z",
      "updatedAt": "2024-07-31T11:46:11.500Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "38dc2673-f8cd-46d7-bc90-a8d90f07be19",
      "number": 28,
      "content": "Khi Ngài cho ăn, chúng liền tụ lại;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.938Z",
      "updatedAt": "2024-07-31T11:46:11.509Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "124dd03c-9b9c-4bc0-a9ff-cf9124d888a0",
      "number": 28,
      "content": "Khi Ngài sè tay ra, chúng được ăn uống no nê.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.938Z",
      "updatedAt": "2024-07-31T11:46:11.518Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "48cb8f8e-d4e5-4473-a29e-96da4a387e55",
      "number": 29,
      "content": "Khi Ngài ẩn mặt, chúng hoảng kinh bối rối;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.943Z",
      "updatedAt": "2024-07-31T11:46:11.528Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "3d5d3e55-2987-40fc-92a8-01ef79fb3d81",
      "number": 29,
      "content": "Khi Ngài lấy hơi thở chúng đi, chúng liền chết và trở về cát bụi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.943Z",
      "updatedAt": "2024-07-31T11:46:11.537Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "6849c733-d6ae-4137-9b0c-25d1f33c889e",
      "number": 30,
      "content": "Khi Ngài ban Thần Khí Ngài ra, chúng được dựng nên,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.949Z",
      "updatedAt": "2024-07-31T11:46:11.546Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "43bae52d-e02d-4019-8398-13bf7d68de70",
      "number": 30,
      "content": "Và Ngài làm cho mặt đất mới lại.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.949Z",
      "updatedAt": "2024-07-31T11:46:11.556Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "c95cd2c1-363c-4ab3-87a9-6a159434fc88",
      "number": 31,
      "content": "Nguyện vinh hiển của Chúa còn đến đời đời.",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.955Z",
      "updatedAt": "2024-07-31T11:46:11.571Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "5cd25aec-c9ef-4a54-aba0-c1543a176d2d",
      "number": 31,
      "content": "Nguyện Chúa vui thích về các công việc Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.955Z",
      "updatedAt": "2024-07-31T11:46:11.580Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "9757d4ef-9eef-4a21-8e24-9b7680bdca0c",
      "number": 32,
      "content": "Ngài nhìn vào trái đất, nó run lên lẩy bẩy;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.960Z",
      "updatedAt": "2024-07-31T11:46:11.592Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "fc307a52-4e22-4306-a8ee-42a30cd957af",
      "number": 32,
      "content": "Ngài đụng đến núi non, chúng bốc khói mịt mù.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.960Z",
      "updatedAt": "2024-07-31T11:46:11.602Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "f52ad1fa-9217-4e29-ab42-db7df965fd5e",
      "number": 33,
      "content": "Hễ tôi còn sống bao lâu, tôi sẽ ca tụng Chúa bấy lâu;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.971Z",
      "updatedAt": "2024-07-31T11:46:11.615Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "305e937c-b94f-439e-8391-5106189cd2eb",
      "number": 33,
      "content": "Hễ tôi còn sống chừng nào, tôi sẽ ca ngợi Ðức Chúa Trời tôi chừng nấy.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.971Z",
      "updatedAt": "2024-07-31T11:46:11.623Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "77973b1b-6866-4bf7-bd50-a5fe03598fbf",
      "number": 34,
      "content": "Nguyện những suy nghĩ của tôi được đẹp lòng Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.977Z",
      "updatedAt": "2024-07-31T11:46:11.632Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "7ea5a8bb-b78c-4b2c-9b71-08c65263a284",
      "number": 34,
      "content": "Về phần tôi, tôi sẽ cứ vui mừng trong Chúa.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.977Z",
      "updatedAt": "2024-07-31T11:46:11.641Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "97569d67-79e5-48e6-9696-b68c1d83c236",
      "number": 35,
      "content": "Nguyện những kẻ tội lỗi bị diệt trừ khỏi mặt đất;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.983Z",
      "updatedAt": "2024-07-31T11:46:11.649Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "8fda4574-8d46-4a9b-8074-2fe2007a1ca8",
      "number": 35,
      "content": "Nguyện những kẻ ác không còn nữa.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.983Z",
      "updatedAt": "2024-07-31T11:46:11.658Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "fdb29719-55cc-4e92-9aca-daf62faaaa32",
      "number": 35,
      "content": "Hãy chúc tụng Chúa, hỡi linh hồn ta,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.983Z",
      "updatedAt": "2024-07-31T11:46:11.669Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    },
    {
      "id": "ead7eb0b-c325-4f5f-b022-beeb3aa42af7",
      "number": 35,
      "content": "Hãy ca ngợi Chúa!",
      "order": 3,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:34.983Z",
      "updatedAt": "2024-07-31T11:46:11.678Z",
      "chapterId": "8567b500-b44f-4402-8efd-265b09b35eda"
    }
  ],
