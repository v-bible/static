{
  "chapter": {
    "id": "9dded301-1c10-4333-915e-12255c457276",
    "number": 63,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2063&version=BD2011",
    "createdAt": "2024-07-12T04:03:46.889Z",
    "updatedAt": "2024-07-24T15:07:47.802Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "a0fc76b0-f5ba-4093-ba9e-b13b7dfc0d71",
      "number": 1,
      "content": "Ðức Chúa Trời ôi, Ngài là Ðức Chúa Trời của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.381Z",
      "updatedAt": "2024-07-31T11:36:03.259Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "6c668d73-316f-4313-a864-960df9e30743",
      "number": 1,
      "content": "Khi hừng đông chưa ló dạng, con đã tìm cầu Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.381Z",
      "updatedAt": "2024-07-31T11:36:03.289Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "4f34f857-2a43-4397-a1f9-599b54ac3f11",
      "number": 1,
      "content": "Linh hồn con khao khát Ngài, thân thể con mong mỏi Ngài,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.381Z",
      "updatedAt": "2024-07-31T11:36:03.310Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "6f45171d-f8d6-48b0-9f61-e9912c6acb34",
      "number": 1,
      "content": "Trong một nơi khô khan cằn cỗi và chẳng có nước.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.381Z",
      "updatedAt": "2024-07-31T11:36:03.324Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "b446f796-4239-455b-bfe8-f02acbedb5c2",
      "number": 2,
      "content": "Con mong ước được thấy quyền năng và vinh hiển Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.392Z",
      "updatedAt": "2024-07-31T11:36:03.336Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "41a6ea69-b790-4a70-a3ca-3bb180b804db",
      "number": 2,
      "content": "Như con đã từng thấy trong nơi thánh.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.392Z",
      "updatedAt": "2024-07-31T11:36:03.348Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "82a88ab8-bac3-48d7-b4d6-3fe5630e7030",
      "number": 3,
      "content": "Vì tình thương của Ngài tốt hơn mạng sống,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.394Z",
      "updatedAt": "2024-07-31T11:36:03.356Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "f99f485c-5cf3-4a56-9707-97c592012a29",
      "number": 3,
      "content": "Nên môi con sẽ ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.394Z",
      "updatedAt": "2024-07-31T11:36:03.364Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "3cb6b75d-a718-49be-9f6b-c976482c1bae",
      "number": 4,
      "content": "Hễ con còn sống bao lâu, con sẽ ca ngợi Ngài bấy lâu;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.396Z",
      "updatedAt": "2024-07-31T11:36:03.371Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "f5ababc9-f026-489d-b144-430162275f5f",
      "number": 4,
      "content": "Con sẽ đưa tay lên và chúc tụng danh Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.396Z",
      "updatedAt": "2024-07-31T11:36:03.379Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "d90d21fa-9592-4438-820e-444c2d65df19",
      "number": 5,
      "content": "Linh hồn con sẽ vui thỏa như được hưởng các thức ăn béo bổ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.399Z",
      "updatedAt": "2024-07-31T11:36:03.387Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "87db6dcb-449b-4e4b-9ffc-bbbca0caacc3",
      "number": 5,
      "content": "Miệng con sẽ ca ngợi Ngài với môi vui vẻ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.399Z",
      "updatedAt": "2024-07-31T11:36:03.395Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "9975e92d-42e8-4edd-83f3-1c28e26c8ec2",
      "number": 6,
      "content": "Khi nằm trên giường con nhớ đến Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.402Z",
      "updatedAt": "2024-07-31T11:36:03.403Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "8fbaa5bf-7316-4bf6-ba8b-8b08699499f9",
      "number": 6,
      "content": "Con nghĩ về Ngài trọn các canh đêm,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.402Z",
      "updatedAt": "2024-07-31T11:36:03.410Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "763dc01c-0b48-4871-9085-e710880b1445",
      "number": 7,
      "content": "Vì Ngài là nguồn cứu giúp của con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.404Z",
      "updatedAt": "2024-07-31T11:36:03.418Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "3486267d-5ce0-4b41-9006-349c916926e0",
      "number": 7,
      "content": "Nên dưới cánh Ngài con hát mừng vui vẻ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.404Z",
      "updatedAt": "2024-07-31T11:36:03.425Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "1961d043-baf7-4ecd-baa1-fd9a25048c72",
      "number": 8,
      "content": "Linh hồn con bám chặt vào Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.406Z",
      "updatedAt": "2024-07-31T11:36:03.434Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "ac1e3a07-28bd-4f7d-b626-a527f531bfbc",
      "number": 8,
      "content": "Tay phải Ngài giữ vững con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.406Z",
      "updatedAt": "2024-07-31T11:36:03.441Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "50613855-10d0-41c6-abed-fd5dc4a7163c",
      "number": 9,
      "content": "Nhưng những kẻ tìm cách hủy diệt mạng sống con sẽ bị rơi vào đáy sâu của lòng đất.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.408Z",
      "updatedAt": "2024-07-31T11:36:03.451Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "7af6c407-a126-4db7-9610-1e25845c93f9",
      "number": 10,
      "content": "Chúng sẽ bị phó cho lưỡi gươm;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.411Z",
      "updatedAt": "2024-07-31T11:36:03.459Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "232abe48-d53a-4553-8cb8-b869a009f324",
      "number": 10,
      "content": "Chúng sẽ thành mồi cho chồn cáo.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.411Z",
      "updatedAt": "2024-07-31T11:36:03.467Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "dc7ed59d-5843-4713-ae96-645a01c09681",
      "number": 11,
      "content": "Nhưng vua sẽ vui mừng trong Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.413Z",
      "updatedAt": "2024-07-31T11:36:03.474Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "45ce6f12-9f8d-4069-8f41-efed22b2a95e",
      "number": 11,
      "content": "Những ai nhân danh Ngài mà thề nguyện sẽ được vinh hiển;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.413Z",
      "updatedAt": "2024-07-31T11:36:03.482Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    },
    {
      "id": "db036e64-ff61-4ea6-aae2-b31a3d84110a",
      "number": 11,
      "content": "Nhưng miệng của những kẻ nói dối phải ngậm lại.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:55:59.413Z",
      "updatedAt": "2024-07-31T11:36:03.489Z",
      "chapterId": "9dded301-1c10-4333-915e-12255c457276"
    }
  ],
