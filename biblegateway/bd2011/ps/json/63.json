{
  "chapter": {
    "id": "e8c61079-6b0c-4696-9c30-2d31e6bfc717",
    "number": 63,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2063&version=BD2011",
    "createdAt": "2024-09-09T04:13:00.452Z",
    "updatedAt": "2024-09-09T04:13:00.452Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "99cb748f-08c4-4618-8c46-aa28c2933005",
      "number": 1,
      "content": "Ðức Chúa Trời ôi, Ngài là Ðức Chúa Trời của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.286Z",
      "updatedAt": "2024-09-09T04:46:14.286Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "41c00b51-3695-483e-b180-84a5ecf6ee3d",
      "number": 1,
      "content": "Khi hừng đông chưa ló dạng, con đã tìm cầu Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.304Z",
      "updatedAt": "2024-09-09T04:46:14.304Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "470690dc-4680-4156-a8eb-b41f593bc821",
      "number": 1,
      "content": "Linh hồn con khao khát Ngài, thân thể con mong mỏi Ngài,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.309Z",
      "updatedAt": "2024-09-09T04:46:14.309Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "cdd0483d-f830-4efe-8fd0-15150e119481",
      "number": 1,
      "content": "Trong một nơi khô khan cằn cỗi và chẳng có nước.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.314Z",
      "updatedAt": "2024-09-09T04:46:14.314Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "e0f4a254-ca8c-46c6-a5c9-fb2c2c7b45e0",
      "number": 2,
      "content": "Con mong ước được thấy quyền năng và vinh hiển Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.319Z",
      "updatedAt": "2024-09-09T04:46:14.319Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "89bb02a1-e2a5-4a24-a70f-4da3488b0793",
      "number": 2,
      "content": "Như con đã từng thấy trong nơi thánh.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.324Z",
      "updatedAt": "2024-09-09T04:46:14.324Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "e8f737f6-d0b4-447f-9412-92f88e9c9972",
      "number": 3,
      "content": "Vì tình thương của Ngài tốt hơn mạng sống,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.329Z",
      "updatedAt": "2024-09-09T04:46:14.329Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "fdddd745-d65d-450c-aba6-81ca1080b671",
      "number": 3,
      "content": "Nên môi con sẽ ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.333Z",
      "updatedAt": "2024-09-09T04:46:14.333Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "870e2d6b-87c2-48dc-861d-1d434b2fe48e",
      "number": 4,
      "content": "Hễ con còn sống bao lâu, con sẽ ca ngợi Ngài bấy lâu;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.338Z",
      "updatedAt": "2024-09-09T04:46:14.338Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "541e8c41-6df1-4faf-af3f-b35289c2352b",
      "number": 4,
      "content": "Con sẽ đưa tay lên và chúc tụng danh Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.343Z",
      "updatedAt": "2024-09-09T04:46:14.343Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "350f983e-f9e2-4490-a14e-b81cb92018c7",
      "number": 5,
      "content": "Linh hồn con sẽ vui thỏa như được hưởng các thức ăn béo bổ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.347Z",
      "updatedAt": "2024-09-09T04:46:14.347Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "243d49c6-1be0-4ddd-af85-b74415c7e3f1",
      "number": 5,
      "content": "Miệng con sẽ ca ngợi Ngài với môi vui vẻ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.352Z",
      "updatedAt": "2024-09-09T04:46:14.352Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "22b13676-b01d-4923-af66-a1283a433628",
      "number": 6,
      "content": "Khi nằm trên giường con nhớ đến Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.357Z",
      "updatedAt": "2024-09-09T04:46:14.357Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "3e543265-d4ef-47e8-a771-a6d1f1af2dda",
      "number": 6,
      "content": "Con nghĩ về Ngài trọn các canh đêm,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.363Z",
      "updatedAt": "2024-09-09T04:46:14.363Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "8df1e6d7-4bd6-4fdd-8585-cf67c44d80b3",
      "number": 7,
      "content": "Vì Ngài là nguồn cứu giúp của con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.368Z",
      "updatedAt": "2024-09-09T04:46:14.368Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "fef16a32-4f06-4fa3-a65e-b20a627caa65",
      "number": 7,
      "content": "Nên dưới cánh Ngài con hát mừng vui vẻ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.373Z",
      "updatedAt": "2024-09-09T04:46:14.373Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "a5eff7ab-cadd-481a-a014-6b6dd8357e6c",
      "number": 8,
      "content": "Linh hồn con bám chặt vào Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.378Z",
      "updatedAt": "2024-09-09T04:46:14.378Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "1dee67b4-afab-4988-b52f-5cf54246ec12",
      "number": 8,
      "content": "Tay phải Ngài giữ vững con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.384Z",
      "updatedAt": "2024-09-09T04:46:14.384Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "d0bfaaac-a77e-40b8-95a7-933d984abedf",
      "number": 9,
      "content": "Nhưng những kẻ tìm cách hủy diệt mạng sống con sẽ bị rơi vào đáy sâu của lòng đất.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.388Z",
      "updatedAt": "2024-09-09T04:46:14.388Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "a4063fc1-baeb-4f1f-9b92-b05ce8be337c",
      "number": 10,
      "content": "Chúng sẽ bị phó cho lưỡi gươm;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.394Z",
      "updatedAt": "2024-09-09T04:46:14.394Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "710db90e-6cf1-425e-a12b-0ecf1fcdb6cd",
      "number": 10,
      "content": "Chúng sẽ thành mồi cho chồn cáo.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.399Z",
      "updatedAt": "2024-09-09T04:46:14.399Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "f6a1e8ee-ffff-4e0e-8a35-ea0234f85192",
      "number": 11,
      "content": "Nhưng vua sẽ vui mừng trong Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.404Z",
      "updatedAt": "2024-09-09T04:46:14.404Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "f583cf17-3b1d-4aad-806c-29ef74572fcc",
      "number": 11,
      "content": "Những ai nhân danh Ngài mà thề nguyện sẽ được vinh hiển;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.408Z",
      "updatedAt": "2024-09-09T04:46:14.408Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    },
    {
      "id": "5e8c72af-8f17-4787-9eeb-4e072ba58018",
      "number": 11,
      "content": "Nhưng miệng của những kẻ nói dối phải ngậm lại.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:14.413Z",
      "updatedAt": "2024-09-09T04:46:14.413Z",
      "chapterId": "e8c61079-6b0c-4696-9c30-2d31e6bfc717"
    }
  ],
