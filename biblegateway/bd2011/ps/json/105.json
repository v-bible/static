{
  "chapter": {
    "id": "990b7469-38e8-4066-b9b6-f0f1ff216bfa",
    "number": 105,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20105&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.317Z",
    "updatedAt": "2024-09-09T04:13:01.317Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "eb0d2f5f-64d1-41b4-b5c7-0723f51bf1b1",
      "number": 1,
      "content": "Hãy cảm tạ Chúa, hãy cầu khẩn danh Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.781Z",
      "updatedAt": "2024-09-09T04:49:41.781Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "6c46678f-6caa-4dc1-a24d-cc268acc47e8",
      "number": 1,
      "content": "Hãy rao truyền cho các dân biết các công việc Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.794Z",
      "updatedAt": "2024-09-09T04:49:41.794Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "170d0b8e-0980-4898-80c9-f86b9c28f946",
      "number": 2,
      "content": "Hãy ca ngợi Ngài, hãy ca hát chúc tụng Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.798Z",
      "updatedAt": "2024-09-09T04:49:41.798Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "46861965-787f-4cfd-a6db-3104f4c97386",
      "number": 2,
      "content": "Hãy kể ra tất cả việc lạ lùng của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.803Z",
      "updatedAt": "2024-09-09T04:49:41.803Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "78d7ed21-59c0-4e08-a804-fbcdc0e285c0",
      "number": 3,
      "content": "Vinh hiển thay cho ai được ở trong danh thánh Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.808Z",
      "updatedAt": "2024-09-09T04:49:41.808Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "74b835ca-6050-4ff4-b2d0-4039fb6c1a0a",
      "number": 3,
      "content": "Nguyện lòng những người tìm kiếm Chúa được vui mừng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.813Z",
      "updatedAt": "2024-09-09T04:49:41.813Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "6d4030ef-7108-4da0-8026-e7e10b95d3cb",
      "number": 4,
      "content": "Hãy tìm kiếm Chúa và quyền năng Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.818Z",
      "updatedAt": "2024-09-09T04:49:41.818Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "94399a87-252f-4e6a-ac41-6705b7315b25",
      "number": 4,
      "content": "Hãy tìm kiếm mặt Ngài luôn luôn.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.823Z",
      "updatedAt": "2024-09-09T04:49:41.823Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "1d87b010-e3c0-4409-aaaa-b0dba3d2dca2",
      "number": 5,
      "content": "Hãy nhớ mãi những việc lạ lùng Ngài làm,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.828Z",
      "updatedAt": "2024-09-09T04:49:41.828Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "70108cd8-c783-43d9-a4c0-0af34a8a465d",
      "number": 5,
      "content": "Tức những việc diệu kỳ và những phán quyết miệng Ngài đã phán,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.833Z",
      "updatedAt": "2024-09-09T04:49:41.833Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "f09d32ab-9913-4528-baf2-177a4b77f8a0",
      "number": 6,
      "content": "Hỡi dòng dõi của Áp-ra-ham, tôi tớ Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.838Z",
      "updatedAt": "2024-09-09T04:49:41.838Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "ef244fbe-7bfe-4aa0-9049-670519b4e932",
      "number": 6,
      "content": "Hỡi con cháu của Gia-cốp, những người Ngài chọn.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.843Z",
      "updatedAt": "2024-09-09T04:49:41.843Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "ef2c7bf6-9389-4a2a-bd5e-ab18d6b215fc",
      "number": 7,
      "content": "Ngài là Chúa, Ðức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.848Z",
      "updatedAt": "2024-09-09T04:49:41.848Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "f4dd21d1-7f9d-467d-aa0a-7ec021a82d2e",
      "number": 7,
      "content": "Các phán quyết của Ngài là định luật cho cả địa cầu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.854Z",
      "updatedAt": "2024-09-09T04:49:41.854Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "442c0c72-b343-4552-b9ce-cd1fa4b6dc14",
      "number": 8,
      "content": "Ngài ghi nhớ giao ước Ngài mãi mãi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.859Z",
      "updatedAt": "2024-09-09T04:49:41.859Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "6d30b3c1-1c76-4093-8e0e-6aa6f393ea34",
      "number": 8,
      "content": "Lời Ngài truyền có hiệu lực đến ngàn thế hệ về sau;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.864Z",
      "updatedAt": "2024-09-09T04:49:41.864Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "83cd3157-eb33-4647-a755-32100d523015",
      "number": 9,
      "content": "Ấy là giao ước Ngài đã lập với Áp-ra-ham,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.868Z",
      "updatedAt": "2024-09-09T04:49:41.868Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "0d5dc317-3b1e-43e7-b46d-faeeb2c69a36",
      "number": 9,
      "content": "Và lời thề Ngài đã thề cùng I-sác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.873Z",
      "updatedAt": "2024-09-09T04:49:41.873Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "1628cd02-c934-4b83-8fe5-ccdd81800df6",
      "number": 10,
      "content": "Sau đó Ngài đã xác nhận với Gia-cốp như một đạo luật,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.879Z",
      "updatedAt": "2024-09-09T04:49:41.879Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "a2031e03-cdb4-4994-8a9b-f4a8ddcb7f66",
      "number": 10,
      "content": "Với I-sơ-ra-ên như một giao ước đời đời,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.884Z",
      "updatedAt": "2024-09-09T04:49:41.884Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "9b9ec1a9-6858-4d02-9711-af149c044a0e",
      "number": 11,
      "content": "Rằng, “Ta sẽ ban xứ Ca-na-an cho các ngươi để làm sản nghiệp,”",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.889Z",
      "updatedAt": "2024-09-09T04:49:41.889Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "a0fb919a-da7e-4930-ac82-c6c9ac40dd0f",
      "number": 12,
      "content": "Trong khi họ chỉ là một số ít người,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.894Z",
      "updatedAt": "2024-09-09T04:49:41.894Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "afe179f0-68d7-4821-ab72-c08bcaebe5c3",
      "number": 12,
      "content": "Một nhóm thiểu số và kiều dân trong xứ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.899Z",
      "updatedAt": "2024-09-09T04:49:41.899Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "2f91caeb-b096-4c4f-a61a-01fba78c8c56",
      "number": 13,
      "content": "Họ đã đi lang thang từ nước này sang nước khác,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.903Z",
      "updatedAt": "2024-09-09T04:49:41.903Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "274e88c5-3719-40d1-a28c-9e3889979b86",
      "number": 13,
      "content": "Từ vương quốc nọ đến vương quốc kia.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.908Z",
      "updatedAt": "2024-09-09T04:49:41.908Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "dc24731c-1986-488b-83bc-ecefc46c19a1",
      "number": 14,
      "content": "Nhưng Ngài đã không cho phép ai hà hiếp họ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.913Z",
      "updatedAt": "2024-09-09T04:49:41.913Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "a00b2e4b-8946-4da7-bf07-451ef81e6324",
      "number": 14,
      "content": "Vì cớ họ Ngài đã quở trách các vua,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.918Z",
      "updatedAt": "2024-09-09T04:49:41.918Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "c286c01e-a925-4a95-825e-b1c255b9924a",
      "number": 15,
      "content": "“Ðừng đụng đến những người được xức dầu của Ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.923Z",
      "updatedAt": "2024-09-09T04:49:41.923Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "5866dd07-a440-4b90-84c3-01f21c48bb3a",
      "number": 15,
      "content": "Chớ làm hại các tiên tri của Ta.”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.928Z",
      "updatedAt": "2024-09-09T04:49:41.928Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "1f609b9b-e269-4f5a-b164-b981a3f89711",
      "number": 16,
      "content": "Khi Ngài sai nạn đói đến trong xứ;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.933Z",
      "updatedAt": "2024-09-09T04:49:41.933Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "4f0411d5-2ba3-4f68-9001-ee78ae9b9414",
      "number": 16,
      "content": "Và dẹp hết mọi nguồn lương thực của họ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.938Z",
      "updatedAt": "2024-09-09T04:49:41.938Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "14a4690e-b482-49a4-b657-6b5398b00e7f",
      "number": 17,
      "content": "Thì Ngài đã sai một người ra đi trước họ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.944Z",
      "updatedAt": "2024-09-09T04:49:41.944Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "cc1e7e28-6a75-4de1-becf-8def4fd9c9a1",
      "number": 17,
      "content": "Ấy là Giô-sép, một người bị bán làm nô lệ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.948Z",
      "updatedAt": "2024-09-09T04:49:41.948Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "be8bb13a-604a-4437-9ba0-fab24d9bc90c",
      "number": 18,
      "content": "Họ tra chân ông vào cùm, khiến ông bị đau đớn;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.954Z",
      "updatedAt": "2024-09-09T04:49:41.954Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "e4e077b7-2496-43b8-8ab7-297087bf000d",
      "number": 18,
      "content": "Họ bắt cổ ông phải mang xích sắt nặng nề,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.959Z",
      "updatedAt": "2024-09-09T04:49:41.959Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "87201da6-535e-4ac3-a811-5be5d0d16a13",
      "number": 19,
      "content": "Cho đến khi những gì ông nói được ứng nghiệm;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.964Z",
      "updatedAt": "2024-09-09T04:49:41.964Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "a6cfc556-57ce-42a4-b5a5-a4419647a1de",
      "number": 19,
      "content": "Lời Chúa minh xác rằng lời ông đúng hoàn toàn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.970Z",
      "updatedAt": "2024-09-09T04:49:41.970Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "1fb6542e-cbd8-4907-a5f6-61565e3ef5f8",
      "number": 20,
      "content": "Do đó vua phải sai người đến thả ông ra;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.974Z",
      "updatedAt": "2024-09-09T04:49:41.974Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "b092c08b-fddd-4871-9609-91eb76e2332f",
      "number": 20,
      "content": "Kẻ cai trị các dân đã cho ông được tự do.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.979Z",
      "updatedAt": "2024-09-09T04:49:41.979Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "af00a7a2-28dc-4ddc-92dc-6e1ca1a12be3",
      "number": 21,
      "content": "Rồi vua ấy lập ông làm tể tướng của vương quốc mình,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.984Z",
      "updatedAt": "2024-09-09T04:49:41.984Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "bc699f73-6a65-4ee9-b79a-131536b38f7e",
      "number": 21,
      "content": "Làm người cai trị trên tất cả cơ nghiệp mình,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:41.998Z",
      "updatedAt": "2024-09-09T04:49:41.998Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "7902cd01-ff7d-4e8c-8576-2560bf911ebc",
      "number": 22,
      "content": "Ðể ông chỉ huy những người quyền thế mình,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.003Z",
      "updatedAt": "2024-09-09T04:49:42.003Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "136b372c-c977-49d3-87b8-0d97b55de2c1",
      "number": 22,
      "content": "Và bày diệu kế cho các thượng quan mình.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.007Z",
      "updatedAt": "2024-09-09T04:49:42.007Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "f626a0d4-7940-4777-a776-3340167d1585",
      "number": 23,
      "content": "Sau đó I-sơ-ra-ên di cư vào Ai-cập;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.012Z",
      "updatedAt": "2024-09-09T04:49:42.012Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "722d645d-4fa8-4c01-8ae5-5e07208a7d7d",
      "number": 23,
      "content": "Gia-cốp đến kiều ngụ trong xứ của Cham.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.017Z",
      "updatedAt": "2024-09-09T04:49:42.017Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "63e5e6e3-344a-4841-9f68-d0efbb1db538",
      "number": 24,
      "content": "Tại đó Chúa làm cho dân Ngài gia tăng đông đúc,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.022Z",
      "updatedAt": "2024-09-09T04:49:42.022Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "907f9811-3971-4d9f-a8b9-fc73ddf59c7a",
      "number": 24,
      "content": "Khiến họ mạnh hơn những kẻ thù của họ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.026Z",
      "updatedAt": "2024-09-09T04:49:42.026Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "a048d7fe-2971-4fa0-8fea-a34072212245",
      "number": 25,
      "content": "Ngài để cho chúng thay lòng đổi dạ ghen ghét dân Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.031Z",
      "updatedAt": "2024-09-09T04:49:42.031Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "d26048ea-9180-450f-bb5e-1ac9fa6048be",
      "number": 25,
      "content": "Và lập mưu hãm hại các tôi tớ Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.035Z",
      "updatedAt": "2024-09-09T04:49:42.035Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "6ccfffab-c6fd-45c9-9ac2-0ac16c1ce870",
      "number": 26,
      "content": "Ngài sai Môi-se đầy tớ Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.040Z",
      "updatedAt": "2024-09-09T04:49:42.040Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "dfd2325d-86a4-48a5-8583-096416193bfb",
      "number": 26,
      "content": "Và A-rôn người Ngài đã chọn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.045Z",
      "updatedAt": "2024-09-09T04:49:42.045Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "8c031141-8243-4794-978a-4cdb00d00556",
      "number": 27,
      "content": "Họ thực hiện những việc diệu kỳ của Ngài giữa chúng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.049Z",
      "updatedAt": "2024-09-09T04:49:42.049Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "2e048a48-497c-432e-a035-0b8b50e83bb1",
      "number": 27,
      "content": "Và tỏ những phép lạ của Ngài trong xứ của Cham.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.054Z",
      "updatedAt": "2024-09-09T04:49:42.054Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "4571cc47-aadd-4676-a129-4c6116be0333",
      "number": 28,
      "content": "Ngài giáng tối tăm, khiến cả xứ trở nên đen kịt,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.059Z",
      "updatedAt": "2024-09-09T04:49:42.059Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "a36e6cc9-3e2e-4106-ae95-b777bab24064",
      "number": 28,
      "content": "Ðể chúng không nghịch lại lời Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.063Z",
      "updatedAt": "2024-09-09T04:49:42.063Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "866f8743-5f9d-487e-b70f-e29b430ef758",
      "number": 29,
      "content": "Ngài làm cho nước của chúng trở nên như máu,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.068Z",
      "updatedAt": "2024-09-09T04:49:42.068Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "97ebe3ce-b557-4c35-8138-edb7eb3d0ef3",
      "number": 29,
      "content": "Và khiến cá của chúng đều phải chết toi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.072Z",
      "updatedAt": "2024-09-09T04:49:42.072Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "9721f968-9cde-46f5-9df9-cdc917466e0b",
      "number": 30,
      "content": "Ếch nhái nhung nhúc tràn lan khắp xứ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.077Z",
      "updatedAt": "2024-09-09T04:49:42.077Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "d2f6d44c-b8b0-41f0-a069-0b3045889b55",
      "number": 30,
      "content": "Chúng tràn tận vào cung thất của nhà vua.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.082Z",
      "updatedAt": "2024-09-09T04:49:42.082Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "30984b4f-24b2-4c08-820f-ac6e6f495fdf",
      "number": 31,
      "content": "Ngài vừa phán, vô số ruồi mòng từ đâu bay đến;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.088Z",
      "updatedAt": "2024-09-09T04:49:42.088Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "61d826bf-7c30-4082-8138-cb53ceeccf9f",
      "number": 31,
      "content": "Muỗi vo ve tràn khắp cõi bờ.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.093Z",
      "updatedAt": "2024-09-09T04:49:42.093Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "68c7c78d-f2a6-47ab-81d4-abfcfbfb8732",
      "number": 32,
      "content": "Ngài cho trận mưa nhưng là mưa đá;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.098Z",
      "updatedAt": "2024-09-09T04:49:42.098Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "a623117b-91f5-44cd-bf5f-75c6cff3adbf",
      "number": 32,
      "content": "Sấm sét nổ vang, gây hỏa hoạn khắp nơi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.103Z",
      "updatedAt": "2024-09-09T04:49:42.103Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "358fcbb9-4c8e-4563-b38b-8e9bf5d51c82",
      "number": 33,
      "content": "Ngài quật ngã cây nho và cây vả của chúng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.108Z",
      "updatedAt": "2024-09-09T04:49:42.108Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "9b47f3a3-db56-4402-a1a6-5b162879f27c",
      "number": 33,
      "content": "Khiến cây cối của chúng ngã rạp khắp xứ.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.114Z",
      "updatedAt": "2024-09-09T04:49:42.114Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "8f0708fb-2070-46fa-a34f-5ab896521d77",
      "number": 34,
      "content": "Ngài phán nữa, vô số cào cào từ đâu bay đến;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.119Z",
      "updatedAt": "2024-09-09T04:49:42.119Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "62f3c1ce-ce97-4254-b435-42a132f795df",
      "number": 34,
      "content": "Chấu chấu phủ tràn lan, không thể đếm cho cùng.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.124Z",
      "updatedAt": "2024-09-09T04:49:42.124Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "b39dacfb-6211-48d7-b33e-085bf0477971",
      "number": 35,
      "content": "Chúng cắn phá mọi cỏ cây trong xứ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.129Z",
      "updatedAt": "2024-09-09T04:49:42.129Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "de58bb83-110c-4dc7-a922-a9f67e84849d",
      "number": 35,
      "content": "Chúng cắn ăn mọi sản vật của đất đai.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.135Z",
      "updatedAt": "2024-09-09T04:49:42.135Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "c022f09b-ec55-4211-93a9-d6b53382ef08",
      "number": 36,
      "content": "Ngài cũng đánh hạ các con đầu lòng trong xứ chúng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.140Z",
      "updatedAt": "2024-09-09T04:49:42.140Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "617cf2c6-0a59-4f25-ab5b-ef319ec2acbb",
      "number": 36,
      "content": "Những đứa con sinh ra khi sức lực chúng dồi dào.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.145Z",
      "updatedAt": "2024-09-09T04:49:42.145Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "169a3235-be39-4cd8-a7fe-27aa4fae32da",
      "number": 37,
      "content": "Sau đó Ngài đem dân I-sơ-ra-ên ra đi với biết bao vàng bạc;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.151Z",
      "updatedAt": "2024-09-09T04:49:42.151Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "f3991c04-b698-41aa-9a79-287b0c5cc0e3",
      "number": 37,
      "content": "Chẳng có ai chùn chân trong các chi tộc của Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.156Z",
      "updatedAt": "2024-09-09T04:49:42.156Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "64b71d77-8bfd-4d6d-9442-6ee8f52543b1",
      "number": 38,
      "content": "Ai-cập vui mừng khi họ lìa khỏi xứ,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.161Z",
      "updatedAt": "2024-09-09T04:49:42.161Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "bb2c2c6b-8be8-4afb-bd25-d0f50f3a841d",
      "number": 38,
      "content": "Vì nỗi sợ hãi họ giáng trên chúng khắp nơi.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.166Z",
      "updatedAt": "2024-09-09T04:49:42.166Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "58785063-a72c-4c96-9fbe-2067eaf4725d",
      "number": 39,
      "content": "Ngài giăng đám mây che mát trên đầu họ,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.171Z",
      "updatedAt": "2024-09-09T04:49:42.171Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "7e2b66d3-438a-4a12-a0c5-949368736bcc",
      "number": 39,
      "content": "Và ban lửa hồng soi sáng lúc đêm về.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.176Z",
      "updatedAt": "2024-09-09T04:49:42.176Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "051c1810-da8b-4f97-98ff-72b876c7efe7",
      "number": 40,
      "content": "Họ đòi thịt, Ngài mang đến họ chim cút nhiều vô kể;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.180Z",
      "updatedAt": "2024-09-09T04:49:42.180Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "244052e7-4b10-4ad0-bb35-4c64058cf317",
      "number": 40,
      "content": "Và họ được no nê bằng bánh đến từ trời.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.186Z",
      "updatedAt": "2024-09-09T04:49:42.186Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "f2aae0ef-245c-41f5-acd1-95a34fb02dbf",
      "number": 41,
      "content": "Ngài mở vầng đá, và nước ngọt tuôn ra lai láng;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.191Z",
      "updatedAt": "2024-09-09T04:49:42.191Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "1aa959f7-87fa-48b5-a60c-93304500ed90",
      "number": 41,
      "content": "Nước phun trào giữa nơi khô hạn tựa dòng sông.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.196Z",
      "updatedAt": "2024-09-09T04:49:42.196Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "07a8c604-8226-4fca-91de-fb832cd3743a",
      "number": 42,
      "content": "Vì Ngài nhớ lại lời hứa thánh của Ngài,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.201Z",
      "updatedAt": "2024-09-09T04:49:42.201Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "c7913e63-637a-4dc7-8625-7858ed5015c4",
      "number": 42,
      "content": "Tức lời hứa với Áp-ra-ham tôi tớ Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.207Z",
      "updatedAt": "2024-09-09T04:49:42.207Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "cb5e3114-7eb9-4f52-9fdf-55906fa14061",
      "number": 43,
      "content": "Vậy Ngài đã đem dân Ngài ra đi trong vui mừng hớn hở;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.213Z",
      "updatedAt": "2024-09-09T04:49:42.213Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "371126cf-6966-4b9f-aa01-f4de9736250f",
      "number": 43,
      "content": "Ðem những người Ngài chọn xuất hành với tiếng vui vẻ reo hò.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.218Z",
      "updatedAt": "2024-09-09T04:49:42.218Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "ee20a5d3-56e7-4ed5-84a6-7286077d7cf3",
      "number": 44,
      "content": "Ngài ban xứ sở của các dân cho họ làm sản nghiệp;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.223Z",
      "updatedAt": "2024-09-09T04:49:42.223Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "40d7e96a-2b28-4a1c-9955-a2056434c29a",
      "number": 44,
      "content": "Và cho họ hưởng công lao làm lụng của các dân,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.228Z",
      "updatedAt": "2024-09-09T04:49:42.228Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "641fc1cc-0794-4fc8-9a98-9885d8cdf498",
      "number": 45,
      "content": "Ðể họ vâng giữ luật lệ Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.233Z",
      "updatedAt": "2024-09-09T04:49:42.233Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "b3d1d183-5d60-4ca5-ba37-b037b75983e9",
      "number": 45,
      "content": "Và tuân theo luật pháp Ngài.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.238Z",
      "updatedAt": "2024-09-09T04:49:42.238Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    },
    {
      "id": "c24c04a2-2726-4c12-af7a-bb5af8932544",
      "number": 45,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:42.243Z",
      "updatedAt": "2024-09-09T04:49:42.243Z",
      "chapterId": "990b7469-38e8-4066-b9b6-f0f1ff216bfa"
    }
  ],
