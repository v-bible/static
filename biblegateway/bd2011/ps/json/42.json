    },
    {
      "id": "31c85897-b01e-49aa-b1fc-3cb94d2a994e",
      "content": "Lòng Khao Khát Chúa trong Cảnh Hoạn Nạn và Lưu Ðày",
      "order": 1,
      "createdAt": "2024-09-09T04:44:28.856Z",
      "updatedAt": "2024-09-09T04:44:28.856Z",
      "verseId": "ee7ee657-77d5-4916-aba1-78e005af29fc",
