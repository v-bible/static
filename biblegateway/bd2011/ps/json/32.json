{
  "chapter": {
    "id": "167af76d-40b9-47cf-8a52-d74da315724f",
    "number": 32,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2032&version=BD2011",
    "createdAt": "2024-07-12T04:03:46.214Z",
    "updatedAt": "2024-07-24T15:07:47.154Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "9532d28b-7a5a-47c3-b91c-7ca8f94f04cc",
      "number": 1,
      "content": "Phước cho người được tha những vi phạm mình,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.606Z",
      "updatedAt": "2024-07-31T11:27:05.611Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "4c861f95-a1ce-48ad-97df-c74e1b091d5e",
      "number": 1,
      "content": "Ðược khỏa lấp những tội lỗi mình!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.606Z",
      "updatedAt": "2024-07-31T11:27:05.618Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "a65d5858-8d69-4870-a7c4-70d5b7b3a1f1",
      "number": 2,
      "content": "Phước cho người được Chúa không kể tội của mình,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.616Z",
      "updatedAt": "2024-07-31T11:27:05.626Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "c9990774-1215-4b25-824e-32b6e09f049d",
      "number": 2,
      "content": "Và trong lòng không có điều chi gian dối!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.616Z",
      "updatedAt": "2024-07-31T11:27:05.636Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "d27cc704-971e-4189-8771-43c65b6e0062",
      "number": 3,
      "content": "Khi con cố nín lặng, các xương cốt con vẫn bị tiêu hao,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.618Z",
      "updatedAt": "2024-07-31T11:27:05.646Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "ec2d7572-c45d-4a6d-8cc7-b22292ac4c25",
      "number": 3,
      "content": "Bởi con phải rên rỉ suốt ngày;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.618Z",
      "updatedAt": "2024-07-31T11:27:05.653Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "1ce10683-cfaa-4d08-bd55-088d0937f6d6",
      "number": 4,
      "content": "Vì cả ngày lẫn đêm tay Ngài đè nặng trên con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.621Z",
      "updatedAt": "2024-07-31T11:27:05.661Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "b8100848-dfe6-427c-9286-2046a137910d",
      "number": 4,
      "content": "Sức sống con cạn dần như gặp hơi nóng mùa hè. (Sê-la)",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.621Z",
      "updatedAt": "2024-07-31T11:27:05.669Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "1160529e-2997-49e0-9e80-2037a64edff0",
      "number": 5,
      "content": "Con nhìn nhận những tội lỗi con đã phạm đối với Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.627Z",
      "updatedAt": "2024-07-31T11:27:05.679Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "c62c8434-c7fa-49f7-a493-8362d9a4f0b5",
      "number": 5,
      "content": "Con chẳng dám giấu giếm các tội lỗi mình;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.627Z",
      "updatedAt": "2024-07-31T11:27:05.687Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "e7830276-80ef-40f7-914e-1a984b522820",
      "number": 5,
      "content": "Con nói, “Con sẽ xưng nhận các vi phạm con với Chúa,”",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.627Z",
      "updatedAt": "2024-07-31T11:27:05.695Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "6695cc01-b661-4e84-8b7c-c086cd01bc2a",
      "number": 5,
      "content": "Và Ngài đã tha thứ các lỗi lầm vi phạm của con. (Sê-la)",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.627Z",
      "updatedAt": "2024-07-31T11:27:05.705Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "95314018-77f7-4c79-9012-84bea9045407",
      "number": 6,
      "content": "Vì vậy mong mọi người có lòng tin kính hãy cầu nguyện với Ngài khi còn có thể tìm được Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.634Z",
      "updatedAt": "2024-07-31T11:27:05.715Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "9e57072e-8378-41d7-8356-c592c7722377",
      "number": 6,
      "content": "Hầu khi những dòng nước lũ ào ạt tràn đến, chúng sẽ không tới gần họ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.634Z",
      "updatedAt": "2024-07-31T11:27:05.722Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "10d2bf15-934f-445d-9832-afddab2fb466",
      "number": 7,
      "content": "Ngài là nơi ẩn náu của con;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.640Z",
      "updatedAt": "2024-07-31T11:27:05.733Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "4ee06167-a46d-47d0-aec3-58664c9688a4",
      "number": 7,
      "content": "Ngài gìn giữ con khỏi bị hoạn nạn;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.640Z",
      "updatedAt": "2024-07-31T11:27:05.740Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "2bbdf578-703c-4728-afd7-7214eb8b9626",
      "number": 7,
      "content": "Ngài bao quanh con bằng những bài ca giải cứu. (Sê-la)",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.640Z",
      "updatedAt": "2024-07-31T11:27:05.748Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "1a07c90d-6660-4f04-8966-1ddf166383b5",
      "number": 8,
      "content": "Ta sẽ dạy bảo ngươi và chỉ cho ngươi con đường phải đi;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.647Z",
      "updatedAt": "2024-07-31T11:27:05.758Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "11b7d95f-1b16-4a5e-9861-8be63ca61875",
      "number": 8,
      "content": "Mắt Ta hằng ở với ngươi để hướng dẫn ngươi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.647Z",
      "updatedAt": "2024-07-31T11:27:05.766Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "5b5693fe-0d9e-4c02-bde1-ed560a768ae1",
      "number": 9,
      "content": "Ðừng như con ngựa hoặc con la là loài không thông sáng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.652Z",
      "updatedAt": "2024-07-31T11:27:05.775Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "d155d508-d1d5-4344-a628-887297e3bf1f",
      "number": 9,
      "content": "Ngươi phải dùng hàm khớp và dây cương để điều khiển chúng,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.652Z",
      "updatedAt": "2024-07-31T11:27:05.782Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "3096d5be-78d3-4512-93d2-c1cee1a8254a",
      "number": 9,
      "content": "Bằng không, chúng chẳng vâng lời ngươi.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.652Z",
      "updatedAt": "2024-07-31T11:27:05.790Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "9d6a38f3-c475-419f-84ec-3aa2ace64359",
      "number": 10,
      "content": "Kẻ ác gặp nhiều đau khổ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.658Z",
      "updatedAt": "2024-07-31T11:27:05.801Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "2d1003c0-f2ea-4ffb-93d1-cfef787152f5",
      "number": 10,
      "content": "Nhưng tình thương của Chúa bao bọc người tin cậy Ngài.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.658Z",
      "updatedAt": "2024-07-31T11:27:05.808Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "4d8c6165-d22a-45e7-b77d-8a663a6c0b07",
      "number": 11,
      "content": "Hỡi người ngay lành, hãy vui vẻ và hân hoan trong Chúa!",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.664Z",
      "updatedAt": "2024-07-31T11:27:05.816Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    },
    {
      "id": "e8e20e8f-35ad-4f27-befa-da6025a2bc72",
      "number": 11,
      "content": "Hỡi mọi người có lòng ngay thẳng, hãy cất tiếng reo mừng!",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:49:59.664Z",
      "updatedAt": "2024-07-31T11:27:05.824Z",
      "chapterId": "167af76d-40b9-47cf-8a52-d74da315724f"
    }
  ],
