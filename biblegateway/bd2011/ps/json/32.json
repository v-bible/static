{
  "chapter": {
    "id": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac",
    "number": 32,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2032&version=BD2011",
    "createdAt": "2024-09-09T04:12:59.788Z",
    "updatedAt": "2024-09-09T04:12:59.788Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "35cf36f7-7dff-4fb5-ab42-46359256b38c",
      "number": 1,
      "content": "Phước cho người được tha những vi phạm mình,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.861Z",
      "updatedAt": "2024-09-09T04:43:38.861Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "637c8f10-bb82-41e3-bfe0-950571319026",
      "number": 1,
      "content": "Ðược khỏa lấp những tội lỗi mình!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.879Z",
      "updatedAt": "2024-09-09T04:43:38.879Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "964964a1-1d1c-4453-92dc-33a790847d15",
      "number": 2,
      "content": "Phước cho người được Chúa không kể tội của mình,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.884Z",
      "updatedAt": "2024-09-09T04:43:38.884Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "e7116547-de3b-4ef1-a53b-73963d58bd73",
      "number": 2,
      "content": "Và trong lòng không có điều chi gian dối!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.889Z",
      "updatedAt": "2024-09-09T04:43:38.889Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "7c29e643-d994-47b5-82ae-591b4fc90ee3",
      "number": 3,
      "content": "Khi con cố nín lặng, các xương cốt con vẫn bị tiêu hao,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.895Z",
      "updatedAt": "2024-09-09T04:43:38.895Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "f1662cf2-b4c4-414b-ab5f-9e2dacf09758",
      "number": 3,
      "content": "Bởi con phải rên rỉ suốt ngày;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.900Z",
      "updatedAt": "2024-09-09T04:43:38.900Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "7b3918ef-c719-4724-8714-7ff804091919",
      "number": 4,
      "content": "Vì cả ngày lẫn đêm tay Ngài đè nặng trên con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.904Z",
      "updatedAt": "2024-09-09T04:43:38.904Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "7b5d5a14-cdcd-4a54-a09d-6e1140cb18be",
      "number": 4,
      "content": "Sức sống con cạn dần như gặp hơi nóng mùa hè. (Sê-la)",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.909Z",
      "updatedAt": "2024-09-09T04:43:38.909Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "bb8383d5-e995-4fda-8c7a-017178001940",
      "number": 5,
      "content": "Con nhìn nhận những tội lỗi con đã phạm đối với Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.914Z",
      "updatedAt": "2024-09-09T04:43:38.914Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "d85e38c6-945c-4527-8352-c483bf8c8f4d",
      "number": 5,
      "content": "Con chẳng dám giấu giếm các tội lỗi mình;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.919Z",
      "updatedAt": "2024-09-09T04:43:38.919Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "de27816e-4105-40d8-9f31-792a33510f8a",
      "number": 5,
      "content": "Con nói, “Con sẽ xưng nhận các vi phạm con với Chúa,”",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.924Z",
      "updatedAt": "2024-09-09T04:43:38.924Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "7851053a-1706-4463-a014-7623482ae028",
      "number": 5,
      "content": "Và Ngài đã tha thứ các lỗi lầm vi phạm của con. (Sê-la)",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.929Z",
      "updatedAt": "2024-09-09T04:43:38.929Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "3c80a92e-953a-4ae2-a039-b7fbcc5074ee",
      "number": 6,
      "content": "Vì vậy mong mọi người có lòng tin kính hãy cầu nguyện với Ngài khi còn có thể tìm được Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.934Z",
      "updatedAt": "2024-09-09T04:43:38.934Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "a6face66-809e-49c5-9272-80285917ccdf",
      "number": 6,
      "content": "Hầu khi những dòng nước lũ ào ạt tràn đến, chúng sẽ không tới gần họ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.940Z",
      "updatedAt": "2024-09-09T04:43:38.940Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "0ab639eb-af62-4e51-b8eb-e51c8683f615",
      "number": 7,
      "content": "Ngài là nơi ẩn náu của con;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.945Z",
      "updatedAt": "2024-09-09T04:43:38.945Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "f64d4d1f-f864-4f98-b2db-32795dad9dad",
      "number": 7,
      "content": "Ngài gìn giữ con khỏi bị hoạn nạn;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.950Z",
      "updatedAt": "2024-09-09T04:43:38.950Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "7a3c6f13-a4c5-4483-973b-4f5bfb01c363",
      "number": 7,
      "content": "Ngài bao quanh con bằng những bài ca giải cứu. (Sê-la)",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.955Z",
      "updatedAt": "2024-09-09T04:43:38.955Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "ccb6a6ac-13b8-4a0d-b177-1fd68d2807e6",
      "number": 8,
      "content": "Ta sẽ dạy bảo ngươi và chỉ cho ngươi con đường phải đi;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.961Z",
      "updatedAt": "2024-09-09T04:43:38.961Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "36e05417-f7eb-4466-a6d7-6d852ec4d4cd",
      "number": 8,
      "content": "Mắt Ta hằng ở với ngươi để hướng dẫn ngươi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.966Z",
      "updatedAt": "2024-09-09T04:43:38.966Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "4903cd2e-1a02-49ef-961c-dd6cca031cce",
      "number": 9,
      "content": "Ðừng như con ngựa hoặc con la là loài không thông sáng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.971Z",
      "updatedAt": "2024-09-09T04:43:38.971Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "f88c401e-9352-4f8b-a828-f86690884d28",
      "number": 9,
      "content": "Ngươi phải dùng hàm khớp và dây cương để điều khiển chúng,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.976Z",
      "updatedAt": "2024-09-09T04:43:38.976Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "8c60d3d1-f41a-41e6-ae9f-dd67502bb769",
      "number": 9,
      "content": "Bằng không, chúng chẳng vâng lời ngươi.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.981Z",
      "updatedAt": "2024-09-09T04:43:38.981Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "22112dec-0d5c-4741-9eda-239dd04d44b7",
      "number": 10,
      "content": "Kẻ ác gặp nhiều đau khổ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.992Z",
      "updatedAt": "2024-09-09T04:43:38.992Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "066cf92e-ac2f-4c03-817f-8f1f65005d16",
      "number": 10,
      "content": "Nhưng tình thương của Chúa bao bọc người tin cậy Ngài.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:38.997Z",
      "updatedAt": "2024-09-09T04:43:38.997Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "2d284a82-33c9-4106-ad83-780dac42c57f",
      "number": 11,
      "content": "Hỡi người ngay lành, hãy vui vẻ và hân hoan trong Chúa!",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:39.002Z",
      "updatedAt": "2024-09-09T04:43:39.002Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    },
    {
      "id": "a7011beb-3700-4d53-8531-fbfe93d22a89",
      "number": 11,
      "content": "Hỡi mọi người có lòng ngay thẳng, hãy cất tiếng reo mừng!",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:39.007Z",
      "updatedAt": "2024-09-09T04:43:39.007Z",
      "chapterId": "45bbe622-d4cc-4f3a-9292-8849f0fe6dac"
    }
  ],
