{
  "chapter": {
    "id": "248a02c6-fd1e-4da6-9f96-30c53016bd30",
    "number": 112,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20112&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.961Z",
    "updatedAt": "2024-07-24T15:07:48.838Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "69d27f1e-813a-477a-bbe0-38beda8f4736",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.868Z",
      "updatedAt": "2024-07-31T11:48:26.870Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "752aa0b4-008b-4074-9412-175d24335845",
      "number": 1,
      "content": "Phước cho người nào kính sợ Chúa,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.868Z",
      "updatedAt": "2024-07-31T11:48:26.885Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "cb35f33d-5be1-4280-ae67-0f971da3a80c",
      "number": 1,
      "content": "Người thích thú vô vàn các mạng lịnh Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.868Z",
      "updatedAt": "2024-07-31T11:48:26.894Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "84ab8a6b-24bb-4a9d-a411-41d547df8332",
      "number": 2,
      "content": "Con cháu người ấy sẽ được cường thịnh trên đất.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.879Z",
      "updatedAt": "2024-07-31T11:48:26.902Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "c7ec33b8-4b00-4f4b-b8a9-b67497ab75a4",
      "number": 2,
      "content": "Dòng dõi người chính trực sẽ được phước.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.879Z",
      "updatedAt": "2024-07-31T11:48:26.914Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "ca4e3f65-5196-4737-9442-fb673970de71",
      "number": 3,
      "content": "Giàu sang và phú quý sẽ ở trong nhà người ấy.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.884Z",
      "updatedAt": "2024-07-31T11:48:26.923Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "d6b2d1c1-7119-4956-a05f-5cfcb75bb35e",
      "number": 3,
      "content": "Ðức công chính của người ấy sẽ còn lại đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.884Z",
      "updatedAt": "2024-07-31T11:48:26.930Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "2f5d0375-2ff5-4c0c-8170-c8ab0cfaa7a0",
      "number": 4,
      "content": "Ánh sáng bừng lên trong tăm tối cho người chính trực,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.890Z",
      "updatedAt": "2024-07-31T11:48:26.940Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "8a035472-8ed7-4287-aa06-3aafc0913d9c",
      "number": 4,
      "content": "Tức cho người nhân từ, thương xót, và ngay lành;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.890Z",
      "updatedAt": "2024-07-31T11:48:26.947Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "eb886b7b-e759-4943-8ccd-631b802923be",
      "number": 5,
      "content": "Phước cho người hay làm ơn và cho vay bất vụ lợi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.896Z",
      "updatedAt": "2024-07-31T11:48:26.956Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "4ac8d23e-4d2d-4598-8b90-1475acd00337",
      "number": 5,
      "content": "Một người điều hành công việc của mình theo lẽ phải.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.896Z",
      "updatedAt": "2024-07-31T11:48:26.963Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "7e129f0e-723e-4697-bd05-118184da94b9",
      "number": 6,
      "content": "Chắc chắn người ấy sẽ không bao giờ bị lung lay.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.902Z",
      "updatedAt": "2024-07-31T11:48:26.972Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "b6db6944-08c7-4833-9473-15302c30f00a",
      "number": 6,
      "content": "Người ngay lành sẽ được tưởng nhớ đến muôn đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.902Z",
      "updatedAt": "2024-07-31T11:48:26.980Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "c0ea286e-7195-4e9e-ad17-6c1aa3498f8a",
      "number": 7,
      "content": "Người ấy sẽ không sợ các hung tin.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.907Z",
      "updatedAt": "2024-07-31T11:48:26.987Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "3c176d99-0ae7-49ae-9d22-7b7ba6ba2d7f",
      "number": 7,
      "content": "Người ấy cứ vững lòng tin cậy Chúa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.907Z",
      "updatedAt": "2024-07-31T11:48:26.995Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "79c20aea-bee3-4c93-8d06-5186b62049dc",
      "number": 8,
      "content": "Người ấy luôn an tâm và chẳng sợ hãi gì,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.913Z",
      "updatedAt": "2024-07-31T11:48:27.003Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "314a9865-4a40-431a-a43b-16b61647f97a",
      "number": 8,
      "content": "Cho đến khi người ấy nhìn kẻ thù mình một cách đắc thắng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.913Z",
      "updatedAt": "2024-07-31T11:48:27.011Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "6a4faabf-eff8-4d1a-8dba-c35e7132c3b7",
      "number": 9,
      "content": "Người ấy rải của mình ra giúp người nghèo khó.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.919Z",
      "updatedAt": "2024-07-31T11:48:27.019Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "97d91010-ec55-40a3-a4d1-ec929683f8a0",
      "number": 9,
      "content": "Ðức công chính của người ấy trường tồn mãi mãi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.919Z",
      "updatedAt": "2024-07-31T11:48:27.028Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "92c16aea-449a-41c1-a43b-d6ccd13e9a5c",
      "number": 9,
      "content": "Sừng người ấy sẽ ngước cao trong vinh hiển.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.919Z",
      "updatedAt": "2024-07-31T11:48:27.108Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "f5fa4c5b-c7a0-4344-aa76-0a9059e74340",
      "number": 10,
      "content": "Kẻ ác thấy thế bèn nổi điên tức giận.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.925Z",
      "updatedAt": "2024-07-31T11:48:27.174Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "5e3a9b78-ef3f-47f7-924f-4537087aad64",
      "number": 10,
      "content": "Nó nghiến răng tức tối rồi bị tiêu tan.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.925Z",
      "updatedAt": "2024-07-31T11:48:27.187Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    },
    {
      "id": "e1ffb865-e7c8-4c01-ae27-64e8503d7f65",
      "number": 10,
      "content": "Ước vọng của kẻ ác sẽ hoàn toàn mất tiệt.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:14.925Z",
      "updatedAt": "2024-07-31T11:48:27.199Z",
      "chapterId": "248a02c6-fd1e-4da6-9f96-30c53016bd30"
    }
  ],
