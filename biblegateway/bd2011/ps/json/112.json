{
  "chapter": {
    "id": "cce8ac42-82e4-4dbe-a73b-59b217d641cc",
    "number": 112,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20112&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.461Z",
    "updatedAt": "2024-09-09T04:13:01.461Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "6da3097d-4520-446b-b41f-b4a476b667a1",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.014Z",
      "updatedAt": "2024-09-09T04:50:18.014Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "7fc159c9-f5b8-427b-9615-4b63298cabd5",
      "number": 1,
      "content": "Phước cho người nào kính sợ Chúa,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.023Z",
      "updatedAt": "2024-09-09T04:50:18.023Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "cc3786b1-6494-4260-be5f-29b9e1cc616e",
      "number": 1,
      "content": "Người thích thú vô vàn các mạng lịnh Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.025Z",
      "updatedAt": "2024-09-09T04:50:18.025Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "28492276-932b-4c8c-9411-9c539eead492",
      "number": 2,
      "content": "Con cháu người ấy sẽ được cường thịnh trên đất.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.027Z",
      "updatedAt": "2024-09-09T04:50:18.027Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "319cb299-cb12-4b2f-88bd-7c1f66b5befe",
      "number": 2,
      "content": "Dòng dõi người chính trực sẽ được phước.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.029Z",
      "updatedAt": "2024-09-09T04:50:18.029Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "ccc84703-119d-42b7-8381-c74e75097bbf",
      "number": 3,
      "content": "Giàu sang và phú quý sẽ ở trong nhà người ấy.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.031Z",
      "updatedAt": "2024-09-09T04:50:18.031Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "3cb3c4e4-c44f-4652-9706-f1fbc60a35fc",
      "number": 3,
      "content": "Ðức công chính của người ấy sẽ còn lại đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.033Z",
      "updatedAt": "2024-09-09T04:50:18.033Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "540d98e2-e405-4222-a5b7-55fd6fb82fbf",
      "number": 4,
      "content": "Ánh sáng bừng lên trong tăm tối cho người chính trực,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.035Z",
      "updatedAt": "2024-09-09T04:50:18.035Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "79d8a345-bc8f-4820-8d6a-cc41af9eb88b",
      "number": 4,
      "content": "Tức cho người nhân từ, thương xót, và ngay lành;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.037Z",
      "updatedAt": "2024-09-09T04:50:18.037Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "28d573f2-57b2-46b0-abcc-68354f1ff613",
      "number": 5,
      "content": "Phước cho người hay làm ơn và cho vay bất vụ lợi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.039Z",
      "updatedAt": "2024-09-09T04:50:18.039Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "edd8f846-3cad-475d-a850-6bd9f9b1f807",
      "number": 5,
      "content": "Một người điều hành công việc của mình theo lẽ phải.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.042Z",
      "updatedAt": "2024-09-09T04:50:18.042Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "66a1a912-32ae-430b-9638-0421602def3e",
      "number": 6,
      "content": "Chắc chắn người ấy sẽ không bao giờ bị lung lay.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.044Z",
      "updatedAt": "2024-09-09T04:50:18.044Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "06f73cf1-3077-4271-92f5-e38aab9b41bb",
      "number": 6,
      "content": "Người ngay lành sẽ được tưởng nhớ đến muôn đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.046Z",
      "updatedAt": "2024-09-09T04:50:18.046Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "8f8a6698-db3a-4e42-b9e7-115edf5ec121",
      "number": 7,
      "content": "Người ấy sẽ không sợ các hung tin.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.049Z",
      "updatedAt": "2024-09-09T04:50:18.049Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "58624513-4577-4f32-97af-e8dfa3adbd24",
      "number": 7,
      "content": "Người ấy cứ vững lòng tin cậy Chúa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.051Z",
      "updatedAt": "2024-09-09T04:50:18.051Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "556ff051-7ac5-46e4-8f7c-c3f769581bac",
      "number": 8,
      "content": "Người ấy luôn an tâm và chẳng sợ hãi gì,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.054Z",
      "updatedAt": "2024-09-09T04:50:18.054Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "d58a3265-4251-4602-806c-d1672cde187b",
      "number": 8,
      "content": "Cho đến khi người ấy nhìn kẻ thù mình một cách đắc thắng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.056Z",
      "updatedAt": "2024-09-09T04:50:18.056Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "0ec1689b-ed5b-49d5-a468-5b7c1081efe0",
      "number": 9,
      "content": "Người ấy rải của mình ra giúp người nghèo khó.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.059Z",
      "updatedAt": "2024-09-09T04:50:18.059Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "7bcb444d-6394-44a3-b2d2-cecff41c6504",
      "number": 9,
      "content": "Ðức công chính của người ấy trường tồn mãi mãi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.062Z",
      "updatedAt": "2024-09-09T04:50:18.062Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "ad222340-feb6-4f46-95d0-197889f5a065",
      "number": 9,
      "content": "Sừng người ấy sẽ ngước cao trong vinh hiển.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.066Z",
      "updatedAt": "2024-09-09T04:50:18.066Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "4b7af4a6-f450-4560-920e-29e1376baf87",
      "number": 10,
      "content": "Kẻ ác thấy thế bèn nổi điên tức giận.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.071Z",
      "updatedAt": "2024-09-09T04:50:18.071Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "7644a1d4-cb62-48ed-939f-a70d4be9e46c",
      "number": 10,
      "content": "Nó nghiến răng tức tối rồi bị tiêu tan.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.075Z",
      "updatedAt": "2024-09-09T04:50:18.075Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    },
    {
      "id": "b7ee41bb-9e2a-42fd-a36c-c9787c3d9823",
      "number": 10,
      "content": "Ước vọng của kẻ ác sẽ hoàn toàn mất tiệt.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:18.081Z",
      "updatedAt": "2024-09-09T04:50:18.081Z",
      "chapterId": "cce8ac42-82e4-4dbe-a73b-59b217d641cc"
    }
  ],
