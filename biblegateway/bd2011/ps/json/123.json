{
  "chapter": {
    "id": "dca17142-0f88-4c37-b4bb-297879134eb3",
    "number": 123,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20123&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.191Z",
    "updatedAt": "2024-07-24T15:07:49.066Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "fd9bb6da-9457-4f8f-b110-18422801b84a",
      "number": 1,
      "content": "Lạy Ðấng ngự trên các tầng trời,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:26.123Z",
      "updatedAt": "2024-07-31T11:50:48.133Z",
      "chapterId": "dca17142-0f88-4c37-b4bb-297879134eb3"
    },
    {
      "id": "66ff5f80-3c2d-4257-a5ee-87e3bc9d1877",
      "number": 1,
      "content": "Con ngước mắt lên hướng về Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:26.123Z",
      "updatedAt": "2024-07-31T11:50:48.159Z",
      "chapterId": "dca17142-0f88-4c37-b4bb-297879134eb3"
    },
    {
      "id": "4e9e2587-3e2d-4cd9-a55f-163e7cfe4b23",
      "number": 2,
      "content": "Kìa, như mắt các đầy tớ trông nơi tay ông chủ họ thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:26.126Z",
      "updatedAt": "2024-07-31T11:50:48.177Z",
      "chapterId": "dca17142-0f88-4c37-b4bb-297879134eb3"
    },
    {
      "id": "89beb183-a312-4a85-bf9b-abe144e46f08",
      "number": 2,
      "content": "Như mắt người tớ gái trông nơi tay bà chủ nàng thể nào,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:26.126Z",
      "updatedAt": "2024-07-31T11:50:48.195Z",
      "chapterId": "dca17142-0f88-4c37-b4bb-297879134eb3"
    },
    {
      "id": "83e556f2-21d1-4130-a090-2b9079f3a464",
      "number": 2,
      "content": "Mắt chúng con cũng vọng trông nơi Chúa, Ðức Chúa Trời chúng con, thể ấy,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:26.126Z",
      "updatedAt": "2024-07-31T11:50:48.212Z",
      "chapterId": "dca17142-0f88-4c37-b4bb-297879134eb3"
    },
    {
      "id": "7a0b5b23-e25e-4ddc-9caa-9e309c05027c",
      "number": 2,
      "content": "Cho đến khi Ngài bày tỏ ơn thương xót đối với chúng con.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:26.126Z",
      "updatedAt": "2024-07-31T11:50:48.231Z",
      "chapterId": "dca17142-0f88-4c37-b4bb-297879134eb3"
    },
    {
      "id": "ad561f7b-bab0-499a-9a1a-2957d2a84956",
      "number": 3,
      "content": "Xin thương xót chúng con, Chúa ôi, xin thương xót chúng con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:26.129Z",
      "updatedAt": "2024-07-31T11:50:48.255Z",
      "chapterId": "dca17142-0f88-4c37-b4bb-297879134eb3"
    },
    {
      "id": "2cef1eba-2858-41ad-86c4-e9484586d9de",
      "number": 3,
      "content": "Vì chúng con bị khinh khi quá đỗi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:26.129Z",
      "updatedAt": "2024-07-31T11:50:48.273Z",
      "chapterId": "dca17142-0f88-4c37-b4bb-297879134eb3"
    },
    {
      "id": "855eb478-9f4d-4bae-9cce-eddee948f1ee",
      "number": 4,
      "content": "Linh hồn chúng con quá ngao ngán những lời nhạo báng của đám người kiêu ngạo,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:26.131Z",
      "updatedAt": "2024-07-31T11:50:48.288Z",
      "chapterId": "dca17142-0f88-4c37-b4bb-297879134eb3"
    },
    {
      "id": "69be7727-6976-4a5f-82e8-51cfee8b1ac7",
      "number": 4,
      "content": "Cùng với giọng khinh người của những kẻ kiêu căng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:26.131Z",
      "updatedAt": "2024-07-31T11:50:48.298Z",
      "chapterId": "dca17142-0f88-4c37-b4bb-297879134eb3"
    }
  ],
