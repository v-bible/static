{
  "chapter": {
    "id": "40aa6752-4a39-4fb7-865e-224e7a78b025",
    "number": 123,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20123&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.679Z",
