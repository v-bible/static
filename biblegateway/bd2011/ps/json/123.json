    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "61c9b572-6f78-4107-beac-0298509c945d",
      "number": 1,
      "content": "Lạy Ðấng ngự trên các tầng trời,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:12.096Z",
      "updatedAt": "2024-09-09T04:51:12.096Z",
      "chapterId": "40aa6752-4a39-4fb7-865e-224e7a78b025"
    },
    {
      "id": "c5bee0d6-3fb2-44dc-9ca9-c90c7014cf3c",
      "number": 1,
      "content": "Con ngước mắt lên hướng về Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:12.108Z",
      "updatedAt": "2024-09-09T04:51:12.108Z",
      "chapterId": "40aa6752-4a39-4fb7-865e-224e7a78b025"
    },
    {
      "id": "7a9ca80a-c1bd-4c42-84e9-49a1e049a96f",
      "number": 2,
      "content": "Kìa, như mắt các đầy tớ trông nơi tay ông chủ họ thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:12.110Z",
      "updatedAt": "2024-09-09T04:51:12.110Z",
      "chapterId": "40aa6752-4a39-4fb7-865e-224e7a78b025"
    },
    {
      "id": "603f8bde-b205-4c65-86e9-4c40aa7bc58a",
      "number": 2,
      "content": "Như mắt người tớ gái trông nơi tay bà chủ nàng thể nào,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:12.112Z",
      "updatedAt": "2024-09-09T04:51:12.112Z",
      "chapterId": "40aa6752-4a39-4fb7-865e-224e7a78b025"
    },
    {
      "id": "b2c1d5a1-fbda-4c5d-8afd-01d1a70b08c1",
      "number": 2,
      "content": "Mắt chúng con cũng vọng trông nơi Chúa, Ðức Chúa Trời chúng con, thể ấy,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:12.114Z",
      "updatedAt": "2024-09-09T04:51:12.114Z",
      "chapterId": "40aa6752-4a39-4fb7-865e-224e7a78b025"
    },
    {
      "id": "7e33a5ab-1bcd-43ab-befc-cc675cb7c95d",
      "number": 2,
      "content": "Cho đến khi Ngài bày tỏ ơn thương xót đối với chúng con.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:12.116Z",
      "updatedAt": "2024-09-09T04:51:12.116Z",
      "chapterId": "40aa6752-4a39-4fb7-865e-224e7a78b025"
    },
    {
      "id": "f08ea633-dc81-4764-bc27-195f471dc8d0",
      "number": 3,
      "content": "Xin thương xót chúng con, Chúa ôi, xin thương xót chúng con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:12.118Z",
      "updatedAt": "2024-09-09T04:51:12.118Z",
      "chapterId": "40aa6752-4a39-4fb7-865e-224e7a78b025"
    },
    {
      "id": "44b25582-2163-43be-8042-b5ecbadea9d1",
      "number": 3,
      "content": "Vì chúng con bị khinh khi quá đỗi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:12.121Z",
      "updatedAt": "2024-09-09T04:51:12.121Z",
      "chapterId": "40aa6752-4a39-4fb7-865e-224e7a78b025"
    },
    {
      "id": "30c57df6-8b95-4dbf-9422-fbdfc7023667",
      "number": 4,
      "content": "Linh hồn chúng con quá ngao ngán những lời nhạo báng của đám người kiêu ngạo,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:12.124Z",
      "updatedAt": "2024-09-09T04:51:12.124Z",
      "chapterId": "40aa6752-4a39-4fb7-865e-224e7a78b025"
    },
    {
      "id": "5dacc9ea-528d-44e4-b551-b578760dca17",
      "number": 4,
      "content": "Cùng với giọng khinh người của những kẻ kiêu căng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:12.129Z",
      "updatedAt": "2024-09-09T04:51:12.129Z",
      "chapterId": "40aa6752-4a39-4fb7-865e-224e7a78b025"
    }
  ],
