  "verses": [
    {
      "id": "704b458f-e925-4659-89eb-fda9b6ba88b6",
      "number": 1,
      "content": "Hãy cảm tạ Chúa vì Ngài rất tốt,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.309Z",
      "updatedAt": "2024-07-31T11:59:24.132Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "06bcc105-a033-402d-a7c1-e25d14daf9d8",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.309Z",
      "updatedAt": "2024-07-31T11:59:24.140Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "519e6d35-2671-4937-8b30-fbf27a5c009e",
      "number": 2,
      "content": "Hãy cảm tạ Thần của các thần,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.319Z",
      "updatedAt": "2024-07-31T11:59:24.149Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "39ac4a6e-1757-4189-a61f-0c526fba1ade",
      "number": 2,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.319Z",
      "updatedAt": "2024-07-31T11:59:24.156Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "4be80d95-ed52-4167-9fb0-955725ab556f",
      "number": 3,
      "content": "Hãy cảm tạ Chúa của các chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.321Z",
      "updatedAt": "2024-07-31T11:59:24.164Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "9111b110-abf3-4fd7-8906-6b3f16ab55a7",
      "number": 3,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.321Z",
      "updatedAt": "2024-07-31T11:59:24.172Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "0cc8b540-a220-4599-a51b-2d4be40eadfb",
      "number": 4,
      "content": "Ngài là Ðấng duy nhất làm những việc lạ lùng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.324Z",
      "updatedAt": "2024-07-31T11:59:24.182Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "9e32bb2c-cc0f-4e16-b45e-94b07c4088f6",
      "number": 4,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.324Z",
      "updatedAt": "2024-07-31T11:59:24.189Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "e3d08c24-6c7a-4c44-8052-3c12d9cbe8b4",
      "number": 5,
      "content": "Ðấng dùng sự khôn ngoan dựng nên các tầng trời,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.326Z",
      "updatedAt": "2024-07-31T11:59:24.197Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "3f7a0bb8-210a-43ab-811f-8e53a66cae4b",
      "number": 5,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.326Z",
      "updatedAt": "2024-07-31T11:59:24.206Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "c9cc6278-863a-4a1e-85cf-312ea5012f63",
      "number": 6,
      "content": "Ðấng trải đất ra trên các luồng nước biển,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.328Z",
      "updatedAt": "2024-07-31T11:59:24.214Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "fbd6ae81-9bc8-40d3-9733-de18a646645c",
      "number": 6,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.328Z",
      "updatedAt": "2024-07-31T11:59:24.221Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "b3bd9932-e71f-4347-8618-20faaee4f4cd",
      "number": 7,
      "content": "Ðấng dựng nên những vì sáng lớn,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.331Z",
      "updatedAt": "2024-07-31T11:59:24.229Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "9988a4eb-d4ce-4feb-be2b-9aac2b0afdd5",
      "number": 7,
      "content": "Vì tình thương của Ngài còn đến đời đời,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.331Z",
      "updatedAt": "2024-07-31T11:59:24.236Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "cacdbd17-1068-4e18-b4a1-15a3d6e790b0",
      "number": 8,
      "content": "Mặt trời để chi phối ban ngày,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.333Z",
      "updatedAt": "2024-07-31T11:59:24.244Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "d491e35e-6ada-4250-bd46-808d523c7f09",
      "number": 8,
      "content": "Vì tình thương của Ngài còn đến đời đời,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.333Z",
      "updatedAt": "2024-07-31T11:59:24.251Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "9785f8c2-0593-4fce-a2bb-6d533612b527",
      "number": 9,
      "content": "Mặt trăng và các ngôi sao để chi phối ban đêm,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.335Z",
      "updatedAt": "2024-07-31T11:59:24.258Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "e461dc36-ae45-4d49-941b-f6f480c557f6",
      "number": 9,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.335Z",
      "updatedAt": "2024-07-31T11:59:24.266Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "b2b92fdd-4fbc-4f17-96af-38fbc909eb60",
      "number": 10,
      "content": "Ðấng đánh hạ các con đầu lòng của Ai-cập,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.337Z",
      "updatedAt": "2024-07-31T11:59:24.275Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "bc2c8f06-ad85-4118-bfc4-21d87bdaf98b",
      "number": 10,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.337Z",
      "updatedAt": "2024-07-31T11:59:24.282Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "7177bb73-ede8-4e72-b0ed-2aac799bfdac",
      "number": 11,
      "content": "Ðấng đem dân I-sơ-ra-ên ra khỏi xứ ấy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
