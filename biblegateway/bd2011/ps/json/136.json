      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.383Z",
      "updatedAt": "2024-07-31T11:59:24.480Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "0c8bfb95-28a8-4ce9-8dff-59c498f53fe8",
      "number": 21,
      "content": "Rồi ban xứ của họ để làm sản nghiệp,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 34,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.390Z",
      "updatedAt": "2024-07-31T11:59:24.501Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "cd5aa643-3de2-4562-b601-f9cb6665f0a0",
      "number": 21,
      "content": "Vì tình thương của Ngài còn đến đời đời,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 35,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.390Z",
      "updatedAt": "2024-07-31T11:59:24.513Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "6cc90c76-a3e9-4761-bc12-7f32ce3a4662",
      "number": 22,
      "content": "Tức làm sản nghiệp cho I-sơ-ra-ên tôi tớ Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 36,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.401Z",
      "updatedAt": "2024-07-31T11:59:24.525Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "cd640579-aead-4739-abcf-11f442a57fb7",
      "number": 22,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 37,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.401Z",
      "updatedAt": "2024-07-31T11:59:24.655Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "7d753607-7c92-4a39-b106-250e0a2f829f",
      "number": 23,
      "content": "Ðấng nhớ đến thân phận thấp hèn của chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.406Z",
      "updatedAt": "2024-07-31T11:59:24.686Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "53250fd2-fa2b-45cc-a7d0-afee0eb50c71",
      "number": 23,
      "content": "Vì tình thương của Ngài còn đến đời đời,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.406Z",
      "updatedAt": "2024-07-31T11:59:24.703Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "f3937d04-2103-472f-89f6-d491fef7933a",
      "number": 24,
      "content": "Và giải cứu chúng ta khỏi những kẻ thù của chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.411Z",
      "updatedAt": "2024-07-31T11:59:24.738Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "ae160c0c-1e71-4224-99a8-577b28ea74ec",
      "number": 24,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.411Z",
      "updatedAt": "2024-07-31T11:59:24.769Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "c263b9c6-a423-4f42-a49b-ff926cdfd922",
      "number": 25,
      "content": "Ðấng ban thực phẩm cho mọi loài xác thịt,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.415Z",
      "updatedAt": "2024-07-31T11:59:24.785Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "31794a59-2f37-40ca-8246-e8719512c745",
      "number": 25,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.415Z",
      "updatedAt": "2024-07-31T11:59:24.826Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "097514c1-32c4-439c-83c6-90eb23998ee9",
      "number": 26,
      "content": "Hãy cảm tạ Ðức Chúa Trời của các tầng trời,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.419Z",
      "updatedAt": "2024-07-31T11:59:24.857Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "15f69a2b-ac99-4b5e-966a-539cae865f6f",
      "number": 26,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:15.419Z",
      "updatedAt": "2024-07-31T11:59:24.874Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    }
  ],
