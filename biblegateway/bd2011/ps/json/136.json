      "updatedAt": "2024-07-31T11:59:24.352Z",
      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "76e54353-766d-498b-87df-bbac7bb6b2bd",
      "number": 15,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 23,
      "isPoetry": true,
