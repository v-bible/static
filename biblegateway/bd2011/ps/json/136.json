      "chapterId": "f902d4df-954e-4c09-b1e5-ae096de0e873"
    },
    {
      "id": "fb3e6588-8f1a-4e99-8ae9-3b7b73c58780",
      "number": 15,
      "content": "Nhưng Ngài lật đổ Pha-ra-ôn và đạo quân vua ấy xuống Hồng Hải,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 22,
