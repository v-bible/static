{
  "chapter": {
    "id": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a",
    "number": 121,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20121&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.640Z",
    "updatedAt": "2024-09-09T04:13:01.640Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "c4cb5b33-f160-4059-a53d-6d68c7e7dca7",
      "number": 1,
      "content": "Tôi ngước mắt nhìn lên đồi núi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.597Z",
      "updatedAt": "2024-09-09T04:51:03.597Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "8c452e07-f735-4a73-aeb2-130ea05fa1ae",
      "number": 1,
      "content": "Sự giúp đỡ tôi đến từ đâu?",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.615Z",
      "updatedAt": "2024-09-09T04:51:03.615Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "0f0a799f-422e-42bd-b1c2-569a0dab576b",
      "number": 2,
      "content": "Sự giúp đỡ tôi đến từ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.619Z",
      "updatedAt": "2024-09-09T04:51:03.619Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "aaa9fdbe-2ca3-483f-ad84-6fab881127e2",
      "number": 2,
      "content": "Ðấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.624Z",
      "updatedAt": "2024-09-09T04:51:03.624Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "3ac8825d-a82b-4840-88c4-db15a6f308a7",
      "number": 3,
      "content": "Ngài sẽ không để cho chân bạn trợt ngã;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.629Z",
      "updatedAt": "2024-09-09T04:51:03.629Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "6dc3bd03-7714-4a80-943b-518901c8c91b",
      "number": 3,
      "content": "Ðấng gìn giữ bạn không hề buồn ngủ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.633Z",
      "updatedAt": "2024-09-09T04:51:03.633Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "6d1b6203-8dc7-4ea3-984a-7a9f609b2c5d",
      "number": 4,
      "content": "Thật vậy Ðấng gìn giữ I-sơ-ra-ên không hề buồn ngủ và không bao giờ ngủ.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.638Z",
      "updatedAt": "2024-09-09T04:51:03.638Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "6fdf29ed-19ed-4c38-a8dd-0aa950ab6631",
      "number": 5,
      "content": "Chúa là Ðấng gìn giữ bạn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.643Z",
      "updatedAt": "2024-09-09T04:51:03.643Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "6e800eac-3ccc-4cfb-87a5-cc0ef0f9f464",
      "number": 5,
      "content": "Chúa là bóng mát bên phải bạn.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.647Z",
      "updatedAt": "2024-09-09T04:51:03.647Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "bc225092-ea29-4ad5-bcbe-7f1746c0be34",
      "number": 6,
      "content": "Mặt trời sẽ không hại bạn ban ngày;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.652Z",
      "updatedAt": "2024-09-09T04:51:03.652Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "33c7be4b-af41-4986-95d8-f72cc7883a1f",
      "number": 6,
      "content": "Mặt trăng sẽ không hại bạn ban đêm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.656Z",
      "updatedAt": "2024-09-09T04:51:03.656Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "a59becf9-7698-45d2-9581-fcb70b52c6be",
      "number": 7,
      "content": "Chúa sẽ gìn giữ bạn khỏi mọi tai họa;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.661Z",
      "updatedAt": "2024-09-09T04:51:03.661Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "bc460034-153b-42ae-a0f0-a463404b5e06",
      "number": 7,
      "content": "Ngài sẽ gìn giữ mạng sống bạn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.665Z",
      "updatedAt": "2024-09-09T04:51:03.665Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "e1a9d67c-8f96-4d03-a59e-7d94360b6a4c",
      "number": 8,
      "content": "Chúa sẽ gìn giữ bạn khi ra khi vào,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.670Z",
      "updatedAt": "2024-09-09T04:51:03.670Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    },
    {
      "id": "24575dd4-2ab3-4860-b37c-8fdda89272c4",
      "number": 8,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:03.675Z",
      "updatedAt": "2024-09-09T04:51:03.675Z",
      "chapterId": "6c79a5ab-bdc0-4c29-85e3-0b0b10bc8c2a"
    }
  ],
