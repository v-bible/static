{
  "chapter": {
    "id": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d",
    "number": 121,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20121&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.148Z",
    "updatedAt": "2024-07-24T15:07:49.015Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "4244bae3-8668-4bb4-8c1b-5b4de68b4b4f",
      "number": 1,
      "content": "Tôi ngước mắt nhìn lên đồi núi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.629Z",
      "updatedAt": "2024-07-31T11:50:24.800Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "f5a75173-50f6-49aa-a438-f732f3710947",
      "number": 1,
      "content": "Sự giúp đỡ tôi đến từ đâu?",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.629Z",
      "updatedAt": "2024-07-31T11:50:24.816Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "790de0ce-6810-496b-9f2a-012c255a3fad",
      "number": 2,
      "content": "Sự giúp đỡ tôi đến từ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.632Z",
      "updatedAt": "2024-07-31T11:50:24.826Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "0b96d8ca-5f35-49b0-bc85-1faad4d6d943",
      "number": 2,
      "content": "Ðấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.632Z",
      "updatedAt": "2024-07-31T11:50:24.836Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "b824219f-4aef-44ef-a47b-2c1999dde36b",
      "number": 3,
      "content": "Ngài sẽ không để cho chân bạn trợt ngã;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.635Z",
      "updatedAt": "2024-07-31T11:50:24.847Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "fb32012a-af3d-4670-9160-563e952e0ec6",
      "number": 3,
      "content": "Ðấng gìn giữ bạn không hề buồn ngủ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.635Z",
      "updatedAt": "2024-07-31T11:50:24.856Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "4acd6447-7cad-40f1-824a-db8305285e64",
      "number": 4,
      "content": "Thật vậy Ðấng gìn giữ I-sơ-ra-ên không hề buồn ngủ và không bao giờ ngủ.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.637Z",
      "updatedAt": "2024-07-31T11:50:24.865Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "18def50f-a68c-4ecb-9b77-6bb7e12f90b4",
      "number": 5,
      "content": "Chúa là Ðấng gìn giữ bạn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.640Z",
      "updatedAt": "2024-07-31T11:50:24.876Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "b5b565a2-6202-456a-8cc1-1115e986005f",
      "number": 5,
      "content": "Chúa là bóng mát bên phải bạn.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.640Z",
      "updatedAt": "2024-07-31T11:50:24.885Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "fff59431-705f-47d0-92e7-6de8bef3ced5",
      "number": 6,
      "content": "Mặt trời sẽ không hại bạn ban ngày;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.642Z",
      "updatedAt": "2024-07-31T11:50:24.894Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "593bbee1-0534-41c1-a3bd-216a7193c7a9",
      "number": 6,
      "content": "Mặt trăng sẽ không hại bạn ban đêm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.642Z",
      "updatedAt": "2024-07-31T11:50:24.902Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "b1e5514b-627a-4b0c-83cc-d579df1fd210",
      "number": 7,
      "content": "Chúa sẽ gìn giữ bạn khỏi mọi tai họa;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.645Z",
      "updatedAt": "2024-07-31T11:50:24.913Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "2994ad11-0898-49c8-a361-2de0596a4a7d",
      "number": 7,
      "content": "Ngài sẽ gìn giữ mạng sống bạn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.645Z",
      "updatedAt": "2024-07-31T11:50:24.922Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "88812e57-3fd6-45c3-87a6-8c4a19f427b0",
      "number": 8,
      "content": "Chúa sẽ gìn giữ bạn khi ra khi vào,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.647Z",
      "updatedAt": "2024-07-31T11:50:24.930Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    },
    {
      "id": "a0c4ab30-879e-4862-bc2b-517055baa0e6",
      "number": 8,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:03.647Z",
      "updatedAt": "2024-07-31T11:50:24.938Z",
      "chapterId": "d811e3e1-4c67-4380-90c4-ba159d9c8f9d"
    }
  ],
