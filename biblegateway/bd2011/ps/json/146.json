{
  "chapter": {
    "id": "15320260-5a46-4915-b2f6-ebaa04113dc6",
    "number": 146,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20146&version=BD2011",
    "createdAt": "2024-09-09T04:13:02.156Z",
    "updatedAt": "2024-09-09T04:13:02.156Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "59b57a59-a7dc-40eb-b69b-9ac4e88a0521",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.370Z",
      "updatedAt": "2024-09-09T04:53:06.370Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "1b6a6a07-9260-49aa-a63f-b0666687533a",
      "number": 10,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.452Z",
      "updatedAt": "2024-09-09T04:53:06.452Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "1f5bcfea-cbb6-4d52-9c19-d794a5514da6",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.381Z",
      "updatedAt": "2024-09-09T04:53:06.381Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "790946c1-d55a-4ccd-b18e-ff4ce73cda6c",
      "number": 2,
      "content": "Tôi sẽ ca ngợi Chúa trọn đời mình;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.383Z",
      "updatedAt": "2024-09-09T04:53:06.383Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "a7bcacb2-b3b8-48bb-8877-2bbed4e6c02a",
      "number": 2,
      "content": "Hễ tôi còn sống bao lâu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.385Z",
      "updatedAt": "2024-09-09T04:53:06.385Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "a8f60203-4e1a-49d8-a7c0-edf0eadcac29",
      "number": 2,
      "content": "Tôi sẽ ca hát tôn ngợi Ðức Chúa Trời của tôi bấy lâu.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.388Z",
      "updatedAt": "2024-09-09T04:53:06.388Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "e0f6a379-d134-4f03-b6ed-fc029f4750ea",
      "number": 3,
      "content": "Ðừng để lòng tin cậy vào những người quyền thế;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.390Z",
      "updatedAt": "2024-09-09T04:53:06.390Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "b2c22886-d946-4aeb-915f-792f0ca816e6",
      "number": 3,
      "content": "Ðừng tin cậy loài người là những kẻ không đủ khả năng cứu giúp.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.392Z",
      "updatedAt": "2024-09-09T04:53:06.392Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "e2d35546-a5de-4ddf-add1-b7aaa5fb94c9",
      "number": 4,
      "content": "Hơi thở tắt đi, thân xác nó liền trở về cát bụi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.395Z",
      "updatedAt": "2024-09-09T04:53:06.395Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "ec6f33a2-10ba-42f9-b8e7-42cecc8ba9e3",
      "number": 4,
      "content": "Trong chính ngày đó các toan tính của nó đều tiêu tan.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.397Z",
      "updatedAt": "2024-09-09T04:53:06.397Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "15aa9416-1f85-4f2b-9489-d0602accaa71",
      "number": 5,
      "content": "Phước cho người được Ðức Chúa Trời của Gia-cốp giúp đỡ mình,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.400Z",
      "updatedAt": "2024-09-09T04:53:06.400Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "13c716c8-dece-4674-8281-927d58d99f03",
      "number": 5,
      "content": "Người đặt hy vọng nơi Chúa, Ðức Chúa Trời mình.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.402Z",
      "updatedAt": "2024-09-09T04:53:06.402Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "f486175f-377e-4130-8707-a3859f74cf4a",
      "number": 6,
      "content": "Ngài là Ðấng dựng nên trời và đất,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.405Z",
      "updatedAt": "2024-09-09T04:53:06.405Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "83d83a62-f679-4d43-841f-c26e2c13165e",
      "number": 6,
      "content": "Biển và mọi vật trong đó.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.408Z",
      "updatedAt": "2024-09-09T04:53:06.408Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "3aa0a4d9-b089-460a-b87c-60f618ac8e3a",
      "number": 6,
      "content": "Ngài giữ sự thành tín đến đời đời.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.411Z",
      "updatedAt": "2024-09-09T04:53:06.411Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "88d606bc-9690-49b7-9eb7-8e54a4a204e6",
      "number": 7,
      "content": "Ngài thực thi công lý cho người bị áp bức;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.414Z",
      "updatedAt": "2024-09-09T04:53:06.414Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "01d079e9-b532-4a21-9688-6fc632d81223",
      "number": 7,
      "content": "Ngài ban bánh cho người đói.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.417Z",
      "updatedAt": "2024-09-09T04:53:06.417Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "d477919e-e35b-490d-b7df-691786ae4b4e",
      "number": 7,
      "content": "Chúa giải thoát người bị giam cầm.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.420Z",
      "updatedAt": "2024-09-09T04:53:06.420Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "a8f75454-6b86-470e-9392-de368e8631e4",
      "number": 8,
      "content": "Chúa mở mắt người bị mù;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.425Z",
      "updatedAt": "2024-09-09T04:53:06.425Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "ee96a061-772c-4f9b-a631-f93641ab54a8",
      "number": 8,
      "content": "Chúa đỡ dậy những ai bị bắt phải sấp mình;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.428Z",
      "updatedAt": "2024-09-09T04:53:06.428Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "a07311b9-6927-4748-9d01-6b2f6db5b1e7",
      "number": 8,
      "content": "Chúa yêu mến người ngay lành;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.431Z",
      "updatedAt": "2024-09-09T04:53:06.431Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "51f585ef-9aff-44c7-ae20-e73914c9189c",
      "number": 9,
      "content": "Chúa bảo vệ những kiều dân;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.434Z",
      "updatedAt": "2024-09-09T04:53:06.434Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "3564d448-4565-411e-8994-797f1b813838",
      "number": 9,
      "content": "Ngài nâng đỡ cô nhi và quả phụ;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.438Z",
      "updatedAt": "2024-09-09T04:53:06.438Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "1f62d340-959e-41b9-a3eb-31523d201879",
      "number": 9,
      "content": "Nhưng Ngài phá hỏng đường lối của những kẻ ác.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.441Z",
      "updatedAt": "2024-09-09T04:53:06.441Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "3b1cdb54-108b-4379-aa0e-8f2fa93210c5",
      "number": 10,
      "content": "Chúa sẽ trị vì đời đời;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.445Z",
      "updatedAt": "2024-09-09T04:53:06.445Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    },
    {
      "id": "d2e6fbdd-a410-4053-b61a-9bf1681285b9",
      "number": 10,
      "content": "Hỡi Si-ôn, Ðức Chúa Trời của ngươi sẽ trị vì đời đời.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:06.449Z",
      "updatedAt": "2024-09-09T04:53:06.449Z",
      "chapterId": "15320260-5a46-4915-b2f6-ebaa04113dc6"
    }
  ],
