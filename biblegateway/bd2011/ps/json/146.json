{
  "chapter": {
    "id": "4a7ed617-184d-428b-be21-60c202311e63",
    "number": 146,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20146&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.711Z",
    "updatedAt": "2024-07-24T15:07:49.543Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "b3eca719-865d-4003-b734-b885b32a365d",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.312Z",
      "updatedAt": "2024-07-31T11:55:30.609Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "7f582635-07ef-4aa8-8e4a-1f1cef933ca4",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.312Z",
      "updatedAt": "2024-07-31T11:55:30.624Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "2c836f4d-4b09-4fcf-9cae-7ca5dba8b9ea",
      "number": 2,
      "content": "Tôi sẽ ca ngợi Chúa trọn đời mình;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.322Z",
      "updatedAt": "2024-07-31T11:55:30.632Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "147ca18d-c2b8-440a-bbb1-5b30a6dd911f",
      "number": 2,
      "content": "Hễ tôi còn sống bao lâu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.322Z",
      "updatedAt": "2024-07-31T11:55:30.640Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "b1296bf7-df72-4164-bee0-52a4bd797c60",
      "number": 2,
      "content": "Tôi sẽ ca hát tôn ngợi Ðức Chúa Trời của tôi bấy lâu.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.322Z",
      "updatedAt": "2024-07-31T11:55:30.648Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "5e185281-ef21-42c2-bd55-85b8b91a2a4e",
      "number": 3,
      "content": "Ðừng để lòng tin cậy vào những người quyền thế;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.325Z",
      "updatedAt": "2024-07-31T11:55:30.658Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "dda3e323-e7b9-4328-a765-3c265f86bd62",
      "number": 3,
      "content": "Ðừng tin cậy loài người là những kẻ không đủ khả năng cứu giúp.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.325Z",
      "updatedAt": "2024-07-31T11:55:30.665Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "0f464a85-6d22-4714-a795-509caaae8fdd",
      "number": 4,
      "content": "Hơi thở tắt đi, thân xác nó liền trở về cát bụi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.327Z",
      "updatedAt": "2024-07-31T11:55:30.673Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "8bf3c699-2e67-4ca4-b28f-63fe357c308a",
      "number": 4,
      "content": "Trong chính ngày đó các toan tính của nó đều tiêu tan.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.327Z",
      "updatedAt": "2024-07-31T11:55:30.681Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "de28add3-04c7-4f0a-9f27-6d858626b993",
      "number": 5,
      "content": "Phước cho người được Ðức Chúa Trời của Gia-cốp giúp đỡ mình,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.329Z",
      "updatedAt": "2024-07-31T11:55:30.692Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "fc4fb95b-1463-4248-beff-8cecc12e7ecc",
      "number": 5,
      "content": "Người đặt hy vọng nơi Chúa, Ðức Chúa Trời mình.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.329Z",
      "updatedAt": "2024-07-31T11:55:30.699Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "c0e73400-5881-4be2-b2f3-8f5ed75fb75a",
      "number": 6,
      "content": "Ngài là Ðấng dựng nên trời và đất,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.332Z",
      "updatedAt": "2024-07-31T11:55:30.728Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "234f06e8-d7d2-42d6-a59c-58b1d7b0f53a",
      "number": 6,
      "content": "Biển và mọi vật trong đó.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.332Z",
      "updatedAt": "2024-07-31T11:55:30.750Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "bc0341a4-3b85-43ef-bfd9-8b2d7dbb5164",
      "number": 6,
      "content": "Ngài giữ sự thành tín đến đời đời.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.332Z",
      "updatedAt": "2024-07-31T11:55:30.772Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "5ae1abba-8270-44df-b6b9-90b662c24562",
      "number": 7,
      "content": "Ngài thực thi công lý cho người bị áp bức;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.334Z",
      "updatedAt": "2024-07-31T11:55:30.794Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "c96b8f71-0309-4280-994e-aed2081d5286",
      "number": 7,
      "content": "Ngài ban bánh cho người đói.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.334Z",
      "updatedAt": "2024-07-31T11:55:30.817Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "146d9053-61d6-4931-93dd-a19c3c2fe414",
      "number": 7,
      "content": "Chúa giải thoát người bị giam cầm.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.334Z",
      "updatedAt": "2024-07-31T11:55:30.839Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "849d3361-c27b-4324-a9d7-31cb0b7dadd8",
      "number": 8,
      "content": "Chúa mở mắt người bị mù;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.336Z",
      "updatedAt": "2024-07-31T11:55:30.862Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "4573fd77-3d67-4541-b959-149cae912901",
      "number": 8,
      "content": "Chúa đỡ dậy những ai bị bắt phải sấp mình;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.336Z",
      "updatedAt": "2024-07-31T11:55:30.884Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "b5ecdb03-1e9e-4149-9ba9-22a2adb69ba5",
      "number": 8,
      "content": "Chúa yêu mến người ngay lành;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.336Z",
      "updatedAt": "2024-07-31T11:55:30.900Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "6a201fbb-8f09-40ff-b966-ebf6e685cfb4",
      "number": 9,
      "content": "Chúa bảo vệ những kiều dân;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.342Z",
      "updatedAt": "2024-07-31T11:55:31.011Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "e86de709-ec34-4001-a7ee-56fe1b1731f4",
      "number": 9,
      "content": "Ngài nâng đỡ cô nhi và quả phụ;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.342Z",
      "updatedAt": "2024-07-31T11:55:31.035Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "6381ca31-c442-4e1f-9ce7-3be56300dd80",
      "number": 9,
      "content": "Nhưng Ngài phá hỏng đường lối của những kẻ ác.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.342Z",
      "updatedAt": "2024-07-31T11:55:31.045Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "cab8d09a-f9df-4139-9b92-7edeccacfd26",
      "number": 10,
      "content": "Chúa sẽ trị vì đời đời;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.345Z",
      "updatedAt": "2024-07-31T11:55:31.058Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "0bef1284-973c-4060-b1bb-cdc5eac0dda3",
      "number": 10,
      "content": "Hỡi Si-ôn, Ðức Chúa Trời của ngươi sẽ trị vì đời đời.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.345Z",
      "updatedAt": "2024-07-31T11:55:31.078Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    },
    {
      "id": "930d739b-7cfb-4f09-ae93-57409f162a5e",
      "number": 10,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:20.345Z",
      "updatedAt": "2024-07-31T11:55:31.092Z",
      "chapterId": "4a7ed617-184d-428b-be21-60c202311e63"
    }
  ],
