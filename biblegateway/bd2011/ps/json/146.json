{
  "chapter": {
    "id": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9",
    "number": 146,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20146&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.356Z",
    "updatedAt": "2024-09-24T01:00:45.356Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "2a29167a-d246-48d2-be19-5f609df2e01f",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.609Z",
      "updatedAt": "2024-09-24T02:13:11.609Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "6bfaa271-6886-4c22-9565-365995a74157",
      "number": 10,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.730Z",
      "updatedAt": "2024-09-24T02:13:11.730Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "9f6ed477-c792-4eac-a3cc-26ea58bb2409",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.621Z",
      "updatedAt": "2024-09-24T02:13:11.621Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "3e7dabf4-1d4c-475e-83ed-b3060cd56b79",
      "number": 2,
      "content": "Tôi sẽ ca ngợi Chúa trọn đời mình;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.623Z",
      "updatedAt": "2024-09-24T02:13:11.623Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "41c91474-29a5-4f7e-8e19-8a32e2081d31",
      "number": 2,
      "content": "Hễ tôi còn sống bao lâu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.625Z",
      "updatedAt": "2024-09-24T02:13:11.625Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "7edbbd23-f3b3-42f4-8f28-4554ea68bf91",
      "number": 2,
      "content": "Tôi sẽ ca hát tôn ngợi Ðức Chúa Trời của tôi bấy lâu.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.628Z",
      "updatedAt": "2024-09-24T02:13:11.628Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "b543b69b-9dd1-46a7-bf3b-8867b27ba591",
      "number": 3,
      "content": "Ðừng để lòng tin cậy vào những người quyền thế;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.633Z",
      "updatedAt": "2024-09-24T02:13:11.633Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "19f2bb4a-9808-44f7-9334-b181cb6c226a",
      "number": 3,
      "content": "Ðừng tin cậy loài người là những kẻ không đủ khả năng cứu giúp.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.638Z",
      "updatedAt": "2024-09-24T02:13:11.638Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "108230ef-272d-4704-b89d-a423848a5556",
      "number": 4,
      "content": "Hơi thở tắt đi, thân xác nó liền trở về cát bụi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.643Z",
      "updatedAt": "2024-09-24T02:13:11.643Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "5a189425-0a6d-4209-8f24-5fa0cc97a1f0",
      "number": 4,
      "content": "Trong chính ngày đó các toan tính của nó đều tiêu tan.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.648Z",
      "updatedAt": "2024-09-24T02:13:11.648Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "6f3735fc-1801-48c3-9ec0-e6f02fd93c15",
      "number": 5,
      "content": "Phước cho người được Ðức Chúa Trời của Gia-cốp giúp đỡ mình,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.653Z",
      "updatedAt": "2024-09-24T02:13:11.653Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "f45fcef3-508b-41f6-b618-9d8c74281c83",
      "number": 5,
      "content": "Người đặt hy vọng nơi Chúa, Ðức Chúa Trời mình.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.658Z",
      "updatedAt": "2024-09-24T02:13:11.658Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "2e008744-eb7d-48b7-b56a-1dc76195e865",
      "number": 6,
      "content": "Ngài là Ðấng dựng nên trời và đất,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.662Z",
      "updatedAt": "2024-09-24T02:13:11.662Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "25e2a35c-195f-49ee-80de-926c4ebc573f",
      "number": 6,
      "content": "Biển và mọi vật trong đó.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.667Z",
      "updatedAt": "2024-09-24T02:13:11.667Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "0466db4e-ee6d-4729-a13f-84154e6fc0e4",
      "number": 6,
      "content": "Ngài giữ sự thành tín đến đời đời.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.672Z",
      "updatedAt": "2024-09-24T02:13:11.672Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "2fede6eb-4410-4d6c-b194-c4d599b924a1",
      "number": 7,
      "content": "Ngài thực thi công lý cho người bị áp bức;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.677Z",
      "updatedAt": "2024-09-24T02:13:11.677Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "2604af20-0845-4a45-aadc-0be671d51f96",
      "number": 7,
      "content": "Ngài ban bánh cho người đói.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.682Z",
      "updatedAt": "2024-09-24T02:13:11.682Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "8d186d10-51f5-40f7-8383-96842682a2f3",
      "number": 7,
      "content": "Chúa giải thoát người bị giam cầm.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.687Z",
      "updatedAt": "2024-09-24T02:13:11.687Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "a7287df5-3c61-4d8d-9278-cc1a031cb216",
      "number": 8,
      "content": "Chúa mở mắt người bị mù;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.692Z",
      "updatedAt": "2024-09-24T02:13:11.692Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "4ac2788d-141f-4a94-8f5d-7c47fe253a57",
      "number": 8,
      "content": "Chúa đỡ dậy những ai bị bắt phải sấp mình;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.696Z",
      "updatedAt": "2024-09-24T02:13:11.696Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "4016e317-5c1b-4269-b094-f2c2e84b0cdc",
      "number": 8,
      "content": "Chúa yêu mến người ngay lành;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.701Z",
      "updatedAt": "2024-09-24T02:13:11.701Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "84c85126-438a-4251-a614-1c3d32e48be2",
      "number": 9,
      "content": "Chúa bảo vệ những kiều dân;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.706Z",
      "updatedAt": "2024-09-24T02:13:11.706Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "39d11ada-e3bb-476e-a4ae-da1d58ac337f",
      "number": 9,
      "content": "Ngài nâng đỡ cô nhi và quả phụ;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.711Z",
      "updatedAt": "2024-09-24T02:13:11.711Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "fb84d9fa-9ef4-4834-981b-6f0d7f638b7d",
      "number": 9,
      "content": "Nhưng Ngài phá hỏng đường lối của những kẻ ác.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.715Z",
      "updatedAt": "2024-09-24T02:13:11.715Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "53a6bbfd-69d8-4397-8b3e-84c90ab3d2ab",
      "number": 10,
      "content": "Chúa sẽ trị vì đời đời;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.720Z",
      "updatedAt": "2024-09-24T02:13:11.720Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    },
    {
      "id": "92663e5b-a4e0-409f-b6b1-5fc8ed5d626b",
      "number": 10,
      "content": "Hỡi Si-ôn, Ðức Chúa Trời của ngươi sẽ trị vì đời đời.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:11.725Z",
      "updatedAt": "2024-09-24T02:13:11.725Z",
      "chapterId": "41faf5ec-9f14-43a2-b928-3b8fb5349ac9"
    }
  ],
