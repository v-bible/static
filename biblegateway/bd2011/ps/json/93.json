{
  "chapter": {
    "id": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362",
    "number": 93,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2093&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.531Z",
    "updatedAt": "2024-07-24T15:07:48.433Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "8d5f6a82-90eb-4d7d-81c3-8b520216fb68",
      "number": 1,
      "content": "Chúa trị vì, Ngài đã mặc lấy cho Ngài vẻ uy nghi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.674Z",
      "updatedAt": "2024-07-31T11:43:01.395Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "9e4f57f6-9b1b-4a21-97c1-c120b49800d4",
      "number": 1,
      "content": "Chúa đã khoát nó trên mình Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.674Z",
      "updatedAt": "2024-07-31T11:43:01.409Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "f245a9d2-a813-44a4-8deb-baa161237135",
      "number": 1,
      "content": "Ngài đã nai nịt cho Ngài bằng quyền năng.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.674Z",
      "updatedAt": "2024-07-31T11:43:01.417Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "1102710a-a761-41a1-b8d2-f7cc809cfe3e",
      "number": 1,
      "content": "Ngài đã lập trái đất được vững vàng;",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.674Z",
      "updatedAt": "2024-07-31T11:43:01.424Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "2c4da3f4-693b-4512-94f0-36a8fc847772",
      "number": 1,
      "content": "Nó sẽ không bị rúng động.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.674Z",
      "updatedAt": "2024-07-31T11:43:01.431Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "8b916165-a1db-4be5-8c5a-f521d5550bd6",
      "number": 2,
      "content": "Ngai của Ngài đã được thiết lập tự ngàn xưa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.684Z",
      "updatedAt": "2024-07-31T11:43:01.438Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "ce3d836c-7b13-45b0-b3fd-84218e007f70",
      "number": 2,
      "content": "Ngài đã hiện hữu từ vô cực trong quá khứ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.684Z",
      "updatedAt": "2024-07-31T11:43:01.446Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "967a5be0-a31b-4708-ae10-f249ec37bcda",
      "number": 3,
      "content": "Ðại dương đã dâng nước lên, Chúa ôi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.688Z",
      "updatedAt": "2024-07-31T11:43:01.455Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "9bc57191-2fd0-4b85-afe5-dbdd56dd3a05",
      "number": 3,
      "content": "Ðại dương đã trỗi tiếng gầm vang;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.688Z",
      "updatedAt": "2024-07-31T11:43:01.462Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "51c2a451-baa4-4654-a23d-d41bf682f203",
      "number": 3,
      "content": "Ðại dương đã nổi dậy với hàng hàng lớp lớp ba đào dồn dập.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.688Z",
      "updatedAt": "2024-07-31T11:43:01.470Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "5d33f1d4-6f67-4290-878e-3548dab4dd76",
      "number": 4,
      "content": "Ðấng Toàn Năng đang ngự trên cao chính là Chúa;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.693Z",
      "updatedAt": "2024-07-31T11:43:01.477Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "b297d22b-650b-44e2-b634-f702c3df2edb",
      "number": 4,
      "content": "Uy quyền Ngài mạnh hơn tiếng gầm thét của đại dương;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.693Z",
      "updatedAt": "2024-07-31T11:43:01.485Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "15fbab65-3d34-44ad-be6e-699cb3c73e63",
      "number": 4,
      "content": "Thật mạnh hơn những ngọn sóng bạc đầu của biển cả.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.693Z",
      "updatedAt": "2024-07-31T11:43:01.493Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "5d1b49a9-5967-4874-a9a1-021fba8d87c2",
      "number": 5,
      "content": "Các định luật của Ngài thật vững chãi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.697Z",
      "updatedAt": "2024-07-31T11:43:01.503Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    },
    {
      "id": "4dcfd107-a423-4e7d-a96d-4f24e78f5eee",
      "number": 5,
      "content": "Sự thánh khiết thật thích hiệp với nhà Ngài cho đến đời đời, Chúa ôi.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:03:17.697Z",
      "updatedAt": "2024-07-31T11:43:01.511Z",
      "chapterId": "c40be14f-7c8f-4d5b-97fb-91e28f5c5362"
    }
  ],
