{
  "chapter": {
    "id": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd",
    "number": 128,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20128&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.295Z",
    "updatedAt": "2024-07-24T15:07:49.177Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "bbf6618f-3a85-46d0-a799-6b04ef3a8608",
      "number": 1,
      "content": "Phước cho người kính sợ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:41.067Z",
      "updatedAt": "2024-07-31T11:51:50.664Z",
      "chapterId": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd"
    },
    {
      "id": "8d066350-9f5c-4a5f-bfee-3b24850518fa",
      "number": 1,
      "content": "Người đi trong đường lối Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:41.067Z",
      "updatedAt": "2024-07-31T11:51:50.678Z",
      "chapterId": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd"
    },
    {
      "id": "af4b9ef6-bbd9-4941-a9c3-46a31e9d1672",
      "number": 2,
      "content": "Bạn sẽ hưởng hoa lợi của tay mình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:41.077Z",
      "updatedAt": "2024-07-31T11:51:50.688Z",
      "chapterId": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd"
    },
    {
      "id": "6751e518-5ce5-44df-b9b3-25e8145904e5",
      "number": 2,
      "content": "Bạn sẽ được phước và thịnh vượng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:41.077Z",
      "updatedAt": "2024-07-31T11:51:50.695Z",
      "chapterId": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd"
    },
    {
      "id": "79f962b7-ff9c-4075-b3cd-30939a3f1785",
      "number": 3,
      "content": "Vợ bạn sẽ như cây nho sai trái trong nhà bạn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:41.079Z",
      "updatedAt": "2024-07-31T11:51:50.703Z",
      "chapterId": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd"
    },
    {
      "id": "8e31d53d-5114-4e12-b03f-af780b76e32a",
      "number": 3,
      "content": "Con cái bạn sẽ như các chồi ô-liu quanh bàn bạn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:41.079Z",
      "updatedAt": "2024-07-31T11:51:50.711Z",
      "chapterId": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd"
    },
    {
      "id": "ed5547d6-c6ab-458d-9588-8467c7487a30",
      "number": 4,
      "content": "Kìa, người nào kính sợ Chúa sẽ được phước như vậy.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:41.081Z",
      "updatedAt": "2024-07-31T11:51:50.718Z",
      "chapterId": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd"
    },
    {
      "id": "015abb69-7680-4366-85e1-f5eaf4e78b62",
      "number": 5,
      "content": "Nguyện Chúa ban phước cho bạn từ Si-ôn.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:41.083Z",
      "updatedAt": "2024-07-31T11:51:50.728Z",
      "chapterId": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd"
    },
    {
      "id": "3f2010a5-d9c6-4cc5-919f-a974f066e769",
      "number": 5,
      "content": "Nguyện suốt đời bạn chứng kiến những phước hạnh của Giê-ru-sa-lem.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:41.083Z",
      "updatedAt": "2024-07-31T11:51:50.735Z",
      "chapterId": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd"
    },
    {
      "id": "dd42402a-fd0e-47fd-9f9c-0d1f3160fb91",
      "number": 6,
      "content": "Nguyện bạn được nhìn thấy con cháu của con cái bạn.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:41.085Z",
      "updatedAt": "2024-07-31T11:51:50.744Z",
      "chapterId": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd"
    },
    {
      "id": "05892fa9-c94b-4c3b-8514-69b180f23a73",
      "number": 6,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:41.085Z",
      "updatedAt": "2024-07-31T11:51:50.751Z",
      "chapterId": "517e4da2-bbb1-4a11-a4f0-45a863f7d2dd"
    }
  ],
