  },
  "verses": [
    {
      "id": "c0e7fb6d-3702-489f-9f8f-45d0e6ba0427",
      "number": 1,
      "content": "Phước cho người kính sợ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:36.760Z",
      "updatedAt": "2024-09-09T04:51:36.760Z",
      "chapterId": "e8f905cc-d9e1-47d5-9076-9984562429d8"
    },
    {
      "id": "bb97441c-88db-4217-b3c5-e95043e59af7",
      "number": 1,
      "content": "Người đi trong đường lối Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:36.772Z",
      "updatedAt": "2024-09-09T04:51:36.772Z",
      "chapterId": "e8f905cc-d9e1-47d5-9076-9984562429d8"
    },
    {
      "id": "6e55f73e-692f-4be3-868c-084839dd6465",
      "number": 2,
      "content": "Bạn sẽ hưởng hoa lợi của tay mình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:36.775Z",
      "updatedAt": "2024-09-09T04:51:36.775Z",
      "chapterId": "e8f905cc-d9e1-47d5-9076-9984562429d8"
    },
    {
      "id": "abe8b4c8-f6bc-4e8b-bab9-a7934034be46",
      "number": 2,
      "content": "Bạn sẽ được phước và thịnh vượng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:36.777Z",
      "updatedAt": "2024-09-09T04:51:36.777Z",
      "chapterId": "e8f905cc-d9e1-47d5-9076-9984562429d8"
    },
    {
      "id": "d2372b23-9161-4847-82c8-3ae8082f6c61",
      "number": 3,
      "content": "Vợ bạn sẽ như cây nho sai trái trong nhà bạn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:36.779Z",
      "updatedAt": "2024-09-09T04:51:36.779Z",
      "chapterId": "e8f905cc-d9e1-47d5-9076-9984562429d8"
    },
    {
      "id": "c029561e-e52f-4ec8-933d-56c9f23a9e9d",
      "number": 3,
      "content": "Con cái bạn sẽ như các chồi ô-liu quanh bàn bạn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:36.781Z",
      "updatedAt": "2024-09-09T04:51:36.781Z",
      "chapterId": "e8f905cc-d9e1-47d5-9076-9984562429d8"
    },
    {
      "id": "0c7264b9-3eca-49c5-ad9a-9cbe76fa0e56",
      "number": 4,
      "content": "Kìa, người nào kính sợ Chúa sẽ được phước như vậy.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:36.784Z",
      "updatedAt": "2024-09-09T04:51:36.784Z",
      "chapterId": "e8f905cc-d9e1-47d5-9076-9984562429d8"
    },
    {
      "id": "6ac625c7-39a6-4b7f-a1a0-77805c12fbe0",
      "number": 5,
      "content": "Nguyện Chúa ban phước cho bạn từ Si-ôn.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:36.786Z",
      "updatedAt": "2024-09-09T04:51:36.786Z",
      "chapterId": "e8f905cc-d9e1-47d5-9076-9984562429d8"
    },
    {
      "id": "8fd4541a-c4a4-475a-a7a7-5f52049b2099",
      "number": 5,
      "content": "Nguyện suốt đời bạn chứng kiến những phước hạnh của Giê-ru-sa-lem.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:36.788Z",
      "updatedAt": "2024-09-09T04:51:36.788Z",
      "chapterId": "e8f905cc-d9e1-47d5-9076-9984562429d8"
    },
    {
      "id": "a34fb1fe-1c10-47aa-bf6a-63089de5c364",
      "number": 6,
      "content": "Nguyện bạn được nhìn thấy con cháu của con cái bạn.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:36.790Z",
      "updatedAt": "2024-09-09T04:51:36.790Z",
      "chapterId": "e8f905cc-d9e1-47d5-9076-9984562429d8"
    },
    {
      "id": "45e243ed-8f09-409e-8232-0b5fd5901238",
      "number": 6,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:36.793Z",
      "updatedAt": "2024-09-09T04:51:36.793Z",
      "chapterId": "e8f905cc-d9e1-47d5-9076-9984562429d8"
    }
  ],
