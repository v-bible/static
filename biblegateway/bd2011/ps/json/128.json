{
  "chapter": {
    "id": "e8f905cc-d9e1-47d5-9076-9984562429d8",
    "number": 128,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20128&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.780Z",
