    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "603f4c61-3b47-40eb-8c65-655c7ccc377a",
      "number": 1,
      "content": "Nếu chẳng phải Chúa bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:16.925Z",
      "updatedAt": "2024-09-09T04:51:16.925Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "870a913d-a129-4e59-b514-f15111edfe44",
      "number": 2,
      "content": "Nếu chẳng phải Chúa bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:16.952Z",
      "updatedAt": "2024-09-09T04:51:16.952Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "2bba8bc8-f2de-4a90-beae-b5e95b5ee4a7",
      "number": 1,
      "content": "Hỡi I-sơ-ra-ên, hãy nói đi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:16.947Z",
      "updatedAt": "2024-09-09T04:51:16.947Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "dfd9f6d4-94c8-422c-b07f-68c13deb498c",
      "number": 2,
      "content": "Thì khi người ta nổi lên chống lại chúng ta,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:16.957Z",
      "updatedAt": "2024-09-09T04:51:16.957Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "1dd8757f-4af7-46c3-afd6-a7e661006660",
      "number": 3,
      "content": "Khi cơn cuồng nộ của chúng bừng lên nghịch lại chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:16.962Z",
      "updatedAt": "2024-09-09T04:51:16.962Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "8dd81faa-a5f0-463a-abc8-a4fa1cceb9ae",
      "number": 3,
      "content": "Chắc hẳn chúng đã nuốt chửng chúng ta rồi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:16.967Z",
      "updatedAt": "2024-09-09T04:51:16.967Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "3a5d1878-f3f5-42e9-ab46-1656063487bb",
      "number": 4,
      "content": "Các dòng nước lũ chắc đã phủ ngập chúng ta;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:16.972Z",
      "updatedAt": "2024-09-09T04:51:16.972Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "b03308e8-292d-4e91-8aef-801fa40a6f6d",
      "number": 4,
      "content": "Luồng nước mạnh chắc đã cuốn trôi linh hồn chúng ta;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:16.977Z",
      "updatedAt": "2024-09-09T04:51:16.977Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "8ad89270-912b-4c9c-9804-8ed6676a9d07",
      "number": 5,
      "content": "Các đợt sóng thần dữ dội chắc đã cuốn mất linh hồn chúng ta rồi.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:16.981Z",
      "updatedAt": "2024-09-09T04:51:16.981Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "ca375d7b-45bd-44b5-a433-cf6a93d45a5d",
      "number": 6,
      "content": "Chúc tụng Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:16.990Z",
      "updatedAt": "2024-09-09T04:51:16.990Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "c6a88c23-21d8-408f-a898-e5535b052248",
      "number": 6,
      "content": "Ðấng không cho phép chúng ta làm mồi cho răng chúng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:16.995Z",
      "updatedAt": "2024-09-09T04:51:16.995Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "06de6d2d-9ab1-4fe7-891e-862194ca5661",
      "number": 7,
      "content": "Linh hồn chúng ta được giải thoát,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:17Z",
      "updatedAt": "2024-09-09T04:51:17Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "c18719c0-cb0f-46fa-8a31-f42e2ac545ea",
      "number": 7,
      "content": "Như con chim thoát khỏi bẫy của kẻ bẫy chim;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:17.004Z",
      "updatedAt": "2024-09-09T04:51:17.004Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "25806c02-71e8-451a-8890-ce07fee1a137",
      "number": 7,
      "content": "Cái bẫy bị bẻ gãy, và chúng ta thoát ra ngoài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:17.009Z",
      "updatedAt": "2024-09-09T04:51:17.009Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "c169f134-9220-4ceb-99bb-43d7f451b7a7",
      "number": 8,
      "content": "Chúng ta được giúp đỡ nhờ danh Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:17.014Z",
      "updatedAt": "2024-09-09T04:51:17.014Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    },
    {
      "id": "539290e5-ac97-4f70-8505-f7986ca1cc88",
      "number": 8,
      "content": "Ðấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:17.019Z",
      "updatedAt": "2024-09-09T04:51:17.019Z",
      "chapterId": "ebcb55c9-3b4e-4434-b213-785f3dcbc232"
    }
  ],
