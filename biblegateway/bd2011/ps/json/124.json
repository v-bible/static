{
  "chapter": {
    "id": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41",
    "number": 124,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20124&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.212Z",
    "updatedAt": "2024-07-24T15:07:49.085Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "549c54a5-389f-4212-8af8-cab26e85f2cc",
      "number": 1,
      "content": "Nếu chẳng phải Chúa bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.689Z",
      "updatedAt": "2024-07-31T11:51:00.028Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "9b48e9f8-8c9a-47ab-8165-35369b3f0e2a",
      "number": 1,
      "content": "Hỡi I-sơ-ra-ên, hãy nói đi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.689Z",
      "updatedAt": "2024-07-31T11:51:00.055Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "414da100-53a5-4eab-be9e-968dc4d642cc",
      "number": 2,
      "content": "Nếu chẳng phải Chúa bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.699Z",
      "updatedAt": "2024-07-31T11:51:00.079Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "dfbcce2c-f3db-4a25-a43b-a23cde8422c8",
      "number": 2,
      "content": "Thì khi người ta nổi lên chống lại chúng ta,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.699Z",
      "updatedAt": "2024-07-31T11:51:00.099Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "e001efa2-d001-4288-b062-c92147bb677f",
      "number": 3,
      "content": "Khi cơn cuồng nộ của chúng bừng lên nghịch lại chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.704Z",
      "updatedAt": "2024-07-31T11:51:00.111Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "02b52baf-581a-40a6-b928-875a257112f9",
      "number": 3,
      "content": "Chắc hẳn chúng đã nuốt chửng chúng ta rồi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.704Z",
      "updatedAt": "2024-07-31T11:51:00.120Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "946daeea-7b2a-4c64-b3d7-bef7efc1180d",
      "number": 4,
      "content": "Các dòng nước lũ chắc đã phủ ngập chúng ta;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.708Z",
      "updatedAt": "2024-07-31T11:51:00.128Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "be92e581-344a-457e-bbba-1537c8ffed5e",
      "number": 4,
      "content": "Luồng nước mạnh chắc đã cuốn trôi linh hồn chúng ta;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.708Z",
      "updatedAt": "2024-07-31T11:51:00.136Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "48840074-fdf2-42b2-bdfb-b08ffe544bee",
      "number": 5,
      "content": "Các đợt sóng thần dữ dội chắc đã cuốn mất linh hồn chúng ta rồi.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.712Z",
      "updatedAt": "2024-07-31T11:51:00.144Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "fe33a58c-90eb-4d74-adff-05aff4c94f53",
      "number": 6,
      "content": "Chúc tụng Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.718Z",
      "updatedAt": "2024-07-31T11:51:00.154Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "8f38fbb4-5b98-4e40-83ec-c6518c5b1dcc",
      "number": 6,
      "content": "Ðấng không cho phép chúng ta làm mồi cho răng chúng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.718Z",
      "updatedAt": "2024-07-31T11:51:00.163Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "a570ac78-3966-45b2-ba19-2882b3f9cd19",
      "number": 7,
      "content": "Linh hồn chúng ta được giải thoát,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.724Z",
      "updatedAt": "2024-07-31T11:51:00.171Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "7d625561-c57a-4e9c-884f-73b546436e33",
      "number": 7,
      "content": "Như con chim thoát khỏi bẫy của kẻ bẫy chim;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.724Z",
      "updatedAt": "2024-07-31T11:51:00.180Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "3862ab7c-8c16-48e3-bcb9-7e3697280d3a",
      "number": 7,
      "content": "Cái bẫy bị bẻ gãy, và chúng ta thoát ra ngoài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.724Z",
      "updatedAt": "2024-07-31T11:51:00.188Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "41955903-c25e-444d-b94c-74aa71759f67",
      "number": 8,
      "content": "Chúng ta được giúp đỡ nhờ danh Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.730Z",
      "updatedAt": "2024-07-31T11:51:00.196Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    },
    {
      "id": "ae117ffc-8aee-41d8-9ee8-1bea7edd1cac",
      "number": 8,
      "content": "Ðấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:38.730Z",
      "updatedAt": "2024-07-31T11:51:00.203Z",
      "chapterId": "d5f1e73d-ed0e-43f8-ac1c-d17b2ea82e41"
    }
  ],
