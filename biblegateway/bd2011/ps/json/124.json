{
  "chapter": {
    "id": "ebcb55c9-3b4e-4434-b213-785f3dcbc232",
    "number": 124,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20124&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.700Z",
