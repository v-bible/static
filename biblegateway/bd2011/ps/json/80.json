{
  "chapter": {
    "id": "399dd77d-f623-4de4-a318-6ab2e75ff6b2",
    "number": 80,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2080&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.248Z",
    "updatedAt": "2024-07-24T15:07:48.171Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "f6b93f50-7ae8-45cc-8fbd-920b2cdd6eec",
      "number": 1,
      "content": "Lạy Ðấng Chăn Giữ I-sơ-ra-ên, xin lắng nghe chúng con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.121Z",
      "updatedAt": "2024-07-31T11:39:48.589Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "e04b2191-9f16-4b1a-8785-aaccd5fe4659",
      "number": 1,
      "content": "Ngài là Ðấng dẫn dắt Gia-cốp như dẫn dắt một đàn chiên;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.121Z",
      "updatedAt": "2024-07-31T11:39:48.597Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "d0d05326-49f9-4a9b-b287-b5be9c207679",
      "number": 1,
      "content": "Ngài là Ðấng đang ngự trị giữa các chê-ru-bim,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.121Z",
      "updatedAt": "2024-07-31T11:39:48.616Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "af8b0afd-e085-4e77-9c78-1e1f1cd1010d",
      "number": 1,
      "content": "Xin chiếu rạng vinh quang Ngài ra!",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.121Z",
      "updatedAt": "2024-07-31T11:39:48.629Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "86fdf43f-23dd-4d92-8192-40575b532bc4",
      "number": 2,
      "content": "Xin khởi động quyền năng Ngài trước mặt Ép-ra-im, Bên-gia-min, và Ma-na-se,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.131Z",
      "updatedAt": "2024-07-31T11:39:48.643Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "f1515356-90e5-47c7-9940-5d930d0780d4",
      "number": 2,
      "content": "Và xin đến cứu chúng con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.131Z",
      "updatedAt": "2024-07-31T11:39:48.656Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "6758127e-cb61-4a11-bac5-796b5e831429",
      "number": 3,
      "content": "Ðức Chúa Trời ôi, xin khôi phục chúng con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.133Z",
      "updatedAt": "2024-07-31T11:39:48.673Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "b217e0f5-19a6-4679-bc74-066f5a889eb8",
      "number": 3,
      "content": "Xin cho thánh nhan Ngài tỏa rạng trên chúng con, để chúng con được cứu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.133Z",
      "updatedAt": "2024-07-31T11:39:48.686Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "0cafe1b9-7ed4-43ff-af04-2393bdf35f7c",
      "number": 4,
      "content": "Lạy Chúa, Ðức Chúa Trời của các đạo quân,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.136Z",
      "updatedAt": "2024-07-31T11:39:48.704Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "c053feaa-d32a-4249-a913-fd472ae6827b",
      "number": 4,
      "content": "Ngài còn giận mà không muốn nghe lời cầu nguyện của con dân Ngài đến bao lâu nữa?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.136Z",
      "updatedAt": "2024-07-31T11:39:48.718Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "5c6dd97c-5581-4b42-b02b-ef5c56066988",
      "number": 5,
      "content": "Ngài đã cho họ ăn bánh chan hòa những giọt lệ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.138Z",
      "updatedAt": "2024-07-31T11:39:48.748Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "5cd6683a-694a-43ee-b135-e64f1a532354",
      "number": 5,
      "content": "Ngài đã để họ uống nước pha với nước mắt.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.138Z",
      "updatedAt": "2024-07-31T11:39:48.758Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "d58f188a-cb6d-43bc-b72f-f04713a1afca",
      "number": 6,
      "content": "Ngài đã làm cho chúng con thành đề tài giễu cợt cho các lân bang của chúng con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.140Z",
      "updatedAt": "2024-07-31T11:39:48.773Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "26e4a168-2894-412a-b13a-6371c0d6e245",
      "number": 6,
      "content": "Những kẻ thù của chúng con đã chế nhạo chúng con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.140Z",
      "updatedAt": "2024-07-31T11:39:48.781Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "53ef1397-353b-4b38-8d81-df551594d61e",
      "number": 7,
      "content": "Ðức Chúa Trời của các đạo quân ôi, xin khôi phục chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.142Z",
      "updatedAt": "2024-07-31T11:39:48.793Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "cae3d3c0-e2f0-4417-b0cb-7bdf0e767b1d",
      "number": 7,
      "content": "Xin cho thánh nhan Ngài tỏa rạng trên chúng con, để chúng con được cứu.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.142Z",
      "updatedAt": "2024-07-31T11:39:48.903Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "42c0d911-fce1-4663-b4b0-0a60e0eb7b23",
      "number": 8,
      "content": "Từ Ai-cập, Ngài đã bứng lên một cây nho;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.145Z",
      "updatedAt": "2024-07-31T11:39:48.935Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "c5ca2a6b-2b85-4cf9-854f-e5ced82e4c0f",
      "number": 8,
      "content": "Ngài đuổi các dân tộc kia đi, rồi trồng cây nho ấy xuống.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.145Z",
      "updatedAt": "2024-07-31T11:39:48.945Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "23306c99-d23a-4134-9021-b7ee71be695c",
      "number": 9,
      "content": "Ngài đã làm sạch mảnh đất cho nó;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.147Z",
      "updatedAt": "2024-07-31T11:39:48.954Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "e864ff4f-a5a5-485f-836f-f03f86fe8b95",
      "number": 9,
      "content": "Nó đâm rễ và tràn ra khắp xứ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.147Z",
      "updatedAt": "2024-07-31T11:39:48.965Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "b9405bf0-9f0a-4f20-95cb-55f51ae772f1",
      "number": 10,
      "content": "Các núi rừng bị bóng nó phủ che;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.150Z",
      "updatedAt": "2024-07-31T11:39:48.974Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "22b47364-f1a0-4a6c-af18-9dc89c2560ac",
      "number": 10,
      "content": "Các nhánh nó cứng mạnh như cây bá hương tươi tốt.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.150Z",
      "updatedAt": "2024-07-31T11:39:48.985Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "f82fbd1c-bced-4ce2-9c46-94fb4057a35d",
      "number": 11,
      "content": "Các cành nó nhô ra đến tận đại dương;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.156Z",
      "updatedAt": "2024-07-31T11:39:48.994Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "13962010-08d0-4b3c-b8db-0471bdda415c",
      "number": 11,
      "content": "Các chồi nó vươn dài đến bờ Sông Lớn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.156Z",
      "updatedAt": "2024-07-31T11:39:49.002Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "cf37e7e2-0da9-4550-93fe-1163e30b1640",
      "number": 12,
      "content": "Nhưng sao Ngài nỡ dỡ rào nó xuống,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.161Z",
      "updatedAt": "2024-07-31T11:39:49.013Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "2143db50-88c6-40a6-8b17-0d19995e3fe8",
      "number": 12,
      "content": "Ðể ai qua lại cũng có thể ngắt bứt nó thỏa tay,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.161Z",
      "updatedAt": "2024-07-31T11:39:49.021Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "25548db2-a086-4f42-af63-89af22d04a6b",
      "number": 13,
      "content": "Ðể các heo rừng cùng nhau cắn phá nó,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.167Z",
      "updatedAt": "2024-07-31T11:39:49.029Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "2f945745-db70-4ac4-8433-c4782c6bc67b",
      "number": 13,
      "content": "Và những thú hoang cấu xé nó tan tành?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.167Z",
      "updatedAt": "2024-07-31T11:39:49.038Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "13a3858a-0dd3-44f4-b36e-d908df290b41",
      "number": 14,
      "content": "Ðức Chúa Trời của các đạo quân ôi, xin Ngài quay trở lại;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.172Z",
      "updatedAt": "2024-07-31T11:39:49.049Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "004aacae-e6fb-4fdb-8dfc-2e81220acd4a",
      "number": 14,
      "content": "Xin Ngài từ trời nhìn xuống và đoái xem chúng con;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.172Z",
      "updatedAt": "2024-07-31T11:39:49.057Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "6523f5c3-6522-495e-ac89-a2d1b8df5f56",
      "number": 14,
      "content": "Xin Ngài thăm viếng cây nho này,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.172Z",
      "updatedAt": "2024-07-31T11:39:49.065Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "2bfac767-2bea-4485-8023-cefcaa197e8b",
      "number": 15,
      "content": "Tức gốc nho do cánh tay phải Ngài đã trồng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.178Z",
      "updatedAt": "2024-07-31T11:39:49.073Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "50a5076c-83f8-4472-b113-e2d513e0ab5d",
      "number": 15,
      "content": "Là đứa con Ngài đã gây dựng cho mình.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.178Z",
      "updatedAt": "2024-07-31T11:39:49.081Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "3b13f7ae-275a-4f23-8168-dc752f3ead63",
      "number": 16,
      "content": "Bây giờ cây nho ấy đang bị đốn chặt và bị đốt cháy;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.184Z",
      "updatedAt": "2024-07-31T11:39:49.089Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "777edcc4-9e42-4088-9e5a-bed8356a8c87",
      "number": 16,
      "content": "Vì khi Ngài nghiêm mặt quở trách, họ liền bị diệt vong.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.184Z",
      "updatedAt": "2024-07-31T11:39:49.098Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "21acfb4b-04f7-4d1b-9dc0-61685dc11c9c",
      "number": 17,
      "content": "Xin đặt cánh tay Ngài trên người bên phải Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.190Z",
      "updatedAt": "2024-07-31T11:39:49.107Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "463cb1cc-4a06-4b9e-b436-6a69ac599af3",
      "number": 17,
      "content": "Tức trên người Ngài đã gây dựng cho mình;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.190Z",
      "updatedAt": "2024-07-31T11:39:49.115Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "c9557b3d-d668-4abc-9a56-3354ce44cf17",
      "number": 18,
      "content": "Ðể chúng con không quay lưng lìa bỏ Ngài.",
      "order": 0,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.195Z",
      "updatedAt": "2024-07-31T11:39:49.124Z",
      "chapterId": "399dd77d-f623-4de4-a318-6ab2e75ff6b2"
    },
    {
      "id": "fdc1a6ee-6047-4422-8c52-40341c2eeddc",
      "number": 18,
      "content": "Xin phục hưng chúng con, để chúng con cầu khẩn danh Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:00:41.195Z",
      "updatedAt": "2024-07-31T11:39:49.132Z",
