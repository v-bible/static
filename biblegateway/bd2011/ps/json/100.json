    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "781ab79f-1347-4e7c-a0b2-5bf61bbd4283",
      "number": 1,
      "content": "Hỡi cả thế giới,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.939Z",
      "updatedAt": "2024-07-31T11:45:14.397Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "8fe9cb00-67da-45e0-9f06-cb8d7e40051a",
      "number": 1,
      "content": "Hãy lớn tiếng reo mừng tung hô Chúa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.939Z",
      "updatedAt": "2024-07-31T11:45:14.412Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "a685b36f-7f3b-431f-9a82-19dec19b8814",
      "number": 2,
      "content": "Hãy thờ phượng Chúa cách vui mừng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.949Z",
      "updatedAt": "2024-07-31T11:45:14.420Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "6e044777-cbe9-4148-919e-6edbbf32c7d3",
      "number": 2,
      "content": "Hãy đến trước thánh nhan Ngài với tiếng hoan ca.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.949Z",
      "updatedAt": "2024-07-31T11:45:14.428Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "7cafbbe0-aeb3-4847-a845-b8f93f94f908",
      "number": 3,
      "content": "Khá biết rằng Chúa là Ðức Chúa Trời.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.953Z",
      "updatedAt": "2024-07-31T11:45:14.438Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "101f5505-b6b1-4654-a44e-d9bf23c14532",
      "number": 3,
      "content": "Ấy là Ngài, Đấng dựng nên chúng ta;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.953Z",
      "updatedAt": "2024-07-31T11:45:14.446Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "64c2a1b1-ab1c-4e7f-a1a8-e8e62c96140b",
      "number": 3,
      "content": "Chúng ta thuộc về Ngài;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.953Z",
      "updatedAt": "2024-07-31T11:45:14.457Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "718b42ef-8820-4c90-b059-752d8b4474d4",
      "number": 3,
      "content": "Chúng ta là con dân Ngài và bầy chiên của đồng cỏ Ngài.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.953Z",
      "updatedAt": "2024-07-31T11:45:14.465Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "7b613ecb-a98a-4ccf-9ac0-a08b84bfc8b6",
      "number": 4,
      "content": "Hãy cảm tạ mà vào các cổng Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.958Z",
      "updatedAt": "2024-07-31T11:45:14.475Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "e1bf41b9-5e7f-4eec-87f3-23882e950014",
      "number": 4,
      "content": "Khá ngợi ca mà vào các sân Ngài;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.958Z",
      "updatedAt": "2024-07-31T11:45:14.483Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "f62aa2a7-577b-4e55-960c-f5b6ae2f87eb",
      "number": 4,
      "content": "Hãy tạ ơn Ngài, khá chúc tụng danh Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.958Z",
      "updatedAt": "2024-07-31T11:45:14.491Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "0039e61a-0fd1-42e8-9945-d0311dad6df6",
      "number": 5,
      "content": "Vì Chúa thật tốt;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.962Z",
      "updatedAt": "2024-07-31T11:45:14.502Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "cdf09c8c-60f3-4884-9991-798d18788698",
      "number": 5,
      "content": "Tình thương của Ngài hằng còn mãi mãi;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.962Z",
      "updatedAt": "2024-07-31T11:45:14.509Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    },
    {
      "id": "418cdbbd-1324-4614-8c47-05e55d980f5a",
      "number": 5,
      "content": "Ðức thành tín của Ngài còn đến muôn đời.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:46.962Z",
      "updatedAt": "2024-07-31T11:45:14.517Z",
      "chapterId": "38839371-6bcb-4641-9c73-90110891ea6c"
    }
  ],
