{
  "chapter": {
    "id": "69e7f580-c936-4999-9c4e-44e46dd11d96",
    "number": 100,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20100&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.211Z",
    "updatedAt": "2024-09-09T04:13:01.211Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "aafa10eb-89e4-4262-b304-2e5bab6d33b4",
      "number": 1,
      "content": "Hỡi cả thế giới,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.646Z",
      "updatedAt": "2024-09-09T04:49:18.646Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "221a93b3-28c2-44e5-8530-dcd247c9b677",
      "number": 1,
      "content": "Hãy lớn tiếng reo mừng tung hô Chúa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.663Z",
      "updatedAt": "2024-09-09T04:49:18.663Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "928ea149-6c9b-4b98-9620-e35978334b0d",
      "number": 2,
      "content": "Hãy thờ phượng Chúa cách vui mừng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.668Z",
      "updatedAt": "2024-09-09T04:49:18.668Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "f8366be3-5655-4662-8921-4bcbb31ce8b5",
      "number": 2,
      "content": "Hãy đến trước thánh nhan Ngài với tiếng hoan ca.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.677Z",
      "updatedAt": "2024-09-09T04:49:18.677Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "4e10326d-849f-40a0-9569-e3bf9e438d22",
      "number": 3,
      "content": "Khá biết rằng Chúa là Ðức Chúa Trời.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.682Z",
      "updatedAt": "2024-09-09T04:49:18.682Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "0faf8710-e082-4584-b441-413b7b32b486",
      "number": 3,
      "content": "Ấy là Ngài, Đấng dựng nên chúng ta;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.687Z",
      "updatedAt": "2024-09-09T04:49:18.687Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "16cd8f89-88d7-45c5-8af0-86cb2d7c6029",
      "number": 3,
      "content": "Chúng ta thuộc về Ngài;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.692Z",
      "updatedAt": "2024-09-09T04:49:18.692Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "a6f15b19-f07b-4cb1-bed1-d09d267f946c",
      "number": 3,
      "content": "Chúng ta là con dân Ngài và bầy chiên của đồng cỏ Ngài.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.696Z",
      "updatedAt": "2024-09-09T04:49:18.696Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "0ef7d0d9-1c94-4805-b002-fb556c932e0a",
      "number": 4,
      "content": "Hãy cảm tạ mà vào các cổng Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.701Z",
      "updatedAt": "2024-09-09T04:49:18.701Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "29a52a08-7d6c-45c0-b19f-563a9c6ab3d8",
      "number": 4,
      "content": "Khá ngợi ca mà vào các sân Ngài;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.705Z",
      "updatedAt": "2024-09-09T04:49:18.705Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "9d952479-761a-4bd6-9370-d19f076c1066",
      "number": 4,
      "content": "Hãy tạ ơn Ngài, khá chúc tụng danh Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.710Z",
      "updatedAt": "2024-09-09T04:49:18.710Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "97a8f9d7-85ee-449a-a768-9e78dba39999",
      "number": 5,
      "content": "Vì Chúa thật tốt;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.715Z",
      "updatedAt": "2024-09-09T04:49:18.715Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "f3f66e8d-df7d-4b14-b863-82720368a50d",
      "number": 5,
      "content": "Tình thương của Ngài hằng còn mãi mãi;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.719Z",
      "updatedAt": "2024-09-09T04:49:18.719Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    },
    {
      "id": "c50b2c5b-8c43-461c-a738-3e24be4ca329",
      "number": 5,
      "content": "Ðức thành tín của Ngài còn đến muôn đời.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:18.724Z",
      "updatedAt": "2024-09-09T04:49:18.724Z",
      "chapterId": "69e7f580-c936-4999-9c4e-44e46dd11d96"
    }
  ],
