{
  "chapter": {
    "id": "38839371-6bcb-4641-9c73-90110891ea6c",
    "number": 100,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20100&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.690Z",
