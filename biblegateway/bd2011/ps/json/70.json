{
  "chapter": {
    "id": "d441b8f1-1256-4322-aa36-075e12d03de1",
    "number": 70,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2070&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.037Z",
    "updatedAt": "2024-07-24T15:07:47.952Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "64f1df3a-8ebf-45b8-aff6-aa299a454688",
      "number": 1,
      "content": "Ðức Chúa Trời ôi, xin mau mau giải cứu con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:58:41.043Z",
      "updatedAt": "2024-07-31T11:37:47.696Z",
      "chapterId": "d441b8f1-1256-4322-aa36-075e12d03de1"
    },
    {
      "id": "f2d4756c-57ca-47b8-8d38-0821981db095",
      "number": 1,
      "content": "Chúa ôi, xin nhanh chóng đến giúp đỡ con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:58:41.043Z",
      "updatedAt": "2024-07-31T11:37:47.712Z",
      "chapterId": "d441b8f1-1256-4322-aa36-075e12d03de1"
    },
    {
      "id": "394d6529-f01a-4d1d-811c-8b512da72e09",
      "number": 2,
      "content": "Nguyện những kẻ tìm cách tiêu diệt con bị hổ thẹn và bối rối;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:58:41.047Z",
      "updatedAt": "2024-07-31T11:37:47.721Z",
      "chapterId": "d441b8f1-1256-4322-aa36-075e12d03de1"
    },
    {
      "id": "bf9794e8-3848-40d4-a0c7-28f290d5fdae",
      "number": 2,
      "content": "Nguyện những kẻ muốn làm tổn thương con phải bỏ đi trong sỉ nhục.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:58:41.047Z",
      "updatedAt": "2024-07-31T11:37:47.729Z",
      "chapterId": "d441b8f1-1256-4322-aa36-075e12d03de1"
    },
    {
      "id": "3a08e862-ddd2-4b20-9a33-17dcff8a57c8",
      "number": 3,
      "content": "Nguyện những kẻ nhạo cười con rằng, “Ha ha! Ha ha!” phải rút lui vì nhục nhã.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:58:41.049Z",
      "updatedAt": "2024-07-31T11:37:47.737Z",
      "chapterId": "d441b8f1-1256-4322-aa36-075e12d03de1"
    },
    {
      "id": "ac1a0618-2cf1-43cb-b933-2d76c5596014",
      "number": 4,
      "content": "Nguyện những ai tìm kiếm Ngài vui vẻ và mừng rỡ trong Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:58:41.050Z",
      "updatedAt": "2024-07-31T11:37:47.767Z",
      "chapterId": "d441b8f1-1256-4322-aa36-075e12d03de1"
    },
    {
      "id": "a1edc1f7-b8f0-40e9-b704-044824ac2030",
      "number": 4,
      "content": "Nguyện những ai yêu mến ơn cứu rỗi của Ngài cứ nói mãi, “Ðức Chúa Trời vĩ đại thay!”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:58:41.050Z",
      "updatedAt": "2024-07-31T11:37:47.786Z",
      "chapterId": "d441b8f1-1256-4322-aa36-075e12d03de1"
    },
    {
      "id": "340dbba1-0ebd-4fde-a90a-b62ca7a37cd2",
      "number": 5,
      "content": "Còn con, con đang gặp cảnh khốn khổ và cùng đường;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:58:41.053Z",
      "updatedAt": "2024-07-31T11:37:47.802Z",
      "chapterId": "d441b8f1-1256-4322-aa36-075e12d03de1"
    },
    {
      "id": "e73f15c8-90ce-4686-9b3d-3dc787215d6a",
      "number": 5,
      "content": "Ðức Chúa Trời ôi, xin mau mau đến với con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:58:41.053Z",
      "updatedAt": "2024-07-31T11:37:47.814Z",
      "chapterId": "d441b8f1-1256-4322-aa36-075e12d03de1"
    },
    {
      "id": "21c60d7b-4871-457d-945b-c7af3f102640",
      "number": 5,
      "content": "Ngài là Ðấng Giúp Ðỡ và Ðấng Giải Cứu của con;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:58:41.053Z",
      "updatedAt": "2024-07-31T11:37:47.839Z",
      "chapterId": "d441b8f1-1256-4322-aa36-075e12d03de1"
    },
    {
      "id": "30f4c33b-35c6-4638-9929-69a91c2f32d2",
      "number": 5,
      "content": "Chúa ôi, xin đừng trì hoãn.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:58:41.053Z",
      "updatedAt": "2024-07-31T11:37:47.846Z",
      "chapterId": "d441b8f1-1256-4322-aa36-075e12d03de1"
    }
  ],
