{
  "chapter": {
    "id": "b2bba512-4136-4e97-a652-54e91ef3b156",
    "number": 135,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20135&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.454Z",
    "updatedAt": "2024-07-24T15:07:49.325Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "9d8a1606-5f67-4dec-9deb-ea15f20c8c13",
      "number": 1,
      "content": "Hãy ca ngợi Chúa! Hãy ca ngợi danh Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.734Z",
      "updatedAt": "2024-07-31T11:53:18.819Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "800ba230-abc2-402d-b914-369bed0da93f",
      "number": 1,
      "content": "Hỡi các tôi tớ Chúa, hãy ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.734Z",
      "updatedAt": "2024-07-31T11:53:18.826Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "cffb258d-a3f3-4315-aa8e-2426232b4647",
      "number": 2,
      "content": "Hỡi các bạn, những người đứng trong nhà Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.744Z",
      "updatedAt": "2024-07-31T11:53:18.834Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "00c53a70-82cb-47c6-9819-9dcdc83a254d",
      "number": 2,
      "content": "Những người đứng trong sân đền thờ Ðức Chúa Trời chúng ta,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.744Z",
      "updatedAt": "2024-07-31T11:53:18.842Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "6d05951b-1a40-4552-a78f-3200ea135c36",
      "number": 3,
      "content": "Hãy ca ngợi Chúa, vì Chúa thật tốt;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.746Z",
      "updatedAt": "2024-07-31T11:53:18.850Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "1d038db7-359d-4c99-a20f-319e1e4c7455",
      "number": 3,
      "content": "Hãy ca tụng danh Ngài, vì danh Ngài đáng kính yêu;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.746Z",
      "updatedAt": "2024-07-31T11:53:18.858Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "c96e7d89-62ee-412b-aff3-ea38172de6e5",
      "number": 4,
      "content": "Vì Chúa đã chọn Gia-cốp cho Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.749Z",
      "updatedAt": "2024-07-31T11:53:18.866Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "7f2481cb-7aaa-42bc-a166-fb4b55526f5e",
      "number": 4,
      "content": "I-sơ-ra-ên làm tài sản riêng của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.749Z",
      "updatedAt": "2024-07-31T11:53:18.873Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "9044c74b-3a14-4d05-b76c-f5a84f455e57",
      "number": 5,
      "content": "Vì tôi biết rằng Chúa thật vĩ đại;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.751Z",
      "updatedAt": "2024-07-31T11:53:18.883Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "5ccbcfb3-809a-4b66-bd8f-eea4ab6ce868",
      "number": 5,
      "content": "Chúa của chúng ta vĩ đại hơn tất cả thần thánh của loài người.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.751Z",
      "updatedAt": "2024-07-31T11:53:18.902Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "d742966c-df0d-4953-9873-a0d8697396a9",
      "number": 6,
      "content": "Bất cứ điều gì Chúa muốn, Ngài làm điều ấy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.753Z",
      "updatedAt": "2024-07-31T11:53:18.923Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "cfbd4cc4-ad17-4cf0-b1bb-d687a5cfbed4",
      "number": 6,
      "content": "Bất kể trên trời hay dưới đất, trong lòng biển hay trong các vực sâu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.753Z",
      "updatedAt": "2024-07-31T11:53:18.944Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "3a53406e-72eb-4454-b76f-5cb9f6be0632",
      "number": 7,
      "content": "Ngài khiến mây trồi lên từ những nơi tận cùng trái đất;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.755Z",
      "updatedAt": "2024-07-31T11:53:18.965Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "410ca0ee-3ac2-48b8-8ac4-8c6358912ac3",
      "number": 7,
      "content": "Ngài làm cho sấm sét nổ vang khi trời đổ cơn mưa;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.755Z",
      "updatedAt": "2024-07-31T11:53:18.986Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "ca2bf1bf-db2c-4c01-86c3-d0a6d94745f8",
      "number": 7,
      "content": "Ngài sai gió bay ra từ các kho lẫm của Ngài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.755Z",
      "updatedAt": "2024-07-31T11:53:18.999Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "4c1b0f6a-c637-4826-8608-565b64478bc9",
      "number": 8,
      "content": "Ngài đánh hạ các con đầu lòng của Ai-cập,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.757Z",
      "updatedAt": "2024-07-31T11:53:19.011Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "c482f676-f8b0-4098-900b-8321761e6335",
      "number": 8,
      "content": "Con của loài người và con của súc vật.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.757Z",
      "updatedAt": "2024-07-31T11:53:19.020Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "0d3531dd-e1ef-4066-aa5b-23c3f95c4109",
      "number": 9,
      "content": "Hỡi Ai-cập, Ngài sai các dấu kỳ và phép lạ đến giữa ngươi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.760Z",
      "updatedAt": "2024-07-31T11:53:19.027Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "8a4cbd1a-f9d2-4715-9c67-e4c5b717f276",
      "number": 9,
      "content": "Ðể trừng phạt Pha-ra-ôn và các tôi tớ của ông ta.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.760Z",
      "updatedAt": "2024-07-31T11:53:19.034Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "42eaaacb-c85f-4b11-8274-5a623a80ebe4",
      "number": 10,
      "content": "Ngài đánh bại nhiều dân tộc,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.762Z",
      "updatedAt": "2024-07-31T11:53:19.042Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "160c0a0d-f7ed-419d-aec9-33f7c4efdcc1",
      "number": 10,
      "content": "Và tiêu diệt các vua mạnh mẽ;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.762Z",
      "updatedAt": "2024-07-31T11:53:19.049Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "517c6056-b92d-47ac-b63a-a19c7582372f",
      "number": 11,
      "content": "Si-hôn vua dân A-mô-ri, Óc vua Ba-san,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.764Z",
      "updatedAt": "2024-07-31T11:53:19.057Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "2f5d50d7-a75a-498b-a131-bb36f60b4a20",
      "number": 11,
      "content": "Và tất cả các vương quốc ở Ca-na-an.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.764Z",
      "updatedAt": "2024-07-31T11:53:19.064Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "b03aa8d7-7096-4dbe-8be7-5da6f2d6df30",
      "number": 12,
      "content": "Ngài ban xứ của họ để làm sản nghiệp,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.766Z",
      "updatedAt": "2024-07-31T11:53:19.072Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "9c7312d2-086c-4952-8cba-871913e9c7b8",
      "number": 12,
      "content": "Một sản nghiệp cho dân I-sơ-ra-ên của Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.766Z",
      "updatedAt": "2024-07-31T11:53:19.079Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "d8734f93-3f83-4577-8bc7-16f8181a99d9",
      "number": 13,
      "content": "Chúa ôi, danh Ngài sẽ trường tồn mãi mãi;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.769Z",
      "updatedAt": "2024-07-31T11:53:19.089Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "613ddecc-2041-4d48-8ad2-cbf8a606d575",
      "number": 13,
      "content": "Chúa ôi, công ơn Ngài sẽ được ghi nhớ từ đời nọ đến đời kia,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.769Z",
      "updatedAt": "2024-07-31T11:53:19.096Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "08359207-00f7-4aa2-b6f5-149f21f3a8ab",
      "number": 14,
      "content": "Vì Chúa sẽ xét đoán dân Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.771Z",
      "updatedAt": "2024-07-31T11:53:19.103Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "68f503aa-8431-43a8-8d8b-6a963a33f388",
      "number": 14,
      "content": "Ngài sẽ tỏ lòng thương xót đối với các tôi tớ Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.771Z",
      "updatedAt": "2024-07-31T11:53:19.111Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "311c7734-d3d5-4570-82a5-f993e54eb922",
      "number": 15,
      "content": "Các thần tượng của các dân chẳng qua là bạc và vàng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.773Z",
      "updatedAt": "2024-07-31T11:53:19.233Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "caedc7c7-1d62-41a0-bee3-dbbd320a3ac4",
      "number": 15,
      "content": "Chúng chỉ là sản phẩm do tay người làm ra.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.773Z",
      "updatedAt": "2024-07-31T11:53:19.277Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "ad3bde62-ff62-4263-986b-f34412899060",
      "number": 16,
      "content": "Chúng có miệng mà chẳng nói;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.775Z",
      "updatedAt": "2024-07-31T11:53:19.289Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "70047aa2-5724-44d8-ae49-de32ec324afa",
      "number": 16,
      "content": "Chúng có mắt mà chẳng thấy;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.775Z",
      "updatedAt": "2024-07-31T11:53:19.301Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "29df1f67-b6ee-4c2c-876a-05dd581a2d43",
      "number": 17,
      "content": "Chúng có tai mà chẳng nghe;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.777Z",
      "updatedAt": "2024-07-31T11:53:19.309Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "ea06bba9-a1eb-42cb-966e-bb3de774b743",
      "number": 17,
      "content": "Không có chút hơi thở nào trong miệng chúng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.777Z",
      "updatedAt": "2024-07-31T11:53:19.321Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "2aea4561-5a34-4542-9af8-7fe8bbc9379b",
      "number": 18,
      "content": "Nguyện những kẻ làm thần tượng sẽ trở nên như chúng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.780Z",
      "updatedAt": "2024-07-31T11:53:19.332Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "c7922788-429c-46f6-98ef-29fb00b33d95",
      "number": 18,
      "content": "Và những ai tin cậy vào thần tượng cũng giống như vậy.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.780Z",
      "updatedAt": "2024-07-31T11:53:19.340Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "c4282dd2-976a-4079-81bf-cd13989437af",
      "number": 19,
      "content": "Hỡi nhà I-sơ-ra-ên, hãy chúc tụng Chúa!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.782Z",
      "updatedAt": "2024-07-31T11:53:19.352Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "e27fd2a0-2f6d-441b-9856-36417b4c0f49",
      "number": 19,
      "content": "Hỡi nhà A-rôn, hãy chúc tụng Chúa!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:12:02.782Z",
      "updatedAt": "2024-07-31T11:53:19.360Z",
      "chapterId": "b2bba512-4136-4e97-a652-54e91ef3b156"
    },
    {
      "id": "1ddc379b-a16c-469f-b904-7248d945c861",
      "number": 20,
      "content": "Hỡi nhà Lê-vi, hãy chúc tụng Chúa!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
