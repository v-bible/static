{
  "chapter": {
    "id": "ea8432d5-184e-4142-aa28-8f19cf614785",
    "number": 135,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20135&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.933Z",
    "updatedAt": "2024-09-09T04:13:01.933Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "60afc726-659e-493b-b5c3-811f8eefb90b",
      "number": 1,
      "content": "Hãy ca ngợi Chúa! Hãy ca ngợi danh Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.862Z",
      "updatedAt": "2024-09-09T04:52:11.862Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "b345aa5b-b688-4389-8c41-c0300760df9a",
      "number": 1,
      "content": "Hỡi các tôi tớ Chúa, hãy ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.881Z",
      "updatedAt": "2024-09-09T04:52:11.881Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "3c215b9e-a3ed-4668-ad01-67105d45e51f",
      "number": 2,
      "content": "Hỡi các bạn, những người đứng trong nhà Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.886Z",
      "updatedAt": "2024-09-09T04:52:11.886Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "7b71bb13-0ec8-44a9-a601-8a1f1b152b1b",
      "number": 2,
      "content": "Những người đứng trong sân đền thờ Ðức Chúa Trời chúng ta,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.891Z",
      "updatedAt": "2024-09-09T04:52:11.891Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "63ab32bb-5d16-4619-a9f6-a51ea33523c6",
      "number": 3,
      "content": "Hãy ca ngợi Chúa, vì Chúa thật tốt;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.896Z",
      "updatedAt": "2024-09-09T04:52:11.896Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "a74a2aec-7682-4eb8-9f6f-ae07271e3be2",
      "number": 3,
      "content": "Hãy ca tụng danh Ngài, vì danh Ngài đáng kính yêu;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.901Z",
      "updatedAt": "2024-09-09T04:52:11.901Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "07ff9699-87b2-43a1-8bc7-85fc92225e48",
      "number": 4,
      "content": "Vì Chúa đã chọn Gia-cốp cho Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.906Z",
      "updatedAt": "2024-09-09T04:52:11.906Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "66aa1c8d-97d3-4a90-97b0-a9e0c0492639",
      "number": 4,
      "content": "I-sơ-ra-ên làm tài sản riêng của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.910Z",
      "updatedAt": "2024-09-09T04:52:11.910Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "063f1a97-95b1-4e55-80c1-e5aa41ff358e",
      "number": 5,
      "content": "Vì tôi biết rằng Chúa thật vĩ đại;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.915Z",
      "updatedAt": "2024-09-09T04:52:11.915Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "f379c457-0f7a-47d2-975d-b5d471985296",
      "number": 5,
      "content": "Chúa của chúng ta vĩ đại hơn tất cả thần thánh của loài người.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.920Z",
      "updatedAt": "2024-09-09T04:52:11.920Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "68db6018-dfad-4008-8116-8acdf44bb11e",
      "number": 6,
      "content": "Bất cứ điều gì Chúa muốn, Ngài làm điều ấy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.925Z",
      "updatedAt": "2024-09-09T04:52:11.925Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "965ab31b-e07e-41da-a1cf-28efec80c514",
      "number": 6,
      "content": "Bất kể trên trời hay dưới đất, trong lòng biển hay trong các vực sâu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.931Z",
      "updatedAt": "2024-09-09T04:52:11.931Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "0cc0bf2f-28aa-42c0-b6a2-9fab4ca3ad99",
      "number": 7,
      "content": "Ngài khiến mây trồi lên từ những nơi tận cùng trái đất;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.935Z",
      "updatedAt": "2024-09-09T04:52:11.935Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "5c02e3d7-103f-4211-91a2-06d9aedc435d",
      "number": 7,
      "content": "Ngài làm cho sấm sét nổ vang khi trời đổ cơn mưa;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.940Z",
      "updatedAt": "2024-09-09T04:52:11.940Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "aece3e91-a1c2-4c14-bea6-da4b24a789ea",
      "number": 7,
      "content": "Ngài sai gió bay ra từ các kho lẫm của Ngài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.945Z",
      "updatedAt": "2024-09-09T04:52:11.945Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "28c1ce76-9bab-469a-a4c3-f0b986259811",
      "number": 8,
      "content": "Ngài đánh hạ các con đầu lòng của Ai-cập,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.950Z",
      "updatedAt": "2024-09-09T04:52:11.950Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "680c0680-c8e5-449b-91f5-821b0903172d",
      "number": 8,
      "content": "Con của loài người và con của súc vật.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.955Z",
      "updatedAt": "2024-09-09T04:52:11.955Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "0a184bbd-22e6-4a23-bc64-ac4b197b7de8",
      "number": 9,
      "content": "Hỡi Ai-cập, Ngài sai các dấu kỳ và phép lạ đến giữa ngươi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.959Z",
      "updatedAt": "2024-09-09T04:52:11.959Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "e049fae6-ccbd-4f8a-9b55-a8da2892e21b",
      "number": 9,
      "content": "Ðể trừng phạt Pha-ra-ôn và các tôi tớ của ông ta.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.965Z",
      "updatedAt": "2024-09-09T04:52:11.965Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "9a326243-9e3e-4110-9eff-e452f3bbccbb",
      "number": 10,
      "content": "Ngài đánh bại nhiều dân tộc,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.970Z",
      "updatedAt": "2024-09-09T04:52:11.970Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "759711a9-1f81-41ed-82a4-88a91d598efd",
      "number": 10,
      "content": "Và tiêu diệt các vua mạnh mẽ;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.976Z",
      "updatedAt": "2024-09-09T04:52:11.976Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "9416a6a7-9d1f-4f53-b0dc-0125ed9b5fec",
      "number": 11,
      "content": "Si-hôn vua dân A-mô-ri, Óc vua Ba-san,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.981Z",
      "updatedAt": "2024-09-09T04:52:11.981Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "a195691b-0694-47c9-a877-2b0cde6afe5e",
      "number": 11,
      "content": "Và tất cả các vương quốc ở Ca-na-an.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.986Z",
      "updatedAt": "2024-09-09T04:52:11.986Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "5b4341fc-6765-4ff1-80ff-728234f38441",
      "number": 12,
      "content": "Ngài ban xứ của họ để làm sản nghiệp,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.992Z",
      "updatedAt": "2024-09-09T04:52:11.992Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "00993b34-0d37-4063-84ff-d8fd8a082e1d",
      "number": 12,
      "content": "Một sản nghiệp cho dân I-sơ-ra-ên của Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:11.997Z",
      "updatedAt": "2024-09-09T04:52:11.997Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "1230e2aa-861f-4593-9289-2580df660c8a",
      "number": 13,
      "content": "Chúa ôi, danh Ngài sẽ trường tồn mãi mãi;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.003Z",
      "updatedAt": "2024-09-09T04:52:12.003Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "59db7c5a-0660-409e-a291-c22f33509e43",
      "number": 13,
      "content": "Chúa ôi, công ơn Ngài sẽ được ghi nhớ từ đời nọ đến đời kia,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.008Z",
      "updatedAt": "2024-09-09T04:52:12.008Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "2d85df93-6b29-4827-b660-d36fde9660b2",
      "number": 14,
      "content": "Vì Chúa sẽ xét đoán dân Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.014Z",
      "updatedAt": "2024-09-09T04:52:12.014Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "b419f1b7-c0b1-4aab-b8bb-3ca265ec07bc",
      "number": 14,
      "content": "Ngài sẽ tỏ lòng thương xót đối với các tôi tớ Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.019Z",
      "updatedAt": "2024-09-09T04:52:12.019Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "9820941a-008d-4e41-9e54-e3e6087034c3",
      "number": 15,
      "content": "Các thần tượng của các dân chẳng qua là bạc và vàng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.023Z",
      "updatedAt": "2024-09-09T04:52:12.023Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "705d468c-7c5c-497a-91b5-d6fded3b7aec",
      "number": 15,
      "content": "Chúng chỉ là sản phẩm do tay người làm ra.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.028Z",
      "updatedAt": "2024-09-09T04:52:12.028Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "cb24b4ec-8cd4-4cec-8713-3c7d1af24d27",
      "number": 16,
      "content": "Chúng có miệng mà chẳng nói;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.033Z",
      "updatedAt": "2024-09-09T04:52:12.033Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "6762023f-5be0-4121-b336-63909ebc273b",
      "number": 16,
      "content": "Chúng có mắt mà chẳng thấy;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.039Z",
      "updatedAt": "2024-09-09T04:52:12.039Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "705182f5-73b7-4880-9206-a21da85c2607",
      "number": 17,
      "content": "Chúng có tai mà chẳng nghe;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.044Z",
      "updatedAt": "2024-09-09T04:52:12.044Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "d422c7ab-cc8d-4bec-9cc1-0bdc17a24d10",
      "number": 17,
      "content": "Không có chút hơi thở nào trong miệng chúng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.049Z",
      "updatedAt": "2024-09-09T04:52:12.049Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "a4c9eb67-954d-4633-8ca0-4e4632fbb682",
      "number": 18,
      "content": "Nguyện những kẻ làm thần tượng sẽ trở nên như chúng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.055Z",
      "updatedAt": "2024-09-09T04:52:12.055Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "cd0b2084-8cce-4999-bbb6-003469a299ba",
      "number": 18,
      "content": "Và những ai tin cậy vào thần tượng cũng giống như vậy.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.060Z",
      "updatedAt": "2024-09-09T04:52:12.060Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "8cfc13b4-50be-4527-8e96-babfb45f025e",
      "number": 19,
      "content": "Hỡi nhà I-sơ-ra-ên, hãy chúc tụng Chúa!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.065Z",
      "updatedAt": "2024-09-09T04:52:12.065Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "e79bdd37-de91-4350-84ac-746e2460d816",
      "number": 19,
      "content": "Hỡi nhà A-rôn, hãy chúc tụng Chúa!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.070Z",
      "updatedAt": "2024-09-09T04:52:12.070Z",
      "chapterId": "ea8432d5-184e-4142-aa28-8f19cf614785"
    },
    {
      "id": "10493bb7-2b82-4903-bd55-6e1baffda0fd",
      "number": 20,
      "content": "Hỡi nhà Lê-vi, hãy chúc tụng Chúa!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:52:12.075Z",
      "updatedAt": "2024-09-09T04:52:12.075Z",
