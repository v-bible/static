{
  "chapter": {
    "id": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316",
    "number": 47,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2047&version=BD2011",
    "createdAt": "2024-07-12T04:03:46.541Z",
    "updatedAt": "2024-07-24T15:07:47.480Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "3b98489d-505b-484a-8175-263f3169cefc",
      "number": 1,
      "content": "Hãy vỗ tay, hỡi mọi người!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.061Z",
      "updatedAt": "2024-07-31T11:32:03.024Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "2e3ed8c3-5d25-4fa7-9430-af4dabbc1da3",
      "number": 1,
      "content": "Hãy lớn tiếng reo mừng ca ngợi Ðức Chúa Trời!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.061Z",
      "updatedAt": "2024-07-31T11:32:03.032Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "c36be9e5-5da8-490f-9836-0e73c4080b83",
      "number": 2,
      "content": "Vì Chúa, Ðấng Tối Cao, thật đáng kính sợ.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.071Z",
      "updatedAt": "2024-07-31T11:32:03.039Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "03608054-3503-4525-829b-dc697f894999",
      "number": 2,
      "content": "Ngài là Vua cao cả trên toàn thế giới.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.071Z",
      "updatedAt": "2024-07-31T11:32:03.047Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "c29a524b-1372-40a2-95cf-cd111c167569",
      "number": 3,
      "content": "Ngài làm cho các dân bị khuất phục dưới chúng tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.073Z",
      "updatedAt": "2024-07-31T11:32:03.054Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "0d79b641-bfaf-4c33-a5a7-0caf8e20f5f6",
      "number": 3,
      "content": "Và các nước ở dưới chân chúng tôi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.073Z",
      "updatedAt": "2024-07-31T11:32:03.062Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "6395addd-b28b-46f6-9004-7823e2402f62",
      "number": 4,
      "content": "Ngài chọn cơ nghiệp cho chúng tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.075Z",
      "updatedAt": "2024-07-31T11:32:03.070Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "65caafd1-b421-4b1e-8791-e390b2cafca8",
      "number": 4,
      "content": "Vinh hiển của Gia-cốp, người Ngài yêu mến. (Sê-la)",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.075Z",
      "updatedAt": "2024-07-31T11:32:03.077Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "69cc6f2e-a75d-4464-91e9-2d0c0416250b",
      "number": 5,
      "content": "Ðức Chúa Trời ngự lên giữa tiếng reo mừng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.078Z",
      "updatedAt": "2024-07-31T11:32:03.087Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "3b290838-6331-49ec-b932-aa1b021b3301",
      "number": 5,
      "content": "Chúa ngự lên giữa tiếng kèn trỗi lên vang lừng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.078Z",
      "updatedAt": "2024-07-31T11:32:03.095Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "c2affc60-7636-4ed1-8413-2f7b39e8d1a4",
      "number": 6,
      "content": "Hãy ca ngợi Ðức Chúa Trời; khá hát lên những bài chúc tụng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.080Z",
      "updatedAt": "2024-07-31T11:32:03.102Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "53b5a269-d75a-49dc-aac1-8ac7ba55f1ee",
      "number": 6,
      "content": "Hãy ca ngợi Vua chúng ta; khá hát lên những bài chúc tụng,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.080Z",
      "updatedAt": "2024-07-31T11:32:03.110Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "cba8276a-7f32-4761-bc84-3521f70aa4e5",
      "number": 7,
      "content": "Vì Ðức Chúa Trời là Vua của toàn thế giới;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.083Z",
      "updatedAt": "2024-07-31T11:32:03.117Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "abf94dca-2feb-4679-98f9-0e4297d4ae0c",
      "number": 7,
      "content": "Hãy ca ngợi Ngài bằng những bài thánh thi chúc tụng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.083Z",
      "updatedAt": "2024-07-31T11:32:03.125Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "87438325-bd31-4e76-b4e3-9f8dd2021f41",
      "number": 8,
      "content": "Ðức Chúa Trời trị vì trên các dân các nước;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.089Z",
      "updatedAt": "2024-07-31T11:32:03.135Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "8da73061-79c5-4006-9872-e1a78b14d2de",
      "number": 8,
      "content": "Ðức Chúa Trời ngự trên ngôi thánh của Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.089Z",
      "updatedAt": "2024-07-31T11:32:03.142Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "24c1a8e9-df1a-44cf-ac01-05a651678217",
      "number": 9,
      "content": "Các lãnh tụ thế giới họp nhau lại làm dân của Ðức Chúa Trời của Áp-ra-ham,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.094Z",
      "updatedAt": "2024-07-31T11:32:03.150Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "cc974e44-d848-47f6-ab87-700031ba0bd4",
      "number": 9,
      "content": "Bởi vì các thủ lãnh trên thế gian đều thuộc về Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.094Z",
      "updatedAt": "2024-07-31T11:32:03.157Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    },
    {
      "id": "1b9e8a49-f5d0-46da-9e45-c29356adc290",
      "number": 9,
      "content": "Ngài được tôn chí cao!",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:52:51.094Z",
      "updatedAt": "2024-07-31T11:32:03.165Z",
      "chapterId": "6e7db0bc-ed4f-472c-b3d5-f38ad3e80316"
    }
  ],
