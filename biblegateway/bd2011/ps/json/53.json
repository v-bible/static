{
  "chapter": {
    "id": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5",
    "number": 53,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2053&version=BD2011",
    "createdAt": "2024-09-09T04:13:00.210Z",
    "updatedAt": "2024-09-09T04:13:00.210Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "e4d5b823-b285-48b0-9dd3-768c96a6bc0d",
      "number": 1,
      "content": "Kẻ dại nói trong lòng rằng, “Không có Ðức Chúa Trời!”",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.887Z",
      "updatedAt": "2024-09-09T04:45:23.887Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "65a761e3-272f-40c2-820e-fc28f76655dc",
      "number": 1,
      "content": "Chúng nó đều bại hoại;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.899Z",
      "updatedAt": "2024-09-09T04:45:23.899Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "e8bc5c3f-359f-4ad5-8e2f-bfffb5fd3bcc",
      "number": 1,
      "content": "Chúng nó phạm những tội lỗi đáng ghê tởm;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.901Z",
      "updatedAt": "2024-09-09T04:45:23.901Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "6dc23d64-ca2c-46af-a318-5d05ecdff8a4",
      "number": 1,
      "content": "Chẳng ai làm điều thiện.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.903Z",
      "updatedAt": "2024-09-09T04:45:23.903Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "fa78ff3a-f781-4244-9f81-49312d2ee4fd",
      "number": 2,
      "content": "Ðức Chúa Trời từ trời nhìn xuống con cái loài người,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.905Z",
      "updatedAt": "2024-09-09T04:45:23.905Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "afe827fa-aaa9-404d-9d7c-0499ab7c1806",
      "number": 2,
      "content": "Xem có ai là người trí thức, biết tìm kiếm Ðức Chúa Trời chăng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.907Z",
      "updatedAt": "2024-09-09T04:45:23.907Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "b0f39c8d-019d-411b-b10e-3cd4c5eb94ec",
      "number": 3,
      "content": "Họ thảy đều bại hoại;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.908Z",
      "updatedAt": "2024-09-09T04:45:23.908Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "e6130b8b-ad92-4103-b467-3cb52cc3532e",
      "number": 3,
      "content": "Họ cùng nhau trở nên ô uế;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.910Z",
      "updatedAt": "2024-09-09T04:45:23.910Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "9824f5fb-21a8-4ce1-8f4b-5ec0418e5d37",
      "number": 3,
      "content": "Chẳng có người nào làm điều thiện,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.912Z",
      "updatedAt": "2024-09-09T04:45:23.912Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "48a8ff80-1499-468f-b189-38b4e86b4722",
      "number": 3,
      "content": "Dẫu một người cũng không.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.915Z",
      "updatedAt": "2024-09-09T04:45:23.915Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "afbcfb57-26af-4239-b0a2-f734e00511ff",
      "number": 4,
      "content": "Tại sao những kẻ làm ác chẳng hiểu biết?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.920Z",
      "updatedAt": "2024-09-09T04:45:23.920Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "3fe7dc9d-0186-4320-b880-a6f316880853",
      "number": 4,
      "content": "Tại sao chúng cứ ăn nuốt dân tôi như thể ăn bánh?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.925Z",
      "updatedAt": "2024-09-09T04:45:23.925Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "acd7ebc5-11b8-40f8-9aa5-a084c4d709ee",
      "number": 4,
      "content": "Tại sao chúng chẳng kêu cầu Ðức Chúa Trời?",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.929Z",
      "updatedAt": "2024-09-09T04:45:23.929Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "cbdf3c48-db73-4c0b-8d43-4cd41221bb2b",
      "number": 5,
      "content": "Chúng sẽ rất sợ hãi dầu chẳng có gì đáng sợ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.934Z",
      "updatedAt": "2024-09-09T04:45:23.934Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "80967130-3dd0-47cc-87c9-3dba0f8b03b0",
      "number": 5,
      "content": "Vì Ðức Chúa Trời sẽ rải xương những kẻ vây hãm bạn;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.939Z",
      "updatedAt": "2024-09-09T04:45:23.939Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "3ff4e28a-551f-45a1-9a24-4f0eb643725c",
      "number": 5,
      "content": "Bạn làm cho chúng phải hổ thẹn,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.943Z",
      "updatedAt": "2024-09-09T04:45:23.943Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "b2623bd1-41ac-4a7b-9b81-6f2c1212b34c",
      "number": 5,
      "content": "Vì Ðức Chúa Trời đã loại bỏ chúng.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.948Z",
      "updatedAt": "2024-09-09T04:45:23.948Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "03c9ab93-e9a7-4abb-acc5-1a70e65699fd",
      "number": 6,
      "content": "Kìa, ơn cứu rỗi của I-sơ-ra-ên đến từ Si-ôn!",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.954Z",
      "updatedAt": "2024-09-09T04:45:23.954Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "99888999-a1b9-4348-8760-9f95642cb0d9",
      "number": 6,
      "content": "Khi Ðức Chúa Trời tái lập vận mạng của con dân Ngài,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.959Z",
      "updatedAt": "2024-09-09T04:45:23.959Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    },
    {
      "id": "6a0a2222-970a-463d-a1bb-1374472eabf9",
      "number": 6,
      "content": "Gia-cốp sẽ mừng rỡ, I-sơ-ra-ên sẽ vui mừng.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:45:23.970Z",
      "updatedAt": "2024-09-09T04:45:23.970Z",
      "chapterId": "7447e523-81d2-44f2-9d7a-13ca93f6b0f5"
    }
  ],
