{
  "chapter": {
    "id": "60d7c9f7-b950-4ddb-a30d-2e6161be455e",
    "number": 11,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2011&version=BD2011",
    "createdAt": "2024-09-09T04:12:59.338Z",
    "updatedAt": "2024-09-09T04:12:59.338Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "5991b7cb-73b8-409a-8b55-448ddd99575c",
      "number": 1,
      "content": "Tôi nương náu mình nơi Chúa.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.313Z",
      "updatedAt": "2024-09-09T04:41:52.313Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "6f49b8c2-56fc-49e3-aa83-12fec9c5b5cd",
      "number": 1,
      "content": "Thế mà sao các người lại bảo linh hồn tôi rằng,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.324Z",
      "updatedAt": "2024-09-09T04:41:52.324Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "ebe412b0-af29-4d49-b44c-23deebbcaf3c",
      "number": 1,
      "content": "“Hãy làm như con chim và bay lên núi trốn đi”?",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.326Z",
      "updatedAt": "2024-09-09T04:41:52.326Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "bfbe3ba1-48e9-4f72-87e8-46dc47eae6db",
      "number": 2,
      "content": "Vì kìa, kẻ ác đã giương cung;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.328Z",
      "updatedAt": "2024-09-09T04:41:52.328Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "1e2d9494-6d2a-453f-a815-784b84a81719",
      "number": 2,
      "content": "Chúng đã đặt mũi tên vào dây cung,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.330Z",
      "updatedAt": "2024-09-09T04:41:52.330Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "61441792-c7ca-42cf-bdb6-b1e39b8ee01a",
      "number": 2,
      "content": "Ðể từ trong bóng tối nhắm bắn người có lòng ngay thẳng.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.332Z",
      "updatedAt": "2024-09-09T04:41:52.332Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "c5e9c265-d9af-4d64-9b4c-2a5945ddadcc",
      "number": 3,
      "content": "Nếu các nền tảng bị phá hủy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.334Z",
      "updatedAt": "2024-09-09T04:41:52.334Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "168c5f61-f0fe-419d-8635-46587f29bd70",
      "number": 3,
      "content": "Người ngay lành còn có thể làm gì?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.336Z",
      "updatedAt": "2024-09-09T04:41:52.336Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "b5333409-3e18-437b-98ba-d0cd38015cc8",
      "number": 4,
      "content": "Chúa ngự trong đền thánh Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.338Z",
      "updatedAt": "2024-09-09T04:41:52.338Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "17692593-bb40-4e92-9357-ff7e1d2b8eed",
      "number": 4,
      "content": "Chúa ngự trên ngôi Ngài trên thiên đàng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.340Z",
      "updatedAt": "2024-09-09T04:41:52.340Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "94fc362d-3b17-42fb-aa64-dce5e2cf10ea",
      "number": 4,
      "content": "Mắt Ngài nhìn thấy rõ hết;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.342Z",
      "updatedAt": "2024-09-09T04:41:52.342Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "05513203-a092-49ea-b4e4-922bc0884113",
      "number": 4,
      "content": "Mí mắt Ngài dò xét con cái loài người.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.344Z",
      "updatedAt": "2024-09-09T04:41:52.344Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "3b65a785-7be6-4f89-b403-3cd1ecfb8d81",
      "number": 5,
      "content": "Chúa thử nghiệm người ngay lành,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.346Z",
      "updatedAt": "2024-09-09T04:41:52.346Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "679d1f76-443c-4136-949d-332b2c44b2e5",
      "number": 5,
      "content": "Nhưng linh hồn Ngài ghét kẻ ác và kẻ ham mến bạo tàn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.348Z",
      "updatedAt": "2024-09-09T04:41:52.348Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "df3f8404-3455-475d-9b7f-84345d49b7e6",
      "number": 6,
      "content": "Ngài sẽ giáng mưa than lửa và diêm sinh xuống kẻ gian ác,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.351Z",
      "updatedAt": "2024-09-09T04:41:52.351Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "429f62c0-212e-49e2-948c-13f0f4c7cf31",
      "number": 6,
      "content": "Gió phỏng sẽ là phần trong chén của chúng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.355Z",
      "updatedAt": "2024-09-09T04:41:52.355Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "ba9fd948-fec3-4351-b878-0ebba5b444b8",
      "number": 7,
      "content": "Vì Chúa là Ðấng công chính.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.360Z",
      "updatedAt": "2024-09-09T04:41:52.360Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "b03c0617-9388-4272-bd03-5613b37f5a76",
      "number": 7,
      "content": "Ngài yêu mến sự công chính.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.364Z",
      "updatedAt": "2024-09-09T04:41:52.364Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    },
    {
      "id": "94c07610-4ec8-41b2-8f74-d8ab44350ac2",
      "number": 7,
      "content": "Người ngay lành sẽ được nhìn thấy thánh nhan Ngài.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:41:52.369Z",
      "updatedAt": "2024-09-09T04:41:52.369Z",
      "chapterId": "60d7c9f7-b950-4ddb-a30d-2e6161be455e"
    }
  ],
