{
  "chapter": {
    "id": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c",
    "number": 106,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20106&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.520Z",
    "updatedAt": "2024-09-24T01:00:44.520Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "76cc000e-766c-4b13-acb1-a068d69ee8dd",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.752Z",
      "updatedAt": "2024-09-24T02:07:28.752Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "609ce584-0d6a-4885-8f20-d9f96ef90626",
      "number": 48,
      "content": "Hãy ca ngợi Chúa!",
      "order": 3,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.157Z",
      "updatedAt": "2024-09-24T02:07:29.157Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "a67d608b-4671-4ad0-9076-a5c82952dbf5",
      "number": 1,
      "content": "Hãy cảm tạ Chúa, vì Ngài thật tốt,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.756Z",
      "updatedAt": "2024-09-24T02:07:28.756Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "9cac1062-74a5-47df-8d7a-648b1c33269b",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.758Z",
      "updatedAt": "2024-09-24T02:07:28.758Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "2c012b05-535c-4e77-95d2-761a6a2f577d",
      "number": 2,
      "content": "Ai có thể thuật lại những việc quyền năng của Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.760Z",
      "updatedAt": "2024-09-24T02:07:28.760Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "6c17d20b-4043-4abb-a9cf-583baf14c8ec",
      "number": 2,
      "content": "Hoặc có thể rao ra tất cả lời ca ngợi về Ngài?",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.762Z",
      "updatedAt": "2024-09-24T02:07:28.762Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "cabac250-b871-4215-8481-aec80c003da6",
      "number": 3,
      "content": "Phước hạnh thay cho những người duy trì công lý,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.764Z",
      "updatedAt": "2024-09-24T02:07:28.764Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "2cf24282-84a8-443a-8586-9c6dc846c5dc",
      "number": 3,
      "content": "Tức những người làm điều công chính ở mọi lúc mọi nơi!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.766Z",
      "updatedAt": "2024-09-24T02:07:28.766Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "723d1b7c-4ebf-4cf1-a63a-e6340923a892",
      "number": 4,
      "content": "Chúa ôi, xin nhớ đến con khi Ngài ban ơn cho dân Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.767Z",
      "updatedAt": "2024-09-24T02:07:28.767Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "7be7d07b-d433-4834-a02c-aaebad693700",
      "number": 4,
      "content": "Xin đoái đến con khi Ngài ban ơn cứu rỗi của Ngài,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.769Z",
      "updatedAt": "2024-09-24T02:07:28.769Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "b633f965-04af-43e6-86c1-e4f5ea0f0e1a",
      "number": 5,
      "content": "Ðể con được thấy sự thịnh vượng của người Ngài chọn,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.771Z",
      "updatedAt": "2024-09-24T02:07:28.771Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "a6e3ac39-62c2-41ba-8737-a6b8367a43f0",
      "number": 5,
      "content": "Ðược hân hoan trong niềm vui của người dân trong nước Ngài,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.773Z",
      "updatedAt": "2024-09-24T02:07:28.773Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "cfcc14f3-512b-4107-b716-a36a3e1a6e4d",
      "number": 5,
      "content": "Và được vinh hiển với những người hưởng cơ nghiệp Ngài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.775Z",
      "updatedAt": "2024-09-24T02:07:28.775Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "a2c00ca8-5c71-4cd6-a1cb-1cfead9aa157",
      "number": 6,
      "content": "Giống như tổ tiên chúng con, chúng con đã phạm tội;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.777Z",
      "updatedAt": "2024-09-24T02:07:28.777Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "86e93159-9bad-4357-926e-1dc0b7797199",
      "number": 6,
      "content": "Chúng con đã làm những điều xấu xa tội lỗi;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.779Z",
      "updatedAt": "2024-09-24T02:07:28.779Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "7018354c-414f-4384-b253-42781f227a00",
      "number": 6,
      "content": "Chúng con đã hành động cách gian tà.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.781Z",
      "updatedAt": "2024-09-24T02:07:28.781Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "25a4b47d-be5e-4b13-bfce-9db83a633117",
      "number": 7,
      "content": "Khi ở Ai-cập tổ tiên chúng con đã không nghĩ đến những việc diệu kỳ của Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.783Z",
      "updatedAt": "2024-09-24T02:07:28.783Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "442beab6-10b6-4b43-895d-da2c8d9d5950",
      "number": 7,
      "content": "Họ đã chẳng nhớ đến lòng thương xót lớn lao của Ngài;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.785Z",
      "updatedAt": "2024-09-24T02:07:28.785Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "c853429f-86fa-4f1d-93cf-fda96ca2a511",
      "number": 7,
      "content": "Họ đã chọc giận Ngài bên bờ biển, tức bên bờ Hồng Hải.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.787Z",
      "updatedAt": "2024-09-24T02:07:28.787Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "13158ba7-6735-41a9-a947-59beb1081a77",
      "number": 8,
      "content": "Dầu vậy vì cớ danh Ngài, Ngài đã cứu họ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.789Z",
      "updatedAt": "2024-09-24T02:07:28.789Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "307d8118-5f62-470e-93c3-afc8ff11d9c4",
      "number": 8,
      "content": "Ðể quyền năng Ngài được biết đến khắp nơi.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.790Z",
      "updatedAt": "2024-09-24T02:07:28.790Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "270bc29a-b388-45ee-b04e-48286f763ceb",
      "number": 9,
      "content": "Ngài quở trách Hồng Hải, và nó liền khô cạn;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.792Z",
      "updatedAt": "2024-09-24T02:07:28.792Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "d2c4eddc-bfd4-45dc-9929-e33d55ef2341",
      "number": 9,
      "content": "Ngài dẫn họ qua biển sâu như qua một sa mạc khô khan.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.794Z",
      "updatedAt": "2024-09-24T02:07:28.794Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "c2226398-60ca-4e66-a660-5caa6232247c",
      "number": 10,
      "content": "Ngài giải cứu họ khỏi tay những kẻ ghét họ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.797Z",
      "updatedAt": "2024-09-24T02:07:28.797Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "516d6c9f-406c-494b-9cad-b7a8cc8303f2",
      "number": 10,
      "content": "Và cứu chuộc họ khỏi tay những kẻ thù của họ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.799Z",
      "updatedAt": "2024-09-24T02:07:28.799Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "c5c7ad12-4928-4c6b-b9ac-7643b0888242",
      "number": 11,
      "content": "Nước biển đã bao phủ những kẻ thù của họ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.801Z",
      "updatedAt": "2024-09-24T02:07:28.801Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "9d1e26ee-a247-481d-8050-880abd3be97f",
      "number": 11,
      "content": "Không chừa lại ai, dù chỉ một người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.804Z",
      "updatedAt": "2024-09-24T02:07:28.804Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "6a481243-2f92-456e-a1a4-24b17563bf3d",
      "number": 12,
      "content": "Bấy giờ họ mới chịu tin lời Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.806Z",
      "updatedAt": "2024-09-24T02:07:28.806Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "e1d2f482-f2b6-416a-a77d-191e49d28c32",
      "number": 12,
      "content": "Họ mới cất tiếng chúc tụng Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.808Z",
      "updatedAt": "2024-09-24T02:07:28.808Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "ba4d89f3-fcd6-4329-8246-4067367be1ef",
      "number": 13,
      "content": "Nhưng chẳng bao lâu họ lại quên các công việc Ngài;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.811Z",
      "updatedAt": "2024-09-24T02:07:28.811Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "dc65b185-56ec-4dee-ac97-514f769369e5",
      "number": 13,
      "content": "Họ chẳng màng đến việc chờ đợi để cầu hỏi ý Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.813Z",
      "updatedAt": "2024-09-24T02:07:28.813Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "d2f30202-1819-4f8d-a111-28e2988d3556",
      "number": 14,
      "content": "Họ chiều theo dục vọng mình quá đỗi trong đồng hoang;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.816Z",
      "updatedAt": "2024-09-24T02:07:28.816Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "4b98b630-0c00-4c32-b2e5-96d1194d85ab",
      "number": 14,
      "content": "Và dám thách đố cả đến Ðức Chúa Trời trong sa mạc.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.819Z",
      "updatedAt": "2024-09-24T02:07:28.819Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "5b3de11f-58b7-4d52-92bc-758a44c1c5e0",
      "number": 15,
      "content": "Ngài cho họ những gì họ đòi hỏi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.822Z",
      "updatedAt": "2024-09-24T02:07:28.822Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "b6bd0f4c-7820-4741-9313-3339d7e815b6",
      "number": 15,
      "content": "Nhưng cũng sai đến một ôn dịch chết người.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.825Z",
      "updatedAt": "2024-09-24T02:07:28.825Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "7454c95b-19cd-43e5-9cc6-9dfeddd19f9c",
      "number": 16,
      "content": "Trong doanh trại họ ganh tị với Môi-se,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.829Z",
      "updatedAt": "2024-09-24T02:07:28.829Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "b406c3e4-bdab-423a-b1d6-87de4ba32021",
      "number": 16,
      "content": "Và với A-rôn người thánh của Chúa.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.833Z",
      "updatedAt": "2024-09-24T02:07:28.833Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "d2551ba3-7372-4d0f-9edc-d803145bed80",
      "number": 17,
      "content": "Ðất mở miệng ra và nuốt chửng Ða-than,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.837Z",
      "updatedAt": "2024-09-24T02:07:28.837Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "c3c8ca38-975c-4d6b-8bc5-478d7ef75182",
      "number": 17,
      "content": "Và ngoạm lấy đồng bọn của A-bi-ram.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.841Z",
      "updatedAt": "2024-09-24T02:07:28.841Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "25d61b96-598c-4f06-95a0-3b8e508b45c8",
      "number": 18,
      "content": "Một ngọn lửa đã bùng lên giữa đám người phản loạn;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.846Z",
      "updatedAt": "2024-09-24T02:07:28.846Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "f92ad5e0-1066-40b5-9ff4-8d529bd8699f",
      "number": 18,
      "content": "Ngọn lửa hừng đã thiêu rụi những kẻ gian tà.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.850Z",
      "updatedAt": "2024-09-24T02:07:28.850Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "f1fb59ec-7702-41da-b587-6c84e4dcd9ab",
      "number": 19,
      "content": "Họ đã đúc một tượng bò con ở Hô-rếp,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.854Z",
      "updatedAt": "2024-09-24T02:07:28.854Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "f7b02b4d-89e5-455c-8c8e-c65473d4f1ca",
      "number": 19,
      "content": "Rồi quỳ mọp xuống thờ lạy tượng vàng họ đúc.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.858Z",
      "updatedAt": "2024-09-24T02:07:28.858Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "b0f0331c-f916-41a5-bad4-684acc128b8f",
      "number": 20,
      "content": "Họ đã đánh đổi vinh hiển mình cho hình tượng một con bò,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.863Z",
      "updatedAt": "2024-09-24T02:07:28.863Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "9529ef67-b599-4dd5-a605-52c0579a6e06",
      "number": 20,
      "content": "Một con vật ăn cỏ.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.867Z",
      "updatedAt": "2024-09-24T02:07:28.867Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "297acd61-d2b3-40f5-8b4b-a64ed1b895ad",
      "number": 21,
      "content": "Họ quên rằng Ðức Chúa Trời là Ðấng Giải Cứu họ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.871Z",
      "updatedAt": "2024-09-24T02:07:28.871Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "570ea99c-aa82-46d5-9f1e-189d9893dbab",
      "number": 21,
      "content": "Ðấng làm những việc lớn lao ở Ai-cập,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.876Z",
      "updatedAt": "2024-09-24T02:07:28.876Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "4c49d1dc-86f0-466b-a49a-4aac322fd54c",
      "number": 22,
      "content": "Những việc lạ lùng trong xứ của Cham,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.880Z",
      "updatedAt": "2024-09-24T02:07:28.880Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "91cdb1ce-c7cd-479b-80e6-cd51579cf095",
      "number": 22,
      "content": "Những việc kinh hoàng bên bờ Hồng Hải.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.886Z",
      "updatedAt": "2024-09-24T02:07:28.886Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "ba71b4bb-db72-4425-8c50-76f0d556aa74",
      "number": 23,
      "content": "Vì thế Ngài phán rằng Ngài sẽ tiêu diệt họ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.890Z",
      "updatedAt": "2024-09-24T02:07:28.890Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "5bc4440f-82de-4ee0-a41f-c3b00b029078",
      "number": 23,
      "content": "May nhờ có Môi-se, người Ngài đã chọn, đứng mũi chịu sào,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.895Z",
      "updatedAt": "2024-09-24T02:07:28.895Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "52ef4a37-66f0-45af-b2c5-3ee54af623fb",
      "number": 23,
      "content": "Ðể van nài Ngài nguôi ngoai cơn giận mà không diệt họ đi.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.900Z",
      "updatedAt": "2024-09-24T02:07:28.900Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "de60fc73-a80e-44bf-81b6-3d5bfcfaa9e9",
      "number": 24,
      "content": "Kế đến họ khinh chê đất hứa;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.904Z",
      "updatedAt": "2024-09-24T02:07:28.904Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "ee560c6a-1d02-4a71-ad38-d3db35eb74f3",
      "number": 24,
      "content": "Họ không tin vào lời hứa của Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.909Z",
      "updatedAt": "2024-09-24T02:07:28.909Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "f185029d-432c-4aac-a95c-d6dd7456c9bf",
      "number": 25,
      "content": "Trong lều họ, họ lầm bầm oán trách;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.914Z",
      "updatedAt": "2024-09-24T02:07:28.914Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "a6e4d644-6a3c-462a-9cd9-08885b29823a",
      "number": 25,
      "content": "Chẳng muốn nghe tiếng Chúa phán truyền.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.919Z",
      "updatedAt": "2024-09-24T02:07:28.919Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "06d461e8-ee23-421e-85b6-35acd56668ec",
      "number": 26,
      "content": "Vì thế Ngài đã đưa tay lên và thề rằng,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.923Z",
      "updatedAt": "2024-09-24T02:07:28.923Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "ee103d27-4b29-482d-9e41-b1e61bb9a057",
      "number": 26,
      "content": "Ngài sẽ cho họ bỏ thây trong sa mạc,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.927Z",
      "updatedAt": "2024-09-24T02:07:28.927Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "c23e5976-1db8-4119-a3f0-d105ebf5b69d",
      "number": 27,
      "content": "Rải con cháu họ ra trong các quốc gia,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.932Z",
      "updatedAt": "2024-09-24T02:07:28.932Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "12eadc89-dac0-4fff-9024-59b10a74c094",
      "number": 27,
      "content": "Và làm cho họ tản lạc khắp nơi trong các nước.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.936Z",
      "updatedAt": "2024-09-24T02:07:28.936Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "706575d4-628b-48cc-b06b-99cdea02c1c3",
      "number": 28,
      "content": "Ðã thế họ còn thờ lạy Ba-anh Pê-ô;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.940Z",
      "updatedAt": "2024-09-24T02:07:28.940Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "52b27ded-70b7-4c72-a5bb-435fbf1f03ab",
      "number": 28,
      "content": "Và ăn đồ cúng cho người chết.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.944Z",
      "updatedAt": "2024-09-24T02:07:28.944Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "eb984436-26a6-4aa6-8aec-07099d7bde7f",
      "number": 29,
      "content": "Họ đã dùng các hành động mình để chọc giận Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.949Z",
      "updatedAt": "2024-09-24T02:07:28.949Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "868e8b32-1bfd-4689-9933-3849e60a7c45",
      "number": 29,
      "content": "Nên ôn dịch đã bộc phát ở giữa họ.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.953Z",
      "updatedAt": "2024-09-24T02:07:28.953Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "c7a1c6ed-36e7-44af-ac0f-603d54cc57e7",
      "number": 30,
      "content": "May nhờ có Phi-nê-a đứng dậy và cầu thay,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.957Z",
      "updatedAt": "2024-09-24T02:07:28.957Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "792f206a-557a-4ee7-857c-a6cdd70750d0",
      "number": 30,
      "content": "Nên bệnh dịch mới ngừng ngay tại chỗ.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.961Z",
      "updatedAt": "2024-09-24T02:07:28.961Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "dae6497d-e786-4bd3-9267-34c7ea822cd9",
      "number": 31,
      "content": "Ðiều ấy được xem là công đức của ông ta,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.966Z",
      "updatedAt": "2024-09-24T02:07:28.966Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "02cdec01-6870-4e1f-b545-33b887939aee",
      "number": 31,
      "content": "Và được ghi nhớ từ đời này sang đời khác.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.970Z",
      "updatedAt": "2024-09-24T02:07:28.970Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "f9889cea-793b-44ef-911c-6f719540858f",
      "number": 32,
      "content": "Họ đã chọc giận Ngài trong vụ nước ở Mê-ri-ba,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.978Z",
      "updatedAt": "2024-09-24T02:07:28.978Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "dfde32fa-bf1b-4c8b-8741-535038a346b4",
      "number": 32,
      "content": "Cũng tại vì họ mà Môi-se đã mang lụy vào thân.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.983Z",
      "updatedAt": "2024-09-24T02:07:28.983Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "ec8adf86-1b21-4a19-8f9c-d65a83413e9d",
      "number": 33,
      "content": "Bởi họ đã làm cho tâm thần ông bực dọc,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.988Z",
      "updatedAt": "2024-09-24T02:07:28.988Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "e743423f-3570-4ab6-b98c-186e91c2d678",
      "number": 33,
      "content": "Nên môi ông đã nói những lời thiếu đắn đo suy nghĩ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.992Z",
      "updatedAt": "2024-09-24T02:07:28.992Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "fcbafb1d-c604-42b8-9342-464732b1871e",
      "number": 34,
      "content": "Họ cũng chẳng tiêu diệt các dân trong xứ,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:28.997Z",
      "updatedAt": "2024-09-24T02:07:28.997Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "5407840f-5342-4529-b007-00b8e5b88fe3",
      "number": 34,
      "content": "Ðể làm đúng theo mạng lịnh Chúa đã truyền,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.001Z",
      "updatedAt": "2024-09-24T02:07:29.001Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "db752f5b-5b1c-4400-95fc-58152b209780",
      "number": 35,
      "content": "Nhưng họ đã sống chung lộn với các dân tộc ấy,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.006Z",
      "updatedAt": "2024-09-24T02:07:29.006Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "d8a5a498-5452-4919-8313-cec149406143",
      "number": 35,
      "content": "Và học theo các thói tục của chúng.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.010Z",
      "updatedAt": "2024-09-24T02:07:29.010Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "928a0a5d-97a4-4f70-be39-25871134a8f1",
      "number": 36,
      "content": "Ðã thế họ còn thờ lạy các thần tượng của chúng;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.016Z",
      "updatedAt": "2024-09-24T02:07:29.016Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "1fe01318-684e-42c3-a6f3-31b1247e69ec",
      "number": 36,
      "content": "Các thần tượng ấy đã trở thành cái bẫy cho họ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.020Z",
      "updatedAt": "2024-09-24T02:07:29.020Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "81c260d3-4989-4cbe-9426-6c5e1bfd3274",
      "number": 37,
      "content": "Thậm chí họ còn sát tế các con trai và các con gái mình,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.025Z",
      "updatedAt": "2024-09-24T02:07:29.025Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "9450e73a-e57a-4390-8f72-4fdcf5d7e075",
      "number": 37,
      "content": "Ðể cúng tế cho các tà linh ma quỷ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.029Z",
      "updatedAt": "2024-09-24T02:07:29.029Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "7530d637-100c-4af5-95ea-f9303b9fdd90",
      "number": 38,
      "content": "Họ đã làm đổ máu vô tội ra,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.033Z",
      "updatedAt": "2024-09-24T02:07:29.033Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "7e1f9083-39d2-49a2-b032-3cf72d5a8ac3",
      "number": 38,
      "content": "Tức máu của các con trai và các con gái họ,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.037Z",
      "updatedAt": "2024-09-24T02:07:29.037Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "fae48a75-cf05-477f-b73b-f1c82f02a436",
      "number": 38,
      "content": "Những trẻ thơ đã bị làm vật tế thần cho các thần tượng xứ Ca-na-an;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.042Z",
      "updatedAt": "2024-09-24T02:07:29.042Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "55565b01-34b5-4105-8eef-bc8a4e414485",
      "number": 38,
      "content": "Và đất đã bị ô nhiễm vì máu các trẻ thơ ấy.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.046Z",
      "updatedAt": "2024-09-24T02:07:29.046Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "22ed08a1-7cb9-4fc2-8209-2649949ebd9d",
      "number": 39,
      "content": "Như vậy họ đã làm cho mình ra ô uế vì các hành động của họ;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.050Z",
      "updatedAt": "2024-09-24T02:07:29.050Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "19419caf-24de-437f-a5ee-3d6ecf59d76e",
      "number": 39,
      "content": "Họ tự biến mình thành những đĩ điếm thuộc linh qua các hành vi của họ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.055Z",
      "updatedAt": "2024-09-24T02:07:29.055Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "3184f43c-26c1-447b-90eb-7421467c11ab",
      "number": 40,
      "content": "Vì thế cơn giận của Chúa đã nổi lên nghịch lại dân Ngài;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.059Z",
      "updatedAt": "2024-09-24T02:07:29.059Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "c16485b7-a58e-4d0b-8ae7-1613ab5e988d",
      "number": 40,
      "content": "Và Ngài ghê tởm cơ nghiệp Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.063Z",
      "updatedAt": "2024-09-24T02:07:29.063Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "57ef5efa-0577-4b32-ba87-576594aae789",
      "number": 41,
      "content": "Ngài phó họ vào tay các dân ngoại,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.068Z",
      "updatedAt": "2024-09-24T02:07:29.068Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "94c142de-0fe4-4cb2-b1fe-043607612f59",
      "number": 41,
      "content": "Để cho những kẻ ghét họ cai trị họ.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.072Z",
      "updatedAt": "2024-09-24T02:07:29.072Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "1d543b4f-6ba9-4f49-9320-07f35854ff65",
      "number": 42,
      "content": "Những kẻ thù của họ hà hiếp họ;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.077Z",
      "updatedAt": "2024-09-24T02:07:29.077Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "0aad58f0-2e04-4a63-8e35-6c7b4680771d",
      "number": 42,
      "content": "Họ bị khuất phục dưới tay chúng.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.081Z",
      "updatedAt": "2024-09-24T02:07:29.081Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "bd5120ec-094e-4180-9b72-fe7f450e8c37",
      "number": 43,
      "content": "Dù nhiều lần Ngài đã giải cứu họ,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.085Z",
      "updatedAt": "2024-09-24T02:07:29.085Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "d36d5c2c-b1d7-40d8-899d-7dde9843d8d1",
      "number": 43,
      "content": "Nhưng họ vẫn cứ bàn với nhau để tính kế phản bội Ngài,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.089Z",
      "updatedAt": "2024-09-24T02:07:29.089Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "814821d6-a294-47b4-8342-4b1c6a2c48b0",
      "number": 43,
      "content": "Và cứ thế họ càng ngày càng lún sâu trong tội lỗi.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.094Z",
      "updatedAt": "2024-09-24T02:07:29.094Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "76da0296-10c5-4eb0-bbcb-029d92b9a13e",
      "number": 44,
      "content": "Dầu vậy Ngài đã đoái xem cảnh khốn cùng của họ,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.098Z",
      "updatedAt": "2024-09-24T02:07:29.098Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "db4a7cf7-e52e-4c2c-9638-50e7db5654fa",
      "number": 44,
      "content": "Khi Ngài nghe tiếng họ kêu cầu.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.102Z",
      "updatedAt": "2024-09-24T02:07:29.102Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "2a65b731-d8b9-43b0-89b6-7af40fafefea",
      "number": 45,
      "content": "Vì cớ họ, Ngài nhớ lại giao ước Ngài;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.107Z",
      "updatedAt": "2024-09-24T02:07:29.107Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "f471b5ba-b609-4949-965a-a3e049da6285",
      "number": 45,
      "content": "Và đoái thương họ vì lòng thương xót lớn lao của Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.111Z",
      "updatedAt": "2024-09-24T02:07:29.111Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "6c3811f0-44f0-4369-be03-8d62e32fad22",
      "number": 46,
      "content": "Ngài làm cho họ được người ta thấy tội nghiệp;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.115Z",
      "updatedAt": "2024-09-24T02:07:29.115Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "bdb0209e-2272-4d1f-9707-658a7c24a7a6",
      "number": 46,
      "content": "Ngài khiến cho những kẻ đem họ đi lưu đày thương hại họ.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.119Z",
      "updatedAt": "2024-09-24T02:07:29.119Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "62747970-afa9-4d74-89ea-857ee0dc7a7d",
      "number": 47,
      "content": "Lạy Chúa, Ðức Chúa Trời của chúng con, xin giải cứu chúng con,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.124Z",
      "updatedAt": "2024-09-24T02:07:29.124Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "24f1b9f7-7ad1-428b-b857-c002bd694837",
      "number": 47,
      "content": "Xin đem chúng con từ giữa các dân ngoại trở về,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.130Z",
      "updatedAt": "2024-09-24T02:07:29.130Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "c934233d-a500-48ec-9161-7fd58d4f0ac5",
      "number": 47,
      "content": "Ðể chúng con cảm tạ danh thánh Ngài,",
      "order": 2,
      "parNumber": 11,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.134Z",
      "updatedAt": "2024-09-24T02:07:29.134Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "bce5b3db-6063-4f9c-9b09-be63ea27f650",
      "number": 47,
      "content": "Và tôn vinh ca ngợi Ngài.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.139Z",
      "updatedAt": "2024-09-24T02:07:29.139Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "c0ab2c96-b2f8-4a2e-b243-cc2be413bf38",
      "number": 48,
      "content": "Chúc tụng Chúa, Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.143Z",
      "updatedAt": "2024-09-24T02:07:29.143Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "258646cb-fc86-453c-88af-31f94b355f0f",
      "number": 48,
      "content": "Từ đời đời cho đến đời đời.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.147Z",
      "updatedAt": "2024-09-24T02:07:29.147Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    },
    {
      "id": "7167a180-ab8c-4470-b764-776c8d18d4ff",
      "number": 48,
      "content": "Xin mọi người hãy nói, “A-men!”",
      "order": 2,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:29.153Z",
      "updatedAt": "2024-09-24T02:07:29.153Z",
      "chapterId": "f84ab9b0-dfa3-4d26-ba48-3fc10202929c"
    }
  ],
