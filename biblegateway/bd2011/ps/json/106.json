{
  "chapter": {
    "id": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5",
    "number": 106,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20106&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.338Z",
    "updatedAt": "2024-09-09T04:13:01.338Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "04d1ecb7-e5fa-4dee-800c-717a4db5e056",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.902Z",
      "updatedAt": "2024-09-09T04:49:47.902Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "d1cdeef3-8504-4ee6-ab27-21bc92728afc",
      "number": 48,
      "content": "Hãy ca ngợi Chúa!",
      "order": 3,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.392Z",
      "updatedAt": "2024-09-09T04:49:48.392Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "95352cf9-5035-4ce2-8162-77cacf596cd1",
      "number": 1,
      "content": "Hãy cảm tạ Chúa, vì Ngài thật tốt,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.914Z",
      "updatedAt": "2024-09-09T04:49:47.914Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "3e51ff84-d2ed-46b5-ae6f-4101145e7e4d",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.916Z",
      "updatedAt": "2024-09-09T04:49:47.916Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "dc2ad92d-04a8-4e90-92f1-1c830a8af194",
      "number": 2,
      "content": "Ai có thể thuật lại những việc quyền năng của Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.918Z",
      "updatedAt": "2024-09-09T04:49:47.918Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "b33778c4-f9b0-4e53-b31f-e25269e7750f",
      "number": 2,
      "content": "Hoặc có thể rao ra tất cả lời ca ngợi về Ngài?",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.920Z",
      "updatedAt": "2024-09-09T04:49:47.920Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "d2d33919-19da-4d08-96f6-b8b5c069681a",
      "number": 3,
      "content": "Phước hạnh thay cho những người duy trì công lý,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.922Z",
      "updatedAt": "2024-09-09T04:49:47.922Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "83e48bc0-df95-45e8-acf1-851c44fa2061",
      "number": 3,
      "content": "Tức những người làm điều công chính ở mọi lúc mọi nơi!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.924Z",
      "updatedAt": "2024-09-09T04:49:47.924Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "1b4a60b6-b930-46f9-bc1b-57ca040d7f2c",
      "number": 4,
      "content": "Chúa ôi, xin nhớ đến con khi Ngài ban ơn cho dân Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.926Z",
      "updatedAt": "2024-09-09T04:49:47.926Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "c7e58bd5-956c-4948-9c6a-ba22103deb39",
      "number": 4,
      "content": "Xin đoái đến con khi Ngài ban ơn cứu rỗi của Ngài,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.928Z",
      "updatedAt": "2024-09-09T04:49:47.928Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "0a190afd-8167-49db-b3eb-b7914bdbe66f",
      "number": 5,
      "content": "Ðể con được thấy sự thịnh vượng của người Ngài chọn,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.930Z",
      "updatedAt": "2024-09-09T04:49:47.930Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "2aa9058c-df48-4750-9e20-cc887bad08c4",
      "number": 5,
      "content": "Ðược hân hoan trong niềm vui của người dân trong nước Ngài,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.932Z",
      "updatedAt": "2024-09-09T04:49:47.932Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "5665160e-1670-43ea-9e22-e4cb1aaf933c",
      "number": 5,
      "content": "Và được vinh hiển với những người hưởng cơ nghiệp Ngài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.934Z",
      "updatedAt": "2024-09-09T04:49:47.934Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "09801213-504d-4d2a-9cd4-1cea5836596c",
      "number": 6,
      "content": "Giống như tổ tiên chúng con, chúng con đã phạm tội;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.936Z",
      "updatedAt": "2024-09-09T04:49:47.936Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "b30c50e2-e286-4089-a27b-03ee25f3614f",
      "number": 6,
      "content": "Chúng con đã làm những điều xấu xa tội lỗi;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.937Z",
      "updatedAt": "2024-09-09T04:49:47.937Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "726a6bd6-97a4-477c-b54c-247f8ee64acd",
      "number": 6,
      "content": "Chúng con đã hành động cách gian tà.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.939Z",
      "updatedAt": "2024-09-09T04:49:47.939Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "9b0bcac3-336e-450d-aec2-c920e0cfcf3c",
      "number": 7,
      "content": "Khi ở Ai-cập tổ tiên chúng con đã không nghĩ đến những việc diệu kỳ của Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.941Z",
      "updatedAt": "2024-09-09T04:49:47.941Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "cf76ac80-6942-44a2-bee0-0d455695488d",
      "number": 7,
      "content": "Họ đã chẳng nhớ đến lòng thương xót lớn lao của Ngài;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.943Z",
      "updatedAt": "2024-09-09T04:49:47.943Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "c26c9353-9151-4b5f-bcc0-e00bf91c57e5",
      "number": 7,
      "content": "Họ đã chọc giận Ngài bên bờ biển, tức bên bờ Hồng Hải.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.945Z",
      "updatedAt": "2024-09-09T04:49:47.945Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "73930e59-0866-402b-b135-dcf877c302d7",
      "number": 8,
      "content": "Dầu vậy vì cớ danh Ngài, Ngài đã cứu họ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.946Z",
      "updatedAt": "2024-09-09T04:49:47.946Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "6412f22c-5815-430f-b14e-4c125e8dca74",
      "number": 8,
      "content": "Ðể quyền năng Ngài được biết đến khắp nơi.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.949Z",
      "updatedAt": "2024-09-09T04:49:47.949Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "8738c085-d80b-4a6c-a63a-f3834f8de4d7",
      "number": 9,
      "content": "Ngài quở trách Hồng Hải, và nó liền khô cạn;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.954Z",
      "updatedAt": "2024-09-09T04:49:47.954Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "76667924-d830-41aa-81db-c0cbd6bdfff1",
      "number": 9,
      "content": "Ngài dẫn họ qua biển sâu như qua một sa mạc khô khan.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.958Z",
      "updatedAt": "2024-09-09T04:49:47.958Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "034e5a79-05fb-4e17-8060-bc107f5070ee",
      "number": 10,
      "content": "Ngài giải cứu họ khỏi tay những kẻ ghét họ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.963Z",
      "updatedAt": "2024-09-09T04:49:47.963Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "f6d540e4-975f-4566-8462-cfbabc8321b3",
      "number": 10,
      "content": "Và cứu chuộc họ khỏi tay những kẻ thù của họ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.967Z",
      "updatedAt": "2024-09-09T04:49:47.967Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "e691f1ee-c8fc-4a02-b40b-cd2caa26f917",
      "number": 11,
      "content": "Nước biển đã bao phủ những kẻ thù của họ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.972Z",
      "updatedAt": "2024-09-09T04:49:47.972Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "69ec002d-1158-4cb5-b9e1-d3ca4cb8649b",
      "number": 11,
      "content": "Không chừa lại ai, dù chỉ một người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.977Z",
      "updatedAt": "2024-09-09T04:49:47.977Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "48cc9b9d-fa50-40f2-91e9-530b882761d4",
      "number": 12,
      "content": "Bấy giờ họ mới chịu tin lời Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.982Z",
      "updatedAt": "2024-09-09T04:49:47.982Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "a9349b7d-46c9-4381-8bf2-b19b0fb2f286",
      "number": 12,
      "content": "Họ mới cất tiếng chúc tụng Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.987Z",
      "updatedAt": "2024-09-09T04:49:47.987Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "df071a3e-a90e-44d0-94a6-fe649ccb3525",
      "number": 13,
      "content": "Nhưng chẳng bao lâu họ lại quên các công việc Ngài;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.991Z",
      "updatedAt": "2024-09-09T04:49:47.991Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "ff7d1a96-0269-4732-8118-e26501b7c4ce",
      "number": 13,
      "content": "Họ chẳng màng đến việc chờ đợi để cầu hỏi ý Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:47.996Z",
      "updatedAt": "2024-09-09T04:49:47.996Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "dcb87bba-24e0-46ed-be74-d06859a049aa",
      "number": 14,
      "content": "Họ chiều theo dục vọng mình quá đỗi trong đồng hoang;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.001Z",
      "updatedAt": "2024-09-09T04:49:48.001Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "3b4b80ac-9293-4e83-be06-19c5c6b84ec5",
      "number": 14,
      "content": "Và dám thách đố cả đến Ðức Chúa Trời trong sa mạc.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.005Z",
      "updatedAt": "2024-09-09T04:49:48.005Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "8b33e53c-fa16-446e-828b-92e5800b4b75",
      "number": 15,
      "content": "Ngài cho họ những gì họ đòi hỏi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.010Z",
      "updatedAt": "2024-09-09T04:49:48.010Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "def5193b-5d2b-4a8c-9162-98b7858ff56a",
      "number": 15,
      "content": "Nhưng cũng sai đến một ôn dịch chết người.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.015Z",
      "updatedAt": "2024-09-09T04:49:48.015Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "c1183a34-3dcb-48a7-a3a2-3253352d7986",
      "number": 16,
      "content": "Trong doanh trại họ ganh tị với Môi-se,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.019Z",
      "updatedAt": "2024-09-09T04:49:48.019Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "de15e227-28e9-451f-83d0-7ba7fc44557d",
      "number": 16,
      "content": "Và với A-rôn người thánh của Chúa.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.024Z",
      "updatedAt": "2024-09-09T04:49:48.024Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "fa852999-2ecc-4c82-89c3-63eac038f89f",
      "number": 17,
      "content": "Ðất mở miệng ra và nuốt chửng Ða-than,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.029Z",
      "updatedAt": "2024-09-09T04:49:48.029Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "fa349397-71e3-45e5-839b-de0bda0aad82",
      "number": 17,
      "content": "Và ngoạm lấy đồng bọn của A-bi-ram.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.033Z",
      "updatedAt": "2024-09-09T04:49:48.033Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "f9203721-b2ae-4cd9-aa1a-96f41590895e",
      "number": 18,
      "content": "Một ngọn lửa đã bùng lên giữa đám người phản loạn;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.038Z",
      "updatedAt": "2024-09-09T04:49:48.038Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "d27b1978-74a3-4922-acaa-db3f12fa7778",
      "number": 18,
      "content": "Ngọn lửa hừng đã thiêu rụi những kẻ gian tà.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.043Z",
      "updatedAt": "2024-09-09T04:49:48.043Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "7f454ac7-7776-45cc-81b1-56ab3b3a22db",
      "number": 19,
      "content": "Họ đã đúc một tượng bò con ở Hô-rếp,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.047Z",
      "updatedAt": "2024-09-09T04:49:48.047Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "10dacb88-57ec-4fc8-a8e2-b1885f390be5",
      "number": 19,
      "content": "Rồi quỳ mọp xuống thờ lạy tượng vàng họ đúc.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.052Z",
      "updatedAt": "2024-09-09T04:49:48.052Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "154c99f6-9380-4556-a5e7-ca508d575dbc",
      "number": 20,
      "content": "Họ đã đánh đổi vinh hiển mình cho hình tượng một con bò,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.056Z",
      "updatedAt": "2024-09-09T04:49:48.056Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "fb9253b9-c115-4d6d-a1a4-a6a6f5f2a425",
      "number": 20,
      "content": "Một con vật ăn cỏ.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.061Z",
      "updatedAt": "2024-09-09T04:49:48.061Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "b696a4f8-138f-40e9-8013-63a6d261465a",
      "number": 21,
      "content": "Họ quên rằng Ðức Chúa Trời là Ðấng Giải Cứu họ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.066Z",
      "updatedAt": "2024-09-09T04:49:48.066Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "10b7dfbf-969e-493d-a2b6-a949a292e528",
      "number": 21,
      "content": "Ðấng làm những việc lớn lao ở Ai-cập,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.070Z",
      "updatedAt": "2024-09-09T04:49:48.070Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "32dba44c-f799-4bfe-940b-6e5cc50a0099",
      "number": 22,
      "content": "Những việc lạ lùng trong xứ của Cham,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.075Z",
      "updatedAt": "2024-09-09T04:49:48.075Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "a8b9f067-1ebb-47c3-bd81-5ef0c0bee3e0",
      "number": 22,
      "content": "Những việc kinh hoàng bên bờ Hồng Hải.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.080Z",
      "updatedAt": "2024-09-09T04:49:48.080Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "a7408ed8-0d72-4d4b-b17a-b7ed7ad30d13",
      "number": 23,
      "content": "Vì thế Ngài phán rằng Ngài sẽ tiêu diệt họ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.085Z",
      "updatedAt": "2024-09-09T04:49:48.085Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "bba4cdfb-7e59-4445-b924-42dc6785ad66",
      "number": 23,
      "content": "May nhờ có Môi-se, người Ngài đã chọn, đứng mũi chịu sào,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.091Z",
      "updatedAt": "2024-09-09T04:49:48.091Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "7bf66e0b-da8d-4dce-b46c-c397c0248c9a",
      "number": 23,
      "content": "Ðể van nài Ngài nguôi ngoai cơn giận mà không diệt họ đi.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.096Z",
      "updatedAt": "2024-09-09T04:49:48.096Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "04a3d673-21bc-484e-9849-b69182cf8356",
      "number": 24,
      "content": "Kế đến họ khinh chê đất hứa;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.100Z",
      "updatedAt": "2024-09-09T04:49:48.100Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "56837dda-e29a-41d4-807a-ece8b8b33428",
      "number": 24,
      "content": "Họ không tin vào lời hứa của Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.105Z",
      "updatedAt": "2024-09-09T04:49:48.105Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "82b5c2ed-ea23-4506-b2cd-b9800dda549b",
      "number": 25,
      "content": "Trong lều họ, họ lầm bầm oán trách;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.111Z",
      "updatedAt": "2024-09-09T04:49:48.111Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "c875a870-11e4-4fc3-bf62-2d5aa55ae83c",
      "number": 25,
      "content": "Chẳng muốn nghe tiếng Chúa phán truyền.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.115Z",
      "updatedAt": "2024-09-09T04:49:48.115Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "3d9ef241-a750-4b7a-a360-bcb5e05c5a99",
      "number": 26,
      "content": "Vì thế Ngài đã đưa tay lên và thề rằng,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.120Z",
      "updatedAt": "2024-09-09T04:49:48.120Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "a70616d0-53fe-4787-b305-a19e80366c59",
      "number": 26,
      "content": "Ngài sẽ cho họ bỏ thây trong sa mạc,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.124Z",
      "updatedAt": "2024-09-09T04:49:48.124Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "a5ff75c4-bd2f-4300-98d1-ebabc09671ef",
      "number": 27,
      "content": "Rải con cháu họ ra trong các quốc gia,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.129Z",
      "updatedAt": "2024-09-09T04:49:48.129Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "01ec5b4a-4fba-40a6-aea0-b1548718a478",
      "number": 27,
      "content": "Và làm cho họ tản lạc khắp nơi trong các nước.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.135Z",
      "updatedAt": "2024-09-09T04:49:48.135Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "56ecd0df-55a5-4481-a861-de05199be133",
      "number": 28,
      "content": "Ðã thế họ còn thờ lạy Ba-anh Pê-ô;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.140Z",
      "updatedAt": "2024-09-09T04:49:48.140Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "f133188c-a265-416f-929f-bdf42ffda092",
      "number": 28,
      "content": "Và ăn đồ cúng cho người chết.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.144Z",
      "updatedAt": "2024-09-09T04:49:48.144Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "5c47e78f-40f5-4ff9-897f-4a9cb5170cec",
      "number": 29,
      "content": "Họ đã dùng các hành động mình để chọc giận Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.149Z",
      "updatedAt": "2024-09-09T04:49:48.149Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "a190e1ba-787f-4879-882c-92fbeb813b99",
      "number": 29,
      "content": "Nên ôn dịch đã bộc phát ở giữa họ.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.154Z",
      "updatedAt": "2024-09-09T04:49:48.154Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "65f80355-ed67-4fed-a30e-a5723f98a2da",
      "number": 30,
      "content": "May nhờ có Phi-nê-a đứng dậy và cầu thay,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.158Z",
      "updatedAt": "2024-09-09T04:49:48.158Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "1e587f4a-4875-44a1-aaf8-0d8a482c781c",
      "number": 30,
      "content": "Nên bệnh dịch mới ngừng ngay tại chỗ.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.163Z",
      "updatedAt": "2024-09-09T04:49:48.163Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "bc2779b5-a797-4ad1-a12b-06cfdac11a3c",
      "number": 31,
      "content": "Ðiều ấy được xem là công đức của ông ta,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.167Z",
      "updatedAt": "2024-09-09T04:49:48.167Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "df355d5a-66ec-4318-b6d4-bdc25c7f5d75",
      "number": 31,
      "content": "Và được ghi nhớ từ đời này sang đời khác.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.172Z",
      "updatedAt": "2024-09-09T04:49:48.172Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "ce8a604b-5995-454c-91c5-6314faff7168",
      "number": 32,
      "content": "Họ đã chọc giận Ngài trong vụ nước ở Mê-ri-ba,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.177Z",
      "updatedAt": "2024-09-09T04:49:48.177Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "1a658357-e02d-4d21-a9f7-f77c8287a981",
      "number": 32,
      "content": "Cũng tại vì họ mà Môi-se đã mang lụy vào thân.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.182Z",
      "updatedAt": "2024-09-09T04:49:48.182Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "2cb1ad22-3c7b-4798-bec0-00798f3d60fe",
      "number": 33,
      "content": "Bởi họ đã làm cho tâm thần ông bực dọc,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.187Z",
      "updatedAt": "2024-09-09T04:49:48.187Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "0b925d4a-5585-4a0e-a23a-3ae1eef8eb18",
      "number": 33,
      "content": "Nên môi ông đã nói những lời thiếu đắn đo suy nghĩ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.192Z",
      "updatedAt": "2024-09-09T04:49:48.192Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "5f53e57e-f66b-431d-8542-70aee975dbe2",
      "number": 34,
      "content": "Họ cũng chẳng tiêu diệt các dân trong xứ,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.198Z",
      "updatedAt": "2024-09-09T04:49:48.198Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "a55d5363-023d-4c2c-ac87-927e974f66e1",
      "number": 34,
      "content": "Ðể làm đúng theo mạng lịnh Chúa đã truyền,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.203Z",
      "updatedAt": "2024-09-09T04:49:48.203Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "25e78234-365e-471c-b9d3-3c344d89e5b1",
      "number": 35,
      "content": "Nhưng họ đã sống chung lộn với các dân tộc ấy,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.208Z",
      "updatedAt": "2024-09-09T04:49:48.208Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "976da554-eb92-49d8-abb0-4b57d846e05e",
      "number": 35,
      "content": "Và học theo các thói tục của chúng.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.213Z",
      "updatedAt": "2024-09-09T04:49:48.213Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "eec7e5c3-725b-4c31-923b-bf9e8e141616",
      "number": 36,
      "content": "Ðã thế họ còn thờ lạy các thần tượng của chúng;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.218Z",
      "updatedAt": "2024-09-09T04:49:48.218Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "d7cca44c-17a4-4f13-89b4-b9c1247a259c",
      "number": 36,
      "content": "Các thần tượng ấy đã trở thành cái bẫy cho họ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.223Z",
      "updatedAt": "2024-09-09T04:49:48.223Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "a5cf8dea-b23a-40e2-a0e4-ea2560361164",
      "number": 37,
      "content": "Thậm chí họ còn sát tế các con trai và các con gái mình,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.228Z",
      "updatedAt": "2024-09-09T04:49:48.228Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "953a32b2-35d2-45a1-a764-5e25ebca2b5c",
      "number": 37,
      "content": "Ðể cúng tế cho các tà linh ma quỷ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.233Z",
      "updatedAt": "2024-09-09T04:49:48.233Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "8fbf7cce-3739-41f1-ade8-f17637330c93",
      "number": 38,
      "content": "Họ đã làm đổ máu vô tội ra,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.238Z",
      "updatedAt": "2024-09-09T04:49:48.238Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "a9019cda-0503-442e-9b81-1f5c3c3f767f",
      "number": 38,
      "content": "Tức máu của các con trai và các con gái họ,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.244Z",
      "updatedAt": "2024-09-09T04:49:48.244Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "2cdaf3fc-70f9-4d8e-a1ba-4451644c6d6d",
      "number": 38,
      "content": "Những trẻ thơ đã bị làm vật tế thần cho các thần tượng xứ Ca-na-an;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.249Z",
      "updatedAt": "2024-09-09T04:49:48.249Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "568137d9-59a3-4407-b4c3-a0a549b3cae0",
      "number": 38,
      "content": "Và đất đã bị ô nhiễm vì máu các trẻ thơ ấy.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.254Z",
      "updatedAt": "2024-09-09T04:49:48.254Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "abe6364e-7753-451f-b0b9-2327b9804402",
      "number": 39,
      "content": "Như vậy họ đã làm cho mình ra ô uế vì các hành động của họ;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.260Z",
      "updatedAt": "2024-09-09T04:49:48.260Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "df2ebbfa-c19e-461b-ab0a-0eb18519da06",
      "number": 39,
      "content": "Họ tự biến mình thành những đĩ điếm thuộc linh qua các hành vi của họ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.264Z",
      "updatedAt": "2024-09-09T04:49:48.264Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "fe0b21ad-0f85-43c5-9993-94ac9a8bc208",
      "number": 40,
      "content": "Vì thế cơn giận của Chúa đã nổi lên nghịch lại dân Ngài;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.270Z",
      "updatedAt": "2024-09-09T04:49:48.270Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "10b3f9f1-57cf-4af9-91d1-c44135a4e753",
      "number": 40,
      "content": "Và Ngài ghê tởm cơ nghiệp Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.275Z",
      "updatedAt": "2024-09-09T04:49:48.275Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "262d12c1-2697-4628-b133-bb18cdf1f574",
      "number": 41,
      "content": "Ngài phó họ vào tay các dân ngoại,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.280Z",
      "updatedAt": "2024-09-09T04:49:48.280Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "4b2f757a-e3f1-445c-b025-3ad903142852",
      "number": 41,
      "content": "Để cho những kẻ ghét họ cai trị họ.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.285Z",
      "updatedAt": "2024-09-09T04:49:48.285Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "ce89b920-d38b-4f04-8ed4-26b42d572f52",
      "number": 42,
      "content": "Những kẻ thù của họ hà hiếp họ;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.291Z",
      "updatedAt": "2024-09-09T04:49:48.291Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "c535d618-6ed8-4dac-9ba5-c67ad0750388",
      "number": 42,
      "content": "Họ bị khuất phục dưới tay chúng.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.296Z",
      "updatedAt": "2024-09-09T04:49:48.296Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "bc0decf5-ac32-41a2-8a12-ba4fc48bab95",
      "number": 43,
      "content": "Dù nhiều lần Ngài đã giải cứu họ,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.301Z",
      "updatedAt": "2024-09-09T04:49:48.301Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "75fbf9fc-82c0-4857-90f1-31340434c758",
      "number": 43,
      "content": "Nhưng họ vẫn cứ bàn với nhau để tính kế phản bội Ngài,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.307Z",
      "updatedAt": "2024-09-09T04:49:48.307Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "14d0d77d-677f-42c7-ae87-751398fdf516",
      "number": 43,
      "content": "Và cứ thế họ càng ngày càng lún sâu trong tội lỗi.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.312Z",
      "updatedAt": "2024-09-09T04:49:48.312Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "57f1f7be-9ece-4ab2-8f36-a85369348c30",
      "number": 44,
      "content": "Dầu vậy Ngài đã đoái xem cảnh khốn cùng của họ,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.317Z",
      "updatedAt": "2024-09-09T04:49:48.317Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "6f93083c-25b3-4bbd-9d5e-e4607d08f018",
      "number": 44,
      "content": "Khi Ngài nghe tiếng họ kêu cầu.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.322Z",
      "updatedAt": "2024-09-09T04:49:48.322Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "eea88625-8bf2-4156-8f8c-6fc529f5ab5a",
      "number": 45,
      "content": "Vì cớ họ, Ngài nhớ lại giao ước Ngài;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.327Z",
      "updatedAt": "2024-09-09T04:49:48.327Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "169deef7-655e-4cfd-8b7e-3c57ed170c7e",
      "number": 45,
      "content": "Và đoái thương họ vì lòng thương xót lớn lao của Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.333Z",
      "updatedAt": "2024-09-09T04:49:48.333Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "58015238-3ab1-4ded-a4a8-ee26272f89c7",
      "number": 46,
      "content": "Ngài làm cho họ được người ta thấy tội nghiệp;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.338Z",
      "updatedAt": "2024-09-09T04:49:48.338Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "46313db8-85ca-4370-bc7c-98f07c20eb12",
      "number": 46,
      "content": "Ngài khiến cho những kẻ đem họ đi lưu đày thương hại họ.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.343Z",
      "updatedAt": "2024-09-09T04:49:48.343Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "8d89c41d-7c66-4986-a971-97b580eb68d6",
      "number": 47,
      "content": "Lạy Chúa, Ðức Chúa Trời của chúng con, xin giải cứu chúng con,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.355Z",
      "updatedAt": "2024-09-09T04:49:48.355Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "36e29f26-a7d4-461d-8a2a-143e7b64b5ad",
      "number": 47,
      "content": "Xin đem chúng con từ giữa các dân ngoại trở về,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.360Z",
      "updatedAt": "2024-09-09T04:49:48.360Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "7c9b61f7-3bb6-4338-9482-89547c358892",
      "number": 47,
      "content": "Ðể chúng con cảm tạ danh thánh Ngài,",
      "order": 2,
      "parNumber": 11,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.365Z",
      "updatedAt": "2024-09-09T04:49:48.365Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "823dc3c6-9ee2-4caf-94c9-33b5d99ca7ea",
      "number": 47,
      "content": "Và tôn vinh ca ngợi Ngài.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.371Z",
      "updatedAt": "2024-09-09T04:49:48.371Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "0de95399-3e45-4835-b9b5-d05672c09486",
      "number": 48,
      "content": "Chúc tụng Chúa, Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.376Z",
      "updatedAt": "2024-09-09T04:49:48.376Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "6b60f856-0ecb-4a43-8d3d-5424ecb40b98",
      "number": 48,
      "content": "Từ đời đời cho đến đời đời.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.381Z",
      "updatedAt": "2024-09-09T04:49:48.381Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    },
    {
      "id": "29361289-389f-4691-9121-59ed200afa68",
      "number": 48,
      "content": "Xin mọi người hãy nói, “A-men!”",
      "order": 2,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:48.386Z",
      "updatedAt": "2024-09-09T04:49:48.386Z",
      "chapterId": "513dd85a-3f2e-4c10-84ba-ced0e68a87f5"
    }
  ],
