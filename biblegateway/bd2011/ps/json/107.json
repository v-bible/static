{
  "chapter": {
    "id": "69a10209-50c0-452b-af52-d0e669017276",
    "number": 107,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20107&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.540Z",
    "updatedAt": "2024-09-24T01:00:44.540Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "a3ad18a4-0b8f-4004-886c-45be23973ab2",
      "number": 1,
      "content": "Hãy cảm tạ Chúa, vì Ngài thật tốt,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.486Z",
      "updatedAt": "2024-09-24T02:07:36.486Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "fff92416-b1d2-4333-872a-89fb9f727cef",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.502Z",
      "updatedAt": "2024-09-24T02:07:36.502Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "d90eb712-27d8-461d-9bfb-0690e187be77",
      "number": 2,
      "content": "Nguyện những người được cứu chuộc của Chúa nói như vậy,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.506Z",
      "updatedAt": "2024-09-24T02:07:36.506Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "11a1b7fa-9c3d-4b48-b94c-44070b96fdcc",
      "number": 2,
      "content": "Những người được Ngài cứu khỏi tay kẻ thù,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.509Z",
      "updatedAt": "2024-09-24T02:07:36.509Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "064a2f8c-238b-4744-925f-b14915fb0102",
      "number": 3,
      "content": "Rồi gom nhóm họ từ các nước trở về;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.511Z",
      "updatedAt": "2024-09-24T02:07:36.511Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "5fcfc68a-3229-4e8f-9598-bdbf3b5c03a0",
      "number": 3,
      "content": "Từ phương đông và phương tây, phương bắc và phương nam.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.513Z",
      "updatedAt": "2024-09-24T02:07:36.513Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "edc58218-5321-4d07-9f1b-60857d082e45",
      "number": 4,
      "content": "Họ lang thang trong đồng hoang, trong nơi hoang vắng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.515Z",
      "updatedAt": "2024-09-24T02:07:36.515Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "b7fd50e0-d985-4835-b2a3-5f52c321979c",
      "number": 4,
      "content": "Họ chẳng tìm được đường về một thành để ở.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.517Z",
      "updatedAt": "2024-09-24T02:07:36.517Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "1cae71a6-4ac4-4df5-9be6-622ccf576a47",
      "number": 5,
      "content": "Họ đói khát;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.519Z",
      "updatedAt": "2024-09-24T02:07:36.519Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "c887bc0d-5282-4664-a473-a29b6148cd43",
      "number": 5,
      "content": "Linh hồn họ mòn mỏi nao sờn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.521Z",
      "updatedAt": "2024-09-24T02:07:36.521Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "352711d4-6fa2-423c-9759-6a479f9f66db",
      "number": 6,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.523Z",
      "updatedAt": "2024-09-24T02:07:36.523Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "e12a0b5d-43a0-435e-8675-005b699ccbf5",
      "number": 19,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.585Z",
      "updatedAt": "2024-09-24T02:07:36.585Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "2ddfca9a-853d-483a-8dfa-0ee05a99811b",
      "number": 28,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.641Z",
      "updatedAt": "2024-09-24T02:07:36.641Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "e6a0be65-964f-4cc3-8a22-6890b4f4ee5b",
      "number": 13,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.555Z",
      "updatedAt": "2024-09-24T02:07:36.555Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "0f72a979-627c-4557-bfd0-a9b15fe685cc",
      "number": 28,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.643Z",
      "updatedAt": "2024-09-24T02:07:36.643Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "351842aa-b0ea-4ff0-a860-5658d635d78e",
      "number": 6,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.525Z",
      "updatedAt": "2024-09-24T02:07:36.525Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "6753a55e-7366-4223-a45e-d920ced3d8f0",
      "number": 13,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.557Z",
      "updatedAt": "2024-09-24T02:07:36.557Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "d9521a77-5248-4ed1-b184-af9dc3ee3682",
      "number": 19,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.587Z",
      "updatedAt": "2024-09-24T02:07:36.587Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "3c586a97-820d-4751-a885-36f57e436903",
      "number": 7,
      "content": "Ngài dẫn đưa họ đúng vào con đường phải đi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.527Z",
      "updatedAt": "2024-09-24T02:07:36.527Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "83664c5f-9f5b-4b73-bd80-8243e939e160",
      "number": 7,
      "content": "Ðể họ đến một thành mà ở.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.529Z",
      "updatedAt": "2024-09-24T02:07:36.529Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "4dc5e165-b16c-4aab-9bba-5b814be7d2bd",
      "number": 8,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.531Z",
      "updatedAt": "2024-09-24T02:07:36.531Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "aa5da590-b44c-4df6-a3ff-ed1140338960",
      "number": 21,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.594Z",
      "updatedAt": "2024-09-24T02:07:36.594Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "c5421194-f3fb-4252-a879-1d3ffebf46f4",
      "number": 31,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.654Z",
      "updatedAt": "2024-09-24T02:07:36.654Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "69305f78-c7ba-4f20-a983-dd76d7914bcf",
      "number": 15,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.564Z",
      "updatedAt": "2024-09-24T02:07:36.564Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "e0779da6-d9da-4a2e-aa86-9ac57ef51b1b",
      "number": 8,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.533Z",
      "updatedAt": "2024-09-24T02:07:36.533Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "812e39a2-e45f-44bf-8bf9-f213e4178427",
      "number": 9,
      "content": "Vì Ngài làm thỏa mãn linh hồn khao khát,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.535Z",
      "updatedAt": "2024-09-24T02:07:36.535Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "12761c7d-9cdc-4a3b-9c1b-3fbfa4fd867a",
      "number": 9,
      "content": "Và làm no nê linh hồn đói lả bằng các thức ăn ngon.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.537Z",
      "updatedAt": "2024-09-24T02:07:36.537Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "6c295eb3-281d-4158-8a7a-79a71a0dee4e",
      "number": 10,
      "content": "Có những kẻ sống trong tối tăm và trong bóng tử thần,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.539Z",
      "updatedAt": "2024-09-24T02:07:36.539Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "9eea4471-437e-4938-8e0e-ba09327245f5",
      "number": 10,
      "content": "Bị giam cầm trong khổ đau và xiềng xích,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.541Z",
      "updatedAt": "2024-09-24T02:07:36.541Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "323c89ac-7fff-4ff6-87d2-b5b3b8b49113",
      "number": 11,
      "content": "Bởi vì họ chống lại những lời của Ðức Chúa Trời,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.543Z",
      "updatedAt": "2024-09-24T02:07:36.543Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "6dfa94f9-2fd3-4649-86aa-29cba6a3740d",
      "number": 11,
      "content": "Và khinh bỉ lời chỉ bảo của Ðấng Chí Cao.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.546Z",
      "updatedAt": "2024-09-24T02:07:36.546Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "77c8c689-e4dd-4c3e-81ea-7faa4bd74aa1",
      "number": 12,
      "content": "Vì thế Ngài lấy gian lao hạ lòng họ xuống;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.548Z",
      "updatedAt": "2024-09-24T02:07:36.548Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "0f4d5606-9465-4f85-acbf-7b8fd0e117a5",
      "number": 12,
      "content": "Họ bị ngã nhào và chẳng ai giúp họ đứng lên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.553Z",
      "updatedAt": "2024-09-24T02:07:36.553Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "65ce4209-6dbf-4fce-9b80-3793580897d1",
      "number": 14,
      "content": "Ngài đem họ ra khỏi cảnh tối tăm và bóng tử thần,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.560Z",
      "updatedAt": "2024-09-24T02:07:36.560Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "b372e1b7-2596-4f55-bdf8-2d15a30ae642",
      "number": 14,
      "content": "Và đập tan xiềng xích gông cùm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.562Z",
      "updatedAt": "2024-09-24T02:07:36.562Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "7a5dcf6a-82e6-4625-b0b2-cf2b2fa976d5",
      "number": 15,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.567Z",
      "updatedAt": "2024-09-24T02:07:36.567Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "d6d833b8-3b83-4c3e-b239-0a1290f61a52",
      "number": 16,
      "content": "Vì Ngài phá tan các cổng bằng đồng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.569Z",
      "updatedAt": "2024-09-24T02:07:36.569Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "ccfee8eb-213b-4970-9375-50097d20be35",
      "number": 16,
      "content": "Và Ngài cắt rời những song bằng sắt.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.572Z",
      "updatedAt": "2024-09-24T02:07:36.572Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "35cf04c7-8b61-452d-956a-c33d97442a4c",
      "number": 17,
      "content": "Có những kẻ cứ bội nghịch nên hóa ra điên dại,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.574Z",
      "updatedAt": "2024-09-24T02:07:36.574Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "5fa2c8ca-2bab-4d7b-93ea-e58cfba41b71",
      "number": 17,
      "content": "Và vì tội lỗi mình nên phải chuốc lấy khổ đau.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.576Z",
      "updatedAt": "2024-09-24T02:07:36.576Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "dbe65acf-b728-48bb-aada-1a1db1ab5609",
      "number": 18,
      "content": "Linh hồn họ gớm ghê mọi thực phẩm,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.580Z",
      "updatedAt": "2024-09-24T02:07:36.580Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "953809a8-7b45-4cdb-b662-270bc1ef6254",
      "number": 18,
      "content": "Và họ bị kéo gần đến cổng của tử thần.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.583Z",
      "updatedAt": "2024-09-24T02:07:36.583Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "2c45e728-bc27-4a13-98f3-03dea7affc93",
      "number": 20,
      "content": "Ngài ban lời Ngài và chữa lành họ;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.590Z",
      "updatedAt": "2024-09-24T02:07:36.590Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "ba2ba9c4-a2f5-4ba3-8e40-dd18d976c105",
      "number": 20,
      "content": "Ngài giải cứu họ khỏi miệng của phần mộ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.592Z",
      "updatedAt": "2024-09-24T02:07:36.592Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "68eb9a11-3051-4275-82e8-80898b4d2290",
      "number": 31,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.656Z",
      "updatedAt": "2024-09-24T02:07:36.656Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "5a2d5571-119c-4b33-95e3-67aa247ca60e",
      "number": 21,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.597Z",
      "updatedAt": "2024-09-24T02:07:36.597Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "9c29a112-a776-47fe-896a-f9dfc49c6227",
      "number": 22,
      "content": "Cũng hãy để họ dâng những của lễ cảm tạ lên Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.599Z",
      "updatedAt": "2024-09-24T02:07:36.599Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "1024b77e-b125-4aff-846d-ec8f88edeafd",
      "number": 22,
      "content": "Và dùng những lời ca ngợi vui mừng thuật lại các công việc Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.601Z",
      "updatedAt": "2024-09-24T02:07:36.601Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "dc9c0019-a6b3-4df1-8d36-4a67a8ec55c8",
      "number": 23,
      "content": "Có những kẻ xuống tàu vượt đại dương,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.603Z",
      "updatedAt": "2024-09-24T02:07:36.603Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "78410ca3-ac10-4085-9cb5-6b4e60750f09",
      "number": 23,
      "content": "Ấy là những người làm ăn trên biển cả;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.605Z",
      "updatedAt": "2024-09-24T02:07:36.605Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "d8ba9d6d-9ece-4cdc-90d3-f2cdf858834c",
      "number": 24,
      "content": "Họ trông thấy những việc làm của Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.611Z",
      "updatedAt": "2024-09-24T02:07:36.611Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "0d654a84-c6e1-42f8-8c5c-445cf7509f42",
      "number": 24,
      "content": "Và những việc lạ lùng của Ngài ở giữa biển sâu.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.619Z",
      "updatedAt": "2024-09-24T02:07:36.619Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "8a2e4d81-61e9-4289-bb7a-d2ac2a86770c",
      "number": 25,
      "content": "Vì Ngài phán một tiếng, bão tố liền nổi lên cuồn cuộn,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.623Z",
      "updatedAt": "2024-09-24T02:07:36.623Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "55841490-52e0-4805-a943-db8310571f40",
      "number": 25,
      "content": "Cuồng phong nâng cao những ngọn sóng kinh hồn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.627Z",
      "updatedAt": "2024-09-24T02:07:36.627Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "285c6a32-a22b-435b-ae9e-2afb8ccceb6b",
      "number": 26,
      "content": "Sóng cả dâng cao đến tận trời, rồi trụt sâu xuống đáy vực thẳm;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.630Z",
      "updatedAt": "2024-09-24T02:07:36.630Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "6810db64-bbbc-4a52-b03e-850eaf28c914",
      "number": 26,
      "content": "Giữa cảnh khốn cùng họ hồn phi phách tán.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.633Z",
      "updatedAt": "2024-09-24T02:07:36.633Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "e584da34-78f5-4ab9-84df-696acd175465",
      "number": 27,
      "content": "Họ bị quay cuồng đảo điên như người say rượu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.636Z",
      "updatedAt": "2024-09-24T02:07:36.636Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "4732aadd-7a8e-4b1d-ae76-2b7021f91d44",
      "number": 27,
      "content": "Mọi tài trí khôn ngoan đều biến mất.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.638Z",
      "updatedAt": "2024-09-24T02:07:36.638Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "66901a20-40ea-4e91-bad1-0361921772b8",
      "number": 29,
      "content": "Ngài khiến bão tố yên lặng như tờ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.645Z",
      "updatedAt": "2024-09-24T02:07:36.645Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "3237035e-773a-4bd3-9f32-98f61e666a1a",
      "number": 29,
      "content": "Những ngọn sóng lớn lặng im.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.648Z",
      "updatedAt": "2024-09-24T02:07:36.648Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "c4ff0f78-1f1c-44ae-9273-de387f664162",
      "number": 30,
      "content": "Họ rất đỗi vui mừng vì biển êm gió lặng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.650Z",
      "updatedAt": "2024-09-24T02:07:36.650Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "4688a532-bb79-4145-ba61-30d424a171aa",
      "number": 30,
      "content": "Rồi Ngài đưa họ đến bến bờ họ mong ước.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.652Z",
      "updatedAt": "2024-09-24T02:07:36.652Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "90c0fd38-808d-4a5a-b3f8-f860582762ce",
      "number": 32,
      "content": "Hãy để họ tôn cao Ngài giữa đại hội,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.658Z",
      "updatedAt": "2024-09-24T02:07:36.658Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "cd0da1d2-3be4-4e6c-866f-9096efb29cf7",
      "number": 32,
      "content": "Và ca tụng Ngài giữa sảnh đường của hội đồng trưởng lão.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.660Z",
      "updatedAt": "2024-09-24T02:07:36.660Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "65b90230-8c0d-43fa-975a-10b46d2fc9e1",
      "number": 33,
      "content": "Ngài biến các dòng sông thành sa mạc hoang vu,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.662Z",
      "updatedAt": "2024-09-24T02:07:36.662Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "d6799163-6a54-48d2-b751-60e9530b062f",
      "number": 33,
      "content": "Các dòng suối thành đất khô khan cằn cỗi,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.664Z",
      "updatedAt": "2024-09-24T02:07:36.664Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "c677b26b-a533-44ea-b11a-fa1b227b46ec",
      "number": 34,
      "content": "Ðất đai màu mỡ thành đồng chua nước mặn,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.666Z",
      "updatedAt": "2024-09-24T02:07:36.666Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "c6479bca-f901-4158-bc5d-e11fcfd9c0e4",
      "number": 34,
      "content": "Vì sự gian ác của dân cư ở đó.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.668Z",
      "updatedAt": "2024-09-24T02:07:36.668Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "143f5a1c-f76a-4ec3-bbb7-c36f5cac2092",
      "number": 35,
      "content": "Ngài biến đồng hoang thành đất có ao hồ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.670Z",
      "updatedAt": "2024-09-24T02:07:36.670Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "49ed7eed-2432-444a-9278-829bf77992d6",
      "number": 35,
      "content": "Và xứ khô cằn thành nơi có dồi dào các dòng nước ngọt;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.672Z",
      "updatedAt": "2024-09-24T02:07:36.672Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "cfb93f6c-f966-4ad2-907d-76ac6cd45d1d",
      "number": 36,
      "content": "Ngài đã cho những kẻ đói khát đến cư ngụ tại đó,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.673Z",
      "updatedAt": "2024-09-24T02:07:36.673Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "117d6ff4-7e90-4cf5-92ea-beed1a812bf8",
      "number": 36,
      "content": "Ðể họ xây dựng một thành mà an cư lạc nghiệp;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.675Z",
      "updatedAt": "2024-09-24T02:07:36.675Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "53325ee3-c2bb-48ec-94a0-eac637546c42",
      "number": 37,
      "content": "Họ gieo mạ ngoài đồng và trồng tỉa vườn nho,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.677Z",
      "updatedAt": "2024-09-24T02:07:36.677Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "21649307-c53d-4e62-b3ad-acc359ea21f3",
      "number": 37,
      "content": "Rồi thu hoạch hoa màu sung túc.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.679Z",
      "updatedAt": "2024-09-24T02:07:36.679Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "2ca6167c-c0b6-4c95-8f71-a43bd1fbfbb0",
      "number": 38,
      "content": "Ngài ban phước cho họ, và dân số họ gia tăng;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.681Z",
      "updatedAt": "2024-09-24T02:07:36.681Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "93669a51-ccc1-4566-a842-66cba6661b5b",
      "number": 38,
      "content": "Ngài chẳng để cho bầy súc vật của họ bị giảm thiểu.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.683Z",
      "updatedAt": "2024-09-24T02:07:36.683Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "5132a92e-6014-45e1-a991-5780a675d46e",
      "number": 39,
      "content": "Có lúc họ bị tiêu hao lụn bại, phải cúi đầu khuất phục ngoại bang,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.685Z",
      "updatedAt": "2024-09-24T02:07:36.685Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "5cb26992-9699-4137-ae79-afa8a6d23117",
      "number": 39,
      "content": "Bị áp bức, khổ đau, và sầu não.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.687Z",
      "updatedAt": "2024-09-24T02:07:36.687Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "a1d06db1-8896-42fa-90a8-2b55456e0a40",
      "number": 40,
      "content": "Ngài trút đổ ô nhục trên những người quyền quý của họ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.689Z",
      "updatedAt": "2024-09-24T02:07:36.689Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "af241a8a-4160-441e-96fa-0fcafac041e1",
      "number": 40,
      "content": "Khiến họ lang thang trong nơi hoang vắng không đường.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.691Z",
      "updatedAt": "2024-09-24T02:07:36.691Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "5d1f4238-55d2-4e7a-9d30-9a720e215634",
      "number": 41,
      "content": "Nhưng Ngài lại cất những người nghèo ra khỏi cảnh lầm than,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.692Z",
      "updatedAt": "2024-09-24T02:07:36.692Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "dbadb17c-a15f-4b9c-90db-6386c6fe08dd",
      "number": 41,
      "content": "Và cho con cháu họ được gia tăng đông đúc.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.695Z",
      "updatedAt": "2024-09-24T02:07:36.695Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "0ed7d946-12d0-47fb-9f70-7aef658a9e4b",
      "number": 42,
      "content": "Những người ngay lành thấy thế bèn vui mừng;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.697Z",
      "updatedAt": "2024-09-24T02:07:36.697Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "87468691-55eb-430c-9bb5-2d9d4824fdf9",
      "number": 42,
      "content": "Còn những kẻ gian ác thảy đều câm miệng lại.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.700Z",
      "updatedAt": "2024-09-24T02:07:36.700Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "e68ea25f-79a9-40db-9518-04b1afcb92b6",
      "number": 43,
      "content": "Hỡi ai là người khôn ngoan, hãy suy gẫm những điều ấy,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.702Z",
      "updatedAt": "2024-09-24T02:07:36.702Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    },
    {
      "id": "1956ccc6-cde6-4933-ae8c-358335285ec0",
      "number": 43,
      "content": "Rồi sẽ hiểu được thế nào là tình thương của Chúa.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:36.703Z",
      "updatedAt": "2024-09-24T02:07:36.703Z",
      "chapterId": "69a10209-50c0-452b-af52-d0e669017276"
    }
  ],
