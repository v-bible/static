{
  "chapter": {
    "id": "e3ea0aee-2636-4070-9a5a-f3c36ff49925",
    "number": 107,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20107&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.841Z",
    "updatedAt": "2024-07-24T15:07:48.737Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "cbb904b7-447c-4362-a2cb-f05661c77606",
      "number": 1,
      "content": "Hãy cảm tạ Chúa, vì Ngài thật tốt,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.538Z",
      "updatedAt": "2024-07-31T11:47:04.221Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "a4500bdb-d000-43b7-961c-9fb9075a3e33",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.538Z",
      "updatedAt": "2024-07-31T11:47:04.229Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "15a67940-9b3f-4314-b39d-60ac9820ff27",
      "number": 2,
      "content": "Nguyện những người được cứu chuộc của Chúa nói như vậy,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.548Z",
      "updatedAt": "2024-07-31T11:47:04.239Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "3c9e2126-f6ed-4640-a49f-7dcb12015c12",
      "number": 2,
      "content": "Những người được Ngài cứu khỏi tay kẻ thù,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.548Z",
      "updatedAt": "2024-07-31T11:47:04.247Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "94114a9a-8c09-4149-b6da-9551ae6b5e57",
      "number": 3,
      "content": "Rồi gom nhóm họ từ các nước trở về;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.551Z",
      "updatedAt": "2024-07-31T11:47:04.256Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "77a76a0b-88fd-450b-b9bc-08a3cda6d99b",
      "number": 3,
      "content": "Từ phương đông và phương tây, phương bắc và phương nam.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.551Z",
      "updatedAt": "2024-07-31T11:47:04.265Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "285cb796-db39-4fc4-bb4e-35d4f8a47325",
      "number": 4,
      "content": "Họ lang thang trong đồng hoang, trong nơi hoang vắng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.553Z",
      "updatedAt": "2024-07-31T11:47:04.277Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "9965eeef-f263-4c65-9ba8-5404a5003254",
      "number": 4,
      "content": "Họ chẳng tìm được đường về một thành để ở.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.553Z",
      "updatedAt": "2024-07-31T11:47:04.285Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "f0d60826-f8d2-4e5f-b36d-0d5e2cb20277",
      "number": 5,
      "content": "Họ đói khát;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.555Z",
      "updatedAt": "2024-07-31T11:47:04.294Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "f0681fed-d53c-4d6c-a7cd-83c82cfc0885",
      "number": 5,
      "content": "Linh hồn họ mòn mỏi nao sờn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.555Z",
      "updatedAt": "2024-07-31T11:47:04.303Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "106a9c22-2247-44dc-9c81-832431dfefa9",
      "number": 6,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.557Z",
      "updatedAt": "2024-07-31T11:47:04.315Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "42970477-7ff8-47fd-be56-521bedd0e53f",
      "number": 6,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.557Z",
      "updatedAt": "2024-07-31T11:47:04.324Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "2eb0014d-6c79-4356-bcad-0fa379bc0774",
      "number": 7,
      "content": "Ngài dẫn đưa họ đúng vào con đường phải đi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.560Z",
      "updatedAt": "2024-07-31T11:47:04.334Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "a3bbf021-43db-4f9b-8c74-9720c0dbb2c1",
      "number": 7,
      "content": "Ðể họ đến một thành mà ở.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.560Z",
      "updatedAt": "2024-07-31T11:47:04.342Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "47b44d3c-bff8-4982-b519-b9cd7f084c1a",
      "number": 8,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.562Z",
      "updatedAt": "2024-07-31T11:47:04.351Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "94697e70-7e0e-488a-b5b8-92fa766be264",
      "number": 8,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.562Z",
      "updatedAt": "2024-07-31T11:47:04.360Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "38d73bfa-3905-4493-a8d8-9e6e8f080c71",
      "number": 9,
      "content": "Vì Ngài làm thỏa mãn linh hồn khao khát,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.564Z",
      "updatedAt": "2024-07-31T11:47:04.369Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "ae225209-80bd-4543-9846-c0efdd442447",
      "number": 9,
      "content": "Và làm no nê linh hồn đói lả bằng các thức ăn ngon.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.564Z",
      "updatedAt": "2024-07-31T11:47:04.378Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "26fcbd4e-9080-4b00-b479-452a0ee36e13",
      "number": 10,
      "content": "Có những kẻ sống trong tối tăm và trong bóng tử thần,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.566Z",
      "updatedAt": "2024-07-31T11:47:04.390Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "593e18ff-08ec-48f6-92b8-215d452aebdf",
      "number": 10,
      "content": "Bị giam cầm trong khổ đau và xiềng xích,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.566Z",
      "updatedAt": "2024-07-31T11:47:04.398Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "6323109d-7f0a-433c-9026-de1e18149fa0",
      "number": 11,
      "content": "Bởi vì họ chống lại những lời của Ðức Chúa Trời,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.568Z",
      "updatedAt": "2024-07-31T11:47:04.408Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "41a84a0a-7691-4c3f-bd50-8f74b9cd216f",
      "number": 11,
      "content": "Và khinh bỉ lời chỉ bảo của Ðấng Chí Cao.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.568Z",
      "updatedAt": "2024-07-31T11:47:04.418Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "2c6972a9-e774-4f8e-869f-29f6b7977ea8",
      "number": 12,
      "content": "Vì thế Ngài lấy gian lao hạ lòng họ xuống;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.572Z",
      "updatedAt": "2024-07-31T11:47:04.429Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "808ecd5e-10ab-4ce7-b3d0-c289181cc832",
      "number": 12,
      "content": "Họ bị ngã nhào và chẳng ai giúp họ đứng lên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.572Z",
      "updatedAt": "2024-07-31T11:47:04.438Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "43abf692-732c-4636-975a-26793c16bddb",
      "number": 13,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.577Z",
      "updatedAt": "2024-07-31T11:47:04.446Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "a25cf8ed-e45d-4457-9261-29445d3cfa39",
      "number": 13,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.577Z",
      "updatedAt": "2024-07-31T11:47:04.455Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "52fdde8f-0431-4074-9f0c-7ca8af126998",
      "number": 14,
      "content": "Ngài đem họ ra khỏi cảnh tối tăm và bóng tử thần,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.583Z",
      "updatedAt": "2024-07-31T11:47:04.464Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "0c8ca78f-04f6-4ed0-850d-879d5274c867",
      "number": 14,
      "content": "Và đập tan xiềng xích gông cùm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.583Z",
      "updatedAt": "2024-07-31T11:47:04.473Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "712cf624-dca3-431b-8061-0e14629eaaba",
      "number": 15,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.589Z",
      "updatedAt": "2024-07-31T11:47:04.482Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "42dd6bc7-b219-4e6c-a817-bf552e8b79b6",
      "number": 15,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.589Z",
      "updatedAt": "2024-07-31T11:47:04.492Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "89452252-0f0c-4dc0-9c27-60e51b515ecb",
      "number": 16,
      "content": "Vì Ngài phá tan các cổng bằng đồng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.594Z",
      "updatedAt": "2024-07-31T11:47:04.502Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "3bf4a854-af9b-4213-a929-d00185186f84",
      "number": 16,
      "content": "Và Ngài cắt rời những song bằng sắt.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.594Z",
      "updatedAt": "2024-07-31T11:47:04.513Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "d9ee6bb1-376e-458e-97e0-12396a62a55b",
      "number": 17,
      "content": "Có những kẻ cứ bội nghịch nên hóa ra điên dại,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.599Z",
      "updatedAt": "2024-07-31T11:47:04.528Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "038658ae-ab2a-4732-89dc-6e2422b11c45",
      "number": 17,
      "content": "Và vì tội lỗi mình nên phải chuốc lấy khổ đau.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.599Z",
      "updatedAt": "2024-07-31T11:47:04.539Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "8c9581ad-b661-44be-9e4d-1c7945e1473f",
      "number": 18,
      "content": "Linh hồn họ gớm ghê mọi thực phẩm,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.605Z",
      "updatedAt": "2024-07-31T11:47:04.548Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "81bcbe92-3c96-43c7-af3a-d253ae3d3112",
      "number": 18,
      "content": "Và họ bị kéo gần đến cổng của tử thần.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.605Z",
      "updatedAt": "2024-07-31T11:47:04.558Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "ccc0b1cf-ca04-482f-b44f-83db1f1c9efe",
      "number": 19,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.610Z",
      "updatedAt": "2024-07-31T11:47:04.567Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "bb051801-d37e-449e-b318-668263c93312",
      "number": 19,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.610Z",
      "updatedAt": "2024-07-31T11:47:04.578Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "331c0c2a-ed40-4eae-b359-6fff943e4b1d",
      "number": 20,
      "content": "Ngài ban lời Ngài và chữa lành họ;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.615Z",
      "updatedAt": "2024-07-31T11:47:04.589Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "2bba3ffb-e1f7-4647-bb5d-f7387d288a7f",
      "number": 20,
      "content": "Ngài giải cứu họ khỏi miệng của phần mộ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.615Z",
      "updatedAt": "2024-07-31T11:47:04.598Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "d82ed213-2252-4159-87d1-0a1f4b4b7392",
      "number": 21,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.621Z",
      "updatedAt": "2024-07-31T11:47:04.608Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "0540a0ee-4d17-48df-a77b-0e88b21fa7cf",
      "number": 21,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.621Z",
      "updatedAt": "2024-07-31T11:47:04.617Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "5ebb3ffc-13e5-443b-a41e-8f3aeb7ad8ca",
      "number": 22,
      "content": "Cũng hãy để họ dâng những của lễ cảm tạ lên Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.626Z",
      "updatedAt": "2024-07-31T11:47:04.628Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "039ac4e7-5d9b-424d-a841-5a8b1e96bbfa",
      "number": 22,
      "content": "Và dùng những lời ca ngợi vui mừng thuật lại các công việc Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.626Z",
      "updatedAt": "2024-07-31T11:47:04.637Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "8e6c37c3-a150-481a-a89e-218d49d4fce5",
      "number": 23,
      "content": "Có những kẻ xuống tàu vượt đại dương,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.632Z",
      "updatedAt": "2024-07-31T11:47:04.650Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "1c330a14-7735-418c-8d94-0d54fdcd9eb5",
      "number": 23,
      "content": "Ấy là những người làm ăn trên biển cả;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.632Z",
      "updatedAt": "2024-07-31T11:47:04.661Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "c6326311-8048-4578-93a1-bd909ae5c85c",
      "number": 24,
      "content": "Họ trông thấy những việc làm của Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.638Z",
      "updatedAt": "2024-07-31T11:47:04.670Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "5668b04e-e6d4-4700-b6a2-87a25bb381fe",
      "number": 24,
      "content": "Và những việc lạ lùng của Ngài ở giữa biển sâu.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.638Z",
      "updatedAt": "2024-07-31T11:47:04.681Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "2a27f588-9c86-45d6-8323-2c6e033c2d05",
      "number": 25,
      "content": "Vì Ngài phán một tiếng, bão tố liền nổi lên cuồn cuộn,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.643Z",
      "updatedAt": "2024-07-31T11:47:04.690Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "5e1890b3-9d4d-4c48-aeb6-f01994e4c437",
      "number": 25,
      "content": "Cuồng phong nâng cao những ngọn sóng kinh hồn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.643Z",
      "updatedAt": "2024-07-31T11:47:04.698Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "76a65a04-78be-4137-924b-3d13f702386e",
      "number": 26,
      "content": "Sóng cả dâng cao đến tận trời, rồi trụt sâu xuống đáy vực thẳm;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.649Z",
      "updatedAt": "2024-07-31T11:47:04.708Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "6b784b7a-0f17-4f72-a9f7-1290cef29b4c",
      "number": 26,
      "content": "Giữa cảnh khốn cùng họ hồn phi phách tán.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.649Z",
      "updatedAt": "2024-07-31T11:47:04.716Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "770f8ee3-49d6-4721-a72d-5fd17b616c00",
      "number": 27,
      "content": "Họ bị quay cuồng đảo điên như người say rượu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.654Z",
      "updatedAt": "2024-07-31T11:47:04.725Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "c446c9bc-2cde-4f03-af3f-0d68986145b9",
      "number": 27,
      "content": "Mọi tài trí khôn ngoan đều biến mất.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.654Z",
      "updatedAt": "2024-07-31T11:47:04.733Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "99ff80f3-de78-40bc-b8fa-634999c473e7",
      "number": 28,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.660Z",
      "updatedAt": "2024-07-31T11:47:04.743Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "a41f01bb-bf2a-4235-899a-16b48a7775ba",
      "number": 28,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.660Z",
      "updatedAt": "2024-07-31T11:47:04.751Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "eacd5d70-2773-4c7e-b09b-97779154123d",
      "number": 29,
      "content": "Ngài khiến bão tố yên lặng như tờ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.665Z",
      "updatedAt": "2024-07-31T11:47:04.761Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "2a9f565c-3857-459f-95fe-3cd9a3cddc89",
      "number": 29,
      "content": "Những ngọn sóng lớn lặng im.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.665Z",
      "updatedAt": "2024-07-31T11:47:04.771Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "12c0adef-5df0-482a-af45-5f19be10cb04",
      "number": 30,
      "content": "Họ rất đỗi vui mừng vì biển êm gió lặng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.671Z",
      "updatedAt": "2024-07-31T11:47:04.781Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "1097175d-f7ef-48a5-b0d4-d9a031f25109",
      "number": 30,
      "content": "Rồi Ngài đưa họ đến bến bờ họ mong ước.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.671Z",
      "updatedAt": "2024-07-31T11:47:04.790Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "44f4f303-dde0-4257-880a-dd11e623b249",
      "number": 31,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.676Z",
      "updatedAt": "2024-07-31T11:47:04.799Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "6a7d6d01-0f1b-4518-9660-28821367bd47",
      "number": 31,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.676Z",
      "updatedAt": "2024-07-31T11:47:04.808Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "671e0752-b5a6-45df-b5ae-ade8311804b2",
      "number": 32,
      "content": "Hãy để họ tôn cao Ngài giữa đại hội,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.682Z",
      "updatedAt": "2024-07-31T11:47:04.817Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "c2f77159-c68f-4a0e-84c3-5db173f9074a",
      "number": 32,
      "content": "Và ca tụng Ngài giữa sảnh đường của hội đồng trưởng lão.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.682Z",
      "updatedAt": "2024-07-31T11:47:04.828Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "9c203b50-b6d9-4ef8-8daa-e6459cdd8ac9",
      "number": 33,
      "content": "Ngài biến các dòng sông thành sa mạc hoang vu,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.687Z",
      "updatedAt": "2024-07-31T11:47:04.840Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "21fd526c-8087-48ce-b244-b80367c7aeb2",
      "number": 33,
      "content": "Các dòng suối thành đất khô khan cằn cỗi,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.687Z",
      "updatedAt": "2024-07-31T11:47:04.848Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "07fe73f3-ebdf-4d63-b12f-e81e3a4a348c",
      "number": 34,
      "content": "Ðất đai màu mỡ thành đồng chua nước mặn,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.691Z",
      "updatedAt": "2024-07-31T11:47:04.857Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "09264540-b110-472c-9a66-4ce7b08ee6ab",
      "number": 34,
      "content": "Vì sự gian ác của dân cư ở đó.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.691Z",
      "updatedAt": "2024-07-31T11:47:04.865Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "a8cbb188-f9c9-4009-bb4b-84c56ef4ad7f",
      "number": 35,
      "content": "Ngài biến đồng hoang thành đất có ao hồ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.695Z",
      "updatedAt": "2024-07-31T11:47:04.874Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "4116cfc9-6b03-4061-aecc-4a4e1df145f0",
      "number": 35,
      "content": "Và xứ khô cằn thành nơi có dồi dào các dòng nước ngọt;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.695Z",
      "updatedAt": "2024-07-31T11:47:04.886Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "d3f70350-dd41-4b17-9516-fa70c44ac940",
      "number": 36,
      "content": "Ngài đã cho những kẻ đói khát đến cư ngụ tại đó,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.698Z",
      "updatedAt": "2024-07-31T11:47:04.896Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "2454086d-880b-4bbc-958b-e0477b7ad3b4",
      "number": 36,
      "content": "Ðể họ xây dựng một thành mà an cư lạc nghiệp;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.698Z",
      "updatedAt": "2024-07-31T11:47:04.905Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "d71517eb-46a8-4a8d-ae5d-21f79214b41f",
      "number": 37,
      "content": "Họ gieo mạ ngoài đồng và trồng tỉa vườn nho,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.702Z",
      "updatedAt": "2024-07-31T11:47:04.914Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "1da9b1ac-108c-478a-9d29-8738fd9264b8",
      "number": 37,
      "content": "Rồi thu hoạch hoa màu sung túc.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.702Z",
      "updatedAt": "2024-07-31T11:47:04.937Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "fef77c44-46a0-459f-bf8a-988983762fbc",
      "number": 38,
      "content": "Ngài ban phước cho họ, và dân số họ gia tăng;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.705Z",
      "updatedAt": "2024-07-31T11:47:04.952Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "58576da1-7c45-46a1-9051-ff0147995508",
      "number": 38,
      "content": "Ngài chẳng để cho bầy súc vật của họ bị giảm thiểu.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.705Z",
      "updatedAt": "2024-07-31T11:47:04.967Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "16983a61-bc60-4314-b307-5f0a1ced8dd7",
      "number": 39,
      "content": "Có lúc họ bị tiêu hao lụn bại, phải cúi đầu khuất phục ngoại bang,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.709Z",
      "updatedAt": "2024-07-31T11:47:04.987Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "1f9fde91-8aae-4f8d-8a4f-048c4040ca5d",
      "number": 39,
      "content": "Bị áp bức, khổ đau, và sầu não.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.709Z",
      "updatedAt": "2024-07-31T11:47:05.002Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "1f6a3b94-1738-4047-8981-06d6de13e80c",
      "number": 40,
      "content": "Ngài trút đổ ô nhục trên những người quyền quý của họ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.712Z",
      "updatedAt": "2024-07-31T11:47:05.017Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "19ced4f1-847a-4180-a2db-6c30d1647739",
      "number": 40,
      "content": "Khiến họ lang thang trong nơi hoang vắng không đường.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.712Z",
      "updatedAt": "2024-07-31T11:47:05.033Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "f9499c03-64f3-4a18-9b12-479b2bbaf0ec",
      "number": 41,
      "content": "Nhưng Ngài lại cất những người nghèo ra khỏi cảnh lầm than,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.716Z",
      "updatedAt": "2024-07-31T11:47:05.044Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "8d6c7907-5477-4d3a-b42c-2e8d65a9c1fb",
      "number": 41,
      "content": "Và cho con cháu họ được gia tăng đông đúc.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.716Z",
      "updatedAt": "2024-07-31T11:47:05.053Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "e5f31a2a-0f89-455b-a625-4721ed096a0e",
      "number": 42,
      "content": "Những người ngay lành thấy thế bèn vui mừng;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.719Z",
      "updatedAt": "2024-07-31T11:47:05.062Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "9c086cec-4980-4c3d-9214-fabb6ccc072b",
      "number": 42,
      "content": "Còn những kẻ gian ác thảy đều câm miệng lại.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.719Z",
      "updatedAt": "2024-07-31T11:47:05.072Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "e087cff2-d552-41a6-962d-e9fac6758f61",
      "number": 43,
      "content": "Hỡi ai là người khôn ngoan, hãy suy gẫm những điều ấy,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.722Z",
      "updatedAt": "2024-07-31T11:47:05.081Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    },
    {
      "id": "a4d72148-0dc0-4322-939a-903a1a5a2b3e",
      "number": 43,
      "content": "Rồi sẽ hiểu được thế nào là tình thương của Chúa.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:06:12.722Z",
      "updatedAt": "2024-07-31T11:47:05.090Z",
      "chapterId": "e3ea0aee-2636-4070-9a5a-f3c36ff49925"
    }
  ],
