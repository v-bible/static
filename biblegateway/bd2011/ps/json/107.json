{
  "chapter": {
    "id": "91f202f1-f399-43e7-afdf-1f14ed6077d1",
    "number": 107,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20107&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.357Z",
    "updatedAt": "2024-09-09T04:13:01.357Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "b138a956-862f-4b00-9e1e-d9c6e4cc8aba",
      "number": 1,
      "content": "Hãy cảm tạ Chúa, vì Ngài thật tốt,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.130Z",
      "updatedAt": "2024-09-09T04:49:53.130Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "b1a0dd5b-0b93-4da3-b1ee-fd17584589bd",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.145Z",
      "updatedAt": "2024-09-09T04:49:53.145Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "81fed3fa-b634-4986-9a50-c8ffd84f043d",
      "number": 2,
      "content": "Nguyện những người được cứu chuộc của Chúa nói như vậy,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.148Z",
      "updatedAt": "2024-09-09T04:49:53.148Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "def236e7-b8d2-4c58-b91b-01809137e824",
      "number": 2,
      "content": "Những người được Ngài cứu khỏi tay kẻ thù,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.150Z",
      "updatedAt": "2024-09-09T04:49:53.150Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "c4ce69e1-2229-4323-abd7-0beb81d44e06",
      "number": 3,
      "content": "Rồi gom nhóm họ từ các nước trở về;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.152Z",
      "updatedAt": "2024-09-09T04:49:53.152Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "b6b2a4b9-495c-44f5-bb07-b3f2d0ee54fa",
      "number": 3,
      "content": "Từ phương đông và phương tây, phương bắc và phương nam.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.154Z",
      "updatedAt": "2024-09-09T04:49:53.154Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "75c416d1-438b-4c5c-8c7f-1f2702ee31ba",
      "number": 4,
      "content": "Họ lang thang trong đồng hoang, trong nơi hoang vắng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.156Z",
      "updatedAt": "2024-09-09T04:49:53.156Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "15518ba0-6750-46da-9d13-4c42dedecebe",
      "number": 4,
      "content": "Họ chẳng tìm được đường về một thành để ở.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.159Z",
      "updatedAt": "2024-09-09T04:49:53.159Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "281048c5-44f7-4df5-a721-41e7421d0611",
      "number": 5,
      "content": "Họ đói khát;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.164Z",
      "updatedAt": "2024-09-09T04:49:53.164Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "acd84fb3-227d-4ac3-9f35-5ba3676adcea",
      "number": 5,
      "content": "Linh hồn họ mòn mỏi nao sờn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.169Z",
      "updatedAt": "2024-09-09T04:49:53.169Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "88cf7560-5072-472f-b8da-bf2cc486cd09",
      "number": 6,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.173Z",
      "updatedAt": "2024-09-09T04:49:53.173Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "65b84e41-640d-4625-8437-2004ecc1cb5f",
      "number": 19,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.304Z",
      "updatedAt": "2024-09-09T04:49:53.304Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "b92c5dfb-fffb-401d-af6d-c22067661647",
      "number": 28,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.389Z",
      "updatedAt": "2024-09-09T04:49:53.389Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "984ea05e-1439-4417-bb08-bc74ae2d824e",
      "number": 13,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.243Z",
      "updatedAt": "2024-09-09T04:49:53.243Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "540d0591-8116-48ee-b8cb-3db2d274ca83",
      "number": 28,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.393Z",
      "updatedAt": "2024-09-09T04:49:53.393Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "66dce407-2fcb-44a5-904d-5fd3a02990f4",
      "number": 6,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.178Z",
      "updatedAt": "2024-09-09T04:49:53.178Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "c7e3fa6b-c99a-4976-b163-553f256295ac",
      "number": 13,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.248Z",
      "updatedAt": "2024-09-09T04:49:53.248Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "fdb2ab3e-934e-4381-94f9-888fb2109e52",
      "number": 19,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.308Z",
      "updatedAt": "2024-09-09T04:49:53.308Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "a53e3ca6-b585-46df-995b-4435db385cbf",
      "number": 7,
      "content": "Ngài dẫn đưa họ đúng vào con đường phải đi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.182Z",
      "updatedAt": "2024-09-09T04:49:53.182Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "be9ce047-9b46-404a-aa71-31b71415ffcb",
      "number": 7,
      "content": "Ðể họ đến một thành mà ở.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.188Z",
      "updatedAt": "2024-09-09T04:49:53.188Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "f0c8204c-3127-4704-9d4d-db902131a0c6",
      "number": 8,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.192Z",
      "updatedAt": "2024-09-09T04:49:53.192Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "8c67e05b-ee58-4577-b6bc-816724d62634",
      "number": 21,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.322Z",
      "updatedAt": "2024-09-09T04:49:53.322Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "958d095a-8176-4cbc-88e1-d5b9beb60e01",
      "number": 31,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.416Z",
      "updatedAt": "2024-09-09T04:49:53.416Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "e8a43199-1a7e-43d1-8a95-fbdd3a748f0b",
      "number": 15,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.263Z",
      "updatedAt": "2024-09-09T04:49:53.263Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "97e2fbb4-da9b-4e35-8045-20cb9d91a906",
      "number": 8,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.197Z",
      "updatedAt": "2024-09-09T04:49:53.197Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "d4bf228d-9417-4f8d-811f-a3e995d954d2",
      "number": 9,
      "content": "Vì Ngài làm thỏa mãn linh hồn khao khát,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.203Z",
      "updatedAt": "2024-09-09T04:49:53.203Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "0dbfce23-49e5-4158-85d5-ea185bf23025",
      "number": 9,
      "content": "Và làm no nê linh hồn đói lả bằng các thức ăn ngon.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.208Z",
      "updatedAt": "2024-09-09T04:49:53.208Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "5e3baa76-d24c-4fca-9a43-6a9bb38c0172",
      "number": 10,
      "content": "Có những kẻ sống trong tối tăm và trong bóng tử thần,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.213Z",
      "updatedAt": "2024-09-09T04:49:53.213Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "6a4f1544-4240-49fd-af08-b374d2a641ae",
      "number": 10,
      "content": "Bị giam cầm trong khổ đau và xiềng xích,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.218Z",
      "updatedAt": "2024-09-09T04:49:53.218Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "1c8bc0ea-4eb3-4ddb-a908-b8b6107c47eb",
      "number": 11,
      "content": "Bởi vì họ chống lại những lời của Ðức Chúa Trời,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.223Z",
      "updatedAt": "2024-09-09T04:49:53.223Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "d73a1aca-e536-4c6b-a47b-b6aad908cbe7",
      "number": 11,
      "content": "Và khinh bỉ lời chỉ bảo của Ðấng Chí Cao.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.227Z",
      "updatedAt": "2024-09-09T04:49:53.227Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "3e166351-77bc-40a4-83a7-84619f68cb4e",
      "number": 12,
      "content": "Vì thế Ngài lấy gian lao hạ lòng họ xuống;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.232Z",
      "updatedAt": "2024-09-09T04:49:53.232Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "16cabca1-c3d0-4e1a-a624-7f1827477beb",
      "number": 12,
      "content": "Họ bị ngã nhào và chẳng ai giúp họ đứng lên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.238Z",
      "updatedAt": "2024-09-09T04:49:53.238Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "f081e3d5-77c0-4eea-b477-58a293d07c8b",
      "number": 14,
      "content": "Ngài đem họ ra khỏi cảnh tối tăm và bóng tử thần,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.252Z",
      "updatedAt": "2024-09-09T04:49:53.252Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "0a16f612-35ba-49cf-8a48-4b4cb896e842",
      "number": 14,
      "content": "Và đập tan xiềng xích gông cùm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.257Z",
      "updatedAt": "2024-09-09T04:49:53.257Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "12d75980-ea9d-4cd8-9e84-a63d04e5ced7",
      "number": 15,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.268Z",
      "updatedAt": "2024-09-09T04:49:53.268Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "f7b3736b-d830-4d5f-a004-6ea2aa521a4a",
      "number": 16,
      "content": "Vì Ngài phá tan các cổng bằng đồng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.273Z",
      "updatedAt": "2024-09-09T04:49:53.273Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "6b7d1f65-c15c-471d-b888-5093b0555c82",
      "number": 16,
      "content": "Và Ngài cắt rời những song bằng sắt.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.278Z",
      "updatedAt": "2024-09-09T04:49:53.278Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "d39350b0-c4ea-41f7-8b49-e8716db6afce",
      "number": 17,
      "content": "Có những kẻ cứ bội nghịch nên hóa ra điên dại,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.283Z",
      "updatedAt": "2024-09-09T04:49:53.283Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "b8f6738b-10f5-4b36-99a7-a8c73a35968f",
      "number": 17,
      "content": "Và vì tội lỗi mình nên phải chuốc lấy khổ đau.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.288Z",
      "updatedAt": "2024-09-09T04:49:53.288Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "b820ad79-48d8-4827-97a9-6e80343067a5",
      "number": 18,
      "content": "Linh hồn họ gớm ghê mọi thực phẩm,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.293Z",
      "updatedAt": "2024-09-09T04:49:53.293Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "cd28f7ee-2f7e-4ac0-9b24-d048c882d906",
      "number": 18,
      "content": "Và họ bị kéo gần đến cổng của tử thần.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.299Z",
      "updatedAt": "2024-09-09T04:49:53.299Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "a869ab9e-b30e-4467-ab76-7fbec724096f",
      "number": 20,
      "content": "Ngài ban lời Ngài và chữa lành họ;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.313Z",
      "updatedAt": "2024-09-09T04:49:53.313Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "e499a9f2-57d0-4f19-a240-3ba78ae81a7a",
      "number": 20,
      "content": "Ngài giải cứu họ khỏi miệng của phần mộ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.317Z",
      "updatedAt": "2024-09-09T04:49:53.317Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "f0949d0e-ee9d-471f-b24f-8f8e178328cf",
      "number": 31,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.421Z",
      "updatedAt": "2024-09-09T04:49:53.421Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "023763ee-0512-4c69-a459-65b65cd56f90",
      "number": 21,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.326Z",
      "updatedAt": "2024-09-09T04:49:53.326Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "726b732d-fde5-42eb-a047-34c004fd5086",
      "number": 22,
      "content": "Cũng hãy để họ dâng những của lễ cảm tạ lên Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.331Z",
      "updatedAt": "2024-09-09T04:49:53.331Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "eaa5f056-63f6-4909-a078-f2ed848593b7",
      "number": 22,
      "content": "Và dùng những lời ca ngợi vui mừng thuật lại các công việc Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.336Z",
      "updatedAt": "2024-09-09T04:49:53.336Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "e8c6325c-b3b9-45c2-90f2-a48450c25a09",
      "number": 23,
      "content": "Có những kẻ xuống tàu vượt đại dương,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.340Z",
      "updatedAt": "2024-09-09T04:49:53.340Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "9addd6cc-bb41-4f85-9b14-ae0e661a1f9a",
      "number": 23,
      "content": "Ấy là những người làm ăn trên biển cả;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.346Z",
      "updatedAt": "2024-09-09T04:49:53.346Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "9282f43f-d4f0-4d48-85ff-529c7628ce87",
      "number": 24,
      "content": "Họ trông thấy những việc làm của Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.351Z",
      "updatedAt": "2024-09-09T04:49:53.351Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "ab22dbc1-4780-4c70-9dcd-fd06ef58b3fe",
      "number": 24,
      "content": "Và những việc lạ lùng của Ngài ở giữa biển sâu.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.356Z",
      "updatedAt": "2024-09-09T04:49:53.356Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "d2983965-b77b-4855-89aa-a4a6c0542c69",
      "number": 25,
      "content": "Vì Ngài phán một tiếng, bão tố liền nổi lên cuồn cuộn,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.360Z",
      "updatedAt": "2024-09-09T04:49:53.360Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "82707b3e-2f72-472c-95c4-e90b27554280",
      "number": 25,
      "content": "Cuồng phong nâng cao những ngọn sóng kinh hồn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.365Z",
      "updatedAt": "2024-09-09T04:49:53.365Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "7ac37316-2fc2-4ca6-a737-ced75d33eeaa",
      "number": 26,
      "content": "Sóng cả dâng cao đến tận trời, rồi trụt sâu xuống đáy vực thẳm;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.370Z",
      "updatedAt": "2024-09-09T04:49:53.370Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "c5400364-7c6b-43f7-ae8b-9a925bc88c33",
      "number": 26,
      "content": "Giữa cảnh khốn cùng họ hồn phi phách tán.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.375Z",
      "updatedAt": "2024-09-09T04:49:53.375Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "53a68890-6c4a-4364-b693-42ec69ae37c2",
      "number": 27,
      "content": "Họ bị quay cuồng đảo điên như người say rượu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.380Z",
      "updatedAt": "2024-09-09T04:49:53.380Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "9215d4eb-57da-4a14-a7b1-6b5166b6922b",
      "number": 27,
      "content": "Mọi tài trí khôn ngoan đều biến mất.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.384Z",
      "updatedAt": "2024-09-09T04:49:53.384Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "4fdcf0b1-3a6b-4897-97ec-491e7feffcca",
      "number": 29,
      "content": "Ngài khiến bão tố yên lặng như tờ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.398Z",
      "updatedAt": "2024-09-09T04:49:53.398Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "2ff78203-2362-42e1-b86d-ae1ba6d7a7ff",
      "number": 29,
      "content": "Những ngọn sóng lớn lặng im.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.403Z",
      "updatedAt": "2024-09-09T04:49:53.403Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "907b8fe9-1515-4198-9ec5-e0ad7872c08f",
      "number": 30,
      "content": "Họ rất đỗi vui mừng vì biển êm gió lặng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.407Z",
      "updatedAt": "2024-09-09T04:49:53.407Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "c28557a3-6fdb-4260-987e-f5ee51af82ff",
      "number": 30,
      "content": "Rồi Ngài đưa họ đến bến bờ họ mong ước.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.412Z",
      "updatedAt": "2024-09-09T04:49:53.412Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "81bda8b7-ac19-4d83-8798-6d2891611330",
      "number": 32,
      "content": "Hãy để họ tôn cao Ngài giữa đại hội,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.425Z",
      "updatedAt": "2024-09-09T04:49:53.425Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "5da141be-530b-4633-bd3e-9545552fff3b",
      "number": 32,
      "content": "Và ca tụng Ngài giữa sảnh đường của hội đồng trưởng lão.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.429Z",
      "updatedAt": "2024-09-09T04:49:53.429Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "c04eb24f-e444-4b3e-a7dd-5a8f8873c711",
      "number": 33,
      "content": "Ngài biến các dòng sông thành sa mạc hoang vu,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.432Z",
      "updatedAt": "2024-09-09T04:49:53.432Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "f862ee70-cdb5-4018-998f-098cbf5b92ac",
      "number": 33,
      "content": "Các dòng suối thành đất khô khan cằn cỗi,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.435Z",
      "updatedAt": "2024-09-09T04:49:53.435Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "c84fbf70-ce90-4d86-b044-ded0517569cb",
      "number": 34,
      "content": "Ðất đai màu mỡ thành đồng chua nước mặn,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.437Z",
      "updatedAt": "2024-09-09T04:49:53.437Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "7404e32a-722a-4255-8da8-952b58a06d41",
      "number": 34,
      "content": "Vì sự gian ác của dân cư ở đó.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.440Z",
      "updatedAt": "2024-09-09T04:49:53.440Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "901cbcf9-6a6b-453c-b930-c819d3e926e5",
      "number": 35,
      "content": "Ngài biến đồng hoang thành đất có ao hồ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.442Z",
      "updatedAt": "2024-09-09T04:49:53.442Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "544dae9b-aae5-4e0e-bd37-d34245adc758",
      "number": 35,
      "content": "Và xứ khô cằn thành nơi có dồi dào các dòng nước ngọt;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.445Z",
      "updatedAt": "2024-09-09T04:49:53.445Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "5c9ccca8-f86f-4f11-9687-05ce8ec20b10",
      "number": 36,
      "content": "Ngài đã cho những kẻ đói khát đến cư ngụ tại đó,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.451Z",
      "updatedAt": "2024-09-09T04:49:53.451Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "1eda0d32-361c-40a9-a5bc-b37e13db3d09",
      "number": 36,
      "content": "Ðể họ xây dựng một thành mà an cư lạc nghiệp;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.454Z",
      "updatedAt": "2024-09-09T04:49:53.454Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "c17d6a73-b54b-486e-81f1-acd995f32085",
      "number": 37,
      "content": "Họ gieo mạ ngoài đồng và trồng tỉa vườn nho,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.459Z",
      "updatedAt": "2024-09-09T04:49:53.459Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "344c7d3d-d529-4b3a-8362-4e32cfd62eda",
      "number": 37,
      "content": "Rồi thu hoạch hoa màu sung túc.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.464Z",
      "updatedAt": "2024-09-09T04:49:53.464Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "4710e6f0-4114-40c1-aed6-e83d176e154a",
      "number": 38,
      "content": "Ngài ban phước cho họ, và dân số họ gia tăng;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.469Z",
      "updatedAt": "2024-09-09T04:49:53.469Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "c0a50df5-17b0-4699-be72-cbafd8a9686a",
      "number": 38,
      "content": "Ngài chẳng để cho bầy súc vật của họ bị giảm thiểu.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.473Z",
      "updatedAt": "2024-09-09T04:49:53.473Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "c469a2ba-9b51-4e06-b7e7-feac15ac6d26",
      "number": 39,
      "content": "Có lúc họ bị tiêu hao lụn bại, phải cúi đầu khuất phục ngoại bang,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.478Z",
      "updatedAt": "2024-09-09T04:49:53.478Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "ba48676b-18a2-496d-9381-48ecb757a79f",
      "number": 39,
      "content": "Bị áp bức, khổ đau, và sầu não.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.483Z",
      "updatedAt": "2024-09-09T04:49:53.483Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "9760afdf-3d6b-4a0a-8f4d-2b294378f278",
      "number": 40,
      "content": "Ngài trút đổ ô nhục trên những người quyền quý của họ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.487Z",
      "updatedAt": "2024-09-09T04:49:53.487Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "bb132fef-a031-4a99-ac3b-cc469f87c156",
      "number": 40,
      "content": "Khiến họ lang thang trong nơi hoang vắng không đường.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.493Z",
      "updatedAt": "2024-09-09T04:49:53.493Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "151e5904-92e0-4c01-8b05-c5c93d5e0361",
      "number": 41,
      "content": "Nhưng Ngài lại cất những người nghèo ra khỏi cảnh lầm than,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.497Z",
      "updatedAt": "2024-09-09T04:49:53.497Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "c9b225be-9f2b-45a2-9da4-72289a7285ae",
      "number": 41,
      "content": "Và cho con cháu họ được gia tăng đông đúc.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.502Z",
      "updatedAt": "2024-09-09T04:49:53.502Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "7a6bc54b-6291-45fc-b89f-ce9f84fba0d7",
      "number": 42,
      "content": "Những người ngay lành thấy thế bèn vui mừng;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.508Z",
      "updatedAt": "2024-09-09T04:49:53.508Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "35983dff-c3a2-4bc9-8b4d-78d8fed64e43",
      "number": 42,
      "content": "Còn những kẻ gian ác thảy đều câm miệng lại.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.513Z",
      "updatedAt": "2024-09-09T04:49:53.513Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "f302e632-9610-4d22-b614-0a1408240519",
      "number": 43,
      "content": "Hỡi ai là người khôn ngoan, hãy suy gẫm những điều ấy,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.518Z",
      "updatedAt": "2024-09-09T04:49:53.518Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    },
    {
      "id": "02c84982-a665-4d19-b77b-0fbdf02f538a",
      "number": 43,
      "content": "Rồi sẽ hiểu được thế nào là tình thương của Chúa.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:53.523Z",
      "updatedAt": "2024-09-09T04:49:53.523Z",
      "chapterId": "91f202f1-f399-43e7-afdf-1f14ed6077d1"
    }
  ],
