  },
  "verses": [
    {
      "id": "2a1cb52b-ab42-46de-bdbe-8df01369a458",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.540Z",
      "updatedAt": "2024-09-24T02:13:46.540Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "155a1a82-49e9-493b-9831-4a009692d791",
      "number": 6,
      "content": "Hãy ca ngợi Chúa!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.568Z",
      "updatedAt": "2024-09-24T02:13:46.568Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "6766fc47-b2ee-428c-84df-e7dcb9ba8149",
      "number": 1,
      "content": "Hãy ca ngợi Ðức Chúa Trời trong nơi thánh Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.545Z",
      "updatedAt": "2024-09-24T02:13:46.545Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "36f29591-f9dc-4746-afb0-7f216a08cd82",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong bầu trời lớn lao vô tận của Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.547Z",
      "updatedAt": "2024-09-24T02:13:46.547Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "113b4d8e-73bd-4ddd-9f04-01ad46a9aff0",
      "number": 2,
      "content": "Hãy ca ngợi Ngài về những việc quyền năng Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.549Z",
      "updatedAt": "2024-09-24T02:13:46.549Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "9008b061-e1b1-411b-b669-249729205243",
      "number": 2,
      "content": "Hãy ca ngợi Ngài theo sự lớn lao vĩ đại của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.551Z",
      "updatedAt": "2024-09-24T02:13:46.551Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "4dde7d51-e89c-4042-b504-3e128d23f9c9",
      "number": 3,
      "content": "Hãy dùng tiếng kèn ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.554Z",
      "updatedAt": "2024-09-24T02:13:46.554Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "0e4b9ccf-fbcd-42d8-8849-16857fc2e8c6",
      "number": 3,
      "content": "Hãy dùng đàn lia và hạc cầm ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.556Z",
      "updatedAt": "2024-09-24T02:13:46.556Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "287384ae-f56e-4364-8428-e921dae35eed",
      "number": 4,
      "content": "Hãy dùng trống lục lạc và điệu vũ ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.558Z",
      "updatedAt": "2024-09-24T02:13:46.558Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "9e7e3f79-b561-4726-8f97-c51c82a40ae2",
      "number": 4,
      "content": "Hãy dùng các đàn dây và ống sáo ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.560Z",
      "updatedAt": "2024-09-24T02:13:46.560Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "8514f1da-84df-4d9c-bfa2-316823f23153",
      "number": 5,
      "content": "Hãy dùng các phèng la vang tiếng ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.562Z",
      "updatedAt": "2024-09-24T02:13:46.562Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "849aecf1-5182-4c18-9304-e8a3c2daddac",
      "number": 5,
      "content": "Hãy dùng các khánh chiêng vang rền ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.564Z",
      "updatedAt": "2024-09-24T02:13:46.564Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    },
    {
      "id": "30fb4242-659d-41dc-bb05-c7ac4739ef10",
      "number": 6,
      "content": "Phàm vật chi thở, hãy ca ngợi Chúa.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:46.566Z",
      "updatedAt": "2024-09-24T02:13:46.566Z",
      "chapterId": "a790a745-005a-46c3-8dc9-7ec61e372509"
    }
  ],
