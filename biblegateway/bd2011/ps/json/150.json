  },
  "verses": [
    {
      "id": "86cac8d2-eac4-4c95-ab6a-1381096faf30",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:08.990Z",
      "updatedAt": "2024-07-31T11:56:22.744Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "133084f9-9b1e-4af6-9c27-f905c979f516",
      "number": 1,
      "content": "Hãy ca ngợi Ðức Chúa Trời trong nơi thánh Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:08.990Z",
      "updatedAt": "2024-07-31T11:56:22.753Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "ce021c36-8184-4a17-832b-84f85e518e29",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong bầu trời lớn lao vô tận của Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:08.990Z",
      "updatedAt": "2024-07-31T11:56:22.762Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "3103bbde-b41a-4339-91a3-195a4daef13c",
      "number": 2,
      "content": "Hãy ca ngợi Ngài về những việc quyền năng Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:09Z",
      "updatedAt": "2024-07-31T11:56:22.770Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "135172d3-a517-4231-88c1-2a01d6c5c491",
      "number": 2,
      "content": "Hãy ca ngợi Ngài theo sự lớn lao vĩ đại của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:09Z",
      "updatedAt": "2024-07-31T11:56:22.778Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "3d496522-b895-46ca-8e7e-60388d81b3a9",
      "number": 3,
      "content": "Hãy dùng tiếng kèn ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:09.002Z",
      "updatedAt": "2024-07-31T11:56:22.811Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "0f8756b5-a092-44e0-a1c6-fcf4b1027e6c",
      "number": 3,
      "content": "Hãy dùng đàn lia và hạc cầm ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:09.002Z",
      "updatedAt": "2024-07-31T11:56:22.841Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "5ee78986-6155-4a6a-9a31-9336a695015a",
      "number": 4,
      "content": "Hãy dùng trống lục lạc và điệu vũ ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:09.004Z",
      "updatedAt": "2024-07-31T11:56:22.861Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "bc31a576-8efc-41bc-989d-93568d96c7b5",
      "number": 4,
      "content": "Hãy dùng các đàn dây và ống sáo ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:09.004Z",
      "updatedAt": "2024-07-31T11:56:22.875Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "ebb6d31a-06ce-4cd1-9e3c-a4a10e96caeb",
      "number": 5,
      "content": "Hãy dùng các phèng la vang tiếng ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:09.007Z",
      "updatedAt": "2024-07-31T11:56:22.884Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "7c974741-9fa6-4a89-9700-def8c38887a6",
      "number": 5,
      "content": "Hãy dùng các khánh chiêng vang rền ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:09.007Z",
      "updatedAt": "2024-07-31T11:56:22.893Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "d64bea6b-870d-4c88-bfae-e3caedc8c763",
      "number": 6,
      "content": "Phàm vật chi thở, hãy ca ngợi Chúa.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:09.009Z",
      "updatedAt": "2024-07-31T11:56:22.902Z",
      "chapterId": "8a2316eb-72a4-4273-83f6-7509db8d39d6"
    },
    {
      "id": "e6e5e6bd-4844-4324-b30a-8d97ba2d42bd",
      "number": 6,
      "content": "Hãy ca ngợi Chúa!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:09.009Z",
      "updatedAt": "2024-07-31T11:56:22.910Z",
