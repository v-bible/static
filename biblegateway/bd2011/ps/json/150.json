  "verses": [
    {
      "id": "4287b795-efbc-4ed8-b764-7f19e6d0321d",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.026Z",
      "updatedAt": "2024-09-09T04:53:27.026Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "47293ea9-dd98-4714-8fc7-bab111baa889",
      "number": 6,
      "content": "Hãy ca ngợi Chúa!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.096Z",
      "updatedAt": "2024-09-09T04:53:27.096Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "ba934556-1e52-4510-8a95-305a339d6dbf",
      "number": 1,
      "content": "Hãy ca ngợi Ðức Chúa Trời trong nơi thánh Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.043Z",
      "updatedAt": "2024-09-09T04:53:27.043Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "233cc90e-c6b3-49be-a575-d23ea6702057",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong bầu trời lớn lao vô tận của Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.048Z",
      "updatedAt": "2024-09-09T04:53:27.048Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "1a284139-9e77-4e65-beed-496932277551",
      "number": 2,
      "content": "Hãy ca ngợi Ngài về những việc quyền năng Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.052Z",
      "updatedAt": "2024-09-09T04:53:27.052Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "cd6bb44e-f697-494f-b657-f6626940ee41",
      "number": 2,
      "content": "Hãy ca ngợi Ngài theo sự lớn lao vĩ đại của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.057Z",
      "updatedAt": "2024-09-09T04:53:27.057Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "0a2170a7-a42e-4914-b744-cab27f35aafd",
      "number": 3,
      "content": "Hãy dùng tiếng kèn ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.062Z",
      "updatedAt": "2024-09-09T04:53:27.062Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "e6301e7a-1b6a-4527-b843-c0d563d860d1",
      "number": 3,
      "content": "Hãy dùng đàn lia và hạc cầm ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.066Z",
      "updatedAt": "2024-09-09T04:53:27.066Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "e0401fbd-2a6c-4ca1-9be1-edc4b27c6a26",
      "number": 4,
      "content": "Hãy dùng trống lục lạc và điệu vũ ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.071Z",
      "updatedAt": "2024-09-09T04:53:27.071Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "4852f4cc-7b53-4ebf-8d39-5c057211502f",
      "number": 4,
      "content": "Hãy dùng các đàn dây và ống sáo ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.076Z",
      "updatedAt": "2024-09-09T04:53:27.076Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "8da203d8-8fb8-4daf-8736-2d084e39d0c7",
      "number": 5,
      "content": "Hãy dùng các phèng la vang tiếng ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.081Z",
      "updatedAt": "2024-09-09T04:53:27.081Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "e58d0dc4-dc25-47d0-92f2-511e00f5a94d",
      "number": 5,
      "content": "Hãy dùng các khánh chiêng vang rền ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.086Z",
      "updatedAt": "2024-09-09T04:53:27.086Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    },
    {
      "id": "06d0719a-e546-46e3-ba82-34a29a08e265",
      "number": 6,
      "content": "Phàm vật chi thở, hãy ca ngợi Chúa.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:27.091Z",
      "updatedAt": "2024-09-09T04:53:27.091Z",
      "chapterId": "5c4109a4-bbc0-4534-9023-c791d2595593"
    }
  ],
