{
  "chapter": {
    "id": "8a2316eb-72a4-4273-83f6-7509db8d39d6",
    "number": 150,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20150&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.794Z",
