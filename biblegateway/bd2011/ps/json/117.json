  "verses": [
    {
      "id": "8ba71b27-6843-43f9-9976-672c6eb092a4",
      "number": 1,
      "content": "Hỡi các nước, hãy ca ngợi Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:13.930Z",
      "updatedAt": "2024-07-31T11:49:38.346Z",
      "chapterId": "97cc7041-471a-4cb3-8491-c86f1895c7b7"
    },
    {
      "id": "13b9e2fd-bb6f-4b14-bf33-696363d77d20",
      "number": 1,
      "content": "Hỡi muôn dân, khá tôn ngợi Ngài!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:13.930Z",
      "updatedAt": "2024-07-31T11:49:38.358Z",
      "chapterId": "97cc7041-471a-4cb3-8491-c86f1895c7b7"
    },
    {
      "id": "a46bbe93-a420-4bfb-b39a-11b5edb227e3",
      "number": 2,
      "content": "Vì tình thương của Ngài đối với chúng ta rất lớn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:13.933Z",
      "updatedAt": "2024-07-31T11:49:38.367Z",
      "chapterId": "97cc7041-471a-4cb3-8491-c86f1895c7b7"
    },
    {
      "id": "9442d819-331f-4588-8c1e-c84c29f0836f",
      "number": 2,
      "content": "Và đức thành tín của Chúa hằng còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:13.933Z",
      "updatedAt": "2024-07-31T11:49:38.374Z",
      "chapterId": "97cc7041-471a-4cb3-8491-c86f1895c7b7"
    },
    {
      "id": "5f1a25a8-2d75-4e9b-b611-a5d34578d259",
      "number": 2,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:13.933Z",
      "updatedAt": "2024-07-31T11:49:38.381Z",
