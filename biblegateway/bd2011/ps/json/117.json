  "verses": [
    {
      "id": "642275a6-d3a2-4721-9878-76dd69e61641",
      "number": 1,
      "content": "Hỡi các nước, hãy ca ngợi Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:41.578Z",
      "updatedAt": "2024-09-09T04:50:41.578Z",
      "chapterId": "dd342cc6-b8b8-4f3b-9406-508fb4fb73f2"
    },
    {
      "id": "47756d72-6c9d-4f38-8f9c-ede6fa3e8c3a",
      "number": 1,
      "content": "Hỡi muôn dân, khá tôn ngợi Ngài!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:41.589Z",
      "updatedAt": "2024-09-09T04:50:41.589Z",
      "chapterId": "dd342cc6-b8b8-4f3b-9406-508fb4fb73f2"
    },
    {
      "id": "f3b52b54-0ffa-445a-8460-429a28ef1cbe",
      "number": 2,
      "content": "Vì tình thương của Ngài đối với chúng ta rất lớn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:41.591Z",
      "updatedAt": "2024-09-09T04:50:41.591Z",
      "chapterId": "dd342cc6-b8b8-4f3b-9406-508fb4fb73f2"
    },
    {
      "id": "9a757d2b-73d6-47be-859a-2f14a115fa09",
      "number": 2,
      "content": "Và đức thành tín của Chúa hằng còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:41.593Z",
      "updatedAt": "2024-09-09T04:50:41.593Z",
      "chapterId": "dd342cc6-b8b8-4f3b-9406-508fb4fb73f2"
    },
    {
      "id": "bb80f41c-2867-4736-a999-d9fda7498789",
      "number": 2,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:41.595Z",
      "updatedAt": "2024-09-09T04:50:41.595Z",
      "chapterId": "dd342cc6-b8b8-4f3b-9406-508fb4fb73f2"
    }
  ],
