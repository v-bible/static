{
  "chapter": {
    "id": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a",
    "number": 114,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20114&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.003Z",
    "updatedAt": "2024-07-24T15:07:48.878Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "21784efd-8bf9-4743-aacb-68e39a4c9d98",
      "number": 1,
      "content": "Khi dân I-sơ-ra-ên ra khỏi Ai-cập,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.312Z",
      "updatedAt": "2024-07-31T11:48:54.786Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "70577cc6-2272-4b92-b2cc-056ed656e903",
      "number": 1,
      "content": "Nhà Gia-cốp ra khỏi một dân nói một ngôn ngữ lạ,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.312Z",
      "updatedAt": "2024-07-31T11:48:54.802Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "45dd1222-e4d7-4ad0-a7ca-a5f5b643a54a",
      "number": 2,
      "content": "Giu-đa trở thành nơi thánh của Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.322Z",
      "updatedAt": "2024-07-31T11:48:54.811Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "1fcbc0c4-bcc5-45dd-8840-c64eaeb2de48",
      "number": 2,
      "content": "I-sơ-ra-ên trở nên vương quốc của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.322Z",
      "updatedAt": "2024-07-31T11:48:54.819Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "91c9c924-c92e-47cd-80f8-016d1368c3e1",
      "number": 3,
      "content": "Nước đại dương trông thấy thế liền hoảng kinh chạy trốn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.324Z",
      "updatedAt": "2024-07-31T11:48:54.831Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "d86795a8-5394-43c1-8745-35fa0c5e198d",
      "number": 3,
      "content": "Nước Sông Giô-đanh đã hỗn loạn chảy ngược về nguồn;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.324Z",
      "updatedAt": "2024-07-31T11:48:54.841Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "fc84b44d-b6d5-4853-af9c-bfa2ffe333ff",
      "number": 4,
      "content": "Các rặng núi nhảy lên như chiên đực;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.326Z",
      "updatedAt": "2024-07-31T11:48:54.850Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "642927ea-5064-43ab-85c9-9067559af90d",
      "number": 4,
      "content": "Các dãy đồi nhảy cỡn như chiên con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.326Z",
      "updatedAt": "2024-07-31T11:48:54.858Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "321d2533-5118-4cc0-9d72-185dc471eaff",
      "number": 5,
      "content": "Ðiều gì đã khiến các ngươi làm như thế?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.329Z",
      "updatedAt": "2024-07-31T11:48:54.866Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "d673f7bd-c7d9-4d04-aa6c-644bb9e075e2",
      "number": 5,
      "content": "Hỡi nước đại dương, sao ngươi chạy trốn?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.329Z",
      "updatedAt": "2024-07-31T11:48:54.874Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "5c1abbc1-6a31-49c1-850d-307920d16217",
      "number": 5,
      "content": "Hỡi nước Sông Giô-đanh, sao ngươi chảy ngược về nguồn?",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.329Z",
      "updatedAt": "2024-07-31T11:48:54.882Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "b0ac511d-f1bd-406a-8df4-383214b49fe5",
      "number": 6,
      "content": "Hỡi các rặng núi, sao các ngươi nhảy lên như chiên đực?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.335Z",
      "updatedAt": "2024-07-31T11:48:54.891Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "fb1bb1df-792a-4437-a260-5653a7e4b833",
      "number": 6,
      "content": "Hỡi các dãy đồi, sao các ngươi nhảy cỡn như chiên con?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.335Z",
      "updatedAt": "2024-07-31T11:48:54.899Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "a7cf72b5-01a0-46ea-9c33-dd40a3de0de6",
      "number": 7,
      "content": "Hỡi trái đất, khá run rẩy trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.341Z",
      "updatedAt": "2024-07-31T11:48:54.909Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "e68cfcf8-8121-4906-bcbb-395b5e7d92dc",
      "number": 7,
      "content": "Tức trước thánh nhan Ðức Chúa Trời của Gia-cốp,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.341Z",
      "updatedAt": "2024-07-31T11:48:54.918Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "87e07bc2-9948-4f3c-bb80-89c00863084e",
      "number": 8,
      "content": "Ðấng khiến nước từ một vầng đá tuôn ra thành ao hồ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.347Z",
      "updatedAt": "2024-07-31T11:48:54.926Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    },
    {
      "id": "81d1cbe1-1b9b-4fec-ae60-f4be96fcde57",
      "number": 8,
      "content": "Từ tảng đá hoa cương, dòng nước ngọt lai láng trào ra.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:38.347Z",
      "updatedAt": "2024-07-31T11:48:54.937Z",
      "chapterId": "2da64fc7-6b78-4ae3-a98c-7da5e385ea1a"
    }
  ],
