{
  "chapter": {
    "id": "333cb57e-ae3f-458f-9d64-b5e22ee4a082",
    "number": 114,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20114&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.501Z",
    "updatedAt": "2024-09-09T04:13:01.501Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "c1dc9050-e51d-4945-bee9-239cb102046c",
      "number": 1,
      "content": "Khi dân I-sơ-ra-ên ra khỏi Ai-cập,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.190Z",
      "updatedAt": "2024-09-09T04:50:27.190Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "7d8f4b10-b77f-4b84-917e-bfda84a5624b",
      "number": 1,
      "content": "Nhà Gia-cốp ra khỏi một dân nói một ngôn ngữ lạ,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.208Z",
      "updatedAt": "2024-09-09T04:50:27.208Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "40a3f29d-f84a-436d-af0c-f409729bf38b",
      "number": 2,
      "content": "Giu-đa trở thành nơi thánh của Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.213Z",
      "updatedAt": "2024-09-09T04:50:27.213Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "e1e4f1d2-07ea-4d35-90af-6a9269ef6ce8",
      "number": 2,
      "content": "I-sơ-ra-ên trở nên vương quốc của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.217Z",
      "updatedAt": "2024-09-09T04:50:27.217Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "e2a200b0-eb75-47ac-88a7-42e27fcd4ec3",
      "number": 3,
      "content": "Nước đại dương trông thấy thế liền hoảng kinh chạy trốn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.222Z",
      "updatedAt": "2024-09-09T04:50:27.222Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "f742cd98-cb22-4705-8b86-90291fd50aed",
      "number": 3,
      "content": "Nước Sông Giô-đanh đã hỗn loạn chảy ngược về nguồn;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.227Z",
      "updatedAt": "2024-09-09T04:50:27.227Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "2e0c8225-8fd2-411f-98e0-839e9023efb7",
      "number": 4,
      "content": "Các rặng núi nhảy lên như chiên đực;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.232Z",
      "updatedAt": "2024-09-09T04:50:27.232Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "464703c4-fcc1-46c8-b72d-3a5dbbdf1ac5",
      "number": 4,
      "content": "Các dãy đồi nhảy cỡn như chiên con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.236Z",
      "updatedAt": "2024-09-09T04:50:27.236Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "6f9748f1-e982-445a-813d-73d2a71bd8c0",
      "number": 5,
      "content": "Ðiều gì đã khiến các ngươi làm như thế?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.241Z",
      "updatedAt": "2024-09-09T04:50:27.241Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "bcba2554-e95d-4b0c-8c2a-bd68e1b5bad5",
      "number": 5,
      "content": "Hỡi nước đại dương, sao ngươi chạy trốn?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.246Z",
      "updatedAt": "2024-09-09T04:50:27.246Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "11f31e0e-bb5d-4578-9a0b-bc987b81ae6c",
      "number": 5,
      "content": "Hỡi nước Sông Giô-đanh, sao ngươi chảy ngược về nguồn?",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.252Z",
      "updatedAt": "2024-09-09T04:50:27.252Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "faa5f80e-1266-401f-a6a1-97c5b95c419f",
      "number": 6,
      "content": "Hỡi các rặng núi, sao các ngươi nhảy lên như chiên đực?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.256Z",
      "updatedAt": "2024-09-09T04:50:27.256Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "10763936-962a-4c49-8d52-959ce25cfdb7",
      "number": 6,
      "content": "Hỡi các dãy đồi, sao các ngươi nhảy cỡn như chiên con?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.262Z",
      "updatedAt": "2024-09-09T04:50:27.262Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "2b7e0a9f-9eb8-4088-abda-ed843971ac56",
      "number": 7,
      "content": "Hỡi trái đất, khá run rẩy trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.267Z",
      "updatedAt": "2024-09-09T04:50:27.267Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "5bb9322e-8786-40cc-b4c5-7a6d97690632",
      "number": 7,
      "content": "Tức trước thánh nhan Ðức Chúa Trời của Gia-cốp,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.272Z",
      "updatedAt": "2024-09-09T04:50:27.272Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "5568e56c-5ca8-42da-b8b1-df7b7bd2dd4f",
      "number": 8,
      "content": "Ðấng khiến nước từ một vầng đá tuôn ra thành ao hồ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.278Z",
      "updatedAt": "2024-09-09T04:50:27.278Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    },
    {
      "id": "6a8f1778-7525-4956-b2b8-98d36a044f30",
      "number": 8,
      "content": "Từ tảng đá hoa cương, dòng nước ngọt lai láng trào ra.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:27.283Z",
      "updatedAt": "2024-09-09T04:50:27.283Z",
      "chapterId": "333cb57e-ae3f-458f-9d64-b5e22ee4a082"
    }
  ],
