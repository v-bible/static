{
  "chapter": {
    "id": "fe4790ab-f755-4948-874a-a43ce6a0a27a",
    "number": 99,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2099&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.669Z",
    "updatedAt": "2024-07-24T15:07:48.577Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "d4860a01-8943-4d9d-8dcb-37a7583f9922",
      "number": 1,
      "content": "Chúa trị vì, muôn dân hãy run sợ!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:31.987Z",
      "updatedAt": "2024-07-31T11:45:01.417Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "d84a9894-2585-4b5b-95ce-5f17118efa93",
      "number": 1,
      "content": "Ngài ngự giữa các chê-ru-bim, trái đất khá rung rinh.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:31.987Z",
      "updatedAt": "2024-07-31T11:45:01.432Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "d758ad7b-bf27-41f1-b229-640b02f21023",
      "number": 2,
      "content": "Chúa thật vĩ đại ở Si-ôn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:31.997Z",
      "updatedAt": "2024-07-31T11:45:01.443Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "5a1655fa-251d-4da4-887d-26e5206d2065",
      "number": 2,
      "content": "Ngài được tôn cao hơn tất cả các dân các nước.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:31.997Z",
      "updatedAt": "2024-07-31T11:45:01.450Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "64130d55-5996-4fd9-9143-25b994b8eeda",
      "number": 3,
      "content": "Nguyện họ ca tụng danh lớn lao và đáng sợ của Ngài.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32Z",
      "updatedAt": "2024-07-31T11:45:01.459Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "3f3453b9-99c8-481b-b276-ab6d07b6d571",
      "number": 3,
      "content": "Ngài là Thánh!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32Z",
      "updatedAt": "2024-07-31T11:45:01.466Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "b77d981a-9858-498b-91fe-de6d17af21cb",
      "number": 4,
      "content": "Lạy Vua quyền năng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.002Z",
      "updatedAt": "2024-07-31T11:45:01.477Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "4cded5eb-afa1-4268-9983-0866c783630b",
      "number": 4,
      "content": "Ðấng yêu công lý;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.002Z",
      "updatedAt": "2024-07-31T11:45:01.485Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "79369fa5-77c9-431a-af74-98d117bfc9e9",
      "number": 4,
      "content": "Ngài lập sự công bằng;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.002Z",
      "updatedAt": "2024-07-31T11:45:01.494Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "7ec5d73b-8692-402b-8b69-cd0cc964db41",
      "number": 4,
      "content": "Ngài thi hành công lý và công chính trong Gia-cốp.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.002Z",
      "updatedAt": "2024-07-31T11:45:01.502Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "d0e282f6-0af3-40e8-a0b0-c3829cdcb242",
      "number": 5,
      "content": "Hãy tôn cao Chúa, Ðức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.005Z",
      "updatedAt": "2024-07-31T11:45:01.512Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "90738f00-9d3e-4378-8d43-90ec31bdd1be",
      "number": 5,
      "content": "Hãy thờ lạy nơi bệ chân Ngài;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.005Z",
      "updatedAt": "2024-07-31T11:45:01.520Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "98ff3529-03c4-417f-81f6-a61f3d2805cb",
      "number": 5,
      "content": "Ngài là Thánh!",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.005Z",
      "updatedAt": "2024-07-31T11:45:01.528Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "e7667047-9c2d-4a5c-ba8e-d78e382ef42e",
      "number": 6,
      "content": "Môi-se và A-rôn là những người trong vòng các tư tế;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.007Z",
      "updatedAt": "2024-07-31T11:45:01.538Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "1b86c0ef-a845-4a96-89f4-ab82fa78033b",
      "number": 6,
      "content": "Sa-mu-ên ở trong số những người cầu khẩn danh Ngài;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.007Z",
      "updatedAt": "2024-07-31T11:45:01.546Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "916b6323-bdc7-4cf9-8b05-4e965a8f8785",
      "number": 6,
      "content": "Họ kêu cầu Chúa, và Ngài đã đáp lời họ.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.007Z",
      "updatedAt": "2024-07-31T11:45:01.571Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "8337471a-398c-4ded-a5b1-ad51a9c68c90",
      "number": 7,
      "content": "Từ trong đám mây, Ngài phán với họ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.010Z",
      "updatedAt": "2024-07-31T11:45:01.596Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "bf945671-fbd9-4228-b316-6d421b008d26",
      "number": 7,
      "content": "Họ tuân giữ các mạng lịnh Ngài và vâng theo các luật lệ Ngài ban cho họ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.010Z",
      "updatedAt": "2024-07-31T11:45:01.619Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "1a04b030-781b-4608-a319-53715cd2eaa9",
      "number": 8,
      "content": "Lạy Chúa, Ðức Chúa Trời chúng con, Ngài đã đáp lời họ;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.012Z",
      "updatedAt": "2024-07-31T11:45:01.652Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "61724b1b-00e9-44d9-867c-5c06866f4425",
      "number": 8,
      "content": "Khi đối xử với họ Ngài thật là Ðức Chúa Trời hay dung thứ,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.012Z",
      "updatedAt": "2024-07-31T11:45:01.662Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "24313ff3-cb2a-4bc4-8a8b-7de1f09cc6e2",
      "number": 8,
      "content": "Dẫu Ngài có sửa phạt họ vì những sai phạm của họ.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.012Z",
      "updatedAt": "2024-07-31T11:45:01.670Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "2b36d5bf-3927-4d91-a764-f990f796d604",
      "number": 9,
      "content": "Hãy tôn cao Chúa, Ðức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.014Z",
      "updatedAt": "2024-07-31T11:45:01.680Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "12ce4a42-ba51-4e4e-9c1e-c8958040e7ec",
      "number": 9,
      "content": "Hãy thờ phượng Ngài trên núi thánh của Ngài,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.014Z",
      "updatedAt": "2024-07-31T11:45:01.688Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    },
    {
      "id": "c94ace55-27ff-4d88-b000-50908b38b132",
      "number": 9,
      "content": "Vì Chúa, Ðức Chúa Trời chúng ta, là thánh.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:04:32.014Z",
      "updatedAt": "2024-07-31T11:45:01.696Z",
      "chapterId": "fe4790ab-f755-4948-874a-a43ce6a0a27a"
    }
  ],
