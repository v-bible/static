{
  "chapter": {
    "id": "45bc32c3-9f94-4b4a-b634-aae29565a99e",
    "number": 130,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20130&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.336Z",
    "updatedAt": "2024-07-24T15:07:49.215Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "de4655a7-7dfc-47eb-b608-5ae0a45fde2f",
      "number": 1,
      "content": "Lạy Chúa, từ chốn vực sâu con kêu cầu Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.723Z",
      "updatedAt": "2024-07-31T11:52:15.256Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "b2a2bb6c-23cb-45b4-9ac5-72bd66aedcd3",
      "number": 2,
      "content": "Chúa ôi, xin nghe tiếng cầu xin của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.733Z",
      "updatedAt": "2024-07-31T11:52:15.264Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "7459e757-fe5c-4dc2-8039-fb9ed7d3f38d",
      "number": 2,
      "content": "Xin lắng tai nghe lời cầu nguyện của con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.733Z",
      "updatedAt": "2024-07-31T11:52:15.271Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "ef87c6d5-a859-4d98-a52d-42f09e691240",
      "number": 3,
      "content": "Lạy Chúa, nếu Ngài ghi nhớ mọi tội lỗi của chúng con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.738Z",
      "updatedAt": "2024-07-31T11:52:15.281Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "9a9f83b3-9181-48bc-9dad-c64a53cd4ebb",
      "number": 3,
      "content": "Chúa ôi, còn ai đứng nổi trước mặt Ngài?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.738Z",
      "updatedAt": "2024-07-31T11:52:15.288Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "6baf2891-0e31-471a-8e03-552e9d445044",
      "number": 4,
      "content": "Nhưng vì Ngài sẵn lòng ban ơn tha thứ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.741Z",
      "updatedAt": "2024-07-31T11:52:15.295Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "5fc3fa86-6adb-4a78-b9b4-72b708f2871d",
      "number": 4,
      "content": "Nên chúng con càng kính sợ Ngài hơn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.741Z",
      "updatedAt": "2024-07-31T11:52:15.303Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "7f7d599f-8b5f-4661-9081-cec0b5eef64c",
      "number": 5,
      "content": "Tôi trông đợi Chúa, linh hồn tôi trông đợi Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.745Z",
      "updatedAt": "2024-07-31T11:52:15.313Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "a7292eab-701a-4ffa-8fa7-1cf5323af413",
      "number": 5,
      "content": "Tôi đặt trọn hy vọng nơi lời Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.745Z",
      "updatedAt": "2024-07-31T11:52:15.320Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "d15868c9-4a3b-4191-baba-bb7e505de25c",
      "number": 6,
      "content": "Linh hồn tôi trông đợi Chúa hơn người lính canh trông đợi sáng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.749Z",
      "updatedAt": "2024-07-31T11:52:15.328Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "2d22f9c2-9698-4661-841b-876b214f7d40",
      "number": 6,
      "content": "Thật hơn người lính canh trông đợi sáng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.749Z",
      "updatedAt": "2024-07-31T11:52:15.336Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "85bd7ae0-f1ac-4f6a-bc74-3110582857db",
      "number": 7,
      "content": "Hỡi I-sơ-ra-ên, hãy đặt trọn hy vọng nơi Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.753Z",
      "updatedAt": "2024-07-31T11:52:15.345Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "d8a6101a-3981-428b-9df8-0e0f28ba55ab",
      "number": 7,
      "content": "Vì nơi Chúa có tình thương;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.753Z",
      "updatedAt": "2024-07-31T11:52:15.354Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "c2c90483-d1b6-45d4-843f-51a3f7ff6ac2",
      "number": 7,
      "content": "Ngài ban ơn cứu chuộc cách dồi dào.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.753Z",
      "updatedAt": "2024-07-31T11:52:15.361Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    },
    {
      "id": "5351b1aa-ab2e-4bc7-bd63-dccdade54641",
      "number": 8,
      "content": "Ngài sẽ cứu chuộc I-sơ-ra-ên khỏi mọi tội lỗi của họ.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:11:03.757Z",
      "updatedAt": "2024-07-31T11:52:15.369Z",
      "chapterId": "45bc32c3-9f94-4b4a-b634-aae29565a99e"
    }
  ],
