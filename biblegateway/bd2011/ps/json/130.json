{
  "chapter": {
    "id": "2f6615b6-785e-41ce-8799-2ff015c6ac99",
    "number": 130,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20130&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.019Z",
    "updatedAt": "2024-09-24T01:00:45.019Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "4075516e-ae06-4eeb-956f-9e8ee4707725",
      "number": 1,
      "content": "Lạy Chúa, từ chốn vực sâu con kêu cầu Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.492Z",
      "updatedAt": "2024-09-24T02:11:14.492Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "04d68eba-6ab0-4306-88c2-b9e88a24bf54",
      "number": 2,
      "content": "Chúa ôi, xin nghe tiếng cầu xin của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.504Z",
      "updatedAt": "2024-09-24T02:11:14.504Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "fb811701-3eac-41c1-ac27-ddd98c5b3bb2",
      "number": 2,
      "content": "Xin lắng tai nghe lời cầu nguyện của con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.507Z",
      "updatedAt": "2024-09-24T02:11:14.507Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "676b046b-7dc8-493f-951a-6578a7f493d9",
      "number": 3,
      "content": "Lạy Chúa, nếu Ngài ghi nhớ mọi tội lỗi của chúng con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.509Z",
      "updatedAt": "2024-09-24T02:11:14.509Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "96a7c7c6-f2bf-4aaf-8839-3132bde60a90",
      "number": 3,
      "content": "Chúa ôi, còn ai đứng nổi trước mặt Ngài?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.512Z",
      "updatedAt": "2024-09-24T02:11:14.512Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "39a5def2-7e06-40d2-853f-c3d76389e2b2",
      "number": 4,
      "content": "Nhưng vì Ngài sẵn lòng ban ơn tha thứ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.514Z",
      "updatedAt": "2024-09-24T02:11:14.514Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "826bbc2b-1212-4f65-b1ef-7fa76585c032",
      "number": 4,
      "content": "Nên chúng con càng kính sợ Ngài hơn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.516Z",
      "updatedAt": "2024-09-24T02:11:14.516Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "847dd686-3c17-4ab4-975f-8cbfd1713e51",
      "number": 5,
      "content": "Tôi trông đợi Chúa, linh hồn tôi trông đợi Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.519Z",
      "updatedAt": "2024-09-24T02:11:14.519Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "626ad0d0-59a9-437d-95a3-31ee7a42c966",
      "number": 5,
      "content": "Tôi đặt trọn hy vọng nơi lời Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.521Z",
      "updatedAt": "2024-09-24T02:11:14.521Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "cb587ada-30ce-4fa7-a3a5-af545ae4e3dc",
      "number": 6,
      "content": "Linh hồn tôi trông đợi Chúa hơn người lính canh trông đợi sáng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.524Z",
      "updatedAt": "2024-09-24T02:11:14.524Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "5321ea1e-24e3-4130-8fef-932b4892045b",
      "number": 6,
      "content": "Thật hơn người lính canh trông đợi sáng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.526Z",
      "updatedAt": "2024-09-24T02:11:14.526Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "eaffb1a7-5a32-4f21-b856-2d031d61b528",
      "number": 7,
      "content": "Hỡi I-sơ-ra-ên, hãy đặt trọn hy vọng nơi Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.528Z",
      "updatedAt": "2024-09-24T02:11:14.528Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "f9d8cd99-dc99-4545-807a-740a86d31558",
      "number": 7,
      "content": "Vì nơi Chúa có tình thương;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.531Z",
      "updatedAt": "2024-09-24T02:11:14.531Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "4ae0560b-491e-4775-86fc-0d5ed2613b11",
      "number": 7,
      "content": "Ngài ban ơn cứu chuộc cách dồi dào.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.533Z",
      "updatedAt": "2024-09-24T02:11:14.533Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    },
    {
      "id": "f2fb8652-b538-4d3b-9d3b-bf82dafd9c8a",
      "number": 8,
      "content": "Ngài sẽ cứu chuộc I-sơ-ra-ên khỏi mọi tội lỗi của họ.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:14.536Z",
      "updatedAt": "2024-09-24T02:11:14.536Z",
      "chapterId": "2f6615b6-785e-41ce-8799-2ff015c6ac99"
    }
  ],
