{
  "chapter": {
    "id": "e22b57a4-9389-438a-9d71-31f102158a9f",
    "number": 130,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20130&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.822Z",
    "updatedAt": "2024-09-09T04:13:01.822Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "43700c6b-4803-4b83-b3c3-26927ea2d5c2",
      "number": 1,
      "content": "Lạy Chúa, từ chốn vực sâu con kêu cầu Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.538Z",
      "updatedAt": "2024-09-09T04:51:46.538Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "370e704f-df94-439f-9677-51828071f8ec",
      "number": 2,
      "content": "Chúa ôi, xin nghe tiếng cầu xin của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.549Z",
      "updatedAt": "2024-09-09T04:51:46.549Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "5b7cfc80-bbbf-401f-90e2-3c8c419b5875",
      "number": 2,
      "content": "Xin lắng tai nghe lời cầu nguyện của con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.551Z",
      "updatedAt": "2024-09-09T04:51:46.551Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "e5bba6a4-81ef-4b39-b20f-c93d175ce397",
      "number": 3,
      "content": "Lạy Chúa, nếu Ngài ghi nhớ mọi tội lỗi của chúng con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.553Z",
      "updatedAt": "2024-09-09T04:51:46.553Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "1937b9e6-18b6-4a9b-a592-9461f9060c34",
      "number": 3,
      "content": "Chúa ôi, còn ai đứng nổi trước mặt Ngài?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.556Z",
      "updatedAt": "2024-09-09T04:51:46.556Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "fa3def79-d776-4f58-b211-7cbc87c9493a",
      "number": 4,
      "content": "Nhưng vì Ngài sẵn lòng ban ơn tha thứ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.558Z",
      "updatedAt": "2024-09-09T04:51:46.558Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "1ddcfb32-7740-4104-b80a-fe0acccd08d3",
      "number": 4,
      "content": "Nên chúng con càng kính sợ Ngài hơn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.560Z",
      "updatedAt": "2024-09-09T04:51:46.560Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "5053dcc8-319d-4ca4-826c-8a49b6442fe3",
      "number": 5,
      "content": "Tôi trông đợi Chúa, linh hồn tôi trông đợi Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.562Z",
      "updatedAt": "2024-09-09T04:51:46.562Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "24f77cb0-cc8b-4bbd-91ba-a5edb96545e1",
      "number": 5,
      "content": "Tôi đặt trọn hy vọng nơi lời Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.564Z",
      "updatedAt": "2024-09-09T04:51:46.564Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "6a3315db-90a7-4526-94c2-50990be1cb56",
      "number": 6,
      "content": "Linh hồn tôi trông đợi Chúa hơn người lính canh trông đợi sáng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.567Z",
      "updatedAt": "2024-09-09T04:51:46.567Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "f0cc4172-83b7-48c3-a505-5e7356fcaf5a",
      "number": 6,
      "content": "Thật hơn người lính canh trông đợi sáng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.569Z",
      "updatedAt": "2024-09-09T04:51:46.569Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "d26fc7c7-88d2-4aca-86dd-1f5e1e531e9d",
      "number": 7,
      "content": "Hỡi I-sơ-ra-ên, hãy đặt trọn hy vọng nơi Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.572Z",
      "updatedAt": "2024-09-09T04:51:46.572Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "065d4744-d5d2-4b20-b67c-e2bc57ff4622",
      "number": 7,
      "content": "Vì nơi Chúa có tình thương;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.574Z",
      "updatedAt": "2024-09-09T04:51:46.574Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "2f9177d4-8f00-4e40-adeb-c6852cf0c0d6",
      "number": 7,
      "content": "Ngài ban ơn cứu chuộc cách dồi dào.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.577Z",
      "updatedAt": "2024-09-09T04:51:46.577Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    },
    {
      "id": "7607dfdd-33ee-4eaf-9238-7a88ecd76703",
      "number": 8,
      "content": "Ngài sẽ cứu chuộc I-sơ-ra-ên khỏi mọi tội lỗi của họ.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:46.580Z",
      "updatedAt": "2024-09-09T04:51:46.580Z",
      "chapterId": "e22b57a4-9389-438a-9d71-31f102158a9f"
    }
  ],
