{
  "chapter": {
    "id": "8ee60902-a472-48d5-912f-2d194d04ffdf",
    "number": 61,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2061&version=BD2011",
    "createdAt": "2024-09-09T04:13:00.406Z",
    "updatedAt": "2024-09-09T04:13:00.406Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "214b2616-c236-4d44-8a69-27bc2d704b1c",
      "number": 1,
      "content": "Ðức Chúa Trời ôi, xin nghe tiếng kêu cầu của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.106Z",
      "updatedAt": "2024-09-09T04:46:05.106Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "b24be3a6-9493-4598-ba3c-dc141fa5aa26",
      "number": 1,
      "content": "Xin lắng nghe lời cầu nguyện của con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.117Z",
      "updatedAt": "2024-09-09T04:46:05.117Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "f07121c0-068d-4271-a6a4-17d326d6ae24",
      "number": 2,
      "content": "Từ nơi xa xôi ở tận cùng trái đất con kêu cầu Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.119Z",
      "updatedAt": "2024-09-09T04:46:05.119Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "72c8cb4d-c627-47e2-ba15-6ce88a015648",
      "number": 2,
      "content": "Lòng con bị choáng ngợp vì biết bao nỗi lo sợ,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.121Z",
      "updatedAt": "2024-09-09T04:46:05.121Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "77a54651-3fad-4246-b948-96a34a774097",
      "number": 2,
      "content": "Xin dẫn đưa con đến một vầng đá cao lớn hơn con;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.123Z",
      "updatedAt": "2024-09-09T04:46:05.123Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "b04f3106-4023-417b-87d5-af4c2e94495f",
      "number": 3,
      "content": "Vì Ngài là nơi ẩn náu của con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.125Z",
      "updatedAt": "2024-09-09T04:46:05.125Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "dd8fc708-3382-4aa3-80d9-7181d2e568f5",
      "number": 3,
      "content": "Một pháo tháp vững chắc để con đối phó với quân thù.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.127Z",
      "updatedAt": "2024-09-09T04:46:05.127Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "9e34fa34-aa29-4d78-854f-d0e662c1b413",
      "number": 4,
      "content": "Xin cho con ở trong nhà Ngài mãi mãi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.129Z",
      "updatedAt": "2024-09-09T04:46:05.129Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "e0e40128-7ecd-4488-a067-e0d6ecf37854",
      "number": 4,
      "content": "Xin cho con nương náu dưới sự che chở của bóng cánh Ngài. (Sê-la)",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.130Z",
      "updatedAt": "2024-09-09T04:46:05.130Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "8797e786-24b1-444c-b3cd-3bae6bf075d0",
      "number": 5,
      "content": "Ðức Chúa Trời ôi, vì Ngài đã nghe lời hứa nguyện của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.132Z",
      "updatedAt": "2024-09-09T04:46:05.132Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "c7e270f6-8337-477b-8918-108e07d6502f",
      "number": 5,
      "content": "Ngài ban cho con cơ nghiệp dành cho những người kính sợ danh Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.134Z",
      "updatedAt": "2024-09-09T04:46:05.134Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "47208dee-8cfa-4feb-8d4e-3d360163da3b",
      "number": 6,
      "content": "Cầu xin Ngài gia tăng tuổi thọ cho vua;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.136Z",
      "updatedAt": "2024-09-09T04:46:05.136Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "4e176bed-6618-4bb3-805d-d7decc4214db",
      "number": 6,
      "content": "Xin cho những năm của đời người kéo dài qua nhiều thế hệ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.138Z",
      "updatedAt": "2024-09-09T04:46:05.138Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "7862abef-459d-4474-a4b1-67620c9b7c04",
      "number": 7,
      "content": "Nguyện người trị vì trước mặt Ðức Chúa Trời mãi mãi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.140Z",
      "updatedAt": "2024-09-09T04:46:05.140Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "65bc9ddd-e90c-4a93-82d1-015ad41aad23",
      "number": 7,
      "content": "Cầu xin Ngài ban tình thương và đức thành tín bảo vệ người.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.142Z",
      "updatedAt": "2024-09-09T04:46:05.142Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "c5423be9-76eb-4405-a0b7-856d2d836125",
      "number": 8,
      "content": "Như vậy con sẽ ca ngợi danh Ngài mãi mãi;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.145Z",
      "updatedAt": "2024-09-09T04:46:05.145Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    },
    {
      "id": "df1c32a4-27b2-48c4-8d23-bc9201942dfc",
      "number": 8,
      "content": "Con sẽ giữ trọn những gì con đã hứa nguyện mỗi ngày.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:05.150Z",
      "updatedAt": "2024-09-09T04:46:05.150Z",
      "chapterId": "8ee60902-a472-48d5-912f-2d194d04ffdf"
    }
  ],
