    },
    {
      "id": "9a747ac1-cd49-4db0-8ab8-df76a77d9cf9",
      "number": 19,
      "content": "Luật lệ Ngài và mạng lịnh Ngài cho I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.203Z",
      "updatedAt": "2024-07-31T11:55:44.455Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "b5704433-a1ae-4263-9354-1c9c1e89a478",
      "number": 20,
      "content": "Ngài chẳng làm như vậy cho một dân nào khác;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.205Z",
      "updatedAt": "2024-07-31T11:55:44.464Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "8ee6b4fa-26a4-4ac8-b5e7-bcb2230d929b",
      "number": 20,
      "content": "Họ chẳng biết gì về các mạng lịnh của Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.205Z",
      "updatedAt": "2024-07-31T11:55:44.474Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "a64c56d0-be37-4f1b-891e-8e05998e01f3",
      "number": 20,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.205Z",
      "updatedAt": "2024-07-31T11:55:44.482Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    }
  ],
