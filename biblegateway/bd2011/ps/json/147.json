{
  "chapter": {
    "id": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45",
    "number": 147,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20147&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.732Z",
    "updatedAt": "2024-07-24T15:07:49.562Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "e9f3666e-d4da-482b-8c6b-f44b57344d01",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.122Z",
      "updatedAt": "2024-07-31T11:55:43.904Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "ddc4abbf-a157-4709-961f-4199b3515f7a",
      "number": 1,
      "content": "Ca hát chúc tụng Ðức Chúa Trời chúng ta là điều tốt đẹp;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.122Z",
      "updatedAt": "2024-07-31T11:55:43.931Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "49da4524-cac7-4855-b9df-ddb4688dc69f",
      "number": 1,
      "content": "Vì tôn ngợi Ngài là điều vui thỏa và hợp lý dường nào!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.122Z",
      "updatedAt": "2024-07-31T11:55:43.952Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "21c985ed-0890-419f-a57b-f9ed89464b24",
      "number": 2,
      "content": "Chúa xây dựng lại Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.132Z",
      "updatedAt": "2024-07-31T11:55:43.979Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "cc2e47a0-686e-4122-8f0f-186423670386",
      "number": 2,
      "content": "Ngài gom nhóm những người bị tản lạc của I-sơ-ra-ên trở về.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.132Z",
      "updatedAt": "2024-07-31T11:55:43.996Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "cd271111-73a5-40a5-9f3c-2b59a14a19b7",
      "number": 3,
      "content": "Ngài rịt lành tâm hồn tan vỡ;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.135Z",
      "updatedAt": "2024-07-31T11:55:44.005Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "58263e87-cf42-4a0b-a170-d591cad25747",
      "number": 3,
      "content": "Và băng bó các vết thương của họ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.135Z",
      "updatedAt": "2024-07-31T11:55:44.013Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "8ed75737-7ec9-4571-a613-a38e1499e0ea",
      "number": 4,
      "content": "Ngài định số lượng các vì sao;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.141Z",
      "updatedAt": "2024-07-31T11:55:44.021Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "a8956f7f-bf4d-4cb6-911c-dcd1641e7368",
      "number": 4,
      "content": "Ngài gọi chúng theo tên riêng của từng vì sao một.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.141Z",
      "updatedAt": "2024-07-31T11:55:44.030Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "2c685137-64b5-415b-9a62-93b5e1f4eca0",
      "number": 5,
      "content": "Chúa chúng ta vĩ đại biết bao!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.147Z",
      "updatedAt": "2024-07-31T11:55:44.038Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "166d686d-e4f4-44f9-a139-7acd792057d4",
      "number": 5,
      "content": "Quyền năng Ngài thật là vô hạn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.147Z",
      "updatedAt": "2024-07-31T11:55:44.047Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "fda8c88d-e253-4c43-a104-14d3b7177f80",
      "number": 5,
      "content": "Kiến thức Ngài quả thật vô cùng.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.147Z",
      "updatedAt": "2024-07-31T11:55:44.055Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "fc6eb139-8193-4f44-9bda-f8e920591ea4",
      "number": 6,
      "content": "Chúa nhấc đưa những người nghèo khó lên cao,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.154Z",
      "updatedAt": "2024-07-31T11:55:44.063Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "86e58192-bac7-49e3-8909-86078ec03d74",
      "number": 6,
      "content": "Nhưng Ngài đánh đổ những kẻ gian ác ngã nhào xuống đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.154Z",
      "updatedAt": "2024-07-31T11:55:44.073Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "b217191c-c430-4c99-b40b-96c2075897e6",
      "number": 7,
      "content": "Hãy dâng lên Chúa những lời cảm tạ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.160Z",
      "updatedAt": "2024-07-31T11:55:44.083Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "2b6a36f4-8ce2-4031-95be-9f8d7091cbf6",
      "number": 7,
      "content": "Hãy dùng nhạc khí ca ngợi Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.160Z",
      "updatedAt": "2024-07-31T11:55:44.091Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "2cb4ed78-f39b-4f92-80f1-ad8b153be78e",
      "number": 8,
      "content": "Ngài bao phủ các tầng trời bằng mây;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.166Z",
      "updatedAt": "2024-07-31T11:55:44.100Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "f6229535-7c09-4325-9b3e-95f593a94a33",
      "number": 8,
      "content": "Ngài chuẩn bị các trận mưa cho đất;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.166Z",
      "updatedAt": "2024-07-31T11:55:44.224Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "bd64fa85-8937-4eb3-923e-0bb83ddb78af",
      "number": 8,
      "content": "Ngài khiến cỏ mọc trên các núi đồi.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.166Z",
      "updatedAt": "2024-07-31T11:55:44.251Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "75144a9b-3719-48af-8cc9-0e2c55af40a1",
      "number": 9,
      "content": "Ngài ban thực phẩm cho muôn loài cầm thú;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.171Z",
      "updatedAt": "2024-07-31T11:55:44.264Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "921ca49c-5c72-4043-b77d-994a67f2850d",
      "number": 9,
      "content": "Ngài ban thức ăn cho các quạ con đói bụng kêu gào.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.171Z",
      "updatedAt": "2024-07-31T11:55:44.277Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "936b82c4-d2b2-4996-93d2-cfffb893de15",
      "number": 10,
      "content": "Ngài chẳng hài lòng về sức lực của ngựa chiến;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.176Z",
      "updatedAt": "2024-07-31T11:55:44.286Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "2c7c0548-3bdd-48ab-9704-fd1734dbd6d5",
      "number": 10,
      "content": "Ngài không vui về đôi chân nhanh nhẹn của loài người;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.176Z",
      "updatedAt": "2024-07-31T11:55:44.295Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "fab43276-2822-4ab1-8d30-6b5d2571736a",
      "number": 11,
      "content": "Nhưng Chúa rất vui lòng về những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.180Z",
      "updatedAt": "2024-07-31T11:55:44.303Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "984b75b3-6748-4907-b383-2ef53115ee12",
      "number": 11,
      "content": "Những người đặt hy vọng vào tình thương của Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.180Z",
      "updatedAt": "2024-07-31T11:55:44.311Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "0acf2ebf-8db4-4369-9a0e-cf108c7916a7",
      "number": 12,
      "content": "Hỡi Giê-ru-sa-lem, hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.183Z",
      "updatedAt": "2024-07-31T11:55:44.322Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "efcbdfc3-25f9-4545-84d7-b91958eafd16",
      "number": 12,
      "content": "Hỡi Si-ôn, hãy tôn ngợi Ðức Chúa Trời!",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.183Z",
      "updatedAt": "2024-07-31T11:55:44.330Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "505d7fe0-cf4c-4643-9988-1867126f2962",
      "number": 13,
      "content": "Vì Ngài làm vững mạnh các song sắt ở các cổng thành của ngươi;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.186Z",
      "updatedAt": "2024-07-31T11:55:44.338Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "3c60a5b8-2be4-41f2-a60e-77af69e86895",
      "number": 13,
      "content": "Ngài ban phước cho con dân ngươi sống trong ngươi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.186Z",
      "updatedAt": "2024-07-31T11:55:44.349Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "91ba8ba9-26d3-4725-98d2-a7ab00055f9c",
      "number": 14,
      "content": "Ngài ban bình an nơi các biên giới ngươi;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.189Z",
      "updatedAt": "2024-07-31T11:55:44.358Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "ed9625dc-aaed-4957-8e13-9d0df3432d0f",
      "number": 14,
      "content": "Ngài cho ngươi no thỏa bằng gạo ngon hảo hạng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.189Z",
      "updatedAt": "2024-07-31T11:55:44.367Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "d5e4b02e-cdde-4ca0-b673-6aee86394fe6",
      "number": 15,
      "content": "Ngài truyền lịnh của Ngài cho trái đất;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.192Z",
      "updatedAt": "2024-07-31T11:55:44.377Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "ea6d6373-6c3e-4362-9b1c-35d40b9f7f88",
      "number": 15,
      "content": "Lời Ngài phán ra được cấp tốc truyền đi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.192Z",
      "updatedAt": "2024-07-31T11:55:44.386Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "b69e817b-8c52-45b1-acbb-1a1072a80bac",
      "number": 16,
      "content": "Ngài ban tuyết xuống phủ trắng như lông chiên;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.195Z",
      "updatedAt": "2024-07-31T11:55:44.394Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "c455afa5-04f7-447a-b17f-b7ddf8153fad",
      "number": 16,
      "content": "Ngài rải sương giá ra như tro bụi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.195Z",
      "updatedAt": "2024-07-31T11:55:44.402Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "b86475c2-2d8b-43e9-9b94-255f329e2d42",
      "number": 17,
      "content": "Ngài quăng băng giá ra từng mảng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.198Z",
      "updatedAt": "2024-07-31T11:55:44.410Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "3872bb60-1cdc-42b1-8d94-fc57580b3bcd",
      "number": 17,
      "content": "Ai có thể chịu nổi cơn buốt giá của Ngài?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.198Z",
      "updatedAt": "2024-07-31T11:55:44.419Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "889378a4-ff27-4c21-9d60-61d9069d01ff",
      "number": 18,
      "content": "Ngài ban lịnh, chúng liền tan chảy;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.200Z",
      "updatedAt": "2024-07-31T11:55:44.427Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "9f738330-f6ac-4da4-b8fb-f79d1be6fb70",
      "number": 18,
      "content": "Ngài khiến gió thổi qua, nước chảy xuống thành dòng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:32.200Z",
      "updatedAt": "2024-07-31T11:55:44.436Z",
      "chapterId": "97a2aa7e-cc26-43a6-bee3-0d00b6047a45"
    },
    {
      "id": "286e15a2-17b0-414f-aad4-b0fc6d004c6d",
      "number": 19,
      "content": "Ngài tuyên bố lời Ngài cho Gia-cốp,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
