{
  "chapter": {
    "id": "60817556-7f57-43f5-9c59-08d0a034929a",
    "number": 33,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2033&version=BD2011",
    "createdAt": "2024-07-12T04:03:46.234Z",
    "updatedAt": "2024-07-24T15:07:47.173Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "24886b0b-7d0a-44c5-892c-cb4112e76e70",
      "number": 1,
      "content": "Hỡi những người ngay lành, hãy vui vẻ ca ngợi Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.832Z",
      "updatedAt": "2024-07-31T11:27:38.433Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "f03858e7-a1b1-41ee-b1e8-3fa0a4fc8d56",
      "number": 1,
      "content": "Sự ca ngợi Ngài thật thích hợp với người ngay thật.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.832Z",
      "updatedAt": "2024-07-31T11:27:38.440Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "14d3e98a-ed60-4cd9-8d86-c6552e8f750c",
      "number": 2,
      "content": "Hãy dùng hạc cầm dâng lời cảm tạ Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.842Z",
      "updatedAt": "2024-07-31T11:27:38.449Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "e8f16965-01fc-486d-bb72-aa6f182391b6",
      "number": 2,
      "content": "Hãy dùng thập huyền cầm ca hát tôn vinh Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.842Z",
      "updatedAt": "2024-07-31T11:27:38.457Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "a942e774-e1f2-4908-b900-7f6c40a7b2e5",
      "number": 3,
      "content": "Hãy hát cho Ngài một bài ca mới;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.845Z",
      "updatedAt": "2024-07-31T11:27:38.465Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "6d81d726-f607-4c9d-b9be-1bc6675007fb",
      "number": 3,
      "content": "Khá đàn thật hay, hòa nhịp với tiếng reo mừng,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.845Z",
      "updatedAt": "2024-07-31T11:27:38.473Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "b8b83a32-d338-46f3-8dd1-c296552dfebc",
      "number": 4,
      "content": "Vì lời Chúa rất chân thật;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.847Z",
      "updatedAt": "2024-07-31T11:27:38.483Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "2acd0ea1-9ea9-4e49-9092-f782fd614fb6",
      "number": 4,
      "content": "Mọi việc Ngài làm đều thành tín.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.847Z",
      "updatedAt": "2024-07-31T11:27:38.492Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "661f2e20-4584-4d8b-835d-885485d79bdc",
      "number": 5,
      "content": "Ngài yêu mến công chính và công lý;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.850Z",
      "updatedAt": "2024-07-31T11:27:38.503Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "046a2453-ba75-47dc-9604-716d3bf149aa",
      "number": 5,
      "content": "Cả thế gian đầy dẫy tình thương của Chúa.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.850Z",
      "updatedAt": "2024-07-31T11:27:38.515Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "3ad42c22-4663-4662-9143-f339607566c0",
      "number": 6,
      "content": "Nhờ lời Chúa, các tầng trời được dựng nên;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.852Z",
      "updatedAt": "2024-07-31T11:27:38.525Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "e9d3d411-e6b6-4d1f-9062-596d52e1d880",
      "number": 6,
      "content": "Bởi hơi thở của miệng Ngài, các thiên thể liền hiện hữu.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.852Z",
      "updatedAt": "2024-07-31T11:27:38.534Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "feae8de1-ca5a-4a10-a7ba-f385fd81bf6c",
      "number": 7,
      "content": "Ngài gom nước biển lại thành một khối;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.855Z",
      "updatedAt": "2024-07-31T11:27:38.542Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "f9e17923-3a86-4c3d-880f-8b17ee29a628",
      "number": 7,
      "content": "Ngài chứa nước trong các vực sâu.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.855Z",
      "updatedAt": "2024-07-31T11:27:38.550Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "bdf51229-f04b-4546-b055-df62805a56f3",
      "number": 8,
      "content": "Hỡi cả thế gian, hãy kính sợ Chúa;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.857Z",
      "updatedAt": "2024-07-31T11:27:38.561Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "9bf98556-bf89-4c90-b9d0-9a0f263530ea",
      "number": 8,
      "content": "Hỡi các dân tộc trên thế gian, hãy khiếp sợ Ngài,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.857Z",
      "updatedAt": "2024-07-31T11:27:38.569Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "e574da1a-d2dc-4070-9248-2711eb1387a6",
      "number": 9,
      "content": "Vì khi Ngài phán, muôn vật liền hiện hữu;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.860Z",
      "updatedAt": "2024-07-31T11:27:38.577Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "d4b6fa26-7ba5-4e5e-af6b-9b6c9e895ec7",
      "number": 9,
      "content": "Ngài ra lịnh, tất cả được đứng vững vàng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.860Z",
      "updatedAt": "2024-07-31T11:27:38.585Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "de074faf-880c-4643-acf1-92ca094afe1f",
      "number": 10,
      "content": "Chúa làm bại mưu các nước;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.862Z",
      "updatedAt": "2024-07-31T11:27:38.596Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "4ea8fe21-4e67-419b-ac32-68cf0b78d92e",
      "number": 10,
      "content": "Ngài khiến kế hoạch của các dân ra hư không.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.862Z",
      "updatedAt": "2024-07-31T11:27:38.604Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "8c2b21f0-bbb2-4c80-bc74-6e9350758c3e",
      "number": 11,
      "content": "Còn ý định của Chúa cứ được vững lập đến đời đời;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.864Z",
      "updatedAt": "2024-07-31T11:27:38.612Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "9250fb4f-a028-4825-8bb7-aa743238bd33",
      "number": 11,
      "content": "Các ý chỉ của lòng Ngài còn từ đời nọ đến đời kia.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.864Z",
      "updatedAt": "2024-07-31T11:27:38.620Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "8f00b986-8885-4fa5-8cf9-b22f27ec8442",
      "number": 12,
      "content": "Phước cho nước nào có Chúa là Thần của mình,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.868Z",
      "updatedAt": "2024-07-31T11:27:38.628Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "1e54cbf0-e97a-446f-8b2e-34f88ac831a6",
      "number": 12,
      "content": "Phước cho dân nào được Ngài chọn làm cơ nghiệp Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.868Z",
      "updatedAt": "2024-07-31T11:27:38.637Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "5ccb9774-ad2b-4f23-ba1e-8756ca4af76c",
      "number": 13,
      "content": "Chúa từ trời nhìn xuống;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.878Z",
      "updatedAt": "2024-07-31T11:27:38.722Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "e00aed6f-62f6-47f0-ba8e-f6bf1b495712",
      "number": 13,
      "content": "Ngài thấy rõ mọi con cái loài người.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.878Z",
      "updatedAt": "2024-07-31T11:27:38.778Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "c8b587c3-e7cf-4c82-9af8-406f159be14d",
      "number": 14,
      "content": "Từ nơi Ngài ngự Ngài quan sát mọi người trên đất.",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.973Z",
      "updatedAt": "2024-07-31T11:27:38.789Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "3d2ecc40-04a3-4305-8cf1-fa8da4b9c8ce",
      "number": 15,
      "content": "Ngài dựng nên lòng dạ mọi người,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.987Z",
      "updatedAt": "2024-07-31T11:27:38.798Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "205a8cc7-7498-49b2-8d51-5f9aa1dad6fe",
      "number": 15,
      "content": "Nên Ngài hiểu rõ mọi việc họ làm.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.987Z",
      "updatedAt": "2024-07-31T11:27:38.810Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "c92b68e6-706c-4a04-adb2-c52675362ec2",
      "number": 16,
      "content": "Chẳng có vua nào được cứu nhờ quân đội mạnh;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.989Z",
      "updatedAt": "2024-07-31T11:27:38.827Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "95556921-8982-4ff7-9353-c44aeec0fa8b",
      "number": 16,
      "content": "Chẳng có chiến sĩ nào được cứu nhờ sức mạnh mình.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.989Z",
      "updatedAt": "2024-07-31T11:27:38.835Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "f69928e5-fd8e-4fc4-9ccb-5aab1306f32b",
      "number": 17,
      "content": "Nhờ vào ngựa để chiến thắng là hy vọng hão huyền;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.991Z",
      "updatedAt": "2024-07-31T11:27:38.845Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "a60357f3-850a-4d6f-8414-8c7ed489dc63",
      "number": 17,
      "content": "Dù ngựa có sức lực nó cũng chẳng cứu được ai.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.991Z",
      "updatedAt": "2024-07-31T11:27:38.854Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "1689051e-eba3-43d2-9eb0-58ebc7475aa7",
      "number": 18,
      "content": "Kìa, mắt Chúa nhìn xem những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.993Z",
      "updatedAt": "2024-07-31T11:27:38.866Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "b4cd3af3-be9c-4eb3-bb3c-edc5d6d36bd1",
      "number": 18,
      "content": "Những người đặt hy vọng vào lòng thương xót của Ngài,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.993Z",
      "updatedAt": "2024-07-31T11:27:38.875Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "772ce566-22a4-458c-aa97-d73bad5787e4",
      "number": 19,
      "content": "Ðể giải cứu linh hồn họ khỏi chết,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.995Z",
      "updatedAt": "2024-07-31T11:27:38.884Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "8df85ac6-3334-40d7-88dd-87fa7b4d0a44",
      "number": 19,
      "content": "Và bảo tồn mạng sống họ khi nạn đói xảy ra.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.995Z",
      "updatedAt": "2024-07-31T11:27:38.893Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "8c69e7bd-dfa3-4c06-9638-738872e8a332",
      "number": 20,
      "content": "Linh hồn chúng ta trông cậy Chúa;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.997Z",
      "updatedAt": "2024-07-31T11:27:38.904Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "15e6878f-efa5-457f-b818-426d97dbfbfb",
      "number": 20,
      "content": "Ngài là nguồn cứu giúp và thuẫn khiên của chúng ta.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:11.997Z",
      "updatedAt": "2024-07-31T11:27:38.913Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "57e21c6d-e257-4754-93e2-20f5e444ae43",
      "number": 21,
      "content": "Lòng chúng ta vui mừng trong Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:12Z",
      "updatedAt": "2024-07-31T11:27:38.929Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "9a074fe7-fba0-4156-bebc-5e6f7cb671ab",
      "number": 21,
      "content": "Bởi vì chúng ta tin cậy vào danh thánh Ngài.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:12Z",
      "updatedAt": "2024-07-31T11:27:38.938Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "3a5d815b-9277-4126-9a8c-e56fe8b567dd",
      "number": 22,
      "content": "Chúa ôi, nguyện tình thương của Ngài ngự trên chúng con,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:12.002Z",
      "updatedAt": "2024-07-31T11:27:38.947Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    },
    {
      "id": "9ba6a869-0ca6-4673-bdf9-89d293e5def3",
      "number": 22,
      "content": "Y như những gì chúng con hy vọng nơi Ngài.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:12.002Z",
      "updatedAt": "2024-07-31T11:27:38.958Z",
      "chapterId": "60817556-7f57-43f5-9c59-08d0a034929a"
    }
  ],
