{
  "chapter": {
    "id": "35795c5d-ce87-422a-91ef-ae9aa717c3fe",
    "number": 66,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2066&version=BD2011",
    "createdAt": "2024-07-12T04:03:46.952Z",
    "updatedAt": "2024-07-24T15:07:47.872Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "811bb031-0c2b-4374-8934-40a290c15ed7",
      "number": 1,
      "content": "Hỡi muôn loài trên đất,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.398Z",
      "updatedAt": "2024-07-31T11:36:45.883Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "81ae5034-4bf5-41cc-a13e-22530c2f0841",
      "number": 1,
      "content": "Hãy cất tiếng reo mừng dâng lên Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.398Z",
      "updatedAt": "2024-07-31T11:36:45.897Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "1b42eab4-9f50-4a79-9a88-72c472695dd7",
      "number": 2,
      "content": "Hãy ca ngợi danh vinh hiển của Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.404Z",
      "updatedAt": "2024-07-31T11:36:45.905Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "a3b3d164-b64f-444c-93ad-9fb34dba224b",
      "number": 2,
      "content": "Hãy tôn vinh ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.404Z",
      "updatedAt": "2024-07-31T11:36:45.912Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "d32137ac-88ec-44d1-93c2-353e095f7d92",
      "number": 3,
      "content": "Hãy thưa cùng Ðức Chúa Trời rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.407Z",
      "updatedAt": "2024-07-31T11:36:45.919Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "659f3374-f51e-4056-8fdc-89a0951ab269",
      "number": 3,
      "content": "“Những công việc của Ngài thật đáng kinh hãi thay!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.407Z",
      "updatedAt": "2024-07-31T11:36:45.927Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "9a3bf8b9-f0f6-4a0c-9ecd-cf448c501035",
      "number": 3,
      "content": "Bởi quyền năng lớn lao của Ngài,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.407Z",
      "updatedAt": "2024-07-31T11:36:45.935Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "b5919a0b-7f5c-471c-858b-df5abd33fe37",
      "number": 3,
      "content": "Những kẻ thù của Ngài phải phủ phục trước mặt Ngài.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.407Z",
      "updatedAt": "2024-07-31T11:36:45.943Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "148c5584-e5dd-4713-b688-327a8e47fdbc",
      "number": 4,
      "content": "Mọi loài trên đất sẽ thờ phượng Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.409Z",
      "updatedAt": "2024-07-31T11:36:45.950Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "891bbb5b-57c8-4f8c-bf91-3a57ee8842bc",
      "number": 4,
      "content": "Chúng sẽ hát ca tôn ngợi Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.409Z",
      "updatedAt": "2024-07-31T11:36:45.957Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "24586697-b4d1-4e1c-9333-a47884b6aa72",
      "number": 4,
      "content": "Chúng sẽ ca hát tôn vinh danh Ngài.” (Sê-la)",
      "order": 2,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.409Z",
      "updatedAt": "2024-07-31T11:36:45.965Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "1372f604-0321-4a32-96ca-823ac8762a81",
      "number": 5,
      "content": "Hãy đến và nhìn xem những công việc của Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.412Z",
      "updatedAt": "2024-07-31T11:36:45.975Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "8357a695-d24f-4f1d-94f7-8e2e15454221",
      "number": 5,
      "content": "Những gì Ngài làm cho loài người thật đáng kinh hãi thay!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.412Z",
      "updatedAt": "2024-07-31T11:36:45.983Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "b9e0b9aa-dace-4f08-b471-f3a05826cdeb",
      "number": 6,
      "content": "Ngài biến đại dương thành đất khô;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.418Z",
      "updatedAt": "2024-07-31T11:36:45.990Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "0ef482a1-e136-441f-b8f6-a2d673ab9213",
      "number": 6,
      "content": "Ngài cho họ đi bộ vượt qua dòng nước mạnh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.418Z",
      "updatedAt": "2024-07-31T11:36:45.998Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "a18b99fa-368e-41ca-a85e-39314d930dff",
      "number": 6,
      "content": "Hãy đến, chúng ta hãy vui mừng trong Ngài!",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.418Z",
      "updatedAt": "2024-07-31T11:36:46.006Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "dfa15856-b615-46c4-8f96-60c62e5b4b4d",
      "number": 7,
      "content": "Ngài dùng quyền năng của Ngài trị vì đời đời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.424Z",
      "updatedAt": "2024-07-31T11:36:46.014Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "3e4ab041-b23a-4727-9918-b256cd706ba4",
      "number": 7,
      "content": "Mắt Ngài nhìn xem các nước,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.424Z",
      "updatedAt": "2024-07-31T11:36:46.022Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "afe13d4c-c5fb-49f8-8847-e5d9709ca9d4",
      "number": 7,
      "content": "Kẻo những kẻ phản loạn nổi lên gây rối chăng. (Sê-la)",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.424Z",
      "updatedAt": "2024-07-31T11:36:46.030Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "f946c235-c9eb-404e-882f-ba39ea9e9b03",
      "number": 8,
      "content": "Hỡi các dân, hãy ca ngợi Ðức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.429Z",
      "updatedAt": "2024-07-31T11:36:46.039Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "f50911b1-dff9-44ad-beda-fa700ba39272",
      "number": 8,
      "content": "Hãy làm cho âm thanh ca ngợi Ngài vang ra khắp chốn,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.429Z",
      "updatedAt": "2024-07-31T11:36:46.047Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "71767ec9-e9f5-4a12-8fd2-858563ccff96",
      "number": 9,
      "content": "Vì Ngài giữ gìn mạng sống chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.435Z",
      "updatedAt": "2024-07-31T11:36:46.054Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "5daaa109-7d32-46fa-bb62-4bfaea3b8a46",
      "number": 9,
      "content": "Ngài không để cho chân chúng ta trợt ngã.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.435Z",
      "updatedAt": "2024-07-31T11:36:46.074Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "a3238ab3-11fd-4f74-af04-3f2c8f2ef8d2",
      "number": 10,
      "content": "Thật vậy Ðức Chúa Trời ôi, Ngài đã thử thách chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.440Z",
      "updatedAt": "2024-07-31T11:36:46.093Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "f3c01249-3d92-480c-b6bb-8d3997f8d862",
      "number": 10,
      "content": "Ngài đã rèn luyện chúng con như người ta luyện bạc.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.440Z",
      "updatedAt": "2024-07-31T11:36:46.101Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "ebbacb6b-de04-4ea9-b519-41d6db729158",
      "number": 11,
      "content": "Ngài đã để cho chúng con bị sa vào lưới;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.446Z",
      "updatedAt": "2024-07-31T11:36:46.109Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "4940654e-2057-4006-b4cb-aec79969a9c3",
      "number": 11,
      "content": "Ngài đã để gánh nặng bị áp bức đè nặng trên lưng chúng con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.446Z",
      "updatedAt": "2024-07-31T11:36:46.117Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "2057ea48-da6b-4bac-9471-9fc017735716",
      "number": 12,
      "content": "Ngài đã để người ta cỡi trên đầu trên cổ chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.451Z",
      "updatedAt": "2024-07-31T11:36:46.124Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "06aa86c6-e549-45af-9198-774c7214d889",
      "number": 12,
      "content": "Chúng con phải đi qua lửa và qua nước;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.451Z",
      "updatedAt": "2024-07-31T11:36:46.149Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "2e879db8-c192-4ee9-84cf-f0a49a959fa4",
      "number": 12,
      "content": "Nhưng rồi Ngài đã đem chúng con ra khỏi,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.451Z",
      "updatedAt": "2024-07-31T11:36:46.265Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "6230ea4d-81e1-4482-a6fb-1c58e43d5321",
      "number": 12,
      "content": "Ðể đưa chúng con vào một nơi trù phú.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.451Z",
      "updatedAt": "2024-07-31T11:36:46.281Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "7cc9ad25-bd7f-4a21-985a-64a012df7dfe",
      "number": 13,
      "content": "Con sẽ vào nhà Ngài với những của lễ thiêu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.457Z",
      "updatedAt": "2024-07-31T11:36:46.294Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "d287f95c-ac64-4542-bf68-027c7d68c587",
      "number": 13,
      "content": "Con sẽ trả xong cho Ngài những gì con đã hứa nguyện,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.457Z",
      "updatedAt": "2024-07-31T11:36:46.307Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "fe281ce1-eea0-4c84-a476-4f2f2b622ba0",
      "number": 14,
      "content": "Những gì môi con đã hứa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.462Z",
      "updatedAt": "2024-07-31T11:36:46.320Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "d6ae4775-b039-49b9-b242-31ab9bc0ed55",
      "number": 14,
      "content": "Và miệng con đã nói ra trong lúc bị khốn khổ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.462Z",
      "updatedAt": "2024-07-31T11:36:46.328Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "e996e824-3953-431f-902b-add32907bc92",
      "number": 15,
      "content": "Con sẽ dâng lên Ngài những con vật hiến tế béo tốt để làm của lễ thiêu, chung với mỡ của các chiên đực;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.468Z",
      "updatedAt": "2024-07-31T11:36:46.338Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "37770fcd-e812-422f-91b3-658e982385ee",
      "number": 15,
      "content": "Con sẽ dâng các bò đực và các dê đực. (Sê-la)",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.468Z",
      "updatedAt": "2024-07-31T11:36:46.347Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "a2685136-8d44-4d62-8b6d-a311dcca3ae0",
      "number": 16,
      "content": "Hỡi mọi người kính sợ Ðức Chúa Trời, xin đến nghe,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.474Z",
      "updatedAt": "2024-07-31T11:36:46.358Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "6a932bc2-516b-4edf-9224-1f42f7c737cd",
      "number": 16,
      "content": "Tôi sẽ thuật lại những gì Ngài đã làm cho tôi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.474Z",
      "updatedAt": "2024-07-31T11:36:46.368Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "9996550d-40af-4e8b-a5b6-9a585d7f04b7",
      "number": 17,
      "content": "Miệng tôi đã kêu cầu Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.479Z",
      "updatedAt": "2024-07-31T11:36:46.377Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "bf7603ea-2243-487a-a631-82e5540eb879",
      "number": 17,
      "content": "Và lưỡi tôi đã tôn vinh Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.479Z",
      "updatedAt": "2024-07-31T11:36:46.388Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "59075278-8e0c-4af3-a99a-50b9e56c23e0",
      "number": 18,
      "content": "Nếu lòng tôi chú về tội ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.485Z",
      "updatedAt": "2024-07-31T11:36:46.396Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "a596d0e6-d9d1-4b51-bb4a-afdb8e729c1a",
      "number": 18,
      "content": "Ắt Chúa chẳng nghe tôi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.485Z",
      "updatedAt": "2024-07-31T11:36:46.405Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "b41e2939-f156-4942-822c-8d26e7490907",
      "number": 19,
      "content": "Nhưng Ðức Chúa Trời thật có nghe;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.490Z",
      "updatedAt": "2024-07-31T11:36:46.413Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "c0f4bcfd-bac1-4ee7-b460-6ad9e8c713ab",
      "number": 19,
      "content": "Ngài đã lắng nghe tiếng cầu nguyện của tôi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.490Z",
      "updatedAt": "2024-07-31T11:36:46.422Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "a086b92b-91bf-4404-baf2-f819744e0ba3",
      "number": 20,
      "content": "Tôn ngợi Ðức Chúa Trời, Ðấng chẳng bỏ qua lời cầu nguyện của tôi,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.496Z",
      "updatedAt": "2024-07-31T11:36:46.460Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    },
    {
      "id": "0f3f36b9-4afb-4241-b394-c82696d30b92",
      "number": 20,
      "content": "Và không rút lại tình thương của Ngài đối với tôi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:32.496Z",
      "updatedAt": "2024-07-31T11:36:46.472Z",
      "chapterId": "35795c5d-ce87-422a-91ef-ae9aa717c3fe"
    }
  ],
