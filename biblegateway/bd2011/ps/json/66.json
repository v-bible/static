{
  "chapter": {
    "id": "b8171cde-8474-4b33-8b32-58298f5cc623",
    "number": 66,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2066&version=BD2011",
    "createdAt": "2024-09-09T04:13:00.514Z",
    "updatedAt": "2024-09-09T04:13:00.514Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "c390dc2d-fd2d-4114-97a9-f16e54d3ff22",
      "number": 1,
      "content": "Hỡi muôn loài trên đất,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.670Z",
      "updatedAt": "2024-09-09T04:46:28.670Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "c7914c0d-a74e-4c55-b3a1-da20880f6c7b",
      "number": 1,
      "content": "Hãy cất tiếng reo mừng dâng lên Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.692Z",
      "updatedAt": "2024-09-09T04:46:28.692Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "5e992d29-e6cb-41dd-8583-759fcb7d2b64",
      "number": 2,
      "content": "Hãy ca ngợi danh vinh hiển của Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.697Z",
      "updatedAt": "2024-09-09T04:46:28.697Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "814bd7f9-abbe-441d-8f4a-b749e809bae0",
      "number": 2,
      "content": "Hãy tôn vinh ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.702Z",
      "updatedAt": "2024-09-09T04:46:28.702Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "a75d64a4-6c26-4703-9320-201fc2326e2d",
      "number": 3,
      "content": "Hãy thưa cùng Ðức Chúa Trời rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.707Z",
      "updatedAt": "2024-09-09T04:46:28.707Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "a266c6a1-792c-410d-88a9-aa4c697273a1",
      "number": 3,
      "content": "“Những công việc của Ngài thật đáng kinh hãi thay!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.711Z",
      "updatedAt": "2024-09-09T04:46:28.711Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "1ff46af0-e815-4934-8b03-5869d2a24c24",
      "number": 3,
      "content": "Bởi quyền năng lớn lao của Ngài,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.716Z",
      "updatedAt": "2024-09-09T04:46:28.716Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "bf5916e1-e9ec-4e10-9849-2c8a315e1400",
      "number": 3,
      "content": "Những kẻ thù của Ngài phải phủ phục trước mặt Ngài.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.721Z",
      "updatedAt": "2024-09-09T04:46:28.721Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "b49acc4c-5090-4826-9045-e1328ba82b33",
      "number": 4,
      "content": "Mọi loài trên đất sẽ thờ phượng Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.727Z",
      "updatedAt": "2024-09-09T04:46:28.727Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "6bfd836c-7115-46a1-bd7b-11b0e3b25ba5",
      "number": 4,
      "content": "Chúng sẽ hát ca tôn ngợi Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.731Z",
      "updatedAt": "2024-09-09T04:46:28.731Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "c35503f6-e374-4139-80ab-a73f8b575b2d",
      "number": 4,
      "content": "Chúng sẽ ca hát tôn vinh danh Ngài.” (Sê-la)",
      "order": 2,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.736Z",
      "updatedAt": "2024-09-09T04:46:28.736Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "d34017a8-5fcc-4fa9-9b18-5a959ba8d755",
      "number": 5,
      "content": "Hãy đến và nhìn xem những công việc của Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.741Z",
      "updatedAt": "2024-09-09T04:46:28.741Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "2bb08f9b-7ea6-4c8a-979d-a274dbc4919e",
      "number": 5,
      "content": "Những gì Ngài làm cho loài người thật đáng kinh hãi thay!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.746Z",
      "updatedAt": "2024-09-09T04:46:28.746Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "460ee0b7-b4fe-4912-b930-5b9dd1af4886",
      "number": 6,
      "content": "Ngài biến đại dương thành đất khô;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.752Z",
      "updatedAt": "2024-09-09T04:46:28.752Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "85e13787-11bd-4d6e-ad45-c3d76b9ae272",
      "number": 6,
      "content": "Ngài cho họ đi bộ vượt qua dòng nước mạnh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.756Z",
      "updatedAt": "2024-09-09T04:46:28.756Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "d8f59ba3-9ee1-4186-969f-d8c8991e6a62",
      "number": 6,
      "content": "Hãy đến, chúng ta hãy vui mừng trong Ngài!",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.761Z",
      "updatedAt": "2024-09-09T04:46:28.761Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "64b52bdd-b11d-4221-826d-905d0e5dd167",
      "number": 7,
      "content": "Ngài dùng quyền năng của Ngài trị vì đời đời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.767Z",
      "updatedAt": "2024-09-09T04:46:28.767Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "1484f1c6-77b8-4deb-8028-06aa0e3d3ad7",
      "number": 7,
      "content": "Mắt Ngài nhìn xem các nước,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.772Z",
      "updatedAt": "2024-09-09T04:46:28.772Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "213e0f31-28a3-4b5b-9823-1d9e76ccde56",
      "number": 7,
      "content": "Kẻo những kẻ phản loạn nổi lên gây rối chăng. (Sê-la)",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.777Z",
      "updatedAt": "2024-09-09T04:46:28.777Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "367ae85c-808d-4833-8676-cbc4bf4c5494",
      "number": 8,
      "content": "Hỡi các dân, hãy ca ngợi Ðức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.782Z",
      "updatedAt": "2024-09-09T04:46:28.782Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "070021d2-9722-4371-a201-9eded71367fb",
      "number": 8,
      "content": "Hãy làm cho âm thanh ca ngợi Ngài vang ra khắp chốn,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.787Z",
      "updatedAt": "2024-09-09T04:46:28.787Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "de2c76e6-efd3-405d-9e8c-1f99dd44a2f0",
      "number": 9,
      "content": "Vì Ngài giữ gìn mạng sống chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.792Z",
      "updatedAt": "2024-09-09T04:46:28.792Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "f8499e4b-da42-44a5-b7a7-e3a7572e0232",
      "number": 9,
      "content": "Ngài không để cho chân chúng ta trợt ngã.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.796Z",
      "updatedAt": "2024-09-09T04:46:28.796Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "17a0d0fb-2783-436c-b86e-152f627e3011",
      "number": 10,
      "content": "Thật vậy Ðức Chúa Trời ôi, Ngài đã thử thách chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.801Z",
      "updatedAt": "2024-09-09T04:46:28.801Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "be64fa20-013a-4757-9bcb-d340ee8c2a07",
      "number": 10,
      "content": "Ngài đã rèn luyện chúng con như người ta luyện bạc.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.806Z",
      "updatedAt": "2024-09-09T04:46:28.806Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "ceebc1c4-2c83-4b28-a3af-4a9e7a39a28f",
      "number": 11,
      "content": "Ngài đã để cho chúng con bị sa vào lưới;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.811Z",
      "updatedAt": "2024-09-09T04:46:28.811Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "24df242e-7bbf-4a5c-ba5f-b7ccf8e4b788",
      "number": 11,
      "content": "Ngài đã để gánh nặng bị áp bức đè nặng trên lưng chúng con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.815Z",
      "updatedAt": "2024-09-09T04:46:28.815Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "222103dd-f97d-4397-b481-f1a03e821a37",
      "number": 12,
      "content": "Ngài đã để người ta cỡi trên đầu trên cổ chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.820Z",
      "updatedAt": "2024-09-09T04:46:28.820Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "687bce93-e288-4ccf-92b1-678f03a093b5",
      "number": 12,
      "content": "Chúng con phải đi qua lửa và qua nước;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.824Z",
      "updatedAt": "2024-09-09T04:46:28.824Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "41a9bab3-a80a-4bdd-bfef-a5be8fad2cdb",
      "number": 12,
      "content": "Nhưng rồi Ngài đã đem chúng con ra khỏi,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.829Z",
      "updatedAt": "2024-09-09T04:46:28.829Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "d08788c2-74cd-431d-bd35-eef1a1601755",
      "number": 12,
      "content": "Ðể đưa chúng con vào một nơi trù phú.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.835Z",
      "updatedAt": "2024-09-09T04:46:28.835Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "fd282679-0e77-4b0f-9b00-224849d1d738",
      "number": 13,
      "content": "Con sẽ vào nhà Ngài với những của lễ thiêu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.840Z",
      "updatedAt": "2024-09-09T04:46:28.840Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "52c09983-b2e1-4090-8b08-f9cf5d45cf66",
      "number": 13,
      "content": "Con sẽ trả xong cho Ngài những gì con đã hứa nguyện,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.845Z",
      "updatedAt": "2024-09-09T04:46:28.845Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "2266939c-2551-4022-a708-964862f48309",
      "number": 14,
      "content": "Những gì môi con đã hứa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.850Z",
      "updatedAt": "2024-09-09T04:46:28.850Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "dd1dd6a2-7066-4d6e-8c44-c1edf71c5360",
      "number": 14,
      "content": "Và miệng con đã nói ra trong lúc bị khốn khổ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.856Z",
      "updatedAt": "2024-09-09T04:46:28.856Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "30dd5975-fef4-47d5-985e-f518ff0b9da0",
      "number": 15,
      "content": "Con sẽ dâng lên Ngài những con vật hiến tế béo tốt để làm của lễ thiêu, chung với mỡ của các chiên đực;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.860Z",
      "updatedAt": "2024-09-09T04:46:28.860Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "73c64b7a-96ab-417d-a4fe-47e29837b954",
      "number": 15,
      "content": "Con sẽ dâng các bò đực và các dê đực. (Sê-la)",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.865Z",
      "updatedAt": "2024-09-09T04:46:28.865Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "3e363ee6-8c2e-4ce3-9613-f3a9e6aab723",
      "number": 16,
      "content": "Hỡi mọi người kính sợ Ðức Chúa Trời, xin đến nghe,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.870Z",
      "updatedAt": "2024-09-09T04:46:28.870Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "2c1b0caf-050b-42db-b042-1435c972928c",
      "number": 16,
      "content": "Tôi sẽ thuật lại những gì Ngài đã làm cho tôi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.875Z",
      "updatedAt": "2024-09-09T04:46:28.875Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "5870b067-a780-4a77-a39a-5f04b974d58b",
      "number": 17,
      "content": "Miệng tôi đã kêu cầu Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.880Z",
      "updatedAt": "2024-09-09T04:46:28.880Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "2be6a51d-cbe3-4cce-954f-44b5a33eb68a",
      "number": 17,
      "content": "Và lưỡi tôi đã tôn vinh Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.885Z",
      "updatedAt": "2024-09-09T04:46:28.885Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "7328e3eb-5a8c-4982-afe5-414885b1b771",
      "number": 18,
      "content": "Nếu lòng tôi chú về tội ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.890Z",
      "updatedAt": "2024-09-09T04:46:28.890Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "23bdaa34-0812-4074-ae18-a2bd10a3ff4a",
      "number": 18,
      "content": "Ắt Chúa chẳng nghe tôi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.894Z",
      "updatedAt": "2024-09-09T04:46:28.894Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "abdf2950-8e90-4316-b4d4-0c8338a5fdfe",
      "number": 19,
      "content": "Nhưng Ðức Chúa Trời thật có nghe;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.899Z",
      "updatedAt": "2024-09-09T04:46:28.899Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "447e24ec-4ae1-423c-89ef-9725332d138f",
      "number": 19,
      "content": "Ngài đã lắng nghe tiếng cầu nguyện của tôi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.907Z",
      "updatedAt": "2024-09-09T04:46:28.907Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "94aac784-dc70-4d28-9b16-bb5808ee7bdc",
      "number": 20,
      "content": "Tôn ngợi Ðức Chúa Trời, Ðấng chẳng bỏ qua lời cầu nguyện của tôi,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.916Z",
      "updatedAt": "2024-09-09T04:46:28.916Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    },
    {
      "id": "1cb0ec58-08cd-42b2-ba3e-65462bf70c40",
      "number": 20,
      "content": "Và không rút lại tình thương của Ngài đối với tôi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:46:28.920Z",
      "updatedAt": "2024-09-09T04:46:28.920Z",
      "chapterId": "b8171cde-8474-4b33-8b32-58298f5cc623"
    }
  ],
