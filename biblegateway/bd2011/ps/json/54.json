{
  "chapter": {
    "id": "cc9f94cd-9499-4fcf-9067-92e44aaede91",
    "number": 54,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2054&version=BD2011",
    "createdAt": "2024-07-12T04:03:46.693Z",
    "updatedAt": "2024-07-24T15:07:47.624Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "fadb99ee-7817-43c7-9f5c-5320398eb4c3",
      "number": 1,
      "content": "Ðức Chúa Trời ôi, xin cứu con, bằng uy danh Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.522Z",
      "updatedAt": "2024-07-31T11:33:39.136Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "246495e5-7578-4050-add9-b895ce4ad853",
      "number": 1,
      "content": "Xin minh oan cho con, bằng quyền năng Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.522Z",
      "updatedAt": "2024-07-31T11:33:39.150Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "2d93d63e-d5c7-401c-b537-cf2f1866ea95",
      "number": 2,
      "content": "Ðức Chúa Trời ôi, xin lắng nghe lời cầu nguyện của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.536Z",
      "updatedAt": "2024-07-31T11:33:39.159Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "3102c007-8113-4868-94a5-75395b1c0780",
      "number": 2,
      "content": "Xin nghiêng tai nghe tiếng của miệng con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.536Z",
      "updatedAt": "2024-07-31T11:33:39.166Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "d1dfec6b-3239-4b59-8ab9-f2d68700388a",
      "number": 3,
      "content": "Vì những kẻ xa lạ đã nổi lên chống lại con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.538Z",
      "updatedAt": "2024-07-31T11:33:39.174Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "74589e8d-680a-4a9b-9bc6-bb8438280b03",
      "number": 3,
      "content": "Những kẻ hung bạo đã tìm cách hại mạng sống con;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.538Z",
      "updatedAt": "2024-07-31T11:33:39.182Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "c631e020-dfb9-4c93-9f06-55a9cbf778f3",
      "number": 3,
      "content": "Quả là chúng chẳng coi Ðức Chúa Trời ra gì. (Sê-la)",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.538Z",
      "updatedAt": "2024-07-31T11:33:39.189Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "a731493c-de02-4a57-b2d6-a1aec70ae25d",
      "number": 4,
      "content": "Này, Ðức Chúa Trời là Ðấng giúp đỡ tôi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.541Z",
      "updatedAt": "2024-07-31T11:33:39.200Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "6db960f3-cc1e-4ad3-acf0-0d0491d38d6b",
      "number": 4,
      "content": "Chúa là Ðấng gìn giữ mạng sống tôi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.541Z",
      "updatedAt": "2024-07-31T11:33:39.208Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "e61b2937-868f-47f9-9aba-3c2263d64155",
      "number": 5,
      "content": "Ngài sẽ báo trả điều ác trên kẻ thù của tôi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.544Z",
      "updatedAt": "2024-07-31T11:33:39.215Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "28a1c879-354e-49ab-b6be-d578819936b8",
      "number": 5,
      "content": "Tiêu diệt chúng vì đức thành tín của Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.544Z",
      "updatedAt": "2024-07-31T11:33:39.223Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "6bd5c39a-2909-40a5-b0df-1ebfdda49d29",
      "number": 6,
      "content": "Con sẽ đem của lễ lạc ý dâng lên Ngài với tất cả niềm vui;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.546Z",
      "updatedAt": "2024-07-31T11:33:39.232Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "f71f8cef-c472-4aa9-bf74-732f434cafa3",
      "number": 6,
      "content": "Con sẽ cảm tạ danh Ngài, Chúa ôi, vì đó là điều tốt đẹp,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.546Z",
      "updatedAt": "2024-07-31T11:33:39.240Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "301af978-7fbc-462b-8d8f-cd59d45b1595",
      "number": 7,
      "content": "Vì Ngài đã giải cứu con khỏi mọi hoạn nạn,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.548Z",
      "updatedAt": "2024-07-31T11:33:39.247Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    },
    {
      "id": "24069bbc-4cff-46f3-b785-dd81eb94e71d",
      "number": 7,
      "content": "Và mắt con được nhìn thấy những kẻ thù của con bị bại.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:54:12.548Z",
      "updatedAt": "2024-07-31T11:33:39.255Z",
      "chapterId": "cc9f94cd-9499-4fcf-9067-92e44aaede91"
    }
  ],
