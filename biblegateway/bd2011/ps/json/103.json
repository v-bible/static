{
  "chapter": {
    "id": "387e9465-3ed1-4666-b264-8a8e06ae586f",
    "number": 103,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20103&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.275Z",
    "updatedAt": "2024-09-09T04:13:01.275Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "b8564d08-949a-4557-a1ef-8f0f33e11f47",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.166Z",
      "updatedAt": "2024-09-09T04:49:32.166Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "6f03a98c-b471-4302-a21e-f10270cc2842",
      "number": 1,
      "content": "Hỡi tất cả những gì ở trong ta, hãy ca tụng danh thánh của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.177Z",
      "updatedAt": "2024-09-09T04:49:32.177Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "b719dd91-5eba-45fd-81e2-34641aa971c6",
      "number": 2,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.179Z",
      "updatedAt": "2024-09-09T04:49:32.179Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "ec2a287d-f139-4b30-9035-ba9cecd7e147",
      "number": 2,
      "content": "Chớ quên mọi phước hạnh của Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.181Z",
      "updatedAt": "2024-09-09T04:49:32.181Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "1b5142f5-b4ee-4d7a-b4bf-9779050ebd96",
      "number": 3,
      "content": "Ðấng tha thứ mọi tội lỗi ngươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.183Z",
      "updatedAt": "2024-09-09T04:49:32.183Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "cfde79e2-0631-4723-9d65-cc7394ffea50",
      "number": 3,
      "content": "Ðấng chữa lành mọi bịnh tật ngươi;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.187Z",
      "updatedAt": "2024-09-09T04:49:32.187Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "8089f2da-852d-4e05-8090-8ec6a8022867",
      "number": 4,
      "content": "Ðấng cứu chuộc mạng sống ngươi thoát khỏi mộ phần;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.189Z",
      "updatedAt": "2024-09-09T04:49:32.189Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "56019f4e-74a2-4313-9fd9-3b0300aab55c",
      "number": 4,
      "content": "Ðấng đội trên đầu ngươi tình thương và lòng thương xót;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.191Z",
      "updatedAt": "2024-09-09T04:49:32.191Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "03f8c762-7b49-4414-85a0-939aa57b3de6",
      "number": 5,
      "content": "Ðấng cho ngươi no thỏa với thức ăn ngon,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.194Z",
      "updatedAt": "2024-09-09T04:49:32.194Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "4492a317-7bfb-4dea-a8f6-f8f76fbb9d37",
      "number": 5,
      "content": "Nhờ đó sức lực ngươi trẻ lại như sức của phượng hoàng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.199Z",
      "updatedAt": "2024-09-09T04:49:32.199Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "2dc4daff-8832-49ff-aeac-d171ca39a1ae",
      "number": 6,
      "content": "Chúa thi hành sự công chính",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.204Z",
      "updatedAt": "2024-09-09T04:49:32.204Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "c382feb7-38ae-4590-8961-e392b870429f",
      "number": 6,
      "content": "Và công lý cho mọi người bị áp bức.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.208Z",
      "updatedAt": "2024-09-09T04:49:32.208Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "3ecd125f-4b65-4eb6-b9de-77330e32959b",
      "number": 7,
      "content": "Ngài bày tỏ cho Môi-se các đường lối Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.213Z",
      "updatedAt": "2024-09-09T04:49:32.213Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "1d00fbd6-4f2d-4933-881d-f17fbb4e3482",
      "number": 7,
      "content": "Cho dân I-sơ-ra-ên các công việc Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.218Z",
      "updatedAt": "2024-09-09T04:49:32.218Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "c40b04ce-da88-4d35-93f0-4707c1ec7ef3",
      "number": 8,
      "content": "Chúa hay thương xót và khoan dung độ lượng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.223Z",
      "updatedAt": "2024-09-09T04:49:32.223Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "3eec5cc1-eb4c-461c-9377-d3dac5095729",
      "number": 8,
      "content": "Chậm nóng giận và đầy dẫy tình thương.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.228Z",
      "updatedAt": "2024-09-09T04:49:32.228Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "3d71d534-e71a-46c7-8eab-d87801a0ce76",
      "number": 9,
      "content": "Ngài không bắt tội luôn luôn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.233Z",
      "updatedAt": "2024-09-09T04:49:32.233Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "75107c3d-3fa5-40fa-a62b-5d815e85205a",
      "number": 9,
      "content": "Ngài cũng chẳng giữ lòng giận đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.238Z",
      "updatedAt": "2024-09-09T04:49:32.238Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "f35b78bf-482f-462e-bd0e-a2855df47cd6",
      "number": 10,
      "content": "Ngài không đối xử với chúng ta theo các tội của chúng ta;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.243Z",
      "updatedAt": "2024-09-09T04:49:32.243Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "45214d0f-9f06-444a-8a35-764581c31ef9",
      "number": 10,
      "content": "Ngài cũng không báo trả chúng ta theo các lỗi của chúng ta.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.251Z",
      "updatedAt": "2024-09-09T04:49:32.251Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "a7509b95-4bdf-4d43-bc06-6f7bfb5a965f",
      "number": 11,
      "content": "Vì các tầng trời cao hơn đất bao nhiêu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.259Z",
      "updatedAt": "2024-09-09T04:49:32.259Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "9ab428ef-581b-4126-b21f-616be795bd22",
      "number": 11,
      "content": "Tình thương của Ngài đối với những người kính sợ Ngài cũng cao lớn bấy nhiêu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.265Z",
      "updatedAt": "2024-09-09T04:49:32.265Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "2b3314de-0d19-459c-a483-c3e6f47120d4",
      "number": 12,
      "content": "Phương đông xa cách phương tây bao nhiêu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.270Z",
      "updatedAt": "2024-09-09T04:49:32.270Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "fa02ecc5-5ffe-4536-8e83-61b66fd2257c",
      "number": 12,
      "content": "Ngài đem những vi phạm của chúng ta xa khỏi chúng ta cũng bấy nhiêu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.275Z",
      "updatedAt": "2024-09-09T04:49:32.275Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "19e72fc0-370e-4d1a-bb63-7043626c7817",
      "number": 13,
      "content": "Như người cha thương xót các con mình thể nào,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.280Z",
      "updatedAt": "2024-09-09T04:49:32.280Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "eef74dbc-f852-4ae6-b587-8ced00a040d4",
      "number": 13,
      "content": "Chúa thương xót những người kính sợ Ngài cũng thể ấy.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.285Z",
      "updatedAt": "2024-09-09T04:49:32.285Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "d6eb18b0-74a5-41b7-82df-afd0f969722b",
      "number": 14,
      "content": "Vì Ngài biết rõ chúng ta được dựng nên bằng những gì;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.291Z",
      "updatedAt": "2024-09-09T04:49:32.291Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "0bde58c8-1e60-4813-b202-8e581bed6d15",
      "number": 14,
      "content": "Ngài nhớ lại rằng chúng ta là bụi đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.295Z",
      "updatedAt": "2024-09-09T04:49:32.295Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "fa41b5dd-6c7f-4582-bd68-8bfd82e725a5",
      "number": 15,
      "content": "Về loài người, đời người như ngọn cỏ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.300Z",
      "updatedAt": "2024-09-09T04:49:32.300Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "a78638e7-818c-4ca9-9e71-c077e55bc493",
      "number": 15,
      "content": "Nó thắm tươi như hoa dại ngoài đồng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.305Z",
      "updatedAt": "2024-09-09T04:49:32.305Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "f8b72204-8bd4-4bbf-8f06-9337bd7254d4",
      "number": 16,
      "content": "Một cơn gió thổi qua, kìa, nó chẳng còn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.309Z",
      "updatedAt": "2024-09-09T04:49:32.309Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "e0c75cef-2db5-43ec-ab08-5ef5e4aaab02",
      "number": 16,
      "content": "Chỗ nó mọc lên cũng không còn biết nó nữa.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.314Z",
      "updatedAt": "2024-09-09T04:49:32.314Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "706d604e-cf90-425c-af21-675987261050",
      "number": 17,
      "content": "Nhưng tình thương của Chúa thì còn mãi mãi cho những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.319Z",
      "updatedAt": "2024-09-09T04:49:32.319Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "21e33950-05ac-4cab-a7fa-adfbc1196a62",
      "number": 17,
      "content": "Và đức công chính Ngài dành cho con cháu chắt chít họ,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.325Z",
      "updatedAt": "2024-09-09T04:49:32.325Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "eb8d2fde-da91-476c-a9ea-7a27ed4984e9",
      "number": 18,
      "content": "Tức cho những người giữ giao ước Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.330Z",
      "updatedAt": "2024-09-09T04:49:32.330Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "96fa5bf8-d669-4758-b173-df227832559c",
      "number": 18,
      "content": "Và cho những người nhớ đến các điều răn Ngài mà làm theo.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.335Z",
      "updatedAt": "2024-09-09T04:49:32.335Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "fee7dd91-fb2f-4d7f-ae4c-d98549631d30",
      "number": 19,
      "content": "Chúa thiết lập ngai Ngài trên các tầng trời;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.339Z",
      "updatedAt": "2024-09-09T04:49:32.339Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "011095be-7893-459a-96c6-560cec81cd78",
      "number": 19,
      "content": "Vương quyền Ngài bao trùm cả vũ trụ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.344Z",
      "updatedAt": "2024-09-09T04:49:32.344Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "5d6086da-3f78-4e24-a29c-35aaa66ce0e3",
      "number": 20,
      "content": "Hãy ca ngợi Chúa, hỡi các thiên sứ của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.349Z",
      "updatedAt": "2024-09-09T04:49:32.349Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "1d2e3086-ee23-4479-a6e2-50752b8a6f0a",
      "number": 20,
      "content": "Tức những đấng dũng mãnh siêu phàm,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.354Z",
      "updatedAt": "2024-09-09T04:49:32.354Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "edd217a9-03e8-4dd3-81bc-545059c4e320",
      "number": 20,
      "content": "Những đấng thi hành mệnh lệnh Ngài,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.360Z",
      "updatedAt": "2024-09-09T04:49:32.360Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "7c5aff33-521e-4c3f-a72d-3101be921698",
      "number": 20,
      "content": "Những đấng vâng theo lời Ngài!",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.365Z",
      "updatedAt": "2024-09-09T04:49:32.365Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "0a29f934-c0cf-436c-b8e9-6d14086bbf71",
      "number": 21,
      "content": "Hãy ca ngợi Chúa, hỡi tất cả các đạo quân của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.370Z",
      "updatedAt": "2024-09-09T04:49:32.370Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "16d2a2fb-03af-4e67-acbf-68f3a90e4c1c",
      "number": 21,
      "content": "Tức các đấng phục vụ Ngài và làm theo thánh ý Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.376Z",
      "updatedAt": "2024-09-09T04:49:32.376Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "d0def1fe-399b-4c9a-a123-d999cd98d0bc",
      "number": 22,
      "content": "Hãy ca ngợi Chúa, hỡi tất cả các tạo vật của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.380Z",
      "updatedAt": "2024-09-09T04:49:32.380Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "c8617788-aa2e-4ae4-98b4-2c158b30eaa6",
      "number": 22,
      "content": "Ở mọi nơi dưới quyền trị vì của Ngài!",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.385Z",
      "updatedAt": "2024-09-09T04:49:32.385Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    },
    {
      "id": "0a61108a-b92a-4d82-804c-99f8af38d1be",
      "number": 22,
      "content": "Hãy ca ngợi Chúa, hỡi linh hồn ta!",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:32.390Z",
      "updatedAt": "2024-09-09T04:49:32.390Z",
      "chapterId": "387e9465-3ed1-4666-b264-8a8e06ae586f"
    }
  ],
