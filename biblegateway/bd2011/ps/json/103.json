{
  "chapter": {
    "id": "5f02d722-4148-48cc-ac35-16b82742a255",
    "number": 103,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20103&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.757Z",
    "updatedAt": "2024-07-24T15:07:48.657Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "b2dc8eec-c693-4a14-b18b-ba4c4747048f",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.157Z",
      "updatedAt": "2024-07-31T11:45:56.667Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "1bb39530-4844-48dc-a82f-abbc66967527",
      "number": 1,
      "content": "Hỡi tất cả những gì ở trong ta, hãy ca tụng danh thánh của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.157Z",
      "updatedAt": "2024-07-31T11:45:56.683Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "670ca648-436a-4570-8dbe-163051afba47",
      "number": 2,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.168Z",
      "updatedAt": "2024-07-31T11:45:56.692Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "634215f0-a8b3-456f-9863-a94210649d0c",
      "number": 2,
      "content": "Chớ quên mọi phước hạnh của Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.168Z",
      "updatedAt": "2024-07-31T11:45:56.701Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "f2c9c537-b4d7-40d2-9293-ae95613b7764",
      "number": 3,
      "content": "Ðấng tha thứ mọi tội lỗi ngươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.175Z",
      "updatedAt": "2024-07-31T11:45:56.716Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "04d3eb75-cf0e-4c89-b185-8f7c5c4653d0",
      "number": 3,
      "content": "Ðấng chữa lành mọi bịnh tật ngươi;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.175Z",
      "updatedAt": "2024-07-31T11:45:56.724Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "5bf4c332-03bc-4978-b5d7-c070b0c1ec63",
      "number": 4,
      "content": "Ðấng cứu chuộc mạng sống ngươi thoát khỏi mộ phần;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.181Z",
      "updatedAt": "2024-07-31T11:45:56.734Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "889d9aa1-8870-41d8-9bfb-1b3fd201b4df",
      "number": 4,
      "content": "Ðấng đội trên đầu ngươi tình thương và lòng thương xót;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.181Z",
      "updatedAt": "2024-07-31T11:45:56.743Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "2adb7df2-3159-46b2-92f0-61b786895c9c",
      "number": 5,
      "content": "Ðấng cho ngươi no thỏa với thức ăn ngon,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.187Z",
      "updatedAt": "2024-07-31T11:45:56.751Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "c780a79c-9fdd-4b04-bd3e-610079137549",
      "number": 5,
      "content": "Nhờ đó sức lực ngươi trẻ lại như sức của phượng hoàng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.187Z",
      "updatedAt": "2024-07-31T11:45:56.759Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "8849f11a-00be-4907-9142-3199e6830a03",
      "number": 6,
      "content": "Chúa thi hành sự công chính",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.192Z",
      "updatedAt": "2024-07-31T11:45:56.770Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "443199cd-3655-4137-95ba-0c45fb352d02",
      "number": 6,
      "content": "Và công lý cho mọi người bị áp bức.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.192Z",
      "updatedAt": "2024-07-31T11:45:56.778Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "2efef636-5fd6-4768-92d9-e24f4b2e219d",
      "number": 7,
      "content": "Ngài bày tỏ cho Môi-se các đường lối Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.198Z",
      "updatedAt": "2024-07-31T11:45:56.787Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "d8a9372e-43c0-4632-ad12-9f031fd8da47",
      "number": 7,
      "content": "Cho dân I-sơ-ra-ên các công việc Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.198Z",
      "updatedAt": "2024-07-31T11:45:56.794Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "8a3884ef-f102-450e-a9ff-1a904bb8a6a1",
      "number": 8,
      "content": "Chúa hay thương xót và khoan dung độ lượng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.204Z",
      "updatedAt": "2024-07-31T11:45:56.802Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "cbef64fb-05c2-4730-8655-0865dd291974",
      "number": 8,
      "content": "Chậm nóng giận và đầy dẫy tình thương.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.204Z",
      "updatedAt": "2024-07-31T11:45:56.810Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "afc9eef0-970b-4012-8c4a-b10db4ce7c82",
      "number": 9,
      "content": "Ngài không bắt tội luôn luôn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.210Z",
      "updatedAt": "2024-07-31T11:45:56.818Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "6c11fa72-5e51-460d-ad3c-6f685149c7b3",
      "number": 9,
      "content": "Ngài cũng chẳng giữ lòng giận đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.210Z",
      "updatedAt": "2024-07-31T11:45:56.826Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "8f995a83-0c57-4182-bf79-b91bb1cd6b6d",
      "number": 10,
      "content": "Ngài không đối xử với chúng ta theo các tội của chúng ta;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.214Z",
      "updatedAt": "2024-07-31T11:45:56.834Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "686fb244-828a-418a-8180-fc966aa83566",
      "number": 10,
      "content": "Ngài cũng không báo trả chúng ta theo các lỗi của chúng ta.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.214Z",
      "updatedAt": "2024-07-31T11:45:56.842Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "49dc2917-afc8-449a-b1d7-259e2426af08",
      "number": 11,
      "content": "Vì các tầng trời cao hơn đất bao nhiêu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.218Z",
      "updatedAt": "2024-07-31T11:45:56.964Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "456f55cc-ec5e-486a-9bfd-b0a6aee184a5",
      "number": 11,
      "content": "Tình thương của Ngài đối với những người kính sợ Ngài cũng cao lớn bấy nhiêu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.218Z",
      "updatedAt": "2024-07-31T11:45:57.010Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "5e837e99-cfca-4db0-888a-af9aafd9abfd",
      "number": 12,
      "content": "Phương đông xa cách phương tây bao nhiêu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.222Z",
      "updatedAt": "2024-07-31T11:45:57.037Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "ffff5607-647d-448d-8ca8-5017e1125616",
      "number": 12,
      "content": "Ngài đem những vi phạm của chúng ta xa khỏi chúng ta cũng bấy nhiêu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.222Z",
      "updatedAt": "2024-07-31T11:45:57.048Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "0bdacea3-2128-49f4-b827-25a5a799a9f0",
      "number": 13,
      "content": "Như người cha thương xót các con mình thể nào,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.225Z",
      "updatedAt": "2024-07-31T11:45:57.057Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "ce302c47-c4ed-4a0c-9922-d124947e692a",
      "number": 13,
      "content": "Chúa thương xót những người kính sợ Ngài cũng thể ấy.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.225Z",
      "updatedAt": "2024-07-31T11:45:57.068Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "a6eaab6d-a704-45ff-ab20-a9326673522c",
      "number": 14,
      "content": "Vì Ngài biết rõ chúng ta được dựng nên bằng những gì;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.228Z",
      "updatedAt": "2024-07-31T11:45:57.077Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "e88ff3cc-3485-47a1-a8e5-138b3c99c157",
      "number": 14,
      "content": "Ngài nhớ lại rằng chúng ta là bụi đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.228Z",
      "updatedAt": "2024-07-31T11:45:57.086Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "ede56290-07a3-4eeb-b23f-857f51cac005",
      "number": 15,
      "content": "Về loài người, đời người như ngọn cỏ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.231Z",
      "updatedAt": "2024-07-31T11:45:57.097Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "88ca6007-74a2-4326-abce-d33a9d5b46b9",
      "number": 15,
      "content": "Nó thắm tươi như hoa dại ngoài đồng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.231Z",
      "updatedAt": "2024-07-31T11:45:57.105Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "77528072-c655-4bd8-a500-ef1e43d4f704",
      "number": 16,
      "content": "Một cơn gió thổi qua, kìa, nó chẳng còn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.234Z",
      "updatedAt": "2024-07-31T11:45:57.114Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "c7074ebd-898f-40e5-8fbc-8e445ab4ed63",
      "number": 16,
      "content": "Chỗ nó mọc lên cũng không còn biết nó nữa.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.234Z",
      "updatedAt": "2024-07-31T11:45:57.123Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "f09f4d2e-6da2-4fa4-ac0a-336df3bfb284",
      "number": 17,
      "content": "Nhưng tình thương của Chúa thì còn mãi mãi cho những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.236Z",
      "updatedAt": "2024-07-31T11:45:57.132Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "0c8a1dc1-8587-4a28-94c7-53dbfb7ab147",
      "number": 17,
      "content": "Và đức công chính Ngài dành cho con cháu chắt chít họ,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.236Z",
      "updatedAt": "2024-07-31T11:45:57.141Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "3243278b-79c9-4aaf-8291-e5a97b317703",
      "number": 18,
      "content": "Tức cho những người giữ giao ước Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.239Z",
      "updatedAt": "2024-07-31T11:45:57.151Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "a56ed014-8c70-46dc-94a5-3a71ea907901",
      "number": 18,
      "content": "Và cho những người nhớ đến các điều răn Ngài mà làm theo.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.239Z",
      "updatedAt": "2024-07-31T11:45:57.159Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "5e28cf3d-b385-4762-a420-c4e923c9e2d7",
      "number": 19,
      "content": "Chúa thiết lập ngai Ngài trên các tầng trời;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.241Z",
      "updatedAt": "2024-07-31T11:45:57.172Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "a1bdb814-c4de-4445-867b-ec83f0ba09bd",
      "number": 19,
      "content": "Vương quyền Ngài bao trùm cả vũ trụ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.241Z",
      "updatedAt": "2024-07-31T11:45:57.182Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "613b0be5-2646-4fe5-a6b4-1d56c974c315",
      "number": 20,
      "content": "Hãy ca ngợi Chúa, hỡi các thiên sứ của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.243Z",
      "updatedAt": "2024-07-31T11:45:57.193Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "1c676f10-99c7-4572-a438-f8db3673e098",
      "number": 20,
      "content": "Tức những đấng dũng mãnh siêu phàm,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.243Z",
      "updatedAt": "2024-07-31T11:45:57.202Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "49eeea3c-01f3-40ba-9e63-0ad771cf5fe9",
      "number": 20,
      "content": "Những đấng thi hành mệnh lệnh Ngài,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.243Z",
      "updatedAt": "2024-07-31T11:45:57.210Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "2779dc90-ee26-4572-bf85-a80076a3bfdd",
      "number": 20,
      "content": "Những đấng vâng theo lời Ngài!",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.243Z",
      "updatedAt": "2024-07-31T11:45:57.219Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "c6eac52a-43b6-4ff6-a092-8ab21a024366",
      "number": 21,
      "content": "Hãy ca ngợi Chúa, hỡi tất cả các đạo quân của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.247Z",
      "updatedAt": "2024-07-31T11:45:57.229Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "0e58a91b-24fd-4dcb-9a3d-834b385ecf0b",
      "number": 21,
      "content": "Tức các đấng phục vụ Ngài và làm theo thánh ý Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.247Z",
      "updatedAt": "2024-07-31T11:45:57.238Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "5f117432-6a24-48a2-b110-2718bf2b939f",
      "number": 22,
      "content": "Hãy ca ngợi Chúa, hỡi tất cả các tạo vật của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.249Z",
      "updatedAt": "2024-07-31T11:45:57.249Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "c4872a2c-eb3c-4a3a-8fcb-1e3af1aa8c8b",
      "number": 22,
      "content": "Ở mọi nơi dưới quyền trị vì của Ngài!",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.249Z",
      "updatedAt": "2024-07-31T11:45:57.257Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    },
    {
      "id": "0d82db61-be76-4573-bbf2-f94f6ed3af7a",
      "number": 22,
      "content": "Hãy ca ngợi Chúa, hỡi linh hồn ta!",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:05:21.249Z",
      "updatedAt": "2024-07-31T11:45:57.266Z",
      "chapterId": "5f02d722-4148-48cc-ac35-16b82742a255"
    }
  ],
