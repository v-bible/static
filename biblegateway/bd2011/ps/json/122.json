{
  "chapter": {
    "id": "5970f3b4-49d5-44d9-9c80-ef370597b6a5",
    "number": 122,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20122&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.170Z",
    "updatedAt": "2024-07-24T15:07:49.046Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "71b545a1-5bde-4094-ba60-daa89d6fc3ec",
      "number": 1,
      "content": "Tôi vui mừng khi người ta nói với tôi rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.349Z",
      "updatedAt": "2024-07-31T11:50:36.196Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "01856e18-713c-4b91-af7d-0be1be46dc0f",
      "number": 1,
      "content": "“Chúng ta hãy đi đến nhà Chúa.”",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.349Z",
      "updatedAt": "2024-07-31T11:50:36.226Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "a40bb1ef-3512-49e4-a4d0-07426ee35c4b",
      "number": 2,
      "content": "Hỡi Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.359Z",
      "updatedAt": "2024-07-31T11:50:36.248Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "b2892050-24ed-4542-a578-6cf25849b19b",
      "number": 2,
      "content": "Chân chúng ta đang đứng bên trong cổng thành ngươi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.359Z",
      "updatedAt": "2024-07-31T11:50:36.271Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "31c1ae1b-f326-43e4-961a-c955d6a80eff",
      "number": 3,
      "content": "Giê-ru-sa-lem đã được xây dựng thành một kinh đô,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.364Z",
      "updatedAt": "2024-07-31T11:50:36.301Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "d85d1a0b-de77-445e-98be-10d0b67a3e69",
      "number": 3,
      "content": "Các tòa nhà trong thành nối kết nhau chặt chẽ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.364Z",
      "updatedAt": "2024-07-31T11:50:36.326Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "f4ec570d-0f77-49dd-8a80-8d98dcc8a803",
      "number": 4,
      "content": "Các chi tộc đi lên thành ấy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.369Z",
      "updatedAt": "2024-07-31T11:50:36.345Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "3986afbc-98ce-45a2-9b7a-d5c34367cc0e",
      "number": 4,
      "content": "Tức các chi tộc của Chúa,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.369Z",
      "updatedAt": "2024-07-31T11:50:36.357Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "5136ede9-da47-46d2-a983-1d056b5d21fb",
      "number": 4,
      "content": "Theo như luật định cho I-sơ-ra-ên,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.369Z",
      "updatedAt": "2024-07-31T11:50:36.366Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "8ae329a0-3ba7-4c3e-8ec8-94b48235e44a",
      "number": 4,
      "content": "Ðể cảm tạ danh Chúa,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.369Z",
      "updatedAt": "2024-07-31T11:50:36.375Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "b3f2d3e8-2c13-4822-8c6d-f46daa31d3e6",
      "number": 5,
      "content": "Vì tại đó các ngai xét đoán được thiết lập,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.374Z",
      "updatedAt": "2024-07-31T11:50:36.383Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "67dea0f0-f6fb-4370-a3d5-76b1961a0308",
      "number": 5,
      "content": "Ðó là các ngai của nhà Ða-vít.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.374Z",
      "updatedAt": "2024-07-31T11:50:36.393Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "988892a4-f00a-4724-baeb-545aecf28f49",
      "number": 6,
      "content": "Hãy cầu hòa bình cho Giê-ru-sa-lem:",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.379Z",
      "updatedAt": "2024-07-31T11:50:36.405Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "68a88b9b-2363-4d1b-88ea-96873ba708d8",
      "number": 6,
      "content": "“Nguyện những ai yêu mến ngươi được thịnh vượng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.379Z",
      "updatedAt": "2024-07-31T11:50:36.413Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "8bbfdcff-adbc-411e-accc-5024c03e14eb",
      "number": 7,
      "content": "Nguyện bình an ở phía trong tường ngươi.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.383Z",
      "updatedAt": "2024-07-31T11:50:36.422Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "099cc2b7-6344-48ee-aa2f-7a61fa04576c",
      "number": 7,
      "content": "Nguyện giàu sang phú quý ở trong các cung điện ngươi.”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.383Z",
      "updatedAt": "2024-07-31T11:50:36.431Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "f277deeb-625c-4bba-a3cf-0674efdc4c9f",
      "number": 8,
      "content": "Nay vì cớ anh chị em ta và các bạn hữu ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.406Z",
      "updatedAt": "2024-07-31T11:50:36.440Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "970d0ff0-e8b6-43de-88b4-136f9436f897",
      "number": 8,
      "content": "Ta cầu chúc ngươi rằng, “Chúc ngươi luôn được bình an thịnh vượng.”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.406Z",
      "updatedAt": "2024-07-31T11:50:36.449Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "89cb82bc-992a-4909-ad9f-ae5b855d1a18",
      "number": 9,
      "content": "Vì cớ nhà Chúa, Ðức Chúa Trời chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.410Z",
      "updatedAt": "2024-07-31T11:50:36.458Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    },
    {
      "id": "cc477917-70d8-4ad8-a9c2-c79357e718e5",
      "number": 9,
      "content": "Ta cầu phước cho ngươi.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:15.410Z",
      "updatedAt": "2024-07-31T11:50:36.467Z",
      "chapterId": "5970f3b4-49d5-44d9-9c80-ef370597b6a5"
    }
  ],
