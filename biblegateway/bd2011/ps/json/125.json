{
  "chapter": {
    "id": "62b2cd20-2890-4879-b9e3-919d46e1e8f7",
    "number": 125,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20125&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.720Z",
    "updatedAt": "2024-09-09T04:13:01.720Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "baaba5c0-e975-46f3-bb1f-60a9f041088f",
      "number": 1,
      "content": "Phàm ai tin cậy Chúa sẽ như Núi Si-ôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.531Z",
      "updatedAt": "2024-09-09T04:51:21.531Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    },
    {
      "id": "a9361c3d-2c17-4847-99f2-932f6e286894",
      "number": 1,
      "content": "Chẳng hề rúng động nhưng cứ bền vững muôn đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.548Z",
      "updatedAt": "2024-09-09T04:51:21.548Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    },
    {
      "id": "bca17096-11fb-4fbb-b0f0-d9ce1b1e5765",
      "number": 2,
      "content": "Như các rặng núi bao quanh Giê-ru-sa-lem thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.553Z",
      "updatedAt": "2024-09-09T04:51:21.553Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    },
    {
      "id": "01e466b7-6ae8-4a91-95ce-9e9b247353d9",
      "number": 2,
      "content": "Chúa bao quanh dân Ngài thể ấy,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.557Z",
      "updatedAt": "2024-09-09T04:51:21.557Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    },
    {
      "id": "94b62b67-89e4-4d36-b9f1-b5ffa625bc13",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.562Z",
      "updatedAt": "2024-09-09T04:51:21.562Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    },
    {
      "id": "3fbc99c5-14a3-4981-bdcf-d6d401c6e950",
      "number": 3,
      "content": "Vì gậy cầm quyền của kẻ ác không được phép hành sử trên địa phận của người ngay lành,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.567Z",
      "updatedAt": "2024-09-09T04:51:21.567Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    },
    {
      "id": "ba3be154-f774-433b-9831-89e4430df00e",
      "number": 3,
      "content": "Nên tay người ngay lành không bị buộc phải tham dự vào việc ác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.571Z",
      "updatedAt": "2024-09-09T04:51:21.571Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    },
    {
      "id": "fbce5bc1-13cd-47c7-86fe-21c4e8ad37f9",
      "number": 4,
      "content": "Chúa ôi, xin ban phước cho những người tốt,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.576Z",
      "updatedAt": "2024-09-09T04:51:21.576Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    },
    {
      "id": "392ecae6-8392-4803-9c60-f5eda36018de",
      "number": 4,
      "content": "Và cho những người có lòng ngay thẳng thật thà.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.581Z",
      "updatedAt": "2024-09-09T04:51:21.581Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    },
    {
      "id": "0146d926-ce63-4654-82e5-2c5a255c7def",
      "number": 5,
      "content": "Nhưng đối với những kẻ bỏ Ngài để đi theo con đường lươn lẹo,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.585Z",
      "updatedAt": "2024-09-09T04:51:21.585Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    },
    {
      "id": "04f57113-e67f-41ba-abae-34bf4eadad04",
      "number": 5,
      "content": "Xin Chúa trục xuất chúng chung với những kẻ làm điều ác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.590Z",
      "updatedAt": "2024-09-09T04:51:21.590Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    },
    {
      "id": "e0b30629-d537-45d3-946f-d8f47aee18c8",
      "number": 5,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:51:21.595Z",
      "updatedAt": "2024-09-09T04:51:21.595Z",
      "chapterId": "62b2cd20-2890-4879-b9e3-919d46e1e8f7"
    }
  ],
