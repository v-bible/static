{
  "chapter": {
    "id": "b08a9064-4c54-4a30-ab03-dd80a783a746",
    "number": 125,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20125&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.233Z",
    "updatedAt": "2024-07-24T15:07:49.104Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "20dd4d77-44cd-4296-97be-73c960d742f9",
      "number": 1,
      "content": "Phàm ai tin cậy Chúa sẽ như Núi Si-ôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.039Z",
      "updatedAt": "2024-07-31T11:51:12.290Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    },
    {
      "id": "2cec079d-748d-4204-8a27-edfff38c63a8",
      "number": 1,
      "content": "Chẳng hề rúng động nhưng cứ bền vững muôn đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.039Z",
      "updatedAt": "2024-07-31T11:51:12.304Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    },
    {
      "id": "046b7a84-37a5-46de-93b7-9c4da7747abb",
      "number": 2,
      "content": "Như các rặng núi bao quanh Giê-ru-sa-lem thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.042Z",
      "updatedAt": "2024-07-31T11:51:12.311Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    },
    {
      "id": "987a381c-323f-42e4-bfca-36a721499491",
      "number": 2,
      "content": "Chúa bao quanh dân Ngài thể ấy,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.042Z",
      "updatedAt": "2024-07-31T11:51:12.318Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    },
    {
      "id": "da397e9b-eb9e-4d46-8fcf-d90d67e49d75",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.042Z",
      "updatedAt": "2024-07-31T11:51:12.326Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    },
    {
      "id": "8c922a86-6330-4cda-97e9-53939ced37d9",
      "number": 3,
      "content": "Vì gậy cầm quyền của kẻ ác không được phép hành sử trên địa phận của người ngay lành,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.044Z",
      "updatedAt": "2024-07-31T11:51:12.336Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    },
    {
      "id": "f71c593c-7c31-4331-aa49-bf4d66e4604d",
      "number": 3,
      "content": "Nên tay người ngay lành không bị buộc phải tham dự vào việc ác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.044Z",
      "updatedAt": "2024-07-31T11:51:12.343Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    },
    {
      "id": "8866c2d5-8890-426b-a605-a0bd77252d93",
      "number": 4,
      "content": "Chúa ôi, xin ban phước cho những người tốt,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.047Z",
      "updatedAt": "2024-07-31T11:51:12.351Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    },
    {
      "id": "a4527a07-318f-43c2-8a00-07948833b1a2",
      "number": 4,
      "content": "Và cho những người có lòng ngay thẳng thật thà.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.047Z",
      "updatedAt": "2024-07-31T11:51:12.359Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    },
    {
      "id": "624720da-52db-4fee-9e60-8c3480952de3",
      "number": 5,
      "content": "Nhưng đối với những kẻ bỏ Ngài để đi theo con đường lươn lẹo,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.049Z",
      "updatedAt": "2024-07-31T11:51:12.367Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    },
    {
      "id": "d863ccec-9819-466a-9a0c-96ae9540d027",
      "number": 5,
      "content": "Xin Chúa trục xuất chúng chung với những kẻ làm điều ác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.049Z",
      "updatedAt": "2024-07-31T11:51:12.374Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    },
    {
      "id": "ddab407b-f178-46c1-b732-e619a842ce2e",
      "number": 5,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:09:59.049Z",
      "updatedAt": "2024-07-31T11:51:12.384Z",
      "chapterId": "b08a9064-4c54-4a30-ab03-dd80a783a746"
    }
  ],
