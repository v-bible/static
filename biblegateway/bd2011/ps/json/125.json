{
  "chapter": {
    "id": "90ed8388-3c85-4b1b-8ffd-da522cb52940",
    "number": 125,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20125&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.918Z",
    "updatedAt": "2024-09-24T01:00:44.918Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "578a5ca8-b24b-4fe1-a8ef-3bc3888d846c",
      "number": 1,
      "content": "Phàm ai tin cậy Chúa sẽ như Núi Si-ôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.873Z",
      "updatedAt": "2024-09-24T02:10:38.873Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    },
    {
      "id": "9dbac09f-b2fa-4ee5-ac83-24fb5c513b28",
      "number": 1,
      "content": "Chẳng hề rúng động nhưng cứ bền vững muôn đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.886Z",
      "updatedAt": "2024-09-24T02:10:38.886Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    },
    {
      "id": "4c172709-ec96-47cc-9a7e-dca22034cb3a",
      "number": 2,
      "content": "Như các rặng núi bao quanh Giê-ru-sa-lem thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.889Z",
      "updatedAt": "2024-09-24T02:10:38.889Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    },
    {
      "id": "00085a06-6e50-4a15-b3f9-50d24f6519d4",
      "number": 2,
      "content": "Chúa bao quanh dân Ngài thể ấy,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.891Z",
      "updatedAt": "2024-09-24T02:10:38.891Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    },
    {
      "id": "1b9d0f68-c541-497c-9ca7-af6ae4027153",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.894Z",
      "updatedAt": "2024-09-24T02:10:38.894Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    },
    {
      "id": "64de5822-167a-41f9-ae72-49755e54780f",
      "number": 3,
      "content": "Vì gậy cầm quyền của kẻ ác không được phép hành sử trên địa phận của người ngay lành,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.897Z",
      "updatedAt": "2024-09-24T02:10:38.897Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    },
    {
      "id": "d60113ff-4ea7-42cd-8dc6-215b4f29c1c1",
      "number": 3,
      "content": "Nên tay người ngay lành không bị buộc phải tham dự vào việc ác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.899Z",
      "updatedAt": "2024-09-24T02:10:38.899Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    },
    {
      "id": "a916c258-dcd6-4221-93ba-b7552e57fa1f",
      "number": 4,
      "content": "Chúa ôi, xin ban phước cho những người tốt,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.902Z",
      "updatedAt": "2024-09-24T02:10:38.902Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    },
    {
      "id": "cebaf0c6-e6b8-4114-9239-23839abc088c",
      "number": 4,
      "content": "Và cho những người có lòng ngay thẳng thật thà.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.905Z",
      "updatedAt": "2024-09-24T02:10:38.905Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    },
    {
      "id": "83fe9fc2-67a4-4530-a892-0a4338ca88f9",
      "number": 5,
      "content": "Nhưng đối với những kẻ bỏ Ngài để đi theo con đường lươn lẹo,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.908Z",
      "updatedAt": "2024-09-24T02:10:38.908Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    },
    {
      "id": "690c0580-d12d-4927-a480-8cbef8ead49d",
      "number": 5,
      "content": "Xin Chúa trục xuất chúng chung với những kẻ làm điều ác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.910Z",
      "updatedAt": "2024-09-24T02:10:38.910Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    },
    {
      "id": "be64642f-0618-4448-923c-bdc2045efece",
      "number": 5,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:38.913Z",
      "updatedAt": "2024-09-24T02:10:38.913Z",
      "chapterId": "90ed8388-3c85-4b1b-8ffd-da522cb52940"
    }
  ],
