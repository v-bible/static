      "chapterId": "667aae7f-bdcb-4312-b9ff-0e61b29cf3d8"
    },
    {
      "id": "9e0e2dbe-d35b-4574-9627-2e032cd52e60",
      "number": 21,
      "content": "Ðiều ác sẽ giết kẻ ác,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:23.666Z",
      "updatedAt": "2024-07-31T11:57:04.971Z",
      "chapterId": "667aae7f-bdcb-4312-b9ff-0e61b29cf3d8"
    },
    {
      "id": "e4fd278c-292b-4fdc-91f4-2a34f3c9251e",
      "number": 21,
      "content": "Và những kẻ ghét người ngay lành sẽ bị kết tội.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:23.666Z",
      "updatedAt": "2024-07-31T11:57:04.984Z",
      "chapterId": "667aae7f-bdcb-4312-b9ff-0e61b29cf3d8"
    },
    {
      "id": "1c860ed2-fb00-4d94-a77e-95684a28eaed",
      "number": 22,
      "content": "Chúa cứu chuộc mạng sống các tôi tớ Ngài;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:23.671Z",
      "updatedAt": "2024-07-31T11:57:05.002Z",
      "chapterId": "667aae7f-bdcb-4312-b9ff-0e61b29cf3d8"
    },
    {
      "id": "66c3b625-3997-45a9-90f3-f50b9a8d159c",
      "number": 22,
      "content": "Không người nào nương cậy Ngài sẽ bị kết tội.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:50:23.671Z",
      "updatedAt": "2024-07-31T11:57:05.020Z",
      "chapterId": "667aae7f-bdcb-4312-b9ff-0e61b29cf3d8"
    }
  ],
