    },
    {
      "id": "74716040-1920-4948-8cee-542068686528",
      "number": 21,
      "content": "Ðiều ác sẽ giết kẻ ác,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:49.196Z",
      "updatedAt": "2024-09-09T04:43:49.196Z",
      "chapterId": "00a572b2-4e61-4fd5-ad6c-0b3fabce910b"
    },
    {
      "id": "50e25817-05ce-45d8-ad2a-ecd7c7ba6259",
      "number": 21,
      "content": "Và những kẻ ghét người ngay lành sẽ bị kết tội.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:49.200Z",
      "updatedAt": "2024-09-09T04:43:49.200Z",
      "chapterId": "00a572b2-4e61-4fd5-ad6c-0b3fabce910b"
    },
    {
      "id": "d073f73f-c90c-41f8-bd36-942aa19d521c",
      "number": 22,
      "content": "Chúa cứu chuộc mạng sống các tôi tớ Ngài;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:49.205Z",
      "updatedAt": "2024-09-09T04:43:49.205Z",
      "chapterId": "00a572b2-4e61-4fd5-ad6c-0b3fabce910b"
    },
    {
      "id": "75189977-3e83-47e0-9fa3-3e0ffeba88f5",
      "number": 22,
      "content": "Không người nào nương cậy Ngài sẽ bị kết tội.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:43:49.210Z",
      "updatedAt": "2024-09-09T04:43:49.210Z",
