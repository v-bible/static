{
  "chapter": {
    "id": "3713bd25-acf7-47b6-8322-95a1b511f3d4",
    "number": 88,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2088&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.426Z",
    "updatedAt": "2024-07-24T15:07:48.333Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "9b310685-ade9-4fa4-9d56-02dc1137f564",
      "number": 1,
      "content": "Lạy Chúa, Ðức Chúa Trời, Ðấng Giải Cứu của con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.067Z",
      "updatedAt": "2024-07-31T11:41:21.239Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "2c5007ee-83e3-4cfe-a36f-74b85c6b0b42",
      "number": 1,
      "content": "Ngày và đêm con kêu cầu trước thánh nhan Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.067Z",
      "updatedAt": "2024-07-31T11:41:21.253Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "a2feb35d-75f6-4334-b73c-e9a6850879fb",
      "number": 2,
      "content": "Nguyện lời cầu nguyện của con thấu đến trước mặt Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.069Z",
      "updatedAt": "2024-07-31T11:41:21.261Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "b19990b5-de72-46dc-b76f-38fa9f26be92",
      "number": 2,
      "content": "Xin nghiêng tai Ngài nghe tiếng kêu cầu của con,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.069Z",
      "updatedAt": "2024-07-31T11:41:21.268Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "b9a40df0-c5d6-427e-b9ff-051bdc1d5198",
      "number": 3,
      "content": "Vì linh hồn con đang dẫy đầy hoạn nạn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.072Z",
      "updatedAt": "2024-07-31T11:41:21.276Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "61817829-25a9-432b-a3d4-0b2f0b54b572",
      "number": 3,
      "content": "Và mạng sống con đang cận kề âm phủ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.072Z",
      "updatedAt": "2024-07-31T11:41:21.283Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "98a263ba-0808-4473-bfde-c3b02cdae7b9",
      "number": 4,
      "content": "Con bị xem như kẻ sắp đi vào huyệt mả;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.074Z",
      "updatedAt": "2024-07-31T11:41:21.291Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "37696999-1b02-49e8-8b3b-21966b9990d0",
      "number": 4,
      "content": "Contrở thành một kẻ chẳng còn chút sức lực nào;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.074Z",
      "updatedAt": "2024-07-31T11:41:21.298Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "a3e8b3d4-aa8b-4804-94b9-dd483e48788b",
      "number": 5,
      "content": "Con bị lãng quên như những người đã chết,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.076Z",
      "updatedAt": "2024-07-31T11:41:21.306Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "53ae4e46-7ba3-438a-bc49-57ad1e08d768",
      "number": 5,
      "content": "Như những kẻ đã nằm trong phần mộ từ lâu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.076Z",
      "updatedAt": "2024-07-31T11:41:21.314Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "37465376-50a4-43c6-8c72-9f8c6cdf9426",
      "number": 5,
      "content": "Những kẻ Ngài không còn nhớ đến nữa,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.076Z",
      "updatedAt": "2024-07-31T11:41:21.321Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "50490677-36a0-499b-acc2-4879a071c81d",
      "number": 5,
      "content": "Và như những người đã bị dứt bỏ khỏi tay Ngài.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.076Z",
      "updatedAt": "2024-07-31T11:41:21.343Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "c531339b-0171-431c-9926-75066aa05f8f",
      "number": 6,
      "content": "Ngài đã bỏ con vào một huyệt sâu không đáy,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.079Z",
      "updatedAt": "2024-07-31T11:41:21.366Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "d34c51e5-2515-4c14-8d75-4f197cca3e8e",
      "number": 6,
      "content": "Vào chỗ cực kỳ tối tăm trong nơi tận cùng sâu thẳm.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.079Z",
      "updatedAt": "2024-07-31T11:41:21.388Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "422226dd-fc6e-4f9a-a066-d8c5341ccb16",
      "number": 7,
      "content": "Cơn thịnh nộ của Ngài đã giáng trên con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.081Z",
      "updatedAt": "2024-07-31T11:41:21.411Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "33abb839-8128-469e-91a7-ffcedb65dfeb",
      "number": 7,
      "content": "Ngài đã phủ trên con tất cả các đợt sóng của Ngài. (Sê-la)",
      "order": 1,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.081Z",
      "updatedAt": "2024-07-31T11:41:21.434Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "3c5f2126-d501-4933-9f05-0f4176cb2616",
      "number": 8,
      "content": "Ngài đã làm cho những người thân quen của con xa lánh con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.084Z",
      "updatedAt": "2024-07-31T11:41:21.468Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "1f293a66-f198-4028-85d1-5dbda3b13486",
      "number": 8,
      "content": "Ngài đã khiến con thành một đối tượng để họ ghê tởm;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.084Z",
      "updatedAt": "2024-07-31T11:41:21.478Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "6730f92f-0426-4f7a-8db2-87d783cb4612",
      "number": 8,
      "content": "Con đã bị cô lập và không còn lối thoát.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.084Z",
      "updatedAt": "2024-07-31T11:41:21.487Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "331afe9d-1739-431e-85e7-cc755213d3bd",
      "number": 9,
      "content": "Mắt con thẫn thờ vì sầu khổ;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.089Z",
      "updatedAt": "2024-07-31T11:41:21.497Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "77c04ea1-c4d2-42e1-b745-25dbe343f6bd",
      "number": 9,
      "content": "Chúa ôi, hằng ngày con kêu cầu Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.089Z",
      "updatedAt": "2024-07-31T11:41:21.506Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "1faf006e-7449-4dd5-8d86-a7272e0e612e",
      "number": 9,
      "content": "Con đưa đôi tay con lên và hướng về Ngài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.089Z",
      "updatedAt": "2024-07-31T11:41:21.516Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "921a1355-8b28-4ca0-86c0-ac6ae45adf0d",
      "number": 10,
      "content": "Chẳng lẽ Ngài sẽ thực hiện phép lạ cho những người đã chết sao?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.095Z",
      "updatedAt": "2024-07-31T11:41:21.527Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "250778b2-af6b-48cc-902f-14281ebb4961",
      "number": 10,
      "content": "Những linh hồn đã xuất ra rồi lẽ nào còn có thể trỗi dậy để ca ngợi Ngài sao?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.095Z",
      "updatedAt": "2024-07-31T11:41:21.645Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "e6a63491-4cdb-41c4-812f-5011a98961c6",
      "number": 10,
      "content": "(Sê-la)",
      "order": 2,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.095Z",
      "updatedAt": "2024-07-31T11:41:21.667Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "d647e76e-5da2-4971-8245-67850df4d241",
      "number": 11,
      "content": "Tình thương của Ngài có thể được công bố trong phần mộ sao?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.101Z",
      "updatedAt": "2024-07-31T11:41:21.675Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "716a088b-5392-48fc-9a44-601994b0ec51",
      "number": 11,
      "content": "Ðức thành tín của Ngài sẽ được giảng ra trong nơi người ta bị hủy diệt sao?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.101Z",
      "updatedAt": "2024-07-31T11:41:21.684Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "bd3da35b-bce8-48d7-9850-defa9e25cc2c",
      "number": 12,
      "content": "Cõi u minh làm sao biết được những việc diệu kỳ của Ngài?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.107Z",
      "updatedAt": "2024-07-31T11:41:21.693Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "bac287b6-f215-4c02-870f-c7dbc76e8104",
      "number": 12,
      "content": "Ðức công chính của Ngài há sẽ được biết đến trong cõi của những kẻ đã bị lãng quên sao?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.107Z",
      "updatedAt": "2024-07-31T11:41:21.701Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "7976f257-806d-4d87-8799-e17791159407",
      "number": 13,
      "content": "Còn con, Chúa ôi, con kêu cầu Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.113Z",
      "updatedAt": "2024-07-31T11:41:21.714Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "eee80cd4-e7d5-4ba7-9faf-0adb651ba2ad",
      "number": 13,
      "content": "Vừa sớm tinh sương con dâng lời cầu nguyện lên trước thánh nhan Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.113Z",
      "updatedAt": "2024-07-31T11:41:21.725Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "a74cea8b-f652-420c-ada5-939e8ce0487e",
      "number": 14,
      "content": "Chúa ôi, sao Ngài nỡ từ bỏ con?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.119Z",
      "updatedAt": "2024-07-31T11:41:21.733Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "e4b046cd-d759-4fd2-a377-3fd444c6b8db",
      "number": 14,
      "content": "Sao Ngài nỡ ẩn mặt Ngài khỏi con?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.119Z",
      "updatedAt": "2024-07-31T11:41:21.743Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "13c5138a-b30e-4cad-9c29-09a2f29f16c4",
      "number": 15,
      "content": "Con đã bị đau đớn và gần chết từ thời niên thiếu;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.124Z",
      "updatedAt": "2024-07-31T11:41:21.754Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "78f470ca-2c92-4b6d-95c7-001d35cec768",
      "number": 15,
      "content": "Con bị những trận đòn kinh hoàng của Ngài nên đâm ra tuyệt vọng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.124Z",
      "updatedAt": "2024-07-31T11:41:21.765Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "9c79b314-2ae6-46dd-bdf5-0fa5bddec3cc",
      "number": 16,
      "content": "Cơn giận phừng phừng của Ngài đã giáng trên con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.130Z",
      "updatedAt": "2024-07-31T11:41:21.775Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "e851c8ff-699d-49f4-a3f1-232873388940",
      "number": 16,
      "content": "Những cơn hãi hùng của Ngài đã tàn phá con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.130Z",
      "updatedAt": "2024-07-31T11:41:21.784Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "b97350a4-c742-4c25-964c-dbf5920f70eb",
      "number": 17,
      "content": "Những điều ấy bao phủ con như nước lụt tràn ngập cả ngày;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.136Z",
      "updatedAt": "2024-07-31T11:41:21.792Z",
      "chapterId": "3713bd25-acf7-47b6-8322-95a1b511f3d4"
    },
    {
      "id": "d1644680-de7e-4f17-aeaa-d65a0c98fa7b",
      "number": 17,
      "content": "Chúng cứ cùng nhau vây chặt lấy con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:02:16.136Z",
      "updatedAt": "2024-07-31T11:41:21.801Z",
