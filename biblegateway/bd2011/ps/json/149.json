{
  "chapter": {
    "id": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b",
    "number": 149,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20149&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.773Z",
    "updatedAt": "2024-07-24T15:07:49.600Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "b5921727-7342-41dc-803e-0c892c4289a0",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.931Z",
      "updatedAt": "2024-07-31T11:56:09.309Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "e531813e-0423-46d1-97dc-9244a2e14e4b",
      "number": 1,
      "content": "Hãy ca ngợi Chúa một bài ca mới.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.931Z",
      "updatedAt": "2024-07-31T11:56:09.329Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "c452d4bf-8fe0-4bc3-9fe1-ba8c73134bb3",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong đại hội của các thánh dân.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.931Z",
      "updatedAt": "2024-07-31T11:56:09.340Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "cea96f46-758d-400e-ac21-9f5a6e612a6a",
      "number": 2,
      "content": "Nguyện I-sơ-ra-ên vui mừng nơi Ðấng tạo nên mình.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.941Z",
      "updatedAt": "2024-07-31T11:56:09.349Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "d65ea3ad-488b-4e24-aefb-b38abb651ad0",
      "number": 2,
      "content": "Nguyện dân cư của Si-ôn vui vẻ nơi Vua mình.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.941Z",
      "updatedAt": "2024-07-31T11:56:09.357Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "e84b0ea8-3113-4b02-9003-4eb72a3bcb0e",
      "number": 3,
      "content": "Nguyện họ nhảy múa ca ngợi danh Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.944Z",
      "updatedAt": "2024-07-31T11:56:09.365Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "564fb13a-70e5-40d9-bb0a-88439287312c",
      "number": 3,
      "content": "Nguyện họ dùng trống và đàn ca tụng Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.944Z",
      "updatedAt": "2024-07-31T11:56:09.374Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "a85093a6-3623-4cb1-a527-8de76619ddca",
      "number": 4,
      "content": "Vì Chúa vui nơi con dân Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.946Z",
      "updatedAt": "2024-07-31T11:56:09.393Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "33f742f8-6c24-48a8-b9ee-fb0520b00c3c",
      "number": 4,
      "content": "Ngài điểm tô cho người khiêm nhường bằng ơn cứu rỗi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.946Z",
      "updatedAt": "2024-07-31T11:56:09.414Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "1496f28d-7400-41b3-90b1-5a6fc3c578bc",
      "number": 5,
      "content": "Nguyện các thánh đồ vui mừng trong vinh hạnh ấy.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.949Z",
      "updatedAt": "2024-07-31T11:56:09.436Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "d782423a-2fc9-4f43-a819-303d36d4dff2",
      "number": 5,
      "content": "Nguyện họ hát ca ngay cả khi ở trên giường.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.949Z",
      "updatedAt": "2024-07-31T11:56:09.457Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "b8334d61-dea8-4bf0-bc33-2d5c2daea7b6",
      "number": 6,
      "content": "Nguyện những lời ca ngợi Ðức Chúa Trời ở nơi miệng họ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.951Z",
      "updatedAt": "2024-07-31T11:56:09.479Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "2a551e7b-0f76-4433-b4ad-c0ab3c803b79",
      "number": 6,
      "content": "Và nguyện một thanh gươm hai lưỡi ở trong tay họ,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.951Z",
      "updatedAt": "2024-07-31T11:56:09.499Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "d9376d1a-f8ec-4eec-bf87-f88ce4c1cea9",
      "number": 7,
      "content": "Ðể họ báo thù các nước,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.953Z",
      "updatedAt": "2024-07-31T11:56:09.521Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "710222f3-805e-4add-986b-405ca469f693",
      "number": 7,
      "content": "Và trừng phạt các dân,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.953Z",
      "updatedAt": "2024-07-31T11:56:09.537Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "ab5c0791-9074-4eb5-92c2-ea79a8fd018c",
      "number": 8,
      "content": "Ðể trói các vua của chúng bằng xích,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.956Z",
      "updatedAt": "2024-07-31T11:56:09.547Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "58cc565e-037e-4468-a691-b6c90b1676fd",
      "number": 8,
      "content": "Và những kẻ quyền quý của chúng bằng xiềng sắt,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.956Z",
      "updatedAt": "2024-07-31T11:56:09.556Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "6766b566-d716-4c7e-9a3f-34eb56a1cd2c",
      "number": 9,
      "content": "Ðể thi hành án phạt đã ghi sẵn cho chúng.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.958Z",
      "updatedAt": "2024-07-31T11:56:09.563Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "8c0c3e3b-dd3d-454f-ad42-045cd8f03274",
      "number": 9,
      "content": "Ðó là niềm vinh dự cho tất cả thánh đồ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.958Z",
      "updatedAt": "2024-07-31T11:56:09.571Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    },
    {
      "id": "67f84893-060a-4178-a2d1-63f9a82a45e0",
      "number": 9,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:14:56.958Z",
      "updatedAt": "2024-07-31T11:56:09.578Z",
      "chapterId": "5a2d9add-b1fd-4a41-91eb-a8192743dc4b"
    }
  ],
