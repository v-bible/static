{
  "chapter": {
    "id": "f2df6fc9-9492-4e20-8817-400e48dce51a",
    "number": 149,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20149&version=BD2011",
    "createdAt": "2024-09-09T04:13:02.217Z",
