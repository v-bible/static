  },
  "verses": [
    {
      "id": "53362071-b38d-4acc-b607-2342c6fc47ff",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.189Z",
      "updatedAt": "2024-09-09T04:53:22.189Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "bd7ae5b3-a8b5-42b0-a56b-54857368d15a",
      "number": 9,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.240Z",
      "updatedAt": "2024-09-09T04:53:22.240Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "8f383e0f-5a35-4b27-bb37-28334fefdc1d",
      "number": 1,
      "content": "Hãy ca ngợi Chúa một bài ca mới.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.200Z",
      "updatedAt": "2024-09-09T04:53:22.200Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "99af8582-be32-4786-aa78-4ba5d088ae81",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong đại hội của các thánh dân.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.202Z",
      "updatedAt": "2024-09-09T04:53:22.202Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "101145b5-17a1-49e4-b26f-bb8f13b2d81c",
      "number": 2,
      "content": "Nguyện I-sơ-ra-ên vui mừng nơi Ðấng tạo nên mình.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.204Z",
      "updatedAt": "2024-09-09T04:53:22.204Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "7889eab9-f2c1-4474-9eeb-c44ef0c80ec7",
      "number": 2,
      "content": "Nguyện dân cư của Si-ôn vui vẻ nơi Vua mình.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.205Z",
      "updatedAt": "2024-09-09T04:53:22.205Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "ddb99271-0bb4-46c9-8205-4cde0075a172",
      "number": 3,
      "content": "Nguyện họ nhảy múa ca ngợi danh Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.207Z",
      "updatedAt": "2024-09-09T04:53:22.207Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "07bcf21d-f27a-4fa8-af77-d32ee06b1ca4",
      "number": 3,
      "content": "Nguyện họ dùng trống và đàn ca tụng Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.210Z",
      "updatedAt": "2024-09-09T04:53:22.210Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "cae08045-8d2b-41f5-833d-2325a162092d",
      "number": 4,
      "content": "Vì Chúa vui nơi con dân Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.212Z",
      "updatedAt": "2024-09-09T04:53:22.212Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "b998d5a8-4558-4de6-b6c7-8bcd386ba005",
      "number": 4,
      "content": "Ngài điểm tô cho người khiêm nhường bằng ơn cứu rỗi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.214Z",
      "updatedAt": "2024-09-09T04:53:22.214Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "52c5c0a4-0412-4e56-b68b-6499aaf7174a",
      "number": 5,
      "content": "Nguyện các thánh đồ vui mừng trong vinh hạnh ấy.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.216Z",
      "updatedAt": "2024-09-09T04:53:22.216Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "089188b6-86e4-4096-ae9c-c6d89980ab68",
      "number": 5,
      "content": "Nguyện họ hát ca ngay cả khi ở trên giường.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.218Z",
      "updatedAt": "2024-09-09T04:53:22.218Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "6c391869-f535-45ef-bea8-7aad8f9d9135",
      "number": 6,
      "content": "Nguyện những lời ca ngợi Ðức Chúa Trời ở nơi miệng họ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.220Z",
      "updatedAt": "2024-09-09T04:53:22.220Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "b177cc1d-d268-497a-b4e8-29c41b272641",
      "number": 6,
      "content": "Và nguyện một thanh gươm hai lưỡi ở trong tay họ,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.222Z",
      "updatedAt": "2024-09-09T04:53:22.222Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "7d039f82-a7b4-4599-ac1a-ba2f16feca84",
      "number": 7,
      "content": "Ðể họ báo thù các nước,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.225Z",
      "updatedAt": "2024-09-09T04:53:22.225Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "9f08e343-f85c-4924-b399-20618e8b27db",
      "number": 7,
      "content": "Và trừng phạt các dân,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.227Z",
      "updatedAt": "2024-09-09T04:53:22.227Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "1ee825be-5f4d-4fd9-8457-09fa32655c13",
      "number": 8,
      "content": "Ðể trói các vua của chúng bằng xích,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.229Z",
      "updatedAt": "2024-09-09T04:53:22.229Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "a6e1f377-6906-4080-bb23-f7d0a99f8aea",
      "number": 8,
      "content": "Và những kẻ quyền quý của chúng bằng xiềng sắt,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.232Z",
      "updatedAt": "2024-09-09T04:53:22.232Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "5a765278-5b72-4cac-8212-071ef4188ab7",
      "number": 9,
      "content": "Ðể thi hành án phạt đã ghi sẵn cho chúng.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.234Z",
      "updatedAt": "2024-09-09T04:53:22.234Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    },
    {
      "id": "42b93bf3-527b-414f-b021-7f3a9c2db8ac",
      "number": 9,
      "content": "Ðó là niềm vinh dự cho tất cả thánh đồ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:53:22.237Z",
      "updatedAt": "2024-09-09T04:53:22.237Z",
      "chapterId": "f2df6fc9-9492-4e20-8817-400e48dce51a"
    }
  ],
