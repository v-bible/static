{
  "chapter": {
    "id": "3ec5e6ae-2147-4125-981e-60718ae7802c",
    "number": 149,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20149&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.415Z",
