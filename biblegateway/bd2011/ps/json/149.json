  },
  "verses": [
    {
      "id": "e5d91bb5-1eb7-4fe5-85ae-a0f29b4fbcdd",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.510Z",
      "updatedAt": "2024-09-24T02:13:38.510Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "9fba3730-b259-4850-a0a2-7bc1c18ae729",
      "number": 9,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.573Z",
      "updatedAt": "2024-09-24T02:13:38.573Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "2019af22-bc26-43f8-9f91-d7acdee01de6",
      "number": 1,
      "content": "Hãy ca ngợi Chúa một bài ca mới.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.522Z",
      "updatedAt": "2024-09-24T02:13:38.522Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "16114b46-a071-4b9b-bf9b-58858b11ced6",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong đại hội của các thánh dân.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.524Z",
      "updatedAt": "2024-09-24T02:13:38.524Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "1c8c1736-d5ea-4f9d-ab32-21b9b8fc4448",
      "number": 2,
      "content": "Nguyện I-sơ-ra-ên vui mừng nơi Ðấng tạo nên mình.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.526Z",
      "updatedAt": "2024-09-24T02:13:38.526Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "4ca62f9b-4f39-4f38-8115-075dc56aee09",
      "number": 2,
      "content": "Nguyện dân cư của Si-ôn vui vẻ nơi Vua mình.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.528Z",
      "updatedAt": "2024-09-24T02:13:38.528Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "fda361d9-d851-467d-8563-c6eefd546fdf",
      "number": 3,
      "content": "Nguyện họ nhảy múa ca ngợi danh Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.530Z",
      "updatedAt": "2024-09-24T02:13:38.530Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "4cb766a0-260b-4a24-ae88-49cd7994cb92",
      "number": 3,
      "content": "Nguyện họ dùng trống và đàn ca tụng Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.532Z",
      "updatedAt": "2024-09-24T02:13:38.532Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "4bc1c9e3-96c9-448a-9a40-8fb92d6b7322",
      "number": 4,
      "content": "Vì Chúa vui nơi con dân Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.534Z",
      "updatedAt": "2024-09-24T02:13:38.534Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "f27a34b2-2523-4723-8d52-4b097e359eb4",
      "number": 4,
      "content": "Ngài điểm tô cho người khiêm nhường bằng ơn cứu rỗi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.535Z",
      "updatedAt": "2024-09-24T02:13:38.535Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "469f6877-285b-4805-89f8-9d1d3145c2f6",
      "number": 5,
      "content": "Nguyện các thánh đồ vui mừng trong vinh hạnh ấy.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.537Z",
      "updatedAt": "2024-09-24T02:13:38.537Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "802e75ae-8c3f-400a-ba7d-33607a9fa8f7",
      "number": 5,
      "content": "Nguyện họ hát ca ngay cả khi ở trên giường.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.539Z",
      "updatedAt": "2024-09-24T02:13:38.539Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "1ae60f01-8e68-4c97-8736-20150a55ccfd",
      "number": 6,
      "content": "Nguyện những lời ca ngợi Ðức Chúa Trời ở nơi miệng họ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.541Z",
      "updatedAt": "2024-09-24T02:13:38.541Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "eb7eb53b-73ee-4ba2-9d3e-9596dec8640c",
      "number": 6,
      "content": "Và nguyện một thanh gươm hai lưỡi ở trong tay họ,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.543Z",
      "updatedAt": "2024-09-24T02:13:38.543Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "722538f5-8bf4-41db-9bd5-e921db2d8fdf",
      "number": 7,
      "content": "Ðể họ báo thù các nước,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.545Z",
      "updatedAt": "2024-09-24T02:13:38.545Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "bb91be5d-8976-4de1-9166-299ef7ea8ee3",
      "number": 7,
      "content": "Và trừng phạt các dân,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.548Z",
      "updatedAt": "2024-09-24T02:13:38.548Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "ac893361-eb93-43a3-ac89-5d1ea7f861ef",
      "number": 8,
      "content": "Ðể trói các vua của chúng bằng xích,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.553Z",
      "updatedAt": "2024-09-24T02:13:38.553Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "a1f38d7b-117e-4cc0-99a4-6bd0e762e7c3",
      "number": 8,
      "content": "Và những kẻ quyền quý của chúng bằng xiềng sắt,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.558Z",
      "updatedAt": "2024-09-24T02:13:38.558Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "6b3b16f2-fbd3-4a29-bd23-97ab3c2461e3",
      "number": 9,
      "content": "Ðể thi hành án phạt đã ghi sẵn cho chúng.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.563Z",
      "updatedAt": "2024-09-24T02:13:38.563Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    },
    {
      "id": "c4a5f919-47d7-4f97-97e3-d0eac5a390ac",
      "number": 9,
      "content": "Ðó là niềm vinh dự cho tất cả thánh đồ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:38.568Z",
      "updatedAt": "2024-09-24T02:13:38.568Z",
      "chapterId": "3ec5e6ae-2147-4125-981e-60718ae7802c"
    }
  ],
