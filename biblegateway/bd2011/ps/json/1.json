{
  "chapter": {
    "id": "06a5eacf-ab17-484d-9ca5-704038da46ba",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%201&version=BD2011",
    "createdAt": "2024-07-12T04:03:45.502Z",
    "updatedAt": "2024-07-24T15:07:46.516Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "30a0572d-7761-440e-8715-171e29f2c291",
      "number": 1,
      "content": "Phước cho người không đi theo kế kẻ gian ác,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.002Z",
      "updatedAt": "2024-07-31T11:17:16.646Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "571076ae-095b-4559-b0a5-a4c5a1dace28",
      "number": 1,
      "content": "Không đứng chung đường với phường tội lỗi,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.002Z",
      "updatedAt": "2024-07-31T11:17:16.664Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "f1d125bb-27a1-4f3d-b25e-e12caedd8219",
      "number": 1,
      "content": "Không ngồi cùng chỗ với đồ nhạo báng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.002Z",
      "updatedAt": "2024-07-31T11:17:16.675Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "44848709-863a-4bf4-8247-30852ad96231",
      "number": 2,
      "content": "Nhưng vui thích về luật pháp của Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.011Z",
      "updatedAt": "2024-07-31T11:17:16.685Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "7b752f15-09cf-4262-8a4c-d8933ed09fd3",
      "number": 2,
      "content": "Và suy gẫm luật pháp ấy ngày và đêm.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.011Z",
      "updatedAt": "2024-07-31T11:17:16.697Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "43fddc6e-3ece-45ed-bc65-91b86fd3659d",
      "number": 3,
      "content": "Người ấy sẽ như cây trồng gần dòng nước,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.014Z",
      "updatedAt": "2024-07-31T11:17:16.708Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "2b156313-69db-40ee-86f8-dcaeda4d1bb7",
      "number": 3,
      "content": "Cứ đến mùa được hoa quả đầy cành,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.014Z",
      "updatedAt": "2024-07-31T11:17:16.720Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "36e9d774-4b84-4272-b280-caddf7d11f5b",
      "number": 3,
      "content": "Lá vẫn xanh chứ chẳng hề héo úa;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.014Z",
      "updatedAt": "2024-07-31T11:17:16.731Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "f2e3cad4-1c55-41b9-b5ef-4ca6995d6bc7",
      "number": 3,
      "content": "Mọi sự người ấy làm đều sẽ thành công.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.014Z",
      "updatedAt": "2024-07-31T11:17:16.740Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "630ea1d7-e230-442a-a43e-e2d2ee11dcb0",
      "number": 4,
      "content": "Những kẻ gian ác sẽ không được như vậy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.017Z",
      "updatedAt": "2024-07-31T11:17:16.752Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "df4a90c7-ae13-424b-a4d4-4b484cdd084e",
      "number": 4,
      "content": "Nhưng chúng chẳng khác gì rác rến bị gió đùa đi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.017Z",
      "updatedAt": "2024-07-31T11:17:16.764Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "74779000-014a-4f8a-ba69-f81e1c83560c",
      "number": 5,
      "content": "Vì thế những kẻ gian ác sẽ chẳng đứng nổi trong ngày phán xét;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.019Z",
      "updatedAt": "2024-07-31T11:17:16.773Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "a4d08f0f-442a-4774-8201-f24ba1a331ef",
      "number": 5,
      "content": "Những kẻ tội lỗi sẽ không được vào hội người ngay lành;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.019Z",
      "updatedAt": "2024-07-31T11:17:16.787Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "d8ba7826-8ff3-4764-a4f4-6bdeb8313990",
      "number": 6,
      "content": "Vì Chúa biết rõ đường lối người ngay lành,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.021Z",
      "updatedAt": "2024-07-31T11:17:16.797Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    },
    {
      "id": "40205fa1-2ed9-4aff-b096-8cc2434baead",
      "number": 6,
      "content": "Còn đường lối những kẻ gian ác sẽ dẫn đến diệt vong.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:42:49.021Z",
      "updatedAt": "2024-07-31T11:17:16.808Z",
      "chapterId": "06a5eacf-ab17-484d-9ca5-704038da46ba"
    }
  ],
