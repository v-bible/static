{
  "chapter": {
    "id": "15fce07a-78c8-4d7e-9b76-35cf9466458c",
    "number": 127,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20127&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.274Z",
    "updatedAt": "2024-07-24T15:07:49.157Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "e1e89563-f822-42c9-9965-50360092bcd4",
      "number": 1,
      "content": "Nếu Chúa không xây dựng nhà,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.934Z",
      "updatedAt": "2024-07-31T11:51:37.858Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    },
    {
      "id": "b43e9a2c-fcdb-4387-815b-da84464b7d0a",
      "number": 1,
      "content": "Những thợ xây dựng làm việc uổng công;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.934Z",
      "updatedAt": "2024-07-31T11:51:37.873Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    },
    {
      "id": "6c6239f0-8477-4f91-85ed-3c23338fe38b",
      "number": 1,
      "content": "Nếu Chúa không canh giữ thành,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.934Z",
      "updatedAt": "2024-07-31T11:51:37.880Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    },
    {
      "id": "dffe2b11-d29c-4fb4-9ea9-8d33516202cf",
      "number": 1,
      "content": "Những lính canh thức canh uổng công.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.934Z",
      "updatedAt": "2024-07-31T11:51:37.888Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    },
    {
      "id": "f90110ff-b1ec-444c-8977-f4be9fc89625",
      "number": 2,
      "content": "Uổng công thay cho các bạn là những người dậy sớm, thức khuya, và ăn uống kham khổ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.944Z",
      "updatedAt": "2024-07-31T11:51:37.896Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    },
    {
      "id": "ad0f87a5-2ed5-4e22-841a-48014383c632",
      "number": 2,
      "content": "Vì Chúa sẽ cho những người Ngài thương no đủ, ngay cả khi họ an giấc ngủ say.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.944Z",
      "updatedAt": "2024-07-31T11:51:37.905Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    },
    {
      "id": "7a229429-b78e-4267-aee6-66df796c06a8",
      "number": 3,
      "content": "Kìa, con cái là cơ nghiệp của Chúa ban cho;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.947Z",
      "updatedAt": "2024-07-31T11:51:37.915Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    },
    {
      "id": "b2df19bc-336c-4495-bcb6-37d3b5e9626e",
      "number": 3,
      "content": "Bông trái của tử cung là phần thưởng của Ngài trao tặng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.947Z",
      "updatedAt": "2024-07-31T11:51:37.922Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    },
    {
      "id": "be2beabd-ef6c-4ed7-85d6-c51634c1882b",
      "number": 4,
      "content": "Con cái sinh ra trong tuổi thanh xuân",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.950Z",
      "updatedAt": "2024-07-31T11:51:37.930Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    },
    {
      "id": "860b9721-386a-4cd2-b274-96086da23455",
      "number": 4,
      "content": "Khác nào các mũi tên trong tay dũng sĩ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.950Z",
      "updatedAt": "2024-07-31T11:51:37.938Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    },
    {
      "id": "c934a8b3-4dd1-4cac-8b7f-eaaa8ded9413",
      "number": 5,
      "content": "Phước cho người có đầy chúng trong ống tên mình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.952Z",
      "updatedAt": "2024-07-31T11:51:37.945Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    },
    {
      "id": "201857ff-de7e-4c0d-a3bc-9f6c3a6c799e",
      "number": 5,
      "content": "Họ sẽ không bẽ mặt khi đối đáp với kẻ thù tại cổng thành.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:10:28.952Z",
      "updatedAt": "2024-07-31T11:51:37.953Z",
      "chapterId": "15fce07a-78c8-4d7e-9b76-35cf9466458c"
    }
  ],
