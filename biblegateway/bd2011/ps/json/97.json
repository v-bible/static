{
  "chapter": {
    "id": "793eb5da-6052-476f-a429-aa6d13aedd3c",
    "number": 97,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2097&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.151Z",
    "updatedAt": "2024-09-09T04:13:01.151Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "82fc0882-e534-4982-bf6a-b42a32818e2f",
      "number": 1,
      "content": "Chúa trị vì!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.531Z",
      "updatedAt": "2024-09-09T04:49:02.531Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "74831bef-c45f-416f-a834-cbbb31969de1",
      "number": 1,
      "content": "Hỡi trái đất, hãy vui mừng;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.542Z",
      "updatedAt": "2024-09-09T04:49:02.542Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "df1c1956-4725-48db-8d83-b50ad5142477",
      "number": 1,
      "content": "Hỡi muôn ngàn hải đảo, hãy hân hoan.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.544Z",
      "updatedAt": "2024-09-09T04:49:02.544Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "3485020c-91ce-482e-88ba-f20a2689b188",
      "number": 2,
      "content": "Mây dày và bóng tối mù mịt bao quanh Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.546Z",
      "updatedAt": "2024-09-09T04:49:02.546Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "370518c4-35ae-4a7b-a75a-c2ddb58338a1",
      "number": 2,
      "content": "Công chính và công lý là nền tảng của ngai Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.548Z",
      "updatedAt": "2024-09-09T04:49:02.548Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "c6f3e9b6-51d0-430e-826f-0224da0af672",
      "number": 3,
      "content": "Lửa hừng đi trước mặt Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.550Z",
      "updatedAt": "2024-09-09T04:49:02.550Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "1b8009d1-c03a-43d0-97e6-3574f4302f5a",
      "number": 3,
      "content": "Thiêu hủy mọi kẻ chống nghịch chung quanh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.552Z",
      "updatedAt": "2024-09-09T04:49:02.552Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "70685f7c-5cc9-4980-b58c-ac746da014f6",
      "number": 4,
      "content": "Các tia chớp của Ngài lóe lên rực sáng khắp thế gian;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.554Z",
      "updatedAt": "2024-09-09T04:49:02.554Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "76b151ca-7d8e-4537-85e9-15e16a065cc1",
      "number": 4,
      "content": "Ðất thấy vậy run lên lẩy bẩy.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.555Z",
      "updatedAt": "2024-09-09T04:49:02.555Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "4e63c95d-dece-4c9b-9a6e-644e06d8a642",
      "number": 5,
      "content": "Các núi đồi tan chảy như sáp trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.557Z",
      "updatedAt": "2024-09-09T04:49:02.557Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "3b452c33-c276-461f-a384-cf3635c38066",
      "number": 5,
      "content": "Tức trước thánh nhan của Ðấng Chúa Tể toàn cầu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.559Z",
      "updatedAt": "2024-09-09T04:49:02.559Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "3649de3d-186a-4b1a-849c-84955cda8f91",
      "number": 6,
      "content": "Các tầng trời rao truyền đức công chính Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.561Z",
      "updatedAt": "2024-09-09T04:49:02.561Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "706cfbcc-379a-411f-ab66-077c1d21916e",
      "number": 6,
      "content": "Tất cả các dân đều thấy vinh quang Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.563Z",
      "updatedAt": "2024-09-09T04:49:02.563Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "b3f872d7-3843-4a49-b803-04c956c92067",
      "number": 7,
      "content": "Hãy để mọi kẻ thờ lạy các hình tượng bị xấu hổ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.565Z",
      "updatedAt": "2024-09-09T04:49:02.565Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "6215f998-f901-459e-ae13-c64e8f0adab1",
      "number": 7,
      "content": "Tức những kẻ khoe mình về các hình tượng vô tri của chúng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.567Z",
      "updatedAt": "2024-09-09T04:49:02.567Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "785bc9e1-e8d9-4b00-881d-f00629f0596d",
      "number": 7,
      "content": "Hỡi tất cả các thần, hãy sấp mình xuống thờ lạy Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.569Z",
      "updatedAt": "2024-09-09T04:49:02.569Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "2d2f1308-04fe-45f3-9d92-3eb65da0e85f",
      "number": 8,
      "content": "Si-ôn nghe thế bèn vui mừng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.570Z",
      "updatedAt": "2024-09-09T04:49:02.570Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "a06c7695-230c-48f8-980f-0088ddc32921",
      "number": 8,
      "content": "Các ái nữ của Giu-đa đều vui vẻ,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.572Z",
      "updatedAt": "2024-09-09T04:49:02.572Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "d343eee8-a85c-4cbd-9746-cb3d3ce3760e",
      "number": 8,
      "content": "Vì những phán quyết của Ngài, Chúa ôi;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.578Z",
      "updatedAt": "2024-09-09T04:49:02.578Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "152a92fe-d771-48b4-b0a7-bb9adffc06cf",
      "number": 9,
      "content": "Vì Ngài, Chúa ôi, là Ðấng Tối Cao trên khắp đất;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.580Z",
      "updatedAt": "2024-09-09T04:49:02.580Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "c5d25980-6b47-4ce2-84ee-cf703bfac867",
      "number": 9,
      "content": "Ngài vượt trỗi bỏ xa tất cả các thần.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.582Z",
      "updatedAt": "2024-09-09T04:49:02.582Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "5b613153-a17d-4340-92d8-6331d5c03c76",
      "number": 10,
      "content": "Hỡi những người yêu mến Chúa, hãy ghét điều ác;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.584Z",
      "updatedAt": "2024-09-09T04:49:02.584Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "882937a2-966a-44f8-a552-b0900ee67c4d",
      "number": 10,
      "content": "Ngài bảo vệ sinh mạng các thánh đồ của Ngài khỏi tay kẻ ác.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.586Z",
      "updatedAt": "2024-09-09T04:49:02.586Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "4d1ec06c-eadf-43a5-8ec6-10d0219dbc0a",
      "number": 11,
      "content": "Ánh sáng tỏa rạng trên người ngay lành,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.589Z",
      "updatedAt": "2024-09-09T04:49:02.589Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "63f8316b-56ef-48ab-908d-cd35223c6fec",
      "number": 11,
      "content": "Và niềm vui cho người có lòng ngay thẳng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.594Z",
      "updatedAt": "2024-09-09T04:49:02.594Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "9142e85c-fce7-4b6f-9b19-32ab83c5d9a8",
      "number": 12,
      "content": "Hỡi những người ngay lành, hãy vui mừng trong Chúa;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.600Z",
      "updatedAt": "2024-09-09T04:49:02.600Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    },
    {
      "id": "62959c80-5efa-417d-94b2-c21cc3e7cd35",
      "number": 12,
      "content": "Hãy dâng lòng biết ơn lên danh thánh của Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:49:02.604Z",
      "updatedAt": "2024-09-09T04:49:02.604Z",
      "chapterId": "793eb5da-6052-476f-a429-aa6d13aedd3c"
    }
  ],
