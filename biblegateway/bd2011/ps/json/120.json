{
  "chapter": {
    "id": "2cdd2a64-6f27-42b9-b34b-e607452da4f0",
    "number": 120,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20120&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.127Z",
    "updatedAt": "2024-07-24T15:07:48.995Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "5612845b-c8c1-472e-b999-edb9817c53ac",
      "number": 1,
      "content": "Trong cơn hoạn nạn tôi kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.721Z",
      "updatedAt": "2024-07-31T11:50:13.320Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "b05c888b-b62a-4462-9bf2-97c926fdf3e4",
      "number": 1,
      "content": "Và Ngài đã đáp lời tôi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.721Z",
      "updatedAt": "2024-07-31T11:50:13.328Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "0794c67a-a489-4a45-a723-1a2660369bb3",
      "number": 2,
      "content": "Chúa ôi, xin giải cứu con khỏi môi dối trá,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.732Z",
      "updatedAt": "2024-07-31T11:50:13.335Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "be145d3a-f422-4566-8e0f-71a48ff2ad69",
      "number": 2,
      "content": "Và khỏi lưỡi dối lừa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.732Z",
      "updatedAt": "2024-07-31T11:50:13.344Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "0d37e17e-71e6-4b2e-a8c5-269a88cccc16",
      "number": 3,
      "content": "Hỡi lưỡi dối trá, Ngài sẽ làm gì cho ngươi?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.737Z",
      "updatedAt": "2024-07-31T11:50:13.353Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "390eab18-2dbf-4425-86a9-a042d04423fa",
      "number": 3,
      "content": "Ngài sẽ làm gì nữa cho ngươi?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.737Z",
      "updatedAt": "2024-07-31T11:50:13.361Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "0de1ee54-90a7-4956-8b61-399cd5d6a1e3",
      "number": 4,
      "content": "Ngài sẽ dùng những tên nhọn của người dũng sĩ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.741Z",
      "updatedAt": "2024-07-31T11:50:13.368Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "96545946-c661-4c2a-bc98-22ec3dc34c73",
      "number": 4,
      "content": "Vàn hững than lửa hừng của bụi cây chổi cho ngươi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.741Z",
      "updatedAt": "2024-07-31T11:50:13.375Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "e1c3d936-7738-4c23-b278-fb548e941e16",
      "number": 5,
      "content": "Khốn thay cho tôi vì tôi trú ngụ ở Mê-séc,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.746Z",
      "updatedAt": "2024-07-31T11:50:13.385Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "096bd2bd-21f9-438a-9c85-d65d297f80ac",
      "number": 5,
      "content": "Vì tôi cư ngụ giữa các trại của Kê-đa.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.746Z",
      "updatedAt": "2024-07-31T11:50:13.392Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "a1cd0dac-ceb3-4a2b-bbb5-ada8b7089691",
      "number": 6,
      "content": "Linh hồn tôi đã sống quá lâu",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.751Z",
      "updatedAt": "2024-07-31T11:50:13.400Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "63821836-30ee-4ccd-ac92-42df3bfecdf5",
      "number": 6,
      "content": "với những kẻ ghét hòa bình.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.751Z",
      "updatedAt": "2024-07-31T11:50:13.408Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "fefa39f1-c686-43dc-8e15-692c8b49c2e9",
      "number": 7,
      "content": "Tôi yêu chuộng hòa bình,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.756Z",
      "updatedAt": "2024-07-31T11:50:13.415Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    },
    {
      "id": "5e796786-5e27-4b94-aefb-91664b07a20d",
      "number": 7,
      "content": "Nhưng khi tôi nói đến hòa bình, chúng chỉ muốn chiến tranh.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:08:52.756Z",
      "updatedAt": "2024-07-31T11:50:13.422Z",
      "chapterId": "2cdd2a64-6f27-42b9-b34b-e607452da4f0"
    }
  ],
