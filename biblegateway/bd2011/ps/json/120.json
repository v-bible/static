{
  "chapter": {
    "id": "bfc5e71f-606e-4f3f-95c3-536d4a838354",
    "number": 120,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20120&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.620Z",
    "updatedAt": "2024-09-09T04:13:01.620Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "1650134e-294f-4d30-8c22-cd1e3daf2a74",
      "number": 1,
      "content": "Trong cơn hoạn nạn tôi kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:58.959Z",
      "updatedAt": "2024-09-09T04:50:58.959Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "0526098e-d570-446b-9f1a-92b0c30a61eb",
      "number": 1,
      "content": "Và Ngài đã đáp lời tôi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:58.975Z",
      "updatedAt": "2024-09-09T04:50:58.975Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "81f26d13-ab27-4506-aedd-8d17c933d026",
      "number": 2,
      "content": "Chúa ôi, xin giải cứu con khỏi môi dối trá,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:58.980Z",
      "updatedAt": "2024-09-09T04:50:58.980Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "507b3f75-b856-449e-97ea-0d4eaeba9635",
      "number": 2,
      "content": "Và khỏi lưỡi dối lừa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:58.985Z",
      "updatedAt": "2024-09-09T04:50:58.985Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "5ef8312b-49b2-454c-9ddd-b31ed88799ef",
      "number": 3,
      "content": "Hỡi lưỡi dối trá, Ngài sẽ làm gì cho ngươi?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:58.989Z",
      "updatedAt": "2024-09-09T04:50:58.989Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "76ed9148-2999-463c-90aa-c248f5276e6c",
      "number": 3,
      "content": "Ngài sẽ làm gì nữa cho ngươi?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:58.994Z",
      "updatedAt": "2024-09-09T04:50:58.994Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "c325c2f7-2c2e-4570-ba45-2d8ef02e2149",
      "number": 4,
      "content": "Ngài sẽ dùng những tên nhọn của người dũng sĩ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:58.999Z",
      "updatedAt": "2024-09-09T04:50:58.999Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "77c880e7-f09b-4161-850c-209c41893811",
      "number": 4,
      "content": "Vàn hững than lửa hừng của bụi cây chổi cho ngươi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:59.003Z",
      "updatedAt": "2024-09-09T04:50:59.003Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "9e0472d0-a495-4f41-adb0-6f509989ebe6",
      "number": 5,
      "content": "Khốn thay cho tôi vì tôi trú ngụ ở Mê-séc,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:59.008Z",
      "updatedAt": "2024-09-09T04:50:59.008Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "caae81f9-5117-4f04-8d3e-0dab82256a6c",
      "number": 5,
      "content": "Vì tôi cư ngụ giữa các trại của Kê-đa.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:59.012Z",
      "updatedAt": "2024-09-09T04:50:59.012Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "576f7a61-ad3b-4bc9-85c8-aebbdb16c209",
      "number": 6,
      "content": "Linh hồn tôi đã sống quá lâu",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:59.017Z",
      "updatedAt": "2024-09-09T04:50:59.017Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "326593b8-bfd6-4cbc-ae65-46a30cb52e9d",
      "number": 6,
      "content": "với những kẻ ghét hòa bình.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:59.021Z",
      "updatedAt": "2024-09-09T04:50:59.021Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "5012cda8-f47c-4559-862b-c6bec1551647",
      "number": 7,
      "content": "Tôi yêu chuộng hòa bình,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:59.026Z",
      "updatedAt": "2024-09-09T04:50:59.026Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    },
    {
      "id": "e1f0b046-f82b-4bfd-be60-7ec4b479ff71",
      "number": 7,
      "content": "Nhưng khi tôi nói đến hòa bình, chúng chỉ muốn chiến tranh.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:59.031Z",
      "updatedAt": "2024-09-09T04:50:59.031Z",
      "chapterId": "bfc5e71f-606e-4f3f-95c3-536d4a838354"
    }
  ],
