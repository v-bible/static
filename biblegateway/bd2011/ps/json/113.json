{
  "chapter": {
    "id": "b3daa9cf-695b-436f-bd89-0bf959bdadf8",
    "number": 113,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20113&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.982Z",
    "updatedAt": "2024-07-24T15:07:48.857Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "b186414b-2c11-48c4-823d-96d3cfd3816f",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.838Z",
      "updatedAt": "2024-07-31T11:48:41.312Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "dff63bef-f2aa-4b18-b2af-5cc692081e52",
      "number": 1,
      "content": "Hỡi các tôi tớ Chúa, hãy ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.838Z",
      "updatedAt": "2024-07-31T11:48:41.320Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "035c06b1-46b3-4fa3-a160-54d2b0113d42",
      "number": 1,
      "content": "Hãy ca ngợi danh Chúa.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.838Z",
      "updatedAt": "2024-07-31T11:48:41.328Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "3b0bb655-217e-4a55-b388-716494069523",
      "number": 2,
      "content": "Hãy chúc tụng danh Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.848Z",
      "updatedAt": "2024-07-31T11:48:41.336Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "5fe067f0-12e0-4139-b771-37c8802df427",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.848Z",
      "updatedAt": "2024-07-31T11:48:41.344Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "25613221-b797-45dc-a043-749086db4a21",
      "number": 3,
      "content": "Từ bình minh cho đến hoàng hôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.850Z",
      "updatedAt": "2024-07-31T11:48:41.352Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "8a521890-a48d-41da-9034-10929d391175",
      "number": 3,
      "content": "Danh Chúa phải được tôn vinh ca ngợi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.850Z",
      "updatedAt": "2024-07-31T11:48:41.361Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "d0cebc2c-fb38-4dfe-8204-0e45d6037258",
      "number": 4,
      "content": "Chúa là Ðấng siêu việt hơn tất cả các dân các nước;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.853Z",
      "updatedAt": "2024-07-31T11:48:41.368Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "543c2aeb-124b-4ab5-9fe6-2970a07e2222",
      "number": 4,
      "content": "Vinh quang Ngài trỗi cao hơn các tầng trời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.853Z",
      "updatedAt": "2024-07-31T11:48:41.376Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "09e56157-0d1d-4a9d-a8b9-306a8367c2cd",
      "number": 5,
      "content": "Có ai giống như Chúa, Ðức Chúa Trời chúng ta,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.855Z",
      "updatedAt": "2024-07-31T11:48:41.386Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "cd10904d-c5c0-4da9-b34d-ace1109b4470",
      "number": 5,
      "content": "Ðấng ngự trên ngai cao cả,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.855Z",
      "updatedAt": "2024-07-31T11:48:41.394Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "19b85132-cc81-46d9-8618-a767c84d3e72",
      "number": 6,
      "content": "Lại tự hạ mình xuống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.857Z",
      "updatedAt": "2024-07-31T11:48:41.401Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "e12d5d41-77c3-452a-8ae6-0ee0295c5580",
      "number": 6,
      "content": "Ðể đoái xem mọi vật trong bầu trời và trên mặt đất chăng?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.857Z",
      "updatedAt": "2024-07-31T11:48:41.409Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "584f7acd-8f23-4bc7-a5ed-6928e0468608",
      "number": 7,
      "content": "Ngài nâng đỡ kẻ nghèo khổ ra khỏi chốn bụi tro,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.859Z",
      "updatedAt": "2024-07-31T11:48:41.416Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "9b37ccf3-8288-4780-b317-c9fbe6924225",
      "number": 7,
      "content": "Và nhấc đưa kẻ khó nghèo ra khỏi gò rác rến.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.859Z",
      "updatedAt": "2024-07-31T11:48:41.423Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "43ca980e-7ed7-4acf-aa27-b02978023853",
      "number": 8,
      "content": "Ngài đặt họ ngồi ngang hàng với các bậc quan quyền,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.861Z",
      "updatedAt": "2024-07-31T11:48:41.431Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "87dc31c5-e090-4cf3-adeb-2e419cc248a9",
      "number": 8,
      "content": "Tức với những người quyền thế của con dân Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.861Z",
      "updatedAt": "2024-07-31T11:48:41.439Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "2fb2e7b1-1e79-44f9-a955-d68feb0785b1",
      "number": 9,
      "content": "Ngài làm cho người đàn bà son sẻ sống quạnh quẽ trong nhà",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.864Z",
      "updatedAt": "2024-07-31T11:48:41.446Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "72db28ff-3044-47da-ab52-8f1eddf46103",
      "number": 9,
      "content": "Thành người mẹ hiền vui vẻ của một đàn con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.864Z",
      "updatedAt": "2024-07-31T11:48:41.454Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    },
    {
      "id": "22becb17-ef87-48a6-a74c-0690b0d3d0c6",
      "number": 9,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:07:26.864Z",
      "updatedAt": "2024-07-31T11:48:41.462Z",
      "chapterId": "b3daa9cf-695b-436f-bd89-0bf959bdadf8"
    }
  ],
