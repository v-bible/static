    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "8fb70840-3583-4fa7-b0d3-dde6a10d7f8c",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.625Z",
      "updatedAt": "2024-09-09T04:50:22.625Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "2b57a46d-52fd-4522-99f0-9648a026ad0c",
      "number": 9,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.733Z",
      "updatedAt": "2024-09-09T04:50:22.733Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "81055a86-1461-4679-ada6-9722bf1b61f7",
      "number": 1,
      "content": "Hỡi các tôi tớ Chúa, hãy ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.643Z",
      "updatedAt": "2024-09-09T04:50:22.643Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "e2736468-015b-496e-b6bb-e917cdd0aeb5",
      "number": 1,
      "content": "Hãy ca ngợi danh Chúa.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.648Z",
      "updatedAt": "2024-09-09T04:50:22.648Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "987fdf83-c0b7-4f83-a33e-73d375d4f21f",
      "number": 2,
      "content": "Hãy chúc tụng danh Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.652Z",
      "updatedAt": "2024-09-09T04:50:22.652Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "e8fc1de0-702a-48c4-980c-90beb2f3e568",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.657Z",
      "updatedAt": "2024-09-09T04:50:22.657Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "440514b4-e8aa-4c31-a2b3-8664cfe850d3",
      "number": 3,
      "content": "Từ bình minh cho đến hoàng hôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.662Z",
      "updatedAt": "2024-09-09T04:50:22.662Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "c944b562-6732-4ae1-86b6-057c7cd0cee3",
      "number": 3,
      "content": "Danh Chúa phải được tôn vinh ca ngợi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.666Z",
      "updatedAt": "2024-09-09T04:50:22.666Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "496ee25f-09c7-4207-a1db-fa5742d4698c",
      "number": 4,
      "content": "Chúa là Ðấng siêu việt hơn tất cả các dân các nước;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.671Z",
      "updatedAt": "2024-09-09T04:50:22.671Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "d836721c-3837-44a1-9f4c-4cab680698e3",
      "number": 4,
      "content": "Vinh quang Ngài trỗi cao hơn các tầng trời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.676Z",
      "updatedAt": "2024-09-09T04:50:22.676Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "c86c1fe7-615b-474d-8ae0-516bc6b4aabe",
      "number": 5,
      "content": "Có ai giống như Chúa, Ðức Chúa Trời chúng ta,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.681Z",
      "updatedAt": "2024-09-09T04:50:22.681Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "4e38b03c-a3e1-4a8d-9e20-b9b90dff2512",
      "number": 5,
      "content": "Ðấng ngự trên ngai cao cả,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.686Z",
      "updatedAt": "2024-09-09T04:50:22.686Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "8e4ec4ea-b9ac-4fb9-9db3-ce519928637a",
      "number": 6,
      "content": "Lại tự hạ mình xuống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.691Z",
      "updatedAt": "2024-09-09T04:50:22.691Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "2e894afd-3fa6-485d-839b-5a352ee8d214",
      "number": 6,
      "content": "Ðể đoái xem mọi vật trong bầu trời và trên mặt đất chăng?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.696Z",
      "updatedAt": "2024-09-09T04:50:22.696Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "3162b8fc-01f8-4ea0-9285-6790a4a663f8",
      "number": 7,
      "content": "Ngài nâng đỡ kẻ nghèo khổ ra khỏi chốn bụi tro,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.702Z",
      "updatedAt": "2024-09-09T04:50:22.702Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "8f4e3e2c-63d9-4d76-9f1a-d962bcc793db",
      "number": 7,
      "content": "Và nhấc đưa kẻ khó nghèo ra khỏi gò rác rến.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.707Z",
      "updatedAt": "2024-09-09T04:50:22.707Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "6c348b53-5874-4cf6-9ffe-01dd3cf7a9c3",
      "number": 8,
      "content": "Ngài đặt họ ngồi ngang hàng với các bậc quan quyền,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.712Z",
      "updatedAt": "2024-09-09T04:50:22.712Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "f8c52e54-4c71-4995-9901-faaaa25a9039",
      "number": 8,
      "content": "Tức với những người quyền thế của con dân Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.718Z",
      "updatedAt": "2024-09-09T04:50:22.718Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "ce619da0-5899-47fb-9597-aa389cf62916",
      "number": 9,
      "content": "Ngài làm cho người đàn bà son sẻ sống quạnh quẽ trong nhà",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.723Z",
      "updatedAt": "2024-09-09T04:50:22.723Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    },
    {
      "id": "da41fdc7-fd23-4889-9ba5-44b998429239",
      "number": 9,
      "content": "Thành người mẹ hiền vui vẻ của một đàn con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:50:22.728Z",
      "updatedAt": "2024-09-09T04:50:22.728Z",
      "chapterId": "5b18e4ba-7759-417f-9409-d794e9c50c3c"
    }
  ],
