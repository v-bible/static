{
  "chapter": {
    "id": "5b18e4ba-7759-417f-9409-d794e9c50c3c",
    "number": 113,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20113&version=BD2011",
    "createdAt": "2024-09-09T04:13:01.482Z",
