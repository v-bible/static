{
  "chapter": {
    "id": "94625ce2-f91a-408f-a834-3dd23ec1922b",
    "number": 68,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2068&version=BD2011",
    "createdAt": "2024-07-12T04:03:46.994Z",
    "updatedAt": "2024-07-24T15:07:47.911Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "97260f98-073a-4eb7-b23a-0cb9e61d4dd9",
      "number": 1,
      "content": "Nguyện Ðức Chúa Trời trỗi dậy;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.210Z",
      "updatedAt": "2024-07-31T11:37:16.247Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "ce2c31de-c638-4ac5-927e-108b2b7b8951",
      "number": 1,
      "content": "Nguyện những kẻ chống nghịch Ngài tản lạc;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.210Z",
      "updatedAt": "2024-07-31T11:37:16.284Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "1657d4fe-6a0e-4b4c-b677-2670a9a39774",
      "number": 1,
      "content": "Nguyện những kẻ ghét Ngài chạy trốn trước mặt Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.210Z",
      "updatedAt": "2024-07-31T11:37:16.297Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "961e6618-193e-4ea9-a378-4726c823fcd4",
      "number": 2,
      "content": "Như khói bị tan biến thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.220Z",
      "updatedAt": "2024-07-31T11:37:16.306Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "950740db-5de9-4d50-aedd-d185829688e6",
      "number": 2,
      "content": "Nguyện chúng bị tan đi thể ấy;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.220Z",
      "updatedAt": "2024-07-31T11:37:16.316Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "382026df-82b7-457b-a7e0-10f1500aefbe",
      "number": 2,
      "content": "Như sáp bị tan chảy trước ngọn lửa thể nào,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.220Z",
      "updatedAt": "2024-07-31T11:37:16.326Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "b3a8f225-d9ae-44e5-9baa-8bd1bd003913",
      "number": 2,
      "content": "Nguyện những kẻ ác bị diệt mất trước mặt Ðức Chúa Trời thể ấy.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.220Z",
      "updatedAt": "2024-07-31T11:37:16.336Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "513c060b-95cb-478d-be8d-73ab730568e4",
      "number": 3,
      "content": "Nguyện những người ngay lành hân hoan,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.226Z",
      "updatedAt": "2024-07-31T11:37:16.344Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "9dcce1f7-8813-4b10-ac2c-04432a56f774",
      "number": 3,
      "content": "Nguyện họ vui vẻ trước mặt Ðức Chúa Trời;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.226Z",
      "updatedAt": "2024-07-31T11:37:16.353Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "a0b245df-4cca-416a-9187-b0ef7fa5a917",
      "number": 3,
      "content": "Thật vậy nguyện họ hớn hở và vui mừng.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.226Z",
      "updatedAt": "2024-07-31T11:37:16.362Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "737fd5be-c275-42e6-a247-a251a5159575",
      "number": 4,
      "content": "Hãy ca ngợi Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.230Z",
      "updatedAt": "2024-07-31T11:37:16.373Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "962f0766-6860-43bf-b221-710ce691e82e",
      "number": 4,
      "content": "Hãy hát ca chúc tụng danh Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.230Z",
      "updatedAt": "2024-07-31T11:37:16.382Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "150814df-457c-45ee-a10f-36d30c8c55e7",
      "number": 4,
      "content": "Khá tôn ngợi Ðấng ngự trên các tầng mây;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.230Z",
      "updatedAt": "2024-07-31T11:37:16.393Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "415ceac5-8e56-48cc-9826-960f91c0df98",
      "number": 4,
      "content": "Danh Ngài là Chúa;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.230Z",
      "updatedAt": "2024-07-31T11:37:16.404Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "a31a0f1e-8c23-4299-b34c-bdb87207ca48",
      "number": 4,
      "content": "Khá reo vui trước mặt Ngài.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.230Z",
      "updatedAt": "2024-07-31T11:37:16.413Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "8547559a-2953-48f6-b9e1-0a458af91903",
      "number": 5,
      "content": "Cha kẻ mồ côi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.234Z",
      "updatedAt": "2024-07-31T11:37:16.423Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "37917e9a-a815-4928-90ea-a2af5fe782c5",
      "number": 5,
      "content": "Ðấng binh vực người góa bụa,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.234Z",
      "updatedAt": "2024-07-31T11:37:16.431Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "00297c15-3048-4deb-8767-3fcd61250539",
      "number": 5,
      "content": "Chính là Ðức Chúa Trời,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.234Z",
      "updatedAt": "2024-07-31T11:37:16.440Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "1bc84b6e-d60b-4015-a460-313601684703",
      "number": 5,
      "content": "Ðấng ngự trong nơi thánh của Ngài.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.234Z",
      "updatedAt": "2024-07-31T11:37:16.449Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "cd07e2b0-3517-42f4-9cb3-369389a5049e",
      "number": 6,
      "content": "Ðức Chúa Trời là Ðấng làm cho kẻ cô đơn có mái ấm gia đình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.238Z",
      "updatedAt": "2024-07-31T11:37:16.458Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "e5dae300-5c65-4617-a9f0-0aab5724debd",
      "number": 6,
      "content": "Ngài dẫn đưa những kẻ bị tù đày đến nơi sung mãn;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.238Z",
      "updatedAt": "2024-07-31T11:37:16.466Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "26fa33f7-635e-4e60-8777-7f3ce35f3f1e",
      "number": 6,
      "content": "Còn những kẻ bội nghịch phải ở trong xứ khô khan.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.238Z",
      "updatedAt": "2024-07-31T11:37:16.474Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "1235e19f-ff51-410a-9c15-662d17a6985f",
      "number": 7,
      "content": "Ðức Chúa Trời ôi, khi Ngài đi ra trước mặt con dân Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.241Z",
      "updatedAt": "2024-07-31T11:37:16.486Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "e39ba0c0-98e6-413c-a9ff-bb276cb18f24",
      "number": 7,
      "content": "Khi Ngài đi qua sa mạc, (Sê-la)",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.241Z",
      "updatedAt": "2024-07-31T11:37:16.494Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "52869e0c-b6c8-4afb-bbca-3c3f4c4408d3",
      "number": 8,
      "content": "Ðất rúng động, các tầng trời ào ạt đổ cơn mưa,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.243Z",
      "updatedAt": "2024-07-31T11:37:16.503Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "e2e27968-3510-4d85-9f3a-625871a767b9",
      "number": 8,
      "content": "Trước mặt Ðức Chúa Trời, Thần của Núi Si-nai,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.243Z",
      "updatedAt": "2024-07-31T11:37:16.512Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "07e1d7d7-f016-4055-8d02-376ad4b3e5bb",
      "number": 8,
      "content": "Trước mặt Ðức Chúa Trời, Thần của I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.243Z",
      "updatedAt": "2024-07-31T11:37:16.521Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "c5c808b2-e855-4570-b723-ee68b904ff19",
      "number": 9,
      "content": "Ðức Chúa Trời ôi, Ngài đã ban mưa xuống cách dồi dào,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.246Z",
      "updatedAt": "2024-07-31T11:37:16.529Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "97bec6fe-e9c6-4ab8-9dad-8799af1a14c9",
      "number": 9,
      "content": "Khi sản nghiệp của Ngài bị khô khan, Ngài đã bồi bổ nó lại.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.246Z",
      "updatedAt": "2024-07-31T11:37:16.539Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "bb67e2ea-8181-4cfc-bffc-9c74a86e376d",
      "number": 10,
      "content": "Con dân Ngài đã định cư trong sản nghiệp đó;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.248Z",
      "updatedAt": "2024-07-31T11:37:16.549Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "3d120fd0-af72-474e-a520-53a83503af76",
      "number": 10,
      "content": "Ðức Chúa Trời ôi, do lòng tốt của Ngài, Ngài đã chu cấp cho những người nghèo khó.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.248Z",
      "updatedAt": "2024-07-31T11:37:16.558Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "120608c2-d5ea-4a45-93a6-4a48cf3b26e7",
      "number": 11,
      "content": "Khi Chúa ban lệnh,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.251Z",
      "updatedAt": "2024-07-31T11:37:16.571Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "6790c1d8-67e2-457c-bef1-5820150f7bcb",
      "number": 11,
      "content": "Những kẻ loan truyền tin mừng ra đi đông vô số;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.251Z",
      "updatedAt": "2024-07-31T11:37:16.580Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "eb9c6077-4309-4cbd-bf92-16d468a10691",
      "number": 11,
      "content": "Họ báo rằng,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.251Z",
      "updatedAt": "2024-07-31T11:37:16.590Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "7983d835-a794-4505-ab1a-24458a39068a",
      "number": 12,
      "content": "“Các vua của các đạo quân đã bỏ chạy. Tất cả đã chạy trốn.”",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.253Z",
      "updatedAt": "2024-07-31T11:37:16.599Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "d5a8aa2f-0aa1-423f-b886-a6674d153ce4",
      "number": 12,
      "content": "Những phụ nữ ở nhà đều sẽ được chia các chiến lợi phẩm,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.253Z",
      "updatedAt": "2024-07-31T11:37:16.608Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "66f31397-3ed7-436d-bcb5-b1119e911366",
      "number": 13,
      "content": "Dù họ chỉ nằm nghỉ ở giữa các trại quân.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.257Z",
      "updatedAt": "2024-07-31T11:37:16.619Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "32594dee-5bad-4a77-99f8-939d3f2761a1",
      "number": 13,
      "content": "Kìa họ trông giống như những cánh bồ câu bọc bạc,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.257Z",
      "updatedAt": "2024-07-31T11:37:16.628Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "21ca4879-f5f6-4f0e-9420-3360c71237a1",
      "number": 13,
      "content": "Với bộ lông như vàng ròng lóng lánh.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.257Z",
      "updatedAt": "2024-07-31T11:37:16.637Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "3014c979-f970-4087-8d35-43b0d52de7bd",
      "number": 14,
      "content": "Khi Ðấng Toàn Năng đánh văng các vua trong xứ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.260Z",
      "updatedAt": "2024-07-31T11:37:16.646Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "7a9d3d51-acdd-4f67-adeb-eb654e9571ee",
      "number": 14,
      "content": "Họ như tuyết rơi tơi tả ở Xanh-môn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.260Z",
      "updatedAt": "2024-07-31T11:37:16.655Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "a4d7bfc1-9ec1-42c6-ad9f-cc4e2356a9e5",
      "number": 15,
      "content": "Núi Ba-san là núi của Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.262Z",
      "updatedAt": "2024-07-31T11:37:16.669Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "9b59c9b2-5c0f-4a65-bb8b-9efc10b15b68",
      "number": 15,
      "content": "Rặng Núi Ba-san quả có nhiều đỉnh cao chót vót.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.262Z",
      "updatedAt": "2024-07-31T11:37:16.678Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "21fef633-4a9e-47fb-8327-d3924c8036e3",
      "number": 16,
      "content": "Hỡi những rặng núi cao có nhiều đỉnh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.264Z",
      "updatedAt": "2024-07-31T11:37:16.687Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "72c913bf-0876-49f8-97ba-8ce22d3ed24d",
      "number": 16,
      "content": "Tại sao các ngươi ganh tị với ngọn núi Ðức Chúa Trời đã chọn để làm nơi Ngài ngự?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.264Z",
      "updatedAt": "2024-07-31T11:37:16.696Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "ffb2f71e-5bb4-42aa-a7dd-aec85a2d5b47",
      "number": 16,
      "content": "Thật vậy Chúa sẽ ngự tại đó đời đời.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.264Z",
      "updatedAt": "2024-07-31T11:37:16.705Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "5a82609a-882c-4aeb-a70d-c3bab9e722e0",
      "number": 17,
      "content": "Các xe chiến mã của Ðức Chúa Trời nhiều đến hằng vạn, thật là hằng ngàn và hằng vạn;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.267Z",
      "updatedAt": "2024-07-31T11:37:16.718Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "15cbf3ec-4b74-4907-a8d7-a10d6ab14f96",
      "number": 17,
      "content": "Chúa ngự giữa chúng như Ngài ngự trong nơi thánh của Ngài ở Si-nai.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.267Z",
      "updatedAt": "2024-07-31T11:37:16.727Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "161777af-cad0-4355-929d-73a8dadb304d",
      "number": 18,
      "content": "Ngài đã lên nơi cao,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.269Z",
      "updatedAt": "2024-07-31T11:37:16.737Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "766b1698-cec8-48e5-b52d-695e7972794b",
      "number": 18,
      "content": "Ngài dẫn theo đoàn tù binh bị bắt đem đi lưu đày;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.269Z",
      "updatedAt": "2024-07-31T11:37:16.745Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "03aad153-6935-443b-ac6a-109b3741ccaf",
      "number": 18,
      "content": "Ngài đã nhận những lễ vật của loài người,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.269Z",
      "updatedAt": "2024-07-31T11:37:16.756Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "be0cb4eb-01b5-4c28-847f-a6c3073929f3",
      "number": 18,
      "content": "Ngay cả những lễ vật của những kẻ bội nghịch,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.269Z",
      "updatedAt": "2024-07-31T11:37:16.765Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "58c8059b-d6c0-4c73-8fba-e82ff19169ca",
      "number": 18,
      "content": "Ðể chứng tỏ rằng Chúa Ðức Chúa Trời cũng ở tại đó.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.269Z",
      "updatedAt": "2024-07-31T11:37:16.773Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "9e686ada-99e3-40a9-8ae5-440e51ab1779",
      "number": 19,
      "content": "Tôn ngợi Chúa, Ðấng hằng ngày cưu mang chúng ta;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.271Z",
      "updatedAt": "2024-07-31T11:37:16.785Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "5ca71f06-6727-403e-892e-614e8da73699",
      "number": 19,
      "content": "Ðức Chúa Trời là Ðấng Giải Cứu của chúng ta. (Sê-la)",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.271Z",
      "updatedAt": "2024-07-31T11:37:16.793Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "20fdd03d-20bb-4eac-9e9e-a2f83ee51089",
      "number": 20,
      "content": "Ðức Chúa Trời chúng ta là Ðức Chúa Trời giải cứu;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.279Z",
      "updatedAt": "2024-07-31T11:37:16.801Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "2e0a9956-65aa-4007-a943-69c7233eb600",
      "number": 20,
      "content": "Nhờ Chúa Hằng Hữu, chúng ta được thoát chết.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.279Z",
      "updatedAt": "2024-07-31T11:37:16.809Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "a1dab387-d1fd-41e6-9b26-43f735451c40",
      "number": 21,
      "content": "Chắc chắn Ðức Chúa Trời sẽ đạp nát đầu kẻ thù của Ngài,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.317Z",
      "updatedAt": "2024-07-31T11:37:16.825Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "4fe148a6-20e1-4782-9baf-d16126225adc",
      "number": 21,
      "content": "Tức đầu tóc của những kẻ cứ miệt mài trong tội ác.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.317Z",
      "updatedAt": "2024-07-31T11:37:16.833Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "e86e5a3e-451b-4eaa-8b89-fef41accf437",
      "number": 22,
      "content": "Chúa phán, “Từ Ba-san, Ta sẽ đem chúng về;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.390Z",
      "updatedAt": "2024-07-31T11:37:16.842Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "3526380b-bff4-440e-b8f0-027ba7afd0e7",
      "number": 22,
      "content": "Từ các đại dương sâu thẳm, Ta sẽ mang chúng về,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.390Z",
      "updatedAt": "2024-07-31T11:37:16.851Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "19518e20-3af1-4052-8d22-22a2a0d3a4b7",
      "number": 23,
      "content": "Ðể các ngươi có thể giẫm chân trên máu kẻ thù của mình,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.401Z",
      "updatedAt": "2024-07-31T11:37:16.859Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "50f6c3e0-941d-47a1-baf1-06e5fa0c9fc5",
      "number": 23,
      "content": "Ngay cả lưỡi của chó các ngươi cũng sẽ được phần trên quân thù của các ngươi.”",
      "order": 1,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.401Z",
      "updatedAt": "2024-07-31T11:37:16.868Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "9163e7a9-153c-4542-aea8-149a3b28429b",
      "number": 24,
      "content": "Ðức Chúa Trời ôi, người ta đã chứng kiến lễ nghinh rước Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.403Z",
      "updatedAt": "2024-07-31T11:37:16.879Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "83955731-9816-4e4a-9a89-a4f44333965d",
      "number": 24,
      "content": "Tức lễ nghinh rước Ðức Chúa Trời của con, Vua của con, vào đền thánh.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.403Z",
      "updatedAt": "2024-07-31T11:37:16.888Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "ebce7c9b-53df-4980-a7e8-3062421d9924",
      "number": 25,
      "content": "Ca đoàn đi trước ngợi ca, ban nhạc theo sau trỗi nhạc,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.406Z",
      "updatedAt": "2024-07-31T11:37:16.917Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "67a34c4a-283f-4f8d-bd2e-f448e6e36161",
      "number": 25,
      "content": "Ở giữa là đoàn thiếu nữ đánh trống lục lạc nhịp nhàng.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.406Z",
      "updatedAt": "2024-07-31T11:37:16.927Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "01901800-d4d8-4ea0-82de-b075f9c4e003",
      "number": 26,
      "content": "Hãy chúc tụng Ðức Chúa Trời giữa những đoàn người đông đúc dự lễ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.408Z",
      "updatedAt": "2024-07-31T11:37:16.936Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "b237be63-2c89-4ece-8f9d-d1fe3f1f10dd",
      "number": 26,
      "content": "Hãy tôn ngợi Chúa, nguồn cội của I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.408Z",
      "updatedAt": "2024-07-31T11:37:16.945Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "bbfc931e-e3b3-48d7-b239-dfac138bb7fa",
      "number": 27,
      "content": "Kìa là Bên-gia-min, chi tộc trẻ nhất, đang dẫn đầu;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.410Z",
      "updatedAt": "2024-07-31T11:37:16.955Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "629ced8b-701e-4e2e-9814-fc5ebbc26ecd",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Giu-đa và phái đoàn của họ;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.410Z",
      "updatedAt": "2024-07-31T11:37:16.964Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "78bd850e-b58a-4b70-8845-a711968865bd",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Xê-bu-lun và các nhà quyền quý của Náp-ta-li.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.410Z",
      "updatedAt": "2024-07-31T11:37:16.973Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "ae2603ea-7e0c-4454-9062-8eeb3685528d",
      "number": 28,
      "content": "Ðức Chúa Trời ôi, xin thể hiện quyền năng Ngài;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.412Z",
      "updatedAt": "2024-07-31T11:37:16.984Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "1a4ab30a-e985-4f38-8e7c-27fa5418a79f",
      "number": 28,
      "content": "Ðức Chúa Trời ôi, xin bày tỏ sức mạnh Ngài,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.412Z",
      "updatedAt": "2024-07-31T11:37:16.994Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "e5e72240-615b-4b24-b39c-3a557f0f00ef",
      "number": 28,
      "content": "Như Ngài đã từng làm cho chúng con khi trước.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.412Z",
      "updatedAt": "2024-07-31T11:37:17.003Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "93e7d0ce-c5f9-4bd5-b13d-aca7247caf4b",
      "number": 29,
      "content": "Vì đền thờ Ngài tọa lạc tại Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.414Z",
      "updatedAt": "2024-07-31T11:37:17.011Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "a199e7c7-0417-4931-9d16-1f184de8fb7d",
      "number": 29,
      "content": "Nên các vua sẽ đem các lễ vật đến dâng lên Ngài tại đó.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.414Z",
      "updatedAt": "2024-07-31T11:37:17.020Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "9f30abaf-3377-4aff-83bc-b24dfe748109",
      "number": 30,
      "content": "Cầu xin Ngài trách phạt bầy thú hoang trong đám lau sậy,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.417Z",
      "updatedAt": "2024-07-31T11:37:17.029Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "4e4fd626-9c45-4ba1-9601-8683e5dfc733",
      "number": 30,
      "content": "Cùng đám bò đực ở giữa đàn bò con của các dân.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.417Z",
      "updatedAt": "2024-07-31T11:37:17.039Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "f212fe8e-968c-448b-b3f6-817e48cd89ae",
      "number": 30,
      "content": "Xin Ngài bắt chúng phải đem bạc nén đến quỳ dâng;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.417Z",
      "updatedAt": "2024-07-31T11:37:17.047Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "2b0b1b89-d12d-41c8-ad14-feed778afcb8",
      "number": 30,
      "content": "Còn những dân hiếu chiến, xin Ngài làm cho chúng bị tản lạc khắp nơi trên đất.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.417Z",
      "updatedAt": "2024-07-31T11:37:17.055Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "cdadf71c-234d-4e6f-8297-9c8e788e978e",
      "number": 31,
      "content": "Những đoàn người quyền thế sẽ từ Ai-cập đến thờ phượng;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.419Z",
      "updatedAt": "2024-07-31T11:37:17.064Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "5f71499f-d984-4b4b-8096-c37818e04a18",
      "number": 31,
      "content": "Người Ê-thi-ô-pi sẽ sớm đưa tay lên hướng về Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.419Z",
      "updatedAt": "2024-07-31T11:37:17.072Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "6ec8aa89-e8b0-4e3a-ae09-1360e5a4d96b",
      "number": 32,
      "content": "Hỡi tất cả quốc gia trên thế giới, hãy ca tụng Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.422Z",
      "updatedAt": "2024-07-31T11:37:17.083Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "3d7b4d4b-47a5-4842-a968-4cda4aa44728",
      "number": 32,
      "content": "Hãy ca ngợi Chúa, (Sê-la)",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.422Z",
      "updatedAt": "2024-07-31T11:37:17.092Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "9b41da06-92c5-4b82-acc3-a17b93b7aea2",
      "number": 33,
      "content": "Ðấng ngự trên nơi chí cao của các tầng trời,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.424Z",
      "updatedAt": "2024-07-31T11:37:17.100Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "708da64e-5e5a-40f2-a291-1647b776ea6e",
      "number": 33,
      "content": "Tức các tầng trời đã được dựng nên từ thời thái cổ;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.424Z",
      "updatedAt": "2024-07-31T11:37:17.108Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "85b6936b-61fe-46f5-8d63-c3586f0364a9",
      "number": 33,
      "content": "Kìa, Ngài cất tiếng nói,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.424Z",
      "updatedAt": "2024-07-31T11:37:17.117Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "1ef6863a-9c26-47a9-81f2-2fb3c9e98179",
      "number": 33,
      "content": "Giọng Ngài thật oai hùng.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.424Z",
      "updatedAt": "2024-07-31T11:37:17.125Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "29a41447-deff-4d80-924f-b6ea0ad779a2",
      "number": 34,
      "content": "Khá biết rằng mọi quyền lực đều thuộc về Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.426Z",
      "updatedAt": "2024-07-31T11:37:17.134Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "6dc84fb1-9fa9-4845-adee-bd72773578d7",
      "number": 34,
      "content": "Nguyện uy nghi Ngài tỏa rạng trên I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.426Z",
      "updatedAt": "2024-07-31T11:37:17.142Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "4a3f68ed-2049-4519-a5c6-30ed2837df36",
      "number": 34,
      "content": "Và quyền năng Ngài đầy dẫy khắp không trung.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.426Z",
      "updatedAt": "2024-07-31T11:37:17.167Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "adbfe9c5-b0d2-4b4c-aef3-356346301ab3",
      "number": 35,
      "content": "Ðức Chúa Trời ôi, trong đền thánh Ngài, Ngài đáng kinh hãi biết bao!",
      "order": 0,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.429Z",
      "updatedAt": "2024-07-31T11:37:17.189Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "01f45ea2-1039-4480-b20c-7d152fc16073",
      "number": 35,
      "content": "Chính Ðức Chúa Trời của I-sơ-ra-ên đã ban sức mạnh và quyền lực cho con dân Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.429Z",
      "updatedAt": "2024-07-31T11:37:17.200Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    },
    {
      "id": "51ca06be-5aab-4a36-9034-8202816b2c89",
      "number": 35,
      "content": "Tôn ngợi Ðức Chúa Trời!",
      "order": 2,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:56:56.429Z",
      "updatedAt": "2024-07-31T11:37:17.212Z",
      "chapterId": "94625ce2-f91a-408f-a834-3dd23ec1922b"
    }
  ],
