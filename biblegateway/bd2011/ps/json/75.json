{
  "chapter": {
    "id": "3a27feb7-fed7-46b1-b626-f448fd514087",
    "number": 75,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2075&version=BD2011",
    "createdAt": "2024-07-12T04:03:47.142Z",
    "updatedAt": "2024-07-24T15:07:48.050Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "a68627c8-ca33-4b86-86fe-05c1dad4d56b",
      "number": 1,
      "content": "Ðức Chúa Trời ôi, chúng con cảm tạ Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.611Z",
      "updatedAt": "2024-07-31T11:38:39.987Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "92bf9102-5bd0-4354-b6cb-427ec068b4f5",
      "number": 1,
      "content": "Chúng con cảm tạ Ngài vì danh Ngài ở gần,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.611Z",
      "updatedAt": "2024-07-31T11:38:40.016Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "15b2706e-b25c-4095-bd23-085940ed5d5b",
      "number": 1,
      "content": "Vì các việc diệu kỳ của Ngài luôn được truyền tụng cao rao.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.611Z",
      "updatedAt": "2024-07-31T11:38:40.038Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "00aa4440-b276-49ff-9bf7-4f07b791ad16",
      "number": 2,
      "content": "Khi đến thời điểm Ta đã định,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.614Z",
      "updatedAt": "2024-07-31T11:38:40.077Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "b047ff0d-650e-4dbd-a230-d1708c961ec5",
      "number": 2,
      "content": "Chính Ta sẽ xét xử công minh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.614Z",
      "updatedAt": "2024-07-31T11:38:40.089Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "0cf36429-324b-465e-800c-4852627b045c",
      "number": 3,
      "content": "Khi đất và mọi kẻ sống trong nó bị lung lay,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.616Z",
      "updatedAt": "2024-07-31T11:38:40.102Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "184950bd-e177-47cc-b4b4-023c654ecfd6",
      "number": 3,
      "content": "Chính Ta nắm giữ các trụ nó để nó được vững vàng. (Sê-la)",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.616Z",
      "updatedAt": "2024-07-31T11:38:40.110Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "519aef7d-81c6-43ab-8cd1-0e60fbfe42e1",
      "number": 4,
      "content": "Ta phán với những kẻ kiêu căng, “Chớ kiêu căng nữa!”",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.619Z",
      "updatedAt": "2024-07-31T11:38:40.119Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "f1efef59-e46b-4cff-a1e0-65974424a14c",
      "number": 4,
      "content": "Với những kẻ gian ác, “Chớ ngước sừng các ngươi lên;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.619Z",
      "updatedAt": "2024-07-31T11:38:40.126Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "b270528e-4f9e-4aee-aac3-03c1834bfb78",
      "number": 5,
      "content": "Chớ ngước sừng ngưỡng cổ dương oai;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.621Z",
      "updatedAt": "2024-07-31T11:38:40.134Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "08d02cf4-e107-45fb-a5c1-06111900a7c0",
      "number": 5,
      "content": "Chớ nói ra những lời kiêu căng xấc xược.”",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.621Z",
      "updatedAt": "2024-07-31T11:38:40.141Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "103482a1-e4a3-417e-908f-a1b9996bee41",
      "number": 6,
      "content": "Vì chẳng phải từ phương đông hoặc từ phương tây,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.623Z",
      "updatedAt": "2024-07-31T11:38:40.149Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "e8eb3495-c180-49ba-a884-0ad803a7302c",
      "number": 6,
      "content": "Hay từ miền sa mạc mà người ta được tôn trọng,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.623Z",
      "updatedAt": "2024-07-31T11:38:40.157Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "0ca0892c-c835-4b99-a889-d1a8fd862c9c",
      "number": 7,
      "content": "Nhưng ấy là từ Ðức Chúa Trời, Ðấng Ðoán Xét;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.626Z",
      "updatedAt": "2024-07-31T11:38:40.165Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "9cd0c0a3-e8bb-4335-8d2e-1644784f7b69",
      "number": 7,
      "content": "Ngài hạ kẻ này xuống và nhấc người kia lên.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.626Z",
      "updatedAt": "2024-07-31T11:38:40.173Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "b4a57edd-5447-4939-bce3-5026573c89f9",
      "number": 8,
      "content": "Trong tay Chúa có ly rượu sủi bọt;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.628Z",
      "updatedAt": "2024-07-31T11:38:40.180Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "d09a5bf6-e8d7-464d-83c9-1fbcd925680f",
      "number": 8,
      "content": "Ấy là một ly rượu pha, đỏ hồng, và cay đắng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.628Z",
      "updatedAt": "2024-07-31T11:38:40.188Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "319a4bfc-fa26-4401-88d5-0fd60b6d9105",
      "number": 8,
      "content": "Ngài rót nó ra và mọi kẻ ác trên đất bị bắt phải uống;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.628Z",
      "updatedAt": "2024-07-31T11:38:40.195Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "eb6a0733-189e-419d-bb83-7c2269320fdb",
      "number": 8,
      "content": "Chúng phải uống cho đến giọt rượu cuối cùng.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.628Z",
      "updatedAt": "2024-07-31T11:38:40.203Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "4c616bd4-0dcb-40f2-b008-db46103423cb",
      "number": 9,
      "content": "Riêng phần tôi, tôi sẽ ca tụng Ngài mãi mãi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.631Z",
      "updatedAt": "2024-07-31T11:38:40.211Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "75d2cdd3-2deb-4403-8670-8376c45d8a88",
      "number": 9,
      "content": "Tôi sẽ ca ngợi Ðức Chúa Trời của Gia-cốp.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.631Z",
      "updatedAt": "2024-07-31T11:38:40.218Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "c7baac7c-9567-41eb-86f5-60176c51e800",
      "number": 10,
      "content": "Vì Ngài sẽ chặt trụi các sừng của quân gian ác,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.633Z",
      "updatedAt": "2024-07-31T11:38:40.228Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    },
    {
      "id": "8d0557da-da2b-47bb-b0ea-308ff32e5ae5",
      "number": 10,
      "content": "Còn sừng của người ngay lành sẽ được nhấc cao lên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:59:40.633Z",
      "updatedAt": "2024-07-31T11:38:40.235Z",
      "chapterId": "3a27feb7-fed7-46b1-b626-f448fd514087"
    }
  ],
