{
  "chapter": {
    "id": "3b63344d-2aaf-4be4-856b-bac1c2a67a03",
    "number": 81,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2081&version=BD2011",
    "createdAt": "2024-09-09T04:13:00.820Z",
    "updatedAt": "2024-09-09T04:13:00.820Z",
    "bookId": "0d206502-6aae-452a-9312-2974be7c7aa6"
  },
  "verses": [
    {
      "id": "447717f6-1011-4f90-bb5c-254938b5a411",
      "number": 1,
      "content": "Hãy cất tiếng ca ngợi Ðức Chúa Trời, sức mạnh của chúng ta;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.767Z",
      "updatedAt": "2024-09-09T04:47:46.767Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "853ff995-0d0c-4a30-b6d3-52f0c5d292ed",
      "number": 1,
      "content": "Hãy lớn tiếng vui vẻ ca tụng Ðức Chúa Trời của Gia-cốp.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.779Z",
      "updatedAt": "2024-09-09T04:47:46.779Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "726bf0bb-0e9f-479a-801e-bd90302d3195",
      "number": 2,
      "content": "Hãy trỗi nhạc, hãy đánh trống,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.781Z",
      "updatedAt": "2024-09-09T04:47:46.781Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "130f4fd9-6e4a-485a-9c90-9e51bfda9c2d",
      "number": 2,
      "content": "Hãy dùng đàn lia và hạc cầm hòa tấu với nhau.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.783Z",
      "updatedAt": "2024-09-09T04:47:46.783Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "96d2a403-20aa-4b16-a6e1-5a3a6e869a66",
      "number": 3,
      "content": "Hãy thổi kèn vang lên vào Ngày Mồng Một,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.785Z",
      "updatedAt": "2024-09-09T04:47:46.785Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "a5f267e0-007e-4041-852b-cb528ce8dfba",
      "number": 3,
      "content": "Vào Ngày Rằm, và vào các ngày lễ trọng thể của chúng ta,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.789Z",
      "updatedAt": "2024-09-09T04:47:46.789Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "78354a4e-f525-4860-a45a-4000af4d8aaf",
      "number": 4,
      "content": "Vì ấy là một luật định cho I-sơ-ra-ên,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.793Z",
      "updatedAt": "2024-09-09T04:47:46.793Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "ddf0c620-b4c0-42fa-b838-285f10861959",
      "number": 4,
      "content": "Một mạng lịnh của Ðức Chúa Trời của Gia-cốp.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.795Z",
      "updatedAt": "2024-09-09T04:47:46.795Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "d2a75355-68ed-4eec-86a6-f2bf753a6881",
      "number": 5,
      "content": "Ngài đã lập điều ấy để làm một chứng tích giữa Giô-sép,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.797Z",
      "updatedAt": "2024-09-09T04:47:46.797Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "c35ad85e-3d58-4ce7-9e9c-1d92ae262fd5",
      "number": 5,
      "content": "Khi Ngài đi trừng phạt trên đất Ai-cập,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.798Z",
      "updatedAt": "2024-09-09T04:47:46.798Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "de2395fe-a171-4a59-9711-14863078d31a",
      "number": 5,
      "content": "Nơi tôi nghe một ngôn ngữ tôi chẳng hiểu.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.800Z",
      "updatedAt": "2024-09-09T04:47:46.800Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "b6f274c6-9dc4-489b-b5e7-3ddedf80bc61",
      "number": 6,
      "content": "“Ta đã cất gánh nặng của kẻ làm nô lệ khỏi vai nó;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.802Z",
      "updatedAt": "2024-09-09T04:47:46.802Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "6690fdb4-df2b-41b7-b345-92358c01f237",
      "number": 6,
      "content": "Ðôi tay nó đã khỏi bưng thúng giỏ của kẻ lao nô.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.805Z",
      "updatedAt": "2024-09-09T04:47:46.805Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "56b35686-94bc-487d-9de3-9b1ba7b64078",
      "number": 7,
      "content": "Trong cơn hoạn nạn ngươi đã kêu cầu, và Ta đã giải cứu ngươi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.807Z",
      "updatedAt": "2024-09-09T04:47:46.807Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "79de6737-bea4-4e52-bec7-c043da64f570",
      "number": 7,
      "content": "Từ trời cao huyền bí, nơi sấm sét phát ra, Ta đã đáp lời ngươi,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.809Z",
      "updatedAt": "2024-09-09T04:47:46.809Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "8add5933-5530-4aa8-9cb3-ee57c827100f",
      "number": 7,
      "content": "Ta đã thử lòng ngươi qua việc thiếu nước ở Mê-ri-ba. (Sê-la)",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.811Z",
      "updatedAt": "2024-09-09T04:47:46.811Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "c7734e8d-9278-46a0-8c78-a0c2878a3a20",
      "number": 8,
      "content": "Hỡi dân Ta, hãy nghe, Ta khuyến cáo các ngươi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.813Z",
      "updatedAt": "2024-09-09T04:47:46.813Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "4b0e2299-67ad-4bcc-814d-b9316bb9efce",
      "number": 8,
      "content": "Hỡi I-sơ-ra-ên, ước gì ngươi chịu nghe Ta!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.815Z",
      "updatedAt": "2024-09-09T04:47:46.815Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "c37aca91-9f42-4b53-b145-45cc95fb2898",
      "number": 9,
      "content": "Ở giữa ngươi chớ có thần lạ nào;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.817Z",
      "updatedAt": "2024-09-09T04:47:46.817Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "4aaf066d-7cb3-499d-bce2-833aadd86aaf",
      "number": 9,
      "content": "Ngươi chớ thờ lạy bất cứ thần nào khác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.820Z",
      "updatedAt": "2024-09-09T04:47:46.820Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "9f6e9fa7-e940-4001-8b38-9f68827d74b3",
      "number": 10,
      "content": "Ta là Chúa, Ðức Chúa Trời của ngươi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.822Z",
      "updatedAt": "2024-09-09T04:47:46.822Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "5ebe721e-057c-48af-9c71-a51d516bc8cf",
      "number": 10,
      "content": "Ðấng đã đem ngươi lên từ đất Ai-cập;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.827Z",
      "updatedAt": "2024-09-09T04:47:46.827Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "6fab0762-bbd5-44d1-aa13-941b6e3e2f5f",
      "number": 10,
      "content": "Hãy mở rộng miệng ngươi ra để Ta cho nó được no đầy.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.829Z",
      "updatedAt": "2024-09-09T04:47:46.829Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "53618cb0-74a3-4def-befb-7f1cadf019f4",
      "number": 11,
      "content": "Nhưng dân Ta chẳng nghe tiếng Ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.832Z",
      "updatedAt": "2024-09-09T04:47:46.832Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "a8878b59-2f1b-4607-be33-f1a35b45a9ac",
      "number": 11,
      "content": "I-sơ-ra-ên chẳng vâng lời Ta;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.835Z",
      "updatedAt": "2024-09-09T04:47:46.835Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "550649db-b632-4829-9b43-f9579a04637a",
      "number": 12,
      "content": "Nên Ta đã phó chúng cho tính bướng bỉnh của lòng chúng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.838Z",
      "updatedAt": "2024-09-09T04:47:46.838Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "be4b98c6-a80e-4059-a27e-f7a62e57c087",
      "number": 12,
      "content": "Ðể chúng cứ đi theo mưu ý của chúng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.841Z",
      "updatedAt": "2024-09-09T04:47:46.841Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "a0604dae-e05e-404f-9674-6388fde377b6",
      "number": 13,
      "content": "Ôi, ước gì dân Ta chịu nghe Ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.845Z",
      "updatedAt": "2024-09-09T04:47:46.845Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "620a94ed-f812-4e58-a3d2-1dc4dfda70ef",
      "number": 13,
      "content": "Ước gì I-sơ-ra-ên cứ đi trong đường lối Ta.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.848Z",
      "updatedAt": "2024-09-09T04:47:46.848Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "88dd17e0-de39-45e8-a0a7-f0eed357bf39",
      "number": 14,
      "content": "Chắc hẳn Ta đã mau lẹ triệt hạ những kẻ thù của chúng rồi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.851Z",
      "updatedAt": "2024-09-09T04:47:46.851Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "0350e52f-ac88-4272-a62b-25149954b168",
      "number": 14,
      "content": "Và dùng tay Ta đối phó những kẻ chống nghịch chúng rồi.”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.855Z",
      "updatedAt": "2024-09-09T04:47:46.855Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "efb120a0-33ef-46b8-bf89-50e7d6fedd3c",
      "number": 15,
      "content": "Nguyện những kẻ ghét Chúa phải phủ phục trước mặt Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.858Z",
      "updatedAt": "2024-09-09T04:47:46.858Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "6a6f8e73-9796-4318-9e93-0f9737a30e39",
      "number": 15,
      "content": "Và số phận của chúng phải như thế đời đời,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.862Z",
      "updatedAt": "2024-09-09T04:47:46.862Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "3225615d-64d2-4253-bd66-d62ca5216401",
      "number": 16,
      "content": "Và nguyện Ngài nuôi con dân Ngài bằng gạo thơm ngon nhất,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.865Z",
      "updatedAt": "2024-09-09T04:47:46.865Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    },
    {
      "id": "bed82922-55cc-4735-9db7-11d6fecce37b",
      "number": 16,
      "content": "Như lời Ngài, “Ta cho ngươi no thỏa bằng mật ngọt lấy ra từ vầng đá.”",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:47:46.869Z",
      "updatedAt": "2024-09-09T04:47:46.869Z",
      "chapterId": "3b63344d-2aaf-4be4-856b-bac1c2a67a03"
    }
  ],
