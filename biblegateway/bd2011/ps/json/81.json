  "headings": [
    {
      "id": "ff732b00-622b-4362-9e0b-cb537c66476d",
      "content": "Lòng Tốt của Ðức Chúa Trời Ðối Với Sự Lầm Lạc của Dân I-sơ-ra-ên",
      "order": 0,
      "createdAt": "2024-07-15T03:00:57.421Z",
      "updatedAt": "2024-07-15T03:00:57.421Z",
