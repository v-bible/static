{
  "chapter": {
    "id": "8b60c1a3-4818-46a5-abab-2eba9a893710",
    "number": 9,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%209&version=BD2011",
    "createdAt": "2024-07-12T04:03:45.668Z",
    "updatedAt": "2024-07-24T15:07:46.676Z",
    "bookId": "356e2ad5-52d3-4295-b594-7b36816b4c5e"
  },
  "verses": [
    {
      "id": "9ef92cee-4896-452e-bf7e-323099878941",
      "number": 1,
      "content": "Con sẽ hết lòng dâng lời cảm tạ Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.869Z",
      "updatedAt": "2024-07-31T11:18:57.173Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "d5c5ed25-1a96-4444-b7a1-f5f9b1e22373",
      "number": 1,
      "content": "Con sẽ kể ra mọi việc kỳ diệu của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.869Z",
      "updatedAt": "2024-07-31T11:18:57.181Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "5f5759b4-e2c8-45a5-a0ea-bbfa2c14db9a",
      "number": 2,
      "content": "Con sẽ hân hoan và hớn hở trong Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.879Z",
      "updatedAt": "2024-07-31T11:18:57.189Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "9294cba9-2910-44d2-a0f1-aabdc7d5df33",
      "number": 2,
      "content": "Con sẽ ca tụng danh Ngài, ôi Ðấng Tối Cao.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.879Z",
      "updatedAt": "2024-07-31T11:18:57.197Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "740fd796-19b3-4212-bf49-cbf6c72b884d",
      "number": 3,
      "content": "Khi những kẻ thù của con quay trở lại,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.882Z",
      "updatedAt": "2024-07-31T11:18:57.208Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "2be43939-a092-4d3e-adfd-5124bbdfd7cc",
      "number": 3,
      "content": "Chúng sẽ ngã xuống và bị diệt vong trước mặt Ngài,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.882Z",
      "updatedAt": "2024-07-31T11:18:57.216Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "ab2b86f4-416d-4c3b-a466-a97661bb1346",
      "number": 4,
      "content": "Vì Ngài binh vực quyền lợi của con và duyên cớ của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.884Z",
      "updatedAt": "2024-07-31T11:18:57.225Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "e80dbddf-08f8-41e0-821b-515cdaaddbd4",
      "number": 4,
      "content": "Ngài ngự trên ngai và phán xét công minh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.884Z",
      "updatedAt": "2024-07-31T11:18:57.233Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "acf88b8c-ef1b-4c28-9cec-b60c009846d9",
      "number": 5,
      "content": "Ngài quở trách các dân;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.886Z",
      "updatedAt": "2024-07-31T11:18:57.244Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "1640225e-e4d1-4c09-a973-19c5af54722c",
      "number": 5,
      "content": "Ngài hủy diệt những kẻ ác;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.886Z",
      "updatedAt": "2024-07-31T11:18:57.251Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "55b080d2-44c4-4e28-bdbf-c577ebce4a60",
      "number": 5,
      "content": "Ngài xóa tên chúng nó đời đời bất tận.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.886Z",
      "updatedAt": "2024-07-31T11:18:57.260Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "27aed6e7-9bdf-49cc-9488-44f30776ef2f",
      "number": 6,
      "content": "Kẻ thù của con đã bị kết liễu trong hoang tàn vĩnh viễn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.889Z",
      "updatedAt": "2024-07-31T11:18:57.267Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "ae2aab06-1457-48bd-8bf4-4b8f92552a34",
      "number": 6,
      "content": "Những thành trì mà Ngài đã diệt trừ,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.889Z",
      "updatedAt": "2024-07-31T11:18:57.275Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "731f7386-f55f-4ba2-a06d-bb3d4c37fbc1",
      "number": 6,
      "content": "Dù kỷ niệm về chúng cũng chẳng còn;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.889Z",
      "updatedAt": "2024-07-31T11:18:57.283Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "6a4fdd60-727d-4337-8eef-6c6f972449ec",
      "number": 7,
      "content": "Nhưng Chúa sẽ còn lại đời đời;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.891Z",
      "updatedAt": "2024-07-31T11:18:57.291Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "a39c6300-104c-44a7-a772-4845c454d4e4",
      "number": 7,
      "content": "Ngài đã lập ngôi Ngài để xét đoán.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.891Z",
      "updatedAt": "2024-07-31T11:18:57.299Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "3f87e62e-cc1d-47b0-b4bc-b86d1bd00c17",
      "number": 8,
      "content": "Ngài sẽ phán xét thế gian theo lẽ công chính;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.893Z",
      "updatedAt": "2024-07-31T11:18:57.308Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "b3884129-8a28-468b-9dec-48da62b39cb5",
      "number": 8,
      "content": "Ngài sẽ xét xử muôn dân theo lẽ công bình.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.893Z",
      "updatedAt": "2024-07-31T11:18:57.316Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "d046c366-6185-48a6-86fb-c5719b115246",
      "number": 9,
      "content": "Chúa là nơi nương náu cho những người bị hà hiếp;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.895Z",
      "updatedAt": "2024-07-31T11:18:57.327Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "abc4f19b-0336-47b0-8719-d476f967422c",
      "number": 9,
      "content": "Ngài là chỗ ẩn núp trong lúc bị nguy nan.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.895Z",
      "updatedAt": "2024-07-31T11:18:57.334Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "a4ad8756-7d76-41fc-97f1-bef3021542ff",
      "number": 10,
      "content": "Phàm ai biết đến danh Ngài sẽ để lòng tin cậy Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.897Z",
      "updatedAt": "2024-07-31T11:18:57.345Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "36e85595-1881-482a-81ac-f1ff0249b39f",
      "number": 10,
      "content": "Vì Chúa ôi, Ngài không bỏ rơi những ai tìm kiếm Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.897Z",
      "updatedAt": "2024-07-31T11:18:57.352Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "3693f3f6-4ae7-43f9-be25-7dad54604bd3",
      "number": 11,
      "content": "Hãy ca tụng Chúa, Ðấng ngự tại Si-ôn!",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.900Z",
      "updatedAt": "2024-07-31T11:18:57.363Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "55f804a9-71f8-4348-8d49-41dd5ebb1719",
      "number": 11,
      "content": "Hãy rao ra những công việc Ngài giữa các dân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.900Z",
      "updatedAt": "2024-07-31T11:18:57.465Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "c2e31fee-791f-4afb-b9dd-fd376231e2d6",
      "number": 12,
      "content": "Vì Ðấng báo thù kẻ gây đổ máu đã nhớ lại họ,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.902Z",
      "updatedAt": "2024-07-31T11:18:57.479Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "fcd0c28b-34b9-4bdc-8adc-92711c3f3998",
      "number": 12,
      "content": "Ngài không quên tiếng kêu cầu của những người khốn khổ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.902Z",
      "updatedAt": "2024-07-31T11:18:57.489Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "bd7a694e-01a3-4f54-9210-ac8da1f761f6",
      "number": 13,
      "content": "Xin thương xót con, Chúa ôi!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.904Z",
      "updatedAt": "2024-07-31T11:18:57.523Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "1efc6c72-79bd-4f0d-b37a-202143bcf68c",
      "number": 13,
      "content": "Xin đoái xem những khổ đau con chịu vì những kẻ ghét con;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.904Z",
      "updatedAt": "2024-07-31T11:18:57.547Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "fcf9c900-ab30-4de6-8070-8a6b32169d07",
      "number": 13,
      "content": "Xin nhấc đưa con ra khỏi cổng của tử thần,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.904Z",
      "updatedAt": "2024-07-31T11:18:57.561Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "e3ebc778-ef7e-438d-b6ee-e41517e1dba8",
      "number": 14,
      "content": "Ðể con có thể thuật lại mọi sự hầu ca ngợi Ngài nơi các cổng của Ái Nữ Si-ôn,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.907Z",
      "updatedAt": "2024-07-31T11:18:57.570Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "d12298f7-0162-4fdb-a692-1a3cb38b8837",
      "number": 14,
      "content": "Hầu bày tỏ niềm vui vì được hưởng ơn giải cứu của Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.907Z",
      "updatedAt": "2024-07-31T11:18:57.584Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "28597863-bcda-431c-9897-7b2348072bce",
      "number": 15,
      "content": "Các dân đã lún sâu vào hố chúng đào;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.909Z",
      "updatedAt": "2024-07-31T11:18:57.604Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "a2f1b337-56db-45a0-b81c-ad3dc240e5fa",
      "number": 15,
      "content": "Chân chúng đã vướng vào bẫy chúng giăng.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.909Z",
      "updatedAt": "2024-07-31T11:18:57.617Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "d6f733bb-2a28-40b7-b086-aa68ccddc136",
      "number": 16,
      "content": "Chúa đã bày tỏ Ngài ra khi Ngài thi hành sự đoán phạt;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.912Z",
      "updatedAt": "2024-07-31T11:18:57.633Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "d9e712fb-c38a-49a3-881e-1103cdfc16fb",
      "number": 16,
      "content": "Kẻ ác đã bị mắc vào bẫy do chính tay nó gài. (Hãy suy gẫm – Sê-la)",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.912Z",
      "updatedAt": "2024-07-31T11:18:57.655Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "04f84e09-214b-4977-b11d-79bebcc89e7e",
      "number": 17,
      "content": "Kẻ ác sẽ đi vào âm phủ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.917Z",
      "updatedAt": "2024-07-31T11:18:57.669Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "6e1e869a-d72f-4d3a-9748-9564ff3ec1a0",
      "number": 17,
      "content": "Mọi dân quên Ðức Chúa Trời cũng vậy.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.917Z",
      "updatedAt": "2024-07-31T11:18:57.682Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "dedddf29-a34f-4618-a9e2-1a315d1d67c1",
      "number": 18,
      "content": "Người thiếu thốn sẽ chẳng bị bỏ quên luôn luôn,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.923Z",
      "updatedAt": "2024-07-31T11:18:57.695Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "6dc68f19-a447-4842-955a-3709806c2882",
      "number": 18,
      "content": "Người nghèo khó sẽ chẳng bị thất vọng mãi mãi.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.923Z",
      "updatedAt": "2024-07-31T11:18:57.705Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "7a36b440-4a71-46a8-9471-c00d9c697633",
      "number": 19,
      "content": "Chúa ôi, xin trỗi dậy, xin đừng để loài người thắng hơn.",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.929Z",
      "updatedAt": "2024-07-31T11:18:57.717Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "2a28c32d-46db-46b0-897d-e6885e53a420",
      "number": 19,
      "content": "Nguyện các dân bị xét xử trước mặt Ngài.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.929Z",
      "updatedAt": "2024-07-31T11:18:57.727Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "24381d7a-7082-41e8-bccc-33e7a16d738b",
      "number": 20,
      "content": "Xin làm cho họ sợ hãi, Chúa ôi,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.934Z",
      "updatedAt": "2024-07-31T11:18:57.735Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    },
    {
      "id": "842e60ca-a258-4c1a-9575-4cfef7c5e433",
      "number": 20,
      "content": "Xin để các dân biết rằng họ chẳng qua chỉ là loài người. (Sê-la)",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T02:44:41.934Z",
      "updatedAt": "2024-07-31T11:18:57.745Z",
      "chapterId": "8b60c1a3-4818-46a5-abab-2eba9a893710"
    }
  ],
