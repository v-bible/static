    },
    {
      "id": "535a013d-a66e-4976-9200-9c0e115b9816",
      "content": "nt: chê-rúp (số ít của chê-ru-bim)",
      "position": 36,
      "order": 1,
      "createdAt": "2024-07-12T05:28:23.119Z",
      "updatedAt": "2024-07-12T05:28:23.119Z",
      "verseId": "5c59b07f-0dd2-42a8-bad7-e27ed537d87b",
      "chapterId": "5d5a7583-cc94-46f0-b4cd-894d40136110"
    },
