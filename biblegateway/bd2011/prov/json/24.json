{
  "chapter": {
    "id": "4301fae4-6a0e-4881-a85b-e844c34ea3a0",
    "number": 24,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2024&version=BD2011",
    "createdAt": "2024-07-12T04:03:49.254Z",
    "updatedAt": "2024-07-24T15:07:50.053Z",
    "bookId": "f88be0f6-2d78-40e0-8b41-6684c15a16e0"
  },
  "verses": [
    {
      "id": "48dbbcda-ea56-4d47-b323-e8e8f75193df",
      "number": 1,
      "content": "Ðừng ganh tị với phường gian ác,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:19:59.656Z",
      "updatedAt": "2024-07-15T03:19:59.867Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "97da1add-4960-4178-b664-305606d40825",
      "number": 1,
      "content": "Cũng đừng mong cộng tác với chúng làm gì;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:19:59.656Z",
      "updatedAt": "2024-07-15T03:19:59.893Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "3ca83ea9-5d03-49fb-ba4f-8260019614c7",
      "number": 2,
      "content": "Vì tâm trí chúng cứ toan tính chuyện bạo tàn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:19:59.666Z",
      "updatedAt": "2024-07-15T03:20:00.095Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "97c322f7-931a-4797-acfb-0db0ac078403",
      "number": 2,
      "content": "Và môi miệng chúng nói toàn lời gây rắc rối.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:19:59.666Z",
      "updatedAt": "2024-07-15T03:20:00.105Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "7f6092b5-ca68-40cf-bd14-fe4df2d90278",
      "number": 3,
      "content": "Nhờ khôn ngoan căn nhà được xây dựng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.670Z",
      "updatedAt": "2024-07-15T03:19:59.670Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "809d7c1e-d8ce-4da7-8e15-975897750125",
      "number": 3,
      "content": "Nhờ thông sáng nó được thiết lập vững vàng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.670Z",
      "updatedAt": "2024-07-15T03:19:59.670Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "8cf63804-9e78-4ae0-80f2-f4990b1cac73",
      "number": 4,
      "content": "Nhờ tri thức các phòng được đầy dẫy",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.673Z",
      "updatedAt": "2024-07-15T03:19:59.673Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "9d43146e-7977-4c05-bc9f-ce94f30ca792",
      "number": 4,
      "content": "Mọi thứ của cải vừa quý lại vừa sang.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.673Z",
      "updatedAt": "2024-07-15T03:19:59.673Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "fe5fbf84-6a40-43fe-a565-31e0013395d5",
      "number": 5,
      "content": "Người khôn ngoan quả có sức lực;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.677Z",
      "updatedAt": "2024-07-15T03:19:59.677Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "45474e12-460c-4efa-8141-f531c4c8227f",
      "number": 5,
      "content": "Người tri thức quyền lực càng gia tăng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.677Z",
      "updatedAt": "2024-07-15T03:19:59.677Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "b9d1d04a-a94b-4f8b-9510-226ac4a7fa1d",
      "number": 6,
      "content": "Con hãy dùng mưu chước khôn ngoan mà đánh giặc,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.680Z",
      "updatedAt": "2024-07-15T03:19:59.680Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "b5133dc9-1b3e-4227-9d22-ac0c83f67fd2",
      "number": 6,
      "content": "Con phải nhờ nhiều mưu sĩ giỏi mới có chiến thắng đem về.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.680Z",
      "updatedAt": "2024-07-15T03:19:59.680Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "b3132b28-d78b-4b72-82fd-993892944a3f",
      "number": 7,
      "content": "Sự khôn ngoan thật quá cao xa với kẻ dại;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.683Z",
      "updatedAt": "2024-07-15T03:19:59.683Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "4bff5780-3800-4234-84ab-6baa5e57192c",
      "number": 7,
      "content": "Ở cổng thành nó chẳng thể mở miệng nói ra lời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.683Z",
      "updatedAt": "2024-07-15T03:19:59.683Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "7537b9c7-8edd-4c3e-b064-3370f9d0fa6b",
      "number": 8,
      "content": "Kẻ nào mưu tính làm điều ác",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.686Z",
      "updatedAt": "2024-07-15T03:19:59.686Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "b00e2df5-fbfd-4596-b86d-abd1543b861f",
      "number": 8,
      "content": "Sẽ bị gọi là kẻ có thủ đoạn độc ác.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.686Z",
      "updatedAt": "2024-07-15T03:19:59.686Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "75c2e2e0-80a1-4926-bb77-902e27e4dadd",
      "number": 9,
      "content": "Toan tính kế hoạch để làm chuyện điên rồ là tội lỗi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.690Z",
      "updatedAt": "2024-07-15T03:19:59.690Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "a849972b-f33c-45a2-a827-70b2d4dc59cf",
      "number": 9,
      "content": "Kẻ nhạo báng bị thiên hạ ghê tởm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.690Z",
      "updatedAt": "2024-07-15T03:19:59.690Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "0e7582ed-33f2-4025-a7a9-6e97cb9c16b6",
      "number": 10,
      "content": "Nếu con ngã lòng trong ngày nghịch cảnh,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.693Z",
      "updatedAt": "2024-07-15T03:19:59.693Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "6a8073c8-c8fb-470d-8032-160c3cf56d2c",
      "number": 10,
      "content": "Sức lực con quả thật yếu thay.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.693Z",
      "updatedAt": "2024-07-15T03:19:59.693Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "070dffad-6f9f-4f06-b7a6-546c4475b8a3",
      "number": 11,
      "content": "Hãy giải cứu những kẻ bị mang đi hành quyết,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.696Z",
      "updatedAt": "2024-07-15T03:19:59.696Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "88fd59a2-c1c3-4824-acd5-0fff6bea27e2",
      "number": 11,
      "content": "Hãy giữ lại mạng những kẻ lảo đảo bước đến chỗ hành hình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.696Z",
      "updatedAt": "2024-07-15T03:19:59.696Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "0f9b1d8a-3f6f-4de2-acb3-27dab4bd7f43",
      "number": 12,
      "content": "Nếu con nói, “Này, chúng tôi nào hay biết chuyện ấy.”",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.700Z",
      "updatedAt": "2024-07-15T03:19:59.700Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "9e38b704-8ca2-40f7-8be1-1a14bf2f6a9e",
      "number": 12,
      "content": "Ðấng cân nhắc lòng người há không biết rõ lòng con sao?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.700Z",
      "updatedAt": "2024-07-15T03:19:59.700Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "f8d28196-1517-44a8-882d-09bb8cbf7525",
      "number": 12,
      "content": "Ðấng gìn giữ linh hồn con há không biết điều đó sao?",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.700Z",
      "updatedAt": "2024-07-15T03:19:59.700Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "539050fa-a68a-4fb7-8c33-979df5a6f52a",
      "number": 12,
      "content": "Ngài há chẳng báo trả mỗi người tùy theo việc họ làm sao?",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.700Z",
      "updatedAt": "2024-07-15T03:19:59.700Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "61bea3bd-048d-422c-8b43-a2993ac0bf0d",
      "number": 13,
      "content": "Con ơi, hãy ăn mật ong, vì nó ngon ngọt lắm,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.703Z",
      "updatedAt": "2024-07-15T03:19:59.703Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "f2a2649f-2392-45b2-b876-59e4fb1596e1",
      "number": 13,
      "content": "Những giọt mật nhểu ra từ tàng ong sẽ làm ngọt lịm lưỡi con,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.703Z",
      "updatedAt": "2024-07-15T03:19:59.703Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "3a008002-9189-4812-b8ac-195f4fcb5bc1",
      "number": 14,
      "content": "Hãy biết rằng khôn ngoan cũng ngọt ngon cho linh hồn như thế;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.707Z",
      "updatedAt": "2024-07-15T03:19:59.707Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "accd0bb3-731b-42e0-b9f1-5413f744734a",
      "number": 14,
      "content": "Nếu con tìm được khôn ngoan, con sẽ có một tương lai tốt đẹp,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.707Z",
      "updatedAt": "2024-07-15T03:19:59.707Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "db3111c6-147a-4a51-868d-4f30235771f1",
      "number": 14,
      "content": "Hy vọng của con sẽ không bị cắt đứt giữa chừng.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.707Z",
      "updatedAt": "2024-07-15T03:19:59.707Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "45c289fc-108e-4eb6-94ac-8ca56c4dd7ea",
      "number": 15,
      "content": "Hỡi kẻ gian ác, chớ nằm đó rình nhà người ngay lành;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.710Z",
      "updatedAt": "2024-07-15T03:19:59.710Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "9b1d3b36-d925-4c1f-b200-13e9672826cf",
      "number": 15,
      "content": "Ðừng xâm phạm nơi ở của người công chính.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.710Z",
      "updatedAt": "2024-07-15T03:19:59.710Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "f1b6d0e1-317b-4b50-9051-9345eba6864f",
      "number": 16,
      "content": "Vì dù họ ngã đến bảy lần, họ cũng đứng dậy được;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.714Z",
      "updatedAt": "2024-07-15T03:19:59.714Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "ea040bee-e49d-4de4-aeb9-c58b4c9db40d",
      "number": 16,
      "content": "Nhưng kẻ gian ác sẽ đổ nhào trong thì tai họa.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.714Z",
      "updatedAt": "2024-07-15T03:19:59.714Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "d4bfdab3-e85b-4d1d-8f99-1e4c381a1941",
      "number": 17,
      "content": "Ðừng vui mừng khi kẻ thù con ngã quỵ;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.720Z",
      "updatedAt": "2024-07-15T03:19:59.720Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "32b751fd-2ac8-4e8e-8fdb-efeee0784973",
      "number": 17,
      "content": "Lòng con chớ hân hoan khi nó thất thế sa cơ.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.720Z",
      "updatedAt": "2024-07-15T03:19:59.720Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "dea5bbde-72c5-4619-8a2e-28d56cf2dc6e",
      "number": 18,
      "content": "Kẻo Chúa thấy điều đó và Ngài chẳng bằng lòng,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.726Z",
      "updatedAt": "2024-07-15T03:19:59.726Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "f5d77fed-25d0-4c77-8160-87cee06b361d",
      "number": 18,
      "content": "Rồi Ngài ngưng cơn thịnh nộ khỏi nó chăng.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.726Z",
      "updatedAt": "2024-07-15T03:19:59.726Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "c164e2a3-bdc8-49c4-9608-e54042ce2238",
      "number": 19,
      "content": "Ðừng bực bội vì những người gian ác,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.732Z",
      "updatedAt": "2024-07-15T03:19:59.732Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "c4ce063c-600f-4669-8a32-0ca6d1f9c7df",
      "number": 19,
      "content": "Chớ phân bì khi kẻ ác thành công;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.732Z",
      "updatedAt": "2024-07-15T03:19:59.732Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "a56fa6ef-4076-4d6d-9588-09adf5a4cc54",
      "number": 20,
      "content": "Vì kẻ ác sẽ không có tương lai,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.738Z",
      "updatedAt": "2024-07-15T03:19:59.738Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "2785898e-05ff-40f6-ae29-243023f384a5",
      "number": 20,
      "content": "Ðèn kẻ ác sẽ nay mai tắt lịm.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.738Z",
      "updatedAt": "2024-07-15T03:19:59.738Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "c55a004f-ee58-48fb-b9d0-4b5918111cae",
      "number": 21,
      "content": "Con ơi, hãy kính sợ Chúa và tôn kính vua;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.744Z",
      "updatedAt": "2024-07-15T03:19:59.744Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "2a3d074f-82aa-4670-a46e-364f0888d03f",
      "number": 21,
      "content": "Ðừng nhập bọn với đám người phản nghịch.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.744Z",
      "updatedAt": "2024-07-15T03:19:59.744Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "c81dedd1-791e-467e-8786-a364ec832a96",
      "number": 22,
      "content": "Vì tai họa của chúng sẽ thình lình vụt đến;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.749Z",
      "updatedAt": "2024-07-15T03:19:59.749Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "a73b8601-73e7-4b64-8299-e574a90296ee",
      "number": 22,
      "content": "Ai biết chúng sẽ tàn rụi ra sao khi cả hai cùng phạt?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.749Z",
      "updatedAt": "2024-07-15T03:19:59.749Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "be55e038-5ab0-45bf-866b-6cb9a428ebaf",
      "number": 23,
      "content": "Ðây cũng là những lời nói của người khôn ngoan:",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:19:59.755Z",
      "updatedAt": "2024-07-15T03:20:00.145Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "c58a4342-b58d-4da7-a06d-8526bf7f0518",
      "number": 23,
      "content": "Thiên vị khi xét xử là không tốt.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:19:59.755Z",
      "updatedAt": "2024-07-15T03:20:00.156Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "71959b1b-f8a1-41ce-8282-04ea2303fe49",
      "number": 24,
      "content": "Ai nói với kẻ ác rằng, “Ngươi vô tội,”",
      "order": 0,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:19:59.761Z",
      "updatedAt": "2024-07-15T03:20:00.165Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "75e16f3e-ecc4-483c-a3fa-62176841a22d",
      "number": 24,
      "content": "Kẻ ấy sẽ bị dân nguyền rủa và các dân khác ghê tởm.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:19:59.761Z",
      "updatedAt": "2024-07-15T03:20:00.179Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "79e6a45c-8950-4411-844a-8ea66040f5c0",
      "number": 25,
      "content": "Nhưng ai quở trách kẻ ác sẽ được vui lòng,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:19:59.767Z",
      "updatedAt": "2024-07-15T03:20:00.190Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "5721ab3f-d058-4e9d-a23e-7d80cd808d61",
      "number": 25,
      "content": "Và phước hạnh sẽ đến với người ấy.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:19:59.767Z",
      "updatedAt": "2024-07-15T03:20:00.199Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "72ae107e-d575-4e73-8e72-23e9a09e330d",
      "number": 26,
      "content": "Lời đáp chính xác giống như một nụ hôn hôn trên môi.",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.772Z",
      "updatedAt": "2024-07-15T03:19:59.772Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "ec41432c-afae-47ee-abca-b38fce0d50bf",
      "number": 27,
      "content": "Hãy sắp đặt công việc của con ở bên ngoài,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.778Z",
      "updatedAt": "2024-07-15T03:19:59.778Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "71f2ef1c-a397-4df3-ad95-918aa02c02c1",
      "number": 27,
      "content": "Hãy chuẩn bị mọi việc của con ngoài đồng áng cho chu tất;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.778Z",
      "updatedAt": "2024-07-15T03:19:59.778Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "159c0429-0467-4f3a-919e-ec68a5b6f65e",
      "number": 27,
      "content": "Rồi sau đó mới bắt đầu xây cất nhà cho con.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.778Z",
      "updatedAt": "2024-07-15T03:19:59.778Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "8c1204e2-5a19-4e82-8125-631f7eb1fdb4",
      "number": 28,
      "content": "Ðừng làm chứng nghịch lại người lân cận con vô cớ;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.784Z",
      "updatedAt": "2024-07-15T03:19:59.784Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "c5d44df3-405c-49fb-acd8-e9d03f38355e",
      "number": 28,
      "content": "Ðừng để môi con nói những lời dối trá hại người.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.784Z",
      "updatedAt": "2024-07-15T03:19:59.784Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "54fe8cdb-23f7-4b50-bff5-4f862b5bb4b8",
      "number": 29,
      "content": "Con đừng nói, “Ta sẽ làm cho nó như nó đã làm cho ta;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.790Z",
      "updatedAt": "2024-07-15T03:19:59.790Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "c8ad86ea-6f63-41e6-b6f6-874271bca273",
      "number": 29,
      "content": "Ta sẽ báo trả cho nó những gì nó đã làm.”",
      "order": 1,
      "parNumber": 13,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.790Z",
      "updatedAt": "2024-07-15T03:19:59.790Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "b90fe924-faeb-4ad7-9f51-019667a794ba",
      "number": 30,
      "content": "Ta có đi ngang qua cánh đồng của một kẻ biếng nhác;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.796Z",
      "updatedAt": "2024-07-15T03:19:59.796Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "a54a0713-dd68-4855-beb2-95bb64ea53ff",
      "number": 30,
      "content": "Đi ngang qua vườn nho của một kẻ thiếu hiểu biết thường tình;",
      "order": 1,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.796Z",
      "updatedAt": "2024-07-15T03:19:59.796Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "21953ca6-13b9-49ef-8c69-118429012220",
      "number": 31,
      "content": "Kìa, gai góc mọc tràn lan khắp chốn,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.802Z",
      "updatedAt": "2024-07-15T03:19:59.802Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "9c25bc33-0863-4c07-b12d-5b3738a42600",
      "number": 31,
      "content": "Các bụi gai phủ mặt đất um tùm, còn tường đá thì chỗ xiêu chỗ đổ.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.802Z",
      "updatedAt": "2024-07-15T03:19:59.802Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "8ad36b39-7b8b-4346-a3b4-d38862530fbc",
      "number": 32,
      "content": "Thấy như thế ta chạnh lòng suy nghĩ;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.808Z",
      "updatedAt": "2024-07-15T03:19:59.808Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "a23e8296-24ea-4792-bb06-5dfaf67afbb3",
      "number": 32,
      "content": "Ta nhìn xem và gẫm được một bài học ở đời:",
      "order": 1,
      "parNumber": 14,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.808Z",
      "updatedAt": "2024-07-15T03:19:59.808Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "c4eb6f38-d23a-4a2a-a9a9-0fee1bf3008e",
      "number": 33,
      "content": "Ngủ một chút, ngủ nướng thêm một chút,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.813Z",
      "updatedAt": "2024-07-15T03:19:59.813Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "51407698-d1c9-4900-a05f-1fdccbf8afa7",
      "number": 33,
      "content": "Khoanh tay nằm nghỉ một chút,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.813Z",
      "updatedAt": "2024-07-15T03:19:59.813Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "8881189a-0065-43bc-a2b0-91f0c5f8d095",
      "number": 34,
      "content": "Thì cái nghèo sẽ vụt đến như tên cướp bất chợt tấn công,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.819Z",
      "updatedAt": "2024-07-15T03:19:59.819Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    },
    {
      "id": "175f1e74-0a77-4301-b987-ef4a55d94fb3",
      "number": 34,
      "content": "Và cái thiếu sẽ ập đến như kẻ ác có vũ khí bất thần đột kích.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:19:59.819Z",
      "updatedAt": "2024-07-15T03:19:59.819Z",
      "chapterId": "4301fae4-6a0e-4881-a85b-e844c34ea3a0"
    }
  ],
