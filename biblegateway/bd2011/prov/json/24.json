    {
      "id": "6a473c4b-3b3c-4a50-bd71-be4ca9aed2ae",
      "content": "Những Lời Khuyên Bảo Khác của Người Khôn Ngoan",
      "order": 0,
      "createdAt": "2024-07-15T03:20:04.228Z",
      "updatedAt": "2024-07-15T03:20:04.228Z",
