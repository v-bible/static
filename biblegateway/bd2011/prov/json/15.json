{
  "chapter": {
    "id": "095b9b42-8d55-4c6b-989a-ff0362ceed89",
    "number": 15,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2015&version=BD2011",
    "createdAt": "2024-07-12T04:03:49.097Z",
    "updatedAt": "2024-07-24T15:07:49.909Z",
    "bookId": "f88be0f6-2d78-40e0-8b41-6684c15a16e0"
  },
  "verses": [
    {
      "id": "1b2ba22e-2b5c-4a9c-a6e4-a3f673254eeb",
      "number": 1,
      "content": "Lời đáp êm dịu làm nguôi cơn giận;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:59.148Z",
      "updatedAt": "2024-07-15T03:17:59.286Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "2f6a3953-a5de-42fb-905e-e655bbc8779f",
      "number": 1,
      "content": "Còn lời xẳng xớm gây phẫn nộ thêm.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:59.148Z",
      "updatedAt": "2024-07-15T03:17:59.315Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "b8be12ac-9b42-42b3-b037-8ddae09e87d2",
      "number": 2,
      "content": "Lưỡi người khôn ngoan phát ban tri thức;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:59.157Z",
      "updatedAt": "2024-07-15T03:17:59.340Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "3dcaaef2-7e44-4e1a-aa4f-b8d4856249f3",
      "number": 2,
      "content": "Miệng kẻ dại dột thốt chuyện điên rồ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:59.157Z",
      "updatedAt": "2024-07-15T03:17:59.365Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "d553ce9a-df94-4a10-a855-627b4b8eb914",
      "number": 3,
      "content": "Mắt Chúa thấy rõ khắp nơi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.159Z",
      "updatedAt": "2024-07-15T03:17:59.159Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "ca12d79b-2810-424d-b11e-ab169e6c4a80",
      "number": 3,
      "content": "Thấy tất cả kẻ xấu và người tốt.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.159Z",
      "updatedAt": "2024-07-15T03:17:59.159Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "cf241f08-ea93-4a56-84c0-e97900c109e4",
      "number": 4,
      "content": "Lưỡi hiền lành là cây sự sống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.162Z",
      "updatedAt": "2024-07-15T03:17:59.162Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "49aa8584-cc07-419e-a403-3e35423b93a1",
      "number": 4,
      "content": "Còn lưỡi độc ác làm tan nát tinh thần.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.162Z",
      "updatedAt": "2024-07-15T03:17:59.162Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "813a65f2-bdf6-427c-8be9-0ab44f86a362",
      "number": 5,
      "content": "Kẻ dại dột khinh lời cha khuyên dạy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.164Z",
      "updatedAt": "2024-07-15T03:17:59.164Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "48868fd1-370e-453b-bd00-e302db5d44a2",
      "number": 5,
      "content": "Còn người khôn nghe lời quở trách khuyên răn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.164Z",
      "updatedAt": "2024-07-15T03:17:59.164Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "23bd8cef-7d1d-4aba-ad3c-526da220ec7e",
      "number": 6,
      "content": "Trong nhà người ngay lành có nhiều của cải;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.166Z",
      "updatedAt": "2024-07-15T03:17:59.166Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "6fac92bd-1c46-40d8-831b-5030974564b0",
      "number": 6,
      "content": "Còn trong lợi tức kẻ ác có rắc rối chờ cơ hội xảy ra.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.166Z",
      "updatedAt": "2024-07-15T03:17:59.166Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "adb412bf-21a4-49a4-96c0-08f0b17fe061",
      "number": 7,
      "content": "Môi người khôn ngoan loan truyền tri thức;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.168Z",
      "updatedAt": "2024-07-15T03:17:59.168Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "5643ed13-9002-4b28-a7cc-0a1e6518c677",
      "number": 7,
      "content": "Còn lòng kẻ dại dột ấm ức nhưng chẳng có kiến thức gì.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.168Z",
      "updatedAt": "2024-07-15T03:17:59.168Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "1ecdd6b7-9c8e-4094-a926-24c9510cde7c",
      "number": 8,
      "content": "Lễ vật của kẻ gian ác là gớm ghiếc trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.170Z",
      "updatedAt": "2024-07-15T03:17:59.170Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "ea3f28b9-48ed-4609-a8c8-4ab9bde309f2",
      "number": 8,
      "content": "Nhưng lời cầu nguyện của người ngay lành thì Ngài lại vui lòng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.170Z",
      "updatedAt": "2024-07-15T03:17:59.170Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "cfb0d0ed-15da-425a-a077-317229283eaa",
      "number": 9,
      "content": "Ðường lối kẻ gian ác là gớm ghiếc trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.173Z",
      "updatedAt": "2024-07-15T03:17:59.173Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "09f79e04-2071-47d2-a9b9-e1bab8033d5a",
      "number": 9,
      "content": "Nhưng Ngài yêu thương người đeo đuổi sự ngay lành.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.173Z",
      "updatedAt": "2024-07-15T03:17:59.173Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "882fbc97-b5e4-4414-9b0b-621bb89141b5",
      "number": 10,
      "content": "Sự nghiêm phạt dành cho kẻ bỏ đường ngay nẻo chánh,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.175Z",
      "updatedAt": "2024-07-15T03:17:59.175Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "301e7944-2213-456e-9e84-f364026d6896",
      "number": 10,
      "content": "Ai ghét sự khuyên răn chẳng tránh khỏi chết đâu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.175Z",
      "updatedAt": "2024-07-15T03:17:59.175Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "dc593b54-34e3-43ce-92ec-a701b52a0c10",
      "number": 11,
      "content": "Ngay cả âm phủ và hỏa ngục mà Chúa còn biết rõ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.177Z",
      "updatedAt": "2024-07-15T03:17:59.177Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "a16f19e4-4982-48fe-92c2-c485fd260866",
      "number": 11,
      "content": "Thì huống chi là lòng dạ loài người.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.177Z",
      "updatedAt": "2024-07-15T03:17:59.177Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "317ac793-0a62-4b76-8732-837c66491c6d",
      "number": 12,
      "content": "Kẻ nhạo báng không thích bị ai quở trách;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.182Z",
      "updatedAt": "2024-07-15T03:17:59.182Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "17e133c4-1db0-4629-b0d0-34c410d94141",
      "number": 12,
      "content": "Nó tránh đến với người khôn để học sự khôn ngoan.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.182Z",
      "updatedAt": "2024-07-15T03:17:59.182Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "9552c152-60ce-4eba-be23-6fd50672a100",
      "number": 13,
      "content": "Lòng vui vẻ khiến mặt mày rạng rỡ;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.185Z",
      "updatedAt": "2024-07-15T03:17:59.185Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "9a7a0cdf-b9a2-4374-842a-8455b9233f2c",
      "number": 13,
      "content": "Dạ âu sầu làm tan vỡ tinh thần.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.185Z",
      "updatedAt": "2024-07-15T03:17:59.185Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "d3aad6c3-b068-45f4-b416-c968783547b4",
      "number": 14,
      "content": "Lòng người thông sáng thích kiếm tìm tri thức;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.187Z",
      "updatedAt": "2024-07-15T03:17:59.187Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "fb8009ea-5bdc-49f2-816e-3389911d6913",
      "number": 14,
      "content": "Miệng kẻ dại khờ ưa ẩm thực điên rồ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.187Z",
      "updatedAt": "2024-07-15T03:17:59.187Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "d5b3c0cc-eb8b-4177-aa17-e36024f60de6",
      "number": 15,
      "content": "Kẻ nghèo khó suốt đời chịu khánh kiệt;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.189Z",
      "updatedAt": "2024-07-15T03:17:59.189Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "5b6df259-2857-4dd2-a218-3ff606b20033",
      "number": 15,
      "content": "Người vui vẻ trong lòng dự yến tiệc luôn luôn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.189Z",
      "updatedAt": "2024-07-15T03:17:59.189Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "7448557e-2f33-419f-89ce-b05190d83aae",
      "number": 16,
      "content": "Thà ít của mà kính sợ Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.191Z",
      "updatedAt": "2024-07-15T03:17:59.191Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "4d44dd2d-c0f2-4e21-83de-59ed5dfd28e2",
      "number": 16,
      "content": "Hơn của cải nhiều mà rắc rối ùa theo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.191Z",
      "updatedAt": "2024-07-15T03:17:59.191Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "8a7cba95-ec0a-4c22-9f09-386de75dd535",
      "number": 17,
      "content": "Thà ăn cơm với rau mà thương yêu nhau,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.194Z",
      "updatedAt": "2024-07-15T03:17:59.194Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "4a256510-c75b-4ee6-b617-e2d041473abb",
      "number": 17,
      "content": "Hơn là ăn thịt bò mập béo mà ghen ghét cặp theo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.194Z",
      "updatedAt": "2024-07-15T03:17:59.194Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "9ae91286-e12e-4c3c-af79-fbc59bf818f9",
      "number": 18,
      "content": "Người nóng tính dễ gây xung đột;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.196Z",
      "updatedAt": "2024-07-15T03:17:59.196Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "ceb86656-34a6-43a8-9e1a-2fce3bb378cc",
      "number": 18,
      "content": "Còn người chậm nóng giận làm cuộc cãi lộn nguôi đi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.196Z",
      "updatedAt": "2024-07-15T03:17:59.196Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "88350e93-538b-498c-8cc4-8b616c1bcda7",
      "number": 19,
      "content": "Đường kẻ biếng nhác gai góc mọc đầy cản trở;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.198Z",
      "updatedAt": "2024-07-15T03:17:59.198Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "6b510bad-5a67-476f-b808-5842509e9192",
      "number": 19,
      "content": "Còn lối người ngay lành lại mở rộng thênh thang.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.198Z",
      "updatedAt": "2024-07-15T03:17:59.198Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "3024b88c-b3bc-45dd-9226-0b8c8dd854c9",
      "number": 20,
      "content": "Ðứa con khôn ngoan làm cho cha vui vẻ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.201Z",
      "updatedAt": "2024-07-15T03:17:59.201Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "ac8ac41d-a051-4136-a228-b916c6a9f26b",
      "number": 20,
      "content": "Còn kẻ dại dột lại khinh rẻ mẹ mình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.201Z",
      "updatedAt": "2024-07-15T03:17:59.201Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "f3c267a7-2026-4fd5-b8f5-7b60f66db0f1",
      "number": 21,
      "content": "Kẻ kém hiểu biết cho sự điên dại là vui;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.203Z",
      "updatedAt": "2024-07-15T03:17:59.203Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "4a1484fe-9488-48bf-b088-ea0ab6fe84f5",
      "number": 21,
      "content": "Nhưng người có trí tuệ cứ bước theo đường chính đáng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.203Z",
      "updatedAt": "2024-07-15T03:17:59.203Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "0c428b34-222a-4e15-937a-096543812f20",
      "number": 22,
      "content": "Thiếu bàn luận các chương trình bị hỏng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.205Z",
      "updatedAt": "2024-07-15T03:17:59.205Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "2528cd88-09be-490d-95c9-a69173dadaaf",
      "number": 22,
      "content": "Nhưng với mưu sĩ nhiều chúng có triển vọng thành công.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.205Z",
      "updatedAt": "2024-07-15T03:17:59.205Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "c89ba1ce-d560-4439-b0e1-f6e213c441f2",
      "number": 23,
      "content": "Lời đáp hay làm mọi người vui thích;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.207Z",
      "updatedAt": "2024-07-15T03:17:59.207Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "73d42d58-85b9-459c-b12f-6fe04f5dc752",
      "number": 23,
      "content": "Lời đáp hay đúng lúc thật bổ ích biết bao!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.207Z",
      "updatedAt": "2024-07-15T03:17:59.207Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "eafae3dc-9aba-40a8-8e08-1350151c1138",
      "number": 24,
      "content": "Người khôn ngoan bước theo con đường sự sống dẫn đi lên,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.209Z",
      "updatedAt": "2024-07-15T03:17:59.209Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "0c79c986-1bca-4695-bd68-379f8c4c9e75",
      "number": 24,
      "content": "Ðể giữ người ấy tránh xa âm phủ ở phía dưới.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.209Z",
      "updatedAt": "2024-07-15T03:17:59.209Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "160e6a4f-f29b-4380-9755-23053c5c1dcd",
      "number": 25,
      "content": "Chúa phá hủy nhà của kẻ kiêu ngạo;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.211Z",
      "updatedAt": "2024-07-15T03:17:59.211Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "a0a74dbd-63ed-4694-8898-f1a6b3987bf6",
      "number": 25,
      "content": "Nhưng Ngài bảo vệ địa giới của người góa bụa.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.211Z",
      "updatedAt": "2024-07-15T03:17:59.211Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "541ea469-bdc7-4d00-8a2f-4639133b17c6",
      "number": 26,
      "content": "Ý đồ của kẻ gian ác là gớm ghiếc trước mặt Chúa;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.213Z",
      "updatedAt": "2024-07-15T03:17:59.213Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "5758beb3-1e88-4156-9d61-c3385b7d7735",
      "number": 26,
      "content": "Nhưng lời của người trong sạch được đẹp lòng Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.213Z",
      "updatedAt": "2024-07-15T03:17:59.213Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "83975972-43b8-4d64-86de-dc8a78fa40df",
      "number": 27,
      "content": "Kẻ tham lợi bất nghĩa chuốc lấy rắc rối cho nhà mình;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.215Z",
      "updatedAt": "2024-07-15T03:17:59.215Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "22b232c5-d2bf-4d75-89ac-9f596b6dc82d",
      "number": 27,
      "content": "Nhưng ai ghét của hối lộ sẽ tiếp tục sống còn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.215Z",
      "updatedAt": "2024-07-15T03:17:59.215Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "6b312b9d-e16e-4f9f-8fee-4f3ef9322602",
      "number": 28,
      "content": "Người ngay lành suy nghĩ trong lòng rồi mới đáp;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.217Z",
      "updatedAt": "2024-07-15T03:17:59.217Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "10191862-9629-4a26-9e61-13b43bcf6c4b",
      "number": 28,
      "content": "Còn miệng kẻ gian ác vội nói những lời độc ác hại người.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.217Z",
      "updatedAt": "2024-07-15T03:17:59.217Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "6c91cdb4-8e25-40da-81be-e6c4110214c3",
      "number": 29,
      "content": "Chúa thật cách xa đối với phường gian ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 14,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.219Z",
      "updatedAt": "2024-07-15T03:17:59.219Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "ef8cdc6e-c534-44b4-a6b8-3ec73b1b02a0",
      "number": 29,
      "content": "Nhưng Ngài lắng nghe lời cầu nguyện của người ngay lành.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 15,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.219Z",
      "updatedAt": "2024-07-15T03:17:59.219Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "6116e57f-a1bc-479d-823c-45915f03819e",
      "number": 30,
      "content": "Ánh mắt hân hoan phản ánh lòng vui vẻ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 16,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.222Z",
      "updatedAt": "2024-07-15T03:17:59.222Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "7d425418-503c-4da7-a226-6d029573be4e",
      "number": 30,
      "content": "Tin tức vui mừng làm mạnh khỏe cốt xương.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 17,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.222Z",
      "updatedAt": "2024-07-15T03:17:59.222Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "31698fd5-9b3e-49ef-91cd-8f8b515e2875",
      "number": 31,
      "content": "Tai chịu lắng nghe những lời quở trách về cuộc sống,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 18,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.228Z",
      "updatedAt": "2024-07-15T03:17:59.228Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "c81a3398-d256-4d53-80ed-64f0efbc8b21",
      "number": 31,
      "content": "Người ấy sẽ ở giữa các bậc hiền triết khôn ngoan.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 19,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.228Z",
      "updatedAt": "2024-07-15T03:17:59.228Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "1b660d60-b2ad-4dfe-b26f-32c94783a5f0",
      "number": 32,
      "content": "Kẻ từ chối nghe lời chỉ giáo khinh bỉ linh hồn mình,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 20,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.234Z",
      "updatedAt": "2024-07-15T03:17:59.234Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "11d6e311-cdbf-4ff1-b809-af7690f070ea",
      "number": 32,
      "content": "Nhưng ai chịu nghe lời khuyên bảo sẽ được sự thông sáng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 21,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.234Z",
      "updatedAt": "2024-07-15T03:17:59.234Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "87f83024-fa01-4fc2-b7b5-9d8ef6c56875",
      "number": 33,
      "content": "Kính sợ Chúa là cách giáo dục khôn ngoan,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 22,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.239Z",
      "updatedAt": "2024-07-15T03:17:59.239Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    },
    {
      "id": "0a6df769-e614-4ea5-af7b-2651d5b5aa6f",
      "number": 33,
      "content": "Ðức khiêm nhường đi trước sự tôn trọng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 23,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:59.239Z",
      "updatedAt": "2024-07-15T03:17:59.239Z",
      "chapterId": "095b9b42-8d55-4c6b-989a-ff0362ceed89"
    }
  ],
  "footnotes": [],
  "headings": [],
  "references": []
}