{
  "chapter": {
    "id": "484c3214-f9bf-46a4-a1c7-64963efa6bf9",
    "number": 15,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2015&version=BD2011",
    "createdAt": "2024-09-09T04:13:02.517Z",
    "updatedAt": "2024-09-09T04:13:02.517Z",
    "bookId": "c469ccff-b0ed-4604-99cb-79ba4ff3c267"
  },
  "verses": [
    {
      "id": "9b89f52d-28b4-4ca8-9834-53b2b47be3e6",
      "number": 1,
      "content": "Lời đáp êm dịu làm nguôi cơn giận;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.152Z",
      "updatedAt": "2024-09-09T04:54:11.152Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "66a83ba3-c3ea-43fd-bf64-5f91a98e0823",
      "number": 1,
      "content": "Còn lời xẳng xớm gây phẫn nộ thêm.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.165Z",
      "updatedAt": "2024-09-09T04:54:11.165Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "2b82d8d2-548a-485a-a75d-7de7b04f11b2",
      "number": 2,
      "content": "Lưỡi người khôn ngoan phát ban tri thức;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.170Z",
      "updatedAt": "2024-09-09T04:54:11.170Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "1b0ce337-09a6-4f79-929c-4fde4ac8faa4",
      "number": 2,
      "content": "Miệng kẻ dại dột thốt chuyện điên rồ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.175Z",
      "updatedAt": "2024-09-09T04:54:11.175Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "bddf9a5a-464c-4cd8-850b-5dfd862c87f8",
      "number": 3,
      "content": "Mắt Chúa thấy rõ khắp nơi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.180Z",
      "updatedAt": "2024-09-09T04:54:11.180Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "ab3e8f9f-65b2-4391-86cf-db881e884135",
      "number": 3,
      "content": "Thấy tất cả kẻ xấu và người tốt.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.184Z",
      "updatedAt": "2024-09-09T04:54:11.184Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "ced9a70b-3562-4ce9-a939-70943358ffe3",
      "number": 4,
      "content": "Lưỡi hiền lành là cây sự sống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.189Z",
      "updatedAt": "2024-09-09T04:54:11.189Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "86ebac3e-7438-431d-b9ff-4ef4f469e82f",
      "number": 4,
      "content": "Còn lưỡi độc ác làm tan nát tinh thần.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.194Z",
      "updatedAt": "2024-09-09T04:54:11.194Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "60bfadc8-2f78-4aac-92f2-61a07db16fc2",
      "number": 5,
      "content": "Kẻ dại dột khinh lời cha khuyên dạy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.198Z",
      "updatedAt": "2024-09-09T04:54:11.198Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "47a06f42-5bbd-471e-8ff7-2cbf8ec2a3ec",
      "number": 5,
      "content": "Còn người khôn nghe lời quở trách khuyên răn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.203Z",
      "updatedAt": "2024-09-09T04:54:11.203Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "e456123c-d9c7-4ebc-80c6-5d695e8cd528",
      "number": 6,
      "content": "Trong nhà người ngay lành có nhiều của cải;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.208Z",
      "updatedAt": "2024-09-09T04:54:11.208Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "96f01d12-f4ab-4ba2-b606-f6e47b51d449",
      "number": 6,
      "content": "Còn trong lợi tức kẻ ác có rắc rối chờ cơ hội xảy ra.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.212Z",
      "updatedAt": "2024-09-09T04:54:11.212Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "e509eda8-e21e-4a70-b8e5-b6334c20e935",
      "number": 7,
      "content": "Môi người khôn ngoan loan truyền tri thức;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.218Z",
      "updatedAt": "2024-09-09T04:54:11.218Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "6ebf06e9-1358-4798-a352-08c56508b579",
      "number": 7,
      "content": "Còn lòng kẻ dại dột ấm ức nhưng chẳng có kiến thức gì.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.223Z",
      "updatedAt": "2024-09-09T04:54:11.223Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "76eccde1-18fe-4689-a78a-e02d4d9dbeec",
      "number": 8,
      "content": "Lễ vật của kẻ gian ác là gớm ghiếc trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.228Z",
      "updatedAt": "2024-09-09T04:54:11.228Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "5ce1148e-fc98-464e-b2f4-b561f07f8c46",
      "number": 8,
      "content": "Nhưng lời cầu nguyện của người ngay lành thì Ngài lại vui lòng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.233Z",
      "updatedAt": "2024-09-09T04:54:11.233Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "54c82fc1-d07b-45dd-abcf-91a6ce59d65f",
      "number": 9,
      "content": "Ðường lối kẻ gian ác là gớm ghiếc trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.239Z",
      "updatedAt": "2024-09-09T04:54:11.239Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "43b63901-18c9-4e10-b938-bcfa5e6da5e9",
      "number": 9,
      "content": "Nhưng Ngài yêu thương người đeo đuổi sự ngay lành.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.244Z",
      "updatedAt": "2024-09-09T04:54:11.244Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "608f647b-811b-48e1-a07a-d997da5c3c89",
      "number": 10,
      "content": "Sự nghiêm phạt dành cho kẻ bỏ đường ngay nẻo chánh,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.249Z",
      "updatedAt": "2024-09-09T04:54:11.249Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "a6ce8b8e-fb6c-4716-95a7-6adce1e5fecb",
      "number": 10,
      "content": "Ai ghét sự khuyên răn chẳng tránh khỏi chết đâu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.255Z",
      "updatedAt": "2024-09-09T04:54:11.255Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "90cfd268-b8d4-421e-a243-6555c83bc53c",
      "number": 11,
      "content": "Ngay cả âm phủ và hỏa ngục mà Chúa còn biết rõ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.260Z",
      "updatedAt": "2024-09-09T04:54:11.260Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "d86cec83-72e9-43d7-bb47-d25ecd068958",
      "number": 11,
      "content": "Thì huống chi là lòng dạ loài người.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.264Z",
      "updatedAt": "2024-09-09T04:54:11.264Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "676f03cb-0ad8-4de1-b7f9-c0b7a5479011",
      "number": 12,
      "content": "Kẻ nhạo báng không thích bị ai quở trách;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.269Z",
      "updatedAt": "2024-09-09T04:54:11.269Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "13cafd74-71af-41e8-8393-bd7df0060831",
      "number": 12,
      "content": "Nó tránh đến với người khôn để học sự khôn ngoan.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.275Z",
      "updatedAt": "2024-09-09T04:54:11.275Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "d3965c70-c4eb-4bc2-a79a-736cf725377b",
      "number": 13,
      "content": "Lòng vui vẻ khiến mặt mày rạng rỡ;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.280Z",
      "updatedAt": "2024-09-09T04:54:11.280Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "bfa3e602-cc84-436c-a5dd-a03fb262508b",
      "number": 13,
      "content": "Dạ âu sầu làm tan vỡ tinh thần.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.285Z",
      "updatedAt": "2024-09-09T04:54:11.285Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "0f2adc2a-97a8-49e8-bf7d-43d587c42bb2",
      "number": 14,
      "content": "Lòng người thông sáng thích kiếm tìm tri thức;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.291Z",
      "updatedAt": "2024-09-09T04:54:11.291Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "2cd36ebc-4cc7-47f9-b098-4dc21b5aa545",
      "number": 14,
      "content": "Miệng kẻ dại khờ ưa ẩm thực điên rồ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.296Z",
      "updatedAt": "2024-09-09T04:54:11.296Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "5e1ce4d9-ac3f-485f-ab19-6c5e38e92e78",
      "number": 15,
      "content": "Kẻ nghèo khó suốt đời chịu khánh kiệt;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.302Z",
      "updatedAt": "2024-09-09T04:54:11.302Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "60c07131-1c33-48c3-b2b5-c85067ae9a27",
      "number": 15,
      "content": "Người vui vẻ trong lòng dự yến tiệc luôn luôn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.307Z",
      "updatedAt": "2024-09-09T04:54:11.307Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "a3c48d1c-80e4-464c-8b1d-0ad9299cf797",
      "number": 16,
      "content": "Thà ít của mà kính sợ Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.312Z",
      "updatedAt": "2024-09-09T04:54:11.312Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "20afc3dd-d1ef-4ad5-bf2c-e2ce4e7b656a",
      "number": 16,
      "content": "Hơn của cải nhiều mà rắc rối ùa theo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.317Z",
      "updatedAt": "2024-09-09T04:54:11.317Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "cb24dc57-5ecd-475f-af1e-1d731faff724",
      "number": 17,
      "content": "Thà ăn cơm với rau mà thương yêu nhau,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.322Z",
      "updatedAt": "2024-09-09T04:54:11.322Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "43552d21-f102-4743-88e5-576a6825b45e",
      "number": 17,
      "content": "Hơn là ăn thịt bò mập béo mà ghen ghét cặp theo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.326Z",
      "updatedAt": "2024-09-09T04:54:11.326Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "59d4a842-64b8-4874-9025-5511ef07e8a3",
      "number": 18,
      "content": "Người nóng tính dễ gây xung đột;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.331Z",
      "updatedAt": "2024-09-09T04:54:11.331Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "c4544ed8-5531-45ef-b43c-37d71af5aee6",
      "number": 18,
      "content": "Còn người chậm nóng giận làm cuộc cãi lộn nguôi đi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.336Z",
      "updatedAt": "2024-09-09T04:54:11.336Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "599f9517-94a6-46ee-ab4b-9d1507d88ae5",
      "number": 19,
      "content": "Đường kẻ biếng nhác gai góc mọc đầy cản trở;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.340Z",
      "updatedAt": "2024-09-09T04:54:11.340Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "650c028b-6af9-47da-84b8-5e2e9944f4d6",
      "number": 19,
      "content": "Còn lối người ngay lành lại mở rộng thênh thang.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.345Z",
      "updatedAt": "2024-09-09T04:54:11.345Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "4dc1e729-68dd-40dd-9c34-03d7abd27d95",
      "number": 20,
      "content": "Ðứa con khôn ngoan làm cho cha vui vẻ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.350Z",
      "updatedAt": "2024-09-09T04:54:11.350Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "6cc29b88-0f57-4957-8ded-b9a1afa0e944",
      "number": 20,
      "content": "Còn kẻ dại dột lại khinh rẻ mẹ mình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.356Z",
      "updatedAt": "2024-09-09T04:54:11.356Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "3b9e9e4b-78aa-434d-ae79-dbcdc19a5034",
      "number": 21,
      "content": "Kẻ kém hiểu biết cho sự điên dại là vui;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.361Z",
      "updatedAt": "2024-09-09T04:54:11.361Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "2dd3834d-b38f-4d0c-864a-41b72a097c56",
      "number": 21,
      "content": "Nhưng người có trí tuệ cứ bước theo đường chính đáng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.365Z",
      "updatedAt": "2024-09-09T04:54:11.365Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "729d4574-0a4b-4cb8-bcca-1caa19ee8b44",
      "number": 22,
      "content": "Thiếu bàn luận các chương trình bị hỏng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.370Z",
      "updatedAt": "2024-09-09T04:54:11.370Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "ccd8969e-d083-43c1-93ea-55b94d36f932",
      "number": 22,
      "content": "Nhưng với mưu sĩ nhiều chúng có triển vọng thành công.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.375Z",
      "updatedAt": "2024-09-09T04:54:11.375Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "6d0792d3-b58f-4f78-8143-59f1a35462a9",
      "number": 23,
      "content": "Lời đáp hay làm mọi người vui thích;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.380Z",
      "updatedAt": "2024-09-09T04:54:11.380Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "8e17bded-b61e-43be-b3d0-e41ab40fd55e",
      "number": 23,
      "content": "Lời đáp hay đúng lúc thật bổ ích biết bao!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.385Z",
      "updatedAt": "2024-09-09T04:54:11.385Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "91c8aac4-98b3-4e45-ae14-bb4afc8ed13e",
      "number": 24,
      "content": "Người khôn ngoan bước theo con đường sự sống dẫn đi lên,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.390Z",
      "updatedAt": "2024-09-09T04:54:11.390Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "e6ba2f4c-5ea4-42bd-bb2a-0b547764bc6e",
      "number": 24,
      "content": "Ðể giữ người ấy tránh xa âm phủ ở phía dưới.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.395Z",
      "updatedAt": "2024-09-09T04:54:11.395Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "27bc2247-0470-488f-8607-dd8a77b7fdbc",
      "number": 25,
      "content": "Chúa phá hủy nhà của kẻ kiêu ngạo;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.400Z",
      "updatedAt": "2024-09-09T04:54:11.400Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "86f5e373-400e-47a8-9869-5d0fc0713524",
      "number": 25,
      "content": "Nhưng Ngài bảo vệ địa giới của người góa bụa.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.406Z",
      "updatedAt": "2024-09-09T04:54:11.406Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "988580b5-99cb-446c-b43d-3360bcc3538b",
      "number": 26,
      "content": "Ý đồ của kẻ gian ác là gớm ghiếc trước mặt Chúa;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.411Z",
      "updatedAt": "2024-09-09T04:54:11.411Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "84160efa-95e9-489d-aeb5-65c9114d21f8",
      "number": 26,
      "content": "Nhưng lời của người trong sạch được đẹp lòng Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.416Z",
      "updatedAt": "2024-09-09T04:54:11.416Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "f1cb4c54-0495-4af7-b75e-55a31822b490",
      "number": 27,
      "content": "Kẻ tham lợi bất nghĩa chuốc lấy rắc rối cho nhà mình;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.422Z",
      "updatedAt": "2024-09-09T04:54:11.422Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "c659fc8b-0d8c-495c-a840-9b61f97917bb",
      "number": 27,
      "content": "Nhưng ai ghét của hối lộ sẽ tiếp tục sống còn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.427Z",
      "updatedAt": "2024-09-09T04:54:11.427Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "1e56389c-b5a0-4660-bacb-070968fc01a1",
      "number": 28,
      "content": "Người ngay lành suy nghĩ trong lòng rồi mới đáp;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.432Z",
      "updatedAt": "2024-09-09T04:54:11.432Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "a4c63a55-22a0-4a90-8c80-a554a2b20043",
      "number": 28,
      "content": "Còn miệng kẻ gian ác vội nói những lời độc ác hại người.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.437Z",
      "updatedAt": "2024-09-09T04:54:11.437Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "d06444a9-5d19-438b-95ee-e02ba3df2747",
      "number": 29,
      "content": "Chúa thật cách xa đối với phường gian ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.442Z",
      "updatedAt": "2024-09-09T04:54:11.442Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "e4ff80f9-8e3f-4089-a3b6-020a297c5dbd",
      "number": 29,
      "content": "Nhưng Ngài lắng nghe lời cầu nguyện của người ngay lành.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.448Z",
      "updatedAt": "2024-09-09T04:54:11.448Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "0ad5c646-bb84-4dbc-97b2-37496190d4be",
      "number": 30,
      "content": "Ánh mắt hân hoan phản ánh lòng vui vẻ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.452Z",
      "updatedAt": "2024-09-09T04:54:11.452Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "7bcb1fec-20e6-441a-ae00-aea6a8fa3d85",
      "number": 30,
      "content": "Tin tức vui mừng làm mạnh khỏe cốt xương.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.458Z",
      "updatedAt": "2024-09-09T04:54:11.458Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "e546eefa-dcaf-46d1-a1e7-0a4959626aa6",
      "number": 31,
      "content": "Tai chịu lắng nghe những lời quở trách về cuộc sống,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.463Z",
      "updatedAt": "2024-09-09T04:54:11.463Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "b2fa49c4-30b7-43d2-be97-f6986fae4e16",
      "number": 31,
      "content": "Người ấy sẽ ở giữa các bậc hiền triết khôn ngoan.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.468Z",
      "updatedAt": "2024-09-09T04:54:11.468Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "d8097f52-8f30-48d5-8123-45091b26859a",
      "number": 32,
      "content": "Kẻ từ chối nghe lời chỉ giáo khinh bỉ linh hồn mình,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.474Z",
      "updatedAt": "2024-09-09T04:54:11.474Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "2d1e07c6-f57f-4bcd-bef4-3542a16925f7",
      "number": 32,
      "content": "Nhưng ai chịu nghe lời khuyên bảo sẽ được sự thông sáng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.479Z",
      "updatedAt": "2024-09-09T04:54:11.479Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "bdd0ee8c-d4eb-40d1-9e4e-8a3cad2b8837",
      "number": 33,
      "content": "Kính sợ Chúa là cách giáo dục khôn ngoan,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.484Z",
      "updatedAt": "2024-09-09T04:54:11.484Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    },
    {
      "id": "b7cdd61b-820c-4677-998b-1c235ae13354",
      "number": 33,
      "content": "Ðức khiêm nhường đi trước sự tôn trọng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 23,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:54:11.490Z",
      "updatedAt": "2024-09-09T04:54:11.490Z",
      "chapterId": "484c3214-f9bf-46a4-a1c7-64963efa6bf9"
    }
  ],
