{
  "chapter": {
    "id": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd",
    "number": 3,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%203&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.527Z",
    "updatedAt": "2024-09-24T01:00:45.527Z",
    "bookId": "2a7cfb76-854f-4125-8376-cd1d8bb799d7"
  },
  "verses": [
    {
      "id": "6f5f34dd-0921-4f93-937f-4f57d914a6aa",
      "number": 1,
      "content": "Con ơi, chớ quên luật pháp ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.922Z",
      "updatedAt": "2024-09-24T02:14:04.922Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "96cc6c44-5311-490b-8dda-b6407020a36e",
      "number": 1,
      "content": "Nhưng hãy để lòng con gìn giữ mệnh lệnh ta;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.939Z",
      "updatedAt": "2024-09-24T02:14:04.939Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "ce5f9a0f-ac60-4f1a-b924-327521db1f2c",
      "number": 2,
      "content": "Vì chúng sẽ làm gia tăng số ngày và số năm của đời con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.944Z",
      "updatedAt": "2024-09-24T02:14:04.944Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "7f5e3bbb-f3e0-4156-9e11-5616c368d6b4",
      "number": 2,
      "content": "Chúng sẽ đem lại cho con bình an thịnh vượng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.948Z",
      "updatedAt": "2024-09-24T02:14:04.948Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "aa236052-e99e-44d7-af15-cf20dbf28643",
      "number": 3,
      "content": "Ðừng để lòng thương xót và tính chân thật lìa khỏi con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.951Z",
      "updatedAt": "2024-09-24T02:14:04.951Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "fcf9d79a-ed4b-40e5-b6cf-5098db994c50",
      "number": 3,
      "content": "Hãy buộc chúng quanh cổ con;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.954Z",
      "updatedAt": "2024-09-24T02:14:04.954Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "d37b51f1-98f7-4b17-859c-cfe22714fb73",
      "number": 3,
      "content": "Hãy ghi tạc chúng vào bia lòng con.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.957Z",
      "updatedAt": "2024-09-24T02:14:04.957Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "03131bc8-faa2-44c8-966f-dc1643ccaca5",
      "number": 4,
      "content": "Vì như thế con sẽ được ơn và được tiếng tốt,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.960Z",
      "updatedAt": "2024-09-24T02:14:04.960Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "491f74cd-44dc-44d5-875e-22b7f6975523",
      "number": 4,
      "content": "Trước mặt Ðức Chúa Trời và luôn cả người ta.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.963Z",
      "updatedAt": "2024-09-24T02:14:04.963Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "20f0c38e-e2d0-4a31-9351-60dfc29c540f",
      "number": 5,
      "content": "Hãy hết lòng tin cậy Chúa;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.966Z",
      "updatedAt": "2024-09-24T02:14:04.966Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "1e8ec455-2fc5-4a3d-84d7-b0743573ca81",
      "number": 5,
      "content": "Chớ cậy vào sự thông sáng của con;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.969Z",
      "updatedAt": "2024-09-24T02:14:04.969Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "bd43f38d-10c5-432d-965c-11c2fb9370c9",
      "number": 6,
      "content": "Phàm trong các việc làm của con, khá nhận biết Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.971Z",
      "updatedAt": "2024-09-24T02:14:04.971Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "a6412141-85af-4d90-835b-369c6e7cb8da",
      "number": 6,
      "content": "Thì Ngài sẽ chỉ dẫn các lối của con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.974Z",
      "updatedAt": "2024-09-24T02:14:04.974Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "1d260440-97db-4c0c-b8a6-fe460f143aa8",
      "number": 7,
      "content": "Ðừng cho mình là khôn ngoan theo mắt mình;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.977Z",
      "updatedAt": "2024-09-24T02:14:04.977Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "80e8d8d3-5ee7-4514-b96d-bc4d6c39ab61",
      "number": 7,
      "content": "Hãy kính sợ Chúa và lánh xa điều ác.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.980Z",
      "updatedAt": "2024-09-24T02:14:04.980Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "19163b8d-dd02-4aa0-8fed-0299fda4636c",
      "number": 8,
      "content": "Như vậy thân xác con sẽ được mạnh khỏe,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.983Z",
      "updatedAt": "2024-09-24T02:14:04.983Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "1301e094-7abe-468d-a636-1b127dec584c",
      "number": 8,
      "content": "Và xương cốt con sẽ được cứng cáp vững vàng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.985Z",
      "updatedAt": "2024-09-24T02:14:04.985Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "b28c2e0a-fafd-480b-a62a-e695b9633b97",
      "number": 9,
      "content": "Hãy dùng của cải con để tôn vinh Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.988Z",
      "updatedAt": "2024-09-24T02:14:04.988Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "65579ef8-459f-4338-9f7b-643a8522cb09",
      "number": 9,
      "content": "Hãy dâng các hoa lợi đầu mùa của con để cảm tạ Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.990Z",
      "updatedAt": "2024-09-24T02:14:04.990Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "7875b9cf-7286-4ab2-bbbe-48c1c2512d3e",
      "number": 10,
      "content": "Như vậy các vựa lẫm của con sẽ tràn đầy lúa thóc,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.993Z",
      "updatedAt": "2024-09-24T02:14:04.993Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "f6be1bf2-fe8d-4860-aeef-70e9b7152cbd",
      "number": 10,
      "content": "Các thùng rượu của con rượu mới sẽ tràn đầy.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.995Z",
      "updatedAt": "2024-09-24T02:14:04.995Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "4e6e868f-38bb-4569-956b-10eddde53ba1",
      "number": 11,
      "content": "Con ơi, chớ coi thường sự sửa phạt của Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:04.998Z",
      "updatedAt": "2024-09-24T02:14:04.998Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "76323edd-73bf-44b5-8b06-3006eae09769",
      "number": 11,
      "content": "Ðừng bất mãn trước sự quở trách của Ngài;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05Z",
      "updatedAt": "2024-09-24T02:14:05Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "1aaa1aa8-165c-404c-a5b4-59ff46f318d2",
      "number": 12,
      "content": "Vì ai được Chúa thương thì Ngài quở trách,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.002Z",
      "updatedAt": "2024-09-24T02:14:05.002Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "95e96291-9a6c-4df0-bc72-81ca165962d7",
      "number": 12,
      "content": "Như cha thương con mình nên trách phạt khuyên răn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.005Z",
      "updatedAt": "2024-09-24T02:14:05.005Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "12fc0cd7-0e22-415a-a644-f889ec1f220a",
      "number": 13,
      "content": "Phước thay cho người tìm được sự khôn ngoan,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.007Z",
      "updatedAt": "2024-09-24T02:14:05.007Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "c6e5cbd3-ccc7-4f93-8210-3a920e208196",
      "number": 13,
      "content": "Và cho người có được sự thông sáng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.011Z",
      "updatedAt": "2024-09-24T02:14:05.011Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "bd01a385-f19f-4102-bba8-39e7b4e5b9fb",
      "number": 14,
      "content": "Vì lợi ích của khôn ngoan trỗi hơn tiền bạc;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.014Z",
      "updatedAt": "2024-09-24T02:14:05.014Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "a5640ef1-04be-46fa-9cce-37a3d7697729",
      "number": 14,
      "content": "Ích lợi của nàng lấn át cả vàng ròng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.016Z",
      "updatedAt": "2024-09-24T02:14:05.016Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "c940cf67-b062-479c-95dc-91f21b75a57d",
      "number": 15,
      "content": "Nàng quý báu hơn châu ngọc;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.018Z",
      "updatedAt": "2024-09-24T02:14:05.018Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "5a794e41-dd7d-4696-9e0b-f8fe2f2e3392",
      "number": 15,
      "content": "Chẳng có vật gì con ưa thích có thể sánh được với nàng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.020Z",
      "updatedAt": "2024-09-24T02:14:05.020Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "c6d3b505-bd04-4b49-92ba-a7e8d2657f3c",
      "number": 16,
      "content": "Tay phải nàng phát ban trường thọ;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.023Z",
      "updatedAt": "2024-09-24T02:14:05.023Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "ac9e9078-5add-4924-a0e1-26edfc9bc59a",
      "number": 16,
      "content": "Tay trái nàng cho giàu có lẫn hiển vinh.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.025Z",
      "updatedAt": "2024-09-24T02:14:05.025Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "38bea297-2977-45d7-8c17-05d7db906e21",
      "number": 17,
      "content": "Con đường nàng là con đường vui thỏa;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.027Z",
      "updatedAt": "2024-09-24T02:14:05.027Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "348e238f-dea0-4c19-8104-b0755621c9ad",
      "number": 17,
      "content": "Mọi nẻo nàng là mọi ngả bình an.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.029Z",
      "updatedAt": "2024-09-24T02:14:05.029Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "0efe2431-f361-44a1-9a84-fe8446d3d0e3",
      "number": 18,
      "content": "Khôn ngoan là cây sự sống cho người nào nắm giữ được;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.032Z",
      "updatedAt": "2024-09-24T02:14:05.032Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "c52a4410-e59d-4441-9079-35e8aea27dd3",
      "number": 18,
      "content": "Ai giữ chặt nàng sẽ được phước lâu dài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.034Z",
      "updatedAt": "2024-09-24T02:14:05.034Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "ca57a882-ecee-4191-8acd-0fb4e7b9214b",
      "number": 19,
      "content": "Chúa dùng sự khôn ngoan để dựng nên trái đất;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.036Z",
      "updatedAt": "2024-09-24T02:14:05.036Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "f7127601-8580-47b6-9d07-f977ffe4a314",
      "number": 19,
      "content": "Ngài dùng sự thông sáng để tạo lập các tầng trời;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.041Z",
      "updatedAt": "2024-09-24T02:14:05.041Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "55f2487d-f015-42ac-a583-dc47bf155cb8",
      "number": 20,
      "content": "Nhờ tri thức của Ngài các vực sâu mở rộng;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.044Z",
      "updatedAt": "2024-09-24T02:14:05.044Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "68e44e62-9f55-4f21-abc3-f1b3646e626d",
      "number": 20,
      "content": "Những áng mây mù nhỏ những giọt sương đêm.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.049Z",
      "updatedAt": "2024-09-24T02:14:05.049Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "d6ed4c32-ca50-438a-a82b-ee69109eb8df",
      "number": 21,
      "content": "Con ơi, đừng để những điều này vuột khỏi tầm mắt con:",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.054Z",
      "updatedAt": "2024-09-24T02:14:05.054Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "88c83387-52eb-43a3-8b17-1ef8874a3ad7",
      "number": 21,
      "content": "Hãy giữ lấy sự khôn ngoan và sự cẩn trọng.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.070Z",
      "updatedAt": "2024-09-24T02:14:05.070Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "d34c3f3f-25b5-4b50-b9f5-e007b6872dc9",
      "number": 22,
      "content": "Những điều ấy sẽ là sức sống của linh hồn con,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.075Z",
      "updatedAt": "2024-09-24T02:14:05.075Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "9887b125-667a-41e7-b6f1-efe9aaa43f2e",
      "number": 22,
      "content": "Và là những đồ trang sức nơi cổ con.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.080Z",
      "updatedAt": "2024-09-24T02:14:05.080Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "2a54ace3-09a8-4555-afa7-902cff681890",
      "number": 23,
      "content": "Nhờ những điều ấy đường con đi sẽ an toàn bảo đảm,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.085Z",
      "updatedAt": "2024-09-24T02:14:05.085Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "82253f6d-a69c-4ed9-b583-9f9f697cf843",
      "number": 23,
      "content": "Bước chân con sẽ chẳng vấp phạm bao giờ.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.090Z",
      "updatedAt": "2024-09-24T02:14:05.090Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "23e5500a-e721-4889-b9fb-dbcb337fd925",
      "number": 24,
      "content": "Lúc nằm nghỉ con sẽ không lo sợ;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.095Z",
      "updatedAt": "2024-09-24T02:14:05.095Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "823a5094-2d89-4881-b202-5b57defe0873",
      "number": 24,
      "content": "Khi đặt lưng con sẽ an giấc ngủ ngon.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.100Z",
      "updatedAt": "2024-09-24T02:14:05.100Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "bb29bab3-9b7b-4077-860f-3796748fc004",
      "number": 25,
      "content": "Ðừng sợ hãi nỗi kinh hoàng xảy ra bất chợt;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.105Z",
      "updatedAt": "2024-09-24T02:14:05.105Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "a3a8d64e-be07-444d-8f1a-3dee6d0b7528",
      "number": 25,
      "content": "Ðừng âu lo khi đợt phá hoại của kẻ ác đến gần;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.110Z",
      "updatedAt": "2024-09-24T02:14:05.110Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "6e832335-2371-4371-880a-b4e95a08ffd2",
      "number": 26,
      "content": "Vì Chúa sẽ là nguồn trông cậy của con;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.115Z",
      "updatedAt": "2024-09-24T02:14:05.115Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "a12b3cae-eaa7-45da-b7a0-be94f34fd602",
      "number": 26,
      "content": "Ngài sẽ giữ chân con khỏi sa vào bẫy.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.120Z",
      "updatedAt": "2024-09-24T02:14:05.120Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "d04cc44e-2763-4481-8096-cad23cd5b386",
      "number": 27,
      "content": "Ðừng từ chối làm việc thiện cho những người đáng giúp,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.124Z",
      "updatedAt": "2024-09-24T02:14:05.124Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "80b28a0a-0e3b-48e2-a3fd-4046d72ecbd8",
      "number": 27,
      "content": "Nếu con có khả năng, hãy giúp đỡ người ta.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.129Z",
      "updatedAt": "2024-09-24T02:14:05.129Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "7b7be566-a724-4c7d-b722-9b5b4018b20c",
      "number": 28,
      "content": "Con đừng nói với người lân cận, “Về đi, mai trở lại, tôi sẽ cho,”",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.134Z",
      "updatedAt": "2024-09-24T02:14:05.134Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "01325518-2549-49b4-a848-8e6baf2531f2",
      "number": 28,
      "content": "Trong khi con có sẵn điều người ấy đang cần.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.139Z",
      "updatedAt": "2024-09-24T02:14:05.139Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "48428904-12bd-481d-88c1-3d782890ba5e",
      "number": 29,
      "content": "Con chớ lập mưu hại người lân cận,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.144Z",
      "updatedAt": "2024-09-24T02:14:05.144Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "2f55b7c8-9123-4d99-8c4b-61738876c54d",
      "number": 29,
      "content": "Người ở gần và tin tưởng nơi con.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.149Z",
      "updatedAt": "2024-09-24T02:14:05.149Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "9e003d4b-7147-411d-ae60-1644fb705673",
      "number": 30,
      "content": "Con đừng gây sự với ai vô cớ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.154Z",
      "updatedAt": "2024-09-24T02:14:05.154Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "aa68f5ec-32c7-4ed4-85fc-9ae4fb0c9104",
      "number": 30,
      "content": "Khi không ai gây cho con thiệt hại điều gì.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.159Z",
      "updatedAt": "2024-09-24T02:14:05.159Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "5c835b4d-cfe0-4c6a-8fa5-0b85f55e223b",
      "number": 31,
      "content": "Ðừng ganh tị với quân tàn bạo;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.164Z",
      "updatedAt": "2024-09-24T02:14:05.164Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "2c31b559-2268-4a4a-a6ec-ab6f509e9b73",
      "number": 31,
      "content": "Chớ chọn đường nào chúng chọn lấy để đi;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.169Z",
      "updatedAt": "2024-09-24T02:14:05.169Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "6cbce856-d76f-46f1-915b-9573ad37ea4c",
      "number": 32,
      "content": "Vì Chúa ghê tởm kẻ gian tà tội lỗi,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.174Z",
      "updatedAt": "2024-09-24T02:14:05.174Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "248e5c43-0e27-439a-bcc6-9e22502987b1",
      "number": 32,
      "content": "Nhưng Ngài kết bạn tâm giao với người chính trực ngay lành.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.179Z",
      "updatedAt": "2024-09-24T02:14:05.179Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "728bc939-3be1-4b67-85aa-75c0a67e35a1",
      "number": 33,
      "content": "Sự nguyền rủa của Chúa giáng trên nhà kẻ gian ác,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.184Z",
      "updatedAt": "2024-09-24T02:14:05.184Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "6c4dc8a8-a471-4bed-b9d5-a4b8f4449bc9",
      "number": 33,
      "content": "Nhưng phước hạnh Ngài ban cho chỗ ở người ngay lành.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.188Z",
      "updatedAt": "2024-09-24T02:14:05.188Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "664aea05-0498-4208-835b-cbee170d95da",
      "number": 34,
      "content": "Ngài chế giễu phường chê bai nhạo báng,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.193Z",
      "updatedAt": "2024-09-24T02:14:05.193Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "2a5cca4c-50bc-44bf-92cc-36d7cc5169da",
      "number": 34,
      "content": "Nhưng Ngài ban ơn cho người thật sự khiêm nhường.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.198Z",
      "updatedAt": "2024-09-24T02:14:05.198Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "b3dde9ff-bc17-4c59-aec5-6874510d5da9",
      "number": 35,
      "content": "Người khôn ngoan sẽ thừa hưởng những hiển vinh tôn trọng,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.203Z",
      "updatedAt": "2024-09-24T02:14:05.203Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    },
    {
      "id": "0e439389-af19-42c1-9fbc-4692283d4278",
      "number": 35,
      "content": "Còn kẻ điên rồ bị tống cho nhục nhã ê chề.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:05.208Z",
      "updatedAt": "2024-09-24T02:14:05.208Z",
      "chapterId": "8aca6f23-0d29-4bbb-a31f-365062ccd8bd"
    }
  ],
