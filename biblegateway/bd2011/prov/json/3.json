    },
    {
      "id": "818f6d0e-8625-4a3a-bd26-5d9068b5ab01",
      "content": "Sự Khôn Ngoan trong Công Cuộc Sáng Tạo",
      "order": 0,
      "createdAt": "2024-09-09T04:53:37.856Z",
      "updatedAt": "2024-09-09T04:53:37.856Z",
