    },
    {
      "id": "b967a038-63a8-4cfb-9525-a05a43fb9f8a",
      "content": "Sự Khôn Ngoan trong Công Cuộc Sáng Tạo",
      "order": 0,
      "createdAt": "2024-09-24T02:14:05.039Z",
      "updatedAt": "2024-09-24T02:14:05.039Z",
