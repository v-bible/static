    },
    {
      "id": "6797e320-74c1-4bdc-b702-f6e8f9584bde",
      "content": "Sự Khôn Ngoan trong Công Cuộc Sáng Tạo",
      "order": 0,
      "createdAt": "2024-07-15T03:15:45.806Z",
      "updatedAt": "2024-07-15T03:15:45.806Z",
