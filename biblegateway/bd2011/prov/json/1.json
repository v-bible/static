    },
    {
      "id": "1a974d09-364e-4530-a8c3-a36ae78ef96f",
      "content": "Hãy Tránh Xa Kẻ Xấu",
      "order": 0,
      "createdAt": "2024-09-09T04:53:31.525Z",
      "updatedAt": "2024-09-09T04:53:31.525Z",
