{
  "chapter": {
    "id": "e931ca43-1fac-4dc4-850e-359ba9403106",
    "number": 14,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2014&version=BD2011",
    "createdAt": "2024-07-12T04:03:49.080Z",
    "updatedAt": "2024-07-24T15:07:49.894Z",
    "bookId": "f88be0f6-2d78-40e0-8b41-6684c15a16e0"
  },
  "verses": [
    {
      "id": "30af9098-b775-40d5-b96d-86e08caca973",
      "number": 1,
      "content": "Người nữ khôn ngoan xây dựng gia đình mình hạnh phúc,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.361Z",
      "updatedAt": "2024-07-15T03:17:49.729Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "9be079ba-4da1-42f1-b126-c953e4f21ca3",
      "number": 1,
      "content": "Còn kẻ dại dột dùng chính tay mình phá hủy nó đi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.361Z",
      "updatedAt": "2024-07-15T03:17:49.772Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "fbc67826-0bd5-4a80-9b60-36389fa63e38",
      "number": 2,
      "content": "Người sống ngay thẳng là người kính sợ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.371Z",
      "updatedAt": "2024-07-15T03:17:49.788Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "978c75b5-4118-4513-a461-434a46371ed1",
      "number": 2,
      "content": "Kẻ ăn ở gian tà chính là kẻ khinh thường Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.371Z",
      "updatedAt": "2024-07-15T03:17:49.800Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "61bfd652-63c4-4e10-8a0b-bb914e0fffb7",
      "number": 3,
      "content": "Lời ngạo mạn ra từ miệng kẻ dại trở thành roi đánh nó,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.373Z",
      "updatedAt": "2024-07-15T03:17:49.814Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "b5043c3d-bf9f-4ac9-84ae-9cea1f65318d",
      "number": 3,
      "content": "Môi người khôn bảo vệ mạng sống mình.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.373Z",
      "updatedAt": "2024-07-15T03:17:49.823Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "b58d7560-a78b-44e4-afec-9a82f5a0aa89",
      "number": 4,
      "content": "Nơi nào bò không cày, nơi đó không lúa gạo,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.376Z",
      "updatedAt": "2024-07-15T03:17:49.832Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "a677b88d-3b26-430c-b867-fc6d52a1a163",
      "number": 4,
      "content": "Hoa lợi nhiều nhờ sức lực của bò đực mà ra.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.376Z",
      "updatedAt": "2024-07-15T03:17:49.841Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "a0033969-c33b-461a-8048-94e0c5054df5",
      "number": 5,
      "content": "Nhân chứng trung tín không bao giờ nói dối,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.378Z",
      "updatedAt": "2024-07-15T03:17:49.851Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "1a43468d-8080-4fba-8aa6-c75703c4c699",
      "number": 5,
      "content": "Kẻ chứng gian vừa thở ra là có hơi hám dối trá rồi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.378Z",
      "updatedAt": "2024-07-15T03:17:49.860Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "a060e94e-eea3-4a83-aafe-0c9bde4ef34a",
      "number": 6,
      "content": "Kẻ nhạo báng tìm khôn ngoan hoài không được,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.380Z",
      "updatedAt": "2024-07-15T03:17:49.869Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "ee083257-e10b-426d-b2cf-ae30cf5a0651",
      "number": 6,
      "content": "Còn người thông minh tri thức đến dễ dàng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.380Z",
      "updatedAt": "2024-07-15T03:17:49.878Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "0a14a51b-9e6d-4d62-a624-d6b131561fd6",
      "number": 7,
      "content": "Hãy dang xa con người điên dại,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.383Z",
      "updatedAt": "2024-07-15T03:17:49.886Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "d2a8624b-595a-4037-b378-258526b42fc4",
      "number": 7,
      "content": "Vì bạn sẽ chẳng thấy tri thức ra từ miệng nó bao giờ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:17:49.383Z",
      "updatedAt": "2024-07-15T03:17:49.896Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "2d0285c4-945a-42e3-b597-ecb861c4894a",
      "number": 8,
      "content": "Cái khôn của người thận trọng là biết toan tính đường lối mình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.385Z",
      "updatedAt": "2024-07-15T03:17:49.385Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "2b25e44e-cee9-4d07-a746-e39e2c0436bb",
      "number": 8,
      "content": "Cái dại của kẻ điên dại là mánh khóe dối gian.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.385Z",
      "updatedAt": "2024-07-15T03:17:49.385Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "56eb9eb9-2982-47ca-88e3-18a3cec44738",
      "number": 9,
      "content": "Những kẻ điên dại chê cười của lễ chuộc tội;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.387Z",
      "updatedAt": "2024-07-15T03:17:49.387Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "e950141f-12c8-479f-83b3-83df6fc8dc2e",
      "number": 9,
      "content": "Nhưng người ngay lành tin rằng nhờ nó mà được đoái thương.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.387Z",
      "updatedAt": "2024-07-15T03:17:49.387Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "99bffe82-671a-407d-9508-f0ea4fba65cb",
      "number": 10,
      "content": "Nỗi đắng cay chỉ có lòng mình biết;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.389Z",
      "updatedAt": "2024-07-15T03:17:49.389Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "de7094bd-9682-4bc3-8edd-c6b861694448",
      "number": 10,
      "content": "Và niềm vui nào chia sẻ hết được với người dưng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.389Z",
      "updatedAt": "2024-07-15T03:17:49.389Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "caa810b6-f2e8-43c9-aad6-0109e0598e65",
      "number": 11,
      "content": "Nhà kẻ gian ác trước sau gì cũng bị hủy hoại,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.392Z",
      "updatedAt": "2024-07-15T03:17:49.392Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "0747f84d-7480-419e-b478-8c1f23e8dcc1",
      "number": 11,
      "content": "Trại người ngay lành cứ vững chãi lâu dài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.392Z",
      "updatedAt": "2024-07-15T03:17:49.392Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "2ba8146e-d603-4c3a-8589-90601f606ce2",
      "number": 12,
      "content": "Có một con đường dường như chính đáng cho người ta,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.394Z",
      "updatedAt": "2024-07-15T03:17:49.394Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "87e195e2-e948-4b5c-9d38-b0022243fd66",
      "number": 12,
      "content": "Nhưng đến cuối đường hóa ra là tử lộ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.394Z",
      "updatedAt": "2024-07-15T03:17:49.394Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "f99aefdb-6493-4a5a-8a56-1f957dd663e0",
      "number": 13,
      "content": "Dù ngoài mặt cười vang, nhưng trong lòng có thể đang sầu héo;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.397Z",
      "updatedAt": "2024-07-15T03:17:49.397Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "fb41b0b2-cb01-4b7c-92cc-dbb1b1855598",
      "number": 13,
      "content": "Cuối cuộc vui nỗi sầu khổ vẫn kéo lại như thường.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.397Z",
      "updatedAt": "2024-07-15T03:17:49.397Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "d1d2ad03-a050-46cd-a06a-6c12e5d5ea0c",
      "number": 14,
      "content": "Kẻ tà tâm sẽ lãnh đủ hậu quả xấu mình gây ra;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.403Z",
      "updatedAt": "2024-07-15T03:17:49.403Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "1b7d5492-1c3e-489d-b6b7-0b28e3738eaf",
      "number": 14,
      "content": "Người thiện tâm sẽ được thưởng xứng đáng với việc mình làm.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.403Z",
      "updatedAt": "2024-07-15T03:17:49.403Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "d4655c13-73e2-41e7-8db5-ebcbbbf69149",
      "number": 15,
      "content": "Kẻ dại dột tin mọi lời người ta nói;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.408Z",
      "updatedAt": "2024-07-15T03:17:49.408Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "6c5abdf0-5a51-4297-ac32-c869f51934bc",
      "number": 15,
      "content": "Nhưng người khôn ngoan thận trọng từng bước một của mình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.408Z",
      "updatedAt": "2024-07-15T03:17:49.408Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "d0ba9a0a-72a5-4757-8c89-9fbc0c0e4270",
      "number": 16,
      "content": "Người khôn ngoan ghê sợ và lánh xa điều ác;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.414Z",
      "updatedAt": "2024-07-15T03:17:49.414Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "cd8c1d26-b6fe-4125-bfc2-bdf4c48aea26",
      "number": 16,
      "content": "Còn kẻ dại dột thì hành động khinh suất và bất cẩn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.414Z",
      "updatedAt": "2024-07-15T03:17:49.414Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "ff02ea4c-de8e-40cd-a93a-1a79962235f4",
      "number": 17,
      "content": "Kẻ nhạy giận dễ hành động điên rồ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.420Z",
      "updatedAt": "2024-07-15T03:17:49.420Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "726a9dac-83a8-4d19-afbb-e26daef3aef9",
      "number": 17,
      "content": "Kẻ toan tính chuyện ác ắt bị người ta ghét.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.420Z",
      "updatedAt": "2024-07-15T03:17:49.420Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "e62cb87c-8a6e-403e-9a3c-5a471f8bfaef",
      "number": 18,
      "content": "Kẻ khờ dại thừa hưởng sự điên dại;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.425Z",
      "updatedAt": "2024-07-15T03:17:49.425Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "43a2a581-f9f1-4fb6-9af3-c102d2faaa8d",
      "number": 18,
      "content": "Người khôn ngoan lấy tri thức làm mão miện của mình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.425Z",
      "updatedAt": "2024-07-15T03:17:49.425Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "61c554ed-91ba-478f-8060-4fee36f95f52",
      "number": 19,
      "content": "Kẻ xấu phải sấp mình trước mặt người tốt,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.431Z",
      "updatedAt": "2024-07-15T03:17:49.431Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "8b23ae03-7ed0-46e8-9a3b-a0a82c6a64ef",
      "number": 19,
      "content": "Kẻ gian ác phải chầu chực trước cổng người ngay lành.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.431Z",
      "updatedAt": "2024-07-15T03:17:49.431Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "6c76159b-7c93-47dd-a2d6-bff465feb65a",
      "number": 20,
      "content": "Người nghèo khó ngay cả láng giềng cũng không thích,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.437Z",
      "updatedAt": "2024-07-15T03:17:49.437Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "6fc9036f-07cc-40df-9587-e5f278e17a5a",
      "number": 20,
      "content": "Kẻ giàu sang tính không hết bạn bè.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.437Z",
      "updatedAt": "2024-07-15T03:17:49.437Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "800e66d1-0286-4b5f-9fe9-7fd53603332b",
      "number": 21,
      "content": "Ai khi dể người lân cận là mắc tội,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.442Z",
      "updatedAt": "2024-07-15T03:17:49.442Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "3185c4da-260b-489b-b1d8-166fd6071d12",
      "number": 21,
      "content": "Phước cho ai thương xót người nghèo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.442Z",
      "updatedAt": "2024-07-15T03:17:49.442Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "6001a666-cdcf-43a9-ad74-cabd19fcbaea",
      "number": 22,
      "content": "Há chẳng phải kẻ toan mưu ác đang sai lạc sao?",
      "order": 0,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.448Z",
      "updatedAt": "2024-07-15T03:17:49.448Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "bb182f54-076b-44f6-9fd7-837d68ede0d6",
      "number": 22,
      "content": "Người nỗ lực làm điều lành được xót thương và tin cậy.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.448Z",
      "updatedAt": "2024-07-15T03:17:49.448Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "30353e95-5dba-4d2a-829e-cce3d29379a8",
      "number": 23,
      "content": "Tất cả các hình thức lao động đều đem lại ích lợi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 16,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.454Z",
      "updatedAt": "2024-07-15T03:17:49.454Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "b5403b0f-bb9d-40a3-9b7a-18d45b0b305f",
      "number": 23,
      "content": "Còn chỉ nói và lười ắt dẫn đến đói nghèo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 17,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.454Z",
      "updatedAt": "2024-07-15T03:17:49.454Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "1e5ea9fe-08b6-499b-bb21-7d64d5e95f37",
      "number": 24,
      "content": "Giàu có là mão miện của người khôn ngoan,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.459Z",
      "updatedAt": "2024-07-15T03:17:49.459Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "3c08e2a4-6b87-42ed-a6dd-9285e5e02a7f",
      "number": 24,
      "content": "Ðiên cuồng là vòng hoa của kẻ điên dại.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 19,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.459Z",
      "updatedAt": "2024-07-15T03:17:49.459Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "a19f0e99-a6d8-4b24-bd35-25daa12e92ca",
      "number": 25,
      "content": "Người chứng chân thật cứu biết bao mạng sống,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 20,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.465Z",
      "updatedAt": "2024-07-15T03:17:49.465Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "61578f03-8e45-42ef-95f6-bd7d162ee84f",
      "number": 25,
      "content": "Còn kẻ nói dối thở ra toàn hơi hám dối gian.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 21,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.465Z",
      "updatedAt": "2024-07-15T03:17:49.465Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "3504fcd6-63a4-4244-bace-88741b6c43c7",
      "number": 26,
      "content": "Người kính sợ Chúa được thành lũy an toàn bảo vệ,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 22,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.471Z",
      "updatedAt": "2024-07-15T03:17:49.471Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "623363d4-a9d6-460a-b657-a5e8f842ff7c",
      "number": 26,
      "content": "Con cái người ấy có một chỗ để an thân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 23,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.471Z",
      "updatedAt": "2024-07-15T03:17:49.471Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "d8277433-e33e-4712-90d8-0be394d1a3b5",
      "number": 27,
      "content": "Kính sợ Chúa là nguồn sự sống,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 24,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.476Z",
      "updatedAt": "2024-07-15T03:17:49.476Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "5499f0da-14f7-4b26-9226-e6fc672411de",
      "number": 27,
      "content": "Giúp người ta tránh được bẫy tử vong.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 25,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.476Z",
      "updatedAt": "2024-07-15T03:17:49.476Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "4405dfc4-c313-47a8-a692-595749b00749",
      "number": 28,
      "content": "Vua được vinh hiển nhờ có đông dân chúng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 26,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.482Z",
      "updatedAt": "2024-07-15T03:17:49.482Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "0ca0b96d-f513-417c-aa89-b7cf88ebb45a",
      "number": 28,
      "content": "Không có dân vua lúng túng chịu thua.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 27,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.482Z",
      "updatedAt": "2024-07-15T03:17:49.482Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "59db8001-7964-4ba9-9791-c83880e4c3d5",
      "number": 29,
      "content": "Người chậm nóng giận là người đại trí,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 28,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.488Z",
      "updatedAt": "2024-07-15T03:17:49.488Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "7064762e-0615-4cff-8da7-26769d3ee1ed",
      "number": 29,
      "content": "Kẻ nóng tính tỏ thái độ liền là tự ý phô sự dại dột mình ra.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 29,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.488Z",
      "updatedAt": "2024-07-15T03:17:49.488Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "9e29f739-fac7-4aa2-abf7-26a272fdf02e",
      "number": 30,
      "content": "Lòng bình an ban sinh lực cho thân xác;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 30,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.494Z",
      "updatedAt": "2024-07-15T03:17:49.494Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "d87ef37e-c69c-4b61-8f68-72b0e75e0ecf",
      "number": 30,
      "content": "Tính ganh tị làm mục nát trong xương.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 31,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.494Z",
      "updatedAt": "2024-07-15T03:17:49.494Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "8e28e3e5-d3fa-48fc-9f88-e9a3a29fe458",
      "number": 31,
      "content": "Ai áp bức người nghèo là làm nhục Ðấng Tạo Hóa mình;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 32,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.499Z",
      "updatedAt": "2024-07-15T03:17:49.499Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "eb751a8a-e922-4e04-89e7-a4554c690f30",
      "number": 31,
      "content": "Ai thương xót người khốn khổ là tôn kính Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 33,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.499Z",
      "updatedAt": "2024-07-15T03:17:49.499Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "d434353f-dc54-4425-a6d1-9a984a200b39",
      "number": 32,
      "content": "Kẻ ác bị việc ác của nó đánh đổ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 34,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.505Z",
      "updatedAt": "2024-07-15T03:17:49.505Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "2a480664-190e-4fde-a9b8-72fa6b312cf1",
      "number": 32,
      "content": "Nhưng người ngay lành khi qua đời vẫn có chỗ nương thân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 35,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.505Z",
      "updatedAt": "2024-07-15T03:17:49.505Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "16569566-c8aa-4baf-9e46-826992626df2",
      "number": 33,
      "content": "Sự khôn ngoan nghỉ ngơi trong lòng người thông sáng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 36,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.510Z",
      "updatedAt": "2024-07-15T03:17:49.510Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "e444f725-ced2-4ff6-ba22-4fa438c47c03",
      "number": 33,
      "content": "Còn kẻ dại dột bộc lộ hết lòng dạ nó ra ngoài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 37,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.510Z",
      "updatedAt": "2024-07-15T03:17:49.510Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "8f2c0624-9709-4cb5-8c6e-073981cfde7e",
      "number": 34,
      "content": "Lẽ công chính làm quốc gia được kính trọng;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.516Z",
      "updatedAt": "2024-07-15T03:17:49.516Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "bc8de080-e5a0-4e30-99eb-33473f55bef2",
      "number": 34,
      "content": "Còn tội lỗi làm xấu hổ toàn dân.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.516Z",
      "updatedAt": "2024-07-15T03:17:49.516Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "52b06865-4b05-4cf6-a78c-01e24ad1f6de",
      "number": 35,
      "content": "Ân điển vua dành cho bầy tôi tài giỏi,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.522Z",
      "updatedAt": "2024-07-15T03:17:49.522Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    },
    {
      "id": "9e031033-1362-4e39-b8ba-243d518cc210",
      "number": 35,
      "content": "Thịnh nộ vua dọi xuống đầu kẻ gây nhục nhã cho người.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:17:49.522Z",
      "updatedAt": "2024-07-15T03:17:49.522Z",
      "chapterId": "e931ca43-1fac-4dc4-850e-359ba9403106"
    }
  ],
  "footnotes": [],
  "headings": [],
  "references": []
}