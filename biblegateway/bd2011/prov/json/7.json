    {
      "id": "67f6e56e-51ae-48de-abcc-59788337b509",
      "content": "Sự Nguy Hại của Tội Ngoại Tình",
      "order": 0,
      "createdAt": "2024-07-15T03:16:25.292Z",
      "updatedAt": "2024-07-15T03:16:25.292Z",
      "verseId": "e4c638cf-a2e5-4994-a7da-e28b65cd6b91",
      "chapterId": "944cde24-ebf8-41e1-aca2-06c0fec83d17"
    }
  ],
