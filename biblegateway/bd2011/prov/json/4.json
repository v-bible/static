{
  "chapter": {
    "id": "7d3aa086-496c-4f33-aca1-664a0da38dda",
    "number": 4,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%204&version=BD2011",
    "createdAt": "2024-07-12T04:03:48.890Z",
    "updatedAt": "2024-07-24T15:07:49.713Z",
    "bookId": "f88be0f6-2d78-40e0-8b41-6684c15a16e0"
  },
  "verses": [
    {
      "id": "375ff402-51e8-4cc2-969e-f1c66adaced9",
      "number": 1,
      "content": "Các con ơi, hãy lắng nghe lời cha dạy bảo;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.242Z",
      "updatedAt": "2024-07-15T03:15:50.325Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "f372ae7d-4c10-4da3-bb03-ad54286fba26",
      "number": 1,
      "content": "Hãy chú tâm lãnh hội đến tinh tường.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.242Z",
      "updatedAt": "2024-07-15T03:15:50.333Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "80f87ee5-9916-4caf-aa50-6098c580cde6",
      "number": 2,
      "content": "Vì cha cho các con những lời khuyên dạy tốt;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.252Z",
      "updatedAt": "2024-07-15T03:15:50.341Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "77c5f928-3799-4f9d-bbf7-58d0c7781af8",
      "number": 2,
      "content": "Mong các con đừng bỏ lời giáo huấn của cha.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.252Z",
      "updatedAt": "2024-07-15T03:15:50.349Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "b9b87601-ee6d-492e-86e0-114ec5f091f1",
      "number": 3,
      "content": "Khi cha còn là một đứa con thơ dại của ông nội các con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.254Z",
      "updatedAt": "2024-07-15T03:15:50.357Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "c9f77f54-da58-4072-89a8-25d43e0912fc",
      "number": 3,
      "content": "Là con một yêu dấu của bà nội các con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.254Z",
      "updatedAt": "2024-07-15T03:15:50.366Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "f0dda43f-b452-478d-8dec-778b772f4696",
      "number": 4,
      "content": "Ông nội các con đã dạy cha rằng:",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.257Z",
      "updatedAt": "2024-07-15T03:15:50.374Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "4cc0ef0a-903c-4afc-915f-61e3bd793774",
      "number": 4,
      "content": "Lòng con khá ghi tạc lời cha dạy bảo;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.257Z",
      "updatedAt": "2024-07-15T03:15:50.382Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "56c823b4-7323-4eee-9017-c4c2bccbf849",
      "number": 4,
      "content": "Giữ mệnh lệnh cha để con sống oai hùng:",
      "order": 2,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.257Z",
      "updatedAt": "2024-07-15T03:15:50.391Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "76f42b4b-d2f8-4525-ada2-d4a251c4bc36",
      "number": 5,
      "content": "Hãy kiếm khôn ngoan, khá tìm thông sáng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.259Z",
      "updatedAt": "2024-07-15T03:15:50.399Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "ac993da9-bf37-444a-aafc-c69c9655fe02",
      "number": 5,
      "content": "Ðừng quên và chớ lìa bỏ lời cha.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.259Z",
      "updatedAt": "2024-07-15T03:15:50.422Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "3a96c337-2671-4735-83df-bc4bb0ff2777",
      "number": 6,
      "content": "Ðừng lìa bỏ khôn ngoan, thì nàng sẽ bảo vệ con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.262Z",
      "updatedAt": "2024-07-15T03:15:50.447Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "51dd982b-fc11-4e7e-8375-23c2cbeeb9e9",
      "number": 6,
      "content": "Yêu mến nàng, thì nàng sẽ gìn giữ con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.262Z",
      "updatedAt": "2024-07-15T03:15:50.472Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "8f5665a7-303c-4030-95e4-c5bfae86ab54",
      "number": 7,
      "content": "Khôn ngoan là nhu cầu tối thượng, nên con phải đạt sự khôn ngoan;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.264Z",
      "updatedAt": "2024-07-15T03:15:50.496Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "4789e643-e4ad-4e9b-8bb2-eabc6248f01b",
      "number": 7,
      "content": "Trong mọi thứ con đạt được ở đời, hãy cố đạt sự thông sáng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.264Z",
      "updatedAt": "2024-07-15T03:15:50.521Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "3a108959-fe66-4d8b-afc3-295e7d425768",
      "number": 8,
      "content": "Hãy coi trọng nàng, nàng sẽ làm con được tôn trọng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.269Z",
      "updatedAt": "2024-07-15T03:15:50.546Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "b36631a6-6899-49f9-ae6a-868a572d090b",
      "number": 8,
      "content": "Ôm giữ nàng, nàng sẽ mang vinh dự đến cho con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.269Z",
      "updatedAt": "2024-07-15T03:15:50.572Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "99f4ae55-f0a2-4d1a-9754-08fe931bf2b2",
      "number": 9,
      "content": "Nàng sẽ đặt trên đầu con một vòng hoa tuyệt mỹ;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.275Z",
      "updatedAt": "2024-07-15T03:15:50.586Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "5b488292-2ddb-4c71-aeb5-dd37d722d07c",
      "number": 9,
      "content": "Nàng sẽ dâng tặng con mão miện đắc thắng huy hoàng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.275Z",
      "updatedAt": "2024-07-15T03:15:50.597Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "7f73fa3b-5166-458f-8451-4c3a6c523e5b",
      "number": 10,
      "content": "Con ơi, hãy lắng nghe và tiếp nhận lời cha,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.277Z",
      "updatedAt": "2024-07-15T03:15:50.612Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "e7e61896-6ece-416c-859d-9aff31505e2c",
      "number": 10,
      "content": "Thì những năm của đời con sẽ thêm nhiều lên.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.277Z",
      "updatedAt": "2024-07-15T03:15:50.639Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "4e85a072-360c-4550-bce2-39f3f8a97540",
      "number": 11,
      "content": "Cha đã dạy dỗ con theo đường lối khôn ngoan;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.279Z",
      "updatedAt": "2024-07-15T03:15:50.665Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "f076b76a-1193-4101-ad6c-39e6296600c4",
      "number": 11,
      "content": "Cha đã hướng dẫn con theo những nẻo đường ngay thẳng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.279Z",
      "updatedAt": "2024-07-15T03:15:50.837Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "09ba73b7-19db-4dd4-8853-57ba03186544",
      "number": 12,
      "content": "Khi con đi, các bước con sẽ không bị cản trở;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.281Z",
      "updatedAt": "2024-07-15T03:15:50.848Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "2215ca8f-5dbb-498f-94c9-24323d6f3979",
      "number": 12,
      "content": "Nếu con chạy, con sẽ không bị vấp ngã bao giờ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.281Z",
      "updatedAt": "2024-07-15T03:15:50.858Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "8bd97022-eab4-47a7-a186-1047bd6693cf",
      "number": 13,
      "content": "Hãy giữ chặt lời giáo huấn, đừng buông nó ra.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.283Z",
      "updatedAt": "2024-07-15T03:15:50.866Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "c3d2f9fe-98fa-4d65-97a4-78a1c04baa49",
      "number": 13,
      "content": "Hãy canh giữ nàng, vì nàng là sự sống của con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.283Z",
      "updatedAt": "2024-07-15T03:15:50.891Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "82656a29-b71a-4895-947c-4012760a5756",
      "number": 14,
      "content": "Con đừng đi vào nẻo đường của phường gian ác,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.285Z",
      "updatedAt": "2024-07-15T03:15:50.901Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "ac220766-1d36-4672-92a6-b612f8a47e79",
      "number": 14,
      "content": "Bước chân con chớ đặt vào lối của bọn gian tà.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.285Z",
      "updatedAt": "2024-07-15T03:15:50.910Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "dfddf4b3-e921-4200-9627-6864df51035d",
      "number": 15,
      "content": "Con hãy tránh xa nó và đừng đi vào đường đó;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.287Z",
      "updatedAt": "2024-07-15T03:15:50.918Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "4d9bebc1-4591-4348-9a00-0ed25582e013",
      "number": 15,
      "content": "Con hãy quay khỏi nó và tiếp tục đường mình.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.287Z",
      "updatedAt": "2024-07-15T03:15:50.929Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "3152afe1-a1cd-46b8-bbc0-057a87fd84c9",
      "number": 16,
      "content": "Vì kẻ ác sẽ không ngủ được nếu không làm gì ác đức;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.290Z",
      "updatedAt": "2024-07-15T03:15:50.939Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "f9a9d1cd-aa45-42cd-b5e6-c3773068c5e6",
      "number": 16,
      "content": "Chúng sẽ bị mất ngủ nếu chưa gây vấp ngã cho ai.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.290Z",
      "updatedAt": "2024-07-15T03:15:50.950Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "7e7568ad-4fe9-4084-8619-38bad7d9d009",
      "number": 17,
      "content": "Vì chúng cho làm chuyện gian tà thiết yếu như ăn cơm ăn bánh,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.292Z",
      "updatedAt": "2024-07-15T03:15:50.959Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "85b8de75-b95b-4209-a00c-ac96a8bbf0d1",
      "number": 17,
      "content": "Và cho việc bạo tàn là cần có như uống rượu khi ăn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.292Z",
      "updatedAt": "2024-07-15T03:15:50.995Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "5b279ea3-b941-49a3-ae54-a24267ad46ff",
      "number": 18,
      "content": "Ðường của người ngay lành giống như ánh bình minh rực rỡ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.294Z",
      "updatedAt": "2024-07-15T03:15:51.010Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "15109261-254b-47c3-9043-1c5210f7217c",
      "number": 18,
      "content": "Càng lúc càng rạng ngời cho đến khi chiếu sáng khắp bầu trời;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.294Z",
      "updatedAt": "2024-07-15T03:15:51.020Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "23939657-10c5-44ed-80f2-12689d1782e3",
      "number": 19,
      "content": "Còn đường kẻ gian ác giống như đêm khuya tăm tối;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.296Z",
      "updatedAt": "2024-07-15T03:15:51.029Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "040e71e4-92e1-4d3a-8307-6fd42baf7efa",
      "number": 19,
      "content": "Chúng ngã nhào mà không biết ngã vì sao.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:15:50.296Z",
      "updatedAt": "2024-07-15T03:15:51.039Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "d2a9dc27-99ed-48cc-add9-421736f1869f",
      "number": 20,
      "content": "Con ơi, hãy chú tâm để ý đến lời cha;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.298Z",
      "updatedAt": "2024-07-15T03:15:50.298Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "b0ebf4dd-ac23-4106-9a5e-5d861a01f4be",
      "number": 20,
      "content": "Hãy nghiêng tai nghe lời cha khuyên bảo.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.298Z",
      "updatedAt": "2024-07-15T03:15:50.298Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "b24359f6-153f-40f6-bd66-a7b25ee49fe5",
      "number": 21,
      "content": "Ðừng để chúng vuột khỏi tầm mắt con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.300Z",
      "updatedAt": "2024-07-15T03:15:50.300Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "37ff1b68-7a16-4851-8185-2205be43f68c",
      "number": 21,
      "content": "Hãy giữ chúng trong lòng con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.300Z",
      "updatedAt": "2024-07-15T03:15:50.300Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "d2b2e650-a04e-4234-acd0-ec7584906500",
      "number": 22,
      "content": "Vì chúng là sự sống cho người nào tìm được chúng;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.303Z",
      "updatedAt": "2024-07-15T03:15:50.303Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "809369db-422d-4406-a1e6-169dc5bfc70f",
      "number": 22,
      "content": "Chúng là sức khỏe cho thân thể ai có chúng trong lòng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.303Z",
      "updatedAt": "2024-07-15T03:15:50.303Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "e4883071-651d-4dc3-94ab-82713ca1bdef",
      "number": 23,
      "content": "Hãy cẩn thận giữ tấm lòng của con hơn hết,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.304Z",
      "updatedAt": "2024-07-15T03:15:50.304Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "ae064a04-4999-4d1c-8e21-47976e2c5e0e",
      "number": 23,
      "content": "Vì các nguồn sự sống đều từ đó mà ra.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.304Z",
      "updatedAt": "2024-07-15T03:15:50.304Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "6466d2b0-8ec2-475c-8f7b-fd6122d06725",
      "number": 24,
      "content": "Hãy bỏ khỏi miệng con những lời điêu ngoa gian dối,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.306Z",
      "updatedAt": "2024-07-15T03:15:50.306Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "a05b975c-2a50-4975-9baf-0592abf4a1fb",
      "number": 24,
      "content": "Hãy quăng khỏi môi con những lời dối trá lọc lừa.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.306Z",
      "updatedAt": "2024-07-15T03:15:50.306Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "6ce9ccb0-9292-478d-af81-ee000f188a56",
      "number": 25,
      "content": "Mắt con hãy nhìn thẳng về phía trước,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.308Z",
      "updatedAt": "2024-07-15T03:15:50.308Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "9c39eabd-fc97-4cd6-8fd5-cdd87c9bd1ce",
      "number": 25,
      "content": "Ánh mắt con khá hướng thẳng về phía trước mặt con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.308Z",
      "updatedAt": "2024-07-15T03:15:50.308Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "7818fc78-94b7-40c8-a438-9a894f97bc98",
      "number": 26,
      "content": "Hãy giữ chân con trong đường ngay nẻo chánh,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.310Z",
      "updatedAt": "2024-07-15T03:15:50.310Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "9b622535-fc41-4f4e-983c-85e6d4ba9290",
      "number": 26,
      "content": "Ðể mọi đường lối con được thiết lập vững vàng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.310Z",
      "updatedAt": "2024-07-15T03:15:50.310Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "44769145-e812-4b83-b233-6f278624c409",
      "number": 27,
      "content": "Ðừng quay qua bên phải hoặc quay qua bên trái;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.313Z",
      "updatedAt": "2024-07-15T03:15:50.313Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    },
    {
      "id": "9b705363-a82a-43a9-92cb-2edf18f3546c",
      "number": 27,
      "content": "Hãy quay bước chân con ra khỏi chỗ gian tà.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:15:50.313Z",
      "updatedAt": "2024-07-15T03:15:50.313Z",
      "chapterId": "7d3aa086-496c-4f33-aca1-664a0da38dda"
    }
  ],
