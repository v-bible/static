{
  "chapter": {
    "id": "150b3744-b79d-4d1b-9337-e8cfe396288b",
    "number": 46,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2046&version=BD2011",
    "createdAt": "2024-09-09T04:13:04.087Z",
    "updatedAt": "2024-09-09T04:13:04.087Z",
    "bookId": "5349a9d9-642f-4c7c-a482-7d88b8006432"
  },
  "verses": [
    {
      "id": "cb7657d2-6a3d-4c4e-ac5b-1dea150f72b9",
      "number": 1,
      "content": "Thần Bên cúi xuống, Thần Nê-bô rạp mình;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.466Z",
      "updatedAt": "2024-09-09T04:58:28.466Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "c3712a3e-7288-4eab-a9da-6f6caeb76941",
      "number": 1,
      "content": "Các thần tượng của chúng được chở đi bằng các xe do thú vật và trâu bò kéo;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.477Z",
      "updatedAt": "2024-09-09T04:58:28.477Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "c47c175a-0c44-4fc5-9d20-94f37220478c",
      "number": 1,
      "content": "Các thần tượng các ngươi mang theo đã trở thành gánh nặng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.479Z",
      "updatedAt": "2024-09-09T04:58:28.479Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "82a10395-3ca3-4fc2-85d4-94fe2bb121c8",
      "number": 1,
      "content": "Một trọng tải nặng nề khiến các thú vật kéo xe mệt nhọc.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.481Z",
      "updatedAt": "2024-09-09T04:58:28.481Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "8f22ea0c-d4c9-49f1-b890-605d4b71c5b2",
      "number": 2,
      "content": "Các thần tượng ấy cúi rạp xuống;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.483Z",
      "updatedAt": "2024-09-09T04:58:28.483Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "f8be105e-dcaa-4dad-8e11-0c5c78a1ca0e",
      "number": 2,
      "content": "Chúng thảy đều gục xuống;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.485Z",
      "updatedAt": "2024-09-09T04:58:28.485Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "75e581cc-cf24-481b-9af6-19259a5055c1",
      "number": 2,
      "content": "Chúng chẳng cứu nổi các súc vật đang kéo nặng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.487Z",
      "updatedAt": "2024-09-09T04:58:28.487Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "0895fb36-6265-4c55-8bce-ec67290e1d6b",
      "number": 2,
      "content": "Vì chính bản thân chúng cũng bị bắt đem đi lưu đày.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.489Z",
      "updatedAt": "2024-09-09T04:58:28.489Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "c0205cd9-0f0d-410c-991a-d297598ab0f1",
      "number": 3,
      "content": "Hỡi nhà Gia-cốp,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.491Z",
      "updatedAt": "2024-09-09T04:58:28.491Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "250db2d3-8636-46ef-acd4-428815922e1f",
      "number": 3,
      "content": "Hỡi những kẻ còn sót lại của nhà I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.493Z",
      "updatedAt": "2024-09-09T04:58:28.493Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "ae5b5427-713f-4b4f-81d8-b636c810f813",
      "number": 3,
      "content": "Hãy lắng nghe Ta:",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.496Z",
      "updatedAt": "2024-09-09T04:58:28.496Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "180bc491-1794-4aa8-838f-711bcc2f6520",
      "number": 3,
      "content": "Ta đã bồng ẵm các ngươi từ lúc các ngươi mới chào đời;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.500Z",
      "updatedAt": "2024-09-09T04:58:28.500Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "a6fedebf-44cb-47b4-9a55-e77bb05ba143",
      "number": 3,
      "content": "Ta đã cưu mang các ngươi từ khi các ngươi còn trong lòng mẹ,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.505Z",
      "updatedAt": "2024-09-09T04:58:28.505Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "ebb7f9b4-5182-4c6d-94a2-13744f7786df",
      "number": 4,
      "content": "Cho đến khi các ngươi già nua cao tuổi, Ta vẫn là Ðấng ấy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.510Z",
      "updatedAt": "2024-09-09T04:58:28.510Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "f9defccb-c9e8-41e3-b833-1a69cafb8131",
      "number": 4,
      "content": "Dầu cho các ngươi đã ra da mồi tóc bạc, Ta sẽ còn bồng ẵm các ngươi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.514Z",
      "updatedAt": "2024-09-09T04:58:28.514Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "53815288-186a-41bf-8650-2da780eef372",
      "number": 4,
      "content": "Ta đã tạo ra, nên Ta sẽ bồng ẵm;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.519Z",
      "updatedAt": "2024-09-09T04:58:28.519Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "12eee1dc-7674-4e89-818e-aef7312fa96e",
      "number": 4,
      "content": "Ta sẽ cưu mang và sẽ giải cứu các ngươi.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.523Z",
      "updatedAt": "2024-09-09T04:58:28.523Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "a0abf87a-78da-446c-9984-c6565a8741d7",
      "number": 5,
      "content": "Các ngươi sẽ ví sánh ai với Ta và cho ai được ngang hàng với Ta?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.528Z",
      "updatedAt": "2024-09-09T04:58:28.528Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "c90e17e7-8601-4517-8dd5-786b1d3ebb15",
      "number": 5,
      "content": "Các ngươi so sánh ai với Ta như thể kẻ đó và Ta có điều gì tương xứng?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.533Z",
      "updatedAt": "2024-09-09T04:58:28.533Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "444d2009-99ce-4317-ae85-60414493f12b",
      "number": 6,
      "content": "Chúng lấy vàng trong túi ra;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.538Z",
      "updatedAt": "2024-09-09T04:58:28.538Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "77e165b4-a100-4e4f-81ea-6db45c3594a0",
      "number": 6,
      "content": "Chúng lấy bạc đem cân trên cân;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.543Z",
      "updatedAt": "2024-09-09T04:58:28.543Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "974b4daa-aa86-4c44-bed1-1f73f4dc086a",
      "number": 6,
      "content": "Chúng mướn một người thợ bạc;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.548Z",
      "updatedAt": "2024-09-09T04:58:28.548Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "fd1082e5-0c81-4805-b406-2232921e5436",
      "number": 6,
      "content": "Chúng nhờ hắn lấy vàng bạc đó làm nên một tượng thần,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.554Z",
      "updatedAt": "2024-09-09T04:58:28.554Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "3f11f1eb-65eb-4834-9f41-3800a5208238",
      "number": 6,
      "content": "Rồi chúng sụp xuống thờ lạy tượng thần đó.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.558Z",
      "updatedAt": "2024-09-09T04:58:28.558Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "48ef12c1-27cc-4c6f-b168-d8c6240cd8ab",
      "number": 7,
      "content": "Chúng nhấc nó lên vai và cùng nhau khiêng nó đi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.564Z",
      "updatedAt": "2024-09-09T04:58:28.564Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "48e38fdf-160d-4cfb-9770-c23f77f47d01",
      "number": 7,
      "content": "Chúng đặt nó vào một chỗ, và nó cứ đứng yên tại đó;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.568Z",
      "updatedAt": "2024-09-09T04:58:28.568Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "8423626e-393d-4455-891f-74b434f5c70a",
      "number": 7,
      "content": "Nó không thể di chuyển khỏi chỗ đứng của nó.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.573Z",
      "updatedAt": "2024-09-09T04:58:28.573Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "d8672b1b-973f-4471-bee7-1206f8b41cd2",
      "number": 7,
      "content": "Nếu ai kêu cầu nó, nó chẳng thể trả lời;",
      "order": 3,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.579Z",
      "updatedAt": "2024-09-09T04:58:28.579Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "24a324b8-772c-4c26-b693-7909148e4ea6",
      "number": 7,
      "content": "Nó không thể cứu ai khỏi cơn hoạn nạn.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.584Z",
      "updatedAt": "2024-09-09T04:58:28.584Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "026d09b7-d5e6-443f-ab04-2f730eb294c5",
      "number": 8,
      "content": "Hãy ghi nhớ điều ấy mà sống xứng đáng làm người,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.588Z",
      "updatedAt": "2024-09-09T04:58:28.588Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "b71eb872-a15c-418d-ba02-940823a6cd0f",
      "number": 8,
      "content": "Hỡi các ngươi, những kẻ hay phạm tội, hãy ghi nhớ trong lòng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.594Z",
      "updatedAt": "2024-09-09T04:58:28.594Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "57bf41c4-5ac5-4184-bedb-0f665c736d2e",
      "number": 9,
      "content": "Hãy nhớ lại những chuyện xưa kia,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.599Z",
      "updatedAt": "2024-09-09T04:58:28.599Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "df3ef78f-cad5-4669-83ba-ec17feff5118",
      "number": 9,
      "content": "Vì Ta là Ðức Chúa Trời, không có thần nào khác;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.604Z",
      "updatedAt": "2024-09-09T04:58:28.604Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "74240772-01a6-41de-972d-6087f3b2c52e",
      "number": 9,
      "content": "Ta là Ðức Chúa Trời, không có thần nào khác như Ta.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.609Z",
      "updatedAt": "2024-09-09T04:58:28.609Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "1f36ebf9-dba1-4769-8678-f3b23d1454e9",
      "number": 10,
      "content": "Từ ban đầu Ta đã cho biết cuối cùng sẽ ra thể nào;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.614Z",
      "updatedAt": "2024-09-09T04:58:28.614Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "102f978d-912b-47e4-8a72-bd87fdd421f2",
      "number": 10,
      "content": "Từ ngàn xưa Ta đã báo trước việc sẽ xảy đến trong tương lai;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.619Z",
      "updatedAt": "2024-09-09T04:58:28.619Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "68886263-0380-481f-9838-3fe89c8d7c23",
      "number": 10,
      "content": "Ta phán: Những gì Ta dự định Ta sẽ làm cho thành tựu;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.624Z",
      "updatedAt": "2024-09-09T04:58:28.624Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "a1da04aa-9976-4b2c-85a9-73cd97da663b",
      "number": 10,
      "content": "Mọi điều lòng Ta vui thích Ta sẽ thực hiện cho xong.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.629Z",
      "updatedAt": "2024-09-09T04:58:28.629Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "acf915af-2cb2-41e3-ab3b-73be7b647b1a",
      "number": 11,
      "content": "Ta sẽ gọi một chim săn mồi từ phương đông bay đến,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.634Z",
      "updatedAt": "2024-09-09T04:58:28.634Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "96517b15-2b6a-4b67-bd4b-724a43368e1e",
      "number": 11,
      "content": "Một người thi hành ý chỉ Ta sẽ đến từ một xứ xa xăm;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.638Z",
      "updatedAt": "2024-09-09T04:58:28.638Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "34d113d6-a3b5-46aa-9cfe-277c7b56f546",
      "number": 11,
      "content": "Thật vậy, điều Ta nói, Ta sẽ làm;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.643Z",
      "updatedAt": "2024-09-09T04:58:28.643Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "b9396152-7cb9-42ac-9450-8a89c73dd5f3",
      "number": 11,
      "content": "Ðiều Ta định, Ta sẽ thực hiện.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.647Z",
      "updatedAt": "2024-09-09T04:58:28.647Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "6ce43560-7a83-44a8-80e4-7f858debf840",
      "number": 12,
      "content": "Hãy lắng nghe Ta, hỡi những kẻ có lòng bướng bỉnh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.652Z",
      "updatedAt": "2024-09-09T04:58:28.652Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "ff5e0b30-6378-40e5-9995-7d8370d408fb",
      "number": 12,
      "content": "Những kẻ đang ở xa lẽ công chính:",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.657Z",
      "updatedAt": "2024-09-09T04:58:28.657Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "0d4bb01d-55af-404b-ad1b-7054c3c57dec",
      "number": 13,
      "content": "Ngày Ta thực hiện lẽ công chính của Ta đã gần;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.662Z",
      "updatedAt": "2024-09-09T04:58:28.662Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "2591a49a-6aea-4809-a5f0-c98e25d06b52",
      "number": 13,
      "content": "Chẳng còn bao lâu nữa đâu;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.667Z",
      "updatedAt": "2024-09-09T04:58:28.667Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "62aa0d55-5acf-455c-b789-5793319ae45d",
      "number": 13,
      "content": "Ơn cứu rỗi của Ta sẽ không đình trệ;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.671Z",
      "updatedAt": "2024-09-09T04:58:28.671Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "92bdf49c-7092-45e4-999a-2feafd1701e4",
      "number": 13,
      "content": "Ta sẽ ban ơn cứu rỗi cho Si-ôn,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.676Z",
      "updatedAt": "2024-09-09T04:58:28.676Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    },
    {
      "id": "bb21b219-f0bb-4ef9-9894-9f5e42734930",
      "number": 13,
      "content": "Vì cớ I-sơ-ra-ên, vinh hiển Ta.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:28.680Z",
      "updatedAt": "2024-09-09T04:58:28.680Z",
      "chapterId": "150b3744-b79d-4d1b-9337-e8cfe396288b"
    }
  ],
