{
  "chapter": {
    "id": "8be2b860-75c5-4611-b053-d650063d7d91",
    "number": 46,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2046&version=BD2011",
    "createdAt": "2024-07-12T04:03:50.744Z",
    "updatedAt": "2024-07-24T15:07:51.403Z",
    "bookId": "e78c99ae-11b2-4687-bcf7-b33bef999af4"
  },
  "verses": [
    {
      "id": "c46e7a9e-47c0-4301-9eb1-bd230e452c55",
      "number": 1,
      "content": "Thần Bên cúi xuống, Thần Nê-bô rạp mình;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:17.545Z",
      "updatedAt": "2024-07-15T03:33:17.589Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "de7c40ca-01b4-4940-82d1-839e5e43d2d9",
      "number": 1,
      "content": "Các thần tượng của chúng được chở đi bằng các xe do thú vật và trâu bò kéo;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:17.545Z",
      "updatedAt": "2024-07-15T03:33:17.599Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "afd10458-48bf-40a5-938e-5747d112a8ec",
      "number": 1,
      "content": "Các thần tượng các ngươi mang theo đã trở thành gánh nặng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:17.545Z",
      "updatedAt": "2024-07-15T03:33:17.608Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "29afaf56-09c4-48fd-b3a4-c3ca715acfa0",
      "number": 1,
      "content": "Một trọng tải nặng nề khiến các thú vật kéo xe mệt nhọc.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:17.545Z",
      "updatedAt": "2024-07-15T03:33:17.617Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "764eebc4-5c17-4621-a0dc-650ad4af353a",
      "number": 2,
      "content": "Các thần tượng ấy cúi rạp xuống;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:17.548Z",
      "updatedAt": "2024-07-15T03:33:17.626Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "89331503-2661-47aa-88db-98fd4772829a",
      "number": 2,
      "content": "Chúng thảy đều gục xuống;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:17.548Z",
      "updatedAt": "2024-07-15T03:33:17.634Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "f50c7911-8574-4f5d-a46d-db282d355f59",
      "number": 2,
      "content": "Chúng chẳng cứu nổi các súc vật đang kéo nặng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:17.548Z",
      "updatedAt": "2024-07-15T03:33:17.642Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "98b58122-1131-4f0a-a7a2-950e6570711a",
      "number": 2,
      "content": "Vì chính bản thân chúng cũng bị bắt đem đi lưu đày.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:17.548Z",
      "updatedAt": "2024-07-15T03:33:17.651Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "b8ee60c9-e169-43e8-a6f9-b79e06870acf",
      "number": 3,
      "content": "Hỡi nhà Gia-cốp,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.551Z",
      "updatedAt": "2024-07-15T03:33:17.551Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "76cf2b86-f4be-4844-8a23-14444f0ffca5",
      "number": 3,
      "content": "Hỡi những kẻ còn sót lại của nhà I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.551Z",
      "updatedAt": "2024-07-15T03:33:17.551Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "5deeb54e-e581-4226-a6b9-3ec1ab28e2ef",
      "number": 3,
      "content": "Hãy lắng nghe Ta:",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.551Z",
      "updatedAt": "2024-07-15T03:33:17.551Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "2da9de2d-7ce5-4695-96f7-d63788506e00",
      "number": 3,
      "content": "Ta đã bồng ẵm các ngươi từ lúc các ngươi mới chào đời;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.551Z",
      "updatedAt": "2024-07-15T03:33:17.551Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "342c5e99-6da1-4c3c-9bf3-def5d822270a",
      "number": 3,
      "content": "Ta đã cưu mang các ngươi từ khi các ngươi còn trong lòng mẹ,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.551Z",
      "updatedAt": "2024-07-15T03:33:17.551Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "b1a58c2d-d765-4e37-9a57-e2f5bd9926a5",
      "number": 4,
      "content": "Cho đến khi các ngươi già nua cao tuổi, Ta vẫn là Ðấng ấy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.553Z",
      "updatedAt": "2024-07-15T03:33:17.553Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "3973cbde-4c43-448d-96b6-2b0ad307aa27",
      "number": 4,
      "content": "Dầu cho các ngươi đã ra da mồi tóc bạc, Ta sẽ còn bồng ẵm các ngươi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.553Z",
      "updatedAt": "2024-07-15T03:33:17.553Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "8ac9033a-db4e-4a7a-92b6-cd32144f0b41",
      "number": 4,
      "content": "Ta đã tạo ra, nên Ta sẽ bồng ẵm;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.553Z",
      "updatedAt": "2024-07-15T03:33:17.553Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "aabf5661-c64a-43e0-9d48-7267449090e2",
      "number": 4,
      "content": "Ta sẽ cưu mang và sẽ giải cứu các ngươi.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.553Z",
      "updatedAt": "2024-07-15T03:33:17.553Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "f8699aff-24e8-493d-9d42-e8e646ba00fb",
      "number": 5,
      "content": "Các ngươi sẽ ví sánh ai với Ta và cho ai được ngang hàng với Ta?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.556Z",
      "updatedAt": "2024-07-15T03:33:17.556Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "ee3684a5-fe72-46a5-b2e2-f9a063fbfacf",
      "number": 5,
      "content": "Các ngươi so sánh ai với Ta như thể kẻ đó và Ta có điều gì tương xứng?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.556Z",
      "updatedAt": "2024-07-15T03:33:17.556Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "715ff1dc-4abb-43c2-85aa-6aa8c9fb1a99",
      "number": 6,
      "content": "Chúng lấy vàng trong túi ra;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.558Z",
      "updatedAt": "2024-07-15T03:33:17.558Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "54648926-3ba9-477d-92d3-c86752bb0b21",
      "number": 6,
      "content": "Chúng lấy bạc đem cân trên cân;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.558Z",
      "updatedAt": "2024-07-15T03:33:17.558Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "b17483fd-3d61-441e-8396-fcdfee4a6057",
      "number": 6,
      "content": "Chúng mướn một người thợ bạc;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.558Z",
      "updatedAt": "2024-07-15T03:33:17.558Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "4329c6de-f92b-4f70-abd9-c1565d262ddb",
      "number": 6,
      "content": "Chúng nhờ hắn lấy vàng bạc đó làm nên một tượng thần,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.558Z",
      "updatedAt": "2024-07-15T03:33:17.558Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "dea66f2c-cdf0-4530-85ad-89f9114c4e23",
      "number": 6,
      "content": "Rồi chúng sụp xuống thờ lạy tượng thần đó.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.558Z",
      "updatedAt": "2024-07-15T03:33:17.558Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "80357b7a-87dc-4506-9034-5b6c1f059d32",
      "number": 7,
      "content": "Chúng nhấc nó lên vai và cùng nhau khiêng nó đi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.560Z",
      "updatedAt": "2024-07-15T03:33:17.560Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "6c945f22-7c21-4d33-b36d-ae1ef9b79168",
      "number": 7,
      "content": "Chúng đặt nó vào một chỗ, và nó cứ đứng yên tại đó;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.560Z",
      "updatedAt": "2024-07-15T03:33:17.560Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "06b7f2a4-5bc9-44cb-90a8-0dddb92cf620",
      "number": 7,
      "content": "Nó không thể di chuyển khỏi chỗ đứng của nó.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.560Z",
      "updatedAt": "2024-07-15T03:33:17.560Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "8f342d38-f1f8-4c9f-85df-ce0e280f5e02",
      "number": 7,
      "content": "Nếu ai kêu cầu nó, nó chẳng thể trả lời;",
      "order": 3,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.560Z",
      "updatedAt": "2024-07-15T03:33:17.560Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "436e6512-9de4-4cc7-a681-de689c6a6773",
      "number": 7,
      "content": "Nó không thể cứu ai khỏi cơn hoạn nạn.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.560Z",
      "updatedAt": "2024-07-15T03:33:17.560Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "244e9320-33be-4694-a8a6-ff1bf32c0aa5",
      "number": 8,
      "content": "Hãy ghi nhớ điều ấy mà sống xứng đáng làm người,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.562Z",
      "updatedAt": "2024-07-15T03:33:17.562Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "2427671d-9552-4725-a1bc-f26363be779a",
      "number": 8,
      "content": "Hỡi các ngươi, những kẻ hay phạm tội, hãy ghi nhớ trong lòng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.562Z",
      "updatedAt": "2024-07-15T03:33:17.562Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "056f9f02-fa9f-44ef-8bee-9b9915ce8a3d",
      "number": 9,
      "content": "Hãy nhớ lại những chuyện xưa kia,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.565Z",
      "updatedAt": "2024-07-15T03:33:17.565Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "8ce61d5f-5d42-42c1-a4b1-0b4ed955e3d5",
      "number": 9,
      "content": "Vì Ta là Ðức Chúa Trời, không có thần nào khác;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.565Z",
      "updatedAt": "2024-07-15T03:33:17.565Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "69d7eaa9-de16-4c4b-bb1f-e83f0e141b83",
      "number": 9,
      "content": "Ta là Ðức Chúa Trời, không có thần nào khác như Ta.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.565Z",
      "updatedAt": "2024-07-15T03:33:17.565Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "f145274b-69d7-477f-90bb-052fee09d905",
      "number": 10,
      "content": "Từ ban đầu Ta đã cho biết cuối cùng sẽ ra thể nào;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.567Z",
      "updatedAt": "2024-07-15T03:33:17.567Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "3aa8fe22-69cd-412b-860f-a5f63c0c375a",
      "number": 10,
      "content": "Từ ngàn xưa Ta đã báo trước việc sẽ xảy đến trong tương lai;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.567Z",
      "updatedAt": "2024-07-15T03:33:17.567Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "4ca5a796-4b4a-46dc-b84a-e8c8190f3c9a",
      "number": 10,
      "content": "Ta phán: Những gì Ta dự định Ta sẽ làm cho thành tựu;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.567Z",
      "updatedAt": "2024-07-15T03:33:17.567Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "207977be-2f97-480f-b4f9-b570e4d975f9",
      "number": 10,
      "content": "Mọi điều lòng Ta vui thích Ta sẽ thực hiện cho xong.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.567Z",
      "updatedAt": "2024-07-15T03:33:17.567Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "6921ff05-6e5c-44b8-938b-8534e51565ec",
      "number": 11,
      "content": "Ta sẽ gọi một chim săn mồi từ phương đông bay đến,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.570Z",
      "updatedAt": "2024-07-15T03:33:17.570Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "422d4665-d80a-4261-b323-dbd5680dc420",
      "number": 11,
      "content": "Một người thi hành ý chỉ Ta sẽ đến từ một xứ xa xăm;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.570Z",
      "updatedAt": "2024-07-15T03:33:17.570Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "a19abf3c-d5bf-4b4f-8c5e-2105a0c1c0db",
      "number": 11,
      "content": "Thật vậy, điều Ta nói, Ta sẽ làm;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.570Z",
      "updatedAt": "2024-07-15T03:33:17.570Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "968ee818-aa11-4f5d-8274-79921a3ea45c",
      "number": 11,
      "content": "Ðiều Ta định, Ta sẽ thực hiện.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.570Z",
      "updatedAt": "2024-07-15T03:33:17.570Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "eb6587b9-d0fc-439b-bef7-990dea1a45d6",
      "number": 12,
      "content": "Hãy lắng nghe Ta, hỡi những kẻ có lòng bướng bỉnh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.572Z",
      "updatedAt": "2024-07-15T03:33:17.572Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "26534b20-7f5d-4991-bf36-76c5718f622c",
      "number": 12,
      "content": "Những kẻ đang ở xa lẽ công chính:",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.572Z",
      "updatedAt": "2024-07-15T03:33:17.572Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "61eb871a-896a-4a7a-a49d-0bcb6ef3c42c",
      "number": 13,
      "content": "Ngày Ta thực hiện lẽ công chính của Ta đã gần;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.574Z",
      "updatedAt": "2024-07-15T03:33:17.574Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "94a16bd2-1e85-4a6e-9869-1a1d11223659",
      "number": 13,
      "content": "Chẳng còn bao lâu nữa đâu;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.574Z",
      "updatedAt": "2024-07-15T03:33:17.574Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "469e60e2-d0a6-45a1-a6fb-19e4e0573118",
      "number": 13,
      "content": "Ơn cứu rỗi của Ta sẽ không đình trệ;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.574Z",
      "updatedAt": "2024-07-15T03:33:17.574Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "c90d8a64-2f26-4d6b-a6b3-812337d8d4d3",
      "number": 13,
      "content": "Ta sẽ ban ơn cứu rỗi cho Si-ôn,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.574Z",
      "updatedAt": "2024-07-15T03:33:17.574Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    },
    {
      "id": "7111106d-992f-46d0-a233-ca470b0fa89e",
      "number": 13,
      "content": "Vì cớ I-sơ-ra-ên, vinh hiển Ta.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:17.574Z",
      "updatedAt": "2024-07-15T03:33:17.574Z",
      "chapterId": "8be2b860-75c5-4611-b053-d650063d7d91"
    }
  ],
