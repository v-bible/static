      "chapterId": "a1e21e00-3f92-484b-91af-2273d037c723"
    },
    {
      "id": "673c589d-bdb5-453b-8ebb-b05dd925ee26",
      "content": "Sự Ðau Khổ và Vinh Hiển của Ðầy Tớ CHÚA",
      "order": 0,
      "createdAt": "2024-07-15T03:34:24.201Z",
      "updatedAt": "2024-07-15T03:34:24.201Z",
