    },
    {
      "id": "ee3cb62c-07bf-435d-b6cf-35c2634b3e96",
      "content": "Sự Ðau Khổ và Vinh Hiển của Ðầy Tớ CHÚA",
      "order": 0,
      "createdAt": "2024-09-24T02:24:48.928Z",
      "updatedAt": "2024-09-24T02:24:48.928Z",
      "verseId": "6deea5b5-af54-4692-aba3-8ac6828b5723",
