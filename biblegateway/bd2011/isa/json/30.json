      "chapterId": "f50add01-d69c-4531-b5e8-66de6c9fb30c"
    },
    {
      "id": "57a81f88-3277-42d9-b8e8-dfe859455bf8",
      "content": "ctd: lò thiêu người của Tô-phết",
      "position": 15,
      "order": 1,
      "createdAt": "2024-07-15T03:30:05.190Z",
      "updatedAt": "2024-07-15T03:30:05.190Z",
      "verseId": "8cf6e169-a2c0-4a7d-b01f-ea40d7177be6",
