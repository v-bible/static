    }
  ],
  "headings": [
    {
      "id": "62c4e753-b41c-44d9-816b-d257d0c2960e",
      "content": "Quở Trách vì Cậy Vào Ai-cập",
      "order": 0,
      "createdAt": "2024-09-09T04:57:37.224Z",
      "updatedAt": "2024-09-09T04:57:37.224Z",
      "verseId": "96d48e9b-3081-4116-a7b4-b2da48e9feee",
      "chapterId": "633908de-c28f-4da5-8b16-f8c31ce0564e"
    },
    {
      "id": "9e494d95-ecd1-4e63-a301-3c89852ae9c8",
      "content": "Một Dân Hay Phản Bội",
      "order": 0,
      "createdAt": "2024-09-09T04:57:37.290Z",
