      "chapterId": "633908de-c28f-4da5-8b16-f8c31ce0564e"
    },
    {
      "id": "243adcdc-9644-4835-b983-a0a64286154a",
      "content": "ctd: lò thiêu người của Tô-phết",
      "position": 15,
      "order": 1,
      "createdAt": "2024-09-09T04:57:37.741Z",
      "updatedAt": "2024-09-09T04:57:37.741Z",
      "verseId": "8c798f35-0e88-4a91-b39e-20d9ac458c59",
