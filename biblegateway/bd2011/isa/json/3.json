    },
    {
      "id": "b91b9b7c-6a35-450c-b2c9-335b364bcaa6",
      "content": "Trong Bản Qumran có chữ \"xấu hổ\"",
      "position": 30,
      "order": 4,
      "createdAt": "2024-09-09T04:56:12.193Z",
      "updatedAt": "2024-09-09T04:56:12.193Z",
