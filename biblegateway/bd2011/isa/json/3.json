    },
    {
      "id": "6a65b70b-2731-4fa7-98fd-4b244b332f0f",
      "content": "nt: y phục làm bằng vải gai mịn",
      "position": 27,
      "order": 3,
      "createdAt": "2024-07-15T03:25:14.940Z",
      "updatedAt": "2024-07-15T03:25:14.940Z",
