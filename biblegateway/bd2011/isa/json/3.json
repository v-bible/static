    },
    {
      "id": "13a72959-559f-4c68-b948-6809ddf92aa9",
      "content": "Trong Bản Qumran có chữ \"xấu hổ\"",
      "position": 30,
      "order": 4,
      "createdAt": "2024-07-15T03:25:14.957Z",
      "updatedAt": "2024-07-15T03:25:14.957Z",
