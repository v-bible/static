    {
      "id": "81b5f5ff-1d21-453f-b88c-dbefb92ddce1",
      "content": "Sự Kiêng Ăn Ðẹp Lòng CHÚA",
      "order": 0,
      "createdAt": "2024-09-09T04:59:05.539Z",
      "updatedAt": "2024-09-09T04:59:05.539Z",
      "verseId": "4ad4a52b-23ce-4e9a-8241-cb06b3de5eaf",
      "chapterId": "e61aa274-2588-4031-93d4-02019c3c4970"
    }
  ],
