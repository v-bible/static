{
  "chapter": {
    "id": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d",
    "number": 25,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2025&version=BD2011",
    "createdAt": "2024-07-12T04:03:50.329Z",
    "updatedAt": "2024-07-24T15:07:51.037Z",
    "bookId": "e78c99ae-11b2-4687-bcf7-b33bef999af4"
  },
  "verses": [
    {
      "id": "b63f53df-d57e-4c6e-b0dd-cd73447e87c7",
      "number": 1,
      "content": "Lạy Chúa, Ngài là Ðức Chúa Trời của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.549Z",
      "updatedAt": "2024-07-15T03:29:03.732Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "b3e192ab-9286-4797-92ed-e68e5a9d1e4e",
      "number": 1,
      "content": "Con tôn ngợi Ngài, con chúc tụng danh Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.549Z",
      "updatedAt": "2024-07-15T03:29:03.742Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "19b3d88b-fbe4-4e2d-a418-1944f742c48b",
      "number": 1,
      "content": "Vì Ngài đã làm những việc lạ lùng.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.549Z",
      "updatedAt": "2024-07-15T03:29:03.752Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "a47d8ee6-bddb-4497-a164-1cc88d202c2f",
      "number": 1,
      "content": "Ngài thực hiện các chương trình đã định sẵn từ xưa cách thành tín và thực tế.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.549Z",
      "updatedAt": "2024-07-15T03:29:03.784Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "a2dafbbf-ea59-489d-a894-b1537a539fa0",
      "number": 2,
      "content": "Vì Ngài đã biến một thành phố thành một đống vụn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.559Z",
      "updatedAt": "2024-07-15T03:29:03.794Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "e647a62c-64a7-4530-b683-c9026fc2a293",
      "number": 2,
      "content": "Một thành trì kiên cố ra một nơi đổ nát điêu tàn;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.559Z",
      "updatedAt": "2024-07-15T03:29:03.806Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "55c21666-4565-4664-bc31-143a089f17c6",
      "number": 2,
      "content": "Khu dinh thự của các dân ngoại không còn là một khu phố nữa;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.559Z",
      "updatedAt": "2024-07-15T03:29:03.820Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "9e9dd1f1-9c71-47bd-851a-cf3900d67aba",
      "number": 2,
      "content": "Nó sẽ không bao giờ được xây dựng lại.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.559Z",
      "updatedAt": "2024-07-15T03:29:03.835Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "1ea6538e-9b23-4a52-aeff-7dee22344638",
      "number": 3,
      "content": "Vì thế các dân tộc hùng cường sẽ tôn vinh Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.562Z",
      "updatedAt": "2024-07-15T03:29:03.844Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "308cfb48-3653-446b-8b22-444ec2344e7c",
      "number": 3,
      "content": "Những thành trì của các dân hung hãn sẽ kính sợ Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.562Z",
      "updatedAt": "2024-07-15T03:29:03.855Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "2f3661bc-58e2-4295-87f1-a151d561be10",
      "number": 4,
      "content": "Vì Ngài là nơi ẩn núp của người nghèo khó,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.564Z",
      "updatedAt": "2024-07-15T03:29:03.864Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "46ea48b2-a43f-404c-8cd8-06240c6891d6",
      "number": 4,
      "content": "Nơi nương náu cho người khốn cùng trong lúc gian nguy,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.564Z",
      "updatedAt": "2024-07-15T03:29:03.873Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "709deeaa-0ad7-4c1f-b82c-0317255ed2a2",
      "number": 4,
      "content": "Nơi trú ẩn trong khi bão tố,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.564Z",
      "updatedAt": "2024-07-15T03:29:03.883Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "3903c6ae-42a6-445c-a515-9f07c7f7b209",
      "number": 4,
      "content": "Và bóng mát giữa lúc nắng thiêu;",
      "order": 3,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.564Z",
      "updatedAt": "2024-07-15T03:29:03.893Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "79c29f34-9ebc-4fb9-85ef-ce01edf7de99",
      "number": 4,
      "content": "Vì sát khí của quân cường bạo như bão tố đập tới tấp vào tường,",
      "order": 4,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.564Z",
      "updatedAt": "2024-07-15T03:29:03.902Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "172acce0-0552-47d6-b6f1-7991454f91b3",
      "number": 5,
      "content": "Tiếng náo động của các dân ngoại như nắng gắt giữa vùng khô hạn;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.567Z",
      "updatedAt": "2024-07-15T03:29:03.911Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "b87dfc18-a837-4d43-83a2-e591758f964b",
      "number": 5,
      "content": "Ngài chế ngự chúng như nắng nóng bị đám mây che mát;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.567Z",
      "updatedAt": "2024-07-15T03:29:03.921Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "b3ea4b7b-5321-4370-a66f-943227921c39",
      "number": 5,
      "content": "Tiếng ca hát của quân cường bạo đành im bặt.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.567Z",
      "updatedAt": "2024-07-15T03:29:03.932Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "220869bc-944b-4a7f-91d8-0071299da238",
      "number": 6,
      "content": "Trên núi này Chúa các đạo quân sẽ thết đãi muôn dân một đại tiệc,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.570Z",
      "updatedAt": "2024-07-15T03:29:03.570Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "fab5bf15-cad4-4b35-acc3-a6b4ab3b0be1",
      "number": 6,
      "content": "Một đại tiệc với thịt béo, một đại tiệc với rượu ngon;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.570Z",
      "updatedAt": "2024-07-15T03:29:03.570Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "cdec2d49-8f66-47f0-9fe5-8d6b1290d064",
      "number": 6,
      "content": "Thịt thật béo bổ và rượu tinh khiết thượng hạng.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.570Z",
      "updatedAt": "2024-07-15T03:29:03.570Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "5d3608d0-5d07-4ebf-b860-67757bf8ecf5",
      "number": 7,
      "content": "Cũng tại trên núi này Ngài sẽ hủy bỏ tấm màn bao trùm mọi dân;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.572Z",
      "updatedAt": "2024-07-15T03:29:03.572Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "38bbd2ca-4265-41da-9193-21cc28c9fef2",
      "number": 7,
      "content": "Một tấm màn bao phủ tất cả các nước.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.572Z",
      "updatedAt": "2024-07-15T03:29:03.572Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "efda46d0-ec86-42cb-a3b1-690415c74b39",
      "number": 8,
      "content": "Ngài sẽ tiêu diệt tử thần vĩnh viễn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.574Z",
      "updatedAt": "2024-07-15T03:29:03.574Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "0c5b15e6-82fa-42bf-96df-1cc563273bb2",
      "number": 8,
      "content": "Chúa Hằng Hữu sẽ lau khô dòng lệ trên mặt mọi người;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.574Z",
      "updatedAt": "2024-07-15T03:29:03.574Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "6451e4fe-a823-4807-b750-704bfb166697",
      "number": 8,
      "content": "Ngài sẽ cất đi nỗi ô nhục của dân Ngài trên toàn thế giới,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.574Z",
      "updatedAt": "2024-07-15T03:29:03.574Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "ed95b2c0-c5bd-4e8a-aefe-b74bbcb504d1",
      "number": 8,
      "content": "Vì Chúa đã phán vậy.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.574Z",
      "updatedAt": "2024-07-15T03:29:03.574Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "882c3d51-ba63-4348-91cb-5365872ea9f0",
      "number": 9,
      "content": "Trong ngày ấy người ta sẽ nói:",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.577Z",
      "updatedAt": "2024-07-15T03:29:03.577Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "f8fe99ae-77d0-4a1d-916c-1bdb66cbfbc7",
      "number": 9,
      "content": "Ðây là Ðức Chúa Trời chúng ta;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.577Z",
      "updatedAt": "2024-07-15T03:29:03.577Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "d8e0cbf3-80e1-41c5-84ea-506b37eb32a3",
      "number": 9,
      "content": "Chúng ta trông đợi Ngài, và Ngài giải cứu chúng ta.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.577Z",
      "updatedAt": "2024-07-15T03:29:03.577Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "63f30cc5-5196-4b14-be88-d7fb22ff6ab1",
      "number": 9,
      "content": "Ðây là Chúa chúng ta hằng chờ đợi;",
      "order": 3,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.577Z",
      "updatedAt": "2024-07-15T03:29:03.577Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "e77b368c-56e6-4c93-b4df-99c5990379b3",
      "number": 9,
      "content": "Chúng ta hãy hân hoan và mừng rỡ hưởng ơn cứu rỗi của Ngài,",
      "order": 4,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.577Z",
      "updatedAt": "2024-07-15T03:29:03.577Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "2e1bb557-df5a-412c-ae6d-0ccce8b19252",
      "number": 10,
      "content": "Vì Chúa sẽ đặt tay Ngài trên núi này.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:03.579Z",
      "updatedAt": "2024-07-15T03:29:03.579Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "e4b04c11-9dac-4525-9f6e-276bab31e252",
      "number": 10,
      "content": "Người Mô-áp sẽ bị chà đạp ngay tại xứ sở của họ,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.579Z",
      "updatedAt": "2024-07-15T03:29:03.946Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "ca6408b7-8b1e-4734-b6b9-eecef1a92003",
      "number": 10,
      "content": "Như rơm bị đạp dưới hố phân.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.579Z",
      "updatedAt": "2024-07-15T03:29:03.955Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "41c7fb43-ba99-422d-b009-714628765ac6",
      "number": 11,
      "content": "Dù giữa lúc ấy họ đưa tay ra tự cứu,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.582Z",
      "updatedAt": "2024-07-15T03:29:03.965Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "7ceb1af4-11de-4422-99af-74870b2b9831",
      "number": 11,
      "content": "Như người bơi vươn tay ra để sải bơi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.582Z",
      "updatedAt": "2024-07-15T03:29:03.974Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "27535316-3171-47e0-93e7-139d425fc2d3",
      "number": 11,
      "content": "Nhưng Ngài sẽ triệt hạ tính kiêu căng của họ,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.582Z",
      "updatedAt": "2024-07-15T03:29:03.985Z",
      "chapterId": "bbb6039f-a808-4a9b-b44c-6ff0fdfac54d"
    },
    {
      "id": "159e63d9-cd3f-4b7b-8fcc-ce5b2aabdd57",
      "number": 11,
      "content": "Dù cánh tay họ cố vùng vẫy vươn lên.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:03.582Z",
