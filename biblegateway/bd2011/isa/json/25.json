{
  "chapter": {
    "id": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1",
    "number": 25,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2025&version=BD2011",
    "createdAt": "2024-09-09T04:13:03.710Z",
    "updatedAt": "2024-09-09T04:13:03.710Z",
    "bookId": "5349a9d9-642f-4c7c-a482-7d88b8006432"
  },
  "verses": [
    {
      "id": "4857324b-51af-4c54-9bad-3421ea422bf3",
      "number": 1,
      "content": "Lạy Chúa, Ngài là Ðức Chúa Trời của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.059Z",
      "updatedAt": "2024-09-09T04:57:21.059Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "768a9467-0d0e-4b5c-8be5-46545c5ca1d0",
      "number": 1,
      "content": "Con tôn ngợi Ngài, con chúc tụng danh Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.070Z",
      "updatedAt": "2024-09-09T04:57:21.070Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "4a99e2da-862f-4bd0-a3a8-2b4f83b3b841",
      "number": 1,
      "content": "Vì Ngài đã làm những việc lạ lùng.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.072Z",
      "updatedAt": "2024-09-09T04:57:21.072Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "abb74eed-b0c4-42be-8c31-97754fa4f7ba",
      "number": 1,
      "content": "Ngài thực hiện các chương trình đã định sẵn từ xưa cách thành tín và thực tế.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.074Z",
      "updatedAt": "2024-09-09T04:57:21.074Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "1b37183e-db46-462e-b108-5d87ce4043f0",
      "number": 2,
      "content": "Vì Ngài đã biến một thành phố thành một đống vụn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.076Z",
      "updatedAt": "2024-09-09T04:57:21.076Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "3880e869-9c14-41bd-a5a4-8469499b1a3d",
      "number": 2,
      "content": "Một thành trì kiên cố ra một nơi đổ nát điêu tàn;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.078Z",
      "updatedAt": "2024-09-09T04:57:21.078Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "bb6a0cef-692c-457e-9fbf-91e50dc831e6",
      "number": 2,
      "content": "Khu dinh thự của các dân ngoại không còn là một khu phố nữa;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.081Z",
      "updatedAt": "2024-09-09T04:57:21.081Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "2e5b537b-fbe7-47fb-9952-d2882830f359",
      "number": 2,
      "content": "Nó sẽ không bao giờ được xây dựng lại.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.082Z",
      "updatedAt": "2024-09-09T04:57:21.082Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "ee00b06d-fc5e-4ea9-b177-9e7fd19c66ac",
      "number": 3,
      "content": "Vì thế các dân tộc hùng cường sẽ tôn vinh Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.086Z",
      "updatedAt": "2024-09-09T04:57:21.086Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "2d4d4794-639e-46f0-ae6b-c2d90d0852fe",
      "number": 3,
      "content": "Những thành trì của các dân hung hãn sẽ kính sợ Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.090Z",
      "updatedAt": "2024-09-09T04:57:21.090Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "f0d28524-9208-4ede-aec4-b284225d8f9d",
      "number": 4,
      "content": "Vì Ngài là nơi ẩn núp của người nghèo khó,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.095Z",
      "updatedAt": "2024-09-09T04:57:21.095Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "564f3014-0edd-4b54-bfde-33173b84d6f6",
      "number": 4,
      "content": "Nơi nương náu cho người khốn cùng trong lúc gian nguy,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.100Z",
      "updatedAt": "2024-09-09T04:57:21.100Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "b7fbbe05-0aab-4ad5-9d93-82ce74f62960",
      "number": 4,
      "content": "Nơi trú ẩn trong khi bão tố,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.104Z",
      "updatedAt": "2024-09-09T04:57:21.104Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "a918ba5a-5a48-43fd-ab0f-307d98e22582",
      "number": 4,
      "content": "Và bóng mát giữa lúc nắng thiêu;",
      "order": 3,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.110Z",
      "updatedAt": "2024-09-09T04:57:21.110Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "822f0aba-3846-4c10-b8df-00eaece62967",
      "number": 4,
      "content": "Vì sát khí của quân cường bạo như bão tố đập tới tấp vào tường,",
      "order": 4,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.114Z",
      "updatedAt": "2024-09-09T04:57:21.114Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "9d4d0f24-81a1-4213-a6da-a4ce435fd582",
      "number": 5,
      "content": "Tiếng náo động của các dân ngoại như nắng gắt giữa vùng khô hạn;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.119Z",
      "updatedAt": "2024-09-09T04:57:21.119Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "b1bd7a17-533a-4242-975d-f45c250d026c",
      "number": 5,
      "content": "Ngài chế ngự chúng như nắng nóng bị đám mây che mát;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.124Z",
      "updatedAt": "2024-09-09T04:57:21.124Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "8dcc25bb-0864-42d5-960f-ece9e82e6924",
      "number": 5,
      "content": "Tiếng ca hát của quân cường bạo đành im bặt.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.129Z",
      "updatedAt": "2024-09-09T04:57:21.129Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "50d01fad-3c7a-4fac-81d0-ae8d3b90d85d",
      "number": 6,
      "content": "Trên núi này Chúa các đạo quân sẽ thết đãi muôn dân một đại tiệc,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.134Z",
      "updatedAt": "2024-09-09T04:57:21.134Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "51a41436-7f8e-451c-a6dc-acc40f483e44",
      "number": 6,
      "content": "Một đại tiệc với thịt béo, một đại tiệc với rượu ngon;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.139Z",
      "updatedAt": "2024-09-09T04:57:21.139Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "6062a2f5-605d-4dc1-898d-ffb4d1871cc8",
      "number": 6,
      "content": "Thịt thật béo bổ và rượu tinh khiết thượng hạng.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.144Z",
      "updatedAt": "2024-09-09T04:57:21.144Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "4044b13b-3041-4a60-afcc-627a6ec10801",
      "number": 7,
      "content": "Cũng tại trên núi này Ngài sẽ hủy bỏ tấm màn bao trùm mọi dân;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.150Z",
      "updatedAt": "2024-09-09T04:57:21.150Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "bb57cf75-2be7-47f6-98c9-b1651bc70994",
      "number": 7,
      "content": "Một tấm màn bao phủ tất cả các nước.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.155Z",
      "updatedAt": "2024-09-09T04:57:21.155Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "1b685e98-c76f-472f-afd6-569dd79cfc38",
      "number": 8,
      "content": "Ngài sẽ tiêu diệt tử thần vĩnh viễn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.161Z",
      "updatedAt": "2024-09-09T04:57:21.161Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "19701227-2146-4879-aade-efaaab7a22f7",
      "number": 8,
      "content": "Chúa Hằng Hữu sẽ lau khô dòng lệ trên mặt mọi người;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.166Z",
      "updatedAt": "2024-09-09T04:57:21.166Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "7eac3544-d8d2-4abe-92e4-251b6a2dc50b",
      "number": 8,
      "content": "Ngài sẽ cất đi nỗi ô nhục của dân Ngài trên toàn thế giới,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.171Z",
      "updatedAt": "2024-09-09T04:57:21.171Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "e76f328f-1d2f-44f7-a377-e5d78408af2b",
      "number": 8,
      "content": "Vì Chúa đã phán vậy.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.176Z",
      "updatedAt": "2024-09-09T04:57:21.176Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "f1667747-8477-4f50-897b-2a54bfb0d82a",
      "number": 9,
      "content": "Trong ngày ấy người ta sẽ nói:",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.181Z",
      "updatedAt": "2024-09-09T04:57:21.181Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "c563fc7a-e183-4e1a-a8d4-de98b819bb5c",
      "number": 9,
      "content": "Ðây là Ðức Chúa Trời chúng ta;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.187Z",
      "updatedAt": "2024-09-09T04:57:21.187Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "f5a13cff-cdbc-421f-9ab3-ea878469a3c4",
      "number": 9,
      "content": "Chúng ta trông đợi Ngài, và Ngài giải cứu chúng ta.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.192Z",
      "updatedAt": "2024-09-09T04:57:21.192Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "f0845f4e-bc21-4cc0-af02-b8c1d7dcf765",
      "number": 9,
      "content": "Ðây là Chúa chúng ta hằng chờ đợi;",
      "order": 3,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.198Z",
      "updatedAt": "2024-09-09T04:57:21.198Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "e26fbda7-f4fd-4322-899b-7ee6360a7abd",
      "number": 9,
      "content": "Chúng ta hãy hân hoan và mừng rỡ hưởng ơn cứu rỗi của Ngài,",
      "order": 4,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.203Z",
      "updatedAt": "2024-09-09T04:57:21.203Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "4d1205f3-7452-416b-889d-0a86c513d645",
      "number": 10,
      "content": "Vì Chúa sẽ đặt tay Ngài trên núi này.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.208Z",
      "updatedAt": "2024-09-09T04:57:21.208Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "212eff76-3b56-4d30-b694-7292e85d6932",
      "number": 10,
      "content": "Người Mô-áp sẽ bị chà đạp ngay tại xứ sở của họ,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.213Z",
      "updatedAt": "2024-09-09T04:57:21.213Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "f9c6751d-d44a-46b7-b5c6-51b9dd34eac0",
      "number": 10,
      "content": "Như rơm bị đạp dưới hố phân.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.224Z",
      "updatedAt": "2024-09-09T04:57:21.224Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "22f494d2-d08b-44a4-b2cb-228848886ab3",
      "number": 11,
      "content": "Dù giữa lúc ấy họ đưa tay ra tự cứu,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.228Z",
      "updatedAt": "2024-09-09T04:57:21.228Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "7879bdb9-ff14-41da-a276-d5818ef9b25d",
      "number": 11,
      "content": "Như người bơi vươn tay ra để sải bơi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.234Z",
      "updatedAt": "2024-09-09T04:57:21.234Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "81eced48-c4ab-4d4b-8e7d-4f8ff675c403",
      "number": 11,
      "content": "Nhưng Ngài sẽ triệt hạ tính kiêu căng của họ,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.239Z",
      "updatedAt": "2024-09-09T04:57:21.239Z",
      "chapterId": "1aa1cf2d-4ea8-4fb4-be8f-b9d81223afe1"
    },
    {
      "id": "5df17b73-e118-4a89-a01a-4bfd75c2e91f",
      "number": 11,
      "content": "Dù cánh tay họ cố vùng vẫy vươn lên.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:57:21.244Z",
