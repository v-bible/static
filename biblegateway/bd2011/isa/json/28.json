{
  "chapter": {
    "id": "035ac4a1-f688-4616-935e-db04d7455be8",
    "number": 28,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2028&version=BD2011",
    "createdAt": "2024-07-12T04:03:50.384Z",
    "updatedAt": "2024-07-24T15:07:51.090Z",
    "bookId": "e78c99ae-11b2-4687-bcf7-b33bef999af4"
  },
  "verses": [
    {
      "id": "95822ac4-d99c-4ccb-ad37-60aebba116ee",
      "number": 1,
      "content": "Khốn cho Ép-ra-im,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.191Z",
      "updatedAt": "2024-07-15T03:29:37.283Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "4b551ecd-248f-4526-96de-e3a6c84c53c8",
      "number": 1,
      "content": "Hào quang của nó đang phai tàn,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.191Z",
      "updatedAt": "2024-07-15T03:29:37.293Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "3835752c-1db0-4bc0-8b75-e656e15ae795",
      "number": 1,
      "content": "Như những mão miện bằng hoa trên đầu bọn lãnh đạo say sưa của nó;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.191Z",
      "updatedAt": "2024-07-15T03:29:37.303Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "99facd2e-004c-4cef-8e80-efe0c0c376dc",
      "number": 1,
      "content": "Chúng trị vì trên thung lũng phì nhiêu,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.191Z",
      "updatedAt": "2024-07-15T03:29:37.313Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "52f77998-f6a8-4bef-a9c0-ed9e94623bc8",
      "number": 1,
      "content": "Nhưng lại ngập chìm trong say sưa men rượu.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.191Z",
      "updatedAt": "2024-07-15T03:29:37.322Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "fb418b25-b542-4fd4-9819-9850e851df0a",
      "number": 2,
      "content": "Kìa, Chúa có một người thật oai hùng và dũng mãnh,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.201Z",
      "updatedAt": "2024-07-15T03:29:37.334Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "aa4611fe-b3fa-4c4b-b37a-74b1628fce54",
      "number": 2,
      "content": "Như trận bão có mưa đá,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.201Z",
      "updatedAt": "2024-07-15T03:29:37.344Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "229b45d2-ae4a-46c7-b037-3f1f3ba3a458",
      "number": 2,
      "content": "Như cuồng phong khốc liệt,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.201Z",
      "updatedAt": "2024-07-15T03:29:37.356Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "93c4e270-ccd1-4e9b-b527-17bae6a096d0",
      "number": 2,
      "content": "Như bão tố kinh hồn,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.201Z",
      "updatedAt": "2024-07-15T03:29:37.365Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "469dfe9c-5762-48e7-aca7-454b8a64b4de",
      "number": 2,
      "content": "Như lũ lụt ngập tràn,",
      "order": 4,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.201Z",
      "updatedAt": "2024-07-15T03:29:37.375Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "6f526dc1-44f7-4b79-b0b3-6745d0c1851e",
      "number": 2,
      "content": "Ngài thẳng tay ném chúng xuống mặt đất.",
      "order": 5,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.201Z",
      "updatedAt": "2024-07-15T03:29:37.384Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "ad32a596-10f1-4521-956f-9c80e205b92d",
      "number": 3,
      "content": "Ngài sẽ giày đạp dưới chân mão miện kiêu ngạo của bọn say sưa của Ép-ra-im.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.204Z",
      "updatedAt": "2024-07-15T03:29:37.394Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "58c1299a-f788-471a-96ff-01d270d4955d",
      "number": 4,
      "content": "Sự hào nhoáng đẹp đẽ trên đầu của những kẻ ngự trị trên thung lũng phì nhiêu là thế đó;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.206Z",
      "updatedAt": "2024-07-15T03:29:37.404Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "ac1ef636-867e-412f-877b-074653bedc32",
      "number": 4,
      "content": "Nó sẽ như một vòng hoa đang héo tàn;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.206Z",
      "updatedAt": "2024-07-15T03:29:37.414Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "4f78a29a-5c8b-4f94-bc85-0d48cb42eca6",
      "number": 4,
      "content": "Nó sẽ như trái vả chín non trước mùa hạ,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.206Z",
      "updatedAt": "2024-07-15T03:29:37.423Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "8f6367a6-04cf-4a84-ad60-c92a19c6d357",
      "number": 4,
      "content": "Ai thấy và hái được sẽ ăn ngấu nghiến trái ấy ngay.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:29:37.206Z",
      "updatedAt": "2024-07-15T03:29:37.455Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "8ddb0740-bbf5-4ddc-9008-b64cc9fffd93",
      "number": 5,
      "content": "Trong ngày ấy Chúa các đạo quân sẽ như một vòng hoa vinh hiển,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.208Z",
      "updatedAt": "2024-07-15T03:29:37.208Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "eed30568-310c-4479-961d-eb2edb1bb9f8",
      "number": 5,
      "content": "Một vương miện tuyệt mỹ cho những người còn sót lại của dân Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.208Z",
      "updatedAt": "2024-07-15T03:29:37.208Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "e8e889ed-63e7-4d70-bbf9-72a3d950eda4",
      "number": 6,
      "content": "Ngài sẽ là Thần Công Lý cho những người ngồi trên tòa xét xử,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.210Z",
      "updatedAt": "2024-07-15T03:29:37.210Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "8f0c3069-7949-4ab4-a12a-5fae5a3a4715",
      "number": 6,
      "content": "Là sức mạnh cho những ai đẩy lui quân thù trong chiến trận tại cổng thành.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.210Z",
      "updatedAt": "2024-07-15T03:29:37.210Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "27c69cbc-19f2-47d9-8470-94db73d2d63c",
      "number": 7,
      "content": "Ðầu óc bọn ấy đều quay cuồng trong men rượu;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.213Z",
      "updatedAt": "2024-07-15T03:29:37.213Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "8f0eb1e3-8bfe-495c-8f66-07b0d81f7387",
      "number": 7,
      "content": "Chúng ngả nghiêng vì rượu mạnh;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.213Z",
      "updatedAt": "2024-07-15T03:29:37.213Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "96204f20-9411-40c3-a022-fd08b31e6f12",
      "number": 7,
      "content": "Tư tế và tiên tri đều chếnh choáng vì rượu mạnh;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.213Z",
      "updatedAt": "2024-07-15T03:29:37.213Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "572a038f-9c59-4a9b-87c7-689de64b68c5",
      "number": 7,
      "content": "Chúng say mèm vì rượu nho;",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.213Z",
      "updatedAt": "2024-07-15T03:29:37.213Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "c1f9e966-55e9-42c8-90fb-7dfa8f86adee",
      "number": 7,
      "content": "Chúng lảo đảo vì rượu mạnh;",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.213Z",
      "updatedAt": "2024-07-15T03:29:37.213Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "157b9854-fa7e-48ff-b0ed-f2c835fcad50",
      "number": 7,
      "content": "Ðầu óc chúng bị rượu làm quay cuồng, nên chẳng nhận rõ được khải tượng;",
      "order": 5,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.213Z",
      "updatedAt": "2024-07-15T03:29:37.213Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "cc5d50de-f7ef-4f12-b12e-dadef7fc67cd",
      "number": 7,
      "content": "Khi xét xử chúng xét đoán lạng quạng, chẳng có thể phân biệt được đúng sai.",
      "order": 6,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.213Z",
      "updatedAt": "2024-07-15T03:29:37.213Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "7b2f8dd0-4e18-417d-9689-186aa95585fa",
      "number": 8,
      "content": "Vì tất cả các bàn tiệc đều bị chúng nôn mửa, trở nên ô uế;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.215Z",
      "updatedAt": "2024-07-15T03:29:37.215Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "6710bc07-fa09-43a4-93c7-400ad596d61f",
      "number": 8,
      "content": "Chẳng còn chỗ nào sạch sẽ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.215Z",
      "updatedAt": "2024-07-15T03:29:37.215Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "3496e690-d9ec-4178-9ee2-058805e7d3b8",
      "number": 9,
      "content": "Ngài sẽ dạy cho ai tri thức?",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.218Z",
      "updatedAt": "2024-07-15T03:29:37.218Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "4247e8a0-0e38-42f5-a197-24650c263962",
      "number": 9,
      "content": "Ngài sẽ giãi bày sứ điệp cho ai?",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.218Z",
      "updatedAt": "2024-07-15T03:29:37.218Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "f1a319e2-9991-4eba-85e3-7384a19900a3",
      "number": 9,
      "content": "Chẳng lẽ cho các trẻ thơ vừa dứt sữa,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.218Z",
      "updatedAt": "2024-07-15T03:29:37.218Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "381307e0-85a3-43dd-83c4-b0086d2449a9",
      "number": 9,
      "content": "Tức cho những con thơ vừa thôi bú hay sao?",
      "order": 3,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.218Z",
      "updatedAt": "2024-07-15T03:29:37.218Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "da109c2c-3baa-400b-8806-6ee11ede432e",
      "number": 10,
      "content": "Thôi thì cứ giảng giải,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.220Z",
      "updatedAt": "2024-07-15T03:29:37.220Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "47319ced-52d0-44bd-9b63-11fbfdd79746",
      "number": 10,
      "content": "Từng điều một,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.220Z",
      "updatedAt": "2024-07-15T03:29:37.220Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "61d83ac9-6a81-4225-8b65-4e52e318e093",
      "number": 10,
      "content": "Xong điều này, qua điều nọ,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.220Z",
      "updatedAt": "2024-07-15T03:29:37.220Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "dd0e17ad-64ff-462c-9436-b2775ea82f16",
      "number": 10,
      "content": "Từng khoản một,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.220Z",
      "updatedAt": "2024-07-15T03:29:37.220Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "351b5599-e2d3-455a-99af-a31a9c5945ed",
      "number": 10,
      "content": "Hết khoản nọ, đến khoản kia;",
      "order": 4,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.220Z",
      "updatedAt": "2024-07-15T03:29:37.220Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "55a41ef4-853a-4dda-9121-82d7bea68a20",
      "number": 10,
      "content": "Một ít chỗ này, một ít chỗ khác.",
      "order": 5,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.220Z",
      "updatedAt": "2024-07-15T03:29:37.220Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "f56726ad-ceee-46a9-88d1-82b980a7aad2",
      "number": 11,
      "content": "Thật vậy Ngài sẽ dùng môi miệng ngọng nghịu và ngôn ngữ xa lạ nói với dân này,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.223Z",
      "updatedAt": "2024-07-15T03:29:37.223Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "872c5e24-ef3f-4e25-b059-92a1c6bfc722",
      "number": 12,
      "content": "Là dân Ngài đã phán,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.225Z",
      "updatedAt": "2024-07-15T03:29:37.225Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "4b26842e-2792-46c0-9a33-c9f3115442dd",
      "number": 12,
      "content": "“Ðây là nơi nghỉ ngơi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.225Z",
      "updatedAt": "2024-07-15T03:29:37.225Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "ca21bd8c-1c86-4d70-bc0d-d4a53b18fd48",
      "number": 12,
      "content": "Hãy để cho những kẻ mệt mỏi được nghỉ ngơi;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.225Z",
      "updatedAt": "2024-07-15T03:29:37.225Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "353a746f-0465-4c82-bb8c-c24555a6efbf",
      "number": 12,
      "content": "Ðây là nơi thư thái.”",
      "order": 3,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.225Z",
      "updatedAt": "2024-07-15T03:29:37.225Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "cf9c93c4-446f-4966-a28b-cc8231787382",
      "number": 12,
      "content": "Nhưng chúng chẳng thèm nghe.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.225Z",
      "updatedAt": "2024-07-15T03:29:37.225Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "19432e9d-0611-43f9-bbd9-371def6983db",
      "number": 13,
      "content": "Nên lời Chúa cho chúng sẽ là: “Từng điều một,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.228Z",
      "updatedAt": "2024-07-15T03:29:37.228Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "d80f5ab7-604d-4a88-aea2-94ad7ac4c85d",
      "number": 13,
      "content": "Xong điều này, qua điều nọ,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.228Z",
      "updatedAt": "2024-07-15T03:29:37.228Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "85684735-9d04-415e-8647-6c4d0c17c2b9",
      "number": 13,
      "content": "Từng khoản một,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.228Z",
      "updatedAt": "2024-07-15T03:29:37.228Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "f628be51-52ee-46e7-8b85-938c29053dd1",
      "number": 13,
      "content": "Hết khoản nọ, đến khoản kia;",
      "order": 3,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.228Z",
      "updatedAt": "2024-07-15T03:29:37.228Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "09fad069-4db9-4de5-a904-944828ce34e9",
      "number": 13,
      "content": "Một ít chỗ này, một ít chỗ khác.”",
      "order": 4,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.228Z",
      "updatedAt": "2024-07-15T03:29:37.228Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "2643ad54-7d04-4294-bd67-27105b627ff2",
      "number": 13,
      "content": "Ðể chúng đi tới, rồi bị ngã lui,",
      "order": 5,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.228Z",
      "updatedAt": "2024-07-15T03:29:37.228Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "7cd4b5e8-30f5-4454-9b21-4f65baa76906",
      "number": 13,
      "content": "Bị giập nát, bị sa bẫy, và bị bắt sống.",
      "order": 6,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.228Z",
      "updatedAt": "2024-07-15T03:29:37.228Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "596afd60-8896-4f2a-baac-84e8f8df7a6e",
      "number": 14,
      "content": "Vậy, hãy lắng nghe lời Chúa, hỡi những kẻ nhạo báng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.231Z",
      "updatedAt": "2024-07-15T03:29:37.231Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "57db600e-41c6-40e8-8092-d9ef9b1222d7",
      "number": 14,
      "content": "Những kẻ lãnh đạo dân này tại Giê-ru-sa-lem.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.231Z",
      "updatedAt": "2024-07-15T03:29:37.231Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "bae3b40c-dc61-4983-9c9b-170e196d67b7",
      "number": 15,
      "content": "Vì các ngươi bảo rằng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.233Z",
      "updatedAt": "2024-07-15T03:29:37.233Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "c305c4c2-905a-45d9-9167-96cb4d0776c6",
      "number": 15,
      "content": "“Chúng tôi đã kết ước với tử thần;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.233Z",
      "updatedAt": "2024-07-15T03:29:37.233Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "b5bf1843-335d-4d57-b006-d84c553724e8",
      "number": 15,
      "content": "Chúng tôi đã có hợp đồng với âm phủ,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.233Z",
      "updatedAt": "2024-07-15T03:29:37.233Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "f2c0e982-e164-44c3-ace9-d75680776fa6",
      "number": 15,
      "content": "Nên khi có tai ương tàn khốc tràn qua,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.233Z",
      "updatedAt": "2024-07-15T03:29:37.233Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "d857aa8d-894d-4ed1-bd9d-4b6f1e40c2b7",
      "number": 15,
      "content": "Chúng tôi sẽ không hề hấn gì,",
      "order": 4,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.233Z",
      "updatedAt": "2024-07-15T03:29:37.233Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "bcf78cf8-e3c5-434a-b2c7-0905d0f6525d",
      "number": 15,
      "content": "Vì chúng tôi sẽ dùng dối trá làm nơi trú ẩn,",
      "order": 5,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.233Z",
      "updatedAt": "2024-07-15T03:29:37.233Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "68f70f57-e82c-417c-a3cb-a50e214f9c34",
      "number": 15,
      "content": "Dùng dối gian làm chỗ trú ngụ cho mình.”",
      "order": 6,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.233Z",
      "updatedAt": "2024-07-15T03:29:37.233Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "bd2062d8-8371-4173-9b95-5f144d99ddbb",
      "number": 16,
      "content": "Vì thế Chúa Hằng Hữu phán,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.236Z",
      "updatedAt": "2024-07-15T03:29:37.236Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "16022f33-5078-45f4-90f6-bb6d457c01a5",
      "number": 16,
      "content": "Này, Ta đặt tại Si-ôn một tảng đá móng,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.236Z",
      "updatedAt": "2024-07-15T03:29:37.236Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "809de7e2-f699-486b-a721-1ff9aa4f2ce2",
      "number": 16,
      "content": "Một tảng đá đã được thử nghiệm,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.236Z",
      "updatedAt": "2024-07-15T03:29:37.236Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "14e671a8-71fa-4ce8-b114-a3be87b42910",
      "number": 16,
      "content": "Một tảng đá góc nhà quý báu,",
      "order": 3,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.236Z",
      "updatedAt": "2024-07-15T03:29:37.236Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "fa0e9dd7-c228-4089-a51a-e767d3efc038",
      "number": 16,
      "content": "Một nền móng vững chắc;",
      "order": 4,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.236Z",
      "updatedAt": "2024-07-15T03:29:37.236Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "3b11cb9c-568a-4579-adbb-138d02cb6634",
      "number": 16,
      "content": "Ai tin cậy sẽ không hoảng hốt.",
      "order": 5,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.236Z",
      "updatedAt": "2024-07-15T03:29:37.236Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "08432e65-8163-41b5-8df4-9a5fecd8fe8e",
      "number": 17,
      "content": "Ta sẽ lấy công lý làm thước đo,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.238Z",
      "updatedAt": "2024-07-15T03:29:37.238Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "c46a81f9-e8d5-42e3-be54-4aff75b40bb5",
      "number": 17,
      "content": "Lấy công chính làm dây chuẩn mực;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.238Z",
      "updatedAt": "2024-07-15T03:29:37.238Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "657dc5c8-5183-4b1f-aa3a-cd4c4507e9a1",
      "number": 17,
      "content": "Mưa đá sẽ quét sạch nơi trú ẩn của dối trá;",
      "order": 2,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.238Z",
      "updatedAt": "2024-07-15T03:29:37.238Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "02f6b959-490c-476b-8338-3e4d28927abb",
      "number": 17,
      "content": "Lũ lụt sẽ ngập tràn chỗ trú ngụ của dối gian.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.238Z",
      "updatedAt": "2024-07-15T03:29:37.238Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "00afc7dd-f056-4373-908e-07605231b7b3",
      "number": 18,
      "content": "Bấy giờ giao ước của các ngươi với tử thần sẽ bị vô hiệu hóa;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.241Z",
      "updatedAt": "2024-07-15T03:29:37.241Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "a0f6b76f-46f4-4dbe-a038-365d597105ee",
      "number": 18,
      "content": "Hợp đồng giữa các ngươi và âm phủ sẽ chẳng còn giá trị gì;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.241Z",
      "updatedAt": "2024-07-15T03:29:37.241Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "151a7fa5-3516-45c1-a39f-3f07a2b450da",
      "number": 18,
      "content": "Ðến khi tai ương tàn khốc tràn qua,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.241Z",
      "updatedAt": "2024-07-15T03:29:37.241Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "05ae2296-59b7-4f2f-b490-ddb0a1b684b5",
      "number": 18,
      "content": "Các ngươi sẽ bị nó giày đạp nát tan.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 13,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.241Z",
      "updatedAt": "2024-07-15T03:29:37.241Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "b312c79c-2eb6-4459-9bad-9eabf16369c1",
      "number": 19,
      "content": "Mỗi khi tai ương tàn khốc đi qua, nó sẽ bắt các ngươi,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 14,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.243Z",
      "updatedAt": "2024-07-15T03:29:37.243Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "99786325-b265-401a-8459-3515b0a99a4e",
      "number": 19,
      "content": "Hết sáng này qua sáng khác, rồi bất kể ngày và đêm,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 15,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.243Z",
      "updatedAt": "2024-07-15T03:29:37.243Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "05e9a230-28a8-4b7a-b287-c27e46f6198f",
      "number": 19,
      "content": "Nó chắc chắn sẽ đi ngang qua các ngươi;",
      "order": 2,
      "parNumber": 6,
      "parIndex": 16,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.243Z",
      "updatedAt": "2024-07-15T03:29:37.243Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "84dfdef4-c93c-41e0-b530-d6c8427ee896",
      "number": 19,
      "content": "Ai hiểu được sứ điệp này sẽ chỉ còn biết kinh hãi mà thôi.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 17,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.243Z",
      "updatedAt": "2024-07-15T03:29:37.243Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "fcf5fb9b-139d-4eb6-8681-4a2f3360bd1a",
      "number": 20,
      "content": "Vì giường quá ngắn, nên không thể duỗi thẳng chân ra được;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.246Z",
      "updatedAt": "2024-07-15T03:29:37.246Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "6e305bee-9099-4ec3-a7ba-b162597cbcc7",
      "number": 20,
      "content": "Còn mền quá nhỏ, nên không đủ để đắp mình,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.246Z",
      "updatedAt": "2024-07-15T03:29:37.246Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "d5851701-0efa-48b8-874e-2da1050a08fb",
      "number": 21,
      "content": "Vì Chúa sẽ trỗi dậy, như ở trên Núi Pê-ra-xim,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.248Z",
      "updatedAt": "2024-07-15T03:29:37.248Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "312f7d0a-7555-4e4d-a3d9-0bfafd55fd74",
      "number": 21,
      "content": "Ngài sẽ nổi giận, như ở Thung Lũng Ghi-bê-ôn,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.248Z",
      "updatedAt": "2024-07-15T03:29:37.248Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "ec8ee6e8-213f-4457-a3da-af0739a50ce6",
      "number": 21,
      "content": "Ðể thực hiện công việc của Ngài, một công việc lạ lùng,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.248Z",
      "updatedAt": "2024-07-15T03:29:37.248Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "6b0584ef-d732-4ef8-a7db-47a94b0d05b3",
      "number": 21,
      "content": "Ðể làm công tác của Ngài, một công tác kỳ lạ.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.248Z",
      "updatedAt": "2024-07-15T03:29:37.248Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "fc570c56-6ba8-4b1a-9893-2d00d23ce51b",
      "number": 22,
      "content": "Vậy bây giờ các ngươi đừng nhạo báng nữa,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.251Z",
      "updatedAt": "2024-07-15T03:29:37.251Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "02107e46-f256-4b86-bf2e-c38826ae4e92",
      "number": 22,
      "content": "Nếu không, dây trói các ngươi sẽ càng siết lại chặt hơn;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.251Z",
      "updatedAt": "2024-07-15T03:29:37.251Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "0b9cbec0-2fab-4af0-b6f8-2e73cb67bbbf",
      "number": 22,
      "content": "Vì tôi đã được nghe từ Chúa, tức từ Chúa các đạo quân,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.251Z",
      "updatedAt": "2024-07-15T03:29:37.251Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "2835d82b-195d-4f2d-9f79-9745a8f12584",
      "number": 22,
      "content": "Một lịnh truyền, bảo phải tiêu diệt toàn xứ.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.251Z",
      "updatedAt": "2024-07-15T03:29:37.251Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "7f3039f6-8c39-473a-8631-79316cf3d6d5",
      "number": 23,
      "content": "Hãy lắng nghe, hãy nghe tiếng tôi;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.253Z",
      "updatedAt": "2024-07-15T03:29:37.253Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "35055e4d-67b9-464d-9363-5abfed8f5481",
      "number": 23,
      "content": "Xin chú ý nghe, xin nghe tôi nói.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.253Z",
      "updatedAt": "2024-07-15T03:29:37.253Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "ef08b383-3e97-42de-b63b-9284bab4ae5b",
      "number": 24,
      "content": "Chẳng phải người cày ruộng cày suốt ngày để đất sẵn sàng cho việc gieo trồng sao?",
      "order": 0,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.256Z",
      "updatedAt": "2024-07-15T03:29:37.256Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "40ec247d-97c8-48a1-b8e2-8166f754e13a",
      "number": 24,
      "content": "Chẳng phải người vỡ đất cứ tiếp tục bừa xới để đất sẵn sàng cho việc trồng trọt sao?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.256Z",
      "updatedAt": "2024-07-15T03:29:37.256Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "896ca224-2c3e-4a49-962f-717d42f0c434",
      "number": 25,
      "content": "Khi mặt đất đã được ban bằng,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.258Z",
      "updatedAt": "2024-07-15T03:29:37.258Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "3b4fa450-827e-42cb-b0d1-d47bc3c07d68",
      "number": 25,
      "content": "Người ta há không rắc thì là đen,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.258Z",
      "updatedAt": "2024-07-15T03:29:37.258Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "1ecfb9d7-bb38-4854-ba78-a3a34f2da9c2",
      "number": 25,
      "content": "Gieo hạt thì là,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.258Z",
      "updatedAt": "2024-07-15T03:29:37.258Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "f6d9f652-641f-4bc4-a507-d967872d1af0",
      "number": 25,
      "content": "Trồng lúa mì thành hàng,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.258Z",
      "updatedAt": "2024-07-15T03:29:37.258Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "e2b9e8e3-44a6-48cd-b33c-c85790209bcf",
      "number": 25,
      "content": "Cấy lúa mạch vào khu định cho chúng,",
      "order": 4,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.258Z",
      "updatedAt": "2024-07-15T03:29:37.258Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "a15be633-dfa1-4285-bb67-320bf1a4b0c6",
      "number": 25,
      "content": "Và trồng lúa mì nâu ven bờ ruộng hay sao?",
      "order": 5,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.258Z",
      "updatedAt": "2024-07-15T03:29:37.258Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "101e6f43-d12e-4696-9fea-cec3102ec13f",
      "number": 26,
      "content": "Vì người ta đã được chỉ dạy cách thức như vậy;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.261Z",
      "updatedAt": "2024-07-15T03:29:37.261Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "5e5d47d8-c382-4d1a-9c6f-19288735003b",
      "number": 26,
      "content": "Chính Ðức Chúa Trời đã chỉ bảo cho người ta như thế.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.261Z",
      "updatedAt": "2024-07-15T03:29:37.261Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "edeaae9d-ad47-476d-8a4d-3584feaca6d4",
      "number": 27,
      "content": "Không ai dùng búa để đập thì là đen bao giờ,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.263Z",
      "updatedAt": "2024-07-15T03:29:37.263Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "a2aca208-77fe-43e1-8354-976841a33d14",
      "number": 27,
      "content": "Cũng không ai dùng trục lăn mà cán để lấy hạt thì là,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.263Z",
      "updatedAt": "2024-07-15T03:29:37.263Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "784619f9-2b5a-464b-8f5b-b5c0ed0a0608",
      "number": 27,
      "content": "Nhưng thì là đen, người ta dùng cây que mà khẻ,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.263Z",
      "updatedAt": "2024-07-15T03:29:37.263Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "6b45f5de-204d-4b92-bea3-1c742f6872c9",
      "number": 27,
      "content": "Còn thì là, họ dùng que nhỏ mà khẻ nhẹ cho hạt rớt ra.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.263Z",
      "updatedAt": "2024-07-15T03:29:37.263Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "fcfff1dd-f0fe-49ad-8ce5-39ddf11ae1b7",
      "number": 28,
      "content": "Người ta xay gạo để làm bánh,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.265Z",
      "updatedAt": "2024-07-15T03:29:37.265Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "3d480e0e-366c-4599-848f-e51f18255fbc",
      "number": 28,
      "content": "Nhưng không ai xay lúa mà muốn gạo nát vụn bao giờ;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.265Z",
      "updatedAt": "2024-07-15T03:29:37.265Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "a2a3df3d-f8dd-4f86-b409-2ec8a5c9f5a1",
      "number": 28,
      "content": "Dù người ta lăn bánh xe qua hoặc cho ngựa giẫm lên cốt chỉ để trốc trấu ra thôi,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.265Z",
      "updatedAt": "2024-07-15T03:29:37.265Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "f0826229-295f-4519-95a0-35eae5589922",
      "number": 28,
      "content": "Chứ không ai muốn hạt gạo bị nghiền đến nát vụn bao giờ.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.265Z",
      "updatedAt": "2024-07-15T03:29:37.265Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "d594268c-f343-4fd5-aa02-5fc250dee410",
      "number": 29,
      "content": "Ðây cũng là điều đến từ Chúa các đạo quân;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.268Z",
      "updatedAt": "2024-07-15T03:29:37.268Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "7617e167-be62-418c-a89e-c2dab730d8b6",
      "number": 29,
      "content": "Kế hoạch của Ngài quả là kỳ diệu,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.268Z",
      "updatedAt": "2024-07-15T03:29:37.268Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    },
    {
      "id": "c31b9681-386d-4bf4-89d1-e4ac852da971",
      "number": 29,
      "content": "Sự khôn ngoan của Ngài thật vượt trỗi vô cùng.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:29:37.268Z",
      "updatedAt": "2024-07-15T03:29:37.268Z",
      "chapterId": "035ac4a1-f688-4616-935e-db04d7455be8"
    }
  ],
