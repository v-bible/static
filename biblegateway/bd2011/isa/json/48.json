{
  "chapter": {
    "id": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5",
    "number": 48,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2048&version=BD2011",
    "createdAt": "2024-07-12T04:03:50.780Z",
    "updatedAt": "2024-07-24T15:07:51.437Z",
    "bookId": "e78c99ae-11b2-4687-bcf7-b33bef999af4"
  },
  "verses": [
    {
      "id": "413934ea-e4eb-4dd3-ae31-f5e2b6d75ed6",
      "number": 1,
      "content": "Hãy nghe đây, hỡi nhà Gia-cốp;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:39.278Z",
      "updatedAt": "2024-07-15T03:33:39.390Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "d6b584a7-e0d6-44d5-9b1c-50c3e9fabb08",
      "number": 1,
      "content": "Hỡi những kẻ được gọi bằng danh I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:39.278Z",
      "updatedAt": "2024-07-15T03:33:39.404Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "0cd1cf95-f430-4c42-9c53-4f5b6df3783c",
      "number": 1,
      "content": "Và những người ra từ nguồn cội của Giu-đa,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:39.278Z",
      "updatedAt": "2024-07-15T03:33:39.414Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "f02ef850-3b36-4d40-a59c-60ed041e69f7",
      "number": 1,
      "content": "Những người lấy danh Chúa mà thề,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:39.278Z",
      "updatedAt": "2024-07-15T03:33:39.424Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "606e3a8b-4670-4ac7-8ea5-d276532f7831",
      "number": 1,
      "content": "Cầu khẩn Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:39.278Z",
      "updatedAt": "2024-07-15T03:33:39.434Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "09f14889-3d74-4596-92bb-308db7449504",
      "number": 1,
      "content": "Nhưng lại không thật lòng và ngay thẳng,",
      "order": 5,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:39.278Z",
      "updatedAt": "2024-07-15T03:33:39.444Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "dcbae582-d6fb-4f3e-af2c-e2e75200d28b",
      "number": 2,
      "content": "Dù chúng cho mình là dân của thành thánh,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:39.288Z",
      "updatedAt": "2024-07-15T03:33:39.455Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "438e06e4-fbb9-4d50-9e8d-0ffab1da0a22",
      "number": 2,
      "content": "Và nương cậy vào Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:39.288Z",
      "updatedAt": "2024-07-15T03:33:39.466Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "e6faf133-0654-45bf-8003-d561da625c82",
      "number": 2,
      "content": "Tức Ðấng có danh là Chúa các đạo quân.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:33:39.288Z",
      "updatedAt": "2024-07-15T03:33:39.476Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "a2073b7b-933c-4bd4-adb3-7b592b3345c4",
      "number": 3,
      "content": "Những chuyện đã qua Ta đã báo trước tự ngàn xưa;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.291Z",
      "updatedAt": "2024-07-15T03:33:39.291Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "bae20cda-6503-4258-a5fe-ee273d6c67ca",
      "number": 3,
      "content": "Những điều ấy đã ra từ miệng Ta,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.291Z",
      "updatedAt": "2024-07-15T03:33:39.291Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "e2560940-3387-4f51-90f2-56ba0db11760",
      "number": 3,
      "content": "Và Ta làm cho người ta biết chúng,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.291Z",
      "updatedAt": "2024-07-15T03:33:39.291Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "42ece11b-4ddf-442b-a4c5-7fd7ff90ddd5",
      "number": 3,
      "content": "Rồi thình lình Ta thực hiện những điều ấy,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.291Z",
      "updatedAt": "2024-07-15T03:33:39.291Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "cd587eb3-d616-4e59-9593-262809bb2390",
      "number": 3,
      "content": "Và chúng đã xảy ra.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.291Z",
      "updatedAt": "2024-07-15T03:33:39.291Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "01247a49-493f-4070-af4f-818fdd86d968",
      "number": 4,
      "content": "Vì Ta biết ngươi là kẻ cứng đầu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.294Z",
      "updatedAt": "2024-07-15T03:33:39.294Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "a73e4c0e-4893-4e9d-b891-c039ede03005",
      "number": 4,
      "content": "Gân cổ ngươi cứng như sắt,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.294Z",
      "updatedAt": "2024-07-15T03:33:39.294Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "8c9330c9-7d34-4a15-a65f-576f196963b9",
      "number": 4,
      "content": "Trán của ngươi cứng như đồng,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.294Z",
      "updatedAt": "2024-07-15T03:33:39.294Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "c5b7791d-d5a4-4a6e-b00a-00fec2933a3b",
      "number": 5,
      "content": "Nên ngay từ ban đầu Ta đã nói cho ngươi những điều ấy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.297Z",
      "updatedAt": "2024-07-15T03:33:39.297Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "76a178bc-fea5-4391-ad59-caa76d8ed8f6",
      "number": 5,
      "content": "Trước khi chúng xảy ra, Ta đã nói về chúng cho ngươi hay,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.297Z",
      "updatedAt": "2024-07-15T03:33:39.297Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "15ec272b-b8cf-4187-904f-2bcdeb1cc9fe",
      "number": 5,
      "content": "Ðể ngươi không thể nói,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.297Z",
      "updatedAt": "2024-07-15T03:33:39.297Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "99d9f01e-3d18-4da7-877a-c1760ea83ed8",
      "number": 5,
      "content": "“Ấy là thần tượng của tôi đã làm những điều ấy;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.297Z",
      "updatedAt": "2024-07-15T03:33:39.297Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "01f1bbdf-8f87-4696-bc1e-3097c06d9a28",
      "number": 5,
      "content": "Ðó là tượng chạm của tôi và tượng đúc của tôi đã ra lịnh cho chúng phải xảy ra.”",
      "order": 4,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.297Z",
      "updatedAt": "2024-07-15T03:33:39.297Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "9c1d6ec3-9fa8-4953-bc99-28b5a688c6f6",
      "number": 6,
      "content": "Ngươi đã nghe rồi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.299Z",
      "updatedAt": "2024-07-15T03:33:39.299Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "87b658a4-5cc9-4629-8a95-edc70396867c",
      "number": 6,
      "content": "Ngươi đã thấy tất cả rồi;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.299Z",
      "updatedAt": "2024-07-15T03:33:39.299Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "ad018dcf-17d0-480a-89b9-eecd11b00b95",
      "number": 6,
      "content": "Sao ngươi không tuyên bố ra đi?",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.299Z",
      "updatedAt": "2024-07-15T03:33:39.299Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "2b4fea08-0d22-44e7-8dd0-249d74e709d5",
      "number": 6,
      "content": "Từ nay Ta sẽ cho ngươi nghe những điều mới,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.299Z",
      "updatedAt": "2024-07-15T03:33:39.299Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "4fbe5842-0cae-414c-8bbb-1b38e5912a4f",
      "number": 6,
      "content": "Ngay cả những điều còn giữ kín mà ngươi chưa hề biết.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.299Z",
      "updatedAt": "2024-07-15T03:33:39.299Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "2a2b4bfb-264b-4b27-8e25-187e82416b3c",
      "number": 7,
      "content": "Ðến bây giờ những điều ấy mới được tạo nên,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.302Z",
      "updatedAt": "2024-07-15T03:33:39.302Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "baddcdf5-6515-4ddb-9d47-67161d243327",
      "number": 7,
      "content": "Chứ chẳng phải chúng đã có sẵn từ lâu rồi đâu;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.302Z",
      "updatedAt": "2024-07-15T03:33:39.302Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "c16590d3-a3b4-47eb-aadf-67321834f991",
      "number": 7,
      "content": "Trước ngày này ngươi chưa bao giờ nghe về chúng,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.302Z",
      "updatedAt": "2024-07-15T03:33:39.302Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "f680a360-236b-4d42-987d-1a7c6c7c0a1e",
      "number": 7,
      "content": "Ðể ngươi không thể nói,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.302Z",
      "updatedAt": "2024-07-15T03:33:39.302Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "12088ba4-c058-4dc1-b7a8-5a2d2fdd39b2",
      "number": 7,
      "content": "“Ồ tưởng gì chứ những điều đó tôi đã biết cả rồi.”",
      "order": 4,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.302Z",
      "updatedAt": "2024-07-15T03:33:39.302Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "8de33736-a545-47db-98ac-1abb25aa2150",
      "number": 8,
      "content": "Chắc chắn ngươi chưa nghe đến;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.304Z",
      "updatedAt": "2024-07-15T03:33:39.304Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "f4da530f-9fe1-494f-94fc-5c9e342f5fac",
      "number": 8,
      "content": "Chắc chắn ngươi chưa biết đến;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.304Z",
      "updatedAt": "2024-07-15T03:33:39.304Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "33e90883-3462-4458-acb5-4c44abf441cb",
      "number": 8,
      "content": "Chắc chắn từ lâu tai ngươi chưa mở ra,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.304Z",
      "updatedAt": "2024-07-15T03:33:39.304Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "d3ccba66-1469-4fc3-be48-521d12088fc5",
      "number": 8,
      "content": "Vì Ta biết ngươi là kẻ phản bội,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.304Z",
      "updatedAt": "2024-07-15T03:33:39.304Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "54d9b1b5-4498-4183-8bad-96fd99acb5e9",
      "number": 8,
      "content": "Ngươi mang tên là kẻ phản phúc từ lúc ngươi còn là một thai nhi.",
      "order": 4,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.304Z",
      "updatedAt": "2024-07-15T03:33:39.304Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "95da5a35-4379-4d45-8c6c-2afe4eb7bbb9",
      "number": 9,
      "content": "Bấy lâu nay, vì danh Ta, Ta cam nén giận,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.307Z",
      "updatedAt": "2024-07-15T03:33:39.307Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "2a963690-8681-4570-8b3f-92dd49826d99",
      "number": 9,
      "content": "Vì sự ca ngợi dành cho Ta, Ta dằn lòng chịu đựng ngươi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.307Z",
      "updatedAt": "2024-07-15T03:33:39.307Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "d9cb41c4-3a85-459d-8032-c085e5f207c0",
      "number": 9,
      "content": "Mà không tiêu diệt ngươi.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.307Z",
      "updatedAt": "2024-07-15T03:33:39.307Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "445f0a87-cce8-4933-aa17-e9e945e66337",
      "number": 10,
      "content": "Này, Ta đã tinh luyện ngươi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.309Z",
      "updatedAt": "2024-07-15T03:33:39.309Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "4ed4ccef-715b-4568-ab62-78d9249b1a18",
      "number": 10,
      "content": "Nhưng không như người ta luyện bạc;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.309Z",
      "updatedAt": "2024-07-15T03:33:39.309Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "e4c1bb17-bf11-403c-ab35-f0053763f278",
      "number": 10,
      "content": "Ta đã rèn luyện ngươi trong lò lửa khổ đau.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.309Z",
      "updatedAt": "2024-07-15T03:33:39.309Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "dd535700-b139-4e40-b6f3-ddd4b7fc6ea9",
      "number": 11,
      "content": "Nhưng nay vì cớ Ta, vì chính Ta, Ta sẽ ra tay hành động;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.312Z",
      "updatedAt": "2024-07-15T03:33:39.312Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "c1df4eef-e706-4d62-b7b5-f9669d8d8d12",
      "number": 11,
      "content": "Chứ lẽ nào Ta cứ để cho danh Ta bị sỉ nhục hay sao?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.312Z",
      "updatedAt": "2024-07-15T03:33:39.312Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "4bbe4072-6c2d-4dd7-b457-ee94c20881dd",
      "number": 11,
      "content": "Ta sẽ không nhường vinh hiển Ta cho một kẻ nào khác.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.312Z",
      "updatedAt": "2024-07-15T03:33:39.312Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "1d2856d5-1754-4737-be77-86acb195aaa4",
      "number": 12,
      "content": "Hỡi Gia-cốp, hãy lắng nghe Ta;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.314Z",
      "updatedAt": "2024-07-15T03:33:39.314Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "18e68757-3c82-49f4-8602-8e95c1df47b8",
      "number": 12,
      "content": "Hỡi I-sơ-ra-ên, người Ta đã gọi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.314Z",
      "updatedAt": "2024-07-15T03:33:39.314Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "e4586205-8d70-4d49-8fb9-bdf672d1afb9",
      "number": 12,
      "content": "Ta là Ðấng ấy,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.314Z",
      "updatedAt": "2024-07-15T03:33:39.314Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "3b19b720-00b6-4a0b-a740-fc9a56d2e887",
      "number": 12,
      "content": "Ta là Ðấng Ðầu Tiên, Ta cũng là Ðấng Sau Cùng.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.314Z",
      "updatedAt": "2024-07-15T03:33:39.314Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "34e2c332-d064-43db-846e-1a38a817ea2a",
      "number": 13,
      "content": "Chính tay Ta đã đặt nền trái đất;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.316Z",
      "updatedAt": "2024-07-15T03:33:39.316Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "c5d2d4de-ba40-4701-815b-65584dd7d488",
      "number": 13,
      "content": "Tay phải Ta đã trải rộng bầu trời;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.316Z",
      "updatedAt": "2024-07-15T03:33:39.316Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "dce4e9ec-bf1e-45bb-af33-4417d49f0e7b",
      "number": 13,
      "content": "Khi Ta gọi chúng, chúng cùng nhau đứng dậy.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.316Z",
      "updatedAt": "2024-07-15T03:33:39.316Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "2c399dd8-f16a-41e5-8df2-c39d3a726ff2",
      "number": 14,
      "content": "Hỡi tất cả các ngươi, hãy họp lại và lắng nghe!",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.319Z",
      "updatedAt": "2024-07-15T03:33:39.319Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "283efa28-579d-4bf1-be3d-28f58cf53d2e",
      "number": 14,
      "content": "Ai trong vòng chúng đã công bố những điều ấy?",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.319Z",
      "updatedAt": "2024-07-15T03:33:39.319Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "275a3678-2b40-4add-9032-4fa5ff9be195",
      "number": 14,
      "content": "Chúa yêu thương nó;",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.319Z",
      "updatedAt": "2024-07-15T03:33:39.319Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "d90d37e5-274e-43d9-851b-69f37128221d",
      "number": 14,
      "content": "Ngài sẽ thực hiện điều đẹp ý Ngài đối với Ba-by-lôn;",
      "order": 3,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.319Z",
      "updatedAt": "2024-07-15T03:33:39.319Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "70b33c36-c6da-4f55-bb8e-6b375ffaefec",
      "number": 14,
      "content": "Ngài sẽ đưa tay ra nghịch lại người Canh-đê.",
      "order": 4,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.319Z",
      "updatedAt": "2024-07-15T03:33:39.319Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "11ce6dee-456f-4817-9270-c81676fc235c",
      "number": 15,
      "content": "Ta, chính Ta, đã phán;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.321Z",
      "updatedAt": "2024-07-15T03:33:39.321Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "26a61398-e9c0-4915-9545-8acf804fda64",
      "number": 15,
      "content": "Phải, Ta đã gọi nó;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.321Z",
      "updatedAt": "2024-07-15T03:33:39.321Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "6fca5cbb-213c-4342-a4a0-e07d437e98e1",
      "number": 15,
      "content": "Ta đã dẫn dắt nó,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.321Z",
      "updatedAt": "2024-07-15T03:33:39.321Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "34f14d99-0588-4735-bd54-6278ca7417d4",
      "number": 15,
      "content": "Nên đường lối nó sẽ thành công thịnh vượng.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.321Z",
      "updatedAt": "2024-07-15T03:33:39.321Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "1a0ba9cc-9f30-4fc8-beff-3746cd8bfb60",
      "number": 16,
      "content": "Hãy đến gần Ta và nghe rõ điều này:",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.323Z",
      "updatedAt": "2024-07-15T03:33:39.323Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "e2d4b22e-c290-45a6-acbc-f9db4cdcd0bd",
      "number": 16,
      "content": "Từ ban đầu, Ta không nói điều gì bí ẩn;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.323Z",
      "updatedAt": "2024-07-15T03:33:39.323Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "866ef496-d69e-447f-b349-2a9f85981bfe",
      "number": 16,
      "content": "Lúc nó xảy ra, Ta đã có ở đó rồi.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.323Z",
      "updatedAt": "2024-07-15T03:33:39.323Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "156a4c4b-5988-49c9-a65a-2eec8f852f50",
      "number": 16,
      "content": "Và bây giờ Chúa Hằng Hữu và Thần của Ngài đã sai tôi.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.323Z",
      "updatedAt": "2024-07-15T03:33:39.323Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "06f11344-e303-4bab-a839-657ad24f258a",
      "number": 17,
      "content": "Chúa, Ðấng Cứu Chuộc ngươi, Ðấng Thánh của I-sơ-ra-ên, phán thế này:",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.326Z",
      "updatedAt": "2024-07-15T03:33:39.326Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "8bc24041-edb7-4a2a-88f4-12bf3488f963",
      "number": 17,
      "content": "Ta là Chúa, Ðức Chúa Trời của ngươi,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.326Z",
      "updatedAt": "2024-07-15T03:33:39.326Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "7eeb2f56-01a4-476b-95bb-1f725b3d8a88",
      "number": 17,
      "content": "Ðấng dạy bảo ngươi để ngươi được phước,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.326Z",
      "updatedAt": "2024-07-15T03:33:39.326Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "05afe185-647a-411a-ac35-a80ef77849d4",
      "number": 17,
      "content": "Ðấng dẫn dắt ngươi vào đường lối ngươi nên đi.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.326Z",
      "updatedAt": "2024-07-15T03:33:39.326Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "c10eb586-41c6-4968-9a3e-1280c034c3a5",
      "number": 18,
      "content": "Ôi, ước chi ngươi chú ý đến các điều răn Ta,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.329Z",
      "updatedAt": "2024-07-15T03:33:39.329Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "61c97b30-327f-4149-8770-cdd2e0cd979c",
      "number": 18,
      "content": "Thì hòa bình thịnh vượng của ngươi sẽ còn mãi như dòng sông vô tận,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.329Z",
      "updatedAt": "2024-07-15T03:33:39.329Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "b0ec926f-f4d2-4533-99a8-5984b5da7e2c",
      "number": 18,
      "content": "Sự thành công của ngươi sẽ không ngừng như sóng ở đại dương,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.329Z",
      "updatedAt": "2024-07-15T03:33:39.329Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "b5d85cd4-cbe4-4567-8bb7-aaccd64a12ef",
      "number": 19,
      "content": "Con cháu của ngươi sẽ đông như cát nơi bờ biển,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.331Z",
      "updatedAt": "2024-07-15T03:33:39.331Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "baab4c7b-f254-4210-a4f9-dee6321808c0",
      "number": 19,
      "content": "Dòng dõi của ngươi sẽ nhiều tựa cát của đại dương,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.331Z",
      "updatedAt": "2024-07-15T03:33:39.331Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "608ba912-5a2c-4522-a2b6-6690fe74bd29",
      "number": 19,
      "content": "Danh của ngươi sẽ không bao giờ bị xóa bỏ,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.331Z",
      "updatedAt": "2024-07-15T03:33:39.331Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "6e193e85-c207-400c-83bf-8d83393f69ab",
      "number": 19,
      "content": "Và chẳng bao giờ bị diệt mất trước mặt Ta.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.331Z",
      "updatedAt": "2024-07-15T03:33:39.331Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "52e09fbf-516f-4c07-b5f7-207439a5bba4",
      "number": 20,
      "content": "Hãy rời khỏi Ba-by-lôn,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.333Z",
      "updatedAt": "2024-07-15T03:33:39.333Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "64b98215-ec11-4b04-9337-418f3041ca0b",
      "number": 20,
      "content": "Hãy trốn khỏi dân Canh-đê,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.333Z",
      "updatedAt": "2024-07-15T03:33:39.333Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "0d852480-62a8-4e0e-9640-7e980603ec3f",
      "number": 20,
      "content": "Hãy cất tiếng hát ca,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.333Z",
      "updatedAt": "2024-07-15T03:33:39.333Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "bdf2e01b-ab4d-4073-815f-ba4ce730f2dc",
      "number": 20,
      "content": "Hãy tuyên bố,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.333Z",
      "updatedAt": "2024-07-15T03:33:39.333Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "4d17bce5-66e3-4af5-bbb0-ae1df54a9372",
      "number": 20,
      "content": "Hãy công bố điều ấy ra,",
      "order": 4,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.333Z",
      "updatedAt": "2024-07-15T03:33:39.333Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "83f46680-5e72-4733-9c73-360821c74c0c",
      "number": 20,
      "content": "Hãy truyền tin ấy đến tận cùng trái đất,",
      "order": 5,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.333Z",
      "updatedAt": "2024-07-15T03:33:39.333Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "32431fd8-4426-4891-9c25-05dca9b6a9dd",
      "number": 20,
      "content": "Bảo rằng, “Chúa cứu chuộc Gia-cốp đầy tớ Ngài.”",
      "order": 6,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.333Z",
      "updatedAt": "2024-07-15T03:33:39.333Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "8147ace5-cbc0-402d-8890-5244ecc81ad6",
      "number": 21,
      "content": "Dù Ngài dẫn họ đi qua sa mạc,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.337Z",
      "updatedAt": "2024-07-15T03:33:39.337Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "fd06a203-2ed6-44c5-be28-473cb112eef7",
      "number": 21,
      "content": "Họ cũng không khát nước,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.337Z",
      "updatedAt": "2024-07-15T03:33:39.337Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "30352d41-7af9-45e2-bc18-06ac601fb5d1",
      "number": 21,
      "content": "Vì Ngài khiến nước từ vầng đá chảy ra cho họ uống.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.337Z",
      "updatedAt": "2024-07-15T03:33:39.337Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "645ced2e-fd12-4cae-9dc8-ea6bbbce9486",
      "number": 21,
      "content": "Ngài chẻ vầng đá, và nước đã trào ra.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.337Z",
      "updatedAt": "2024-07-15T03:33:39.337Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "c577bd96-c1ec-43a5-b47a-d009e681f64c",
      "number": 22,
      "content": "Tuy nhiên phường gian tà sẽ không có bình an,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.342Z",
      "updatedAt": "2024-07-15T03:33:39.342Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    },
    {
      "id": "a857f56a-a769-43ea-9ad2-3b8455ff7422",
      "number": 22,
      "content": "Chúa phán.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:33:39.342Z",
      "updatedAt": "2024-07-15T03:33:39.342Z",
      "chapterId": "79e1bb3b-e4ef-4530-bb2e-7ce860b7ccf5"
    }
  ],
