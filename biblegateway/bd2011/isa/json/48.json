{
  "chapter": {
    "id": "7b033faa-da40-4b71-a4df-d984b8e33e4b",
    "number": 48,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2048&version=BD2011",
    "createdAt": "2024-09-09T04:13:04.122Z",
    "updatedAt": "2024-09-09T04:13:04.122Z",
    "bookId": "5349a9d9-642f-4c7c-a482-7d88b8006432"
  },
  "verses": [
    {
      "id": "70ddc4a2-0594-4c2f-9ef3-2397268c22b4",
      "number": 1,
      "content": "Hãy nghe đây, hỡi nhà Gia-cốp;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.337Z",
      "updatedAt": "2024-09-09T04:58:34.337Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "471a2a3d-8557-47f2-a6da-3da1f12e8352",
      "number": 1,
      "content": "Hỡi những kẻ được gọi bằng danh I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.342Z",
      "updatedAt": "2024-09-09T04:58:34.342Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "fdd29961-6502-4a77-88b0-48cfad15cb78",
      "number": 1,
      "content": "Và những người ra từ nguồn cội của Giu-đa,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.344Z",
      "updatedAt": "2024-09-09T04:58:34.344Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "70584513-51b3-47fa-ba5e-3bb2821b01ca",
      "number": 1,
      "content": "Những người lấy danh Chúa mà thề,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.346Z",
      "updatedAt": "2024-09-09T04:58:34.346Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "bfb70710-06b2-447d-aea5-c765e04903a5",
      "number": 1,
      "content": "Cầu khẩn Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.348Z",
      "updatedAt": "2024-09-09T04:58:34.348Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "9d7ede02-5d03-4e44-abd6-24f3b241f2b2",
      "number": 1,
      "content": "Nhưng lại không thật lòng và ngay thẳng,",
      "order": 5,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.350Z",
      "updatedAt": "2024-09-09T04:58:34.350Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "0838285e-22e0-423d-8aaf-3de32ea0bc59",
      "number": 2,
      "content": "Dù chúng cho mình là dân của thành thánh,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.352Z",
      "updatedAt": "2024-09-09T04:58:34.352Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "f3739b53-21b3-4b0d-85b0-b9bd20d5d468",
      "number": 2,
      "content": "Và nương cậy vào Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.354Z",
      "updatedAt": "2024-09-09T04:58:34.354Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "fef8b0bd-8d1f-4301-90cc-5245828076b4",
      "number": 2,
      "content": "Tức Ðấng có danh là Chúa các đạo quân.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.356Z",
      "updatedAt": "2024-09-09T04:58:34.356Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "66afdb55-244c-46e4-a40a-29492156ae3e",
      "number": 3,
      "content": "Những chuyện đã qua Ta đã báo trước tự ngàn xưa;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.358Z",
      "updatedAt": "2024-09-09T04:58:34.358Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "b40ad81a-f1f0-4275-97fc-8bcb0cfba0b8",
      "number": 3,
      "content": "Những điều ấy đã ra từ miệng Ta,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.360Z",
      "updatedAt": "2024-09-09T04:58:34.360Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "06288813-4478-40b8-83b3-7c8e707025f9",
      "number": 3,
      "content": "Và Ta làm cho người ta biết chúng,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.362Z",
      "updatedAt": "2024-09-09T04:58:34.362Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "0d13f7d6-9821-4b87-afbb-659efbb2041c",
      "number": 3,
      "content": "Rồi thình lình Ta thực hiện những điều ấy,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.364Z",
      "updatedAt": "2024-09-09T04:58:34.364Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "b3cc6dcb-ab7b-44db-b5fa-4594d0f602d2",
      "number": 3,
      "content": "Và chúng đã xảy ra.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.365Z",
      "updatedAt": "2024-09-09T04:58:34.365Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "c712cb36-1948-4cb8-b240-b2601b82722b",
      "number": 4,
      "content": "Vì Ta biết ngươi là kẻ cứng đầu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.367Z",
      "updatedAt": "2024-09-09T04:58:34.367Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "3f66fcb0-a88e-4522-a5ff-44dfa3d0b24e",
      "number": 4,
      "content": "Gân cổ ngươi cứng như sắt,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.371Z",
      "updatedAt": "2024-09-09T04:58:34.371Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "d6ac9c85-8a9e-4762-8467-f8043c5a20f8",
      "number": 4,
      "content": "Trán của ngươi cứng như đồng,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.375Z",
      "updatedAt": "2024-09-09T04:58:34.375Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "31ca324a-19e1-45b3-90c8-45aee5844538",
      "number": 5,
      "content": "Nên ngay từ ban đầu Ta đã nói cho ngươi những điều ấy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.380Z",
      "updatedAt": "2024-09-09T04:58:34.380Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "c6d0fc1d-59c2-43d6-a1c5-8a547abf7185",
      "number": 5,
      "content": "Trước khi chúng xảy ra, Ta đã nói về chúng cho ngươi hay,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.384Z",
      "updatedAt": "2024-09-09T04:58:34.384Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "2b41923c-1074-49d2-ae58-ef0b65c3a3d3",
      "number": 5,
      "content": "Ðể ngươi không thể nói,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.389Z",
      "updatedAt": "2024-09-09T04:58:34.389Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "a277ffbc-2014-46dc-ad90-13dfae5a4ff2",
      "number": 7,
      "content": "Ðể ngươi không thể nói,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.441Z",
      "updatedAt": "2024-09-09T04:58:34.441Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "f49f0635-e6fd-44f7-a054-316135f79668",
      "number": 5,
      "content": "“Ấy là thần tượng của tôi đã làm những điều ấy;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.394Z",
      "updatedAt": "2024-09-09T04:58:34.394Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "ebf607f2-3a2c-4f1f-849d-9207567b5c13",
      "number": 5,
      "content": "Ðó là tượng chạm của tôi và tượng đúc của tôi đã ra lịnh cho chúng phải xảy ra.”",
      "order": 4,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.399Z",
      "updatedAt": "2024-09-09T04:58:34.399Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "d0be240b-010d-4f0b-a82f-92084adfd3ec",
      "number": 6,
      "content": "Ngươi đã nghe rồi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.404Z",
      "updatedAt": "2024-09-09T04:58:34.404Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "985647c2-d813-4dfd-bd6b-bab353b9aad6",
      "number": 6,
      "content": "Ngươi đã thấy tất cả rồi;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.409Z",
      "updatedAt": "2024-09-09T04:58:34.409Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "2c0b17b4-79a1-413c-9618-07df6298fa03",
      "number": 6,
      "content": "Sao ngươi không tuyên bố ra đi?",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.413Z",
      "updatedAt": "2024-09-09T04:58:34.413Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "9ff8220e-e911-49fa-9c14-62e064d6f285",
      "number": 6,
      "content": "Từ nay Ta sẽ cho ngươi nghe những điều mới,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.418Z",
      "updatedAt": "2024-09-09T04:58:34.418Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "cc88e798-61d3-461c-8dc5-c9737d085bb0",
      "number": 6,
      "content": "Ngay cả những điều còn giữ kín mà ngươi chưa hề biết.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.422Z",
      "updatedAt": "2024-09-09T04:58:34.422Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "f3443d0a-d3da-4ff8-9223-7241908ccd3c",
      "number": 7,
      "content": "Ðến bây giờ những điều ấy mới được tạo nên,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.427Z",
      "updatedAt": "2024-09-09T04:58:34.427Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "5206411c-2c21-4a1e-860b-3b873c491911",
      "number": 7,
      "content": "Chứ chẳng phải chúng đã có sẵn từ lâu rồi đâu;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.432Z",
      "updatedAt": "2024-09-09T04:58:34.432Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "4b53a862-05f8-490f-b378-86552f025e42",
      "number": 7,
      "content": "Trước ngày này ngươi chưa bao giờ nghe về chúng,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.436Z",
      "updatedAt": "2024-09-09T04:58:34.436Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "87f946f8-631f-45f0-a1c2-ac9feef67a68",
      "number": 7,
      "content": "“Ồ tưởng gì chứ những điều đó tôi đã biết cả rồi.”",
      "order": 4,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.446Z",
      "updatedAt": "2024-09-09T04:58:34.446Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "0376bbae-fcf0-4775-bca0-d1031bc96422",
      "number": 8,
      "content": "Chắc chắn ngươi chưa nghe đến;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.451Z",
      "updatedAt": "2024-09-09T04:58:34.451Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "0137e775-8982-469b-bdff-ce3212b83444",
      "number": 8,
      "content": "Chắc chắn ngươi chưa biết đến;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.456Z",
      "updatedAt": "2024-09-09T04:58:34.456Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "03d3a871-a068-455c-9b89-3dc8e903b5f0",
      "number": 8,
      "content": "Chắc chắn từ lâu tai ngươi chưa mở ra,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.460Z",
      "updatedAt": "2024-09-09T04:58:34.460Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "f571d940-75d5-4270-84d8-297834a67182",
      "number": 8,
      "content": "Vì Ta biết ngươi là kẻ phản bội,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.465Z",
      "updatedAt": "2024-09-09T04:58:34.465Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "4beb201f-ffad-448f-950a-e06a8d906409",
      "number": 8,
      "content": "Ngươi mang tên là kẻ phản phúc từ lúc ngươi còn là một thai nhi.",
      "order": 4,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.470Z",
      "updatedAt": "2024-09-09T04:58:34.470Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "5b673e5c-5f8d-49aa-9bb0-707c4b5a94d5",
      "number": 9,
      "content": "Bấy lâu nay, vì danh Ta, Ta cam nén giận,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.475Z",
      "updatedAt": "2024-09-09T04:58:34.475Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "9ce820c6-8a61-4b26-8906-73c64bcc909b",
      "number": 9,
      "content": "Vì sự ca ngợi dành cho Ta, Ta dằn lòng chịu đựng ngươi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.480Z",
      "updatedAt": "2024-09-09T04:58:34.480Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "4b9cb8a5-4391-4c60-bde3-db6c9ea94ea3",
      "number": 9,
      "content": "Mà không tiêu diệt ngươi.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.484Z",
      "updatedAt": "2024-09-09T04:58:34.484Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "09dcdddb-361c-4c02-a531-a6cdcc9e6585",
      "number": 10,
      "content": "Này, Ta đã tinh luyện ngươi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.489Z",
      "updatedAt": "2024-09-09T04:58:34.489Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "cd35c871-2742-4f0b-b430-41cee07249ac",
      "number": 10,
      "content": "Nhưng không như người ta luyện bạc;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.494Z",
      "updatedAt": "2024-09-09T04:58:34.494Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "db20a1db-de46-4bca-ad21-a175c7fc0013",
      "number": 10,
      "content": "Ta đã rèn luyện ngươi trong lò lửa khổ đau.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.498Z",
      "updatedAt": "2024-09-09T04:58:34.498Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "e7c35db9-67c2-4e45-b35e-a5515015bb76",
      "number": 11,
      "content": "Nhưng nay vì cớ Ta, vì chính Ta, Ta sẽ ra tay hành động;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.503Z",
      "updatedAt": "2024-09-09T04:58:34.503Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "511e689d-5598-41d1-9ca0-1710d8267ce2",
      "number": 11,
      "content": "Chứ lẽ nào Ta cứ để cho danh Ta bị sỉ nhục hay sao?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.508Z",
      "updatedAt": "2024-09-09T04:58:34.508Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "bb055eed-3543-4a63-b14e-db6fbd519f92",
      "number": 11,
      "content": "Ta sẽ không nhường vinh hiển Ta cho một kẻ nào khác.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.513Z",
      "updatedAt": "2024-09-09T04:58:34.513Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "7020e356-a48e-437b-bcc6-9a2242fa0022",
      "number": 12,
      "content": "Hỡi Gia-cốp, hãy lắng nghe Ta;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.518Z",
      "updatedAt": "2024-09-09T04:58:34.518Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "d0619f36-d219-4d23-8d49-0d902f007fbe",
      "number": 12,
      "content": "Hỡi I-sơ-ra-ên, người Ta đã gọi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.522Z",
      "updatedAt": "2024-09-09T04:58:34.522Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "413a899b-f36a-4ff4-a566-ccaa092c3886",
      "number": 12,
      "content": "Ta là Ðấng ấy,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.527Z",
      "updatedAt": "2024-09-09T04:58:34.527Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "4e9625ed-162b-420c-b3b4-1ccbc465a11c",
      "number": 12,
      "content": "Ta là Ðấng Ðầu Tiên, Ta cũng là Ðấng Sau Cùng.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.532Z",
      "updatedAt": "2024-09-09T04:58:34.532Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "47ac230a-d6fb-4a63-9127-6966e30cbaba",
      "number": 13,
      "content": "Chính tay Ta đã đặt nền trái đất;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.537Z",
      "updatedAt": "2024-09-09T04:58:34.537Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "c259274c-68c8-4caa-bbec-47abd6d5c607",
      "number": 13,
      "content": "Tay phải Ta đã trải rộng bầu trời;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.542Z",
      "updatedAt": "2024-09-09T04:58:34.542Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "53b4e2b0-92b7-4120-ac61-2e874cd0b23d",
      "number": 13,
      "content": "Khi Ta gọi chúng, chúng cùng nhau đứng dậy.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.547Z",
      "updatedAt": "2024-09-09T04:58:34.547Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "632c579a-66ca-4751-8b1b-c102aed0f09b",
      "number": 14,
      "content": "Hỡi tất cả các ngươi, hãy họp lại và lắng nghe!",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.552Z",
      "updatedAt": "2024-09-09T04:58:34.552Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "53174e5d-9d3b-4183-a631-26c6d1080fd8",
      "number": 14,
      "content": "Ai trong vòng chúng đã công bố những điều ấy?",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.556Z",
      "updatedAt": "2024-09-09T04:58:34.556Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "4b067db0-6ff8-437a-b93f-ad97a32706e0",
      "number": 14,
      "content": "Chúa yêu thương nó;",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.561Z",
      "updatedAt": "2024-09-09T04:58:34.561Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "081a3ea1-9c0b-454a-bb6d-6ba0a20da901",
      "number": 14,
      "content": "Ngài sẽ thực hiện điều đẹp ý Ngài đối với Ba-by-lôn;",
      "order": 3,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.566Z",
      "updatedAt": "2024-09-09T04:58:34.566Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "e6418621-ab54-474f-b36e-999a4063cee5",
      "number": 14,
      "content": "Ngài sẽ đưa tay ra nghịch lại người Canh-đê.",
      "order": 4,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.571Z",
      "updatedAt": "2024-09-09T04:58:34.571Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "e5078220-d21c-489b-8169-9da401dfaa38",
      "number": 15,
      "content": "Ta, chính Ta, đã phán;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.576Z",
      "updatedAt": "2024-09-09T04:58:34.576Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "f7ad1f8c-9851-4bfb-9ed0-e9d41a2afadb",
      "number": 15,
      "content": "Phải, Ta đã gọi nó;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.582Z",
      "updatedAt": "2024-09-09T04:58:34.582Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "ca7766d1-3bce-44af-b789-27f3efa95d22",
      "number": 15,
      "content": "Ta đã dẫn dắt nó,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.586Z",
      "updatedAt": "2024-09-09T04:58:34.586Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "1cc789ef-f44a-4bab-92d9-b4b62e266144",
      "number": 15,
      "content": "Nên đường lối nó sẽ thành công thịnh vượng.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.592Z",
      "updatedAt": "2024-09-09T04:58:34.592Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "b79900f9-8d02-4795-85a5-0eaa417565cd",
      "number": 16,
      "content": "Hãy đến gần Ta và nghe rõ điều này:",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.596Z",
      "updatedAt": "2024-09-09T04:58:34.596Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "67ebe861-14d2-42cb-a70b-fe2f009f085a",
      "number": 16,
      "content": "Từ ban đầu, Ta không nói điều gì bí ẩn;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.601Z",
      "updatedAt": "2024-09-09T04:58:34.601Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "3fe0e3e5-f589-483b-8d35-910e78e2146f",
      "number": 16,
      "content": "Lúc nó xảy ra, Ta đã có ở đó rồi.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.607Z",
      "updatedAt": "2024-09-09T04:58:34.607Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "04aa9dd0-e24a-4a5e-a380-fcb0dc25b1b9",
      "number": 16,
      "content": "Và bây giờ Chúa Hằng Hữu và Thần của Ngài đã sai tôi.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.612Z",
      "updatedAt": "2024-09-09T04:58:34.612Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "4c1c7e0e-9462-4c23-b602-a2262620b5fd",
      "number": 17,
      "content": "Chúa, Ðấng Cứu Chuộc ngươi, Ðấng Thánh của I-sơ-ra-ên, phán thế này:",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.617Z",
      "updatedAt": "2024-09-09T04:58:34.617Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "20d391e4-4869-41b9-b0f2-8af067218478",
      "number": 17,
      "content": "Ta là Chúa, Ðức Chúa Trời của ngươi,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.623Z",
      "updatedAt": "2024-09-09T04:58:34.623Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "716185f1-e3a6-4948-a9f7-4fb619c49fce",
      "number": 17,
      "content": "Ðấng dạy bảo ngươi để ngươi được phước,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.628Z",
      "updatedAt": "2024-09-09T04:58:34.628Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "3a5b8934-1a22-454a-849b-369713b2b773",
      "number": 17,
      "content": "Ðấng dẫn dắt ngươi vào đường lối ngươi nên đi.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.633Z",
      "updatedAt": "2024-09-09T04:58:34.633Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "19e0fc17-6cd6-4dc7-bf10-5b5269c79131",
      "number": 18,
      "content": "Ôi, ước chi ngươi chú ý đến các điều răn Ta,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.638Z",
      "updatedAt": "2024-09-09T04:58:34.638Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "608ec340-8bcc-455f-9e22-86951548c62f",
      "number": 18,
      "content": "Thì hòa bình thịnh vượng của ngươi sẽ còn mãi như dòng sông vô tận,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.643Z",
      "updatedAt": "2024-09-09T04:58:34.643Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "b1137f66-0ba0-446e-9a6b-c4c3f25b2096",
      "number": 18,
      "content": "Sự thành công của ngươi sẽ không ngừng như sóng ở đại dương,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.648Z",
      "updatedAt": "2024-09-09T04:58:34.648Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "5ab01c65-1bea-4e4e-bbdc-dd7bf1a44390",
      "number": 19,
      "content": "Con cháu của ngươi sẽ đông như cát nơi bờ biển,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.653Z",
      "updatedAt": "2024-09-09T04:58:34.653Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "ddffb134-8f3e-47bb-915d-cf4be3687eca",
      "number": 19,
      "content": "Dòng dõi của ngươi sẽ nhiều tựa cát của đại dương,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.658Z",
      "updatedAt": "2024-09-09T04:58:34.658Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "228695db-0d5f-40db-ae7c-9a0214516ba3",
      "number": 19,
      "content": "Danh của ngươi sẽ không bao giờ bị xóa bỏ,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.663Z",
      "updatedAt": "2024-09-09T04:58:34.663Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "17832ab5-70ec-4ed1-8562-117334199915",
      "number": 19,
      "content": "Và chẳng bao giờ bị diệt mất trước mặt Ta.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.668Z",
      "updatedAt": "2024-09-09T04:58:34.668Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "bf744cc2-a0f5-4ec8-b18e-60d67bd56e7d",
      "number": 20,
      "content": "Hãy rời khỏi Ba-by-lôn,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.673Z",
      "updatedAt": "2024-09-09T04:58:34.673Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "e8139d88-7697-4302-af11-afe18c2b9345",
      "number": 20,
      "content": "Hãy trốn khỏi dân Canh-đê,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.679Z",
      "updatedAt": "2024-09-09T04:58:34.679Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "c7be9b8b-b8e9-453b-87eb-38ee06ab531b",
      "number": 20,
      "content": "Hãy cất tiếng hát ca,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.684Z",
      "updatedAt": "2024-09-09T04:58:34.684Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "fa0f5889-7fd2-4465-bf72-29dbac1f1477",
      "number": 20,
      "content": "Hãy tuyên bố,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.689Z",
      "updatedAt": "2024-09-09T04:58:34.689Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "3910f073-7e14-4616-9300-558e2a5aafa5",
      "number": 20,
      "content": "Hãy công bố điều ấy ra,",
      "order": 4,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.695Z",
      "updatedAt": "2024-09-09T04:58:34.695Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "c480e66c-b9bf-4fbf-9857-d8c2cebf6132",
      "number": 20,
      "content": "Hãy truyền tin ấy đến tận cùng trái đất,",
      "order": 5,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.700Z",
      "updatedAt": "2024-09-09T04:58:34.700Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "bf9da918-64b8-4217-8410-d6ffbd74e727",
      "number": 20,
      "content": "Bảo rằng, “Chúa cứu chuộc Gia-cốp đầy tớ Ngài.”",
      "order": 6,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.705Z",
      "updatedAt": "2024-09-09T04:58:34.705Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "c68cb659-5e5e-4cba-9a47-371694baf9b9",
      "number": 21,
      "content": "Dù Ngài dẫn họ đi qua sa mạc,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.711Z",
      "updatedAt": "2024-09-09T04:58:34.711Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "8437a292-ba88-4303-a5a7-f6b653287337",
      "number": 21,
      "content": "Họ cũng không khát nước,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.716Z",
      "updatedAt": "2024-09-09T04:58:34.716Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "27998f0a-30af-40b5-ba68-d686f0f3a6dc",
      "number": 21,
      "content": "Vì Ngài khiến nước từ vầng đá chảy ra cho họ uống.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.721Z",
      "updatedAt": "2024-09-09T04:58:34.721Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "0fc8f279-ecff-4d11-9221-0d79b9ace2b9",
      "number": 21,
      "content": "Ngài chẻ vầng đá, và nước đã trào ra.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.726Z",
      "updatedAt": "2024-09-09T04:58:34.726Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "6c01b219-249c-4ca4-a784-d847dc98fe56",
      "number": 22,
      "content": "Tuy nhiên phường gian tà sẽ không có bình an,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.732Z",
      "updatedAt": "2024-09-09T04:58:34.732Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    },
    {
      "id": "02e52137-b250-4bcf-aad0-47862a857475",
      "number": 22,
      "content": "Chúa phán.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:34.736Z",
      "updatedAt": "2024-09-09T04:58:34.736Z",
      "chapterId": "7b033faa-da40-4b71-a4df-d984b8e33e4b"
    }
  ],
