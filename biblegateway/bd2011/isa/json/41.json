    },
    {
      "id": "3875b179-07bb-4238-9e7e-99e809896e9d",
      "content": "Sự Hư Không của Thần Tượng",
      "order": 0,
      "createdAt": "2024-07-15T03:32:16.588Z",
      "updatedAt": "2024-07-15T03:32:16.588Z",
