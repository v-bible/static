    },
    {
      "id": "9c5a833c-cf44-4278-88e5-2874262d44c7",
      "number": 23,
      "content": "Ngay cả người què cũng chiếm được một phần cho mình.",
      "order": 4,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.709Z",
      "updatedAt": "2024-07-15T03:30:35.709Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "9ab79629-0a01-4bf4-8334-4c11dcdea1de",
      "number": 24,
      "content": "Rồi không cư dân nào trong thành sẽ nói, “Tôi bị bịnh;”",
      "order": 0,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.711Z",
      "updatedAt": "2024-07-15T03:30:35.711Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "39e9f24e-bcfe-4369-bda0-364c1acf3272",
      "number": 24,
      "content": "Tội lỗi của dân sống trong thành đều sẽ được tha.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.711Z",
      "updatedAt": "2024-07-15T03:30:35.711Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    }
  ],
