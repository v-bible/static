{
  "chapter": {
    "id": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6",
    "number": 33,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2033&version=BD2011",
    "createdAt": "2024-07-12T04:03:50.489Z",
    "updatedAt": "2024-07-24T15:07:51.176Z",
    "bookId": "e78c99ae-11b2-4687-bcf7-b33bef999af4"
  },
  "verses": [
    {
      "id": "7b6193e3-6453-4eef-a000-e69460c31cf9",
      "number": 1,
      "content": "Khốn thay cho ngươi!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.650Z",
      "updatedAt": "2024-07-15T03:30:35.725Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "9dbed170-c957-42bd-b623-ac5a0531245d",
      "number": 1,
      "content": "Không ai hại ngươi mà ngươi đã hại người;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.650Z",
      "updatedAt": "2024-07-15T03:30:35.734Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "f229a125-df2b-4bfa-b657-49076e7938fe",
      "number": 1,
      "content": "Không ai phản bội ngươi mà ngươi đã phản bội người;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.650Z",
      "updatedAt": "2024-07-15T03:30:35.743Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "e73399b6-68ce-4a7c-85f4-9d642645d542",
      "number": 1,
      "content": "Khi ngươi hại người xong, chính ngươi sẽ bị hại;",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.650Z",
      "updatedAt": "2024-07-15T03:30:35.919Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "347c99a9-df60-4f0e-8413-c653a3a1074b",
      "number": 1,
      "content": "Khi ngươi phản bội người xong, chính ngươi sẽ bị phản bội.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.650Z",
      "updatedAt": "2024-07-15T03:30:35.934Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "15fefac9-2d4a-4a4a-9f17-58d9c35e926e",
      "number": 2,
      "content": "Lạy Chúa, xin thương xót chúng con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.653Z",
      "updatedAt": "2024-07-15T03:30:35.653Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "100169bb-f710-4602-9542-48a8ac8689d0",
      "number": 2,
      "content": "Chúng con trông cậy Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.653Z",
      "updatedAt": "2024-07-15T03:30:35.653Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "420d30dd-326b-4d8a-a23e-53bcc8cab22d",
      "number": 2,
      "content": "Mỗi buổi sáng xin Ngài làm cánh tay mạnh mẽ giúp đỡ chúng con;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.653Z",
      "updatedAt": "2024-07-15T03:30:35.653Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "fe957489-1aa2-4cd3-9a01-b1d999a07647",
      "number": 2,
      "content": "Xin làm Ðấng cứu giúp chúng con trong khi hoạn nạn.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.653Z",
      "updatedAt": "2024-07-15T03:30:35.653Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "5ab77915-3e82-4d90-990d-75ddce6fb063",
      "number": 3,
      "content": "Nghe tiếng động của Ngài, các dân liền bỏ chạy;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.655Z",
      "updatedAt": "2024-07-15T03:30:35.655Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "99e550ed-9b26-424c-b7c7-3ec49ac9d26c",
      "number": 3,
      "content": "Khi Ngài đứng lên, các nước chạy tán loạn khắp nơi.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.655Z",
      "updatedAt": "2024-07-15T03:30:35.655Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "efc84422-27d3-43f9-8319-c98e727f933e",
      "number": 4,
      "content": "Người ta thu chiến lợi phẩm như con sâu gom lấy thực phẩm,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.657Z",
      "updatedAt": "2024-07-15T03:30:35.657Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "f621fa8a-bc3b-4a0c-b65a-09d660ab2465",
      "number": 4,
      "content": "Người ta xông vào cướp giựt hổn loạn như đám cào cào búng nhảy tứ tung.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.657Z",
      "updatedAt": "2024-07-15T03:30:35.657Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "c8685c1a-6acc-4a13-b2de-8f9b42e92b4a",
      "number": 5,
      "content": "Chúa được tôn cao, vì Ngài ngự trên cõi trời cao;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.662Z",
      "updatedAt": "2024-07-15T03:30:35.662Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "b2d503c5-4aea-408c-864a-0f840f0779f0",
      "number": 5,
      "content": "Ngài làm cho Si-ôn đầy dẫy công lý và công chính.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.662Z",
      "updatedAt": "2024-07-15T03:30:35.662Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "702e0ed4-6719-46aa-a561-eaf1c545089a",
      "number": 6,
      "content": "Ngài sẽ là sự ổn định trong thời của ngươi;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.668Z",
      "updatedAt": "2024-07-15T03:30:35.668Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "35df8f31-a37d-43be-a431-a820968a9389",
      "number": 6,
      "content": "Ngài là kho tàng của sự giải cứu, khôn ngoan, và tri thức;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.668Z",
      "updatedAt": "2024-07-15T03:30:35.668Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "8ec9e73c-fafe-45da-bee2-a52c86b5dc99",
      "number": 6,
      "content": "Lòng kính sợ Chúa là chìa khóa để nhận được kho tàng ấy.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.668Z",
      "updatedAt": "2024-07-15T03:30:35.668Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "f357f6d7-0752-40ec-b862-5ee25140f075",
      "number": 7,
      "content": "Này, những người dũng mãnh sẽ kêu la ngoài đường phố;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.671Z",
      "updatedAt": "2024-07-15T03:30:35.671Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "1a75c12c-8770-497a-a79f-e567f1f9b269",
      "number": 7,
      "content": "Các sứ giả hòa bình sẽ khóc lóc đắng cay.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.671Z",
      "updatedAt": "2024-07-15T03:30:35.671Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "dee0584d-75c1-4df6-a848-ad220a9004e6",
      "number": 8,
      "content": "Các đại lộ sẽ vắng tanh;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.673Z",
      "updatedAt": "2024-07-15T03:30:35.673Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "92dd24e3-87e1-4abe-8ac6-1aa6e1835e07",
      "number": 8,
      "content": "Các du khách sẽ chẳng còn lai vãng ngược xuôi;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.673Z",
      "updatedAt": "2024-07-15T03:30:35.673Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "e72f049c-4230-4303-b16d-6c2e6c354780",
      "number": 8,
      "content": "Hiệp ước bị hủy bỏ;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.673Z",
      "updatedAt": "2024-07-15T03:30:35.673Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "b081ae33-2b3f-4076-926e-2a4375ac63cb",
      "number": 8,
      "content": "Các nhân chứng bị khinh thường;",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.673Z",
      "updatedAt": "2024-07-15T03:30:35.673Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "7106adc2-141c-453a-ad3e-789fbef2088e",
      "number": 8,
      "content": "Chẳng ai được tôn trọng.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.673Z",
      "updatedAt": "2024-07-15T03:30:35.673Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "59585d03-8586-4656-a0bd-76f8819fa7a2",
      "number": 9,
      "content": "Cả xứ đều nhuộm màu tang tóc thê lương;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.675Z",
      "updatedAt": "2024-07-15T03:30:35.675Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "deb5e4f4-0cfe-46c0-8cbe-25456dab6f7a",
      "number": 9,
      "content": "Li-băng xấu hổ và sầu héo;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.675Z",
      "updatedAt": "2024-07-15T03:30:35.675Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "6d54999a-aae8-4fc7-ad67-491a2b3b4b8f",
      "number": 9,
      "content": "Sa-rôn giống như hoang địa;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.675Z",
      "updatedAt": "2024-07-15T03:30:35.675Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "9e8f1268-a9f8-4f60-a997-cc7d06f9f85c",
      "number": 9,
      "content": "Ba-san và Cạt-mên đều xơ xác tiêu điều.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.675Z",
      "updatedAt": "2024-07-15T03:30:35.675Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "84fcac4e-e10c-43b8-8d44-656f570a34db",
      "number": 10,
      "content": "Chúa phán, “Bây giờ Ta sẽ trỗi dậy;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.678Z",
      "updatedAt": "2024-07-15T03:30:35.678Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "ea6a0177-2819-4684-86a5-4fc36a165bf5",
      "number": 10,
      "content": "Bây giờ Ta sẽ được tôn cao;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.678Z",
      "updatedAt": "2024-07-15T03:30:35.678Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "77b205b3-34c9-4716-8fd3-6dcf69ae734b",
      "number": 10,
      "content": "Bây giờ Ta sẽ làm cho mình được tán dương.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.678Z",
      "updatedAt": "2024-07-15T03:30:35.678Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "46a0445a-e04b-49b7-8983-bb0ce223e901",
      "number": 11,
      "content": "Các ngươi đã mang mầm cỏ dại,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.680Z",
      "updatedAt": "2024-07-15T03:30:35.680Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "2b7a53d5-0c30-440b-b57f-2b14f8823152",
      "number": 11,
      "content": "Nên các ngươi chỉ sinh ra toàn cỏ rác;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.680Z",
      "updatedAt": "2024-07-15T03:30:35.680Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "85c068ea-8998-4a51-b912-77d5ef41ba1e",
      "number": 11,
      "content": "Hơi thở của Ta sẽ như ngọn lửa thiêu rụi các ngươi.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.680Z",
      "updatedAt": "2024-07-15T03:30:35.680Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "e2286fdc-7ca5-4ad4-9f0a-98fb158bb82c",
      "number": 12,
      "content": "Các dân sẽ bị cháy thành vôi;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.682Z",
      "updatedAt": "2024-07-15T03:30:35.682Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "4661eb56-cf26-42e8-9b67-da0d4c418fec",
      "number": 12,
      "content": "Chúng sẽ giống như những nhánh gai bị chặt và bị thiêu trong lửa.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.682Z",
      "updatedAt": "2024-07-15T03:30:35.682Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "eb896ae6-33ed-434e-82fb-b81a4e4f383e",
      "number": 13,
      "content": "Hỡi những kẻ ở xa, hãy nghe cho rõ những việc Ta đã làm;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.684Z",
      "updatedAt": "2024-07-15T03:30:35.684Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "220bba57-b946-4755-9342-1f539fedc23d",
      "number": 13,
      "content": "Hỡi những kẻ ở gần, khá nhận biết sức mạnh của Ta.”",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.684Z",
      "updatedAt": "2024-07-15T03:30:35.684Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "943afe8b-83bc-40f9-a9b8-b797a9fcf429",
      "number": 14,
      "content": "Quân tội lỗi ở Si-ôn khiếp đảm;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.687Z",
      "updatedAt": "2024-07-15T03:30:35.687Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "83424236-8b1a-45a9-9ee3-e95ef0fcd624",
      "number": 14,
      "content": "Phường đạo đức giả run sợ bảo nhau,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.687Z",
      "updatedAt": "2024-07-15T03:30:35.687Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "ea789b39-5b7d-456a-b699-32736aedc055",
      "number": 14,
      "content": "“Ai trong chúng ta có thể sống gần lửa hừng hay thiêu đốt?",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.687Z",
      "updatedAt": "2024-07-15T03:30:35.687Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "729d45ee-2133-4f83-a8b5-7b787425f7e6",
      "number": 14,
      "content": "Ai trong chúng ta có thể sống với ngọn lửa cháy đời đời?”",
      "order": 3,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.687Z",
      "updatedAt": "2024-07-15T03:30:35.687Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "7a5fa159-94dc-421b-9edc-f35c33ca5a32",
      "number": 15,
      "content": "Ấy là người sống ngay lành,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.689Z",
      "updatedAt": "2024-07-15T03:30:35.689Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "6ccec53c-3023-4d5b-bb3a-45f048fca8d5",
      "number": 15,
      "content": "Nói điều chính trực,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.689Z",
      "updatedAt": "2024-07-15T03:30:35.689Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "bd6376aa-c01d-4193-8126-0bd39f691a65",
      "number": 15,
      "content": "Khinh lợi lộc nhờ bức hiếp,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.689Z",
      "updatedAt": "2024-07-15T03:30:35.689Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "25e08d97-00e1-43e0-ba13-b31750f890a2",
      "number": 15,
      "content": "Xua tay từ chối của hối lộ,",
      "order": 3,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.689Z",
      "updatedAt": "2024-07-15T03:30:35.689Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "d4533f29-07f8-43b0-87a6-b954c554f081",
      "number": 15,
      "content": "Quay tai không muốn nghe chuyện làm đổ máu người ngay,",
      "order": 4,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.689Z",
      "updatedAt": "2024-07-15T03:30:35.689Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "ace716f2-c0f4-447c-962e-d8563efed75b",
      "number": 15,
      "content": "Nhắm mắt không muốn thấy việc làm gian ác.",
      "order": 5,
      "parNumber": 6,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.689Z",
      "updatedAt": "2024-07-15T03:30:35.689Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "46632e24-0727-4327-baeb-a78ea3628937",
      "number": 16,
      "content": "Người như thế sẽ được sống ở nơi cao đẹp,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.692Z",
      "updatedAt": "2024-07-15T03:30:35.692Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "e119c22b-10c6-48f3-a987-e871c1323a3b",
      "number": 16,
      "content": "Ðược bảo vệ bằng thành trì xây trên núi đá vững vàng;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.692Z",
      "updatedAt": "2024-07-15T03:30:35.692Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "78335c24-b6c5-4bac-ac43-d95b7be8dd8f",
      "number": 16,
      "content": "Các thức ăn sẽ được cung cấp dư dật,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.692Z",
      "updatedAt": "2024-07-15T03:30:35.692Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "266c6d8a-358e-440f-85bb-e6b125d3640d",
      "number": 16,
      "content": "Còn thức uống lúc nào cũng được tiếp tế đầy tràn.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 13,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.692Z",
      "updatedAt": "2024-07-15T03:30:35.692Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "2c14bbf2-a702-4b7f-981b-9a2650f72665",
      "number": 17,
      "content": "Chính mắt các ngươi sẽ thấy Ðức Vua và sự toàn bích của Người;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.694Z",
      "updatedAt": "2024-07-15T03:30:35.951Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "7727fa1b-6c03-478f-8a69-da58793d4716",
      "number": 17,
      "content": "Người ta sẽ thấy xứ sở của Người mở rộng thật xa.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.694Z",
      "updatedAt": "2024-07-15T03:30:35.961Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "7673efbf-7ce2-48ea-8da2-990f1f0db9ae",
      "number": 18,
      "content": "Lòng các ngươi sẽ hồi tưởng nỗi kinh hoàng ngày trước:",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.696Z",
      "updatedAt": "2024-07-15T03:30:35.971Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "5b62bc1d-b695-443a-95b3-1069f70c447a",
      "number": 18,
      "content": "Thầy ký lục đâu rồi?",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.696Z",
      "updatedAt": "2024-07-15T03:30:35.983Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "fc7d07ee-bcea-4aed-81b1-6e959d901167",
      "number": 18,
      "content": "Người thu thuế đâu rồi?",
      "order": 2,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.696Z",
      "updatedAt": "2024-07-15T03:30:35.992Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "24e52535-afaf-491b-bac7-1d5ad0db9eed",
      "number": 18,
      "content": "Người kiểm kê các pháo tháp đâu rồi?",
      "order": 3,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.696Z",
      "updatedAt": "2024-07-15T03:30:36.004Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "50c1a684-39ab-4f2c-b10b-b9b4caf029b7",
      "number": 19,
      "content": "Các ngươi sẽ không còn thấy một dân hung dữ;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.699Z",
      "updatedAt": "2024-07-15T03:30:36.014Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "4479e2e4-ee60-49be-a5c9-d50591841dbc",
      "number": 19,
      "content": "Một dân có ngôn ngữ lạ, không ai hiểu họ muốn nói gì,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.699Z",
      "updatedAt": "2024-07-15T03:30:36.024Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "f907606f-1698-45fa-ae58-0aba8425eee1",
      "number": 19,
      "content": "Một thứ tiếng líu lo, chẳng ai hiểu được ý nghĩa.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:30:35.699Z",
      "updatedAt": "2024-07-15T03:30:36.038Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "fd96651f-027f-4f4e-be2a-16ebdb1854bb",
      "number": 20,
      "content": "Hãy nhìn lên Si-ôn, thành được chỉ định để tổ chức những ngày lễ của chúng ta;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.701Z",
      "updatedAt": "2024-07-15T03:30:35.701Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "873282cb-55d8-4941-9e18-4f2ad83dc7f3",
      "number": 20,
      "content": "Mắt các ngươi sẽ thấy Giê-ru-sa-lem, một nơi cư trú yên tịnh,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.701Z",
      "updatedAt": "2024-07-15T03:30:35.701Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "c8b6c33c-48e7-4d7c-9c0f-99963546e29c",
      "number": 20,
      "content": "Nơi Lều Tạm sẽ không bị dỡ xuống,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.701Z",
      "updatedAt": "2024-07-15T03:30:35.701Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "f04b1fb7-d811-4858-8dd7-8ce1a63615f1",
      "number": 20,
      "content": "Không một cọc nào của Lều sẽ bị nhổ đi,",
      "order": 3,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.701Z",
      "updatedAt": "2024-07-15T03:30:35.701Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "fd57ee25-cdca-44a5-a090-575b6f3c43c5",
      "number": 20,
      "content": "Cũng không một dây căng nào của Lều sẽ bị đứt.",
      "order": 4,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.701Z",
      "updatedAt": "2024-07-15T03:30:35.701Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "039d951d-13e1-4a9b-8121-28d23c761842",
      "number": 21,
      "content": "Nhưng tại đó uy nghi của Chúa sẽ biểu lộ cho chúng ta,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.704Z",
      "updatedAt": "2024-07-15T03:30:35.704Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "e00b702d-b134-4973-864b-a83a34ef866a",
      "number": 21,
      "content": "Từ nơi ấy phước hạnh Ngài sẽ như các sông lớn và các suối lớn chảy ra;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.704Z",
      "updatedAt": "2024-07-15T03:30:35.704Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "8a9857c2-a265-430d-b6bd-8e18f0b9a51f",
      "number": 21,
      "content": "Dĩ nhiên nơi ấy sẽ không có thuyền bè qua lại,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.704Z",
      "updatedAt": "2024-07-15T03:30:35.704Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "8cec98eb-ed71-4503-95cf-e62c4311a44f",
      "number": 21,
      "content": "Và các tàu to đẹp không thể đi qua.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.704Z",
      "updatedAt": "2024-07-15T03:30:35.704Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "10806324-6df9-4086-8e2f-c5600978622d",
      "number": 22,
      "content": "Vì Chúa là Ðấng Phán Xét của chúng ta;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.706Z",
      "updatedAt": "2024-07-15T03:30:35.706Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "b3958397-03fd-41ee-892f-00cc9a618b05",
      "number": 22,
      "content": "Chúa là Nhà Lập Pháp của chúng ta;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.706Z",
      "updatedAt": "2024-07-15T03:30:35.706Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "73a47e8e-b2d9-4bf3-8102-c5ab3d98fe98",
      "number": 22,
      "content": "Chúa là Vua chúng ta;",
      "order": 2,
      "parNumber": 8,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.706Z",
      "updatedAt": "2024-07-15T03:30:35.706Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "1768acf7-d4bc-4ec7-b583-b3372b410097",
      "number": 22,
      "content": "Chính Ngài sẽ cứu chúng ta.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.706Z",
      "updatedAt": "2024-07-15T03:30:35.706Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "5e7b706e-654b-46d8-b797-d1b06d33988c",
      "number": 23,
      "content": "Các dây thừng của các ngươi đã lỏng;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 13,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.709Z",
      "updatedAt": "2024-07-15T03:30:35.709Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "44139545-fef3-4f9a-acf9-69a4938972b7",
      "number": 23,
      "content": "Chúng không thể giữ cho chân cột buồm đứng vững;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 14,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.709Z",
      "updatedAt": "2024-07-15T03:30:35.709Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "100b8d4d-e89f-4834-a627-cde76c259c2b",
      "number": 23,
      "content": "Chúng không thể giương cánh buồm lên.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 15,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.709Z",
      "updatedAt": "2024-07-15T03:30:35.709Z",
      "chapterId": "dccf3297-9f1c-4739-b4a7-17d5c2292ad6"
    },
    {
      "id": "c8902086-9889-4e24-b128-be6fece8f1cf",
      "number": 23,
      "content": "Bấy giờ người ta sẽ chia nhau vô vàn chiến lợi phẩm,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:30:35.709Z",
      "updatedAt": "2024-07-15T03:30:35.709Z",
