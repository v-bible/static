{
  "chapter": {
    "id": "06b79f5e-c8a9-4624-b06e-7420552ec43c",
    "number": 40,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2040&version=BD2011",
    "createdAt": "2024-07-12T04:03:50.633Z",
    "updatedAt": "2024-07-24T15:07:51.295Z",
    "bookId": "e78c99ae-11b2-4687-bcf7-b33bef999af4"
  },
  "verses": [
    {
      "id": "1e01bc76-3a43-4250-8fe5-6dbd57d128bf",
      "number": 1,
      "content": "Ðức Chúa Trời của các ngươi phán rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:31:54.103Z",
      "updatedAt": "2024-07-15T03:31:54.344Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "345af9b8-7af0-4974-a4e8-e8581c7c2346",
      "number": 1,
      "content": "“Hãy an ủi, hãy an ủi dân Ta.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:31:54.103Z",
      "updatedAt": "2024-07-15T03:31:54.371Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "6a023403-f1cd-4f94-9033-0dac65d32064",
      "number": 2,
      "content": "Hãy nói với Giê-ru-sa-lem cách trìu mến,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:31:54.114Z",
      "updatedAt": "2024-07-15T03:31:54.396Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "f8e9d7bf-030e-4452-9008-3f5b7e7f331a",
      "number": 2,
      "content": "Và rao báo cho nó biết rằng,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:31:54.114Z",
      "updatedAt": "2024-07-15T03:31:54.422Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "c74b77e0-b33c-4d8b-a8e4-62242a7b4073",
      "number": 2,
      "content": "Nó đã chịu hình phạt đủ rồi;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:31:54.114Z",
      "updatedAt": "2024-07-15T03:31:54.447Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "9e6a0b0d-585e-416e-be83-5ffc2f64964e",
      "number": 2,
      "content": "Nó đã đền tội xong rồi;",
      "order": 3,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:31:54.114Z",
      "updatedAt": "2024-07-15T03:31:54.472Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "3a72be5b-6bde-4911-8460-cf51bef19ce9",
      "number": 2,
      "content": "Nó đã nhận gấp đôi từ tay Chúa vì những tội của nó.”",
      "order": 4,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:31:54.114Z",
      "updatedAt": "2024-07-15T03:31:54.497Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "5117241b-87c2-46cc-a4c6-4ea689887329",
      "number": 3,
      "content": "Có tiếng kêu lên rằng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.124Z",
      "updatedAt": "2024-07-15T03:31:54.124Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "190b7d4f-1e56-401a-8512-9b14dde99e35",
      "number": 3,
      "content": "“Hãy mở cho Chúa một con đường trong đồng hoang;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.124Z",
      "updatedAt": "2024-07-15T03:31:54.124Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "694aceac-0a21-48ae-b50b-90144925e41a",
      "number": 3,
      "content": "Hãy làm một đại lộ thẳng bằng cho Ðức Chúa Trời chúng ta trong sa mạc.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.124Z",
      "updatedAt": "2024-07-15T03:31:54.124Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "cecf1e1e-7e18-4787-84b5-7f029bfa643d",
      "number": 4,
      "content": "Mọi thung lũng phải lấp cho đầy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.131Z",
      "updatedAt": "2024-07-15T03:31:54.131Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "556fcce7-d7db-4460-a35b-236ce1067205",
      "number": 4,
      "content": "Mọi núi đồi phải san cho phẳng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.131Z",
      "updatedAt": "2024-07-15T03:31:54.131Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "ff805168-2530-4086-9615-a6461e7abde2",
      "number": 4,
      "content": "Mọi gò nổng phải ủi cho bằng;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.131Z",
      "updatedAt": "2024-07-15T03:31:54.131Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "97bf6d55-8b85-45d3-8182-65ea1e1b1b86",
      "number": 4,
      "content": "Những nơi gồ ghề phải làm cho bằng phẳng,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.131Z",
      "updatedAt": "2024-07-15T03:31:54.131Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "f4868c7a-2390-47fb-a421-e664f07e4ec3",
      "number": 5,
      "content": "Bấy giờ vinh quang của Chúa sẽ hiển lộ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.137Z",
      "updatedAt": "2024-07-15T03:31:54.137Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "a0e2344b-a12d-406d-8632-9db44faff3dc",
      "number": 5,
      "content": "Và mọi loài xác thịt sẽ cùng nhau xem thấy,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.137Z",
      "updatedAt": "2024-07-15T03:31:54.137Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "f3eb3427-09d2-4b0f-9a8e-11a609165429",
      "number": 5,
      "content": "Vì miệng Chúa đã phán vậy.”",
      "order": 2,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.137Z",
      "updatedAt": "2024-07-15T03:31:54.137Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "0db7acc0-8320-49ab-87a7-b4c08e7923ab",
      "number": 6,
      "content": "Có tiếng phán rằng, “Ngươi hãy kêu lên!”",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.143Z",
      "updatedAt": "2024-07-15T03:31:54.143Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "8fe5ed07-6a7f-4314-99d7-58f7756f0e84",
      "number": 6,
      "content": "Tôi thưa rằng, “Con phải nói gì?”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.143Z",
      "updatedAt": "2024-07-15T03:31:54.143Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "80e9dc1d-4d89-4d63-8c06-aa91dc9a035a",
      "number": 6,
      "content": "Mọi loài xác thịt như cỏ dại,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.143Z",
      "updatedAt": "2024-07-15T03:31:54.143Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "da1144ef-c258-4170-8353-8a1826a00b10",
      "number": 6,
      "content": "Vẻ đẹp của chúng như hoa ngoài đồng.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.143Z",
      "updatedAt": "2024-07-15T03:31:54.143Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "824fa2ed-c272-43bf-8a0c-092a09ac20ea",
      "number": 7,
      "content": "Cỏ héo, hoa tàn,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.149Z",
      "updatedAt": "2024-07-15T03:31:54.149Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "d1c50645-c1d8-4ed2-bb10-7b5a34c76211",
      "number": 7,
      "content": "Vì hơi thở của Chúa thổi qua trên chúng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.149Z",
      "updatedAt": "2024-07-15T03:31:54.149Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "3557a932-42e1-4599-a0a2-69cf349cfb02",
      "number": 7,
      "content": "Thật vậy loài người chẳng khác gì cỏ dại.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.149Z",
      "updatedAt": "2024-07-15T03:31:54.149Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "210e5afe-8e0e-4e80-b740-16a2fa82ce9a",
      "number": 8,
      "content": "Cỏ héo, hoa tàn,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.155Z",
      "updatedAt": "2024-07-15T03:31:54.155Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "7ae47c02-a8d4-4521-8854-9ea3f07c8389",
      "number": 8,
      "content": "Nhưng lời của Ðức Chúa Trời chúng ta đứng vững đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.155Z",
      "updatedAt": "2024-07-15T03:31:54.155Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "aaf918cd-99e3-44e3-8db4-536c6c1a256d",
      "number": 9,
      "content": "Hỡi các ngươi, những người đem tin mừng đến Si-ôn,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.161Z",
      "updatedAt": "2024-07-15T03:31:54.161Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "def3775e-378e-4687-b604-d820eaa590a2",
      "number": 9,
      "content": "Hãy lên núi cao.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.161Z",
      "updatedAt": "2024-07-15T03:31:54.161Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "1f8379fb-a9a8-4e9c-8f3e-136f3acc9b36",
      "number": 9,
      "content": "Hỡi các ngươi, những người đem tin mừng đến Giê-ru-sa-lem,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.161Z",
      "updatedAt": "2024-07-15T03:31:54.161Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "67fa6152-8ab3-4a96-b09d-686155092eaf",
      "number": 9,
      "content": "Hãy hết sức nói lớn tiếng lên,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.161Z",
      "updatedAt": "2024-07-15T03:31:54.161Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "75baa0e1-f938-4c84-8a29-b1941008c4f0",
      "number": 9,
      "content": "Hãy cất tiếng lên, đừng sợ hãi,",
      "order": 4,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.161Z",
      "updatedAt": "2024-07-15T03:31:54.161Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "4421c778-2840-4632-8a9d-ee598d12d3a7",
      "number": 9,
      "content": "Hãy nói cho các thành của Giu-đa biết rằng, “Ðây là Ðức Chúa Trời của các ngươi.”",
      "order": 5,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.161Z",
      "updatedAt": "2024-07-15T03:31:54.161Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "71c79794-e7f8-41b7-9458-430135f6ef12",
      "number": 10,
      "content": "Này, Chúa Hằng Hữu ngự đến với uy quyền;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.168Z",
      "updatedAt": "2024-07-15T03:31:54.168Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "3921ea9b-7af5-4fc9-8255-704aff2aa913",
      "number": 10,
      "content": "Ngài sẽ trị vì bằng cánh tay Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.168Z",
      "updatedAt": "2024-07-15T03:31:54.168Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "821fa961-8ea8-46bc-bcf6-f5de20e7523f",
      "number": 10,
      "content": "Này, Ngài đem phần thưởng theo với Ngài;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.168Z",
      "updatedAt": "2024-07-15T03:31:54.168Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "3f73c7a1-b922-4699-b253-320e5bc604a8",
      "number": 10,
      "content": "Các vật tưởng thưởng của Ngài đi trước Ngài.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.168Z",
      "updatedAt": "2024-07-15T03:31:54.168Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "1fcfc88c-1723-475f-b7a8-56bf03cd3622",
      "number": 11,
      "content": "Ngài sẽ chăn giữ bầy chiên của Ngài như một người chăn;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.174Z",
      "updatedAt": "2024-07-15T03:31:54.174Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "364de7f4-38be-461f-a82f-1f02c940e6a3",
      "number": 11,
      "content": "Ngài sẽ bồng ẵm các chiên con trong tay Ngài;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.174Z",
      "updatedAt": "2024-07-15T03:31:54.174Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "76e335ed-eea2-4ad0-8bbc-612dce326d19",
      "number": 11,
      "content": "Ngài sẽ ôm chúng vào lòng Ngài,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.174Z",
      "updatedAt": "2024-07-15T03:31:54.174Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "660a4774-29e0-4d5f-b33b-8aff8f5c330a",
      "number": 11,
      "content": "Và Ngài sẽ chăm sóc các chiên mẹ có con còn thơ dại.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.174Z",
      "updatedAt": "2024-07-15T03:31:54.174Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "68624b25-8650-42e9-89d1-aebd5f8a5dbe",
      "number": 12,
      "content": "Ai đã dùng lòng bàn tay lường nước biển,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.180Z",
      "updatedAt": "2024-07-15T03:31:54.180Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "c592eadb-a7fb-4c5a-9ebd-5baa8ae9bec3",
      "number": 12,
      "content": "Dùng gang tay đo các tầng trời,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.180Z",
      "updatedAt": "2024-07-15T03:31:54.180Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "dfe6117c-6b85-4352-90b3-ab6af82ac4f7",
      "number": 12,
      "content": "Lấy cái đấu đong bụi đất?",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.180Z",
      "updatedAt": "2024-07-15T03:31:54.180Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "0a14f858-29b3-4bc7-b996-dc99c2c69c7b",
      "number": 12,
      "content": "Ai lấy bàn cân cân núi lớn,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.180Z",
      "updatedAt": "2024-07-15T03:31:54.180Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "1c639aad-0236-4929-b0c3-20130d95dde9",
      "number": 12,
      "content": "Hoặc dùng dĩa cân cân các đồi cao?",
      "order": 4,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.180Z",
      "updatedAt": "2024-07-15T03:31:54.180Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "72688b74-e2bd-452f-854f-4b5eee1f568b",
      "number": 13,
      "content": "Ai có thể hướng dẫn được Thần của Chúa,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.187Z",
      "updatedAt": "2024-07-15T03:31:54.187Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "c0e2ee7f-407b-4a6f-9284-35d67fff7f82",
      "number": 13,
      "content": "Hoặc làm quân sư để chỉ dạy Ngài?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.187Z",
      "updatedAt": "2024-07-15T03:31:54.187Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "14b76bca-70b0-407c-a7b7-64924582eebf",
      "number": 14,
      "content": "Ngài đã tham khảo mưu kế của ai?",
      "order": 0,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.192Z",
      "updatedAt": "2024-07-15T03:31:54.192Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "2c2037ed-eeba-4e4b-b66d-145207842c8b",
      "number": 14,
      "content": "Ai đã chỉ dạy Ngài?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.192Z",
      "updatedAt": "2024-07-15T03:31:54.192Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "f282e383-d237-46f5-aa1c-c5864f164248",
      "number": 14,
      "content": "Ai đã dạy Ngài con đường công lý?",
      "order": 2,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.192Z",
      "updatedAt": "2024-07-15T03:31:54.192Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "7f377af5-de52-4110-8952-ea7b9c8d8155",
      "number": 14,
      "content": "Ai đã dạy Ngài tri thức vô biên?",
      "order": 3,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.192Z",
      "updatedAt": "2024-07-15T03:31:54.192Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "0dacb20c-f34c-4840-b456-10e9b4e660a0",
      "number": 14,
      "content": "Ai đã chỉ cho Ngài đường nào để được trí tuệ?",
      "order": 4,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.192Z",
      "updatedAt": "2024-07-15T03:31:54.192Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "7f83e8bc-4d40-4ffb-b279-fb2357feb719",
      "number": 15,
      "content": "Này, các quốc gia khác nào giọt nước trong thùng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.198Z",
      "updatedAt": "2024-07-15T03:31:54.198Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "a6b9e6f8-cddd-4db8-9348-cb6f59074b7c",
      "number": 15,
      "content": "Họ bị xem như chút bụi dính nơi dĩa cân;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.198Z",
      "updatedAt": "2024-07-15T03:31:54.198Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "b826f579-59ea-4ce9-9efc-d8f4472d586f",
      "number": 15,
      "content": "Này, Ngài nhấc bổng các đảo như một vật bé nhỏ.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.198Z",
      "updatedAt": "2024-07-15T03:31:54.198Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "150da6e9-0e7c-4ec2-a604-a555edb32da8",
      "number": 16,
      "content": "Gỗ của núi Li-băng không đủ đốt dâng của lễ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.204Z",
      "updatedAt": "2024-07-15T03:31:54.204Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "0bbadcda-db39-41e6-884e-f576e9856855",
      "number": 16,
      "content": "Các thú trên núi ấy cũng không đủ làm của lễ thiêu.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.204Z",
      "updatedAt": "2024-07-15T03:31:54.204Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "c329dd37-c0a7-46b8-8bb9-25ec340d2a4d",
      "number": 17,
      "content": "Tất cả các quốc gia chẳng có nghĩa lý gì trước mặt Ngài;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.209Z",
      "updatedAt": "2024-07-15T03:31:54.209Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "d5a6c522-d349-4efa-aee1-118f72b1254c",
      "number": 17,
      "content": "Ðối với Ngài, mọi quốc gia trên thế giới chẳng là gì cả và chỉ là hư vô.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.209Z",
      "updatedAt": "2024-07-15T03:31:54.209Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "043c00a2-9c10-4d88-b74a-9aa0654d33bd",
      "number": 18,
      "content": "Thế thì các ngươi ví sánh Ðức Chúa Trời với ai?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.215Z",
      "updatedAt": "2024-07-15T03:31:54.215Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "91a3e787-39bb-46a7-a562-52264f794eda",
      "number": 18,
      "content": "Hay lấy hình ảnh nào để so sánh với Ngài được?",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.215Z",
      "updatedAt": "2024-07-15T03:31:54.215Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "7e04525c-deee-4ac9-9a29-26ebc9fba440",
      "number": 19,
      "content": "Phải chăng một pho tượng do thợ đúc tượng đúc ra?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.220Z",
      "updatedAt": "2024-07-15T03:31:54.220Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "c7029ed9-5293-4229-8c56-b5f9cd5e6ab0",
      "number": 19,
      "content": "Rồi thợ dát vàng thếp một lớp vàng mỏng lên trên,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.220Z",
      "updatedAt": "2024-07-15T03:31:54.220Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "7b6f91d3-d9bd-45ea-be10-8c134b5563c5",
      "number": 19,
      "content": "Và người thợ bạc đúc thêm một dây chuyền bạc.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.220Z",
      "updatedAt": "2024-07-15T03:31:54.220Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "41bfa022-dcc5-4b73-8daa-10c8e506b004",
      "number": 20,
      "content": "Người nghèo không đủ khả năng làm tượng như thế để thờ thì chọn một khúc gỗ chưa mục,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.226Z",
      "updatedAt": "2024-07-15T03:31:54.226Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "2caf87f3-0869-498b-b303-a52eb6ceb1cc",
      "number": 20,
      "content": "Rồi tìm một người thợ mộc khéo tay,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.226Z",
      "updatedAt": "2024-07-15T03:31:54.226Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "477a9adb-89e3-4587-9dde-7593aaeae4dd",
      "number": 20,
      "content": "Mướn thợ ấy đục đẽo thành một tượng gỗ không bị ngã.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.226Z",
      "updatedAt": "2024-07-15T03:31:54.226Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "633fc97a-fe36-4fb7-bcf1-92c88dd70a3c",
      "number": 21,
      "content": "Các ngươi chưa biết sao?",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.231Z",
      "updatedAt": "2024-07-15T03:31:54.231Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "3aa81882-d93c-4080-a3a6-a51b5d98f9db",
      "number": 21,
      "content": "Các ngươi chưa nghe sao?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.231Z",
      "updatedAt": "2024-07-15T03:31:54.231Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "9fea304b-9f43-4604-9c4d-a39813789e3f",
      "number": 21,
      "content": "Từ lúc ban đầu các ngươi chưa được bảo cho biết sao?",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.231Z",
      "updatedAt": "2024-07-15T03:31:54.231Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "55635a85-8b5f-4d8e-974d-251aadfa20ec",
      "number": 21,
      "content": "Từ lúc lập nền trái đất các ngươi há chưa hiểu sao?",
      "order": 3,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.231Z",
      "updatedAt": "2024-07-15T03:31:54.231Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "03ac2ffc-90dd-43fb-9c1b-25902a573319",
      "number": 22,
      "content": "Ðấng ngự trên vòm trái đất,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.238Z",
      "updatedAt": "2024-07-15T03:31:54.238Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "aa6fa265-0c50-4d60-8e5f-cb5ba23d2b64",
      "number": 22,
      "content": "Nhìn xuống nhân gian như châu chấu cào cào;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.238Z",
      "updatedAt": "2024-07-15T03:31:54.238Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "914899ba-11ec-4b36-85ea-f61b22cd618f",
      "number": 22,
      "content": "Ngài giăng bầu trời ra như giăng một tấm màn;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.238Z",
      "updatedAt": "2024-07-15T03:31:54.238Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "6976a1d3-0625-48f4-9ad1-df44a08a63e1",
      "number": 22,
      "content": "Ngài căng chúng ra như người ta căng lều để ở.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.238Z",
      "updatedAt": "2024-07-15T03:31:54.238Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "503e543a-c526-420f-bbb1-08e68239d592",
      "number": 23,
      "content": "Ngài biến những kẻ quyền thế ra hư không;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.243Z",
      "updatedAt": "2024-07-15T03:31:54.243Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "1f1eb18f-6bff-4142-81b6-5ac5da71c71e",
      "number": 23,
      "content": "Những kẻ cầm quyền xét đoán trên đất trở thành hư ảo.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.243Z",
      "updatedAt": "2024-07-15T03:31:54.243Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "04580761-170d-4d8d-bd9b-b3e4cec34efe",
      "number": 24,
      "content": "Chúng vừa mới được trồng;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.249Z",
      "updatedAt": "2024-07-15T03:31:54.249Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "031f705a-325b-4eb5-b92a-3074e9d0a933",
      "number": 24,
      "content": "Chúng vừa mới được gieo;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.249Z",
      "updatedAt": "2024-07-15T03:31:54.249Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "202176e5-3eb6-432f-85c4-c5d97081deac",
      "number": 24,
      "content": "Chúng vừa đâm rễ vào đất;",
      "order": 2,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.249Z",
      "updatedAt": "2024-07-15T03:31:54.249Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "f2846e95-56c6-44ff-9eda-45a3b82e8ef5",
      "number": 24,
      "content": "Ngài thổi qua chúng một hơi,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.249Z",
      "updatedAt": "2024-07-15T03:31:54.249Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "154d0a43-5610-4e92-a2c7-d9c4fabca15c",
      "number": 24,
      "content": "Chúng liền héo khô ngay,",
      "order": 4,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.249Z",
      "updatedAt": "2024-07-15T03:31:54.249Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "288ebebe-4831-45c3-b280-88d2a05cb86c",
      "number": 24,
      "content": "Và một cơn lốc đùa chúng đi như gốc rạ.",
      "order": 5,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.249Z",
      "updatedAt": "2024-07-15T03:31:54.249Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "a923cd5b-98d5-416b-853f-469dc7bb1ca5",
      "number": 25,
      "content": "Ðấng Thánh phán hỏi:",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.257Z",
      "updatedAt": "2024-07-15T03:31:54.257Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "e3ba62f7-3275-4984-9da6-ab0ffba6a0b5",
      "number": 25,
      "content": "Các ngươi ví sánh Ta với ai?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.257Z",
      "updatedAt": "2024-07-15T03:31:54.257Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "551cd748-87eb-4d7b-8396-2e68f226274b",
      "number": 25,
      "content": "Các ngươi cho Ta bằng ai?",
      "order": 2,
      "parNumber": 11,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.257Z",
      "updatedAt": "2024-07-15T03:31:54.257Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "50bdc241-e01c-4692-9962-014e1974526c",
      "number": 26,
      "content": "Hãy ngước mắt nhìn lên trời,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.263Z",
      "updatedAt": "2024-07-15T03:31:54.263Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "c9e371f6-c91d-486b-95f7-fc17eb788514",
      "number": 26,
      "content": "Xem ai đã tạo nên những thiên thể ấy?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.263Z",
      "updatedAt": "2024-07-15T03:31:54.263Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "db993ca5-cc14-48e0-b9ff-4411d73d90e2",
      "number": 26,
      "content": "Ai đã làm chúng hiện hữu vô sốnhư vậy?",
      "order": 2,
      "parNumber": 11,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.263Z",
      "updatedAt": "2024-07-15T03:31:54.263Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "12a948a0-f2d2-4a3c-8ba4-e65b2ea5c709",
      "number": 26,
      "content": "Thế mà Ngài gọi chúng theo từng tên riêng.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.263Z",
      "updatedAt": "2024-07-15T03:31:54.263Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "9544ccd8-961f-43f4-ab7c-3e9d47222ff0",
      "number": 26,
      "content": "Nhờ quyền năng lớn lao của Ngài,",
      "order": 4,
      "parNumber": 11,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.263Z",
      "updatedAt": "2024-07-15T03:31:54.263Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "6dbe5420-a138-463c-8186-f66166893726",
      "number": 26,
      "content": "Nhờ sức mạnh vô hạn của Ngài,",
      "order": 5,
      "parNumber": 11,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.263Z",
      "updatedAt": "2024-07-15T03:31:54.263Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "ee6b4ffc-0002-4f06-a9fc-ccb5c5b0e81e",
      "number": 26,
      "content": "Không một thiên thể nào bị bỏ sót.",
      "order": 6,
      "parNumber": 11,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.263Z",
      "updatedAt": "2024-07-15T03:31:54.263Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "35b33ac0-78e2-4825-a783-a79be42aaa95",
      "number": 27,
      "content": "Hỡi Gia-cốp, tại sao ngươi nói,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.270Z",
      "updatedAt": "2024-07-15T03:31:54.270Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "d12b48f4-21c1-4cd3-adfc-039afc907611",
      "number": 27,
      "content": "Hỡi I-sơ-ra-ên, tại sao ngươi bảo:",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.270Z",
      "updatedAt": "2024-07-15T03:31:54.270Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "f72bc1d3-6ac8-464e-a6d3-0a7227c7f540",
      "number": 27,
      "content": "“Ðường lối tôi đã bị che khuất khỏi mắt Chúa,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.270Z",
      "updatedAt": "2024-07-15T03:31:54.270Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "ffc5d06c-4d35-4da8-a4fc-e27f9ac3f06d",
      "number": 27,
      "content": "Công lý dành cho tôi Ðức Chúa Trời đã chẳng đoái hoài.”",
      "order": 3,
      "parNumber": 12,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.270Z",
      "updatedAt": "2024-07-15T03:31:54.270Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "a0e199d7-9001-4508-83e5-bb20c9546569",
      "number": 28,
      "content": "Các ngươi chưa biết sao?",
      "order": 0,
      "parNumber": 12,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.276Z",
      "updatedAt": "2024-07-15T03:31:54.276Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "ddf45924-fd0f-4662-9207-57abfe1ac856",
      "number": 28,
      "content": "Các ngươi chưa nghe sao?",
      "order": 1,
      "parNumber": 12,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.276Z",
      "updatedAt": "2024-07-15T03:31:54.276Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "dd1babf3-5c5c-4c7e-a005-9ef0e1e80676",
      "number": 28,
      "content": "Chúa là Ðức Chúa Trời đời đời,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.276Z",
      "updatedAt": "2024-07-15T03:31:54.276Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "a1d8783f-1928-48cb-9c29-be3977b7c260",
      "number": 28,
      "content": "Ðấng Tạo Hóa của toàn cả địa cầu;",
      "order": 3,
      "parNumber": 12,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.276Z",
      "updatedAt": "2024-07-15T03:31:54.276Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "26a4d308-c18d-45d8-95ac-47a64d808920",
      "number": 28,
      "content": "Ngài không kiệt lực và không mệt mỏi;",
      "order": 4,
      "parNumber": 12,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.276Z",
      "updatedAt": "2024-07-15T03:31:54.276Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "acf9ef72-af84-46bd-a94d-e5c06766e3c9",
      "number": 28,
      "content": "Trí tuệ Ngài thật khôn dò.",
      "order": 5,
      "parNumber": 12,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.276Z",
      "updatedAt": "2024-07-15T03:31:54.276Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "81e2e5e0-0623-4808-94c2-30548c5fcc76",
      "number": 29,
      "content": "Ngài ban năng lực cho người kiệt lực;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.285Z",
      "updatedAt": "2024-07-15T03:31:54.285Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "256287e9-e46c-48fd-8e89-9197dccd85b1",
      "number": 29,
      "content": "Ngài ban sức mạnh cho người đuối sức.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.285Z",
      "updatedAt": "2024-07-15T03:31:54.285Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "7834ca6c-abb8-4b53-b80b-f7c7a0ea777f",
      "number": 30,
      "content": "Ngay cả những người trẻ tuổi cũng sẽ kiệt lực và mệt mỏi;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.291Z",
      "updatedAt": "2024-07-15T03:31:54.291Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "da35419e-3b9c-4c47-bfac-c4d7e246eafc",
      "number": 30,
      "content": "Các thanh niên cũng sẽ đuối sức ngã dài;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 13,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.291Z",
      "updatedAt": "2024-07-15T03:31:54.291Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "40bcd30c-dc73-4a8d-ad2b-eadf814ef0b7",
      "number": 31,
      "content": "Nhưng ai trông cậy Chúa sẽ được sức mới;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 14,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.296Z",
      "updatedAt": "2024-07-15T03:31:54.296Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "f6723852-cb20-43ea-8dcd-adc37d3584b2",
      "number": 31,
      "content": "Họ sẽ cất cánh bay cao như phượng hoàng;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 15,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.296Z",
      "updatedAt": "2024-07-15T03:31:54.296Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "dc5f11d7-6b0a-4f86-8734-482ad1154a61",
      "number": 31,
      "content": "Họ sẽ chạy mà không mệt mỏi;",
      "order": 2,
      "parNumber": 12,
      "parIndex": 16,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.296Z",
      "updatedAt": "2024-07-15T03:31:54.296Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    },
    {
      "id": "35922b66-7c62-49fd-9741-83339b40ed07",
      "number": 31,
      "content": "Họ sẽ đi mà không kiệt lực.",
      "order": 3,
      "parNumber": 12,
      "parIndex": 17,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:31:54.296Z",
      "updatedAt": "2024-07-15T03:31:54.296Z",
      "chapterId": "06b79f5e-c8a9-4624-b06e-7420552ec43c"
    }
  ],
