{
  "chapter": {
    "id": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f",
    "number": 40,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2040&version=BD2011",
    "createdAt": "2024-09-24T01:00:47.153Z",
    "updatedAt": "2024-09-24T01:00:47.153Z",
    "bookId": "1e0ea116-0110-44af-8fdc-87130c3208f3"
  },
  "verses": [
    {
      "id": "5d76a81a-a485-4e40-b3f5-8802b70d8e4f",
      "number": 1,
      "content": "Ðức Chúa Trời của các ngươi phán rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.091Z",
      "updatedAt": "2024-09-24T02:23:38.091Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "e71919a8-2c4b-4e97-b0bb-34241dd7dd38",
      "number": 1,
      "content": "“Hãy an ủi, hãy an ủi dân Ta.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.103Z",
      "updatedAt": "2024-09-24T02:23:38.103Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "507aca12-8bfa-4083-8ffb-dc5abaa26317",
      "number": 2,
      "content": "Hãy nói với Giê-ru-sa-lem cách trìu mến,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.105Z",
      "updatedAt": "2024-09-24T02:23:38.105Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "198ebaed-2586-46a6-a3f1-ccd6bde28b95",
      "number": 2,
      "content": "Và rao báo cho nó biết rằng,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.107Z",
      "updatedAt": "2024-09-24T02:23:38.107Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "7496f896-e19b-4660-a2a3-c486a6ed073b",
      "number": 2,
      "content": "Nó đã chịu hình phạt đủ rồi;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.111Z",
      "updatedAt": "2024-09-24T02:23:38.111Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "52bebaae-0548-4edf-bc4d-810b3d976383",
      "number": 2,
      "content": "Nó đã đền tội xong rồi;",
      "order": 3,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.116Z",
      "updatedAt": "2024-09-24T02:23:38.116Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "97191128-b7ce-4539-8a94-ba2e22fdcfe3",
      "number": 2,
      "content": "Nó đã nhận gấp đôi từ tay Chúa vì những tội của nó.”",
      "order": 4,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.121Z",
      "updatedAt": "2024-09-24T02:23:38.121Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "e936811f-d2ba-4225-ba38-dae1f0b15368",
      "number": 3,
      "content": "Có tiếng kêu lên rằng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.125Z",
      "updatedAt": "2024-09-24T02:23:38.125Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "c1fb01b7-ab51-44dd-bb9c-eeb37be54033",
      "number": 3,
      "content": "“Hãy mở cho Chúa một con đường trong đồng hoang;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.131Z",
      "updatedAt": "2024-09-24T02:23:38.131Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "f6b7261d-9d61-4c17-8551-7b40d4bac7f5",
      "number": 3,
      "content": "Hãy làm một đại lộ thẳng bằng cho Ðức Chúa Trời chúng ta trong sa mạc.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.135Z",
      "updatedAt": "2024-09-24T02:23:38.135Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "af05f34d-0ade-40d9-94be-2b3a5fc644d5",
      "number": 4,
      "content": "Mọi thung lũng phải lấp cho đầy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.140Z",
      "updatedAt": "2024-09-24T02:23:38.140Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "b1b479d3-0c77-4b47-ba30-3c8bc9cc89cd",
      "number": 4,
      "content": "Mọi núi đồi phải san cho phẳng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.145Z",
      "updatedAt": "2024-09-24T02:23:38.145Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "08ddb977-1032-4801-b975-983ddc2552dc",
      "number": 4,
      "content": "Mọi gò nổng phải ủi cho bằng;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.150Z",
      "updatedAt": "2024-09-24T02:23:38.150Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "3e23b10e-f939-4c60-9178-8f9e66798319",
      "number": 4,
      "content": "Những nơi gồ ghề phải làm cho bằng phẳng,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.155Z",
      "updatedAt": "2024-09-24T02:23:38.155Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "37449bec-e0ac-4dee-afe4-536b185b145a",
      "number": 5,
      "content": "Bấy giờ vinh quang của Chúa sẽ hiển lộ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.160Z",
      "updatedAt": "2024-09-24T02:23:38.160Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "dd6643b7-0961-4473-ac20-3d2ef6167251",
      "number": 5,
      "content": "Và mọi loài xác thịt sẽ cùng nhau xem thấy,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.165Z",
      "updatedAt": "2024-09-24T02:23:38.165Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "f09e7cd0-6f95-419b-aff9-6675c41313c8",
      "number": 5,
      "content": "Vì miệng Chúa đã phán vậy.”",
      "order": 2,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.171Z",
      "updatedAt": "2024-09-24T02:23:38.171Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "e3ef06c5-7fb5-4ea7-a957-6f3c3645c922",
      "number": 6,
      "content": "Có tiếng phán rằng, “Ngươi hãy kêu lên!”",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.176Z",
      "updatedAt": "2024-09-24T02:23:38.176Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "9b770f2b-d721-498f-a15b-e9d4539217f0",
      "number": 6,
      "content": "Tôi thưa rằng, “Con phải nói gì?”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.181Z",
      "updatedAt": "2024-09-24T02:23:38.181Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "4ad44fbc-5cdd-4ba2-aaca-9a15546de2e0",
      "number": 6,
      "content": "Mọi loài xác thịt như cỏ dại,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.186Z",
      "updatedAt": "2024-09-24T02:23:38.186Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "30b13ab5-2734-4668-855d-8e31363e86d2",
      "number": 6,
      "content": "Vẻ đẹp của chúng như hoa ngoài đồng.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.190Z",
      "updatedAt": "2024-09-24T02:23:38.190Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "afa94e91-4e23-49f9-bd6b-9b6081e2ff9e",
      "number": 7,
      "content": "Cỏ héo, hoa tàn,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.193Z",
      "updatedAt": "2024-09-24T02:23:38.193Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "1f82490b-4797-4a28-bb85-d63c95c839e2",
      "number": 8,
      "content": "Cỏ héo, hoa tàn,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.202Z",
      "updatedAt": "2024-09-24T02:23:38.202Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "351de3fe-333f-416b-819a-276ab0b22f10",
      "number": 7,
      "content": "Vì hơi thở của Chúa thổi qua trên chúng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.196Z",
      "updatedAt": "2024-09-24T02:23:38.196Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "f87ce9ce-d7b5-48be-88ea-e7cd8920ad22",
      "number": 7,
      "content": "Thật vậy loài người chẳng khác gì cỏ dại.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.199Z",
      "updatedAt": "2024-09-24T02:23:38.199Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "c25eee58-1a37-4149-85bf-d5f5bec3352a",
      "number": 8,
      "content": "Nhưng lời của Ðức Chúa Trời chúng ta đứng vững đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.205Z",
      "updatedAt": "2024-09-24T02:23:38.205Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "1e99461c-c0e1-4fb8-b931-9f551c907d04",
      "number": 9,
      "content": "Hỡi các ngươi, những người đem tin mừng đến Si-ôn,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.207Z",
      "updatedAt": "2024-09-24T02:23:38.207Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "0ef294ee-4b39-49c2-9813-04ae1fd94adb",
      "number": 9,
      "content": "Hãy lên núi cao.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.209Z",
      "updatedAt": "2024-09-24T02:23:38.209Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "559e4509-5a94-41bc-8c3f-fac4f7fc9a51",
      "number": 9,
      "content": "Hỡi các ngươi, những người đem tin mừng đến Giê-ru-sa-lem,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.211Z",
      "updatedAt": "2024-09-24T02:23:38.211Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "220cc366-2eec-4c4a-afcb-f705ad86c1a2",
      "number": 9,
      "content": "Hãy hết sức nói lớn tiếng lên,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.213Z",
      "updatedAt": "2024-09-24T02:23:38.213Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "aadc9b14-88d7-4e06-91bd-86f26ab451fd",
      "number": 9,
      "content": "Hãy cất tiếng lên, đừng sợ hãi,",
      "order": 4,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.215Z",
      "updatedAt": "2024-09-24T02:23:38.215Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "f7b604b1-c6b5-46cd-821e-22012eefde66",
      "number": 9,
      "content": "Hãy nói cho các thành của Giu-đa biết rằng, “Ðây là Ðức Chúa Trời của các ngươi.”",
      "order": 5,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.217Z",
      "updatedAt": "2024-09-24T02:23:38.217Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "47513319-9adc-41da-a1c6-668303526cc1",
      "number": 10,
      "content": "Này, Chúa Hằng Hữu ngự đến với uy quyền;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.220Z",
      "updatedAt": "2024-09-24T02:23:38.220Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "1923ac03-8b3c-4bcc-a397-d1fa9471af47",
      "number": 10,
      "content": "Ngài sẽ trị vì bằng cánh tay Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.222Z",
      "updatedAt": "2024-09-24T02:23:38.222Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "12186cda-ac15-452d-96ad-4b2186c1ae01",
      "number": 10,
      "content": "Này, Ngài đem phần thưởng theo với Ngài;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.224Z",
      "updatedAt": "2024-09-24T02:23:38.224Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "72a09ec9-8a5f-4839-8a5b-1575f81d2d41",
      "number": 10,
      "content": "Các vật tưởng thưởng của Ngài đi trước Ngài.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.226Z",
      "updatedAt": "2024-09-24T02:23:38.226Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "dfcd60cc-7685-4b0c-bcef-85aefb127143",
      "number": 11,
      "content": "Ngài sẽ chăn giữ bầy chiên của Ngài như một người chăn;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.229Z",
      "updatedAt": "2024-09-24T02:23:38.229Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "f68f4944-5fa6-4c1e-8b51-b374eae835d1",
      "number": 11,
      "content": "Ngài sẽ bồng ẵm các chiên con trong tay Ngài;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.231Z",
      "updatedAt": "2024-09-24T02:23:38.231Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "fb154280-e87e-42a6-a479-df2493ffab17",
      "number": 11,
      "content": "Ngài sẽ ôm chúng vào lòng Ngài,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.234Z",
      "updatedAt": "2024-09-24T02:23:38.234Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "61454188-180b-4a5b-9b4d-4479559e928a",
      "number": 11,
      "content": "Và Ngài sẽ chăm sóc các chiên mẹ có con còn thơ dại.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.236Z",
      "updatedAt": "2024-09-24T02:23:38.236Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "105992c6-7de6-4bfa-8c5a-670bf094faf7",
      "number": 12,
      "content": "Ai đã dùng lòng bàn tay lường nước biển,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.238Z",
      "updatedAt": "2024-09-24T02:23:38.238Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "5aa4db7f-571f-4f9c-966e-882886aae850",
      "number": 12,
      "content": "Dùng gang tay đo các tầng trời,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.240Z",
      "updatedAt": "2024-09-24T02:23:38.240Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "04f5a1e4-e82c-484c-9544-1aaf9312decc",
      "number": 12,
      "content": "Lấy cái đấu đong bụi đất?",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.242Z",
      "updatedAt": "2024-09-24T02:23:38.242Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "993bf89b-ad16-4620-ba95-d43d82c6f312",
      "number": 12,
      "content": "Ai lấy bàn cân cân núi lớn,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.244Z",
      "updatedAt": "2024-09-24T02:23:38.244Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "e587287c-d936-48d6-adcd-7a810f2dd420",
      "number": 12,
      "content": "Hoặc dùng dĩa cân cân các đồi cao?",
      "order": 4,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.246Z",
      "updatedAt": "2024-09-24T02:23:38.246Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "df9bf795-cd9a-4f86-ba43-1d1d0a26bb96",
      "number": 13,
      "content": "Ai có thể hướng dẫn được Thần của Chúa,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.249Z",
      "updatedAt": "2024-09-24T02:23:38.249Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "198f2533-4431-4098-8fa6-13adc6227f01",
      "number": 13,
      "content": "Hoặc làm quân sư để chỉ dạy Ngài?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.251Z",
      "updatedAt": "2024-09-24T02:23:38.251Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "d619be9c-25ff-4cc1-b6f8-4bdf05fd9d16",
      "number": 14,
      "content": "Ngài đã tham khảo mưu kế của ai?",
      "order": 0,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.253Z",
      "updatedAt": "2024-09-24T02:23:38.253Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "d7a0fb5b-5b87-4879-a9c3-906e8b7f1eda",
      "number": 14,
      "content": "Ai đã chỉ dạy Ngài?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.255Z",
      "updatedAt": "2024-09-24T02:23:38.255Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "49dc3456-a496-4952-a381-a89dcea8679e",
      "number": 14,
      "content": "Ai đã dạy Ngài con đường công lý?",
      "order": 2,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.257Z",
      "updatedAt": "2024-09-24T02:23:38.257Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "d0a039d1-9909-4df8-ae17-2b830a07d814",
      "number": 14,
      "content": "Ai đã dạy Ngài tri thức vô biên?",
      "order": 3,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.259Z",
      "updatedAt": "2024-09-24T02:23:38.259Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "97dc7f5c-48cb-466e-b0be-70bf4a87f161",
      "number": 14,
      "content": "Ai đã chỉ cho Ngài đường nào để được trí tuệ?",
      "order": 4,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.261Z",
      "updatedAt": "2024-09-24T02:23:38.261Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "6a8455ac-01b7-489b-bf89-c61dc8b66bf3",
      "number": 15,
      "content": "Này, các quốc gia khác nào giọt nước trong thùng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.263Z",
      "updatedAt": "2024-09-24T02:23:38.263Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "62416831-7785-428d-9da6-1500cde820f1",
      "number": 15,
      "content": "Họ bị xem như chút bụi dính nơi dĩa cân;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.265Z",
      "updatedAt": "2024-09-24T02:23:38.265Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "ccc687ba-8e0b-49d8-9d92-25b8d4fdc385",
      "number": 15,
      "content": "Này, Ngài nhấc bổng các đảo như một vật bé nhỏ.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.267Z",
      "updatedAt": "2024-09-24T02:23:38.267Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "c5b80241-1e55-40c7-abfe-a9bf3cf36269",
      "number": 16,
      "content": "Gỗ của núi Li-băng không đủ đốt dâng của lễ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.270Z",
      "updatedAt": "2024-09-24T02:23:38.270Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "bc682f66-3519-4c14-b102-473eb7d60190",
      "number": 16,
      "content": "Các thú trên núi ấy cũng không đủ làm của lễ thiêu.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.272Z",
      "updatedAt": "2024-09-24T02:23:38.272Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "f70168e0-1d17-4694-8667-14284763be1a",
      "number": 17,
      "content": "Tất cả các quốc gia chẳng có nghĩa lý gì trước mặt Ngài;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.274Z",
      "updatedAt": "2024-09-24T02:23:38.274Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "6b5e4fda-2586-4922-9a70-fc936f938710",
      "number": 17,
      "content": "Ðối với Ngài, mọi quốc gia trên thế giới chẳng là gì cả và chỉ là hư vô.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.277Z",
      "updatedAt": "2024-09-24T02:23:38.277Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "41b9f5c6-d391-477b-a203-f6f632f01637",
      "number": 18,
      "content": "Thế thì các ngươi ví sánh Ðức Chúa Trời với ai?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.279Z",
      "updatedAt": "2024-09-24T02:23:38.279Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "7dcb9188-ff8e-4237-bda9-206cdb57bd6c",
      "number": 18,
      "content": "Hay lấy hình ảnh nào để so sánh với Ngài được?",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.281Z",
      "updatedAt": "2024-09-24T02:23:38.281Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "f82647a9-577f-40e9-a37c-e734025feaff",
      "number": 19,
      "content": "Phải chăng một pho tượng do thợ đúc tượng đúc ra?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.284Z",
      "updatedAt": "2024-09-24T02:23:38.284Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "e3710681-a34a-4944-9522-acf9f0a9e496",
      "number": 19,
      "content": "Rồi thợ dát vàng thếp một lớp vàng mỏng lên trên,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.287Z",
      "updatedAt": "2024-09-24T02:23:38.287Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "251502b2-8d3b-4053-9216-f5bd6bb9864f",
      "number": 19,
      "content": "Và người thợ bạc đúc thêm một dây chuyền bạc.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.290Z",
      "updatedAt": "2024-09-24T02:23:38.290Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "38c6fbf3-4c60-4d6e-8453-dfcd976dbecc",
      "number": 20,
      "content": "Người nghèo không đủ khả năng làm tượng như thế để thờ thì chọn một khúc gỗ chưa mục,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.292Z",
      "updatedAt": "2024-09-24T02:23:38.292Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "3af78008-ce18-4664-82f1-768e6bfe196f",
      "number": 20,
      "content": "Rồi tìm một người thợ mộc khéo tay,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.295Z",
      "updatedAt": "2024-09-24T02:23:38.295Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "e2fd4d38-57d3-4317-9daa-f69eeca15d49",
      "number": 20,
      "content": "Mướn thợ ấy đục đẽo thành một tượng gỗ không bị ngã.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.298Z",
      "updatedAt": "2024-09-24T02:23:38.298Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "4918b778-507e-481b-9b49-ec787b5b9574",
      "number": 21,
      "content": "Các ngươi chưa biết sao?",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.300Z",
      "updatedAt": "2024-09-24T02:23:38.300Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "07b5d169-1a99-42a5-99f0-6fb5d930a370",
      "number": 28,
      "content": "Các ngươi chưa biết sao?",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.420Z",
      "updatedAt": "2024-09-24T02:23:38.420Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "35b2298c-a3b8-4d45-8342-7cfd37d09d9e",
      "number": 28,
      "content": "Các ngươi chưa nghe sao?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.425Z",
      "updatedAt": "2024-09-24T02:23:38.425Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "f5368025-34f4-4adb-9856-d13be0a3e406",
      "number": 21,
      "content": "Các ngươi chưa nghe sao?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.303Z",
      "updatedAt": "2024-09-24T02:23:38.303Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "35ba75f9-51d0-47a2-b27c-beca03eab86f",
      "number": 21,
      "content": "Từ lúc ban đầu các ngươi chưa được bảo cho biết sao?",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.306Z",
      "updatedAt": "2024-09-24T02:23:38.306Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "882228ad-8100-47a3-9779-87d3ca65d9d2",
      "number": 21,
      "content": "Từ lúc lập nền trái đất các ngươi há chưa hiểu sao?",
      "order": 3,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.309Z",
      "updatedAt": "2024-09-24T02:23:38.309Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "a5b8b813-7f22-4efb-a9ed-d35483b58d2b",
      "number": 22,
      "content": "Ðấng ngự trên vòm trái đất,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.313Z",
      "updatedAt": "2024-09-24T02:23:38.313Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "371c1bb1-1a4c-44f8-a2b9-b72148b6d550",
      "number": 22,
      "content": "Nhìn xuống nhân gian như châu chấu cào cào;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.317Z",
      "updatedAt": "2024-09-24T02:23:38.317Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "57fcc837-0f2e-45a1-bc29-1ffb5e72539b",
      "number": 22,
      "content": "Ngài giăng bầu trời ra như giăng một tấm màn;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.321Z",
      "updatedAt": "2024-09-24T02:23:38.321Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "c7be3477-dc74-42e1-9375-56176c036a63",
      "number": 22,
      "content": "Ngài căng chúng ra như người ta căng lều để ở.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.325Z",
      "updatedAt": "2024-09-24T02:23:38.325Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "7319591f-d4d5-4bfa-8ea8-42420bc18e12",
      "number": 23,
      "content": "Ngài biến những kẻ quyền thế ra hư không;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.328Z",
      "updatedAt": "2024-09-24T02:23:38.328Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "cca0ee62-af84-4958-b314-250c80859936",
      "number": 23,
      "content": "Những kẻ cầm quyền xét đoán trên đất trở thành hư ảo.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.333Z",
      "updatedAt": "2024-09-24T02:23:38.333Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "ba19f8b1-61ea-404b-bf4c-936cd78b6f22",
      "number": 24,
      "content": "Chúng vừa mới được trồng;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.337Z",
      "updatedAt": "2024-09-24T02:23:38.337Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "fcd700e4-bc43-41fa-ab9d-afac4eb6d41c",
      "number": 24,
      "content": "Chúng vừa mới được gieo;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.341Z",
      "updatedAt": "2024-09-24T02:23:38.341Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "35985182-d248-4433-b8ba-c6c6ce8a20f2",
      "number": 24,
      "content": "Chúng vừa đâm rễ vào đất;",
      "order": 2,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.345Z",
      "updatedAt": "2024-09-24T02:23:38.345Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "a27a232e-6b88-4f16-bff9-3549d692cd10",
      "number": 24,
      "content": "Ngài thổi qua chúng một hơi,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.349Z",
      "updatedAt": "2024-09-24T02:23:38.349Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "17396b1a-7cf9-427f-b6d8-e9bab4887272",
      "number": 24,
      "content": "Chúng liền héo khô ngay,",
      "order": 4,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.354Z",
      "updatedAt": "2024-09-24T02:23:38.354Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "7a12f104-055d-47ce-b5bf-33d1825ccd88",
      "number": 24,
      "content": "Và một cơn lốc đùa chúng đi như gốc rạ.",
      "order": 5,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.358Z",
      "updatedAt": "2024-09-24T02:23:38.358Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "8653f18f-df6d-4359-8549-5a4d45c87045",
      "number": 25,
      "content": "Ðấng Thánh phán hỏi:",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.361Z",
      "updatedAt": "2024-09-24T02:23:38.361Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "72155d4e-d0f4-4af8-8f92-13d842b07d03",
      "number": 25,
      "content": "Các ngươi ví sánh Ta với ai?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.365Z",
      "updatedAt": "2024-09-24T02:23:38.365Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "ee10038a-7965-4e78-8d89-1d0eaa698449",
      "number": 25,
      "content": "Các ngươi cho Ta bằng ai?",
      "order": 2,
      "parNumber": 11,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.369Z",
      "updatedAt": "2024-09-24T02:23:38.369Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "16cda8ba-5c5e-4501-8d2b-ece1abc7b971",
      "number": 26,
      "content": "Hãy ngước mắt nhìn lên trời,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.373Z",
      "updatedAt": "2024-09-24T02:23:38.373Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "f4721ca9-b386-47f3-bb65-25d7f2b4d4ba",
      "number": 26,
      "content": "Xem ai đã tạo nên những thiên thể ấy?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.377Z",
      "updatedAt": "2024-09-24T02:23:38.377Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "bb6956ed-8f01-4086-bd74-f9e27c7651a3",
      "number": 26,
      "content": "Ai đã làm chúng hiện hữu vô sốnhư vậy?",
      "order": 2,
      "parNumber": 11,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.381Z",
      "updatedAt": "2024-09-24T02:23:38.381Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "9ba99a22-8b3a-414a-ae11-c37e2b8feead",
      "number": 26,
      "content": "Thế mà Ngài gọi chúng theo từng tên riêng.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.385Z",
      "updatedAt": "2024-09-24T02:23:38.385Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "d2fe9663-f60f-431f-b308-0ed4d114ae9d",
      "number": 26,
      "content": "Nhờ quyền năng lớn lao của Ngài,",
      "order": 4,
      "parNumber": 11,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.389Z",
      "updatedAt": "2024-09-24T02:23:38.389Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "70a0004e-b1fa-48a4-93a3-66f4c42c7709",
      "number": 26,
      "content": "Nhờ sức mạnh vô hạn của Ngài,",
      "order": 5,
      "parNumber": 11,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.394Z",
      "updatedAt": "2024-09-24T02:23:38.394Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "162220a6-c3d5-46c0-9f97-62a236bf54a7",
      "number": 26,
      "content": "Không một thiên thể nào bị bỏ sót.",
      "order": 6,
      "parNumber": 11,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.398Z",
      "updatedAt": "2024-09-24T02:23:38.398Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "c3f7919a-97f3-42eb-89d4-f9e02526f7ab",
      "number": 27,
      "content": "Hỡi Gia-cốp, tại sao ngươi nói,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.402Z",
      "updatedAt": "2024-09-24T02:23:38.402Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "4021a4d2-e1fe-4dc3-a044-a3859630d379",
      "number": 27,
      "content": "Hỡi I-sơ-ra-ên, tại sao ngươi bảo:",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.406Z",
      "updatedAt": "2024-09-24T02:23:38.406Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "312ed550-faee-49a5-8e4e-b7b965168946",
      "number": 27,
      "content": "“Ðường lối tôi đã bị che khuất khỏi mắt Chúa,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.410Z",
      "updatedAt": "2024-09-24T02:23:38.410Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "689fa7b7-515d-4d68-8965-886b5f3c0cf9",
      "number": 27,
      "content": "Công lý dành cho tôi Ðức Chúa Trời đã chẳng đoái hoài.”",
      "order": 3,
      "parNumber": 12,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.415Z",
      "updatedAt": "2024-09-24T02:23:38.415Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "1b51913c-44f6-483b-9605-b67de744ee12",
      "number": 28,
      "content": "Chúa là Ðức Chúa Trời đời đời,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.430Z",
      "updatedAt": "2024-09-24T02:23:38.430Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "843f7f87-b1ec-4642-af0b-2614f8007724",
      "number": 28,
      "content": "Ðấng Tạo Hóa của toàn cả địa cầu;",
      "order": 3,
      "parNumber": 12,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.434Z",
      "updatedAt": "2024-09-24T02:23:38.434Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "0289cc2f-90ce-4640-8257-6f521318b7f8",
      "number": 28,
      "content": "Ngài không kiệt lực và không mệt mỏi;",
      "order": 4,
      "parNumber": 12,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.440Z",
      "updatedAt": "2024-09-24T02:23:38.440Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "f030606e-d6c3-4d3a-8cb5-558380ee3f9c",
      "number": 28,
      "content": "Trí tuệ Ngài thật khôn dò.",
      "order": 5,
      "parNumber": 12,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.445Z",
      "updatedAt": "2024-09-24T02:23:38.445Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "b69cd158-0db5-4fb9-bba1-c6fb1f7620c4",
      "number": 29,
      "content": "Ngài ban năng lực cho người kiệt lực;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.450Z",
      "updatedAt": "2024-09-24T02:23:38.450Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "ef625abd-ccec-46b7-9e37-776221beceba",
      "number": 29,
      "content": "Ngài ban sức mạnh cho người đuối sức.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.454Z",
      "updatedAt": "2024-09-24T02:23:38.454Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "1531c08a-0ec7-4347-9242-695fc75fc4b8",
      "number": 30,
      "content": "Ngay cả những người trẻ tuổi cũng sẽ kiệt lực và mệt mỏi;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.459Z",
      "updatedAt": "2024-09-24T02:23:38.459Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "b6448d65-61ad-4d35-9ec9-06f9854310c4",
      "number": 30,
      "content": "Các thanh niên cũng sẽ đuối sức ngã dài;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.465Z",
      "updatedAt": "2024-09-24T02:23:38.465Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "664e1b61-105f-4a09-8a65-e5cf936d7992",
      "number": 31,
      "content": "Nhưng ai trông cậy Chúa sẽ được sức mới;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.470Z",
      "updatedAt": "2024-09-24T02:23:38.470Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "d8dbdc01-907c-4579-934b-4d52e6ecdb9d",
      "number": 31,
      "content": "Họ sẽ cất cánh bay cao như phượng hoàng;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.474Z",
      "updatedAt": "2024-09-24T02:23:38.474Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "d8da13ad-00ee-423c-93a4-f4de3bfcbe64",
      "number": 31,
      "content": "Họ sẽ chạy mà không mệt mỏi;",
      "order": 2,
      "parNumber": 12,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.479Z",
      "updatedAt": "2024-09-24T02:23:38.479Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    },
    {
      "id": "cb903470-8dc8-442a-ad70-f32e1f65b8cf",
      "number": 31,
      "content": "Họ sẽ đi mà không kiệt lực.",
      "order": 3,
      "parNumber": 12,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:23:38.484Z",
      "updatedAt": "2024-09-24T02:23:38.484Z",
      "chapterId": "a2aac0f5-b9c8-4b01-8964-8f88cd8c590f"
    }
  ],
