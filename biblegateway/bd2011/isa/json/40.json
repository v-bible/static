{
  "chapter": {
    "id": "939d5bdd-fffd-4a61-9b98-de157de08513",
    "number": 40,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2040&version=BD2011",
    "createdAt": "2024-09-09T04:13:03.983Z",
    "updatedAt": "2024-09-09T04:13:03.983Z",
    "bookId": "5349a9d9-642f-4c7c-a482-7d88b8006432"
  },
  "verses": [
    {
      "id": "02109385-f731-4e89-9183-0b23307cb14e",
      "number": 1,
      "content": "Ðức Chúa Trời của các ngươi phán rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.560Z",
      "updatedAt": "2024-09-09T04:58:07.560Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "b3758c56-8b8f-4818-8449-f0fc97c95c4b",
      "number": 1,
      "content": "“Hãy an ủi, hãy an ủi dân Ta.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.572Z",
      "updatedAt": "2024-09-09T04:58:07.572Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "2001aa9f-d8e6-4746-bc0a-5f1c1347ed83",
      "number": 2,
      "content": "Hãy nói với Giê-ru-sa-lem cách trìu mến,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.574Z",
      "updatedAt": "2024-09-09T04:58:07.574Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "65c39c16-9b86-4935-99b9-36dc1ebf7711",
      "number": 2,
      "content": "Và rao báo cho nó biết rằng,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.576Z",
      "updatedAt": "2024-09-09T04:58:07.576Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "1354acb0-eda8-48ad-ba96-78f783a1c6e1",
      "number": 2,
      "content": "Nó đã chịu hình phạt đủ rồi;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.578Z",
      "updatedAt": "2024-09-09T04:58:07.578Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "43a52bee-759c-4b15-a110-4db99228efe8",
      "number": 2,
      "content": "Nó đã đền tội xong rồi;",
      "order": 3,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.581Z",
      "updatedAt": "2024-09-09T04:58:07.581Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "28df245c-1888-41d4-bda5-b4b225f909ff",
      "number": 2,
      "content": "Nó đã nhận gấp đôi từ tay Chúa vì những tội của nó.”",
      "order": 4,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.583Z",
      "updatedAt": "2024-09-09T04:58:07.583Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "9b28ee57-a481-414a-b29c-0669ab74a79f",
      "number": 3,
      "content": "Có tiếng kêu lên rằng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.585Z",
      "updatedAt": "2024-09-09T04:58:07.585Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "deaa54d0-ef7b-42eb-bc24-9886414afa3a",
      "number": 3,
      "content": "“Hãy mở cho Chúa một con đường trong đồng hoang;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.588Z",
      "updatedAt": "2024-09-09T04:58:07.588Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "57c25498-2f3c-41b2-88a3-47b009b42348",
      "number": 3,
      "content": "Hãy làm một đại lộ thẳng bằng cho Ðức Chúa Trời chúng ta trong sa mạc.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.590Z",
      "updatedAt": "2024-09-09T04:58:07.590Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "cbeab6d6-c3a2-4b16-a49a-787618e6beef",
      "number": 4,
      "content": "Mọi thung lũng phải lấp cho đầy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.593Z",
      "updatedAt": "2024-09-09T04:58:07.593Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "fea29f48-3880-47c3-9b1d-d0f265b6b8d0",
      "number": 4,
      "content": "Mọi núi đồi phải san cho phẳng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.595Z",
      "updatedAt": "2024-09-09T04:58:07.595Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "5206ea36-e368-405a-80cf-7f98f5e1926b",
      "number": 4,
      "content": "Mọi gò nổng phải ủi cho bằng;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.597Z",
      "updatedAt": "2024-09-09T04:58:07.597Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "ad8849bd-c79c-4236-a4c2-4a0f5a145191",
      "number": 4,
      "content": "Những nơi gồ ghề phải làm cho bằng phẳng,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.600Z",
      "updatedAt": "2024-09-09T04:58:07.600Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "521b1783-7469-49c3-9969-82561e4fa73c",
      "number": 5,
      "content": "Bấy giờ vinh quang của Chúa sẽ hiển lộ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.602Z",
      "updatedAt": "2024-09-09T04:58:07.602Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "41f06d68-abcb-42f2-8fda-fac7b72b9965",
      "number": 5,
      "content": "Và mọi loài xác thịt sẽ cùng nhau xem thấy,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.606Z",
      "updatedAt": "2024-09-09T04:58:07.606Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "0059ddb8-78d5-4858-9f32-3ccc25c3fcd2",
      "number": 5,
      "content": "Vì miệng Chúa đã phán vậy.”",
      "order": 2,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.610Z",
      "updatedAt": "2024-09-09T04:58:07.610Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "5520e0b5-e022-47b8-b89b-340a5f75b555",
      "number": 6,
      "content": "Có tiếng phán rằng, “Ngươi hãy kêu lên!”",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.615Z",
      "updatedAt": "2024-09-09T04:58:07.615Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "6d99fc93-cb4d-446f-b7a1-bbab4838eb34",
      "number": 6,
      "content": "Tôi thưa rằng, “Con phải nói gì?”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.620Z",
      "updatedAt": "2024-09-09T04:58:07.620Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "07cd71e8-1cd2-4cb7-9e67-63bb40295a10",
      "number": 6,
      "content": "Mọi loài xác thịt như cỏ dại,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.624Z",
      "updatedAt": "2024-09-09T04:58:07.624Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "bc5a4be5-cdf4-41db-abe4-03daa2ae931d",
      "number": 6,
      "content": "Vẻ đẹp của chúng như hoa ngoài đồng.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.629Z",
      "updatedAt": "2024-09-09T04:58:07.629Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "64ee9a06-5824-44ff-ad03-d2365726e34d",
      "number": 7,
      "content": "Cỏ héo, hoa tàn,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.633Z",
      "updatedAt": "2024-09-09T04:58:07.633Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "5731ab6a-520d-4dfa-851c-53db50172c65",
      "number": 8,
      "content": "Cỏ héo, hoa tàn,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.649Z",
      "updatedAt": "2024-09-09T04:58:07.649Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "28da644d-f1ba-4ddc-a006-8a031bd44fc1",
      "number": 7,
      "content": "Vì hơi thở của Chúa thổi qua trên chúng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.638Z",
      "updatedAt": "2024-09-09T04:58:07.638Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "f9d0a3b7-8b8c-42d4-b881-a36155aa1a9d",
      "number": 7,
      "content": "Thật vậy loài người chẳng khác gì cỏ dại.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.644Z",
      "updatedAt": "2024-09-09T04:58:07.644Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "c3780d6f-c3aa-4ebf-823d-f171c96fd439",
      "number": 8,
      "content": "Nhưng lời của Ðức Chúa Trời chúng ta đứng vững đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.654Z",
      "updatedAt": "2024-09-09T04:58:07.654Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "8b590fea-9451-4448-872b-542abe449acc",
      "number": 9,
      "content": "Hỡi các ngươi, những người đem tin mừng đến Si-ôn,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.659Z",
      "updatedAt": "2024-09-09T04:58:07.659Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "a40fcb93-2de0-4e76-9b6e-fb35419ebc96",
      "number": 9,
      "content": "Hãy lên núi cao.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.664Z",
      "updatedAt": "2024-09-09T04:58:07.664Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "c1d850d2-2ae5-479c-8cef-7f590aab89f2",
      "number": 9,
      "content": "Hỡi các ngươi, những người đem tin mừng đến Giê-ru-sa-lem,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.668Z",
      "updatedAt": "2024-09-09T04:58:07.668Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "6b49984e-650d-4b3e-a997-eeef30f33d51",
      "number": 9,
      "content": "Hãy hết sức nói lớn tiếng lên,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.673Z",
      "updatedAt": "2024-09-09T04:58:07.673Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "8bd42607-5883-4619-818a-bf3758a905d4",
      "number": 9,
      "content": "Hãy cất tiếng lên, đừng sợ hãi,",
      "order": 4,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.678Z",
      "updatedAt": "2024-09-09T04:58:07.678Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "b95c983c-a872-41cb-8c91-9dcf3eec19cd",
      "number": 9,
      "content": "Hãy nói cho các thành của Giu-đa biết rằng, “Ðây là Ðức Chúa Trời của các ngươi.”",
      "order": 5,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.684Z",
      "updatedAt": "2024-09-09T04:58:07.684Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "519eee61-5b63-4d6c-b1bf-65b165c9bd6e",
      "number": 10,
      "content": "Này, Chúa Hằng Hữu ngự đến với uy quyền;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.688Z",
      "updatedAt": "2024-09-09T04:58:07.688Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "b6a2c597-2bef-45a1-a479-b37090547898",
      "number": 10,
      "content": "Ngài sẽ trị vì bằng cánh tay Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.694Z",
      "updatedAt": "2024-09-09T04:58:07.694Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "04d45432-db3a-4baa-904c-d9e084b89bf7",
      "number": 10,
      "content": "Này, Ngài đem phần thưởng theo với Ngài;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.698Z",
      "updatedAt": "2024-09-09T04:58:07.698Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "19f0068a-9ae3-41f0-9f20-21c858fe58f2",
      "number": 10,
      "content": "Các vật tưởng thưởng của Ngài đi trước Ngài.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.703Z",
      "updatedAt": "2024-09-09T04:58:07.703Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "cae72c9c-e2fc-49c7-9921-c74566a0868a",
      "number": 11,
      "content": "Ngài sẽ chăn giữ bầy chiên của Ngài như một người chăn;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.708Z",
      "updatedAt": "2024-09-09T04:58:07.708Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "5f016519-d95f-4b6e-8e2d-97afbd99f0b1",
      "number": 11,
      "content": "Ngài sẽ bồng ẵm các chiên con trong tay Ngài;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.714Z",
      "updatedAt": "2024-09-09T04:58:07.714Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "4d94f9db-ad14-48ef-afc7-7bcb6713cc7c",
      "number": 11,
      "content": "Ngài sẽ ôm chúng vào lòng Ngài,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.719Z",
      "updatedAt": "2024-09-09T04:58:07.719Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "906a938f-dfe4-4af5-903a-978680a3bbed",
      "number": 11,
      "content": "Và Ngài sẽ chăm sóc các chiên mẹ có con còn thơ dại.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.724Z",
      "updatedAt": "2024-09-09T04:58:07.724Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "760bd4b6-caad-4be9-9324-12342333c68b",
      "number": 12,
      "content": "Ai đã dùng lòng bàn tay lường nước biển,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.730Z",
      "updatedAt": "2024-09-09T04:58:07.730Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "5c2e6ed3-fa20-4b6a-a94e-2c35e851a232",
      "number": 12,
      "content": "Dùng gang tay đo các tầng trời,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.735Z",
      "updatedAt": "2024-09-09T04:58:07.735Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "c2c78767-0c84-4939-9150-38a075b76fcc",
      "number": 12,
      "content": "Lấy cái đấu đong bụi đất?",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.740Z",
      "updatedAt": "2024-09-09T04:58:07.740Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "de0a090b-ab58-410e-a8de-d2e68bec3541",
      "number": 12,
      "content": "Ai lấy bàn cân cân núi lớn,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.745Z",
      "updatedAt": "2024-09-09T04:58:07.745Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "912ce921-6346-4a52-8019-a6e721c5cf13",
      "number": 12,
      "content": "Hoặc dùng dĩa cân cân các đồi cao?",
      "order": 4,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.751Z",
      "updatedAt": "2024-09-09T04:58:07.751Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "62919743-c681-4ee9-94e7-a446ddf05c13",
      "number": 13,
      "content": "Ai có thể hướng dẫn được Thần của Chúa,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.756Z",
      "updatedAt": "2024-09-09T04:58:07.756Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "e92dbf5f-17ae-47c2-aff5-028eb3718fb3",
      "number": 13,
      "content": "Hoặc làm quân sư để chỉ dạy Ngài?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.762Z",
      "updatedAt": "2024-09-09T04:58:07.762Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "80fca6af-49f3-42f5-8c08-467d99947a07",
      "number": 14,
      "content": "Ngài đã tham khảo mưu kế của ai?",
      "order": 0,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.767Z",
      "updatedAt": "2024-09-09T04:58:07.767Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "3f285162-6988-46d2-a611-f0d9db52505f",
      "number": 14,
      "content": "Ai đã chỉ dạy Ngài?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.772Z",
      "updatedAt": "2024-09-09T04:58:07.772Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "9c7561b0-cd0d-4890-b990-2f2595b70caa",
      "number": 14,
      "content": "Ai đã dạy Ngài con đường công lý?",
      "order": 2,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.777Z",
      "updatedAt": "2024-09-09T04:58:07.777Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "d687f23d-1565-4db9-bbda-cd9f70a14ddd",
      "number": 14,
      "content": "Ai đã dạy Ngài tri thức vô biên?",
      "order": 3,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.781Z",
      "updatedAt": "2024-09-09T04:58:07.781Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "82894aa4-d5b3-479a-89f4-4a0ebe9a1f31",
      "number": 14,
      "content": "Ai đã chỉ cho Ngài đường nào để được trí tuệ?",
      "order": 4,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.786Z",
      "updatedAt": "2024-09-09T04:58:07.786Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "85fee137-c9ae-4591-a42a-ca04dec810b5",
      "number": 15,
      "content": "Này, các quốc gia khác nào giọt nước trong thùng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.791Z",
      "updatedAt": "2024-09-09T04:58:07.791Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "ae0a4781-7132-42b4-82a7-9fe5418610c3",
      "number": 15,
      "content": "Họ bị xem như chút bụi dính nơi dĩa cân;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.796Z",
      "updatedAt": "2024-09-09T04:58:07.796Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "32c93664-23d0-4814-87ee-87cf7bd1d1b1",
      "number": 15,
      "content": "Này, Ngài nhấc bổng các đảo như một vật bé nhỏ.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.800Z",
      "updatedAt": "2024-09-09T04:58:07.800Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "8b90f316-45d7-4863-b6d6-993ce38f77ac",
      "number": 16,
      "content": "Gỗ của núi Li-băng không đủ đốt dâng của lễ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.805Z",
      "updatedAt": "2024-09-09T04:58:07.805Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "1ca2e90f-9fa6-4a9f-a55c-7bb8cbd4848d",
      "number": 16,
      "content": "Các thú trên núi ấy cũng không đủ làm của lễ thiêu.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.810Z",
      "updatedAt": "2024-09-09T04:58:07.810Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "63c45e29-70f4-471a-ae65-fd63d9db1320",
      "number": 17,
      "content": "Tất cả các quốc gia chẳng có nghĩa lý gì trước mặt Ngài;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.815Z",
      "updatedAt": "2024-09-09T04:58:07.815Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "c8b7a837-526d-4ca6-a89f-19e9bcffd86e",
      "number": 17,
      "content": "Ðối với Ngài, mọi quốc gia trên thế giới chẳng là gì cả và chỉ là hư vô.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.820Z",
      "updatedAt": "2024-09-09T04:58:07.820Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "6a54ebcb-ff2d-4636-be10-7470463d5eb4",
      "number": 18,
      "content": "Thế thì các ngươi ví sánh Ðức Chúa Trời với ai?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.825Z",
      "updatedAt": "2024-09-09T04:58:07.825Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "20757e60-978e-4d8f-83b0-dd6894189e6b",
      "number": 18,
      "content": "Hay lấy hình ảnh nào để so sánh với Ngài được?",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.830Z",
      "updatedAt": "2024-09-09T04:58:07.830Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "10b7a3da-1959-4b40-88ed-22c2d901caa7",
      "number": 19,
      "content": "Phải chăng một pho tượng do thợ đúc tượng đúc ra?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.834Z",
      "updatedAt": "2024-09-09T04:58:07.834Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "03707ef1-bf82-4b34-8210-026f50f04d85",
      "number": 19,
      "content": "Rồi thợ dát vàng thếp một lớp vàng mỏng lên trên,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.839Z",
      "updatedAt": "2024-09-09T04:58:07.839Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "e232330b-2a5f-4eb4-8488-9c18d0039572",
      "number": 19,
      "content": "Và người thợ bạc đúc thêm một dây chuyền bạc.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.844Z",
      "updatedAt": "2024-09-09T04:58:07.844Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "dfbe953e-6a11-4020-a2b7-68d2ffc81b9e",
      "number": 20,
      "content": "Người nghèo không đủ khả năng làm tượng như thế để thờ thì chọn một khúc gỗ chưa mục,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.849Z",
      "updatedAt": "2024-09-09T04:58:07.849Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "12cae2c7-cc22-49c5-a0fb-7a295e9b6d18",
      "number": 20,
      "content": "Rồi tìm một người thợ mộc khéo tay,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.855Z",
      "updatedAt": "2024-09-09T04:58:07.855Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "18f72122-2099-4473-9d1c-7e22e15ba288",
      "number": 20,
      "content": "Mướn thợ ấy đục đẽo thành một tượng gỗ không bị ngã.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.859Z",
      "updatedAt": "2024-09-09T04:58:07.859Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "ebd946d2-df31-4cd0-8150-b59fb4cfbfde",
      "number": 21,
      "content": "Các ngươi chưa biết sao?",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.864Z",
      "updatedAt": "2024-09-09T04:58:07.864Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "bb6e0154-13d4-45f2-838a-b806e3f967c9",
      "number": 28,
      "content": "Các ngươi chưa biết sao?",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.015Z",
      "updatedAt": "2024-09-09T04:58:08.015Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "d0035a05-fa66-497b-acc6-6d194d425b97",
      "number": 28,
      "content": "Các ngươi chưa nghe sao?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.020Z",
      "updatedAt": "2024-09-09T04:58:08.020Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "2941c08e-fb4f-4ac2-b954-8bc542e63348",
      "number": 21,
      "content": "Các ngươi chưa nghe sao?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.869Z",
      "updatedAt": "2024-09-09T04:58:07.869Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "77e25f77-9949-4380-a068-6c376aef2e6e",
      "number": 21,
      "content": "Từ lúc ban đầu các ngươi chưa được bảo cho biết sao?",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.874Z",
      "updatedAt": "2024-09-09T04:58:07.874Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "671c67cc-41b2-4fea-b515-5d6334f59d9a",
      "number": 21,
      "content": "Từ lúc lập nền trái đất các ngươi há chưa hiểu sao?",
      "order": 3,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.879Z",
      "updatedAt": "2024-09-09T04:58:07.879Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "d7637f7b-5ac1-4ce2-96d7-fb1205b2b1c1",
      "number": 22,
      "content": "Ðấng ngự trên vòm trái đất,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.884Z",
      "updatedAt": "2024-09-09T04:58:07.884Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "22bd9bc3-7c64-4998-9667-e2dcae0f89bc",
      "number": 22,
      "content": "Nhìn xuống nhân gian như châu chấu cào cào;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.889Z",
      "updatedAt": "2024-09-09T04:58:07.889Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "ba76d4f7-0fed-4ffd-b3bc-a729ed184ac6",
      "number": 22,
      "content": "Ngài giăng bầu trời ra như giăng một tấm màn;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.894Z",
      "updatedAt": "2024-09-09T04:58:07.894Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "6c4769ae-f326-42fb-8769-eff048c75437",
      "number": 22,
      "content": "Ngài căng chúng ra như người ta căng lều để ở.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.899Z",
      "updatedAt": "2024-09-09T04:58:07.899Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "769738c7-1ad0-431a-b6dc-667f6f5e0091",
      "number": 23,
      "content": "Ngài biến những kẻ quyền thế ra hư không;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.904Z",
      "updatedAt": "2024-09-09T04:58:07.904Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "de8b6f99-4e62-4a95-9ec5-db5ebbf83d9c",
      "number": 23,
      "content": "Những kẻ cầm quyền xét đoán trên đất trở thành hư ảo.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.908Z",
      "updatedAt": "2024-09-09T04:58:07.908Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "81ada72b-f127-4fba-ae42-934b713b585f",
      "number": 24,
      "content": "Chúng vừa mới được trồng;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.913Z",
      "updatedAt": "2024-09-09T04:58:07.913Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "03011b6c-e445-44c3-a916-e159f2d72a33",
      "number": 24,
      "content": "Chúng vừa mới được gieo;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.919Z",
      "updatedAt": "2024-09-09T04:58:07.919Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "a7be8b7f-8e40-406d-958e-b318b7a737ce",
      "number": 24,
      "content": "Chúng vừa đâm rễ vào đất;",
      "order": 2,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.924Z",
      "updatedAt": "2024-09-09T04:58:07.924Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "6bb74b30-7aa1-473d-a0d6-dcbb60a5e53e",
      "number": 24,
      "content": "Ngài thổi qua chúng một hơi,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.929Z",
      "updatedAt": "2024-09-09T04:58:07.929Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "50ac6541-b769-4e3c-8c32-98b0c46c4c27",
      "number": 24,
      "content": "Chúng liền héo khô ngay,",
      "order": 4,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.934Z",
      "updatedAt": "2024-09-09T04:58:07.934Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "1b356665-c0b2-4cd4-833c-a939c7faa33a",
      "number": 24,
      "content": "Và một cơn lốc đùa chúng đi như gốc rạ.",
      "order": 5,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.939Z",
      "updatedAt": "2024-09-09T04:58:07.939Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "464f5927-08c7-4220-8278-abadbfdb7db4",
      "number": 25,
      "content": "Ðấng Thánh phán hỏi:",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.944Z",
      "updatedAt": "2024-09-09T04:58:07.944Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "cac43320-4685-44e7-b6b8-72fc6217c971",
      "number": 25,
      "content": "Các ngươi ví sánh Ta với ai?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.950Z",
      "updatedAt": "2024-09-09T04:58:07.950Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "7369ad8b-dc1e-4e85-867d-1a0b556c78a5",
      "number": 25,
      "content": "Các ngươi cho Ta bằng ai?",
      "order": 2,
      "parNumber": 11,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.955Z",
      "updatedAt": "2024-09-09T04:58:07.955Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "f0504977-c199-4401-a99b-38fb9c41df1a",
      "number": 26,
      "content": "Hãy ngước mắt nhìn lên trời,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.960Z",
      "updatedAt": "2024-09-09T04:58:07.960Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "24e159eb-7eb2-47fd-b5d8-6e9a96b01651",
      "number": 26,
      "content": "Xem ai đã tạo nên những thiên thể ấy?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.964Z",
      "updatedAt": "2024-09-09T04:58:07.964Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "51483bab-7056-40fb-ad08-5cb5d1c341b4",
      "number": 26,
      "content": "Ai đã làm chúng hiện hữu vô sốnhư vậy?",
      "order": 2,
      "parNumber": 11,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.969Z",
      "updatedAt": "2024-09-09T04:58:07.969Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "168b68e2-958f-41fe-8322-65160d412daa",
      "number": 26,
      "content": "Thế mà Ngài gọi chúng theo từng tên riêng.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.975Z",
      "updatedAt": "2024-09-09T04:58:07.975Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "c051a96b-3349-4539-92d4-84fe01c19fe5",
      "number": 26,
      "content": "Nhờ quyền năng lớn lao của Ngài,",
      "order": 4,
      "parNumber": 11,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.980Z",
      "updatedAt": "2024-09-09T04:58:07.980Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "c9079866-dc27-4114-9d58-2005052ed274",
      "number": 26,
      "content": "Nhờ sức mạnh vô hạn của Ngài,",
      "order": 5,
      "parNumber": 11,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.985Z",
      "updatedAt": "2024-09-09T04:58:07.985Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "6caa7d1e-4149-4fb5-9568-4c25b7c06960",
      "number": 26,
      "content": "Không một thiên thể nào bị bỏ sót.",
      "order": 6,
      "parNumber": 11,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.990Z",
      "updatedAt": "2024-09-09T04:58:07.990Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "9ddc9082-c80d-4a2e-856f-58f3371cde0a",
      "number": 27,
      "content": "Hỡi Gia-cốp, tại sao ngươi nói,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:07.994Z",
      "updatedAt": "2024-09-09T04:58:07.994Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "a6b462c5-8313-48f4-9b29-b357b80419c9",
      "number": 27,
      "content": "Hỡi I-sơ-ra-ên, tại sao ngươi bảo:",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08Z",
      "updatedAt": "2024-09-09T04:58:08Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "13f450e2-81d8-4140-befb-5cb97e0cdc56",
      "number": 27,
      "content": "“Ðường lối tôi đã bị che khuất khỏi mắt Chúa,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.005Z",
      "updatedAt": "2024-09-09T04:58:08.005Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "95d67424-0f5b-4632-9791-2dc6c6a450d1",
      "number": 27,
      "content": "Công lý dành cho tôi Ðức Chúa Trời đã chẳng đoái hoài.”",
      "order": 3,
      "parNumber": 12,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.010Z",
      "updatedAt": "2024-09-09T04:58:08.010Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "27437168-8dd9-4a6b-b985-b3c62b26a12d",
      "number": 28,
      "content": "Chúa là Ðức Chúa Trời đời đời,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.025Z",
      "updatedAt": "2024-09-09T04:58:08.025Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "40c443d4-9470-441e-9356-554e289446c7",
      "number": 28,
      "content": "Ðấng Tạo Hóa của toàn cả địa cầu;",
      "order": 3,
      "parNumber": 12,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.030Z",
      "updatedAt": "2024-09-09T04:58:08.030Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "b723ee9d-79ac-4a09-b1b2-1c7aedaaa180",
      "number": 28,
      "content": "Ngài không kiệt lực và không mệt mỏi;",
      "order": 4,
      "parNumber": 12,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.035Z",
      "updatedAt": "2024-09-09T04:58:08.035Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "68165db3-369e-4eed-b913-d07fecf0ad19",
      "number": 28,
      "content": "Trí tuệ Ngài thật khôn dò.",
      "order": 5,
      "parNumber": 12,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.040Z",
      "updatedAt": "2024-09-09T04:58:08.040Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "d5953e7e-db10-4485-b6cd-0c2e83043805",
      "number": 29,
      "content": "Ngài ban năng lực cho người kiệt lực;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.044Z",
      "updatedAt": "2024-09-09T04:58:08.044Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "a64a3f2e-4c2c-496b-8c5d-8eb7a0cf8175",
      "number": 29,
      "content": "Ngài ban sức mạnh cho người đuối sức.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.050Z",
      "updatedAt": "2024-09-09T04:58:08.050Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "7205f155-af30-47b8-9a61-0fbffad98649",
      "number": 30,
      "content": "Ngay cả những người trẻ tuổi cũng sẽ kiệt lực và mệt mỏi;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.055Z",
      "updatedAt": "2024-09-09T04:58:08.055Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "72b3cf5c-1cfd-4610-b5e7-c355dd704a2f",
      "number": 30,
      "content": "Các thanh niên cũng sẽ đuối sức ngã dài;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.060Z",
      "updatedAt": "2024-09-09T04:58:08.060Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "94b9eb3d-066e-426a-be86-0120e9f2ccc3",
      "number": 31,
      "content": "Nhưng ai trông cậy Chúa sẽ được sức mới;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.066Z",
      "updatedAt": "2024-09-09T04:58:08.066Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "f45d791d-8428-4db6-811b-38076ce4f5af",
      "number": 31,
      "content": "Họ sẽ cất cánh bay cao như phượng hoàng;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.071Z",
      "updatedAt": "2024-09-09T04:58:08.071Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "c3d402d9-ea21-490d-9054-89aec5eed938",
      "number": 31,
      "content": "Họ sẽ chạy mà không mệt mỏi;",
      "order": 2,
      "parNumber": 12,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.076Z",
      "updatedAt": "2024-09-09T04:58:08.076Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    },
    {
      "id": "6bba18ac-719f-489f-bcea-5ba4dd586471",
      "number": 31,
      "content": "Họ sẽ đi mà không kiệt lực.",
      "order": 3,
      "parNumber": 12,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:58:08.082Z",
      "updatedAt": "2024-09-09T04:58:08.082Z",
      "chapterId": "939d5bdd-fffd-4a61-9b98-de157de08513"
    }
  ],
