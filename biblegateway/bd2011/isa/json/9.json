  "headings": [
    {
      "id": "914e1bef-224e-4f09-9a6c-31983d254b1d",
      "content": "Cơn Giận của Chúa đối với Những Kẻ Lãnh Ðạo Gian Ác",
      "order": 0,
      "createdAt": "2024-09-09T04:56:30.176Z",
      "updatedAt": "2024-09-09T04:56:30.176Z",
