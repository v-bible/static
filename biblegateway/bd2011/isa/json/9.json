    }
  ],
  "headings": [
    {
      "id": "1d46a5ad-3212-4e87-b941-870bb90b4fcc",
      "content": "Cơn Giận của Chúa đối với Những Kẻ Lãnh Ðạo Gian Ác",
      "order": 0,
      "createdAt": "2024-07-15T03:26:15.514Z",
      "updatedAt": "2024-07-15T03:26:15.514Z",
