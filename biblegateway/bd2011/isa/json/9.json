{
  "chapter": {
    "id": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9",
    "number": 9,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%209&version=BD2011",
    "createdAt": "2024-09-09T04:13:03.399Z",
    "updatedAt": "2024-09-09T04:13:03.399Z",
    "bookId": "5349a9d9-642f-4c7c-a482-7d88b8006432"
  },
  "verses": [
    {
      "id": "6206e2f3-a65a-4ba5-ab7a-b164c816e405",
      "number": 1,
      "content": "Tuy nhiên sẽ không còn sầu thảm cho những kẻ bị hoạn nạn nữa.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.007Z",
      "updatedAt": "2024-09-09T04:56:30.007Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "d8380bba-e6dc-4701-b0f8-885ac9ac478d",
      "number": 1,
      "content": "Trong quá khứ Ngài đã hạ xứ Xê-bu-lun và xứ Náp-ta-li xuống,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.016Z",
      "updatedAt": "2024-09-09T04:56:30.016Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "9e37e291-bf50-42fe-b565-21fb6cde30b3",
      "number": 1,
      "content": "Nhưng trong tương lai Ngài sẽ làm rạng rỡ con đường nằm ven biển,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.018Z",
      "updatedAt": "2024-09-09T04:56:30.018Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "8b432b1d-e897-4435-a083-54c1c4b973c0",
      "number": 1,
      "content": "Tức miền bên kia Sông Giô-đanh,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.020Z",
      "updatedAt": "2024-09-09T04:56:30.020Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "c5d28624-b4b2-4aed-a322-224fcdb2b036",
      "number": 1,
      "content": "Thuộc vùng Ga-li-lê của các dân ngoại.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.022Z",
      "updatedAt": "2024-09-09T04:56:30.022Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "2d10f57c-0212-439b-b1c7-d011eb7f8b61",
      "number": 2,
      "content": "Dân đi trong bóng tối sẽ thấy ánh sáng lớn;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.025Z",
      "updatedAt": "2024-09-09T04:56:30.025Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "4e785160-956d-4ef3-b2bd-15047e29b03e",
      "number": 2,
      "content": "Ðối với những kẻ sống trong xứ của bóng tử thần,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.030Z",
      "updatedAt": "2024-09-09T04:56:30.030Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "b3089cf4-8aa6-49bb-9c0f-1a7d0c7e566d",
      "number": 2,
      "content": "Một vầng chân quang đã bừng lên chiếu sáng trên họ.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.035Z",
      "updatedAt": "2024-09-09T04:56:30.035Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "33d2704d-4d53-4b1d-b693-10e5301936bd",
      "number": 3,
      "content": "Ngài đã làm cho đất nước này được tăng trưởng bội phần;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.039Z",
      "updatedAt": "2024-09-09T04:56:30.039Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "caf06662-6920-4ad1-86ec-41efb3b4bff5",
      "number": 3,
      "content": "Ngài đã cho niềm vui của họ gia tăng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.044Z",
      "updatedAt": "2024-09-09T04:56:30.044Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "cf9ba106-ec60-4b26-8d17-5104c28706fd",
      "number": 3,
      "content": "Họ vui mừng trước mặt Ngài, như vui trong mùa gặt, như vui khi chia nhau chiến lợi phẩm,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.048Z",
      "updatedAt": "2024-09-09T04:56:30.048Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "4bc27ca1-4d5a-403a-9903-8edbcfe09012",
      "number": 4,
      "content": "Vì cái ách trên cổ họ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.054Z",
      "updatedAt": "2024-09-09T04:56:30.054Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "37142d87-4c0f-42c5-8e5e-6fc76f40726b",
      "number": 4,
      "content": "Ðòn gánh trên vai họ,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.059Z",
      "updatedAt": "2024-09-09T04:56:30.059Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "a713f7ad-67d5-4354-9de3-9b847e7cc552",
      "number": 4,
      "content": "Và cây roi của kẻ áp bức họ",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.067Z",
      "updatedAt": "2024-09-09T04:56:30.067Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "4204ad8a-d4ed-4ee2-ac93-e6719c84d0ee",
      "number": 4,
      "content": "Ðã bị Ngài bẻ gãy như trong ngày Mi-đi-an bị đánh bại.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.075Z",
      "updatedAt": "2024-09-09T04:56:30.075Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "94aaf3d8-f4ba-425b-9d29-682a65b90caf",
      "number": 5,
      "content": "Vì mọi giày trận mang ra chiến trường,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.080Z",
      "updatedAt": "2024-09-09T04:56:30.080Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "aff79b44-e449-4452-a70f-881e1ec1f1f6",
      "number": 5,
      "content": "Và mọi quân phục đẫm máu quân thù,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.084Z",
      "updatedAt": "2024-09-09T04:56:30.084Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "6a450030-0e7d-441b-84c4-76f19c76847f",
      "number": 5,
      "content": "Ðều sẽ bị đem ra thiêu hủy như nhiên liệu được dùng để đốt.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.089Z",
      "updatedAt": "2024-09-09T04:56:30.089Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "c5bdf417-9fa3-4396-a94a-5338d4ca7fe0",
      "number": 6,
      "content": "Vì sẽ có một Con Trẻ sinh cho chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.094Z",
      "updatedAt": "2024-09-09T04:56:30.094Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "4a8a5863-76e6-4eef-a548-6dac0ba7d99e",
      "number": 6,
      "content": "Một Con Trai ban cho chúng ta;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.099Z",
      "updatedAt": "2024-09-09T04:56:30.099Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "49e9a150-2de7-48ec-ba2f-e29c0654244f",
      "number": 6,
      "content": "Quyền trị vì sẽ đặt trên vai Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.108Z",
      "updatedAt": "2024-09-09T04:56:30.108Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "002c8796-f4d0-41fb-8cf0-aff689fa79fc",
      "number": 6,
      "content": "Ngài sẽ được xưng là",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.113Z",
      "updatedAt": "2024-09-09T04:56:30.113Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "00f2cc1d-8963-4b68-9e74-be0301f046c7",
      "number": 6,
      "content": "Ðấng Mưu Luận Lạ Lùng,",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.118Z",
      "updatedAt": "2024-09-09T04:56:30.118Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "d5bf5796-8611-4e9e-a1cc-4f5f7e4e0a63",
      "number": 6,
      "content": "Ðức Chúa Trời Quyền Năng,",
      "order": 5,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.123Z",
      "updatedAt": "2024-09-09T04:56:30.123Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "d4ff32ea-49c6-4b50-8cce-b1a01471d9c0",
      "number": 6,
      "content": "Cha Ðời Ðời,",
      "order": 6,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.128Z",
      "updatedAt": "2024-09-09T04:56:30.128Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "bfa431e7-be62-4408-b3e1-7aecdde47613",
      "number": 6,
      "content": "Chúa Bình An.",
      "order": 7,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.134Z",
      "updatedAt": "2024-09-09T04:56:30.134Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "7cfd994a-d22a-47fa-adf0-a9503a3991ee",
      "number": 7,
      "content": "Quyền trị vì của Ngài sẽ thêm mãi không ngừng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.139Z",
      "updatedAt": "2024-09-09T04:56:30.139Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "47e913fa-bf5a-413b-9e45-17eedb659db2",
      "number": 7,
      "content": "Và hòa bình sẽ kéo dài vô tận",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.144Z",
      "updatedAt": "2024-09-09T04:56:30.144Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "b150966d-a1e8-4fcf-9210-144ef7d1abe2",
      "number": 7,
      "content": "trên ngai của Ða-vít và trong vương quốc của Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.149Z",
      "updatedAt": "2024-09-09T04:56:30.149Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "ae02b35f-a914-4a09-b3b6-202653598b4f",
      "number": 7,
      "content": "Ngài sẽ thiết lập và duy trì quyền trị vì của Ngài bằng công lý và công chính,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.155Z",
      "updatedAt": "2024-09-09T04:56:30.155Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "bcb64563-82aa-4f3e-8725-ac8a4aef6ef2",
      "number": 7,
      "content": "Từ nay cho đến đời đời.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.160Z",
      "updatedAt": "2024-09-09T04:56:30.160Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "69a36d99-24e2-4984-b3e8-4dd0411e5dfd",
      "number": 7,
      "content": "Nguyện lòng nhiệt thành của Chúa các đạo quân sẽ thực hiện điều ấy.",
      "order": 5,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.165Z",
      "updatedAt": "2024-09-09T04:56:30.165Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "4cbd276e-6d92-48e2-ba48-9d9d47fc121a",
      "number": 8,
      "content": "Chúa đã truyền một sứ điệp nghịch lại Gia-cốp;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.170Z",
      "updatedAt": "2024-09-09T04:56:30.170Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "1d6a5ce9-86d5-448f-99e7-5027a49bd2b3",
      "number": 8,
      "content": "Sứ điệp ấy bao gồm toàn thể I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.181Z",
      "updatedAt": "2024-09-09T04:56:30.181Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "0dfd3226-7aba-4d13-860c-70cbd9a0f31f",
      "number": 9,
      "content": "Toàn dân sẽ biết đến,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.186Z",
      "updatedAt": "2024-09-09T04:56:30.186Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "0d1d95ed-8412-442d-898f-851c530be4d2",
      "number": 9,
      "content": "Tức mọi người ở Ép-ra-im và dân chúng ở Sa-ma-ri.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.191Z",
      "updatedAt": "2024-09-09T04:56:30.191Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "192612ee-b547-43c8-8cac-3d3143a143d2",
      "number": 9,
      "content": "Với lòng đầy kiêu căng và ngạo mạn, họ nói rằng,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.197Z",
      "updatedAt": "2024-09-09T04:56:30.197Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "b1bc77f3-2941-4316-882e-2b058c3a3ecc",
      "number": 10,
      "content": "“Gạch đã đổ xuống, ta sẽ xây lại bằng đá khối;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.202Z",
      "updatedAt": "2024-09-09T04:56:30.202Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "3751d2e4-b604-42ec-be19-7ad5c6ad99ce",
      "number": 10,
      "content": "Những cây sung đã bị đốn hạ, ta sẽ thay thế bằng những cây tùng.”",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.207Z",
      "updatedAt": "2024-09-09T04:56:30.207Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "462773d5-3e6e-4ae0-a0b6-09e49a591189",
      "number": 11,
      "content": "Nhưng Chúa sẽ khiến những kẻ thù của Rê-xin chống lại họ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.211Z",
      "updatedAt": "2024-09-09T04:56:30.211Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "452f0f7f-a068-4857-90e6-da4fe302b9a1",
      "number": 11,
      "content": "Ngài khích động quân thù của họ nổi lên.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.216Z",
      "updatedAt": "2024-09-09T04:56:30.216Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "d92ee6af-1969-4222-b739-429e3c7f212c",
      "number": 12,
      "content": "Phía đông có A-ram, phía tây có Phi-li-tin;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.220Z",
      "updatedAt": "2024-09-09T04:56:30.220Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "1033d1a8-adf2-48fc-963c-1f36b77d2e22",
      "number": 12,
      "content": "Chúng hả mồm nuốt lấy I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.225Z",
      "updatedAt": "2024-09-09T04:56:30.225Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "2766dc4c-480b-44e5-8769-7ded589aee66",
      "number": 12,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.230Z",
      "updatedAt": "2024-09-09T04:56:30.230Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "c4b39745-f947-48f4-b570-93198fe6a29e",
      "number": 21,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 3,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.355Z",
      "updatedAt": "2024-09-09T04:56:30.355Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "2856d40a-4b98-49d2-ab9a-69eb4df5ccea",
      "number": 17,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 4,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.298Z",
      "updatedAt": "2024-09-09T04:56:30.298Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "48a34566-8954-42cc-b3af-75e017500feb",
      "number": 12,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.235Z",
      "updatedAt": "2024-09-09T04:56:30.235Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "c29abc57-3487-4cb0-83cc-24d22ae266bf",
      "number": 21,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.357Z",
      "updatedAt": "2024-09-09T04:56:30.357Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "85a2ec1c-ad04-4637-a9d4-a4c7efd4d005",
      "number": 17,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 5,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.303Z",
      "updatedAt": "2024-09-09T04:56:30.303Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "b0296bdd-ef7a-4c95-8f4e-98f96a064a61",
      "number": 13,
      "content": "Thế mà con dân Ngài vẫn không quay về với Ðấng đánh phạt mình;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.240Z",
      "updatedAt": "2024-09-09T04:56:30.240Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "ad089daf-d3a6-44af-84ea-707df8ec3888",
      "number": 13,
      "content": "Họ không tìm kiếm Chúa của các đạo quân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.245Z",
      "updatedAt": "2024-09-09T04:56:30.245Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "39301e77-0c04-4861-ae20-3ccbc56c427b",
      "number": 14,
      "content": "Vì vậy Chúa sẽ chặt đầu chặt đuôi của I-sơ-ra-ên;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.250Z",
      "updatedAt": "2024-09-09T04:56:30.250Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "e2196a9b-32d6-4b4b-a3fc-c1e9e6284c40",
      "number": 14,
      "content": "Trong một ngày, cả cành cây chà là và cây sậy đều bị chặt.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.254Z",
      "updatedAt": "2024-09-09T04:56:30.254Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "84b8f834-f7ef-4955-9516-9931f7a62527",
      "number": 15,
      "content": "Các trưởng lão và các người quyền quý của họ là đầu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.259Z",
      "updatedAt": "2024-09-09T04:56:30.259Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "ea493802-089d-41cb-b314-3d8c7e557903",
      "number": 15,
      "content": "Các tiên tri dạy dỗ những điều dối trá là đuôi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.264Z",
      "updatedAt": "2024-09-09T04:56:30.264Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "91d11f32-df3a-4e41-abf9-0f5dd04f1f70",
      "number": 16,
      "content": "Những kẻ lãnh đạo dân này đã dẫn họ đi lạc hướng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.268Z",
      "updatedAt": "2024-09-09T04:56:30.268Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "872e8e0c-0cd8-4daa-a76b-a2ee44c62ee3",
      "number": 16,
      "content": "Những kẻ dẫn dắt dân này đã đưa họ đến chỗ diệt vong.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.273Z",
      "updatedAt": "2024-09-09T04:56:30.273Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "1811e2bb-6232-4831-8657-18167feb67d5",
      "number": 17,
      "content": "Vì thế Chúa chẳng thương tiếc các trai tráng của họ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.279Z",
      "updatedAt": "2024-09-09T04:56:30.279Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "53f2a916-cf9e-46b0-94a9-f17d7b67bb99",
      "number": 17,
      "content": "Ngài cũng chẳng đoái hoài các cô nhi quả phụ của họ,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.284Z",
      "updatedAt": "2024-09-09T04:56:30.284Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "5dfb7f47-8cf2-4d7f-917f-6260cf59c7a8",
      "number": 17,
      "content": "Bởi ai nấy đều gian manh và tà ác;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.289Z",
      "updatedAt": "2024-09-09T04:56:30.289Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "dbbd1a71-d7c2-47ef-b6ef-437619f005d4",
      "number": 17,
      "content": "Mọi miệng đều nói những lời rồ dại ngông cuồng.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.294Z",
      "updatedAt": "2024-09-09T04:56:30.294Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "863e6dbd-241d-4e0e-a532-208d4804ce1a",
      "number": 18,
      "content": "Sự gian ác bùng lên như lửa cháy;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.307Z",
      "updatedAt": "2024-09-09T04:56:30.307Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "614ad9a9-c799-42b8-9d4b-936da21231af",
      "number": 18,
      "content": "Nó đốt thiêu gai góc và bụi gai;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.312Z",
      "updatedAt": "2024-09-09T04:56:30.312Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "f4534279-e50b-462c-8913-e7b2f981238e",
      "number": 18,
      "content": "Nó thiêu sạch các bụi rậm trong rừng,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.316Z",
      "updatedAt": "2024-09-09T04:56:30.316Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "f96c2ac5-0ca9-4214-9cdf-9ac698540ee0",
      "number": 18,
      "content": "Rồi bốc lên thành một luồng khói đen mù mịt.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.319Z",
      "updatedAt": "2024-09-09T04:56:30.319Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "76d84e7d-dff0-4e02-b386-e5d8d8f91918",
      "number": 19,
      "content": "Vì Chúa các đạo quân nổi cơn thịnh nộ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.322Z",
      "updatedAt": "2024-09-09T04:56:30.322Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "214cc02b-0418-4b20-bdc8-1c4a1ac332f2",
      "number": 19,
      "content": "Nên đất nước này bị lửa cháy ngập trời,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.325Z",
      "updatedAt": "2024-09-09T04:56:30.325Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "2a2e9bd7-e184-4da4-aef3-600bc8865c89",
      "number": 19,
      "content": "Còn dân chúng trở thành mồi cho lửa hừng thiêu đốt,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.329Z",
      "updatedAt": "2024-09-09T04:56:30.329Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "f3cd45f7-3800-41a2-a309-c8424a5d1e19",
      "number": 19,
      "content": "Ðến nỗi anh chị em cũng chẳng thương tiếc nhau.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.331Z",
      "updatedAt": "2024-09-09T04:56:30.331Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "89da71a5-d6f8-4d1d-8361-019a0b42cf6a",
      "number": 20,
      "content": "Chúng chộp giật bên phải, nhưng bụng vẫn đói;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.336Z",
      "updatedAt": "2024-09-09T04:56:30.336Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "dfbcf223-d006-4c8b-a83c-2ee45e977d33",
      "number": 20,
      "content": "Chúng ngoạm lấy bên trái, nhưng bụng vẫn không no;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.339Z",
      "updatedAt": "2024-09-09T04:56:30.339Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "3391aa51-72d5-4bab-8990-74b47769dfad",
      "number": 20,
      "content": "Thậm chí chúng ăn thịt luôn cả người thân của mình!",
      "order": 2,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.342Z",
      "updatedAt": "2024-09-09T04:56:30.342Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "3beb4893-ce3b-4e12-926a-8772300cda92",
      "number": 21,
      "content": "Ma-na-se ăn nuốt Ép-ra-im;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.347Z",
      "updatedAt": "2024-09-09T04:56:30.347Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "661b06bf-010a-470b-80f3-395e31e0a950",
      "number": 21,
      "content": "Ép-ra-im ăn nuốt Ma-na-se;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.350Z",
      "updatedAt": "2024-09-09T04:56:30.350Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    },
    {
      "id": "b8db5ebf-07ad-4730-ae69-a8cb7c6e84f6",
      "number": 21,
      "content": "Rồi cả hai cùng đánh Giu-đa.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:56:30.352Z",
      "updatedAt": "2024-09-09T04:56:30.352Z",
      "chapterId": "e8ef7fc5-70e5-40d6-a0fa-76e71ff9d8b9"
    }
  ],
