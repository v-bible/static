{
  "chapter": {
    "id": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc",
    "number": 9,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%209&version=BD2011",
    "createdAt": "2024-07-12T04:03:50.008Z",
    "updatedAt": "2024-07-24T15:07:50.750Z",
    "bookId": "e78c99ae-11b2-4687-bcf7-b33bef999af4"
  },
  "verses": [
    {
      "id": "4d733acf-b7e9-48f7-9cf2-2554be0b1817",
      "number": 1,
      "content": "Tuy nhiên sẽ không còn sầu thảm cho những kẻ bị hoạn nạn nữa.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:26:10.711Z",
      "updatedAt": "2024-07-15T03:26:10.880Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "d09e5d7f-b877-4716-bffd-d1798de66900",
      "number": 1,
      "content": "Trong quá khứ Ngài đã hạ xứ Xê-bu-lun và xứ Náp-ta-li xuống,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:26:10.711Z",
      "updatedAt": "2024-07-15T03:26:10.903Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "c2f07f04-e602-4d51-971b-96e2ee7598de",
      "number": 1,
      "content": "Nhưng trong tương lai Ngài sẽ làm rạng rỡ con đường nằm ven biển,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:26:10.711Z",
      "updatedAt": "2024-07-15T03:26:10.933Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "11919a37-4606-4e95-b90f-e6d76009dab0",
      "number": 1,
      "content": "Tức miền bên kia Sông Giô-đanh,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:26:10.711Z",
      "updatedAt": "2024-07-15T03:26:10.966Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "a2ceded7-5428-436a-baea-5a49483b8b2f",
      "number": 1,
      "content": "Thuộc vùng Ga-li-lê của các dân ngoại.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:26:10.711Z",
      "updatedAt": "2024-07-15T03:26:10.990Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "4c4fb1b8-b4fa-4987-bb38-565a0e062cb8",
      "number": 2,
      "content": "Dân đi trong bóng tối sẽ thấy ánh sáng lớn;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:26:10.722Z",
      "updatedAt": "2024-07-15T03:26:11.007Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "fb5cec3c-8d7b-4058-97dc-2b7523993d86",
      "number": 2,
      "content": "Ðối với những kẻ sống trong xứ của bóng tử thần,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:26:10.722Z",
      "updatedAt": "2024-07-15T03:26:11.019Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "558f2a9f-1b4b-4339-981b-a253b2d64483",
      "number": 2,
      "content": "Một vầng chân quang đã bừng lên chiếu sáng trên họ.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:26:10.722Z",
      "updatedAt": "2024-07-15T03:26:11.030Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "8a554832-db4c-41e2-83bc-48362ef20501",
      "number": 3,
      "content": "Ngài đã làm cho đất nước này được tăng trưởng bội phần;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.729Z",
      "updatedAt": "2024-07-15T03:26:10.729Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "fcb2e3da-3ff7-451a-a5f1-bde24865ee26",
      "number": 3,
      "content": "Ngài đã cho niềm vui của họ gia tăng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.729Z",
      "updatedAt": "2024-07-15T03:26:10.729Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "52bff0af-1fea-414d-a2ae-c2654112aee8",
      "number": 3,
      "content": "Họ vui mừng trước mặt Ngài, như vui trong mùa gặt, như vui khi chia nhau chiến lợi phẩm,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.729Z",
      "updatedAt": "2024-07-15T03:26:10.729Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "1ab68fd5-122a-4361-a26d-b2e2fd563fd5",
      "number": 4,
      "content": "Vì cái ách trên cổ họ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.735Z",
      "updatedAt": "2024-07-15T03:26:10.735Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "c0dd8d18-0289-4c59-9fb8-0e5dc9d8dca3",
      "number": 4,
      "content": "Ðòn gánh trên vai họ,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.735Z",
      "updatedAt": "2024-07-15T03:26:10.735Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "57531992-e003-45f9-8c00-b1058996d508",
      "number": 4,
      "content": "Và cây roi của kẻ áp bức họ",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.735Z",
      "updatedAt": "2024-07-15T03:26:10.735Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "c898d6de-0764-4317-869e-37cdee17b14a",
      "number": 4,
      "content": "Ðã bị Ngài bẻ gãy như trong ngày Mi-đi-an bị đánh bại.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.735Z",
      "updatedAt": "2024-07-15T03:26:10.735Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "5c6c0a77-5017-4a2a-82e6-a3c464b4f2c2",
      "number": 5,
      "content": "Vì mọi giày trận mang ra chiến trường,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.741Z",
      "updatedAt": "2024-07-15T03:26:10.741Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "14952941-7324-471d-b79e-dc6dabae6d13",
      "number": 5,
      "content": "Và mọi quân phục đẫm máu quân thù,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.741Z",
      "updatedAt": "2024-07-15T03:26:10.741Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "a1143f12-3ce2-4635-af93-c32df6415745",
      "number": 5,
      "content": "Ðều sẽ bị đem ra thiêu hủy như nhiên liệu được dùng để đốt.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.741Z",
      "updatedAt": "2024-07-15T03:26:10.741Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "a6fb0a93-e0c5-4f78-bb7d-37cf9173edb3",
      "number": 6,
      "content": "Vì sẽ có một Con Trẻ sinh cho chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.748Z",
      "updatedAt": "2024-07-15T03:26:10.748Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "a15617de-cf24-4fe8-8945-6c2e980b4e7d",
      "number": 6,
      "content": "Một Con Trai ban cho chúng ta;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.748Z",
      "updatedAt": "2024-07-15T03:26:10.748Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "7181c61e-77dc-46f1-8817-e70e4eb08f42",
      "number": 6,
      "content": "Quyền trị vì sẽ đặt trên vai Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.748Z",
      "updatedAt": "2024-07-15T03:26:10.748Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "b06cc6a7-7d56-495b-bc73-4471132a982b",
      "number": 6,
      "content": "Ngài sẽ được xưng là",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.748Z",
      "updatedAt": "2024-07-15T03:26:10.748Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "b1cec46e-7488-41d3-8c2c-7d54ea848f49",
      "number": 6,
      "content": "Ðấng Mưu Luận Lạ Lùng,",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.748Z",
      "updatedAt": "2024-07-15T03:26:10.748Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "32880146-2a5e-41a9-b1e9-f43e5bf5c91a",
      "number": 6,
      "content": "Ðức Chúa Trời Quyền Năng,",
      "order": 5,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.748Z",
      "updatedAt": "2024-07-15T03:26:10.748Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "c234c7ce-ec58-4899-a61a-a6efd02847e7",
      "number": 6,
      "content": "Cha Ðời Ðời,",
      "order": 6,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.748Z",
      "updatedAt": "2024-07-15T03:26:10.748Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "74790919-e4d1-45e8-b295-2a76541e4ef2",
      "number": 6,
      "content": "Chúa Bình An.",
      "order": 7,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.748Z",
      "updatedAt": "2024-07-15T03:26:10.748Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "2cb674ad-e3cc-40f1-bce1-0e70cae00a0b",
      "number": 7,
      "content": "Quyền trị vì của Ngài sẽ thêm mãi không ngừng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.755Z",
      "updatedAt": "2024-07-15T03:26:10.755Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "c900f913-be40-45bf-92e5-03c4b2a47bb6",
      "number": 7,
      "content": "Và hòa bình sẽ kéo dài vô tận",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.755Z",
      "updatedAt": "2024-07-15T03:26:10.755Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "b73a188f-34b4-43a6-9f55-5b1f88cef826",
      "number": 7,
      "content": "trên ngai của Ða-vít và trong vương quốc của Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.755Z",
      "updatedAt": "2024-07-15T03:26:10.755Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "810b7548-108d-4d74-af37-1c934310b94f",
      "number": 7,
      "content": "Ngài sẽ thiết lập và duy trì quyền trị vì của Ngài bằng công lý và công chính,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.755Z",
      "updatedAt": "2024-07-15T03:26:10.755Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "3409898a-89e4-423a-977f-f0146e292d33",
      "number": 7,
      "content": "Từ nay cho đến đời đời.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.755Z",
      "updatedAt": "2024-07-15T03:26:10.755Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "29e34d61-134b-47db-b489-60c5cdd168f2",
      "number": 7,
      "content": "Nguyện lòng nhiệt thành của Chúa các đạo quân sẽ thực hiện điều ấy.",
      "order": 5,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.755Z",
      "updatedAt": "2024-07-15T03:26:10.755Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "ae8f14ba-f9a7-4a09-802a-8d6e349aa93d",
      "number": 8,
      "content": "Chúa đã truyền một sứ điệp nghịch lại Gia-cốp;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:26:10.762Z",
      "updatedAt": "2024-07-15T03:26:11.046Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "13c7ef10-a38a-4e62-9a24-e2fedc6cfbe0",
      "number": 8,
      "content": "Sứ điệp ấy bao gồm toàn thể I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:26:10.762Z",
      "updatedAt": "2024-07-15T03:26:11.058Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "e8ba471d-ab99-453d-afb7-c5aba3882f0d",
      "number": 9,
      "content": "Toàn dân sẽ biết đến,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.768Z",
      "updatedAt": "2024-07-15T03:26:10.768Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "246fca74-cb5e-49cf-a476-e14867ee4066",
      "number": 9,
      "content": "Tức mọi người ở Ép-ra-im và dân chúng ở Sa-ma-ri.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.768Z",
      "updatedAt": "2024-07-15T03:26:10.768Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "0506c177-7ef5-4851-a004-9f3e36802487",
      "number": 9,
      "content": "Với lòng đầy kiêu căng và ngạo mạn, họ nói rằng,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.768Z",
      "updatedAt": "2024-07-15T03:26:10.768Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "9b412aff-216b-428a-9cf3-bd495e64cf36",
      "number": 10,
      "content": "“Gạch đã đổ xuống, ta sẽ xây lại bằng đá khối;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.774Z",
      "updatedAt": "2024-07-15T03:26:10.774Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "c9b0c9e3-f7a2-4aa6-8f77-4b286461b66b",
      "number": 10,
      "content": "Những cây sung đã bị đốn hạ, ta sẽ thay thế bằng những cây tùng.”",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.774Z",
      "updatedAt": "2024-07-15T03:26:10.774Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "96f37137-9cc8-4dd8-86b7-154da66dd172",
      "number": 11,
      "content": "Nhưng Chúa sẽ khiến những kẻ thù của Rê-xin chống lại họ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.780Z",
      "updatedAt": "2024-07-15T03:26:10.780Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "5a678e8e-d1ba-4a18-962d-85af6001574a",
      "number": 11,
      "content": "Ngài khích động quân thù của họ nổi lên.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.780Z",
      "updatedAt": "2024-07-15T03:26:10.780Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "bbbfcf11-36d3-4806-a2a3-05711b26ed85",
      "number": 12,
      "content": "Phía đông có A-ram, phía tây có Phi-li-tin;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.785Z",
      "updatedAt": "2024-07-15T03:26:10.785Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "d424e08f-f36a-4f54-997a-120667d0d991",
      "number": 12,
      "content": "Chúng hả mồm nuốt lấy I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.785Z",
      "updatedAt": "2024-07-15T03:26:10.785Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "3e20dd86-b5fc-40f5-8d40-12977d321812",
      "number": 12,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.785Z",
      "updatedAt": "2024-07-15T03:26:10.785Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "372210ab-ab2e-4267-a539-5544f887a7a1",
      "number": 12,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.785Z",
      "updatedAt": "2024-07-15T03:26:10.785Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "ec023cad-cc1f-45a9-a7be-422003aa5774",
      "number": 13,
      "content": "Thế mà con dân Ngài vẫn không quay về với Ðấng đánh phạt mình;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.792Z",
      "updatedAt": "2024-07-15T03:26:10.792Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "0b83bb3b-8d67-47db-8ead-82b8079f8ed9",
      "number": 13,
      "content": "Họ không tìm kiếm Chúa của các đạo quân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.792Z",
      "updatedAt": "2024-07-15T03:26:10.792Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "7914b557-5e0e-4ff2-ad54-3cb7318b194e",
      "number": 14,
      "content": "Vì vậy Chúa sẽ chặt đầu chặt đuôi của I-sơ-ra-ên;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.797Z",
      "updatedAt": "2024-07-15T03:26:10.797Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "c90a32ff-8a2f-4bb7-a09f-a917621850a9",
      "number": 14,
      "content": "Trong một ngày, cả cành cây chà là và cây sậy đều bị chặt.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.797Z",
      "updatedAt": "2024-07-15T03:26:10.797Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "54c457eb-b3b3-401c-8ca1-dede69af5fb0",
      "number": 15,
      "content": "Các trưởng lão và các người quyền quý của họ là đầu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.803Z",
      "updatedAt": "2024-07-15T03:26:10.803Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "d888c511-9360-485f-bf87-5a4b90261681",
      "number": 15,
      "content": "Các tiên tri dạy dỗ những điều dối trá là đuôi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 16,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.803Z",
      "updatedAt": "2024-07-15T03:26:10.803Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "ec428d85-4a56-45b6-b61e-7d6f695d3a67",
      "number": 16,
      "content": "Những kẻ lãnh đạo dân này đã dẫn họ đi lạc hướng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 17,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.809Z",
      "updatedAt": "2024-07-15T03:26:10.809Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "d0f65a33-6cdb-40fb-9cea-0fe71803d62c",
      "number": 16,
      "content": "Những kẻ dẫn dắt dân này đã đưa họ đến chỗ diệt vong.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.809Z",
      "updatedAt": "2024-07-15T03:26:10.809Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "697bc90d-ec58-4c54-9109-1aeeb8f1135f",
      "number": 17,
      "content": "Vì thế Chúa chẳng thương tiếc các trai tráng của họ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 19,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.815Z",
      "updatedAt": "2024-07-15T03:26:10.815Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "de408f91-1cde-4b79-a834-5d11bb76f644",
      "number": 17,
      "content": "Ngài cũng chẳng đoái hoài các cô nhi quả phụ của họ,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 20,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.815Z",
      "updatedAt": "2024-07-15T03:26:10.815Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "2b75f589-3492-485c-8c62-a8b1520108d2",
      "number": 17,
      "content": "Bởi ai nấy đều gian manh và tà ác;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 21,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.815Z",
      "updatedAt": "2024-07-15T03:26:10.815Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "bd02e06d-4a16-44c2-89f8-0c4bb81db2c6",
      "number": 17,
      "content": "Mọi miệng đều nói những lời rồ dại ngông cuồng.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 22,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.815Z",
      "updatedAt": "2024-07-15T03:26:10.815Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "90e682e4-8613-4b3b-8b7c-503d252ce939",
      "number": 17,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 4,
      "parNumber": 4,
      "parIndex": 23,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.815Z",
      "updatedAt": "2024-07-15T03:26:10.815Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "0f865439-8826-4a8a-9131-05ca60f5ea4f",
      "number": 17,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 5,
      "parNumber": 4,
      "parIndex": 24,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.815Z",
      "updatedAt": "2024-07-15T03:26:10.815Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "d77420dd-8274-4f2c-97d9-beef630869e5",
      "number": 18,
      "content": "Sự gian ác bùng lên như lửa cháy;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.821Z",
      "updatedAt": "2024-07-15T03:26:10.821Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "41590fc9-07d9-44e2-a5f5-9b73426d1a13",
      "number": 18,
      "content": "Nó đốt thiêu gai góc và bụi gai;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.821Z",
      "updatedAt": "2024-07-15T03:26:10.821Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "28e59819-8da8-4e6b-9427-6a1bf1e9dba6",
      "number": 18,
      "content": "Nó thiêu sạch các bụi rậm trong rừng,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.821Z",
      "updatedAt": "2024-07-15T03:26:10.821Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "e071889b-74a0-4981-88d9-a306ff581c85",
      "number": 18,
      "content": "Rồi bốc lên thành một luồng khói đen mù mịt.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.821Z",
      "updatedAt": "2024-07-15T03:26:10.821Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "7aeacbc8-91b1-473d-8743-0985ff2c41d5",
      "number": 19,
      "content": "Vì Chúa các đạo quân nổi cơn thịnh nộ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.827Z",
      "updatedAt": "2024-07-15T03:26:10.827Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "daca6263-1bf9-4ab6-899a-b1e391f10bf4",
      "number": 19,
      "content": "Nên đất nước này bị lửa cháy ngập trời,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.827Z",
      "updatedAt": "2024-07-15T03:26:10.827Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "1d8a5cbf-a52d-42a2-b9a3-7f13e420ff7f",
      "number": 19,
      "content": "Còn dân chúng trở thành mồi cho lửa hừng thiêu đốt,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.827Z",
      "updatedAt": "2024-07-15T03:26:10.827Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "7ca3746a-c1af-4af6-85bd-7e995c4a0391",
      "number": 19,
      "content": "Ðến nỗi anh chị em cũng chẳng thương tiếc nhau.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.827Z",
      "updatedAt": "2024-07-15T03:26:10.827Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "86bf0b7f-9957-4484-b69b-6badfa073187",
      "number": 20,
      "content": "Chúng chộp giật bên phải, nhưng bụng vẫn đói;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.833Z",
      "updatedAt": "2024-07-15T03:26:10.833Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "2e33b7bc-0984-42d0-bdc2-92005fb688ba",
      "number": 20,
      "content": "Chúng ngoạm lấy bên trái, nhưng bụng vẫn không no;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.833Z",
      "updatedAt": "2024-07-15T03:26:10.833Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "5bf67d09-390f-424b-8ba4-033b4c2d2212",
      "number": 20,
      "content": "Thậm chí chúng ăn thịt luôn cả người thân của mình!",
      "order": 2,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.833Z",
      "updatedAt": "2024-07-15T03:26:10.833Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "2736256d-23f5-41b8-9a46-c9c08cdb5aba",
      "number": 21,
      "content": "Ma-na-se ăn nuốt Ép-ra-im;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.839Z",
      "updatedAt": "2024-07-15T03:26:10.839Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "16ec3b73-9ab0-476b-a93d-f843bcfd5bcf",
      "number": 21,
      "content": "Ép-ra-im ăn nuốt Ma-na-se;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.839Z",
      "updatedAt": "2024-07-15T03:26:10.839Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "fe54b7c1-b938-4180-88c1-2cc78070bab9",
      "number": 21,
      "content": "Rồi cả hai cùng đánh Giu-đa.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.839Z",
      "updatedAt": "2024-07-15T03:26:10.839Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "7eba2b83-24c0-429d-b940-cad1fd90f176",
      "number": 21,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 3,
      "parNumber": 5,
      "parIndex": 14,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.839Z",
      "updatedAt": "2024-07-15T03:26:10.839Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    },
    {
      "id": "685f5742-84b9-4e6b-8ac0-9567d421fdc9",
      "number": 21,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 15,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:26:10.839Z",
      "updatedAt": "2024-07-15T03:26:10.839Z",
      "chapterId": "fb1036a8-72e6-44fc-9681-7b1bcdfd8cdc"
    }
  ],
