    {
      "id": "8dfb4f16-4e85-47d8-8cdb-4a147e8edcf3",
      "content": "Sứ Ðiệp Nghịch Lại A-sy-ri",
      "order": 0,
      "createdAt": "2024-07-15T03:26:26.353Z",
      "updatedAt": "2024-07-15T03:26:26.353Z",
      "verseId": "9c7e426f-1008-497c-b421-2828ceb1bd74",
      "chapterId": "66196d6b-9b88-4b38-b3e0-d1bfb4558fab"
    },
    {
      "id": "b7c89e8b-b5aa-4e3a-aeae-ab5036bdaf7b",
      "content": "Dân I-sơ-ra-ên Còn Sót Lại Ăn Năn",
      "order": 0,
      "createdAt": "2024-07-15T03:26:26.353Z",
      "updatedAt": "2024-07-15T03:26:26.353Z",
