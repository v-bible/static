      "chapterId": "0788a6b3-a21f-4ac5-b1f0-fe5d5977dab2"
    },
    {
      "id": "59f90cd9-fcd3-4ce4-9027-03685f71a2c4",
      "content": "Trùng tên với chú của ông",
      "position": 68,
      "order": 1,
      "createdAt": "2024-09-09T04:33:15.179Z",
      "updatedAt": "2024-09-09T04:33:15.179Z",
