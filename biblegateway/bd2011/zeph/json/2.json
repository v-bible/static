{
  "chapter": {
    "id": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5",
    "number": 2,
    "ref": "https://www.biblegateway.com/passage/?search=S%C3%B4-ph%C3%B4-ni%202&version=BD2011",
    "createdAt": "2024-09-09T04:13:07.730Z",
    "updatedAt": "2024-09-09T04:13:07.730Z",
    "bookId": "65b91b59-85a4-49aa-8b56-aa09da501f8a"
  },
  "verses": [
    {
      "id": "91ccf846-84bd-4159-b28b-9c5ffad99c10",
      "number": 1,
      "content": "Hỡi dân không còn biết xấu hổ, hãy nhóm họp lại, phải, hãy nhóm họp lại.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.376Z",
      "updatedAt": "2024-09-09T05:10:31.376Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "28fcddda-9742-4b71-9a1c-a32900f628d3",
      "number": 2,
      "content": "Trước khi lịnh ban ra,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.381Z",
      "updatedAt": "2024-09-09T05:10:31.381Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "5a08959e-95e8-403f-b39c-1f89d418b88c",
      "number": 2,
      "content": "Trước ngày các ngươi bị vãi ra khắp nơi như rơm rác,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.383Z",
      "updatedAt": "2024-09-09T05:10:31.383Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "1f2acd96-56e9-4684-99fc-ddd146de2840",
      "number": 2,
      "content": "Trước khi cơn giận bừng bừng của Chúa đến trên các ngươi,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.386Z",
      "updatedAt": "2024-09-09T05:10:31.386Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "abd6ae50-f320-41f1-b4ee-703d9cee738e",
      "number": 2,
      "content": "Trước ngày cơn thịnh nộ của Chúa giáng trên các ngươi.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.392Z",
      "updatedAt": "2024-09-09T05:10:31.392Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "819cdea5-de59-472a-873a-02660292ad36",
      "number": 3,
      "content": "Hãy tìm kiếm Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.397Z",
      "updatedAt": "2024-09-09T05:10:31.397Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "59bbba8b-021d-4022-b093-f6add116b8a1",
      "number": 3,
      "content": "Hỡi tất cả những người khiêm nhu trong xứ,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.402Z",
      "updatedAt": "2024-09-09T05:10:31.402Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "761863a9-3a01-47b6-abad-a14a62de447f",
      "number": 3,
      "content": "Hỡi những người thi hành các mạng lịnh Ngài đã truyền.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.407Z",
      "updatedAt": "2024-09-09T05:10:31.407Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "d48b882d-3f7b-4889-bc2c-f12b2fbaabdf",
      "number": 3,
      "content": "Hãy tìm sự công chính,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.416Z",
      "updatedAt": "2024-09-09T05:10:31.416Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "c63e5a95-ec99-4059-a858-abb0df2abbf3",
      "number": 3,
      "content": "Hãy tìm đức khiêm nhường,",
      "order": 4,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.421Z",
      "updatedAt": "2024-09-09T05:10:31.421Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "d8bb2c7d-88a8-4897-9ac6-a42ca847078b",
      "number": 3,
      "content": "May ra các ngươi sẽ được che chở trong ngày Chúa nổi giận.",
      "order": 5,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.426Z",
      "updatedAt": "2024-09-09T05:10:31.426Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "4b50974b-5b10-4511-91f5-0e084c2bdf6d",
      "number": 4,
      "content": "Quả thật Ga-xa sẽ bị bỏ rơi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.431Z",
      "updatedAt": "2024-09-09T05:10:31.431Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "a7b3235b-b6f6-4078-8a10-d83938eac177",
      "number": 4,
      "content": "Ách-kê-lôn sẽ thành nơi hoang vắng,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.440Z",
      "updatedAt": "2024-09-09T05:10:31.440Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "46133a3a-14e4-4fe6-a102-b8e7a4ed8d2f",
      "number": 4,
      "content": "Ách-đốt sẽ bị trục xuất giữa trưa,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.445Z",
      "updatedAt": "2024-09-09T05:10:31.445Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "b9b4de71-be41-4424-a110-881ecdee10dd",
      "number": 4,
      "content": "Và Éc-rôn sẽ bị nhổ bật gốc.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.450Z",
      "updatedAt": "2024-09-09T05:10:31.450Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "8b328781-36d2-4b1c-9c0e-14c1efad37d0",
      "number": 5,
      "content": "Khốn thay cho dân sống dọc theo bờ biển, dân Kê-rê-thi!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.455Z",
      "updatedAt": "2024-09-09T05:10:31.455Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "61a5239c-80f0-4661-a764-0f40321cf184",
      "number": 5,
      "content": "Lời Chúa nghịch lại ngươi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.460Z",
      "updatedAt": "2024-09-09T05:10:31.460Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "bc101b5f-35c3-4664-91e5-27aca29188fa",
      "number": 5,
      "content": "“Hỡi Ca-na-an, xứ của dân Phi-li-tin,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.465Z",
      "updatedAt": "2024-09-09T05:10:31.465Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "1b68c06c-1ec8-4b59-9131-5c4d34e5d75a",
      "number": 5,
      "content": "Ta sẽ tiêu diệt ngươi, và sẽ không còn ai trong các ngươi sống ở đó.”",
      "order": 3,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.469Z",
      "updatedAt": "2024-09-09T05:10:31.469Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "857b3ba1-2164-4689-b172-3a2e4e325944",
      "number": 6,
      "content": "Xứ nằm nơi ven biển sẽ thành những đồng cỏ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.474Z",
      "updatedAt": "2024-09-09T05:10:31.474Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "e6678103-a1cc-43ea-9c50-5a56357211c6",
      "number": 6,
      "content": "Thành những cánh đồng cho bọn chăn chiên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.479Z",
      "updatedAt": "2024-09-09T05:10:31.479Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "e4db9dbe-d782-4430-af97-cf6bedc203d6",
      "number": 6,
      "content": "Và những nơi cho các ràn chiên.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.484Z",
      "updatedAt": "2024-09-09T05:10:31.484Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "6e9c7531-ef49-4bd4-8596-ae435e818b71",
      "number": 7,
      "content": "Miền duyên hải sẽ thuộc về những người còn sót lại của nhà Giu-đa;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.489Z",
      "updatedAt": "2024-09-09T05:10:31.489Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "b3669782-b75b-4cfd-8b5d-2127b8c82acc",
      "number": 7,
      "content": "Họ sẽ chăn bầy của họ ở đó.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.494Z",
      "updatedAt": "2024-09-09T05:10:31.494Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "3cc70c9e-c80e-4f49-9305-5fd629c06e7f",
      "number": 7,
      "content": "Ðêm về họ sẽ nằm ngủ trong các nhà ở Ách-kê-lôn.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.499Z",
      "updatedAt": "2024-09-09T05:10:31.499Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "009482e0-ed3c-4f8c-86ae-5c0d70882f6f",
      "number": 7,
      "content": "Vì Chúa, Ðức Chúa Trời của họ, sẽ chăm nom họ,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.504Z",
      "updatedAt": "2024-09-09T05:10:31.504Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "134d798a-67d0-47ab-9a7f-3ee668f0b8ae",
      "number": 7,
      "content": "Ngài sẽ tái lập vận mạng của họ.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.509Z",
      "updatedAt": "2024-09-09T05:10:31.509Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "0f05c4cd-bd56-4f46-b176-6559cde9355b",
      "number": 8,
      "content": "“Ta đã nghe những lời nhục mạ của Mô-áp và những lời sỉ vả của con cái Am-môn,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.519Z",
      "updatedAt": "2024-09-09T05:10:31.519Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "0d8d0b04-7e65-4635-bc95-2714ed63e0ab",
      "number": 8,
      "content": "Những lời chúng sỉ nhục dân Ta và đe dọa xâm lăng biên thùy của họ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.529Z",
      "updatedAt": "2024-09-09T05:10:31.529Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "33973d69-88dc-4789-b96a-eb5d8e3b7b61",
      "number": 9,
      "content": "Vì thế, như Ta hằng sống,”",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.533Z",
      "updatedAt": "2024-09-09T05:10:31.533Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "77d89e75-4215-461b-9739-a4dd537502bd",
      "number": 9,
      "content": "Chúa các đạo quân, Ðức Chúa Trời của I-sơ-ra-ên, phán,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.539Z",
      "updatedAt": "2024-09-09T05:10:31.539Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "9e1c1be9-ab80-4da5-9f76-1939fad6dc5b",
      "number": 9,
      "content": "“Chắc chắn Mô-áp sẽ thành như Sô-đôm, và xứ sở của con cái Am-môn sẽ ra như Gô-mô-ra.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.544Z",
      "updatedAt": "2024-09-09T05:10:31.544Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "6b8dc4d6-7032-47c0-af6a-ebd873fdcda1",
      "number": 9,
      "content": "Ðất nước của chúng sẽ thành đất hoang cho cỏ dại, đầy các hầm muối, một vùng hoang phế muôn đời.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.549Z",
      "updatedAt": "2024-09-09T05:10:31.549Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "3875fb51-8444-4743-9aa6-5ab3b5f7d5a1",
      "number": 9,
      "content": "Những kẻ còn sót lại của dân Ta sẽ đến cướp lấy chúng,",
      "order": 4,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.554Z",
      "updatedAt": "2024-09-09T05:10:31.554Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "736ed7eb-f166-49f3-9b96-072e42c33867",
      "number": 9,
      "content": "Những kẻ sót lại của dân Ta sẽ đến chiếm hữu chúng.",
      "order": 5,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.559Z",
      "updatedAt": "2024-09-09T05:10:31.559Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "f234f871-94ea-4eb0-bb67-6638d8beb128",
      "number": 10,
      "content": "Ðó là giá chúng phải trả cho sự kiêu ngạo của chúng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.564Z",
      "updatedAt": "2024-09-09T05:10:31.564Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "5036b091-005e-41bf-9867-f370afe3d905",
      "number": 10,
      "content": "Vì chúng đã sỉ nhục và tự cao tự đại đối với dân của Chúa các đạo quân.”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.568Z",
      "updatedAt": "2024-09-09T05:10:31.568Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "e3af1580-3845-4cc4-bd00-8be829094b15",
      "number": 11,
      "content": "Chúa sẽ là cớ kinh hoàng cho chúng,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.573Z",
      "updatedAt": "2024-09-09T05:10:31.573Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "6db2e384-b3c3-4029-a163-ff14283e4535",
      "number": 11,
      "content": "Vì Ngài sẽ tiêu diệt tất cả các thần trên đất.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.578Z",
      "updatedAt": "2024-09-09T05:10:31.578Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "ea13c140-d129-4997-8387-a9fce63a4676",
      "number": 11,
      "content": "Bấy giờ người ta sẽ thờ phượng Ngài;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.583Z",
      "updatedAt": "2024-09-09T05:10:31.583Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "85c7f4ac-8a99-4199-982b-6476bd2d9fad",
      "number": 11,
      "content": "Ai nấy đều thờ phượng Ngài tại địa phương của mình,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.587Z",
      "updatedAt": "2024-09-09T05:10:31.587Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "46d97b04-c6c8-42dd-ac29-80226395f8b1",
      "number": 11,
      "content": "Tất cả các dân trên các hải đảo cũng vậy.",
      "order": 4,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.592Z",
      "updatedAt": "2024-09-09T05:10:31.592Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "14216dff-9b5c-46e4-b1df-cfb963cb7822",
      "number": 12,
      "content": "“Còn các ngươi, hỡi dân Cút,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.597Z",
      "updatedAt": "2024-09-09T05:10:31.597Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "bca9716a-cdad-4889-9fec-8445bb8eb73f",
      "number": 12,
      "content": "Các ngươi sẽ bị gươm của Ta tiêu diệt.”",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.612Z",
      "updatedAt": "2024-09-09T05:10:31.612Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "6c6955f5-a9fa-4b50-a073-144db951ff65",
      "number": 13,
      "content": "Ngài sẽ đưa tay ra chống lại phương bắc và tiêu diệt A-sy-ri;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.616Z",
      "updatedAt": "2024-09-09T05:10:31.616Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "0cb417d9-37f7-4ef0-b02e-320bfce9356d",
      "number": 13,
      "content": "Ngài sẽ biến Ni-ni-ve thành một nơi hoang phế và khô cằn như sa mạc.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.625Z",
      "updatedAt": "2024-09-09T05:10:31.625Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "79e69c25-2de8-494d-aeba-47861e3fc6a0",
      "number": 14,
      "content": "Các đàn súc vật sẽ nằm nghỉ giữa thành;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.630Z",
      "updatedAt": "2024-09-09T05:10:31.630Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "801b4562-724c-46ea-9926-7ac561e4ecc6",
      "number": 14,
      "content": "Mọi loài thú sẽ sống ở đó;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.635Z",
      "updatedAt": "2024-09-09T05:10:31.635Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "e09c88c7-96b0-4c75-8db4-34a198100e19",
      "number": 14,
      "content": "Chim bồ nông và chim vạc sẽ ở trên đầu cột các tòa nhà bỏ hoang;",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.640Z",
      "updatedAt": "2024-09-09T05:10:31.640Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "083a47ce-687c-4a42-ac37-1a5bebb05240",
      "number": 14,
      "content": "Tiếng chúng kêu sẽ vọng qua các cửa sổ;",
      "order": 3,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.645Z",
      "updatedAt": "2024-09-09T05:10:31.645Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "d75f3a3a-4202-40ee-918e-99815b4d6137",
      "number": 14,
      "content": "Cảnh hoang phế thể hiện rõ nơi các ngạch cửa,",
      "order": 4,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.650Z",
      "updatedAt": "2024-09-09T05:10:31.650Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "cbab53e4-f23b-4723-bb78-ac8181a97dfb",
      "number": 14,
      "content": "Vì những đồ làm bằng gỗ bá hương đều bị bỏ mặc cho tiêu điều.",
      "order": 5,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.655Z",
      "updatedAt": "2024-09-09T05:10:31.655Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "1ee5ac31-cb0f-42f9-8467-dd3ee1ed7a52",
      "number": 15,
      "content": "Ðó là thành trước kia thật tưng bừng nhộn nhịp,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.661Z",
      "updatedAt": "2024-09-09T05:10:31.661Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "fabf88ba-068d-43ec-b8d5-7bb9e4711c44",
      "number": 15,
      "content": "Dân sống an toàn và tự nhủ trong lòng,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.665Z",
      "updatedAt": "2024-09-09T05:10:31.665Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "392999b2-99a0-4c27-8257-65df6c5dc525",
      "number": 15,
      "content": "“Chẳng có thành nào được như thành của ta.”",
      "order": 2,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.670Z",
      "updatedAt": "2024-09-09T05:10:31.670Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "2e2689ea-a7e0-4550-9236-fc766d78a27d",
      "number": 15,
      "content": "Thế mà nay nó biến ra điêu tàn đổ nát,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.675Z",
      "updatedAt": "2024-09-09T05:10:31.675Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "23e0b827-58f4-4874-b0d1-ec147fa84f75",
      "number": 15,
      "content": "Thành một nơi cho các loài thú trú thân!",
      "order": 4,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.680Z",
      "updatedAt": "2024-09-09T05:10:31.680Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "9d7b0b31-8697-4130-b3d7-94bc8512e928",
      "number": 15,
      "content": "Ai qua lại thấy vậy đều xuýt xoa thương hại,",
      "order": 5,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.685Z",
      "updatedAt": "2024-09-09T05:10:31.685Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    },
    {
      "id": "3977e0f2-50d5-46e0-ae77-391adc567bec",
      "number": 15,
      "content": "Phất cánh tay biểu lộ nỗi thất vọng não nề.",
      "order": 6,
      "parNumber": 5,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-09T05:10:31.691Z",
      "updatedAt": "2024-09-09T05:10:31.691Z",
      "chapterId": "62f201d7-c3e3-488a-af9d-6232ae0f8ad5"
    }
  ],
