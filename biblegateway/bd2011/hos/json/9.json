    },
    {
      "id": "7038939d-17e4-48b8-a41c-e41f91c4b322",
      "content": "Theo LXX, vì nguyên bản bị hỏng, không đọc được",
      "position": 41,
      "order": 1,
      "createdAt": "2024-09-09T05:08:07.823Z",
      "updatedAt": "2024-09-09T05:08:07.823Z",
      "verseId": "1426244f-8e30-4c27-ae56-038c88a4c009",
      "chapterId": "d69e1af3-1491-499a-9632-16e8dc96124e"
    }
  ],
