    },
    {
      "id": "887e4321-8457-45d7-b076-7dc25ac69d3f",
      "content": "Tiên Tri của Chúa Bị Khủng Bố",
      "order": 0,
      "createdAt": "2024-09-09T05:08:07.697Z",
      "updatedAt": "2024-09-09T05:08:07.697Z",
      "verseId": "05e6302a-fc2e-46f9-9cb6-d6ec7e4b0491",
      "chapterId": "d69e1af3-1491-499a-9632-16e8dc96124e"
    },
    {
      "id": "0f5ac5d8-ce40-4891-817c-5b39b1913908",
      "content": "Hình Phạt Xưa và Nay",
      "order": 0,
      "createdAt": "2024-09-09T05:08:07.774Z",
