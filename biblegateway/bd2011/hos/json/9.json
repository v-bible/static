    },
    {
      "id": "adf2ebd9-5d90-4b0f-bc93-3dc2104263e4",
      "content": "Tiên Tri của Chúa Bị Khủng Bố",
      "order": 0,
      "createdAt": "2024-07-15T04:02:36.020Z",
      "updatedAt": "2024-07-15T04:02:36.020Z",
      "verseId": "5eb7c46b-4a88-4656-8461-94876fa0cbea",
      "chapterId": "9ee3425d-312f-4be1-9a60-80c0d3f2f01c"
    },
    {
      "id": "4d17a49f-5424-4532-bd8e-5a07ff84c719",
      "content": "Hình Phạt Xưa và Nay",
      "order": 0,
      "createdAt": "2024-07-15T04:02:36.020Z",
      "updatedAt": "2024-07-15T04:02:36.020Z",
