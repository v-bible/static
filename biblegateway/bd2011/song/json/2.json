{
  "chapter": {
    "id": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19",
    "number": 2,
    "ref": "https://www.biblegateway.com/passage/?search=Nh%C3%A3%20Ca%202&version=BD2011",
    "createdAt": "2024-07-12T04:03:49.684Z",
    "updatedAt": "2024-07-24T15:07:50.459Z",
    "bookId": "f2917e1a-a19e-4c15-816a-e11ac6434f94"
  },
  "verses": [
    {
      "id": "2c51f22e-b28d-4879-bd36-d39d2c99af72",
      "number": 1,
      "content": "Tôi là đóa hoa hồng của Sa-rôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.492Z",
      "updatedAt": "2024-07-15T03:23:31.639Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "e6f17725-ab03-4ee8-851d-4fb5a6fd506c",
      "number": 1,
      "content": "Là hoa huệ trong các thung lũng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.492Z",
      "updatedAt": "2024-07-15T03:23:31.649Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "194aab1f-41a5-4325-82e8-95f3b77aa441",
      "number": 2,
      "content": "Như đóa hoa huệ ở giữa những gai góc thể nào,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.502Z",
      "updatedAt": "2024-07-15T03:23:31.662Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "ad951cb2-0242-4b33-b9ff-1a53f7191e03",
      "number": 2,
      "content": "Người yêu của tôi ở giữa các thiếu nữ cũng như vậy.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.502Z",
      "updatedAt": "2024-07-15T03:23:31.670Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "4f50e386-bd07-4bda-b771-8650a3b8feb2",
      "number": 3,
      "content": "Như cây táo ở giữa những cây rừng thể nào,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.504Z",
      "updatedAt": "2024-07-15T03:23:31.682Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "af063474-d58b-4980-b6c3-a85718aef9e0",
      "number": 3,
      "content": "Người yêu của tôi ở giữa các chàng trai cũng như vậy.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.504Z",
      "updatedAt": "2024-07-15T03:23:31.691Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "9b9b3c17-eaff-4ff2-9e9c-8cc868721945",
      "number": 3,
      "content": "Tôi yêu thích được ngồi dưới bóng mát của chàng,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.504Z",
      "updatedAt": "2024-07-15T03:23:31.702Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "4425f08a-3511-47f4-af61-52c88a33943d",
      "number": 3,
      "content": "Trái của chàng làm ngọt lịm miệng tôi.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.504Z",
      "updatedAt": "2024-07-15T03:23:31.711Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "d8853dbf-01fd-4485-9433-e4dda0fd93af",
      "number": 4,
      "content": "Chàng đưa tôi vào phòng tiệc;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.507Z",
      "updatedAt": "2024-07-15T03:23:31.719Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "731ff238-f57c-4ef5-acdb-28dc5335ddee",
      "number": 4,
      "content": "Băng biểu ngữ của chàng trên tôi là tình yêu.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.507Z",
      "updatedAt": "2024-07-15T03:23:31.728Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "94be83de-041c-44b5-a0ae-a8856d23ea36",
      "number": 5,
      "content": "Hãy cho tôi những trái nho khô để bổ sức tôi lại,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.509Z",
      "updatedAt": "2024-07-15T03:23:31.738Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "cee3f15f-558e-48fd-a77b-c636ceff151d",
      "number": 5,
      "content": "Hãy cho tôi những trái táo tươi để tôi được bồi dưỡng lại,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.509Z",
      "updatedAt": "2024-07-15T03:23:31.748Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "f80342b4-0b6a-48c1-a32b-e33809d2e05e",
      "number": 5,
      "content": "Bởi tôi đang kiệt lực vì yêu.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.509Z",
      "updatedAt": "2024-07-15T03:23:31.758Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "42f117dd-c8a3-4b12-8b33-331b0e420588",
      "number": 6,
      "content": "Tay trái chàng để dưới đầu tôi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.512Z",
      "updatedAt": "2024-07-15T03:23:31.767Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "55bde696-438e-48e1-9bbc-a51b69d272cd",
      "number": 6,
      "content": "Còn tay phải chàng ôm tôi.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.512Z",
      "updatedAt": "2024-07-15T03:23:31.776Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "ddb19b73-57bf-4917-ac33-95998e769897",
      "number": 7,
      "content": "Hỡi các thiếu nữ ở Giê-ru-sa-lem, tôi nài xin các chị:",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:23:31.514Z",
      "updatedAt": "2024-07-15T03:23:31.514Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "da1debfc-599c-4081-92d4-0607c5f4a34c",
      "number": 7,
      "content": "Hãy quan sát những linh dương và những nai tơ trong đồng,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:23:31.514Z",
      "updatedAt": "2024-07-15T03:23:31.514Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "2ffb8b4f-84fa-4147-81f4-4ce1c2158198",
      "number": 7,
      "content": "Mà đừng khơi dậy hay đánh thức ái tình cho đến khi nó muốn.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:23:31.514Z",
      "updatedAt": "2024-07-15T03:23:31.514Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "90778405-2328-4719-bf4a-0a24f7252a15",
      "number": 8,
      "content": "Hãy lắng nghe! Tiếng của người yêu tôi đó.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.516Z",
      "updatedAt": "2024-07-15T03:23:31.788Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "aa1c6156-b169-4efe-ab38-11f0e3730659",
      "number": 8,
      "content": "Xem kìa! Chàng đang đến,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.516Z",
      "updatedAt": "2024-07-15T03:23:31.799Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "aa1fb1a1-d451-4f69-ac28-3d35d0abc5e4",
      "number": 8,
      "content": "Vượt qua các rặng núi, băng qua các ngọn đồi.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.516Z",
      "updatedAt": "2024-07-15T03:23:31.808Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "43ee836a-7078-4db5-a480-47591c4afe17",
      "number": 9,
      "content": "Người yêu của tôi như một con linh dương, như một chú nai tơ.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.519Z",
      "updatedAt": "2024-07-15T03:23:31.818Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "f584f5bb-6d8c-4168-b025-2d59f7f2281d",
      "number": 9,
      "content": "Kìa, chàng đang đứng sau tường của chúng tôi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.519Z",
      "updatedAt": "2024-07-15T03:23:31.827Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "6443edde-da09-4753-8075-184b4f7cc35f",
      "number": 9,
      "content": "Chàng đang nhìn qua cửa sổ,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.519Z",
      "updatedAt": "2024-07-15T03:23:31.836Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "08056db2-62eb-4886-91d3-7bd6e6ecf75c",
      "number": 9,
      "content": "Từ sau các chấn song, chàng đang đưa mắt nhìn tôi.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.519Z",
      "updatedAt": "2024-07-15T03:23:31.845Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "4c775b90-800d-47a0-82cc-521656816453",
      "number": 10,
      "content": "Người yêu của tôi cất tiếng nói, chàng bảo tôi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.525Z",
      "updatedAt": "2024-07-15T03:23:31.854Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "94fa2fe6-3d50-4ea9-ac74-25080abc55b7",
      "number": 10,
      "content": "“Dậy đi em, người yêu của anh, người đẹp của anh,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.525Z",
      "updatedAt": "2024-07-15T03:23:31.863Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "9ec82d19-e12e-43b3-8dd7-e3f5caaf6504",
      "number": 10,
      "content": "Hãy ra đây với anh,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.525Z",
      "updatedAt": "2024-07-15T03:23:31.872Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "732c0631-b2ed-4995-98d2-543bc8162aee",
      "number": 11,
      "content": "Vì kìa, mùa đông đã qua rồi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.531Z",
      "updatedAt": "2024-07-15T03:23:31.882Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "e631cdd3-57d0-4bbb-ab62-d8595b353ba4",
      "number": 11,
      "content": "Mưa đã tạnh và đã dứt hẳn rồi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.531Z",
      "updatedAt": "2024-07-15T03:23:31.891Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "b878b906-9645-4311-b5a4-e7db350cf3d6",
      "number": 12,
      "content": "Hoa tươi đang nở rộ khắp nơi trên đất,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.537Z",
      "updatedAt": "2024-07-15T03:23:31.900Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "feb1a435-4ece-4424-b2c2-65a54d1bea54",
      "number": 12,
      "content": "Mùa ca hát đã đến,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.537Z",
      "updatedAt": "2024-07-15T03:23:31.908Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "46481386-4301-4505-ad06-ed47cd415604",
      "number": 12,
      "content": "Tiếng chim gáy đã trỗi lên trong khắp xứ chúng ta,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.537Z",
      "updatedAt": "2024-07-15T03:23:31.918Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "0687c3a7-b5b3-42c5-b0ba-3709973a3632",
      "number": 13,
      "content": "Cây vả đang ra trái non,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.544Z",
      "updatedAt": "2024-07-15T03:23:31.927Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "41b4b1cf-d554-49c0-8848-ebc431daa952",
      "number": 13,
      "content": "Hoa nho đang tỏa mùi thơm ngát.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.544Z",
      "updatedAt": "2024-07-15T03:23:31.936Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "915e7ba6-894e-4d85-890c-52d76e2a4db5",
      "number": 13,
      "content": "Dậy đi em, người yêu của anh, người đẹp của anh,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.544Z",
      "updatedAt": "2024-07-15T03:23:31.945Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "3a71be8b-050e-4ebb-a223-823680a64167",
      "number": 13,
      "content": "Hãy ra đây với anh.”",
      "order": 3,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.544Z",
      "updatedAt": "2024-07-15T03:23:31.954Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "2e443e45-a353-40e4-9038-2a178426af1c",
      "number": 14,
      "content": "Hỡi bồ câu của anh trong hốc đá,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.550Z",
      "updatedAt": "2024-07-15T03:23:31.966Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "c909b7d4-529f-4451-a4f0-53dbb6f7c48b",
      "number": 14,
      "content": "Trong hốc nhỏ giữa lưng chừng vách núi,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.550Z",
      "updatedAt": "2024-07-15T03:23:31.975Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "42fe820d-0301-4861-ac69-834fd6d36293",
      "number": 14,
      "content": "Hãy cho anh trông thấy mặt em,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.550Z",
      "updatedAt": "2024-07-15T03:23:31.984Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "eb1aecf3-9a6f-41cc-ba4e-9c42e9d3f331",
      "number": 14,
      "content": "Xin cho anh nghe tiếng nói của em.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.550Z",
      "updatedAt": "2024-07-15T03:23:31.993Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "cc963823-3413-455f-b38c-24b379605284",
      "number": 14,
      "content": "Vì tiếng em ngọt ngào,",
      "order": 4,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.550Z",
      "updatedAt": "2024-07-15T03:23:32.002Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "cb35a8b2-f96c-4b38-b71b-2df1c2f3994f",
      "number": 14,
      "content": "Và mặt em xinh đẹp đáng yêu.",
      "order": 5,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.550Z",
      "updatedAt": "2024-07-15T03:23:32.014Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "bc564fb8-9113-4cac-b6f9-ab458c232b46",
      "number": 15,
      "content": "Hãy bắt giùm chúng tôi mấy con chồn,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:23:31.558Z",
      "updatedAt": "2024-07-15T03:23:31.558Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "669188f1-e9f2-4d67-910c-b8a7b19beaed",
      "number": 15,
      "content": "Mấy con chồn nhỏ phá hoại vườn nho.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:23:31.558Z",
      "updatedAt": "2024-07-15T03:23:31.558Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "7cd4d379-7a04-4720-aca2-4f5910b0f65a",
      "number": 15,
      "content": "Vì vườn nho của chúng tôi đang trổ hoa.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-07-15T03:23:31.558Z",
      "updatedAt": "2024-07-15T03:23:31.558Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "28047338-1df5-415c-bf87-9a9b28726745",
      "number": 16,
      "content": "Người yêu của tôi là của tôi, và tôi là của chàng;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.564Z",
      "updatedAt": "2024-07-15T03:23:32.026Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "60c94747-75bd-42aa-852f-21d6a258d577",
      "number": 16,
      "content": "Chàng cho bầy chiên của chàng ăn cỏ giữa đám hoa huệ.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.564Z",
      "updatedAt": "2024-07-15T03:23:32.052Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "22bcf94c-3683-4ad3-99f8-2940987dc98c",
      "number": 17,
      "content": "Trước khi bình minh đến và bóng tối biến đi,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.570Z",
      "updatedAt": "2024-07-15T03:23:32.080Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "f328e7f8-9efd-4983-bd7c-f58f54419472",
      "number": 17,
      "content": "Hãy quay lại với em, anh yêu.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.570Z",
      "updatedAt": "2024-07-15T03:23:32.107Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    },
    {
      "id": "0548aaf4-b327-4f8b-b480-99984139e3e9",
      "number": 17,
      "content": "Hãy như con linh dương hay như con nai tơ trên gành núi lởm chởm.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-07-15T03:23:31.570Z",
      "updatedAt": "2024-07-15T03:23:32.135Z",
      "chapterId": "334caaa9-b67e-4888-8d54-e5d3a2c7aa19"
    }
  ],
