    {
      "id": "dd7c242b-8510-42f4-b824-50edb30c85de",
      "content": "Vinh Quang của Ðức Chúa Trời Rời Khỏi Giê-ru-sa-lem",
      "order": 0,
      "createdAt": "2024-09-09T05:03:26.243Z",
