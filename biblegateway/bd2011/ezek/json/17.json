    },
    {
      "id": "a6f9f56b-57f9-4c4d-966b-388fc50036f9",
      "content": "Tương Lai của Dân I-sơ-ra-ên",
      "order": 0,
      "createdAt": "2024-09-09T05:05:12.608Z",
      "updatedAt": "2024-09-09T05:05:12.608Z",
      "verseId": "1689ec03-114e-420d-bf3f-a4d5fd63a59a",
      "chapterId": "a9ac84df-3d86-40c7-95e8-543a317f8784"
    }
  ],
