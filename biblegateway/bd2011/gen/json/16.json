      "chapterId": "c5372995-7f05-45a9-b09f-c4cccfa1974e"
    },
    {
      "id": "d769b116-7d7d-40d4-b04b-11216cca194a",
      "content": "nt: El Roi",
      "position": 78,
      "order": 1,
      "createdAt": "2024-09-09T04:14:12.796Z",
      "updatedAt": "2024-09-09T04:14:12.796Z",
