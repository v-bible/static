      "chapterId": "c5372995-7f05-45a9-b09f-c4cccfa1974e"
    },
    {
      "id": "63903c2d-ba81-4650-a617-36ab8123e607",
      "content": "nghĩa là Giếng của Ðấng Hằng Sống thấy tôi",
      "position": 53,
      "order": 2,
      "createdAt": "2024-09-09T04:14:12.806Z",
      "updatedAt": "2024-09-09T04:14:12.806Z",
