{
  "chapter": {
    "id": "10134a2e-b60b-490f-87d5-e87b6b65af0d",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Gi%C3%B4-%C3%AAn%201&version=BD2011",
    "createdAt": "2024-09-24T01:00:50.105Z",
    "updatedAt": "2024-09-24T01:00:50.105Z",
    "bookId": "e865539e-a9b0-456d-8616-2611b758a8fd"
  },
  "verses": [
    {
      "id": "ffb9e4f7-bf8d-4da0-b5f4-e92c6d728182",
      "number": 1,
      "content": "Lời của Chúa phán với Giô-ên con trai Pê-thu-ên:",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:39:31.517Z",
      "updatedAt": "2024-09-24T02:39:31.517Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "a7c47259-e858-42b3-96da-8da342ced8c5",
      "number": 2,
      "content": "Hỡi các trưởng lão, hãy lắng nghe những điều nầy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.528Z",
      "updatedAt": "2024-09-24T02:39:31.528Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "8b0f16e3-0fbd-4c5c-a4f4-8852123f2dac",
      "number": 2,
      "content": "Hỡi tất cả dân cư trong xứ, hãy lắng tai nghe.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.532Z",
      "updatedAt": "2024-09-24T02:39:31.532Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "dc89c24a-cf5c-4e3d-b644-ecb96efcea05",
      "number": 2,
      "content": "Có bao giờ việc như thế nầy đã xảy ra trong thời của các ngươi,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.534Z",
      "updatedAt": "2024-09-24T02:39:31.534Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "19f85a86-8e2f-42c3-b2f9-704e20ced482",
      "number": 2,
      "content": "Hoặc trong thời của tổ tiên các ngươi chăng?",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.537Z",
      "updatedAt": "2024-09-24T02:39:31.537Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "29ebe187-9f29-49c1-83c0-6c6467d41c35",
      "number": 3,
      "content": "Hãy kể lại cho con cái các ngươi để chúng biết,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.539Z",
      "updatedAt": "2024-09-24T02:39:31.539Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "a03622a9-e208-44cf-b358-3c8005d83803",
      "number": 3,
      "content": "Hầu con cái các ngươi sẽ kể lại cho con cái của chúng,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.540Z",
      "updatedAt": "2024-09-24T02:39:31.540Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "3a6ac852-bfa7-4632-a4e2-46917cf1f3d8",
      "number": 3,
      "content": "Con cái của chúng sẽ kể lại cho thế hệ tiếp theo:",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.543Z",
      "updatedAt": "2024-09-24T02:39:31.543Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "e775929f-7a4e-4af8-bfdd-f465fd87dc00",
      "number": 4,
      "content": "Vật gì cào cào đầu nhỏ bỏ lại, cào cào đầu lớn ăn.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.545Z",
      "updatedAt": "2024-09-24T02:39:31.545Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "324681fb-73f4-43ba-921c-6b0213efcec7",
      "number": 4,
      "content": "Vật gì cào cào đầu lớn bỏ lại, cào cào cánh ngắn ăn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.547Z",
      "updatedAt": "2024-09-24T02:39:31.547Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "a676e640-1cba-4443-8ae9-ae450649c81e",
      "number": 4,
      "content": "Vật gì cào cào cánh ngắn bỏ lại, cào cào cánh dài ăn.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.549Z",
      "updatedAt": "2024-09-24T02:39:31.549Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "c0275e42-6d2a-49d7-8787-dd59b3d703a9",
      "number": 5,
      "content": "Hỡi những kẻ say sưa, hãy tỉnh dậy và khóc lóc;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.551Z",
      "updatedAt": "2024-09-24T02:39:31.551Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "cb2dfba0-a189-4df2-a883-cfba1e683194",
      "number": 5,
      "content": "Hỡi mọi kẻ ghiền rượu, hãy mếu máo than van,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.553Z",
      "updatedAt": "2024-09-24T02:39:31.553Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "21a5c618-fd2a-48ae-972d-982494c3a891",
      "number": 5,
      "content": "Vì rượu mới chưa kịp uống đã bị cướp mất;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.555Z",
      "updatedAt": "2024-09-24T02:39:31.555Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "e7ad3199-adc1-4a46-a95a-1a821fd14a9e",
      "number": 6,
      "content": "Vì một dân hùng mạnh và đông vô số đã xâm lăng xứ sở chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.557Z",
      "updatedAt": "2024-09-24T02:39:31.557Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "fd9ac76d-eb3d-4152-bffd-5da6ae4905ba",
      "number": 6,
      "content": "Răng của nó như răng sư tử;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.562Z",
      "updatedAt": "2024-09-24T02:39:31.562Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "33fa1355-119a-4c62-8e46-c85b0b00a2cb",
      "number": 6,
      "content": "Nanh của nó như nanh sư tử cái.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.564Z",
      "updatedAt": "2024-09-24T02:39:31.564Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "b5d526c1-941d-4d15-ab5f-bf977e8befd1",
      "number": 7,
      "content": "Nó đã phá tan hoang cây nho của chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.566Z",
      "updatedAt": "2024-09-24T02:39:31.566Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "abcd1c96-9cb9-4850-85c2-fc06c3abe890",
      "number": 7,
      "content": "Nó đã biến cây vả của chúng ta thành đống củi lộn.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.568Z",
      "updatedAt": "2024-09-24T02:39:31.568Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "0a88c014-7151-48e5-9e05-387712e6c52e",
      "number": 7,
      "content": "Nó tước sạch vỏ rồi quăng xuống;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.571Z",
      "updatedAt": "2024-09-24T02:39:31.571Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "50fddaa6-bae0-4bc7-a0e2-64ab7538caa3",
      "number": 7,
      "content": "Nó lột sạch các cành, khiến chúng trắng hếu trụi trơ.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.575Z",
      "updatedAt": "2024-09-24T02:39:31.575Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "cda1c4f5-942f-423f-8340-58c704cfb73b",
      "number": 8,
      "content": "Hãy than khóc như người trinh nữ trẻ mặc tang phục khóc thương cho hôn phu nàng vừa mới mất.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.579Z",
      "updatedAt": "2024-09-24T02:39:31.579Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "0a7c10ff-7016-48f4-bed3-23d2c6c9425b",
      "number": 9,
      "content": "Trong nhà Chúa, của lễ chay và của lễ quán không còn dâng nữa.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.584Z",
      "updatedAt": "2024-09-24T02:39:31.584Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "32e41cbc-3fd8-4d80-8b81-055a9c327f70",
      "number": 9,
      "content": "Các tư tế, tức các tôi tớ Chúa, buồn thảm như thể đang thọ tang.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.589Z",
      "updatedAt": "2024-09-24T02:39:31.589Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "51decace-0700-4e7d-b5d7-05f61eab087b",
      "number": 10,
      "content": "Ruộng đồng bị bỏ hoang, đất đai tiêu điều,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.594Z",
      "updatedAt": "2024-09-24T02:39:31.594Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "99be925f-293e-4c2d-910d-a194671735e1",
      "number": 10,
      "content": "Lúa gạo hư hại, rượu mới chẳng còn, dầu ô-liu khan hiếm.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.599Z",
      "updatedAt": "2024-09-24T02:39:31.599Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "e580c057-1c69-4cd6-9f6c-df6b021381f5",
      "number": 11,
      "content": "Hỡi các nông phu, hãy thẹn thùng xấu hổ,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.604Z",
      "updatedAt": "2024-09-24T02:39:31.604Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "4671b7e5-ae7e-4c86-9c68-36e40ccbb96d",
      "number": 11,
      "content": "Hỡi những kẻ trồng nho, hãy khóc lóc than van,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.613Z",
      "updatedAt": "2024-09-24T02:39:31.613Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "bc156d70-e1fc-46d0-8b33-517f7ced8ddf",
      "number": 11,
      "content": "Vì lúa mì và lúa mạch không còn nữa,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.618Z",
      "updatedAt": "2024-09-24T02:39:31.618Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "4cbbf280-542d-4959-b318-eee97193ef0b",
      "number": 11,
      "content": "Bởi ruộng nương bị phá hủy cả rồi.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.623Z",
      "updatedAt": "2024-09-24T02:39:31.623Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "955cd6f1-8fde-4dfd-8a15-6ed755568d58",
      "number": 12,
      "content": "Cây nho cằn cỗi, cây vả héo khô;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.628Z",
      "updatedAt": "2024-09-24T02:39:31.628Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "5dda8c59-ffd7-4db2-b7eb-dae590267b46",
      "number": 12,
      "content": "Cây lựu, cây chà là, và cây táo cùng chung số phận;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.633Z",
      "updatedAt": "2024-09-24T02:39:31.633Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "82f05bf9-1fe6-4548-b0e1-bf01ba9b05e7",
      "number": 12,
      "content": "Tất cả cây cối ngoài đồng khô héo,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.638Z",
      "updatedAt": "2024-09-24T02:39:31.638Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "a05cebaa-f5a2-4a2d-8c27-3dc54919ff56",
      "number": 12,
      "content": "Và như thế niềm vui của con cái loài người cũng khô héo theo.”",
      "order": 3,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.643Z",
      "updatedAt": "2024-09-24T02:39:31.643Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "251481d0-1aa4-4812-99b9-1f60434aec02",
      "number": 13,
      "content": "Hỡi các tư tế, hãy mặc tang phục và than khóc;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.649Z",
      "updatedAt": "2024-09-24T02:39:31.649Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "937164d1-2021-4d55-8f65-5dba99460179",
      "number": 13,
      "content": "Hỡi những người phục vụ nơi bàn thờ, hãy khóc lóc thở than;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.658Z",
      "updatedAt": "2024-09-24T02:39:31.658Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "e6957df9-f2c4-4cb7-9b51-7a40136905c8",
      "number": 13,
      "content": "Hỡi các tôi tớ của Ðức Chúa Trời, hãy đến,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.663Z",
      "updatedAt": "2024-09-24T02:39:31.663Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "5fd715cb-a780-4e66-b052-fe35e80dc7cd",
      "number": 13,
      "content": "Hãy mặc vải thô và nằm than thở suốt đêm,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.668Z",
      "updatedAt": "2024-09-24T02:39:31.668Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "af3104cf-f4f7-4fda-916a-44cb476a9c5c",
      "number": 13,
      "content": "Vì nơi nhà Ðức Chúa Trời của lễ chay và của lễ quán đã bị cắt.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.673Z",
      "updatedAt": "2024-09-24T02:39:31.673Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "50970887-ab86-4082-8cb4-0b123bdf50df",
      "number": 14,
      "content": "Hãy biệt riêng ra một cuộc kiêng ăn;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.678Z",
      "updatedAt": "2024-09-24T02:39:31.678Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "52e998c4-96d2-4ea8-91c4-d9cf7851ad9c",
      "number": 14,
      "content": "Hãy triệu tập một đại hội trọng thể;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.683Z",
      "updatedAt": "2024-09-24T02:39:31.683Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "a60e137f-27d2-44c8-8ad2-c62a03789b26",
      "number": 14,
      "content": "Hãy mời các trưởng lão và toàn dân trong xứ đến nhà Chúa, Ðức Chúa Trời của anh chị em,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.688Z",
      "updatedAt": "2024-09-24T02:39:31.688Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "52b25eaf-79df-450c-8eaf-3d35c7872e28",
      "number": 14,
      "content": "Rồi hãy cùng nhau kêu cầu Chúa.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.693Z",
      "updatedAt": "2024-09-24T02:39:31.693Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "28cc78f6-e1d0-4279-a86f-fc30a5e482d4",
      "number": 15,
      "content": "Than ôi, ngày ấy đang đến!",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.697Z",
      "updatedAt": "2024-09-24T02:39:31.697Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "84b864a3-9112-45cb-854f-69a7c013b4e6",
      "number": 15,
      "content": "Ngày của Chúa đang đến gần;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.702Z",
      "updatedAt": "2024-09-24T02:39:31.702Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "2c66dae0-b06c-43e3-8904-bf44027c974a",
      "number": 15,
      "content": "Nó đến như một cơn hủy diệt từ Ðấng Toàn Năng.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.707Z",
      "updatedAt": "2024-09-24T02:39:31.707Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "42809c10-20b8-4b81-a993-2493f7bf9086",
      "number": 16,
      "content": "Há chẳng phải thực phẩm bị cắt trước mắt chúng ta sao?",
      "order": 0,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.712Z",
      "updatedAt": "2024-09-24T02:39:31.712Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "30efda93-429e-49fc-a442-a706833801e4",
      "number": 16,
      "content": "Sự hân hoan và vui vẻ cũng chẳng còn thấy nơi nhà Ðức Chúa Trời chúng ta sao?",
      "order": 1,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.717Z",
      "updatedAt": "2024-09-24T02:39:31.717Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "c869e865-6a4f-4f99-a473-06faaf3209cb",
      "number": 17,
      "content": "Hạt giống chết khô dưới lớp đất,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.722Z",
      "updatedAt": "2024-09-24T02:39:31.722Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "909bcebe-c4a8-416b-9a56-ce1387f01a39",
      "number": 17,
      "content": "Các vựa lúa trở thành nhà hoang,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.728Z",
      "updatedAt": "2024-09-24T02:39:31.728Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "240756d7-6ec0-48b0-a918-cb7380b817a4",
      "number": 17,
      "content": "Các kho gạo trống không đổ nát,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.732Z",
      "updatedAt": "2024-09-24T02:39:31.732Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "e46b65e1-566c-4575-b1f6-e3de6df069c6",
      "number": 17,
      "content": "Bởi vì chẳng còn lúa gạo.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.737Z",
      "updatedAt": "2024-09-24T02:39:31.737Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "74e0957e-7541-41eb-9366-c5d803b7a465",
      "number": 18,
      "content": "Súc vật rên rỉ thảm thương,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.742Z",
      "updatedAt": "2024-09-24T02:39:31.742Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "df4fe262-47f0-43e7-a6fc-819b52d3d747",
      "number": 18,
      "content": "Trâu bò lang thang đói khát,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.747Z",
      "updatedAt": "2024-09-24T02:39:31.747Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "8ebca905-eca6-4316-8cc8-6596a0d7c384",
      "number": 18,
      "content": "Vì chẳng có đồng cỏ cho chúng;",
      "order": 2,
      "parNumber": 7,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.753Z",
      "updatedAt": "2024-09-24T02:39:31.753Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "5fa0b86e-63da-480e-92b3-1781627a199d",
      "number": 18,
      "content": "Ngay cả các đàn chiên cũng cùng chung số phận.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.758Z",
      "updatedAt": "2024-09-24T02:39:31.758Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "dd108567-ad58-484e-8752-59be90f7de6e",
      "number": 19,
      "content": "Lạy Chúa, con cầu xin Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.763Z",
      "updatedAt": "2024-09-24T02:39:31.763Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "e9718e6f-7055-4184-9b18-37baa6c80239",
      "number": 19,
      "content": "Vì lửa đã thiêu rụi những đồng cỏ trong đồng hoang,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.768Z",
      "updatedAt": "2024-09-24T02:39:31.768Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "a701ffe7-0ff0-47d9-ba50-d323931c12bc",
      "number": 19,
      "content": "Và ngọn lửa đã đốt cháy mọi cây cối ngoài đồng.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.774Z",
      "updatedAt": "2024-09-24T02:39:31.774Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "5f73b4ff-31a9-4151-a627-6031c9cb8c08",
      "number": 20,
      "content": "Ngay cả các thú hoang cũng van xin Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.779Z",
      "updatedAt": "2024-09-24T02:39:31.779Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "9773cf24-acd5-4c91-8bef-41f3703f1794",
      "number": 20,
      "content": "Vì các dòng nước đều khô cạn,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.783Z",
      "updatedAt": "2024-09-24T02:39:31.783Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "3e75aca9-4303-440a-a7ce-5e6e36678dbd",
      "number": 20,
      "content": "Và lửa đã thiêu rụi những đồng cỏ trong đồng hoang.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:39:31.786Z",
      "updatedAt": "2024-09-24T02:39:31.786Z",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    }
  ],
