      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "b6a7592b-9595-43dd-bf73-e4cf72e02a97",
      "content": "Tai Họa Ðến trên Ðất Nước",
      "order": 0,
      "createdAt": "2024-09-24T02:39:31.530Z",
      "updatedAt": "2024-09-24T02:39:31.530Z",
      "verseId": "a7c47259-e858-42b3-96da-8da342ced8c5",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    },
    {
      "id": "5d0c1df2-8413-40c8-8091-499b547709e7",
      "content": "Kêu Gọi Ăn Năn và Cầu Nguyện",
      "order": 0,
      "createdAt": "2024-09-24T02:39:31.653Z",
      "updatedAt": "2024-09-24T02:39:31.653Z",
      "verseId": "251481d0-1aa4-4812-99b9-1f60434aec02",
      "chapterId": "10134a2e-b60b-490f-87d5-e87b6b65af0d"
    }
  ],
