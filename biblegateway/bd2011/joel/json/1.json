    },
    {
      "id": "5ea50d45-7e3e-4395-b614-422710ab16a9",
      "content": "Tai Họa Ðến trên Ðất Nước",
      "order": 0,
      "createdAt": "2024-09-09T05:08:30.798Z",
      "updatedAt": "2024-09-09T05:08:30.798Z",
      "verseId": "eeb36738-099c-426c-a424-3f5fd9f70f1a",
