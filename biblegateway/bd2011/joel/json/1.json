      "chapterId": "4faaa8ab-ec18-44d8-b3f2-341b9ed0fbfb"
    },
    {
      "id": "554b930c-d359-49c2-a9b7-687c635a392c",
      "content": "Tai Họa Ðến trên Ðất Nước",
      "order": 0,
      "createdAt": "2024-07-15T04:04:20.393Z",
      "updatedAt": "2024-07-15T04:04:20.393Z",
      "verseId": "fc39dd93-c96f-4823-a834-aa084c60d9f4",
      "chapterId": "4faaa8ab-ec18-44d8-b3f2-341b9ed0fbfb"
    },
    {
      "id": "8859e2dc-77a2-4855-858f-b994b3060aa5",
      "content": "Kêu Gọi Ăn Năn và Cầu Nguyện",
      "order": 0,
      "createdAt": "2024-07-15T04:04:20.393Z",
      "updatedAt": "2024-07-15T04:04:20.393Z",
