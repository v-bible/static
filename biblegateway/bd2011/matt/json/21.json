    {
      "id": "ca08c2d8-1f61-4dca-9312-7ceb5116c3ac",
      "content": "Một số bản cổ không có câu 44 này",
      "position": 86,
      "order": 1,
      "createdAt": "2024-09-09T05:12:49.684Z",
      "updatedAt": "2024-09-09T05:12:49.684Z",
