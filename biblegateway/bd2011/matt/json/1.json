  "headings": [
    {
      "id": "5451e093-33da-4e02-a819-5b4bedbafdef",
      "content": "GIA PHẢ VÀ SỰ GIÁNG SINH CỦA ÐỨC CHÚA JESUS",
      "order": 0,
      "createdAt": "2024-09-24T02:45:40.117Z",
      "updatedAt": "2024-09-24T02:45:40.117Z",
      "verseId": "a9eeb975-9fa8-46b8-8964-49453133ee09",
      "chapterId": "88d4f526-b4d3-4c22-9a87-6cbb6cd7cd6e"
    },
