  "headings": [
    {
      "id": "f31eceab-39c6-4878-a029-9236f28a45a9",
      "content": "Gia Phả của Ðức Chúa Jesus",
      "order": 0,
      "createdAt": "2024-07-15T04:16:30.770Z",
      "updatedAt": "2024-07-15T04:16:30.770Z",
      "verseId": "e0606486-2ea0-430b-9f81-568c0b6b91f9",
      "chapterId": "8e23c8c8-1406-4d70-9a9b-fb47eb6983a5"
    },
