  "headings": [
    {
      "id": "6f0d329a-216b-4455-8028-7b6bd5bff7a5",
      "content": "Ê-li-pha Cho Rằng Sự Gian Ác của Gióp Rất Lớn",
      "order": 0,
      "createdAt": "2024-09-24T01:48:48.742Z",
      "updatedAt": "2024-09-24T01:48:48.742Z",
      "verseId": "e185d267-eea4-4433-8d9c-2d60a318271d",
      "chapterId": "bce836b6-d17e-404d-8f86-40702331fafe"
    }
  ],
