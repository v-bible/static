    {
      "id": "d04a76bc-9fcb-44ab-b37a-736c4feb0e60",
      "content": "Ê-li-pha Cho Rằng Sự Gian Ác của Gióp Rất Lớn",
      "order": 0,
      "createdAt": "2024-07-15T02:38:20.604Z",
      "updatedAt": "2024-07-15T02:38:20.604Z",
