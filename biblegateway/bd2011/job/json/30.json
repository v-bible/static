  "headings": [
    {
      "id": "3066f639-c43a-4016-88a0-aa9061051b52",
      "content": "Gióp Lại Tiếp Tục Binh Vực Ông",
      "order": 0,
      "createdAt": "2024-09-09T04:40:17.024Z",
      "updatedAt": "2024-09-09T04:40:17.024Z",
      "verseId": "dec0bc13-f54c-4ebd-91c0-eaf30dec4028",
      "chapterId": "c084430e-a080-42ee-8a45-f4958d0369eb"
    }
  ],
