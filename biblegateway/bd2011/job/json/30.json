    {
      "id": "286c8728-b819-411c-a469-492402f3cae5",
      "content": "Gióp Lại Tiếp Tục Binh Vực Ông",
      "order": 0,
      "createdAt": "2024-07-15T02:40:17.536Z",
      "updatedAt": "2024-07-15T02:40:17.536Z",
      "verseId": "71a3db3e-4953-4503-966e-0f4dfa08bd45",
      "chapterId": "74db7da5-1758-4a6c-af21-06e038199530"
    }
  ],
