  "headings": [
    {
      "id": "72a3fa98-989c-40f0-b5c7-de64dc5ad676",
      "content": "Ê-li-hu Trách Gióp",
      "order": 0,
      "createdAt": "2024-09-09T04:40:27.425Z",
      "updatedAt": "2024-09-09T04:40:27.425Z",
      "verseId": "59895881-9d1e-45fd-bc8a-54c2a2407032",
      "chapterId": "bc6d2fa4-c6c8-4e28-ac0b-13ff84f962ce"
    }
  ],
