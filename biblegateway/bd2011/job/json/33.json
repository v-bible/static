    {
      "id": "58fa79d5-750d-47f9-bb2b-239210fe0095",
      "content": "Ê-li-hu Trách Gióp",
      "order": 0,
      "createdAt": "2024-07-15T02:40:52.991Z",
      "updatedAt": "2024-07-15T02:40:52.991Z",
      "verseId": "5256e6ef-b8ca-4879-8027-20e0ddb25721",
      "chapterId": "84005269-52f2-45bd-abbe-2272903c022f"
    }
  ],
