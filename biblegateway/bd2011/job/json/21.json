    {
      "id": "23ac35ad-81c5-4289-8df5-49ab29d1dad2",
      "content": "Gióp Cho Rằng Kẻ Gian Ác Thường Không Bị Phạt",
      "order": 0,
      "createdAt": "2024-07-15T02:38:08.393Z",
      "updatedAt": "2024-07-15T02:38:08.393Z",
