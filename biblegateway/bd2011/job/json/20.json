    {
      "id": "4e689ac9-0dc4-4502-a4b4-7724a38da02b",
      "content": "Xô-pha Cho Rằng Kẻ Gian Ác Bị Phạt Là Ðích Ðáng",
      "order": 0,
      "createdAt": "2024-09-09T04:39:48.082Z",
      "updatedAt": "2024-09-09T04:39:48.082Z",
