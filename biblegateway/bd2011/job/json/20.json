    {
      "id": "5cb04b8d-a484-4a96-ace2-4ade1b49728f",
      "content": "Xô-pha Cho Rằng Kẻ Gian Ác Bị Phạt Là Ðích Ðáng",
      "order": 0,
      "createdAt": "2024-07-15T02:37:56.064Z",
      "updatedAt": "2024-07-15T02:37:56.064Z",
