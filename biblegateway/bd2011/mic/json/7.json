  "headings": [
    {
      "id": "c423bf48-2715-4be5-b195-f7689b9c2200",
      "content": "Xã Hội Băng Hoại Hoàn Toàn",
      "order": 0,
      "createdAt": "2024-07-15T04:09:33.316Z",
      "updatedAt": "2024-07-15T04:09:33.316Z",
      "verseId": "b4f1c659-3f44-461f-ac8c-0249eb696bd9",
      "chapterId": "7488705b-3ee1-41e8-b1ca-f5fff0a73d26"
    },
    {
      "id": "0bee966a-83e3-44be-9c9f-aa6694692678",
      "content": "Bền Lòng Tin Cậy CHÚA",
      "order": 0,
      "createdAt": "2024-07-15T04:09:33.316Z",
      "updatedAt": "2024-07-15T04:09:33.316Z",
      "verseId": "2a6dd877-51b3-496f-9d02-4c96a06af183",
      "chapterId": "7488705b-3ee1-41e8-b1ca-f5fff0a73d26"
    },
    {
      "id": "23b6df47-880e-47dd-b1fc-018a44f60861",
      "content": "Lời Tiên Tri về Sự Tái Lập",
      "order": 0,
      "createdAt": "2024-07-15T04:09:33.316Z",
      "updatedAt": "2024-07-15T04:09:33.316Z",
      "verseId": "f5008259-aef5-44da-8902-52250a750af2",
      "chapterId": "7488705b-3ee1-41e8-b1ca-f5fff0a73d26"
    },
    {
      "id": "25ef41c2-9aab-40e1-9f96-e987aad80e03",
      "content": "Lòng Nhân Từ của Ðức Chúa Trời",
      "order": 0,
      "createdAt": "2024-07-15T04:09:33.316Z",
      "updatedAt": "2024-07-15T04:09:33.316Z",
