  "headings": [
    {
      "id": "1be3ac0b-3d0f-40a3-9e3a-b50189a791d6",
      "content": "Xã Hội Băng Hoại Hoàn Toàn",
      "order": 0,
      "createdAt": "2024-09-09T05:09:55.774Z",
      "updatedAt": "2024-09-09T05:09:55.774Z",
      "verseId": "35cf34f1-c598-4180-8b8e-c11ef97fdd93",
      "chapterId": "28aa9d50-a144-4fcf-b50f-33e9bed6d24c"
    },
    {
      "id": "f99025dd-6553-479d-ab18-63403c09db15",
      "content": "Bền Lòng Tin Cậy CHÚA",
      "order": 0,
      "createdAt": "2024-09-09T05:09:55.913Z",
      "updatedAt": "2024-09-09T05:09:55.913Z",
      "verseId": "46b5a521-ff00-4c83-8e47-d4f046a915b2",
      "chapterId": "28aa9d50-a144-4fcf-b50f-33e9bed6d24c"
    },
    {
      "id": "c38b1f31-c8b5-4009-9cf2-6047c722ae7b",
      "content": "Lời Tiên Tri về Sự Tái Lập",
      "order": 0,
      "createdAt": "2024-09-09T05:09:55.996Z",
      "updatedAt": "2024-09-09T05:09:55.996Z",
      "verseId": "3c43c130-6cbf-42e8-9c70-e57e2d27c69c",
      "chapterId": "28aa9d50-a144-4fcf-b50f-33e9bed6d24c"
    },
    {
      "id": "760795ea-3b60-46a8-81c6-4d95a74da775",
      "content": "Lòng Nhân Từ của Ðức Chúa Trời",
      "order": 0,
