    },
    {
      "id": "929cdcbf-3de4-40a5-a46f-fbe0d8e359b5",
      "content": "Dân Sa-phia thời xưa nổi tiếng là mặc đẹp",
      "position": 13,
      "order": 1,
      "createdAt": "2024-07-15T04:07:51.342Z",
      "updatedAt": "2024-07-15T04:07:51.342Z",
      "verseId": "6a8f3861-9578-4fe4-bb28-3f8fb9cb1533",
      "chapterId": "1e07a743-cc25-4b10-8438-26a8a955baa9"
    },
    {
      "id": "7ed6cd51-ae50-43e6-b3d8-b5bd48f1b7e2",
      "content": "Xa-a-nan có nghĩa là “ra đi”",
      "position": 14,
      "order": 2,
      "createdAt": "2024-07-15T04:07:51.352Z",
      "updatedAt": "2024-07-15T04:07:51.352Z",
      "verseId": "53901bce-631b-48d8-a951-0d35059eb7d6",
      "chapterId": "1e07a743-cc25-4b10-8438-26a8a955baa9"
    },
    {
      "id": "c975c022-4a85-4d9a-a0f6-0b810a580061",
      "content": "Bết Ê-xên có nghĩa là “nhà chật”",
      "position": 15,
      "order": 3,
      "createdAt": "2024-07-15T04:07:51.366Z",
      "updatedAt": "2024-07-15T04:07:51.366Z",
      "verseId": "4920d84e-c4f3-4622-a898-6685766b21d1",
      "chapterId": "1e07a743-cc25-4b10-8438-26a8a955baa9"
    },
    {
      "id": "c860c53c-6776-40e9-8f1b-f0f09dcd7470",
      "content": "nt: của ngươi",
      "position": 48,
      "order": 4,
      "createdAt": "2024-07-15T04:07:51.376Z",
      "updatedAt": "2024-07-15T04:07:51.376Z",
      "verseId": "4920d84e-c4f3-4622-a898-6685766b21d1",
      "chapterId": "1e07a743-cc25-4b10-8438-26a8a955baa9"
    },
    {
      "id": "ff46ed64-a29c-4655-8f3d-5b4ed71ea20e",
      "content": "Ách-xíp có nghĩa là “dối gạt”",
      "position": 53,
      "order": 5,
      "createdAt": "2024-07-15T04:07:51.382Z",
      "updatedAt": "2024-07-15T04:07:51.382Z",
      "verseId": "4ea17105-723f-4a7d-a8e8-a349341cd507",
      "chapterId": "1e07a743-cc25-4b10-8438-26a8a955baa9"
    },
    {
      "id": "d2ef6463-532e-442a-a056-831dad57fa6f",
      "content": "Ma-rê-sa có nghĩa là “kẻ chinh phục”",
      "position": 20,
      "order": 6,
      "createdAt": "2024-07-15T04:07:51.389Z",
      "updatedAt": "2024-07-15T04:07:51.389Z",
      "verseId": "e00b1c27-18eb-4701-8def-cc7949100f62",
      "chapterId": "1e07a743-cc25-4b10-8438-26a8a955baa9"
    }
