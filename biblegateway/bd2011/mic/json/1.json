    },
    {
      "id": "d969bffa-ed61-42aa-b0d2-7b60cd0b2340",
      "content": "Dân Sa-phia thời xưa nổi tiếng là mặc đẹp",
      "position": 13,
      "order": 1,
      "createdAt": "2024-09-24T02:41:00.596Z",
      "updatedAt": "2024-09-24T02:41:00.596Z",
      "verseId": "61e95112-145d-438a-b232-2e592bfe9182",
      "chapterId": "2c4c7ab3-0ad8-4b2d-b7be-36c398b2c236"
    },
    {
      "id": "1c980a05-1660-4d33-a88b-dbde0b89100f",
      "content": "Xa-a-nan có nghĩa là “ra đi”",
      "position": 14,
      "order": 2,
      "createdAt": "2024-09-24T02:41:00.602Z",
      "updatedAt": "2024-09-24T02:41:00.602Z",
      "verseId": "fffebf68-d8fb-4f23-9f2f-4f36ddaa50f9",
      "chapterId": "2c4c7ab3-0ad8-4b2d-b7be-36c398b2c236"
    },
    {
      "id": "56f7ccf1-554e-4f39-b069-099cacf5f052",
      "content": "Bết Ê-xên có nghĩa là “nhà chật”",
      "position": 15,
      "order": 3,
      "createdAt": "2024-09-24T02:41:00.608Z",
      "updatedAt": "2024-09-24T02:41:00.608Z",
      "verseId": "2d08587c-c9d3-421a-ba48-b2738e8b7ecb",
      "chapterId": "2c4c7ab3-0ad8-4b2d-b7be-36c398b2c236"
    },
    {
      "id": "74f08a9d-cd1b-4def-9d10-d1cc74d6e603",
      "content": "nt: của ngươi",
      "position": 48,
      "order": 4,
      "createdAt": "2024-09-24T02:41:00.611Z",
      "updatedAt": "2024-09-24T02:41:00.611Z",
      "verseId": "2d08587c-c9d3-421a-ba48-b2738e8b7ecb",
      "chapterId": "2c4c7ab3-0ad8-4b2d-b7be-36c398b2c236"
    },
    {
      "id": "0ebc3658-4fd1-4dd2-971d-3cb2c20ab4fb",
      "content": "Ách-xíp có nghĩa là “dối gạt”",
      "position": 53,
      "order": 5,
      "createdAt": "2024-09-24T02:41:00.636Z",
      "updatedAt": "2024-09-24T02:41:00.636Z",
      "verseId": "7d64375d-8eaa-41b4-8660-0a7756389589",
      "chapterId": "2c4c7ab3-0ad8-4b2d-b7be-36c398b2c236"
    },
    {
      "id": "3ee7c787-98cd-4d83-8c75-84b3f5f6c955",
      "content": "Ma-rê-sa có nghĩa là “kẻ chinh phục”",
      "position": 20,
      "order": 6,
      "createdAt": "2024-09-24T02:41:00.642Z",
      "updatedAt": "2024-09-24T02:41:00.642Z",
      "verseId": "2e11dcdb-1101-49a7-897f-26e877219dc0",
      "chapterId": "2c4c7ab3-0ad8-4b2d-b7be-36c398b2c236"
    }
  ],
