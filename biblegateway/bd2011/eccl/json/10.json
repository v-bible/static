      "chapterId": "986b9aee-7bd9-466d-a8b0-0cdc3e507820"
    },
    {
      "id": "f2081ab7-3976-45ee-b45a-57a067ce384d",
      "number": 20,
      "content": "Ðừng nguyền rủa người giàu sang quyền quý dù chỉ trong phòng riêng,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 26,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:55:32.827Z",
      "updatedAt": "2024-09-09T04:55:32.827Z",
      "chapterId": "986b9aee-7bd9-466d-a8b0-0cdc3e507820"
    },
    {
      "id": "c2aa5b04-ec1f-4d31-81da-2a37bcc6bd06",
      "number": 20,
      "content": "Vì chim trời có thể chuyển tải tiếng nói của bạn,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 27,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:55:32.832Z",
      "updatedAt": "2024-09-09T04:55:32.832Z",
      "chapterId": "986b9aee-7bd9-466d-a8b0-0cdc3e507820"
    },
    {
      "id": "4574cddc-0429-4ead-850c-2f6d4a7456dc",
      "number": 20,
      "content": "Hoặc các loài có cánh có thể tường thuật lời của bạn.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 28,
      "isPoetry": true,
      "createdAt": "2024-09-09T04:55:32.837Z",
      "updatedAt": "2024-09-09T04:55:32.837Z",
      "chapterId": "986b9aee-7bd9-466d-a8b0-0cdc3e507820"
    }
  ],
