    },
    {
      "id": "62256fff-680b-407b-ba5f-98aeb01ff5fa",
      "content": "Giá Trị của Hình Tượng",
      "order": 0,
      "createdAt": "2024-07-15T04:10:33.874Z",
      "updatedAt": "2024-07-15T04:10:33.874Z",
