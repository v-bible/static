    },
    {
      "id": "6faeef02-61f1-4aca-b388-490008a81df3",
      "content": "Giá Trị của Hình Tượng",
      "order": 0,
      "createdAt": "2024-09-09T05:10:22.094Z",
      "updatedAt": "2024-09-09T05:10:22.094Z",
