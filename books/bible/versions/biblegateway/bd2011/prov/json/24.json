{
  "chapter": {
    "id": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33",
    "number": 24,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2024&version=BD2011",
    "createdAt": "2025-04-22T07:37:49.915Z",
    "updatedAt": "2025-04-22T07:37:49.915Z",
    "bookId": "eebb80de-01b2-4f4a-a1ff-0a76541f8302"
  },
  "verses": [
    {
      "id": "42c5a3ab-e5ac-4b18-a116-7297a16ed513",
      "number": 1,
      "content": "Ðừng ganh tị với phường gian ác,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.323Z",
      "updatedAt": "2025-04-22T08:39:49.323Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "8581ffa9-83b9-4da3-964d-9ca00daa4fa7",
      "number": 1,
      "content": "Cũng đừng mong cộng tác với chúng làm gì;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.340Z",
      "updatedAt": "2025-04-22T08:39:49.340Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "372d70bb-044e-41aa-a6a3-a6e5ff7ded36",
      "number": 2,
      "content": "Vì tâm trí chúng cứ toan tính chuyện bạo tàn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.350Z",
      "updatedAt": "2025-04-22T08:39:49.350Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "dab8846b-130e-4801-852d-50ef47921a77",
      "number": 2,
      "content": "Và môi miệng chúng nói toàn lời gây rắc rối.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.360Z",
      "updatedAt": "2025-04-22T08:39:49.360Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "04d23d0b-b7ea-4b36-8fce-104b7cefb88c",
      "number": 3,
      "content": "Nhờ khôn ngoan căn nhà được xây dựng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.371Z",
      "updatedAt": "2025-04-22T08:39:49.371Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "89d48dfc-c264-41f2-a5ad-1353d6adf35d",
      "number": 3,
      "content": "Nhờ thông sáng nó được thiết lập vững vàng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.381Z",
      "updatedAt": "2025-04-22T08:39:49.381Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "46178a00-be3b-4de7-abe3-4e5a6772d06b",
      "number": 4,
      "content": "Nhờ tri thức các phòng được đầy dẫy",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.392Z",
      "updatedAt": "2025-04-22T08:39:49.392Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "ff9892c7-b66d-4cba-baaf-5ad6f82f9b04",
      "number": 4,
      "content": "Mọi thứ của cải vừa quý lại vừa sang.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.404Z",
      "updatedAt": "2025-04-22T08:39:49.404Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "0bcf8eaa-1cc8-4a34-bf8b-24a82e4338ff",
      "number": 5,
      "content": "Người khôn ngoan quả có sức lực;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.417Z",
      "updatedAt": "2025-04-22T08:39:49.417Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "54d9cd51-21e2-4247-a761-f4180078f33b",
      "number": 5,
      "content": "Người tri thức quyền lực càng gia tăng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.430Z",
      "updatedAt": "2025-04-22T08:39:49.430Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "8ec7c383-40b3-456e-b176-d45e6cfa60a7",
      "number": 6,
      "content": "Con hãy dùng mưu chước khôn ngoan mà đánh giặc,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.444Z",
      "updatedAt": "2025-04-22T08:39:49.444Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "7e359b28-1df4-49a8-bcb1-4639a3e1f003",
      "number": 6,
      "content": "Con phải nhờ nhiều mưu sĩ giỏi mới có chiến thắng đem về.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.457Z",
      "updatedAt": "2025-04-22T08:39:49.457Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "8ba1a0e3-6ce6-479d-8e71-8d7a18adc9d3",
      "number": 7,
      "content": "Sự khôn ngoan thật quá cao xa với kẻ dại;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.471Z",
      "updatedAt": "2025-04-22T08:39:49.471Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "6b4051af-ab3c-43cf-8e07-f4639dcdbb7e",
      "number": 7,
      "content": "Ở cổng thành nó chẳng thể mở miệng nói ra lời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.484Z",
      "updatedAt": "2025-04-22T08:39:49.484Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "59f992a9-3c66-4d8f-b305-6e61a72f98e2",
      "number": 8,
      "content": "Kẻ nào mưu tính làm điều ác",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.497Z",
      "updatedAt": "2025-04-22T08:39:49.497Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "b74298af-c548-4af1-83b1-a3822a8d008e",
      "number": 8,
      "content": "Sẽ bị gọi là kẻ có thủ đoạn độc ác.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.510Z",
      "updatedAt": "2025-04-22T08:39:49.510Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "bed8312c-8589-4116-b658-e152b0c49052",
      "number": 9,
      "content": "Toan tính kế hoạch để làm chuyện điên rồ là tội lỗi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.523Z",
      "updatedAt": "2025-04-22T08:39:49.523Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "f4b08354-8b01-4416-955a-34a0f0ecb043",
      "number": 9,
      "content": "Kẻ nhạo báng bị thiên hạ ghê tởm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.536Z",
      "updatedAt": "2025-04-22T08:39:49.536Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "260f253e-9bfe-4642-8e48-64883c251c8d",
      "number": 10,
      "content": "Nếu con ngã lòng trong ngày nghịch cảnh,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.549Z",
      "updatedAt": "2025-04-22T08:39:49.549Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "dbe0633b-3d29-4b0d-b158-88e7ecf945b3",
      "number": 10,
      "content": "Sức lực con quả thật yếu thay.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.562Z",
      "updatedAt": "2025-04-22T08:39:49.562Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "87b5b0d1-d365-4646-b78a-1cb6bfdb981e",
      "number": 11,
      "content": "Hãy giải cứu những kẻ bị mang đi hành quyết,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.575Z",
      "updatedAt": "2025-04-22T08:39:49.575Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "e3738490-4e35-4c96-b724-8700cc7c78b5",
      "number": 11,
      "content": "Hãy giữ lại mạng những kẻ lảo đảo bước đến chỗ hành hình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.588Z",
      "updatedAt": "2025-04-22T08:39:49.588Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "9e56126a-ac11-49ab-aead-778c6d42d2b6",
      "number": 12,
      "content": "Nếu con nói, “Này, chúng tôi nào hay biết chuyện ấy.”",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.601Z",
      "updatedAt": "2025-04-22T08:39:49.601Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "02204597-f690-4ccb-bd45-2483bf705f07",
      "number": 12,
      "content": "Ðấng cân nhắc lòng người há không biết rõ lòng con sao?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.615Z",
      "updatedAt": "2025-04-22T08:39:49.615Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "67de2ccb-f82f-4791-a217-9c1ea5bb6952",
      "number": 12,
      "content": "Ðấng gìn giữ linh hồn con há không biết điều đó sao?",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.628Z",
      "updatedAt": "2025-04-22T08:39:49.628Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "5555afbe-1aee-45aa-8336-993556859f73",
      "number": 12,
      "content": "Ngài há chẳng báo trả mỗi người tùy theo việc họ làm sao?",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.641Z",
      "updatedAt": "2025-04-22T08:39:49.641Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "d80ae3f7-fbbd-447d-81bb-2733cdeef53a",
      "number": 13,
      "content": "Con ơi, hãy ăn mật ong, vì nó ngon ngọt lắm,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.653Z",
      "updatedAt": "2025-04-22T08:39:49.653Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "de34afe5-5691-4f4f-84a3-d797f2dc99ed",
      "number": 13,
      "content": "Những giọt mật nhểu ra từ tàng ong sẽ làm ngọt lịm lưỡi con,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.664Z",
      "updatedAt": "2025-04-22T08:39:49.664Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "e6cb4484-70c9-4f44-8cf7-349d660046f7",
      "number": 14,
      "content": "Hãy biết rằng khôn ngoan cũng ngọt ngon cho linh hồn như thế;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.678Z",
      "updatedAt": "2025-04-22T08:39:49.678Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "130040dd-b423-4106-a04a-8eab2ff5aebe",
      "number": 14,
      "content": "Nếu con tìm được khôn ngoan, con sẽ có một tương lai tốt đẹp,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.691Z",
      "updatedAt": "2025-04-22T08:39:49.691Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "46f72e6f-3af7-4c8c-a9ed-845de7b1f104",
      "number": 14,
      "content": "Hy vọng của con sẽ không bị cắt đứt giữa chừng.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.705Z",
      "updatedAt": "2025-04-22T08:39:49.705Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "e9a4ac0f-a0ce-4f3f-9037-4be020575c4a",
      "number": 15,
      "content": "Hỡi kẻ gian ác, chớ nằm đó rình nhà người ngay lành;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.718Z",
      "updatedAt": "2025-04-22T08:39:49.718Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "a5fc1ddb-b208-4f03-b68e-47438ca35e76",
      "number": 15,
      "content": "Ðừng xâm phạm nơi ở của người công chính.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.731Z",
      "updatedAt": "2025-04-22T08:39:49.731Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "24c7d2e7-5186-4e2b-b290-6fdd28a3fcfb",
      "number": 16,
      "content": "Vì dù họ ngã đến bảy lần, họ cũng đứng dậy được;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.745Z",
      "updatedAt": "2025-04-22T08:39:49.745Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "febcc271-c589-4e06-8667-a93555dad66d",
      "number": 16,
      "content": "Nhưng kẻ gian ác sẽ đổ nhào trong thì tai họa.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.758Z",
      "updatedAt": "2025-04-22T08:39:49.758Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "61388d12-9f88-4472-87e4-a390700cd63e",
      "number": 17,
      "content": "Ðừng vui mừng khi kẻ thù con ngã quỵ;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.771Z",
      "updatedAt": "2025-04-22T08:39:49.771Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "d896ad19-f9b8-4d44-83f4-b2b027a3bddd",
      "number": 17,
      "content": "Lòng con chớ hân hoan khi nó thất thế sa cơ.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.784Z",
      "updatedAt": "2025-04-22T08:39:49.784Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "65309047-bbad-46f9-a97c-835beb11c4db",
      "number": 18,
      "content": "Kẻo Chúa thấy điều đó và Ngài chẳng bằng lòng,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.797Z",
      "updatedAt": "2025-04-22T08:39:49.797Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "8e77e74e-5fd0-43e3-970a-59c9987223fa",
      "number": 18,
      "content": "Rồi Ngài ngưng cơn thịnh nộ khỏi nó chăng.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.810Z",
      "updatedAt": "2025-04-22T08:39:49.810Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "2d9be6e5-2ca4-4b2b-b370-2b6501133bbd",
      "number": 19,
      "content": "Ðừng bực bội vì những người gian ác,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.823Z",
      "updatedAt": "2025-04-22T08:39:49.823Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "8e9b077c-975e-4ddd-95c8-16b6d69d4011",
      "number": 19,
      "content": "Chớ phân bì khi kẻ ác thành công;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.836Z",
      "updatedAt": "2025-04-22T08:39:49.836Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "e7306e6d-afdc-4e84-8fe6-702b6c1f9446",
      "number": 20,
      "content": "Vì kẻ ác sẽ không có tương lai,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.849Z",
      "updatedAt": "2025-04-22T08:39:49.849Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "134e2ef6-2855-4e16-9287-f06b75b9d8e7",
      "number": 20,
      "content": "Ðèn kẻ ác sẽ nay mai tắt lịm.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.862Z",
      "updatedAt": "2025-04-22T08:39:49.862Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "5d332852-6d59-48ba-871c-ea6ebc1f56a5",
      "number": 21,
      "content": "Con ơi, hãy kính sợ Chúa và tôn kính vua;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.875Z",
      "updatedAt": "2025-04-22T08:39:49.875Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "6704d4cd-94ce-404c-9ca8-d39e93637478",
      "number": 21,
      "content": "Ðừng nhập bọn với đám người phản nghịch.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.889Z",
      "updatedAt": "2025-04-22T08:39:49.889Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "1c8f820d-af1b-4a98-a1ce-4484f9e79bfe",
      "number": 22,
      "content": "Vì tai họa của chúng sẽ thình lình vụt đến;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.902Z",
      "updatedAt": "2025-04-22T08:39:49.902Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "ba37aa67-d5b8-4914-a577-1b166c43dd17",
      "number": 22,
      "content": "Ai biết chúng sẽ tàn rụi ra sao khi cả hai cùng phạt?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.916Z",
      "updatedAt": "2025-04-22T08:39:49.916Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "39f87bb0-93b6-4034-8326-46babd78544e",
      "number": 23,
      "content": "Ðây cũng là những lời nói của người khôn ngoan:",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.929Z",
      "updatedAt": "2025-04-22T08:39:49.929Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "5f3e3c1f-c615-4828-a9a7-49810e021e86",
      "number": 23,
      "content": "Thiên vị khi xét xử là không tốt.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.956Z",
      "updatedAt": "2025-04-22T08:39:49.956Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "95fae978-73f9-4891-897b-b64c5a08931c",
      "number": 24,
      "content": "Ai nói với kẻ ác rằng, “Ngươi vô tội,”",
      "order": 0,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.969Z",
      "updatedAt": "2025-04-22T08:39:49.969Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "ab92857d-441b-4a47-9165-24867a3d16ba",
      "number": 24,
      "content": "Kẻ ấy sẽ bị dân nguyền rủa và các dân khác ghê tởm.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.982Z",
      "updatedAt": "2025-04-22T08:39:49.982Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "ea634d10-0d37-498e-88d9-c706b02ffabd",
      "number": 25,
      "content": "Nhưng ai quở trách kẻ ác sẽ được vui lòng,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:49.995Z",
      "updatedAt": "2025-04-22T08:39:49.995Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "f266d92e-cb7f-4381-b9a5-28cc94040749",
      "number": 25,
      "content": "Và phước hạnh sẽ đến với người ấy.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.008Z",
      "updatedAt": "2025-04-22T08:39:50.008Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "027032bf-2a2b-4f95-8a58-f278e3bcfb23",
      "number": 26,
      "content": "Lời đáp chính xác giống như một nụ hôn hôn trên môi.",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.022Z",
      "updatedAt": "2025-04-22T08:39:50.022Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "c8808920-b3e3-4e63-a154-b15c392f2b83",
      "number": 27,
      "content": "Hãy sắp đặt công việc của con ở bên ngoài,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.035Z",
      "updatedAt": "2025-04-22T08:39:50.035Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "8cea9d40-e2f6-4fdc-903f-09597511fcdd",
      "number": 27,
      "content": "Hãy chuẩn bị mọi việc của con ngoài đồng áng cho chu tất;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.047Z",
      "updatedAt": "2025-04-22T08:39:50.047Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "b768e2ee-32da-4860-988f-c09054639e32",
      "number": 27,
      "content": "Rồi sau đó mới bắt đầu xây cất nhà cho con.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.059Z",
      "updatedAt": "2025-04-22T08:39:50.059Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "c56d5581-4ca3-4855-8a75-556f8af5ce87",
      "number": 28,
      "content": "Ðừng làm chứng nghịch lại người lân cận con vô cớ;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.070Z",
      "updatedAt": "2025-04-22T08:39:50.070Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "e49abcc6-ab39-4a25-9c81-2acf5cb2a672",
      "number": 28,
      "content": "Ðừng để môi con nói những lời dối trá hại người.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.080Z",
      "updatedAt": "2025-04-22T08:39:50.080Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "e8a1c1d4-9e6c-4680-8bb3-5d299678ea79",
      "number": 29,
      "content": "Con đừng nói, “Ta sẽ làm cho nó như nó đã làm cho ta;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.092Z",
      "updatedAt": "2025-04-22T08:39:50.092Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "0cd73496-d3aa-4391-8e86-606955660828",
      "number": 29,
      "content": "Ta sẽ báo trả cho nó những gì nó đã làm.”",
      "order": 1,
      "parNumber": 13,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.105Z",
      "updatedAt": "2025-04-22T08:39:50.105Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "63f8057a-b217-4dba-86ea-377f2d6701da",
      "number": 30,
      "content": "Ta có đi ngang qua cánh đồng của một kẻ biếng nhác;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.118Z",
      "updatedAt": "2025-04-22T08:39:50.118Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "733f6cba-699a-445f-9141-2a0524294453",
      "number": 30,
      "content": "Đi ngang qua vườn nho của một kẻ thiếu hiểu biết thường tình;",
      "order": 1,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.131Z",
      "updatedAt": "2025-04-22T08:39:50.131Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "746d5e24-b0b8-48b9-9437-b94aca789689",
      "number": 31,
      "content": "Kìa, gai góc mọc tràn lan khắp chốn,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.144Z",
      "updatedAt": "2025-04-22T08:39:50.144Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "c93c0801-39be-4737-a9d3-4b5130fe9cfe",
      "number": 31,
      "content": "Các bụi gai phủ mặt đất um tùm, còn tường đá thì chỗ xiêu chỗ đổ.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.160Z",
      "updatedAt": "2025-04-22T08:39:50.160Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "9fbe5dcc-4075-4c0e-880c-559233c1dc71",
      "number": 32,
      "content": "Thấy như thế ta chạnh lòng suy nghĩ;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.173Z",
      "updatedAt": "2025-04-22T08:39:50.173Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "ce561643-8480-458f-97e0-72bce095c952",
      "number": 32,
      "content": "Ta nhìn xem và gẫm được một bài học ở đời:",
      "order": 1,
      "parNumber": 14,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.186Z",
      "updatedAt": "2025-04-22T08:39:50.186Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "9a6cc927-08f2-4119-babd-195d205b3253",
      "number": 33,
      "content": "Ngủ một chút, ngủ nướng thêm một chút,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.200Z",
      "updatedAt": "2025-04-22T08:39:50.200Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "bafc9227-b175-4fb9-a917-e0ffabb775c5",
      "number": 33,
      "content": "Khoanh tay nằm nghỉ một chút,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.213Z",
      "updatedAt": "2025-04-22T08:39:50.213Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "cf4ae3e7-c40e-4718-80b1-79158a71300b",
      "number": 34,
      "content": "Thì cái nghèo sẽ vụt đến như tên cướp bất chợt tấn công,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.226Z",
      "updatedAt": "2025-04-22T08:39:50.226Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    },
    {
      "id": "7a493bce-b5bc-4f4d-bd53-c616472e12bf",
      "number": 34,
      "content": "Và cái thiếu sẽ ập đến như kẻ ác có vũ khí bất thần đột kích.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:50.239Z",
      "updatedAt": "2025-04-22T08:39:50.239Z",
      "chapterId": "aba01d6e-5c1e-41bb-9173-f6f53e6e3d33"
    }
  ],
