{
  "chapter": {
    "id": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852",
    "number": 15,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2015&version=BD2011",
    "createdAt": "2025-04-24T03:03:19.084Z",
    "updatedAt": "2025-04-24T03:03:19.084Z",
    "bookId": "1b06f6cc-342f-4633-b290-6d1686eb4b30"
  },
  "verses": [
    {
      "id": "9426cd16-5de3-4584-8def-37f97eed52d1",
      "number": 1,
      "content": "Lời đáp êm dịu làm nguôi cơn giận;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.564Z",
      "updatedAt": "2025-04-24T04:07:18.564Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "ca0ba7d7-b127-4e50-a330-121605c243ab",
      "number": 1,
      "content": "Còn lời xẳng xớm gây phẫn nộ thêm.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.581Z",
      "updatedAt": "2025-04-24T04:07:18.581Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "284c7fe5-0a7a-4bcc-9fc6-6487022dcd83",
      "number": 2,
      "content": "Lưỡi người khôn ngoan phát ban tri thức;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.591Z",
      "updatedAt": "2025-04-24T04:07:18.591Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "2a3d88bb-403f-4413-ab02-8443fa1082fd",
      "number": 2,
      "content": "Miệng kẻ dại dột thốt chuyện điên rồ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.601Z",
      "updatedAt": "2025-04-24T04:07:18.601Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "42367ff6-6cda-4584-9917-0a2b18b7f161",
      "number": 3,
      "content": "Mắt Chúa thấy rõ khắp nơi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.612Z",
      "updatedAt": "2025-04-24T04:07:18.612Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "8e25ff23-f092-43f6-8cb6-98afb8557abb",
      "number": 3,
      "content": "Thấy tất cả kẻ xấu và người tốt.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.622Z",
      "updatedAt": "2025-04-24T04:07:18.622Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "08bf8d31-a373-43b4-847b-8dc1c1243d62",
      "number": 4,
      "content": "Lưỡi hiền lành là cây sự sống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.634Z",
      "updatedAt": "2025-04-24T04:07:18.634Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "6dcbc5cb-41df-482d-8802-61b68bdab24a",
      "number": 4,
      "content": "Còn lưỡi độc ác làm tan nát tinh thần.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.647Z",
      "updatedAt": "2025-04-24T04:07:18.647Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "eac9306d-0b42-4471-89ea-75d6174acd97",
      "number": 5,
      "content": "Kẻ dại dột khinh lời cha khuyên dạy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.660Z",
      "updatedAt": "2025-04-24T04:07:18.660Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "61c648b3-8939-42a4-9b0c-920562067149",
      "number": 5,
      "content": "Còn người khôn nghe lời quở trách khuyên răn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.673Z",
      "updatedAt": "2025-04-24T04:07:18.673Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "ba107372-1500-4e2c-96ea-1142c3ea7768",
      "number": 6,
      "content": "Trong nhà người ngay lành có nhiều của cải;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.686Z",
      "updatedAt": "2025-04-24T04:07:18.686Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "357b84eb-04f7-481c-adea-00c1b9ce4118",
      "number": 6,
      "content": "Còn trong lợi tức kẻ ác có rắc rối chờ cơ hội xảy ra.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.699Z",
      "updatedAt": "2025-04-24T04:07:18.699Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "f9ea595f-74c5-43d7-86de-2d3039916404",
      "number": 7,
      "content": "Môi người khôn ngoan loan truyền tri thức;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.712Z",
      "updatedAt": "2025-04-24T04:07:18.712Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "333e69be-e69c-45f8-8cf3-da746bcaf53d",
      "number": 7,
      "content": "Còn lòng kẻ dại dột ấm ức nhưng chẳng có kiến thức gì.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.726Z",
      "updatedAt": "2025-04-24T04:07:18.726Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "ec283d7d-e1e2-4ced-ad76-df6f8dcb4e5c",
      "number": 8,
      "content": "Lễ vật của kẻ gian ác là gớm ghiếc trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.739Z",
      "updatedAt": "2025-04-24T04:07:18.739Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "8a86f87d-fa7a-4ad7-88d6-5eb2d1bde32c",
      "number": 8,
      "content": "Nhưng lời cầu nguyện của người ngay lành thì Ngài lại vui lòng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.752Z",
      "updatedAt": "2025-04-24T04:07:18.752Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "c6a04a63-9beb-4000-bd22-7f11bdeeb952",
      "number": 9,
      "content": "Ðường lối kẻ gian ác là gớm ghiếc trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.765Z",
      "updatedAt": "2025-04-24T04:07:18.765Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "1924b8f2-8733-4010-8cd4-6fe4bf83d261",
      "number": 9,
      "content": "Nhưng Ngài yêu thương người đeo đuổi sự ngay lành.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.778Z",
      "updatedAt": "2025-04-24T04:07:18.778Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "2686b8be-81b3-4b58-89e3-1885d336e2e2",
      "number": 10,
      "content": "Sự nghiêm phạt dành cho kẻ bỏ đường ngay nẻo chánh,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.792Z",
      "updatedAt": "2025-04-24T04:07:18.792Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "e6b39c18-1fcc-48dc-8748-6591df40677a",
      "number": 10,
      "content": "Ai ghét sự khuyên răn chẳng tránh khỏi chết đâu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.805Z",
      "updatedAt": "2025-04-24T04:07:18.805Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "964e0fee-28c0-4fbd-a79d-e587f5c7a01a",
      "number": 11,
      "content": "Ngay cả âm phủ và hỏa ngục mà Chúa còn biết rõ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.819Z",
      "updatedAt": "2025-04-24T04:07:18.819Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "47bc58b2-876d-433a-866f-c9711dc159d8",
      "number": 11,
      "content": "Thì huống chi là lòng dạ loài người.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.832Z",
      "updatedAt": "2025-04-24T04:07:18.832Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "1b969d0c-13dd-422a-86cf-d896740a86b1",
      "number": 12,
      "content": "Kẻ nhạo báng không thích bị ai quở trách;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.845Z",
      "updatedAt": "2025-04-24T04:07:18.845Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "22c067c3-9105-4b07-a201-f1e371ed84e5",
      "number": 12,
      "content": "Nó tránh đến với người khôn để học sự khôn ngoan.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.859Z",
      "updatedAt": "2025-04-24T04:07:18.859Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "2b1ff7ef-e94f-4ef6-8419-34a95a281c8d",
      "number": 13,
      "content": "Lòng vui vẻ khiến mặt mày rạng rỡ;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.872Z",
      "updatedAt": "2025-04-24T04:07:18.872Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "dfb6baa8-1bb1-47ea-a1db-cfd7a0c18dd8",
      "number": 13,
      "content": "Dạ âu sầu làm tan vỡ tinh thần.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.885Z",
      "updatedAt": "2025-04-24T04:07:18.885Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "10663aab-7f66-4e3c-b3aa-56c3fc6408f1",
      "number": 14,
      "content": "Lòng người thông sáng thích kiếm tìm tri thức;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.898Z",
      "updatedAt": "2025-04-24T04:07:18.898Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "696e3a7c-1e3e-4399-ac1b-1e5b2fd98946",
      "number": 14,
      "content": "Miệng kẻ dại khờ ưa ẩm thực điên rồ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.912Z",
      "updatedAt": "2025-04-24T04:07:18.912Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "436e1ea9-613d-4231-b01e-b7e1f3ff730c",
      "number": 15,
      "content": "Kẻ nghèo khó suốt đời chịu khánh kiệt;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.925Z",
      "updatedAt": "2025-04-24T04:07:18.925Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "3f83c02a-785a-402f-b2d0-e7e06b360798",
      "number": 15,
      "content": "Người vui vẻ trong lòng dự yến tiệc luôn luôn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.941Z",
      "updatedAt": "2025-04-24T04:07:18.941Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "b278646f-95a8-42cc-bf8e-f0d38f34a4d2",
      "number": 16,
      "content": "Thà ít của mà kính sợ Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.955Z",
      "updatedAt": "2025-04-24T04:07:18.955Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "af3af913-640e-498d-b56d-41b17e59da52",
      "number": 16,
      "content": "Hơn của cải nhiều mà rắc rối ùa theo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.968Z",
      "updatedAt": "2025-04-24T04:07:18.968Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "3bdecbe2-861d-4696-bc46-64b17a1ca85d",
      "number": 17,
      "content": "Thà ăn cơm với rau mà thương yêu nhau,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.981Z",
      "updatedAt": "2025-04-24T04:07:18.981Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "90049999-576c-498d-9036-f83e29c344ec",
      "number": 17,
      "content": "Hơn là ăn thịt bò mập béo mà ghen ghét cặp theo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:18.994Z",
      "updatedAt": "2025-04-24T04:07:18.994Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "f1f499be-4898-49cc-8aa5-adc43f65e8c0",
      "number": 18,
      "content": "Người nóng tính dễ gây xung đột;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.007Z",
      "updatedAt": "2025-04-24T04:07:19.007Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "0a628b67-eece-4af2-8300-d214931b26e2",
      "number": 18,
      "content": "Còn người chậm nóng giận làm cuộc cãi lộn nguôi đi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.020Z",
      "updatedAt": "2025-04-24T04:07:19.020Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "1e1e0d07-d800-4bd3-af01-6571bb6c2ebb",
      "number": 19,
      "content": "Đường kẻ biếng nhác gai góc mọc đầy cản trở;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.034Z",
      "updatedAt": "2025-04-24T04:07:19.034Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "3be9619f-651c-43c6-ab83-b398237288b0",
      "number": 19,
      "content": "Còn lối người ngay lành lại mở rộng thênh thang.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.048Z",
      "updatedAt": "2025-04-24T04:07:19.048Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "22c32bd9-212e-4baf-85d8-ee51395fe21d",
      "number": 20,
      "content": "Ðứa con khôn ngoan làm cho cha vui vẻ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.061Z",
      "updatedAt": "2025-04-24T04:07:19.061Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "5ae3f8e0-d3fb-4fd1-9083-5bddd07828e0",
      "number": 20,
      "content": "Còn kẻ dại dột lại khinh rẻ mẹ mình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.074Z",
      "updatedAt": "2025-04-24T04:07:19.074Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "fd74f9aa-adcc-4453-802b-8f7c18fa5764",
      "number": 21,
      "content": "Kẻ kém hiểu biết cho sự điên dại là vui;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.090Z",
      "updatedAt": "2025-04-24T04:07:19.090Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "ac52aae0-0412-44f4-a617-5d0c92c7a6d6",
      "number": 21,
      "content": "Nhưng người có trí tuệ cứ bước theo đường chính đáng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.104Z",
      "updatedAt": "2025-04-24T04:07:19.104Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "b25fd8ad-64bc-4979-86d7-37bfd8fb5d12",
      "number": 22,
      "content": "Thiếu bàn luận các chương trình bị hỏng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.117Z",
      "updatedAt": "2025-04-24T04:07:19.117Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "abe2af5d-b9e7-4627-86fc-5314e575d888",
      "number": 22,
      "content": "Nhưng với mưu sĩ nhiều chúng có triển vọng thành công.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.130Z",
      "updatedAt": "2025-04-24T04:07:19.130Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "301a51f8-de9e-4269-8603-d67518ee4802",
      "number": 23,
      "content": "Lời đáp hay làm mọi người vui thích;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.143Z",
      "updatedAt": "2025-04-24T04:07:19.143Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "d0d7a16a-e46a-4008-9ddc-b89314dfb99a",
      "number": 23,
      "content": "Lời đáp hay đúng lúc thật bổ ích biết bao!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.156Z",
      "updatedAt": "2025-04-24T04:07:19.156Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "e5a7e698-49c9-4463-8aec-8b6a01e52779",
      "number": 24,
      "content": "Người khôn ngoan bước theo con đường sự sống dẫn đi lên,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.169Z",
      "updatedAt": "2025-04-24T04:07:19.169Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "4db2dd66-ecfe-46cb-9173-efb3edfb5e41",
      "number": 24,
      "content": "Ðể giữ người ấy tránh xa âm phủ ở phía dưới.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.183Z",
      "updatedAt": "2025-04-24T04:07:19.183Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "3601458e-7444-40a0-a0da-85ce0e9bf396",
      "number": 25,
      "content": "Chúa phá hủy nhà của kẻ kiêu ngạo;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.196Z",
      "updatedAt": "2025-04-24T04:07:19.196Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "e629e052-4d9b-4d07-9510-d753208f18bb",
      "number": 25,
      "content": "Nhưng Ngài bảo vệ địa giới của người góa bụa.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.209Z",
      "updatedAt": "2025-04-24T04:07:19.209Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "67540a22-f248-48af-be2d-1c3f700b8d6e",
      "number": 26,
      "content": "Ý đồ của kẻ gian ác là gớm ghiếc trước mặt Chúa;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.222Z",
      "updatedAt": "2025-04-24T04:07:19.222Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "0b860571-8f5e-42fa-8244-43a7fc47a8a5",
      "number": 26,
      "content": "Nhưng lời của người trong sạch được đẹp lòng Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.235Z",
      "updatedAt": "2025-04-24T04:07:19.235Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "d2034b90-a64b-4851-81e3-8f1f87ddcb0b",
      "number": 27,
      "content": "Kẻ tham lợi bất nghĩa chuốc lấy rắc rối cho nhà mình;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.249Z",
      "updatedAt": "2025-04-24T04:07:19.249Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "a29a8178-c09b-4fe6-907e-4fd729d8cbfa",
      "number": 27,
      "content": "Nhưng ai ghét của hối lộ sẽ tiếp tục sống còn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.262Z",
      "updatedAt": "2025-04-24T04:07:19.262Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "6ac43fc6-62fa-4331-8135-d78274806c72",
      "number": 28,
      "content": "Người ngay lành suy nghĩ trong lòng rồi mới đáp;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.275Z",
      "updatedAt": "2025-04-24T04:07:19.275Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "e6da9c0e-741d-478e-8be7-36b047832bde",
      "number": 28,
      "content": "Còn miệng kẻ gian ác vội nói những lời độc ác hại người.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.291Z",
      "updatedAt": "2025-04-24T04:07:19.291Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "9d068217-36e5-4622-b916-551f3288936d",
      "number": 29,
      "content": "Chúa thật cách xa đối với phường gian ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.304Z",
      "updatedAt": "2025-04-24T04:07:19.304Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "1e9a1daf-5373-499c-bc14-3c2ec4ecb0de",
      "number": 29,
      "content": "Nhưng Ngài lắng nghe lời cầu nguyện của người ngay lành.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.317Z",
      "updatedAt": "2025-04-24T04:07:19.317Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "2af95697-7b1c-41d7-bdc2-8435c45d1f7c",
      "number": 30,
      "content": "Ánh mắt hân hoan phản ánh lòng vui vẻ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.330Z",
      "updatedAt": "2025-04-24T04:07:19.330Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "52d9158a-3b1a-4d4c-9022-e8e5c538c93c",
      "number": 30,
      "content": "Tin tức vui mừng làm mạnh khỏe cốt xương.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.344Z",
      "updatedAt": "2025-04-24T04:07:19.344Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "76aa3fa8-a7da-4d8c-904a-2038757bd554",
      "number": 31,
      "content": "Tai chịu lắng nghe những lời quở trách về cuộc sống,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.357Z",
      "updatedAt": "2025-04-24T04:07:19.357Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "23333239-6ead-4272-990a-f6685eb37d70",
      "number": 31,
      "content": "Người ấy sẽ ở giữa các bậc hiền triết khôn ngoan.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.371Z",
      "updatedAt": "2025-04-24T04:07:19.371Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "c3e4d9e6-ed0c-4ed8-8e89-e04d986273d6",
      "number": 32,
      "content": "Kẻ từ chối nghe lời chỉ giáo khinh bỉ linh hồn mình,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.384Z",
      "updatedAt": "2025-04-24T04:07:19.384Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "559ac45f-2211-4c4b-b495-8d723c3edef9",
      "number": 32,
      "content": "Nhưng ai chịu nghe lời khuyên bảo sẽ được sự thông sáng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.397Z",
      "updatedAt": "2025-04-24T04:07:19.397Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "863e7b85-4203-4fc1-bb06-06d6474b6a5b",
      "number": 33,
      "content": "Kính sợ Chúa là cách giáo dục khôn ngoan,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.411Z",
      "updatedAt": "2025-04-24T04:07:19.411Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    },
    {
      "id": "7459e5bc-8171-4738-86ea-696d908a36c9",
      "number": 33,
      "content": "Ðức khiêm nhường đi trước sự tôn trọng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 23,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:19.424Z",
      "updatedAt": "2025-04-24T04:07:19.424Z",
      "chapterId": "a2900c32-9ecb-4dbd-bdf2-60903f4d9852"
    }
  ],
