{
  "chapter": {
    "id": "576f5bd7-b946-4ae4-b47b-8c5842bf3750",
    "number": 15,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2015&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.722Z",
    "updatedAt": "2024-09-24T01:00:45.722Z",
    "bookId": "2a7cfb76-854f-4125-8376-cd1d8bb799d7"
  },
  "verses": [
    {
      "id": "af70d042-e37f-4e80-ac29-50712c4b3651",
      "number": 1,
      "content": "Lời đáp êm dịu làm nguôi cơn giận;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.245Z",
      "updatedAt": "2024-09-24T02:15:09.245Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "d708c5a9-090f-4677-bdd3-8a388bd13de3",
      "number": 1,
      "content": "Còn lời xẳng xớm gây phẫn nộ thêm.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.254Z",
      "updatedAt": "2024-09-24T02:15:09.254Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "fb860133-1c78-4ae9-9a1d-5c42fb4507e5",
      "number": 2,
      "content": "Lưỡi người khôn ngoan phát ban tri thức;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.257Z",
      "updatedAt": "2024-09-24T02:15:09.257Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "719b0b94-1d3a-41be-b6f6-52cab8900753",
      "number": 2,
      "content": "Miệng kẻ dại dột thốt chuyện điên rồ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.259Z",
      "updatedAt": "2024-09-24T02:15:09.259Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "6075fb8e-1f36-4dfc-acb9-311ee3657578",
      "number": 3,
      "content": "Mắt Chúa thấy rõ khắp nơi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.261Z",
      "updatedAt": "2024-09-24T02:15:09.261Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "80e03076-88be-44b0-8344-2de7b8b19764",
      "number": 3,
      "content": "Thấy tất cả kẻ xấu và người tốt.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.263Z",
      "updatedAt": "2024-09-24T02:15:09.263Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "4c68a596-91b0-41d3-b4f1-c1c18285dcb4",
      "number": 4,
      "content": "Lưỡi hiền lành là cây sự sống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.265Z",
      "updatedAt": "2024-09-24T02:15:09.265Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "1163fb1a-f7e7-4616-af89-75e4341d7793",
      "number": 4,
      "content": "Còn lưỡi độc ác làm tan nát tinh thần.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.267Z",
      "updatedAt": "2024-09-24T02:15:09.267Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "ae421130-4c10-42d9-9ee7-e1d146e29358",
      "number": 5,
      "content": "Kẻ dại dột khinh lời cha khuyên dạy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.270Z",
      "updatedAt": "2024-09-24T02:15:09.270Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "53719afb-b255-40c4-b743-096c4e67cfe0",
      "number": 5,
      "content": "Còn người khôn nghe lời quở trách khuyên răn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.272Z",
      "updatedAt": "2024-09-24T02:15:09.272Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "8b33553c-01b6-4482-960d-9519d417e448",
      "number": 6,
      "content": "Trong nhà người ngay lành có nhiều của cải;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.274Z",
      "updatedAt": "2024-09-24T02:15:09.274Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "be7aac61-8df2-4219-9118-bac215045388",
      "number": 6,
      "content": "Còn trong lợi tức kẻ ác có rắc rối chờ cơ hội xảy ra.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.276Z",
      "updatedAt": "2024-09-24T02:15:09.276Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "8fc05903-80f2-464c-a56d-8f3ee1fe011d",
      "number": 7,
      "content": "Môi người khôn ngoan loan truyền tri thức;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.278Z",
      "updatedAt": "2024-09-24T02:15:09.278Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "b2690162-946b-4bf7-a024-ed14b17f6e36",
      "number": 7,
      "content": "Còn lòng kẻ dại dột ấm ức nhưng chẳng có kiến thức gì.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.281Z",
      "updatedAt": "2024-09-24T02:15:09.281Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "0e4100dd-62f0-498f-b498-fdae2427981f",
      "number": 8,
      "content": "Lễ vật của kẻ gian ác là gớm ghiếc trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.286Z",
      "updatedAt": "2024-09-24T02:15:09.286Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "07268319-5dd5-4ba5-a043-e8ea50a0c436",
      "number": 8,
      "content": "Nhưng lời cầu nguyện của người ngay lành thì Ngài lại vui lòng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.291Z",
      "updatedAt": "2024-09-24T02:15:09.291Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "fe6518d5-5777-4b6a-b7a3-a2a6d3afb323",
      "number": 9,
      "content": "Ðường lối kẻ gian ác là gớm ghiếc trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.297Z",
      "updatedAt": "2024-09-24T02:15:09.297Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "99cfbbb5-b00f-4531-91d4-a1f6782b5fea",
      "number": 9,
      "content": "Nhưng Ngài yêu thương người đeo đuổi sự ngay lành.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.302Z",
      "updatedAt": "2024-09-24T02:15:09.302Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "e0387bf3-8764-4797-851d-52b8728d3c28",
      "number": 10,
      "content": "Sự nghiêm phạt dành cho kẻ bỏ đường ngay nẻo chánh,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.307Z",
      "updatedAt": "2024-09-24T02:15:09.307Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "7c452968-00f2-4bb1-880d-84faac6afdcc",
      "number": 10,
      "content": "Ai ghét sự khuyên răn chẳng tránh khỏi chết đâu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.312Z",
      "updatedAt": "2024-09-24T02:15:09.312Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "82fb1b24-7ce5-4dd3-81b9-d6e3d2eaf87d",
      "number": 11,
      "content": "Ngay cả âm phủ và hỏa ngục mà Chúa còn biết rõ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.317Z",
      "updatedAt": "2024-09-24T02:15:09.317Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "61c4f859-fb41-4a99-bfc2-5f31661c56fd",
      "number": 11,
      "content": "Thì huống chi là lòng dạ loài người.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.322Z",
      "updatedAt": "2024-09-24T02:15:09.322Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "8b1b2f5e-9561-4ecb-a04f-864b4df328c8",
      "number": 12,
      "content": "Kẻ nhạo báng không thích bị ai quở trách;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.327Z",
      "updatedAt": "2024-09-24T02:15:09.327Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "66681d7c-7ff1-429b-9cff-63acb51c5e36",
      "number": 12,
      "content": "Nó tránh đến với người khôn để học sự khôn ngoan.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.331Z",
      "updatedAt": "2024-09-24T02:15:09.331Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "cacfbc0b-f756-4688-a7ef-24dbb2fc45b3",
      "number": 13,
      "content": "Lòng vui vẻ khiến mặt mày rạng rỡ;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.336Z",
      "updatedAt": "2024-09-24T02:15:09.336Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "f2149218-b099-4f28-a4d4-f2f6e65bad1d",
      "number": 13,
      "content": "Dạ âu sầu làm tan vỡ tinh thần.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.341Z",
      "updatedAt": "2024-09-24T02:15:09.341Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "c1851df3-2b3c-4652-9f66-25cd895876f5",
      "number": 14,
      "content": "Lòng người thông sáng thích kiếm tìm tri thức;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.346Z",
      "updatedAt": "2024-09-24T02:15:09.346Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "e2a80170-d214-44e1-931f-0a10de31fc1c",
      "number": 14,
      "content": "Miệng kẻ dại khờ ưa ẩm thực điên rồ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.351Z",
      "updatedAt": "2024-09-24T02:15:09.351Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "906a282a-b0c1-42ae-b8e0-041fdff1f31c",
      "number": 15,
      "content": "Kẻ nghèo khó suốt đời chịu khánh kiệt;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.355Z",
      "updatedAt": "2024-09-24T02:15:09.355Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "7e964d3f-470a-4f54-8011-7c1b3ae919b9",
      "number": 15,
      "content": "Người vui vẻ trong lòng dự yến tiệc luôn luôn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.360Z",
      "updatedAt": "2024-09-24T02:15:09.360Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "04d83972-dfda-4b13-915b-9d5d6874d298",
      "number": 16,
      "content": "Thà ít của mà kính sợ Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.365Z",
      "updatedAt": "2024-09-24T02:15:09.365Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "369ba9da-ad82-4439-b51c-1e431e2bc554",
      "number": 16,
      "content": "Hơn của cải nhiều mà rắc rối ùa theo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.370Z",
      "updatedAt": "2024-09-24T02:15:09.370Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "d949f43a-0895-40bb-a3c7-3522550b70fb",
      "number": 17,
      "content": "Thà ăn cơm với rau mà thương yêu nhau,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.374Z",
      "updatedAt": "2024-09-24T02:15:09.374Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "774db7c6-778f-4889-87a8-b4e3346ae7a2",
      "number": 17,
      "content": "Hơn là ăn thịt bò mập béo mà ghen ghét cặp theo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.379Z",
      "updatedAt": "2024-09-24T02:15:09.379Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "27cca0e0-8732-466c-b856-31134bc51593",
      "number": 18,
      "content": "Người nóng tính dễ gây xung đột;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.385Z",
      "updatedAt": "2024-09-24T02:15:09.385Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "626ae985-e0b6-4bd0-a1e6-3ab75f569ea8",
      "number": 18,
      "content": "Còn người chậm nóng giận làm cuộc cãi lộn nguôi đi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.390Z",
      "updatedAt": "2024-09-24T02:15:09.390Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "d0302ff0-a85a-432c-851c-da891d0cbef0",
      "number": 19,
      "content": "Đường kẻ biếng nhác gai góc mọc đầy cản trở;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.395Z",
      "updatedAt": "2024-09-24T02:15:09.395Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "40b433f4-728c-463e-8361-7ef7596bed46",
      "number": 19,
      "content": "Còn lối người ngay lành lại mở rộng thênh thang.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.399Z",
      "updatedAt": "2024-09-24T02:15:09.399Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "a51e7895-be53-4653-9e27-31737ca9cfc8",
      "number": 20,
      "content": "Ðứa con khôn ngoan làm cho cha vui vẻ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.404Z",
      "updatedAt": "2024-09-24T02:15:09.404Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "c8ea0300-8abd-4a6f-b764-cf5abd30aac4",
      "number": 20,
      "content": "Còn kẻ dại dột lại khinh rẻ mẹ mình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.409Z",
      "updatedAt": "2024-09-24T02:15:09.409Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "aee6cbd4-8d89-4131-9577-9cfc44a2cee4",
      "number": 21,
      "content": "Kẻ kém hiểu biết cho sự điên dại là vui;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.414Z",
      "updatedAt": "2024-09-24T02:15:09.414Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "9cf51d4e-03b9-4b55-84f6-e25e88050b16",
      "number": 21,
      "content": "Nhưng người có trí tuệ cứ bước theo đường chính đáng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.419Z",
      "updatedAt": "2024-09-24T02:15:09.419Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "03e92283-56ff-4e10-a53d-d6618e76934a",
      "number": 22,
      "content": "Thiếu bàn luận các chương trình bị hỏng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.423Z",
      "updatedAt": "2024-09-24T02:15:09.423Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "ddccc24a-a059-41b0-9b48-c42c0b64cdcd",
      "number": 22,
      "content": "Nhưng với mưu sĩ nhiều chúng có triển vọng thành công.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.428Z",
      "updatedAt": "2024-09-24T02:15:09.428Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "0449aa1b-5650-47b6-bfa1-6e65e9fd36af",
      "number": 23,
      "content": "Lời đáp hay làm mọi người vui thích;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.433Z",
      "updatedAt": "2024-09-24T02:15:09.433Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "33fc9987-95f2-4a23-aa7e-16e15d0b4d38",
      "number": 23,
      "content": "Lời đáp hay đúng lúc thật bổ ích biết bao!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.438Z",
      "updatedAt": "2024-09-24T02:15:09.438Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "ff067b9b-c76b-461a-9045-9e32a315481e",
      "number": 24,
      "content": "Người khôn ngoan bước theo con đường sự sống dẫn đi lên,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.442Z",
      "updatedAt": "2024-09-24T02:15:09.442Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "c28d1b97-94c2-497a-bfe8-5a0be001adcb",
      "number": 24,
      "content": "Ðể giữ người ấy tránh xa âm phủ ở phía dưới.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.447Z",
      "updatedAt": "2024-09-24T02:15:09.447Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "37c53d56-ffba-4998-b04d-fc22edfb0643",
      "number": 25,
      "content": "Chúa phá hủy nhà của kẻ kiêu ngạo;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.452Z",
      "updatedAt": "2024-09-24T02:15:09.452Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "3858ad67-b0f0-46fd-85df-6cb44072bf3e",
      "number": 25,
      "content": "Nhưng Ngài bảo vệ địa giới của người góa bụa.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.456Z",
      "updatedAt": "2024-09-24T02:15:09.456Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "0096e1e3-f226-4a63-9342-cfde1f90f0c6",
      "number": 26,
      "content": "Ý đồ của kẻ gian ác là gớm ghiếc trước mặt Chúa;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.461Z",
      "updatedAt": "2024-09-24T02:15:09.461Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "b8c06deb-5630-4ecc-8e04-f9485f39f9ad",
      "number": 26,
      "content": "Nhưng lời của người trong sạch được đẹp lòng Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.466Z",
      "updatedAt": "2024-09-24T02:15:09.466Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "131d33e1-7f88-4db0-832d-89d3945b66b2",
      "number": 27,
      "content": "Kẻ tham lợi bất nghĩa chuốc lấy rắc rối cho nhà mình;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.471Z",
      "updatedAt": "2024-09-24T02:15:09.471Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "775757e3-b3c6-4f3d-873e-abbf4effa505",
      "number": 27,
      "content": "Nhưng ai ghét của hối lộ sẽ tiếp tục sống còn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.475Z",
      "updatedAt": "2024-09-24T02:15:09.475Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "7baf8165-cf57-4252-9b8d-fdf55a1d88c3",
      "number": 28,
      "content": "Người ngay lành suy nghĩ trong lòng rồi mới đáp;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.480Z",
      "updatedAt": "2024-09-24T02:15:09.480Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "383764df-bea8-4b92-abe3-7fd5f508f165",
      "number": 28,
      "content": "Còn miệng kẻ gian ác vội nói những lời độc ác hại người.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.485Z",
      "updatedAt": "2024-09-24T02:15:09.485Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "39f75f1c-7267-4d7f-b0cc-101bca375f3e",
      "number": 29,
      "content": "Chúa thật cách xa đối với phường gian ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.490Z",
      "updatedAt": "2024-09-24T02:15:09.490Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "b5ae992e-0175-4b37-a2ba-2e12dc8246ea",
      "number": 29,
      "content": "Nhưng Ngài lắng nghe lời cầu nguyện của người ngay lành.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.494Z",
      "updatedAt": "2024-09-24T02:15:09.494Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "a9550eb3-164f-4681-92bc-dba8b433cff9",
      "number": 30,
      "content": "Ánh mắt hân hoan phản ánh lòng vui vẻ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.500Z",
      "updatedAt": "2024-09-24T02:15:09.500Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "28958669-6bbe-4ee9-8869-f538b73e57fa",
      "number": 30,
      "content": "Tin tức vui mừng làm mạnh khỏe cốt xương.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.505Z",
      "updatedAt": "2024-09-24T02:15:09.505Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "3f0d7213-049e-46e4-9c8a-afd3544466e0",
      "number": 31,
      "content": "Tai chịu lắng nghe những lời quở trách về cuộc sống,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.509Z",
      "updatedAt": "2024-09-24T02:15:09.509Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "7f5c1ecf-832b-49b1-8927-a261e836fd16",
      "number": 31,
      "content": "Người ấy sẽ ở giữa các bậc hiền triết khôn ngoan.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.514Z",
      "updatedAt": "2024-09-24T02:15:09.514Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "c4d18953-2baa-4f04-90fe-72bd19d67309",
      "number": 32,
      "content": "Kẻ từ chối nghe lời chỉ giáo khinh bỉ linh hồn mình,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.520Z",
      "updatedAt": "2024-09-24T02:15:09.520Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "965df519-eaff-4e33-96f8-5abf44c0a722",
      "number": 32,
      "content": "Nhưng ai chịu nghe lời khuyên bảo sẽ được sự thông sáng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.525Z",
      "updatedAt": "2024-09-24T02:15:09.525Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "62a2fca0-77b4-4041-bc4b-0143e928c482",
      "number": 33,
      "content": "Kính sợ Chúa là cách giáo dục khôn ngoan,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.530Z",
      "updatedAt": "2024-09-24T02:15:09.530Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    },
    {
      "id": "8dfea971-ef2e-4dea-bee6-f9e7f35fb26d",
      "number": 33,
      "content": "Ðức khiêm nhường đi trước sự tôn trọng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 23,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:09.535Z",
      "updatedAt": "2024-09-24T02:15:09.535Z",
      "chapterId": "576f5bd7-b946-4ae4-b47b-8c5842bf3750"
    }
  ],
