{
  "chapter": {
    "id": "8626766b-0061-4d29-9aaf-e11638e3d25b",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%201&version=BD2011",
    "createdAt": "2025-04-22T07:37:49.343Z",
    "updatedAt": "2025-04-22T07:37:49.343Z",
    "bookId": "eebb80de-01b2-4f4a-a1ff-0a76541f8302"
  },
  "verses": [
    {
      "id": "aba3bcda-2c66-413d-bfad-1fdfbc682db7",
      "number": 1,
      "content": "Châm ngôn của Sa-lô-môn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.466Z",
      "updatedAt": "2025-04-22T08:37:38.466Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "494c06fc-3252-4f19-8c78-918cd0abbf4b",
      "number": 1,
      "content": "Con của Ða-vít, vua I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.493Z",
      "updatedAt": "2025-04-22T08:37:38.493Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "e9d35886-9ae2-49e5-84aa-ba6722f49d6f",
      "number": 2,
      "content": "Ðể giúp người ta biết lẽ khôn ngoan và lời giáo huấn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.504Z",
      "updatedAt": "2025-04-22T08:37:38.504Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "50279352-9f4c-48eb-aff3-3b665a53c589",
      "number": 2,
      "content": "Cùng hiểu được những lời lẽ uyên thâm,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.514Z",
      "updatedAt": "2025-04-22T08:37:38.514Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "2ee27d48-eb67-4464-84f1-0ae7c78175bf",
      "number": 3,
      "content": "Ðể họ lãnh hội những lời chỉ dạy hầu biết cư xử khôn ngoan,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.524Z",
      "updatedAt": "2025-04-22T08:37:38.524Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "690e3c4d-1263-4ff9-ba44-602ded3171b0",
      "number": 3,
      "content": "Sống công chính, thực thi công lý, và thể hiện sự công bình,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.534Z",
      "updatedAt": "2025-04-22T08:37:38.534Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "43abb1f5-cdee-4227-87c9-674d657b3b94",
      "number": 4,
      "content": "Ðể ban khôn ngoan cho người ngây thơ khờ khạo,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.544Z",
      "updatedAt": "2025-04-22T08:37:38.544Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "d7c267d7-da9c-4844-9372-079722b58e0b",
      "number": 4,
      "content": "Ban kiến thức và sáng suốt cho người trẻ tuổi,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.554Z",
      "updatedAt": "2025-04-22T08:37:38.554Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "c8291a31-4d57-4eff-9096-87d347c5097f",
      "number": 5,
      "content": "Ðể người khôn ngoan lắng nghe và gia thêm tri thức,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.566Z",
      "updatedAt": "2025-04-22T08:37:38.566Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "2b951934-021b-4fe6-b28c-17646e3c14e5",
      "number": 5,
      "content": "Còn người thông sáng được lời hướng dẫn khôn ngoan,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.579Z",
      "updatedAt": "2025-04-22T08:37:38.579Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "1f30a875-c237-4c44-8cab-83e6c5d64afd",
      "number": 6,
      "content": "Ðể người ta hiểu thấu những câu châm ngôn và ẩn dụ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.593Z",
      "updatedAt": "2025-04-22T08:37:38.593Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "28b79b20-d24f-470a-8532-4e64f2aa28d2",
      "number": 6,
      "content": "Cùng những lời và câu đố của người khôn ngoan.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.611Z",
      "updatedAt": "2025-04-22T08:37:38.611Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "4991c3d5-dfa8-48ce-90b5-e1c54ed921fa",
      "number": 7,
      "content": "Kính sợ Chúa là khởi đầu của tri thức,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.631Z",
      "updatedAt": "2025-04-22T08:37:38.631Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "ba38636f-f7f4-43f3-93b7-6e1065c81994",
      "number": 7,
      "content": "Chỉ kẻ dại mới khinh bỉ sự khôn ngoan và lời giáo huấn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.652Z",
      "updatedAt": "2025-04-22T08:37:38.652Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "5fc24aec-fb16-449b-a1c2-6528f144d2e9",
      "number": 8,
      "content": "Con ơi, hãy lắng nghe lời khuyên dạy của cha con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.675Z",
      "updatedAt": "2025-04-22T08:37:38.675Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "d2c6336b-d149-4d46-8aa2-2bcaa3fd31b8",
      "number": 8,
      "content": "Chớ lìa bỏ những lời răn bảo của mẹ con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.700Z",
      "updatedAt": "2025-04-22T08:37:38.700Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "577b8751-ba54-4230-8508-f66ba557abfe",
      "number": 9,
      "content": "Chúng sẽ như vòng hoa tươi đẹp trên đầu con,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.712Z",
      "updatedAt": "2025-04-22T08:37:38.712Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "06ee93c0-2a99-4f1b-ade1-e05e8c2a3498",
      "number": 9,
      "content": "Như dây chuyền đẹp đẽ nơi cổ con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.722Z",
      "updatedAt": "2025-04-22T08:37:38.722Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "3ca7ac07-f0b4-454d-b0af-ad50c8a07af2",
      "number": 10,
      "content": "Con ơi, nếu những kẻ xấu xa tội lỗi dụ dỗ con,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.733Z",
      "updatedAt": "2025-04-22T08:37:38.733Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "b7f166be-d455-4fce-890a-70d181dcd692",
      "number": 10,
      "content": "Con chớ nghe theo chúng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.755Z",
      "updatedAt": "2025-04-22T08:37:38.755Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "34001331-36d1-4866-ada3-6d896dfb1844",
      "number": 11,
      "content": "Nếu chúng nói, “Hãy theo chúng tôi,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.765Z",
      "updatedAt": "2025-04-22T08:37:38.765Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "1c3b0eba-205d-4dfe-a64f-3486664a590d",
      "number": 11,
      "content": "Chúng ta sẽ nằm đây rình làm đổ máu người ta;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.775Z",
      "updatedAt": "2025-04-22T08:37:38.775Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "9eb60344-2915-4b69-b810-7755889a721d",
      "number": 11,
      "content": "Chúng ta sẽ phục kích và giết hại những người vô tội;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.789Z",
      "updatedAt": "2025-04-22T08:37:38.789Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "1f977ee1-1442-40ad-b567-09db3b76fb0b",
      "number": 12,
      "content": "Chúng ta sẽ nuốt sống họ giống âm phủ nuốt người;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.803Z",
      "updatedAt": "2025-04-22T08:37:38.803Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "6f64ee3e-d193-4980-9a83-27e34c5368f3",
      "number": 12,
      "content": "Nuốt nguyên vẹn như huyệt mả nuốt lấy người chết.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.813Z",
      "updatedAt": "2025-04-22T08:37:38.813Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "3a3bbde7-f45b-4a51-8baf-93f0b1d3c88e",
      "number": 13,
      "content": "Chúng ta sẽ chiếm đoạt những vật có giá trị;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.823Z",
      "updatedAt": "2025-04-22T08:37:38.823Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "b71db492-814e-4857-8c54-4e542a6f7600",
      "number": 13,
      "content": "Nhà chúng ta sẽ chứa đầy những của cướp đem về.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.833Z",
      "updatedAt": "2025-04-22T08:37:38.833Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "731a9c72-8446-43eb-ad0f-bf7a19aa3218",
      "number": 14,
      "content": "Hãy nhập bọn với chúng tôi,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.843Z",
      "updatedAt": "2025-04-22T08:37:38.843Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "bdb5a783-4e57-4576-bb4e-a4278355ecc1",
      "number": 14,
      "content": "Rồi tất cả chúng ta sẽ xài chung một túi tiền.”",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.853Z",
      "updatedAt": "2025-04-22T08:37:38.853Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "bd922d66-2074-4496-8e57-14a1c70183de",
      "number": 15,
      "content": "Con ơi, đừng đi theo đường với chúng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.863Z",
      "updatedAt": "2025-04-22T08:37:38.863Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "6dc4b386-1011-43b9-a5fb-c7bee012be68",
      "number": 15,
      "content": "Ðừng đặt chân con vào những lối chúng đi;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.873Z",
      "updatedAt": "2025-04-22T08:37:38.873Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "f9320e75-d955-407a-9121-5b1a8f02f3ca",
      "number": 16,
      "content": "Vì chân chúng chạy vào tội lỗi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.883Z",
      "updatedAt": "2025-04-22T08:37:38.883Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "917a851f-d3ba-4252-ad6e-5c829e45a6f1",
      "number": 16,
      "content": "Chúng nhanh chân làm đổ máu người ta.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.893Z",
      "updatedAt": "2025-04-22T08:37:38.893Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "29f7241b-87b6-4e46-8957-36d7c714d597",
      "number": 17,
      "content": "Giăng lưới bắt chim đang khi chim nhìn thấy quả là phí công vô ích.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.904Z",
      "updatedAt": "2025-04-22T08:37:38.904Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "dc878706-0f38-4c20-90c9-0986a7a867bf",
      "number": 18,
      "content": "Thế nhưng bọn đó mai phục để làm đổ máu chính mình;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.914Z",
      "updatedAt": "2025-04-22T08:37:38.914Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "048a34cd-2129-416b-ae68-6a03bf66c96e",
      "number": 18,
      "content": "Chúng phục kích để giết hại chính mạng sống của chúng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.924Z",
      "updatedAt": "2025-04-22T08:37:38.924Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "fee1cd29-1547-43b4-a0cb-8eedc76c7565",
      "number": 19,
      "content": "Sự cuối cùng của kẻ tham lợi bất chính là như thế;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.935Z",
      "updatedAt": "2025-04-22T08:37:38.935Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "2a50fd02-d6ec-4953-a0ec-e45f6c18d4d4",
      "number": 19,
      "content": "Thứ lợi ấy sẽ giết hại chính những người chiếm được nó.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.946Z",
      "updatedAt": "2025-04-22T08:37:38.946Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "cf48d4d8-b8c8-49a1-b6c6-f5535a739ab5",
      "number": 20,
      "content": "Sự khôn ngoan hô vang ngoài đường phố;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.956Z",
      "updatedAt": "2025-04-22T08:37:38.956Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "dce0ddf9-aa84-4400-bd7b-921425170eb4",
      "number": 20,
      "content": "Nàng cất tiếng lên giữa các quảng trường.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.977Z",
      "updatedAt": "2025-04-22T08:37:38.977Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "12206b93-7d62-4419-b045-61746a8d3206",
      "number": 21,
      "content": "Nàng kêu to nơi đầu đường náo nhiệt;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.988Z",
      "updatedAt": "2025-04-22T08:37:38.988Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "3ec2f7cb-4a4c-4ace-9b14-243ebd17772b",
      "number": 21,
      "content": "Nơi cổng thành nàng trỗi tiếng kêu vang:",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:38.999Z",
      "updatedAt": "2025-04-22T08:37:38.999Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "408de3c9-5223-414c-a168-92f529a43c1d",
      "number": 22,
      "content": "“Hỡi những kẻ khờ dại!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.009Z",
      "updatedAt": "2025-04-22T08:37:39.009Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "3276f44e-cd58-43da-bb40-52d44cfc26d0",
      "number": 22,
      "content": "Các ngươi cứ yêu mến sự khờ khạo cho đến bao giờ?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.019Z",
      "updatedAt": "2025-04-22T08:37:39.019Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "fb695868-c95b-4029-bb8c-96c1cd474e1b",
      "number": 22,
      "content": "Sao những kẻ ưa nhạo báng cứ đam mê nhạo báng,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.032Z",
      "updatedAt": "2025-04-22T08:37:39.032Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "d6a37447-edd9-4c50-bbe0-66d28813cccd",
      "number": 22,
      "content": "Và những kẻ điên rồ cứ mãi ghét khôn ngoan?",
      "order": 3,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.042Z",
      "updatedAt": "2025-04-22T08:37:39.042Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "56f945c1-7d18-460e-84f8-34dd3c4965b3",
      "number": 23,
      "content": "Nếu các ngươi trở lại khi nghe Ta quở trách,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.054Z",
      "updatedAt": "2025-04-22T08:37:39.054Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "70dd5e68-a908-4e05-9330-705eaeefa2f5",
      "number": 23,
      "content": "Này, Thần Ta sẽ tuôn đổ trên các ngươi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.065Z",
      "updatedAt": "2025-04-22T08:37:39.065Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "d499db5c-ed27-43d1-9a77-814fe0ceb630",
      "number": 23,
      "content": "Ta sẽ làm cho các ngươi hiểu biết lời Ta.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.078Z",
      "updatedAt": "2025-04-22T08:37:39.078Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "105e4e53-a17c-4329-a76f-afc10028b37e",
      "number": 24,
      "content": "Nhưng vì Ta mời gọi mà các ngươi từ chối;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.088Z",
      "updatedAt": "2025-04-22T08:37:39.088Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "d14cb278-eea9-4970-a786-d082422ccfb0",
      "number": 24,
      "content": "Ta dang tay đón mời nhưng các ngươi vẫn lãnh đạm làm ngơ.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.098Z",
      "updatedAt": "2025-04-22T08:37:39.098Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "8f20a555-227c-4342-8f07-19f13b0ce028",
      "number": 25,
      "content": "Vì các ngươi không màng đến lời Ta khuyên bảo,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.108Z",
      "updatedAt": "2025-04-22T08:37:39.108Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "9444f168-4712-400a-8304-c053f0bb5daa",
      "number": 25,
      "content": "Không muốn nghe những lời khuyến cáo của Ta,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.118Z",
      "updatedAt": "2025-04-22T08:37:39.118Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "6b78b65f-b2c3-415b-8f73-86579f9f4091",
      "number": 26,
      "content": "Nên Ta sẽ cười khi các ngươi lâm nạn;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.128Z",
      "updatedAt": "2025-04-22T08:37:39.128Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "5f6dedb2-fbfe-4ec5-b8ff-236c201d0ca1",
      "number": 26,
      "content": "Ta sẽ làm ngơ khi cơn khủng hoảng giáng xuống các ngươi,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.138Z",
      "updatedAt": "2025-04-22T08:37:39.138Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "67e57279-fa9f-4e90-b607-ac2efa193a37",
      "number": 27,
      "content": "Khi nỗi kinh hoàng của các ngươi ập đến như bão tố,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.148Z",
      "updatedAt": "2025-04-22T08:37:39.148Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "2ecb31b1-aa8f-48c4-8585-b6c37918835d",
      "number": 27,
      "content": "Khi tai ương của các ngươi dồn dập tợ cuồng phong,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.158Z",
      "updatedAt": "2025-04-22T08:37:39.158Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "a1f8be2c-e33b-437f-b07b-012e9bb88231",
      "number": 27,
      "content": "Khi khổ đau và hoạn nạn ập đến trên các ngươi tới tấp.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.168Z",
      "updatedAt": "2025-04-22T08:37:39.168Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "869457ea-cf55-4432-ba03-60461ad3100c",
      "number": 28,
      "content": "Bấy giờ họ sẽ kêu cầu Ta, nhưng Ta chẳng đáp lời;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.178Z",
      "updatedAt": "2025-04-22T08:37:39.178Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "6d16a2f8-ab50-4059-a947-76163ddf5580",
      "number": 28,
      "content": "Họ sẽ chăm chỉ tìm Ta, nhưng họ chẳng tìm được Ta;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.189Z",
      "updatedAt": "2025-04-22T08:37:39.189Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "14b7eb41-c4a5-4e88-8c9d-77f4af0444b0",
      "number": 29,
      "content": "Bởi vì họ đã ghét tri thức,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.202Z",
      "updatedAt": "2025-04-22T08:37:39.202Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "099d572d-1fa6-4972-ac82-5e8c9aae509e",
      "number": 29,
      "content": "Và chẳng chọn sự kính sợ Chúa.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 23,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.212Z",
      "updatedAt": "2025-04-22T08:37:39.212Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "b518c20a-b79b-4088-81d5-bf41ae0b9163",
      "number": 30,
      "content": "Bởi vì họ chẳng chấp nhận lời Ta khuyên bảo;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 24,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.224Z",
      "updatedAt": "2025-04-22T08:37:39.224Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "0d97d5d1-ecc1-46a6-a1b7-dffca8b3bdd8",
      "number": 30,
      "content": "Họ chê bai mọi lời khuyến cáo của Ta,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 25,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.234Z",
      "updatedAt": "2025-04-22T08:37:39.234Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "c58929c6-c589-41a7-9493-a5cbbd3e603e",
      "number": 31,
      "content": "Vì thế họ phải chuốc lấy hậu quả của đường lối mình;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 26,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.245Z",
      "updatedAt": "2025-04-22T08:37:39.245Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "711e872b-c316-4718-a961-0eccab5aaadc",
      "number": 31,
      "content": "Họ sẽ được no nê bằng hậu quả của các mưu đồ mình.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 27,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.256Z",
      "updatedAt": "2025-04-22T08:37:39.256Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "4ea9991d-736b-43f9-af99-c5adf5fabf40",
      "number": 32,
      "content": "Vì sự ương ngạnh của những kẻ dại sẽ giết hại chúng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 28,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.269Z",
      "updatedAt": "2025-04-22T08:37:39.269Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "4e8b831a-186f-429e-b9f8-5d666ea94597",
      "number": 32,
      "content": "Sự tự mãn của những kẻ ngốc sẽ tiêu diệt chúng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 29,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.285Z",
      "updatedAt": "2025-04-22T08:37:39.285Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "bcda2f4f-6946-4e57-b9c2-393b5aba8b22",
      "number": 33,
      "content": "Nhưng ai nghe lời Ta sẽ được sống bình an vô sự,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 30,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.296Z",
      "updatedAt": "2025-04-22T08:37:39.296Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    },
    {
      "id": "33a8e1eb-7525-45b2-90cb-c60732485f40",
      "number": 33,
      "content": "Người ấy sẽ sống an lành, chẳng sợ tai họa hiểm nguy.”",
      "order": 1,
      "parNumber": 5,
      "parIndex": 31,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:39.314Z",
      "updatedAt": "2025-04-22T08:37:39.314Z",
      "chapterId": "8626766b-0061-4d29-9aaf-e11638e3d25b"
    }
  ],
