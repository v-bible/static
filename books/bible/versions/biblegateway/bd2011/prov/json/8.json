      "chapterId": "1430ef5c-7829-4a2d-a799-f0e019279afe"
    },
    {
      "id": "34c8cbaa-a447-45d0-85c8-a77fb4162055",
      "content": "Sự Khôn Ngoan Dự Phần trong Cuộc Sáng Tạo",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T04:06:40.222Z",
      "updatedAt": "2025-04-24T04:06:40.222Z",
