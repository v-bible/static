      "chapterId": "5c9a5277-880f-446c-8b29-69d36a6a86b9"
    },
    {
      "id": "24d23c7a-bca3-494f-bff1-303a0e7ec6f6",
      "content": "Sự Khôn Ngoan Dự Phần trong Cuộc Sáng Tạo",
      "order": 0,
      "createdAt": "2024-09-24T02:14:30.932Z",
      "updatedAt": "2024-09-24T02:14:30.932Z",
