{
  "chapter": {
    "id": "36d36d10-8cc9-4bed-b24d-0c755873a402",
    "number": 14,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2014&version=BD2011",
    "createdAt": "2025-04-24T03:03:19.060Z",
    "updatedAt": "2025-04-24T03:03:19.060Z",
    "bookId": "1b06f6cc-342f-4633-b290-6d1686eb4b30"
  },
  "verses": [
    {
      "id": "4bb32107-6b80-412a-b548-e8122d8ad1ce",
      "number": 1,
      "content": "Người nữ khôn ngoan xây dựng gia đình mình hạnh phúc,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.009Z",
      "updatedAt": "2025-04-24T04:07:13.009Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "0b6f813a-1d62-4b2d-b7ee-d7e5e294a7b4",
      "number": 1,
      "content": "Còn kẻ dại dột dùng chính tay mình phá hủy nó đi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.027Z",
      "updatedAt": "2025-04-24T04:07:13.027Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "6fdafa00-da23-4b34-86cc-210dd7bca27d",
      "number": 2,
      "content": "Người sống ngay thẳng là người kính sợ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.037Z",
      "updatedAt": "2025-04-24T04:07:13.037Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "62745529-a15f-4cd6-b354-0a492db1080c",
      "number": 2,
      "content": "Kẻ ăn ở gian tà chính là kẻ khinh thường Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.048Z",
      "updatedAt": "2025-04-24T04:07:13.048Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "a99f7abc-fd25-4fab-9275-d32f898d6f3e",
      "number": 3,
      "content": "Lời ngạo mạn ra từ miệng kẻ dại trở thành roi đánh nó,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.059Z",
      "updatedAt": "2025-04-24T04:07:13.059Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "d94f4060-b093-41fa-930a-40600be95525",
      "number": 3,
      "content": "Môi người khôn bảo vệ mạng sống mình.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.071Z",
      "updatedAt": "2025-04-24T04:07:13.071Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "ee41b133-488a-4159-becd-dd62a32a2a85",
      "number": 4,
      "content": "Nơi nào bò không cày, nơi đó không lúa gạo,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.083Z",
      "updatedAt": "2025-04-24T04:07:13.083Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "bb938fb1-271d-4791-b9db-22a2742903b6",
      "number": 4,
      "content": "Hoa lợi nhiều nhờ sức lực của bò đực mà ra.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.098Z",
      "updatedAt": "2025-04-24T04:07:13.098Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "409a2fc7-c3d4-4f1b-8f93-94af0788a326",
      "number": 5,
      "content": "Nhân chứng trung tín không bao giờ nói dối,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.113Z",
      "updatedAt": "2025-04-24T04:07:13.113Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "6cc70751-afd2-4451-91e2-969860288a35",
      "number": 5,
      "content": "Kẻ chứng gian vừa thở ra là có hơi hám dối trá rồi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.126Z",
      "updatedAt": "2025-04-24T04:07:13.126Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "474e7999-44f8-4e04-8a18-ea64e2532a5e",
      "number": 6,
      "content": "Kẻ nhạo báng tìm khôn ngoan hoài không được,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.138Z",
      "updatedAt": "2025-04-24T04:07:13.138Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "cd1180e1-6982-4d5d-afe0-857703ade0d6",
      "number": 6,
      "content": "Còn người thông minh tri thức đến dễ dàng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.151Z",
      "updatedAt": "2025-04-24T04:07:13.151Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "a5262b59-5c6a-4121-bd7a-d8e9013fc28f",
      "number": 7,
      "content": "Hãy dang xa con người điên dại,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.164Z",
      "updatedAt": "2025-04-24T04:07:13.164Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "e19824c7-5287-4cef-afab-bbc579b3766e",
      "number": 7,
      "content": "Vì bạn sẽ chẳng thấy tri thức ra từ miệng nó bao giờ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.177Z",
      "updatedAt": "2025-04-24T04:07:13.177Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "ef00c50a-e436-4fab-ba04-536597dfb91f",
      "number": 8,
      "content": "Cái khôn của người thận trọng là biết toan tính đường lối mình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.190Z",
      "updatedAt": "2025-04-24T04:07:13.190Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "237f8f86-3f38-4b21-bcec-ecd7375c2dbe",
      "number": 8,
      "content": "Cái dại của kẻ điên dại là mánh khóe dối gian.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.204Z",
      "updatedAt": "2025-04-24T04:07:13.204Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "613508e4-567a-4954-a9ec-75bd27e62548",
      "number": 9,
      "content": "Những kẻ điên dại chê cười của lễ chuộc tội;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.217Z",
      "updatedAt": "2025-04-24T04:07:13.217Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "fc71e322-9fbb-419a-897f-77a8ec5a1eab",
      "number": 9,
      "content": "Nhưng người ngay lành tin rằng nhờ nó mà được đoái thương.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.230Z",
      "updatedAt": "2025-04-24T04:07:13.230Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "b840095a-7051-4ccc-8e48-cfc155615a76",
      "number": 10,
      "content": "Nỗi đắng cay chỉ có lòng mình biết;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.244Z",
      "updatedAt": "2025-04-24T04:07:13.244Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "4288a685-c894-422e-ab42-1b7e3e88c9c6",
      "number": 10,
      "content": "Và niềm vui nào chia sẻ hết được với người dưng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.257Z",
      "updatedAt": "2025-04-24T04:07:13.257Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "35749d74-92e2-4d07-960b-9c3cff9d0592",
      "number": 11,
      "content": "Nhà kẻ gian ác trước sau gì cũng bị hủy hoại,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.271Z",
      "updatedAt": "2025-04-24T04:07:13.271Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "b776bab3-7755-40f2-92b1-b18fcb19f238",
      "number": 11,
      "content": "Trại người ngay lành cứ vững chãi lâu dài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.284Z",
      "updatedAt": "2025-04-24T04:07:13.284Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "06dbd7e9-b5aa-4b2b-81ee-ab760a13287a",
      "number": 12,
      "content": "Có một con đường dường như chính đáng cho người ta,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.297Z",
      "updatedAt": "2025-04-24T04:07:13.297Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "bac295b3-b9a7-4936-894d-14b3d74a7d20",
      "number": 12,
      "content": "Nhưng đến cuối đường hóa ra là tử lộ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.311Z",
      "updatedAt": "2025-04-24T04:07:13.311Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "fa6e18d3-25dd-44dd-b124-d1b403a0b893",
      "number": 13,
      "content": "Dù ngoài mặt cười vang, nhưng trong lòng có thể đang sầu héo;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.324Z",
      "updatedAt": "2025-04-24T04:07:13.324Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "487e11d1-3c8a-4ce4-9619-db3860773bc1",
      "number": 13,
      "content": "Cuối cuộc vui nỗi sầu khổ vẫn kéo lại như thường.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.340Z",
      "updatedAt": "2025-04-24T04:07:13.340Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "7c889174-5889-4979-9d5c-919923c86eef",
      "number": 14,
      "content": "Kẻ tà tâm sẽ lãnh đủ hậu quả xấu mình gây ra;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.353Z",
      "updatedAt": "2025-04-24T04:07:13.353Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "a2bfd44f-5a43-4e7b-9519-3984d77f0091",
      "number": 14,
      "content": "Người thiện tâm sẽ được thưởng xứng đáng với việc mình làm.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.367Z",
      "updatedAt": "2025-04-24T04:07:13.367Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "8c92ed48-84b2-4a26-8974-2d172cb0d068",
      "number": 15,
      "content": "Kẻ dại dột tin mọi lời người ta nói;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.379Z",
      "updatedAt": "2025-04-24T04:07:13.379Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "dcff6611-88af-4e59-9807-2ebad1de0818",
      "number": 15,
      "content": "Nhưng người khôn ngoan thận trọng từng bước một của mình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.393Z",
      "updatedAt": "2025-04-24T04:07:13.393Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "eb8091b7-b0e7-48f5-be97-6263a144e502",
      "number": 16,
      "content": "Người khôn ngoan ghê sợ và lánh xa điều ác;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.406Z",
      "updatedAt": "2025-04-24T04:07:13.406Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "64b0f7f0-6c8e-4c23-824b-41389b1375bc",
      "number": 16,
      "content": "Còn kẻ dại dột thì hành động khinh suất và bất cẩn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.419Z",
      "updatedAt": "2025-04-24T04:07:13.419Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "3b536b41-c7c1-407f-92e9-5057f1fa07ea",
      "number": 17,
      "content": "Kẻ nhạy giận dễ hành động điên rồ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.433Z",
      "updatedAt": "2025-04-24T04:07:13.433Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "e6bb02f4-ea4e-4df7-a0b6-822c85fe352d",
      "number": 17,
      "content": "Kẻ toan tính chuyện ác ắt bị người ta ghét.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.446Z",
      "updatedAt": "2025-04-24T04:07:13.446Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "e26dd295-e576-457b-b5a3-2c6d7b6f26f3",
      "number": 18,
      "content": "Kẻ khờ dại thừa hưởng sự điên dại;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.459Z",
      "updatedAt": "2025-04-24T04:07:13.459Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "7c846e3d-3c21-44f1-9923-cb3b2f338f4d",
      "number": 18,
      "content": "Người khôn ngoan lấy tri thức làm mão miện của mình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.472Z",
      "updatedAt": "2025-04-24T04:07:13.472Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "d4338bc1-2b88-460f-943b-2bfb584d8132",
      "number": 19,
      "content": "Kẻ xấu phải sấp mình trước mặt người tốt,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.483Z",
      "updatedAt": "2025-04-24T04:07:13.483Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "a1aec0ea-60d4-4597-a06e-bb1a8625ae6d",
      "number": 19,
      "content": "Kẻ gian ác phải chầu chực trước cổng người ngay lành.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.494Z",
      "updatedAt": "2025-04-24T04:07:13.494Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "fb1bee4f-751c-4cbb-b002-d0c25840341c",
      "number": 20,
      "content": "Người nghèo khó ngay cả láng giềng cũng không thích,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.504Z",
      "updatedAt": "2025-04-24T04:07:13.504Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "830e8346-2a86-4ce0-99df-96cb35cf9283",
      "number": 20,
      "content": "Kẻ giàu sang tính không hết bạn bè.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.514Z",
      "updatedAt": "2025-04-24T04:07:13.514Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "d342cf1e-d666-4df9-b6f0-715938905c46",
      "number": 21,
      "content": "Ai khi dể người lân cận là mắc tội,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.524Z",
      "updatedAt": "2025-04-24T04:07:13.524Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "3fb6c49b-ee9f-4a44-90fe-949ab24fd907",
      "number": 21,
      "content": "Phước cho ai thương xót người nghèo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.537Z",
      "updatedAt": "2025-04-24T04:07:13.537Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "3e10c115-5d63-4abf-ab08-57cbb54f0ad5",
      "number": 22,
      "content": "Há chẳng phải kẻ toan mưu ác đang sai lạc sao?",
      "order": 0,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.547Z",
      "updatedAt": "2025-04-24T04:07:13.547Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "0b0e01b0-f0c2-450f-bc1b-de32d6b348b6",
      "number": 22,
      "content": "Người nỗ lực làm điều lành được xót thương và tin cậy.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.557Z",
      "updatedAt": "2025-04-24T04:07:13.557Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "4fee161f-c2e9-4906-8d50-bd375e7577c3",
      "number": 23,
      "content": "Tất cả các hình thức lao động đều đem lại ích lợi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.567Z",
      "updatedAt": "2025-04-24T04:07:13.567Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "7944bccf-abb5-4273-a539-c38b0b826615",
      "number": 23,
      "content": "Còn chỉ nói và lười ắt dẫn đến đói nghèo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.577Z",
      "updatedAt": "2025-04-24T04:07:13.577Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "56eb238c-4acb-434a-afd0-de9a1cd28e9a",
      "number": 24,
      "content": "Giàu có là mão miện của người khôn ngoan,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.587Z",
      "updatedAt": "2025-04-24T04:07:13.587Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "d9c62264-3c5a-4b09-917a-eac61b95fb24",
      "number": 24,
      "content": "Ðiên cuồng là vòng hoa của kẻ điên dại.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.597Z",
      "updatedAt": "2025-04-24T04:07:13.597Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "0ce48d12-3655-4a7e-9295-07790a1dd75a",
      "number": 25,
      "content": "Người chứng chân thật cứu biết bao mạng sống,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.607Z",
      "updatedAt": "2025-04-24T04:07:13.607Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "45327da4-edf8-4841-b17e-25f320c6de88",
      "number": 25,
      "content": "Còn kẻ nói dối thở ra toàn hơi hám dối gian.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.617Z",
      "updatedAt": "2025-04-24T04:07:13.617Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "1d96b357-7f9e-4960-9f79-a31a362536a1",
      "number": 26,
      "content": "Người kính sợ Chúa được thành lũy an toàn bảo vệ,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.628Z",
      "updatedAt": "2025-04-24T04:07:13.628Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "2337661e-afb7-420e-9af1-6a55c4ed1795",
      "number": 26,
      "content": "Con cái người ấy có một chỗ để an thân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 23,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.638Z",
      "updatedAt": "2025-04-24T04:07:13.638Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "0a09754d-87cf-4e5f-9461-92e898d5808f",
      "number": 27,
      "content": "Kính sợ Chúa là nguồn sự sống,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 24,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.650Z",
      "updatedAt": "2025-04-24T04:07:13.650Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "096fd8eb-b23e-40a3-8589-bb0d127f8616",
      "number": 27,
      "content": "Giúp người ta tránh được bẫy tử vong.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 25,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.663Z",
      "updatedAt": "2025-04-24T04:07:13.663Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "eeddc457-dc0d-491e-bd4c-70cd374590d9",
      "number": 28,
      "content": "Vua được vinh hiển nhờ có đông dân chúng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 26,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.675Z",
      "updatedAt": "2025-04-24T04:07:13.675Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "2c589bc0-9d1e-4b7b-82d9-d90b8a621ea4",
      "number": 28,
      "content": "Không có dân vua lúng túng chịu thua.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 27,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.692Z",
      "updatedAt": "2025-04-24T04:07:13.692Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "16f9d79c-3285-4325-b93b-74e81b728cbb",
      "number": 29,
      "content": "Người chậm nóng giận là người đại trí,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 28,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.705Z",
      "updatedAt": "2025-04-24T04:07:13.705Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "d78d3a71-131a-447e-9d3e-13103142741d",
      "number": 29,
      "content": "Kẻ nóng tính tỏ thái độ liền là tự ý phô sự dại dột mình ra.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 29,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.719Z",
      "updatedAt": "2025-04-24T04:07:13.719Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "2ee6ccb5-ea18-48c1-8833-972ff6900a27",
      "number": 30,
      "content": "Lòng bình an ban sinh lực cho thân xác;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 30,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.733Z",
      "updatedAt": "2025-04-24T04:07:13.733Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "8b3e72c6-b557-424c-bf26-eda2e9139aae",
      "number": 30,
      "content": "Tính ganh tị làm mục nát trong xương.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 31,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.746Z",
      "updatedAt": "2025-04-24T04:07:13.746Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "1b1a77e3-3260-43ab-b071-4c9246c289a0",
      "number": 31,
      "content": "Ai áp bức người nghèo là làm nhục Ðấng Tạo Hóa mình;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 32,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.759Z",
      "updatedAt": "2025-04-24T04:07:13.759Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "037b47f5-3640-4e9f-b2c6-6a004f2e624f",
      "number": 31,
      "content": "Ai thương xót người khốn khổ là tôn kính Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 33,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.776Z",
      "updatedAt": "2025-04-24T04:07:13.776Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "d492057c-5ead-4b72-941d-da6faa1030ee",
      "number": 32,
      "content": "Kẻ ác bị việc ác của nó đánh đổ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 34,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.788Z",
      "updatedAt": "2025-04-24T04:07:13.788Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "897c7795-e6d6-49f3-b891-57dcde20137f",
      "number": 32,
      "content": "Nhưng người ngay lành khi qua đời vẫn có chỗ nương thân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 35,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.800Z",
      "updatedAt": "2025-04-24T04:07:13.800Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "ae5f363e-acd1-4f3d-ac1a-771779e9700e",
      "number": 33,
      "content": "Sự khôn ngoan nghỉ ngơi trong lòng người thông sáng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 36,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.811Z",
      "updatedAt": "2025-04-24T04:07:13.811Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "4128fff3-413d-447e-b202-434476aa4e5c",
      "number": 33,
      "content": "Còn kẻ dại dột bộc lộ hết lòng dạ nó ra ngoài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 37,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.822Z",
      "updatedAt": "2025-04-24T04:07:13.822Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "dc1bd804-8d3f-4a8b-aea2-d4ec43d6ab5f",
      "number": 34,
      "content": "Lẽ công chính làm quốc gia được kính trọng;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.833Z",
      "updatedAt": "2025-04-24T04:07:13.833Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "51d668af-8173-4adb-afbd-3117a7a6777d",
      "number": 34,
      "content": "Còn tội lỗi làm xấu hổ toàn dân.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.845Z",
      "updatedAt": "2025-04-24T04:07:13.845Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "33458dd3-26c4-4c10-b412-8fcf65ef93dd",
      "number": 35,
      "content": "Ân điển vua dành cho bầy tôi tài giỏi,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.856Z",
      "updatedAt": "2025-04-24T04:07:13.856Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    },
    {
      "id": "417ab5ed-e184-4bf2-8a91-cd3363122547",
      "number": 35,
      "content": "Thịnh nộ vua dọi xuống đầu kẻ gây nhục nhã cho người.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:13.867Z",
      "updatedAt": "2025-04-24T04:07:13.867Z",
      "chapterId": "36d36d10-8cc9-4bed-b24d-0c755873a402"
    }
  ],
