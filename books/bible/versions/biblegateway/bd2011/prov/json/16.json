{
  "chapter": {
    "id": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63",
    "number": 16,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2016&version=BD2011",
    "createdAt": "2025-04-22T07:37:49.708Z",
    "updatedAt": "2025-04-22T07:37:49.708Z",
    "bookId": "eebb80de-01b2-4f4a-a1ff-0a76541f8302"
  },
  "verses": [
    {
      "id": "74c084bb-1d1b-4990-8eba-04b49a2b7923",
      "number": 1,
      "content": "Sự toan tính trong lòng thuộc về loài người,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.379Z",
      "updatedAt": "2025-04-22T08:39:05.379Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "f190c3b4-23c4-48cc-9da0-18980f5b331a",
      "number": 1,
      "content": "Nhưng sự đáp lời của lưỡi đến từ Chúa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.398Z",
      "updatedAt": "2025-04-22T08:39:05.398Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "430f2dd0-5474-421a-86d6-b5dc862f0dea",
      "number": 2,
      "content": "Mọi đường lối của người ta đều trong sạch theo mắt họ;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.411Z",
      "updatedAt": "2025-04-22T08:39:05.411Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "cb8bbed4-a027-48d3-9973-d6809a1e167a",
      "number": 2,
      "content": "Nhưng Chúa cân nhắc tấm lòng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.423Z",
      "updatedAt": "2025-04-22T08:39:05.423Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "05ddaef6-7a15-40f1-b013-32534440e5c6",
      "number": 3,
      "content": "Hãy phó thác các công việc của bạn cho Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.436Z",
      "updatedAt": "2025-04-22T08:39:05.436Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "83f5a440-4636-4814-a68b-3449f0d8e1d5",
      "number": 3,
      "content": "Thì các chương trình của bạn sẽ thành công.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.448Z",
      "updatedAt": "2025-04-22T08:39:05.448Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "b04d4507-bfe7-4e9c-9864-dc9d6683805c",
      "number": 4,
      "content": "Chúa có mục đích khi dựng nên mọi vật,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.458Z",
      "updatedAt": "2025-04-22T08:39:05.458Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "de98200e-07ee-4713-a3e9-1b86f52d57e2",
      "number": 4,
      "content": "Ngay cả kẻ gian ác cho ngày tai vạ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.470Z",
      "updatedAt": "2025-04-22T08:39:05.470Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "08e7a37f-712f-4721-a75c-75709828d781",
      "number": 5,
      "content": "Chúa gớm ghiếc kẻ có lòng kiêu ngạo;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.481Z",
      "updatedAt": "2025-04-22T08:39:05.481Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "a016ed94-a5ee-42e5-b749-1e36d101055b",
      "number": 5,
      "content": "Chắc chắn nó sẽ không khỏi bị phạt.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.491Z",
      "updatedAt": "2025-04-22T08:39:05.491Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "93127b7e-7942-43bc-956c-adacbc7ad10d",
      "number": 6,
      "content": "Nhờ ơn thương xót và đức chân thật mà lỗi lầm được chuộc,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.502Z",
      "updatedAt": "2025-04-22T08:39:05.502Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "7bdafb5d-a970-4e4e-887d-68805792cfd1",
      "number": 6,
      "content": "Do sự kính sợ Chúa mà người ta lánh xa điều ác.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.512Z",
      "updatedAt": "2025-04-22T08:39:05.512Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "ad71dbee-3f4e-4188-82b0-c5b12deccb4c",
      "number": 7,
      "content": "Khi đường lối người nào đẹp lòng Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.522Z",
      "updatedAt": "2025-04-22T08:39:05.522Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "7b2ed272-9beb-4277-8b42-3dbc09dc52e0",
      "number": 7,
      "content": "Thì Ngài khiến ngay cả kẻ thù cũng làm hòa với người ấy.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.532Z",
      "updatedAt": "2025-04-22T08:39:05.532Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "58083887-e5d8-4de2-9aec-1fcabb4ff379",
      "number": 8,
      "content": "Thà ít của mà ngay lành đạo đức,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.542Z",
      "updatedAt": "2025-04-22T08:39:05.542Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "c2412bd7-4fea-45a0-933b-542828001264",
      "number": 8,
      "content": "Còn hơn lợi tức nhiều mà bất công bất chính cặp theo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.551Z",
      "updatedAt": "2025-04-22T08:39:05.551Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "bd100bbc-d305-48f4-96f1-63007e7f86f4",
      "number": 9,
      "content": "Lòng người ta toan tính đường lối mình,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.561Z",
      "updatedAt": "2025-04-22T08:39:05.561Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "d07f13bd-d769-4492-86e3-7618cb6b4b80",
      "number": 9,
      "content": "Nhưng Chúa định đoạt các bước của mỗi người.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.571Z",
      "updatedAt": "2025-04-22T08:39:05.571Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "e924abe5-5bd2-4e60-b231-ffc9e32650c1",
      "number": 10,
      "content": "Khi môi vua đã phán những lời như sấm ngôn bất biến,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.581Z",
      "updatedAt": "2025-04-22T08:39:05.581Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "384f653f-b8f3-4c21-a3f2-a34d2f8084aa",
      "number": 10,
      "content": "Thì miệng vua không được truyền ra một phán quyết bất công.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.591Z",
      "updatedAt": "2025-04-22T08:39:05.591Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "2d8bcdab-516a-4056-832a-d8b8e70a5297",
      "number": 11,
      "content": "Cán cân công bình và bàn cân chính xác đều thuộc về Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.601Z",
      "updatedAt": "2025-04-22T08:39:05.601Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "ed5114e5-4625-4100-b2c5-8c569b5c0025",
      "number": 11,
      "content": "Tất cả trái cân trong bao là công việc của Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.611Z",
      "updatedAt": "2025-04-22T08:39:05.611Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "8b985587-ec82-4d48-9249-ab3c1239f368",
      "number": 12,
      "content": "Vua mà còn làm ác thì quả là điều gớm ghiếc,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.621Z",
      "updatedAt": "2025-04-22T08:39:05.621Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "5ca1ec9c-1c0d-43b1-9bc0-20a34784d4de",
      "number": 12,
      "content": "Vì ngôi vua được lập lên để thi hành sự công chính.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.631Z",
      "updatedAt": "2025-04-22T08:39:05.631Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "aef66ff6-a3c8-4be9-883d-501188dc0a2f",
      "number": 13,
      "content": "Môi chân thật được các vua vui thích,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.641Z",
      "updatedAt": "2025-04-22T08:39:05.641Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "8d1551d5-870f-4e16-bb64-7a72e6ba5cb6",
      "number": 13,
      "content": "Ai nói lời chân chính thì được họ mến thương.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.651Z",
      "updatedAt": "2025-04-22T08:39:05.651Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "411da64f-bd72-4ea7-a0f3-9741983ab732",
      "number": 14,
      "content": "Cơn thịnh nộ của vua khác nào sứ giả của tử thần,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.661Z",
      "updatedAt": "2025-04-22T08:39:05.661Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "0637223d-4556-41e1-85a1-da134e525765",
      "number": 14,
      "content": "Nhưng người khôn ngoan biết làm cho nguôi nó.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.671Z",
      "updatedAt": "2025-04-22T08:39:05.671Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "96492fc3-ab1a-4c02-b511-2bff5723d9c9",
      "number": 15,
      "content": "Nét mặt vua vui đem đến hy vọng sống,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.681Z",
      "updatedAt": "2025-04-22T08:39:05.681Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "f987203f-4815-4f51-977d-14066f08ec5d",
      "number": 15,
      "content": "Ân điển của vua khác nào mây mang lại trận mưa xuân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.691Z",
      "updatedAt": "2025-04-22T08:39:05.691Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "e51cffb1-8c3e-4f3e-8587-a209688dc506",
      "number": 16,
      "content": "Ðược khôn ngoan sang hơn vàng biết mấy!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.701Z",
      "updatedAt": "2025-04-22T08:39:05.701Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "e485b4be-fb82-43d8-b39b-ff53f6679591",
      "number": 16,
      "content": "Ðược thông sáng đáng hơn bạc biết bao!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.711Z",
      "updatedAt": "2025-04-22T08:39:05.711Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "7b985f7d-7df1-4f1b-a4c1-58e7a098bc98",
      "number": 17,
      "content": "Con đường của người chính trực luôn tránh xa điều ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.721Z",
      "updatedAt": "2025-04-22T08:39:05.721Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "74740772-675d-4fb4-b5dc-8e77e35df0b5",
      "number": 17,
      "content": "Ai canh giữ đường lối mình giữ được mạng sống mình.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.731Z",
      "updatedAt": "2025-04-22T08:39:05.731Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "e458c34f-b940-4372-9f4e-fa95643940f5",
      "number": 18,
      "content": "Sự kiêu ngạo đi trước sự hủy diệt;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.741Z",
      "updatedAt": "2025-04-22T08:39:05.741Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "226c2074-009d-4a67-ae08-3d733a330050",
      "number": 18,
      "content": "Tính kiêu căng đi trước sự sụp đổ.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.751Z",
      "updatedAt": "2025-04-22T08:39:05.751Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "6047fe6a-54a0-4ca0-b8a1-164ad737e025",
      "number": 19,
      "content": "Thà có tinh thần khiêm tốn với người nghèo khó,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.761Z",
      "updatedAt": "2025-04-22T08:39:05.761Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "d2da56dd-a090-4032-ba2e-c41719cf0e5e",
      "number": 19,
      "content": "Còn hơn chia chiến lợi phẩm với kẻ kiêu ngạo.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.771Z",
      "updatedAt": "2025-04-22T08:39:05.771Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "00f33462-9b27-4556-a4fb-7a3d7b81e375",
      "number": 20,
      "content": "Ai chú tâm làm theo lời Chúa sẽ tìm được phước;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.781Z",
      "updatedAt": "2025-04-22T08:39:05.781Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "8039d675-6e5a-41a0-bc62-07ff6166cf1b",
      "number": 20,
      "content": "Người nào tin cậy Chúa thật phước hạnh biết bao.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.792Z",
      "updatedAt": "2025-04-22T08:39:05.792Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "ef8a2873-52bc-4a6c-986a-2559e6da6ce7",
      "number": 21,
      "content": "Người có lòng khôn ngoan được gọi là người thông sáng;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.802Z",
      "updatedAt": "2025-04-22T08:39:05.802Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "311d1fbb-d0db-4864-9dde-8c54ea2b8625",
      "number": 21,
      "content": "Lời nói dịu dàng làm tăng sức thuyết phục người nghe.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.812Z",
      "updatedAt": "2025-04-22T08:39:05.812Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "be490203-e3b4-4942-8fdb-e78059e506c7",
      "number": 22,
      "content": "Sự thông sáng là suối sự sống cho người có nó;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.822Z",
      "updatedAt": "2025-04-22T08:39:05.822Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "3496a114-de93-46c5-bee7-722d337e68bd",
      "number": 22,
      "content": "Còn lời chỉ bảo của kẻ dại chỉ là sự điên rồ.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.833Z",
      "updatedAt": "2025-04-22T08:39:05.833Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "df3c6489-0f74-41dd-a672-b436bb9f4dcf",
      "number": 23,
      "content": "Lòng người khôn dạy miệng mình ăn nói;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.843Z",
      "updatedAt": "2025-04-22T08:39:05.843Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "f02d1780-b032-4105-9fff-ae125305a948",
      "number": 23,
      "content": "Và tạo thêm sức thuyết phục ở trên môi.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.853Z",
      "updatedAt": "2025-04-22T08:39:05.853Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "ce46e904-043b-4fed-bed2-21b5cf4703aa",
      "number": 24,
      "content": "Lời hay ý đẹp giống như mật của tàng ong,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.863Z",
      "updatedAt": "2025-04-22T08:39:05.863Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "48c1b70b-9261-4189-bf7b-04cbe095b25a",
      "number": 24,
      "content": "Làm ngọt ngào tâm hồn và khỏe mạnh xương cốt.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.877Z",
      "updatedAt": "2025-04-22T08:39:05.877Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "be1d4dc7-ce36-47af-b8f5-1e7b35b6eecc",
      "number": 25,
      "content": "Có một con đường dường như chính đáng cho người ta,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.890Z",
      "updatedAt": "2025-04-22T08:39:05.890Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "27f3374a-367c-4e2e-ac1f-54f5617a6ad4",
      "number": 25,
      "content": "Nhưng đến cuối đường hóa ra là tử lộ.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.903Z",
      "updatedAt": "2025-04-22T08:39:05.903Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "c3d709b9-f3a4-40ab-b3ac-1e67ff3acacb",
      "number": 26,
      "content": "Cơn đói khát thúc người lao công kiếm việc,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.916Z",
      "updatedAt": "2025-04-22T08:39:05.916Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "006b6ba3-3de9-44cf-bfe6-c689e07b1727",
      "number": 26,
      "content": "Miệng muốn ăn giục người ấy tìm kế sinh nhai.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.929Z",
      "updatedAt": "2025-04-22T08:39:05.929Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "3acd0aac-541a-404c-b2f7-143623381d69",
      "number": 27,
      "content": "Kẻ vô lại toan tính chuyện hại người,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.948Z",
      "updatedAt": "2025-04-22T08:39:05.948Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "a486c31e-7388-4e93-8c6a-e655d31f50d2",
      "number": 27,
      "content": "Môi miệng nó như lửa hừng thiêu hủy.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.962Z",
      "updatedAt": "2025-04-22T08:39:05.962Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "cba4d94f-2751-40ce-9393-a3a23b40981b",
      "number": 28,
      "content": "Kẻ xuyên tạc gieo rắc sự xung đột;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.975Z",
      "updatedAt": "2025-04-22T08:39:05.975Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "31889641-3730-4221-aac4-eed80460de8f",
      "number": 28,
      "content": "Kẻ rỉ tai gây chia rẽ bạn bè.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:05.988Z",
      "updatedAt": "2025-04-22T08:39:05.988Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "1fdcb9bb-f546-480d-b1d7-094a7e26a1d5",
      "number": 29,
      "content": "Kẻ hung bạo xúi bậy người lân cận mình,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:06.001Z",
      "updatedAt": "2025-04-22T08:39:06.001Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "1ca81cf1-de7e-45c2-aaec-5922fc9a684a",
      "number": 29,
      "content": "Và dẫn người ấy vào con đường bất hảo.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:06.016Z",
      "updatedAt": "2025-04-22T08:39:06.016Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "cad2d47b-e4aa-4a22-a0ae-75bed68dfd4d",
      "number": 30,
      "content": "Nó nháy mắt làm hiệu toan tính chuyện gian tà,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:06.030Z",
      "updatedAt": "2025-04-22T08:39:06.030Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "c7ba2bc5-35f9-4ee3-ace6-bffbd91ad7bf",
      "number": 30,
      "content": "Nó bặm môi ngầm bảo thực hiện điều gian ác.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:06.043Z",
      "updatedAt": "2025-04-22T08:39:06.043Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "7916d6f5-ef6b-475b-99de-80090c8fa261",
      "number": 31,
      "content": "Tóc bạc là mão miện hiển vinh;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:06.058Z",
      "updatedAt": "2025-04-22T08:39:06.058Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "60fd4b43-f6e8-4921-84fb-4aadc89fe216",
      "number": 31,
      "content": "Có được nó nhờ sống đời công chính.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:06.071Z",
      "updatedAt": "2025-04-22T08:39:06.071Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "b0db6aa4-46d8-4637-bb5e-d33dc7e1449a",
      "number": 32,
      "content": "Người chậm nóng giận giỏi hơn người dũng mãnh;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:06.084Z",
      "updatedAt": "2025-04-22T08:39:06.084Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "819a557c-55f8-42a3-9a02-365e8dcf86ec",
      "number": 32,
      "content": "Người kiềm chế tính khí mình giỏi hơn người chiếm được thành.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:06.097Z",
      "updatedAt": "2025-04-22T08:39:06.097Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "705f54aa-128e-4b2d-ad6d-2214c66168b2",
      "number": 33,
      "content": "Người ta bắt thăm trong vạt áo,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:06.110Z",
      "updatedAt": "2025-04-22T08:39:06.110Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    },
    {
      "id": "107fe24b-905a-45c9-8aee-4fc4dccbb8cb",
      "number": 33,
      "content": "Nhưng Chúa định đoạt mọi sự.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:39:06.122Z",
      "updatedAt": "2025-04-22T08:39:06.122Z",
      "chapterId": "4084153d-22ed-4f14-a61a-1ac1d3ffbe63"
    }
  ],
