{
  "chapter": {
    "id": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a",
    "number": 16,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2016&version=BD2011",
    "createdAt": "2025-04-24T03:03:19.108Z",
    "updatedAt": "2025-04-24T03:03:19.108Z",
    "bookId": "1b06f6cc-342f-4633-b290-6d1686eb4b30"
  },
  "verses": [
    {
      "id": "6d6a0730-54a2-451a-b0d9-3b44471aaa2a",
      "number": 1,
      "content": "Sự toan tính trong lòng thuộc về loài người,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.386Z",
      "updatedAt": "2025-04-24T04:07:24.386Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "ce4b7d05-8401-4dad-8c8b-e7728c579b32",
      "number": 1,
      "content": "Nhưng sự đáp lời của lưỡi đến từ Chúa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.397Z",
      "updatedAt": "2025-04-24T04:07:24.397Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "d312eaf6-75e8-4580-85c5-801a5d0f51c4",
      "number": 2,
      "content": "Mọi đường lối của người ta đều trong sạch theo mắt họ;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.407Z",
      "updatedAt": "2025-04-24T04:07:24.407Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "2752b35f-6e85-4b8a-9b44-e55e7d4f3db0",
      "number": 2,
      "content": "Nhưng Chúa cân nhắc tấm lòng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.417Z",
      "updatedAt": "2025-04-24T04:07:24.417Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "42ce7348-71d7-42a7-aee7-c9b4c5e42896",
      "number": 3,
      "content": "Hãy phó thác các công việc của bạn cho Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.427Z",
      "updatedAt": "2025-04-24T04:07:24.427Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "772090fe-72a1-4908-8c88-287f05ef3f0d",
      "number": 3,
      "content": "Thì các chương trình của bạn sẽ thành công.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.438Z",
      "updatedAt": "2025-04-24T04:07:24.438Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "0e7672cd-652c-4785-884e-c103b1cbf6b5",
      "number": 4,
      "content": "Chúa có mục đích khi dựng nên mọi vật,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.449Z",
      "updatedAt": "2025-04-24T04:07:24.449Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "dd640cbf-50ca-4ded-a5c3-50f899b76430",
      "number": 4,
      "content": "Ngay cả kẻ gian ác cho ngày tai vạ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.462Z",
      "updatedAt": "2025-04-24T04:07:24.462Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "c8b418fa-9278-4d14-b4ae-07985dbf11de",
      "number": 5,
      "content": "Chúa gớm ghiếc kẻ có lòng kiêu ngạo;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.475Z",
      "updatedAt": "2025-04-24T04:07:24.475Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "f70cc85c-f3aa-424f-a335-f1d1705a2651",
      "number": 5,
      "content": "Chắc chắn nó sẽ không khỏi bị phạt.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.488Z",
      "updatedAt": "2025-04-24T04:07:24.488Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "add5af57-2684-4be7-9fd8-6c2a818365e8",
      "number": 6,
      "content": "Nhờ ơn thương xót và đức chân thật mà lỗi lầm được chuộc,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.505Z",
      "updatedAt": "2025-04-24T04:07:24.505Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "8a75286a-5397-400c-b2a1-4e1c52467acc",
      "number": 6,
      "content": "Do sự kính sợ Chúa mà người ta lánh xa điều ác.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.522Z",
      "updatedAt": "2025-04-24T04:07:24.522Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "e705be51-d1ac-4841-9388-6057700d6a5b",
      "number": 7,
      "content": "Khi đường lối người nào đẹp lòng Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.535Z",
      "updatedAt": "2025-04-24T04:07:24.535Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "6c738f7d-8ab7-4e09-aa91-d5f0c7000511",
      "number": 7,
      "content": "Thì Ngài khiến ngay cả kẻ thù cũng làm hòa với người ấy.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.549Z",
      "updatedAt": "2025-04-24T04:07:24.549Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "64f52328-01d7-49d6-aa32-cb74a60fb887",
      "number": 8,
      "content": "Thà ít của mà ngay lành đạo đức,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.563Z",
      "updatedAt": "2025-04-24T04:07:24.563Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "85a2bb81-7416-463c-9f34-8819f9266ab3",
      "number": 8,
      "content": "Còn hơn lợi tức nhiều mà bất công bất chính cặp theo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.576Z",
      "updatedAt": "2025-04-24T04:07:24.576Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "c399555c-0434-43c1-88e2-792c0feef4e5",
      "number": 9,
      "content": "Lòng người ta toan tính đường lối mình,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.589Z",
      "updatedAt": "2025-04-24T04:07:24.589Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "4eb62459-5abc-4d1a-a407-1f8cb917df63",
      "number": 9,
      "content": "Nhưng Chúa định đoạt các bước của mỗi người.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.603Z",
      "updatedAt": "2025-04-24T04:07:24.603Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "001bb3b4-4bd2-4c88-a1aa-53e400f08d62",
      "number": 10,
      "content": "Khi môi vua đã phán những lời như sấm ngôn bất biến,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.617Z",
      "updatedAt": "2025-04-24T04:07:24.617Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "4e3b62f7-a86a-4c69-8ea4-6bc9b07f6a55",
      "number": 10,
      "content": "Thì miệng vua không được truyền ra một phán quyết bất công.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.630Z",
      "updatedAt": "2025-04-24T04:07:24.630Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "4a3dda13-efbf-4715-b4a2-76b1a167ad2f",
      "number": 11,
      "content": "Cán cân công bình và bàn cân chính xác đều thuộc về Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.643Z",
      "updatedAt": "2025-04-24T04:07:24.643Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "d713117c-c93b-42b7-aeab-f0cbc2bcde4a",
      "number": 11,
      "content": "Tất cả trái cân trong bao là công việc của Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.656Z",
      "updatedAt": "2025-04-24T04:07:24.656Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "dab91e31-dd49-4479-8dd5-269d12b0de5e",
      "number": 12,
      "content": "Vua mà còn làm ác thì quả là điều gớm ghiếc,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.670Z",
      "updatedAt": "2025-04-24T04:07:24.670Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "5fbea3de-2a3d-46df-bff3-87d347a5c8b1",
      "number": 12,
      "content": "Vì ngôi vua được lập lên để thi hành sự công chính.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.684Z",
      "updatedAt": "2025-04-24T04:07:24.684Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "2c820a61-2c11-42d1-b301-e82acb56b8e0",
      "number": 13,
      "content": "Môi chân thật được các vua vui thích,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.697Z",
      "updatedAt": "2025-04-24T04:07:24.697Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "181f8f75-26d3-47d9-8c0f-2272e4e8a47d",
      "number": 13,
      "content": "Ai nói lời chân chính thì được họ mến thương.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.710Z",
      "updatedAt": "2025-04-24T04:07:24.710Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "c9ab236e-a6b3-42bb-82cd-f9d61954db2f",
      "number": 14,
      "content": "Cơn thịnh nộ của vua khác nào sứ giả của tử thần,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.723Z",
      "updatedAt": "2025-04-24T04:07:24.723Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "d32aac25-be73-45a4-913d-d54d285f9d54",
      "number": 14,
      "content": "Nhưng người khôn ngoan biết làm cho nguôi nó.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.737Z",
      "updatedAt": "2025-04-24T04:07:24.737Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "e3fc14a6-dac2-474f-aa80-4d7c182d6b47",
      "number": 15,
      "content": "Nét mặt vua vui đem đến hy vọng sống,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.750Z",
      "updatedAt": "2025-04-24T04:07:24.750Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "f0f40f23-69d9-4f97-8a40-8344b9d6d443",
      "number": 15,
      "content": "Ân điển của vua khác nào mây mang lại trận mưa xuân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.763Z",
      "updatedAt": "2025-04-24T04:07:24.763Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "63de9c19-7056-44ca-a21c-d6840673423e",
      "number": 16,
      "content": "Ðược khôn ngoan sang hơn vàng biết mấy!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.777Z",
      "updatedAt": "2025-04-24T04:07:24.777Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "8828ada3-0b61-46a2-8a05-88a1a95fd549",
      "number": 16,
      "content": "Ðược thông sáng đáng hơn bạc biết bao!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.793Z",
      "updatedAt": "2025-04-24T04:07:24.793Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "5f42fb74-913b-4c4c-8efe-bb7d6fd3ff23",
      "number": 17,
      "content": "Con đường của người chính trực luôn tránh xa điều ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.806Z",
      "updatedAt": "2025-04-24T04:07:24.806Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "e6d013f6-ff93-4334-a60f-38e487826b6d",
      "number": 17,
      "content": "Ai canh giữ đường lối mình giữ được mạng sống mình.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.819Z",
      "updatedAt": "2025-04-24T04:07:24.819Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "6789a334-def3-45bf-be1f-80b10b0b8daf",
      "number": 18,
      "content": "Sự kiêu ngạo đi trước sự hủy diệt;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.833Z",
      "updatedAt": "2025-04-24T04:07:24.833Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "57398145-7497-49db-8472-e3644ad73de8",
      "number": 18,
      "content": "Tính kiêu căng đi trước sự sụp đổ.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.846Z",
      "updatedAt": "2025-04-24T04:07:24.846Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "edef15b7-4cd3-476e-9dc5-6545ff7c5ba0",
      "number": 19,
      "content": "Thà có tinh thần khiêm tốn với người nghèo khó,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.860Z",
      "updatedAt": "2025-04-24T04:07:24.860Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "39e67d67-6a01-43d0-ab35-4784a8d647fd",
      "number": 19,
      "content": "Còn hơn chia chiến lợi phẩm với kẻ kiêu ngạo.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.873Z",
      "updatedAt": "2025-04-24T04:07:24.873Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "1a4eab05-294e-4c8c-bca2-1b632886113f",
      "number": 20,
      "content": "Ai chú tâm làm theo lời Chúa sẽ tìm được phước;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.886Z",
      "updatedAt": "2025-04-24T04:07:24.886Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "8e3d466c-1bd2-40d4-9582-c30dc375360b",
      "number": 20,
      "content": "Người nào tin cậy Chúa thật phước hạnh biết bao.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.899Z",
      "updatedAt": "2025-04-24T04:07:24.899Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "1b69beb1-1636-400f-bbe1-5dc9bf6d0fcd",
      "number": 21,
      "content": "Người có lòng khôn ngoan được gọi là người thông sáng;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.913Z",
      "updatedAt": "2025-04-24T04:07:24.913Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "f17b9fd6-bfae-440e-90ba-527b9bc44cac",
      "number": 21,
      "content": "Lời nói dịu dàng làm tăng sức thuyết phục người nghe.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.926Z",
      "updatedAt": "2025-04-24T04:07:24.926Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "57b497b5-99d1-46fc-bfb9-2f6e486de895",
      "number": 22,
      "content": "Sự thông sáng là suối sự sống cho người có nó;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.939Z",
      "updatedAt": "2025-04-24T04:07:24.939Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "22749cb6-c997-42f6-b4e5-f0201825897e",
      "number": 22,
      "content": "Còn lời chỉ bảo của kẻ dại chỉ là sự điên rồ.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.953Z",
      "updatedAt": "2025-04-24T04:07:24.953Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "456868db-9ee8-4399-a94b-1764fce654e3",
      "number": 23,
      "content": "Lòng người khôn dạy miệng mình ăn nói;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.966Z",
      "updatedAt": "2025-04-24T04:07:24.966Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "faa87591-b41d-4f9c-a7bd-79a89a80c2c4",
      "number": 23,
      "content": "Và tạo thêm sức thuyết phục ở trên môi.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.980Z",
      "updatedAt": "2025-04-24T04:07:24.980Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "de62717b-22d4-48cf-a4ab-5494077214a6",
      "number": 24,
      "content": "Lời hay ý đẹp giống như mật của tàng ong,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:24.993Z",
      "updatedAt": "2025-04-24T04:07:24.993Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "95644f42-73ef-46d9-b3a8-57b17d71e995",
      "number": 24,
      "content": "Làm ngọt ngào tâm hồn và khỏe mạnh xương cốt.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.008Z",
      "updatedAt": "2025-04-24T04:07:25.008Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "5257a482-bf44-405c-9133-02ce4f3b0866",
      "number": 25,
      "content": "Có một con đường dường như chính đáng cho người ta,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.021Z",
      "updatedAt": "2025-04-24T04:07:25.021Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "ff76b106-eeca-47a5-8f11-a17d9a4be556",
      "number": 25,
      "content": "Nhưng đến cuối đường hóa ra là tử lộ.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.034Z",
      "updatedAt": "2025-04-24T04:07:25.034Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "b097699b-fd2a-447f-af11-ab1709367308",
      "number": 26,
      "content": "Cơn đói khát thúc người lao công kiếm việc,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.048Z",
      "updatedAt": "2025-04-24T04:07:25.048Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "1ed28319-38b6-449b-85b6-52c8ed3f8430",
      "number": 26,
      "content": "Miệng muốn ăn giục người ấy tìm kế sinh nhai.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.063Z",
      "updatedAt": "2025-04-24T04:07:25.063Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "554d6a27-a1bb-4cb6-adfe-ea670994e823",
      "number": 27,
      "content": "Kẻ vô lại toan tính chuyện hại người,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.076Z",
      "updatedAt": "2025-04-24T04:07:25.076Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "a4e52f80-f84e-4151-889b-5223beb9ac4a",
      "number": 27,
      "content": "Môi miệng nó như lửa hừng thiêu hủy.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.089Z",
      "updatedAt": "2025-04-24T04:07:25.089Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "36863a76-901a-4894-9b5d-834c4f8b50c2",
      "number": 28,
      "content": "Kẻ xuyên tạc gieo rắc sự xung đột;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.102Z",
      "updatedAt": "2025-04-24T04:07:25.102Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "5b222756-9362-491e-ac72-fb5cc93fba11",
      "number": 28,
      "content": "Kẻ rỉ tai gây chia rẽ bạn bè.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.116Z",
      "updatedAt": "2025-04-24T04:07:25.116Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "a90bc272-f959-4861-838f-d8cbff83f251",
      "number": 29,
      "content": "Kẻ hung bạo xúi bậy người lân cận mình,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.129Z",
      "updatedAt": "2025-04-24T04:07:25.129Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "09d386e7-8988-4d65-829f-65a3866495ad",
      "number": 29,
      "content": "Và dẫn người ấy vào con đường bất hảo.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.142Z",
      "updatedAt": "2025-04-24T04:07:25.142Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "b8345e5e-012a-45ed-8449-005306b69608",
      "number": 30,
      "content": "Nó nháy mắt làm hiệu toan tính chuyện gian tà,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.155Z",
      "updatedAt": "2025-04-24T04:07:25.155Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "35e251cb-53be-43ba-b510-731620a6fbe5",
      "number": 30,
      "content": "Nó bặm môi ngầm bảo thực hiện điều gian ác.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.168Z",
      "updatedAt": "2025-04-24T04:07:25.168Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "16791a47-791c-40ff-b66d-2673b735ad21",
      "number": 31,
      "content": "Tóc bạc là mão miện hiển vinh;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.181Z",
      "updatedAt": "2025-04-24T04:07:25.181Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "b4f754ad-b5d2-42af-bb4e-fddb3ceaf0ee",
      "number": 31,
      "content": "Có được nó nhờ sống đời công chính.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.194Z",
      "updatedAt": "2025-04-24T04:07:25.194Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "a60b72ac-5422-4d51-b7e5-2291c765746c",
      "number": 32,
      "content": "Người chậm nóng giận giỏi hơn người dũng mãnh;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.208Z",
      "updatedAt": "2025-04-24T04:07:25.208Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "9c57d78c-1183-4567-9cea-0b183227fae5",
      "number": 32,
      "content": "Người kiềm chế tính khí mình giỏi hơn người chiếm được thành.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.221Z",
      "updatedAt": "2025-04-24T04:07:25.221Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "eeb1cecd-ff21-4ae5-b075-c3d2889eb085",
      "number": 33,
      "content": "Người ta bắt thăm trong vạt áo,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.234Z",
      "updatedAt": "2025-04-24T04:07:25.234Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    },
    {
      "id": "6fc1d461-325c-4018-9067-ef0809d773a6",
      "number": 33,
      "content": "Nhưng Chúa định đoạt mọi sự.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:07:25.245Z",
      "updatedAt": "2025-04-24T04:07:25.245Z",
      "chapterId": "29275a7f-06c8-462a-9b8f-4b9777fe9e4a"
    }
  ],
