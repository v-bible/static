    },
    {
      "id": "4daa926c-7c51-4d58-8a00-9cb1d5246cb8",
      "content": "Những Lời của Mẹ Vua Lê-mu-ên Khuyên Con Bà",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T08:40:30.203Z",
      "updatedAt": "2025-04-22T08:40:30.203Z",
