{
  "chapter": {
    "id": "95184c6e-4baf-433b-84a0-029972704f42",
    "number": 3,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%203&version=BD2011",
    "createdAt": "2025-04-22T07:37:49.388Z",
    "updatedAt": "2025-04-22T07:37:49.388Z",
    "bookId": "eebb80de-01b2-4f4a-a1ff-0a76541f8302"
  },
  "verses": [
    {
      "id": "d7a0d8a7-5eb2-46ce-acca-8eb7a7befa77",
      "number": 1,
      "content": "Con ơi, chớ quên luật pháp ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.013Z",
      "updatedAt": "2025-04-22T08:37:50.013Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "cebcd80a-93ee-41fa-b0ff-2ea641f3c59c",
      "number": 1,
      "content": "Nhưng hãy để lòng con gìn giữ mệnh lệnh ta;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.040Z",
      "updatedAt": "2025-04-22T08:37:50.040Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "ee5e8f21-1b1f-469e-afe2-a78ef9fc9a6a",
      "number": 2,
      "content": "Vì chúng sẽ làm gia tăng số ngày và số năm của đời con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.051Z",
      "updatedAt": "2025-04-22T08:37:50.051Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "fea360d0-73d2-4064-b481-2fa290ec088c",
      "number": 2,
      "content": "Chúng sẽ đem lại cho con bình an thịnh vượng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.062Z",
      "updatedAt": "2025-04-22T08:37:50.062Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "5b03887a-419d-428c-8bf9-184bc353435f",
      "number": 3,
      "content": "Ðừng để lòng thương xót và tính chân thật lìa khỏi con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.073Z",
      "updatedAt": "2025-04-22T08:37:50.073Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "0769983e-11f7-4f0b-ab29-dd47eb942022",
      "number": 3,
      "content": "Hãy buộc chúng quanh cổ con;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.086Z",
      "updatedAt": "2025-04-22T08:37:50.086Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "fd725c26-ea9c-41bd-aad7-b73411830b96",
      "number": 3,
      "content": "Hãy ghi tạc chúng vào bia lòng con.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.099Z",
      "updatedAt": "2025-04-22T08:37:50.099Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "4ebe0e4f-cd08-47c0-a1b9-0a2d65bcbac9",
      "number": 4,
      "content": "Vì như thế con sẽ được ơn và được tiếng tốt,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.113Z",
      "updatedAt": "2025-04-22T08:37:50.113Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "1bb275b0-6eaf-4ce9-905f-7136eb433c9d",
      "number": 4,
      "content": "Trước mặt Ðức Chúa Trời và luôn cả người ta.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.129Z",
      "updatedAt": "2025-04-22T08:37:50.129Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "8a505ccb-9959-4713-b20c-5ec3620dc70f",
      "number": 5,
      "content": "Hãy hết lòng tin cậy Chúa;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.146Z",
      "updatedAt": "2025-04-22T08:37:50.146Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "cbbd032d-b259-47c3-b855-4e8154e87970",
      "number": 5,
      "content": "Chớ cậy vào sự thông sáng của con;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.166Z",
      "updatedAt": "2025-04-22T08:37:50.166Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "c539c9bf-ada6-4c93-9f61-0ade4219e3a0",
      "number": 6,
      "content": "Phàm trong các việc làm của con, khá nhận biết Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.179Z",
      "updatedAt": "2025-04-22T08:37:50.179Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "a5673b29-f6e4-4630-80c6-c57f5747ca22",
      "number": 6,
      "content": "Thì Ngài sẽ chỉ dẫn các lối của con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.193Z",
      "updatedAt": "2025-04-22T08:37:50.193Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "fd76bfe5-f0a1-406f-a5fe-15a426189541",
      "number": 7,
      "content": "Ðừng cho mình là khôn ngoan theo mắt mình;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.206Z",
      "updatedAt": "2025-04-22T08:37:50.206Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "6ff2407b-4538-4910-aa3c-c037f5b83a21",
      "number": 7,
      "content": "Hãy kính sợ Chúa và lánh xa điều ác.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.218Z",
      "updatedAt": "2025-04-22T08:37:50.218Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "077f69c7-68ea-4e0f-be2c-190687ab90c3",
      "number": 8,
      "content": "Như vậy thân xác con sẽ được mạnh khỏe,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.231Z",
      "updatedAt": "2025-04-22T08:37:50.231Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "c32a6865-5c49-4f20-82aa-8cadab1efcdf",
      "number": 8,
      "content": "Và xương cốt con sẽ được cứng cáp vững vàng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.242Z",
      "updatedAt": "2025-04-22T08:37:50.242Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "c6b85299-e86a-410b-bcac-4945f5d3f91f",
      "number": 9,
      "content": "Hãy dùng của cải con để tôn vinh Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.255Z",
      "updatedAt": "2025-04-22T08:37:50.255Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "b49d4ca4-6212-471c-a8d7-b1882dcb87bd",
      "number": 9,
      "content": "Hãy dâng các hoa lợi đầu mùa của con để cảm tạ Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.264Z",
      "updatedAt": "2025-04-22T08:37:50.264Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "9c78b41b-d490-435d-bd08-864f47fe05b4",
      "number": 10,
      "content": "Như vậy các vựa lẫm của con sẽ tràn đầy lúa thóc,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.275Z",
      "updatedAt": "2025-04-22T08:37:50.275Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "04e8aa25-b5d6-455b-b7e1-fee15e5b250f",
      "number": 10,
      "content": "Các thùng rượu của con rượu mới sẽ tràn đầy.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.285Z",
      "updatedAt": "2025-04-22T08:37:50.285Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "5e980bd4-8c3a-4ca6-8302-f045aef373c9",
      "number": 11,
      "content": "Con ơi, chớ coi thường sự sửa phạt của Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.301Z",
      "updatedAt": "2025-04-22T08:37:50.301Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "fca39783-8268-4813-813d-ed3f8a60f394",
      "number": 11,
      "content": "Ðừng bất mãn trước sự quở trách của Ngài;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.311Z",
      "updatedAt": "2025-04-22T08:37:50.311Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "b4b15b0a-5075-44fa-ba57-fd96bd4b243c",
      "number": 12,
      "content": "Vì ai được Chúa thương thì Ngài quở trách,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.323Z",
      "updatedAt": "2025-04-22T08:37:50.323Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "61db7b3c-8ba8-417e-973f-04797adb7a51",
      "number": 12,
      "content": "Như cha thương con mình nên trách phạt khuyên răn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.333Z",
      "updatedAt": "2025-04-22T08:37:50.333Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "86c0247a-5407-4aae-9bbb-fbb63a2a6104",
      "number": 13,
      "content": "Phước thay cho người tìm được sự khôn ngoan,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.343Z",
      "updatedAt": "2025-04-22T08:37:50.343Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "b0e12115-e891-474b-85cf-f1b0e6c54a33",
      "number": 13,
      "content": "Và cho người có được sự thông sáng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.364Z",
      "updatedAt": "2025-04-22T08:37:50.364Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "7e71dd48-b41a-4978-a7fe-29516f1cad74",
      "number": 14,
      "content": "Vì lợi ích của khôn ngoan trỗi hơn tiền bạc;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.374Z",
      "updatedAt": "2025-04-22T08:37:50.374Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "b733f6b9-3372-4f3f-be56-b0870b88d44a",
      "number": 14,
      "content": "Ích lợi của nàng lấn át cả vàng ròng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.386Z",
      "updatedAt": "2025-04-22T08:37:50.386Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "aaa5a776-f014-4526-8127-5d347d276447",
      "number": 15,
      "content": "Nàng quý báu hơn châu ngọc;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.397Z",
      "updatedAt": "2025-04-22T08:37:50.397Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "6a973471-7d7b-45d6-9d7b-d6f5f00201e3",
      "number": 15,
      "content": "Chẳng có vật gì con ưa thích có thể sánh được với nàng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.423Z",
      "updatedAt": "2025-04-22T08:37:50.423Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "e7b6af32-55ef-4052-81a7-066b589b7f04",
      "number": 16,
      "content": "Tay phải nàng phát ban trường thọ;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.434Z",
      "updatedAt": "2025-04-22T08:37:50.434Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "723c8384-6d72-48a9-af51-53092cecba22",
      "number": 16,
      "content": "Tay trái nàng cho giàu có lẫn hiển vinh.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.447Z",
      "updatedAt": "2025-04-22T08:37:50.447Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "4474df3a-2bd6-4263-8b88-b5ce99df227c",
      "number": 17,
      "content": "Con đường nàng là con đường vui thỏa;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.459Z",
      "updatedAt": "2025-04-22T08:37:50.459Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "0be26d82-a3dc-4ef1-acdb-d6dd10ad9a63",
      "number": 17,
      "content": "Mọi nẻo nàng là mọi ngả bình an.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.469Z",
      "updatedAt": "2025-04-22T08:37:50.469Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "4c2a5893-28dd-4939-aa21-3686cb4120b6",
      "number": 18,
      "content": "Khôn ngoan là cây sự sống cho người nào nắm giữ được;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.479Z",
      "updatedAt": "2025-04-22T08:37:50.479Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "e530ffb5-f16d-4f6e-b751-61f7b75e9051",
      "number": 18,
      "content": "Ai giữ chặt nàng sẽ được phước lâu dài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.492Z",
      "updatedAt": "2025-04-22T08:37:50.492Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "cf1ada99-c439-4f9c-a903-0f35be6bab39",
      "number": 19,
      "content": "Chúa dùng sự khôn ngoan để dựng nên trái đất;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.502Z",
      "updatedAt": "2025-04-22T08:37:50.502Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "05404e83-a95c-45d3-a5d2-46e4b2013141",
      "number": 19,
      "content": "Ngài dùng sự thông sáng để tạo lập các tầng trời;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.526Z",
      "updatedAt": "2025-04-22T08:37:50.526Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "61aeb62d-9eca-4bec-b2dd-0e1abacfceec",
      "number": 20,
      "content": "Nhờ tri thức của Ngài các vực sâu mở rộng;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.536Z",
      "updatedAt": "2025-04-22T08:37:50.536Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "9884aa52-8c95-4e65-9e61-236904088376",
      "number": 20,
      "content": "Những áng mây mù nhỏ những giọt sương đêm.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.547Z",
      "updatedAt": "2025-04-22T08:37:50.547Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "65fcb9fe-2e6c-45a6-845a-21edbcc2063d",
      "number": 21,
      "content": "Con ơi, đừng để những điều này vuột khỏi tầm mắt con:",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.558Z",
      "updatedAt": "2025-04-22T08:37:50.558Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "4207844e-db4c-4d78-aa81-bc22368c99e4",
      "number": 21,
      "content": "Hãy giữ lấy sự khôn ngoan và sự cẩn trọng.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.579Z",
      "updatedAt": "2025-04-22T08:37:50.579Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "f81a4d0d-97e5-4dc3-b6b5-e3336a48fbaf",
      "number": 22,
      "content": "Những điều ấy sẽ là sức sống của linh hồn con,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.589Z",
      "updatedAt": "2025-04-22T08:37:50.589Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "ac8d6dd1-d909-41ae-a353-f4064b952946",
      "number": 22,
      "content": "Và là những đồ trang sức nơi cổ con.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.599Z",
      "updatedAt": "2025-04-22T08:37:50.599Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "4aa577d7-8fb9-4746-9121-e0c864a5c125",
      "number": 23,
      "content": "Nhờ những điều ấy đường con đi sẽ an toàn bảo đảm,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.615Z",
      "updatedAt": "2025-04-22T08:37:50.615Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "b967be11-755b-49af-ba32-b77077d58986",
      "number": 23,
      "content": "Bước chân con sẽ chẳng vấp phạm bao giờ.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.627Z",
      "updatedAt": "2025-04-22T08:37:50.627Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "9bb68ee1-2542-42a0-b89e-ac0fb0f46b63",
      "number": 24,
      "content": "Lúc nằm nghỉ con sẽ không lo sợ;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.638Z",
      "updatedAt": "2025-04-22T08:37:50.638Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "d9f2d7c8-436c-4ac8-a01a-c208c89307d1",
      "number": 24,
      "content": "Khi đặt lưng con sẽ an giấc ngủ ngon.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.654Z",
      "updatedAt": "2025-04-22T08:37:50.654Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "df6f77b8-9052-476e-8a8e-47bedbf56f07",
      "number": 25,
      "content": "Ðừng sợ hãi nỗi kinh hoàng xảy ra bất chợt;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.668Z",
      "updatedAt": "2025-04-22T08:37:50.668Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "79f51eec-fe91-4d19-9399-2277bf6c3d9d",
      "number": 25,
      "content": "Ðừng âu lo khi đợt phá hoại của kẻ ác đến gần;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.678Z",
      "updatedAt": "2025-04-22T08:37:50.678Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "b659f2a2-0754-48b8-8683-aaedeb6102cd",
      "number": 26,
      "content": "Vì Chúa sẽ là nguồn trông cậy của con;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.692Z",
      "updatedAt": "2025-04-22T08:37:50.692Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "12cd608f-a3d6-406d-ad44-50814134e9f8",
      "number": 26,
      "content": "Ngài sẽ giữ chân con khỏi sa vào bẫy.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.703Z",
      "updatedAt": "2025-04-22T08:37:50.703Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "cf3e4618-f109-4b06-a9d1-48a108189bce",
      "number": 27,
      "content": "Ðừng từ chối làm việc thiện cho những người đáng giúp,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.713Z",
      "updatedAt": "2025-04-22T08:37:50.713Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "6a5d61d6-e926-48f9-b773-197b18709faf",
      "number": 27,
      "content": "Nếu con có khả năng, hãy giúp đỡ người ta.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.723Z",
      "updatedAt": "2025-04-22T08:37:50.723Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "27d1e077-f70e-41b4-87db-4161488aadb1",
      "number": 28,
      "content": "Con đừng nói với người lân cận, “Về đi, mai trở lại, tôi sẽ cho,”",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.735Z",
      "updatedAt": "2025-04-22T08:37:50.735Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "cc5fff90-d2f7-4905-9ad0-3cfa1a44e4c7",
      "number": 28,
      "content": "Trong khi con có sẵn điều người ấy đang cần.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.749Z",
      "updatedAt": "2025-04-22T08:37:50.749Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "d4cc87e5-1059-4cd7-bc9a-da53a3f969a1",
      "number": 29,
      "content": "Con chớ lập mưu hại người lân cận,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.762Z",
      "updatedAt": "2025-04-22T08:37:50.762Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "81446b48-73ff-4249-80e6-a2b13b91d407",
      "number": 29,
      "content": "Người ở gần và tin tưởng nơi con.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.775Z",
      "updatedAt": "2025-04-22T08:37:50.775Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "888ad057-3257-4cdc-a6b9-3d4ee9616033",
      "number": 30,
      "content": "Con đừng gây sự với ai vô cớ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.790Z",
      "updatedAt": "2025-04-22T08:37:50.790Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "aeac0dc0-eca5-46a6-b2ef-d3345b0ca658",
      "number": 30,
      "content": "Khi không ai gây cho con thiệt hại điều gì.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.810Z",
      "updatedAt": "2025-04-22T08:37:50.810Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "ad9d7385-56fd-472e-8775-99b86ea491c9",
      "number": 31,
      "content": "Ðừng ganh tị với quân tàn bạo;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.822Z",
      "updatedAt": "2025-04-22T08:37:50.822Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "bef9140f-d163-4bb1-8b55-db68a292b82e",
      "number": 31,
      "content": "Chớ chọn đường nào chúng chọn lấy để đi;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.833Z",
      "updatedAt": "2025-04-22T08:37:50.833Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "224f07b9-c809-44e1-9fe4-9dd5132a660e",
      "number": 32,
      "content": "Vì Chúa ghê tởm kẻ gian tà tội lỗi,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.846Z",
      "updatedAt": "2025-04-22T08:37:50.846Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "e451b0b7-c370-40fb-813a-29666ba8aabd",
      "number": 32,
      "content": "Nhưng Ngài kết bạn tâm giao với người chính trực ngay lành.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.859Z",
      "updatedAt": "2025-04-22T08:37:50.859Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "57e410de-0bd2-4499-bf70-0279275b6fd7",
      "number": 33,
      "content": "Sự nguyền rủa của Chúa giáng trên nhà kẻ gian ác,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.869Z",
      "updatedAt": "2025-04-22T08:37:50.869Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "88c955ba-20fc-4d04-94c8-4e5d05b17249",
      "number": 33,
      "content": "Nhưng phước hạnh Ngài ban cho chỗ ở người ngay lành.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.879Z",
      "updatedAt": "2025-04-22T08:37:50.879Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "a7e33727-6754-481a-9d58-c5c5ce8ad4fb",
      "number": 34,
      "content": "Ngài chế giễu phường chê bai nhạo báng,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.891Z",
      "updatedAt": "2025-04-22T08:37:50.891Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "2388e481-b630-41aa-8244-938fc3d97333",
      "number": 34,
      "content": "Nhưng Ngài ban ơn cho người thật sự khiêm nhường.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.901Z",
      "updatedAt": "2025-04-22T08:37:50.901Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "81503314-fd99-4296-bbdf-5d6aac61de17",
      "number": 35,
      "content": "Người khôn ngoan sẽ thừa hưởng những hiển vinh tôn trọng,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.911Z",
      "updatedAt": "2025-04-22T08:37:50.911Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    },
    {
      "id": "b5611a53-99d3-423d-93ca-c37a0f5c5938",
      "number": 35,
      "content": "Còn kẻ điên rồ bị tống cho nhục nhã ê chề.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:50.921Z",
      "updatedAt": "2025-04-22T08:37:50.921Z",
      "chapterId": "95184c6e-4baf-433b-84a0-029972704f42"
    }
  ],
