  "headings": [
    {
      "id": "9d384ab4-3c0d-4e50-91dd-4c4e143281e6",
      "content": "Xã Hội Băng Hoại Hoàn Toàn",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T09:03:25.356Z",
      "updatedAt": "2025-04-22T09:03:25.356Z",
      "verseId": "0dc00190-95d8-4f52-9ecd-3366f27c13ef",
      "chapterId": "293845dc-cd4c-4dac-94a3-1e0205ac9e59"
    },
    {
      "id": "7b0594e3-62d3-4e59-a8df-6af2ecd260bf",
      "content": "Lời Tiên Tri về Sự Tái Lập",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T09:03:25.944Z",
      "updatedAt": "2025-04-22T09:03:25.944Z",
      "verseId": "15d5938a-9f9e-4e78-af6d-cbbc440b420f",
      "chapterId": "293845dc-cd4c-4dac-94a3-1e0205ac9e59"
    },
    {
      "id": "adb3f15d-6e8d-4f4e-ac37-3da3c40bb501",
      "content": "Lòng Nhân Từ của Ðức Chúa Trời",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T09:03:26.264Z",
      "updatedAt": "2025-04-22T09:03:26.264Z",
      "verseId": "7cb061ca-206b-485b-8c84-bc89f109e263",
      "chapterId": "293845dc-cd4c-4dac-94a3-1e0205ac9e59"
    },
    {
      "id": "fdeed57b-43d4-474e-b056-b15fa81c047f",
      "content": "Bền Lòng Tin Cậy CHÚA",
      "level": 1,
      "order": 0,
