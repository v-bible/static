    },
    {
      "id": "7d16914c-383b-4bcd-8a97-d3a6e7eaedf0",
      "content": "Dân Sa-phia thời xưa nổi tiếng là mặc đẹp",
      "position": 13,
      "order": 1,
      "createdAt": "2025-04-24T04:31:57.569Z",
      "updatedAt": "2025-04-24T04:31:57.569Z",
      "verseId": "70285726-f5a5-42ba-b474-572c73e3a319",
      "chapterId": "f00ce6d7-72a6-41d7-ae85-ce1fca597b34"
    },
    {
      "id": "3bc957f4-5c08-43f4-9e7b-e84aac9c1a7f",
      "content": "Xa-a-nan có nghĩa là “ra đi”",
      "position": 14,
      "order": 2,
      "createdAt": "2025-04-24T04:31:57.599Z",
      "updatedAt": "2025-04-24T04:31:57.599Z",
      "verseId": "259afd6d-865b-4807-a255-1dde0f638c7c",
      "chapterId": "f00ce6d7-72a6-41d7-ae85-ce1fca597b34"
    },
    {
      "id": "208a2de7-feca-4dda-afcf-0e2b019442be",
      "content": "Bết Ê-xên có nghĩa là “nhà chật”",
      "position": 15,
      "order": 3,
      "createdAt": "2025-04-24T04:31:57.625Z",
      "updatedAt": "2025-04-24T04:31:57.625Z",
      "verseId": "44bdf309-a7a8-4231-8d97-b1b57d1b7a50",
      "chapterId": "f00ce6d7-72a6-41d7-ae85-ce1fca597b34"
    },
    {
      "id": "fd1e4b77-55eb-4032-9cde-ea2f0fc2e073",
      "content": "nt: của ngươi",
      "position": 48,
      "order": 4,
      "createdAt": "2025-04-24T04:31:57.637Z",
      "updatedAt": "2025-04-24T04:31:57.637Z",
      "verseId": "44bdf309-a7a8-4231-8d97-b1b57d1b7a50",
      "chapterId": "f00ce6d7-72a6-41d7-ae85-ce1fca597b34"
    },
    {
      "id": "b1af3ea7-e2d9-4490-91bf-ec56fa1ec5b8",
      "content": "Ách-xíp có nghĩa là “dối gạt”",
      "position": 53,
      "order": 5,
      "createdAt": "2025-04-24T04:31:57.754Z",
      "updatedAt": "2025-04-24T04:31:57.754Z",
      "verseId": "e9da9822-0148-46ec-a066-6f719fa95381",
      "chapterId": "f00ce6d7-72a6-41d7-ae85-ce1fca597b34"
    },
    {
      "id": "1b40fd23-2578-4595-9dfe-3d00b9142fd0",
      "content": "Ma-rê-sa có nghĩa là “kẻ chinh phục”",
      "position": 20,
      "order": 6,
      "createdAt": "2025-04-24T04:31:57.775Z",
      "updatedAt": "2025-04-24T04:31:57.775Z",
      "verseId": "4443f873-2dcf-4062-95f9-ce60b13087da",
      "chapterId": "f00ce6d7-72a6-41d7-ae85-ce1fca597b34"
    }
  ],
