    },
    {
      "id": "3864acdd-6b9b-4c16-a237-f97b4a081b93",
      "content": "Dân Sa-phia thời xưa nổi tiếng là mặc đẹp",
      "position": 13,
      "order": 1,
      "createdAt": "2025-04-22T09:02:53.920Z",
      "updatedAt": "2025-04-22T09:02:53.920Z",
      "verseId": "9a62e3ca-dc89-4416-a426-56d7fc9cb37a",
      "chapterId": "9267dfb2-fd22-452a-92be-55816bd3498e"
    },
    {
      "id": "c92a638b-ab91-4c26-8740-ab0ba14bc8df",
      "content": "Xa-a-nan có nghĩa là “ra đi”",
      "position": 14,
      "order": 2,
      "createdAt": "2025-04-22T09:02:53.946Z",
      "updatedAt": "2025-04-22T09:02:53.946Z",
      "verseId": "3df9d8ba-4c47-4b0d-8aaa-3c5fb5bb32c4",
      "chapterId": "9267dfb2-fd22-452a-92be-55816bd3498e"
    },
    {
      "id": "9cfafec4-5706-4307-b3be-93e1a84066f3",
      "content": "Bết Ê-xên có nghĩa là “nhà chật”",
      "position": 15,
      "order": 3,
      "createdAt": "2025-04-22T09:02:53.968Z",
      "updatedAt": "2025-04-22T09:02:53.968Z",
      "verseId": "bc0750f0-d5c2-4a07-b338-c28eec3fc441",
      "chapterId": "9267dfb2-fd22-452a-92be-55816bd3498e"
    },
    {
      "id": "fc8857cf-92e1-46f5-9c23-3fd5b276cff0",
      "content": "nt: của ngươi",
      "position": 48,
      "order": 4,
      "createdAt": "2025-04-22T09:02:53.979Z",
      "updatedAt": "2025-04-22T09:02:53.979Z",
      "verseId": "bc0750f0-d5c2-4a07-b338-c28eec3fc441",
      "chapterId": "9267dfb2-fd22-452a-92be-55816bd3498e"
    },
    {
      "id": "2a9bb1dc-5892-4948-a72c-21f0120ffe49",
      "content": "Ách-xíp có nghĩa là “dối gạt”",
      "position": 53,
      "order": 5,
      "createdAt": "2025-04-22T09:02:54.089Z",
      "updatedAt": "2025-04-22T09:02:54.089Z",
      "verseId": "bb3731b2-b6f6-4465-9e06-defd02b3eb2e",
      "chapterId": "9267dfb2-fd22-452a-92be-55816bd3498e"
    },
    {
      "id": "e1ccb27e-6741-4dc3-9654-09746e052646",
      "content": "Ma-rê-sa có nghĩa là “kẻ chinh phục”",
      "position": 20,
      "order": 6,
      "createdAt": "2025-04-22T09:02:54.121Z",
      "updatedAt": "2025-04-22T09:02:54.121Z",
