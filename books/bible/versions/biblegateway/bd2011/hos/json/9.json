      "chapterId": "784e85e1-973c-4f74-814f-ac6661709111"
    },
    {
      "id": "c1a5937a-63e3-42aa-9290-a6a51f66c708",
      "content": "Hình Phạt Xưa và Nay",
      "order": 0,
      "createdAt": "2024-09-24T02:39:01.116Z",
