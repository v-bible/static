    },
    {
      "id": "12fbaa8e-de50-4d36-aff6-c3ec8fe8aacb",
      "content": "Cổng Hẹp và Ðường Hẹp",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T09:06:44.583Z",
      "updatedAt": "2025-04-22T09:06:44.583Z",
