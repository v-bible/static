      "chapterId": "5c07f169-9cdf-4281-8036-d696a33a2b79"
    },
    {
      "id": "4d8f9079-cbf3-4d02-8c00-8a2f9858f4f2",
      "content": "Xét Ðoán Người Khác",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T09:06:44.350Z",
      "updatedAt": "2025-04-22T09:06:44.350Z",
      "verseId": "99369e25-0d80-4ab4-bc4b-5c36b5ebcb53",
      "chapterId": "5c07f169-9cdf-4281-8036-d696a33a2b79"
