    }
  ],
  "headings": [
    {
      "id": "314e0ef7-8c47-47cd-9425-3cda22390a24",
      "content": "Sự Tích Chúa Giáng Sinh",
      "level": 2,
      "order": 0,
      "createdAt": "2025-04-22T09:06:12.303Z",
      "updatedAt": "2025-04-22T09:06:12.303Z",
      "verseId": "21142466-3012-4d09-a9b9-6f78f035161f",
      "chapterId": "7d425f2a-b6bf-49ac-b426-8ded783224dc"
    },
    {
      "id": "cc4c8f9c-0ec8-44d0-a90c-5c735193d72b",
      "content": "GIA PHẢ VÀ SỰ GIÁNG SINH CỦA ÐỨC CHÚA JESUS",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T09:06:11.984Z",
      "updatedAt": "2025-04-22T09:06:11.984Z",
      "verseId": "885e26fc-22ac-4421-9200-aac178dc3eaa",
      "chapterId": "7d425f2a-b6bf-49ac-b426-8ded783224dc"
