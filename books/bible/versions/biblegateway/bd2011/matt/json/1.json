  "headings": [
    {
      "id": "af22430a-013e-4313-843c-8c5e0e44c2a2",
      "content": "Sự Tích Chúa Giáng Sinh",
      "level": 2,
      "order": 0,
      "createdAt": "2025-04-24T04:35:09.282Z",
      "updatedAt": "2025-04-24T04:35:09.282Z",
      "verseId": "694f3b29-86a5-4a0e-a284-d4522c69c02d",
      "chapterId": "bbe469ab-f019-4adb-8dca-9c710055387d"
    },
    {
      "id": "6a4f6f25-53f0-4427-930a-17c3e1d136ec",
      "content": "GIA PHẢ VÀ SỰ GIÁNG SINH CỦA ÐỨC CHÚA JESUS",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T04:35:08.961Z",
      "updatedAt": "2025-04-24T04:35:08.961Z",
      "verseId": "ad2e091b-79ae-472a-9e29-2bd2266db37c",
      "chapterId": "bbe469ab-f019-4adb-8dca-9c710055387d"
