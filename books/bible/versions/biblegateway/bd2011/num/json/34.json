      "chapterId": "ef533a74-1c82-4fa1-a4ce-e179a3a1989a"
    },
    {
      "id": "54c99e40-c0c9-4f07-a707-1889f3fcd64c",
      "content": "Biển Ðịa Trung Hải",
      "position": 48,
      "order": 2,
      "createdAt": "2025-04-24T03:17:36.686Z",
