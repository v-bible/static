{
  "chapter": {
    "id": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Ai%20Ca%201&version=BD2011",
    "createdAt": "2024-09-24T01:00:48.682Z",
    "updatedAt": "2024-09-24T01:00:48.682Z",
    "bookId": "8f8552cf-33c5-4092-92d0-c47be22f31ba"
  },
  "verses": [
    {
      "id": "7c15ed9b-cb7a-436c-83ff-e642af5965e8",
      "number": 1,
      "content": "Kìa thành phố một thời đông người nhộn nhịp,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.255Z",
      "updatedAt": "2024-09-24T02:31:35.255Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "d1e3a917-6796-47d4-b6ff-5e56b35f528b",
      "number": 1,
      "content": "Nay thưa người đơn chiếc đìu hiu!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.260Z",
      "updatedAt": "2024-09-24T02:31:35.260Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "70e19ea2-f79d-456e-8419-859613f52f73",
      "number": 1,
      "content": "Kìa nàng vốn đại đô các nước,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.263Z",
      "updatedAt": "2024-09-24T02:31:35.263Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "c3beafa9-b82e-4c27-b326-b5a5fbf2514d",
      "number": 1,
      "content": "Giờ trở thành góa phụ cô đơn!",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.265Z",
      "updatedAt": "2024-09-24T02:31:35.265Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "9a01e209-4695-45a8-91d5-da81fe6ac239",
      "number": 1,
      "content": "Kìa nàng vốn nữ hoàng các tiểu quốc,",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.267Z",
      "updatedAt": "2024-09-24T02:31:35.267Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "e45e6bde-9cff-4163-98ab-cdb499c2764c",
      "number": 1,
      "content": "Nay biến thành nữ nô lệ phục tùng!",
      "order": 5,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.270Z",
      "updatedAt": "2024-09-24T02:31:35.270Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "0fe6d889-f06d-43a1-a682-115d068c1112",
      "number": 2,
      "content": "Giữa đêm khuya nàng khóc than cay đắng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.272Z",
      "updatedAt": "2024-09-24T02:31:35.272Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "fee0ae87-671d-420a-b7ca-685c19646b8e",
      "number": 2,
      "content": "Lệ tuôn tràn trên đôi má gầy gò;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.274Z",
      "updatedAt": "2024-09-24T02:31:35.274Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "69600da1-f053-465f-ac2f-89dfb73bf13a",
      "number": 2,
      "content": "Trong số tất cả tình nhân của nàng, không ai an ủi nàng;",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.277Z",
      "updatedAt": "2024-09-24T02:31:35.277Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "3c245469-d8a4-4c55-b6a4-bf0d5e074c7d",
      "number": 2,
      "content": "Tất cả các bạn nàng đã phụ rẫy nàng,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.279Z",
      "updatedAt": "2024-09-24T02:31:35.279Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "adff8df1-827b-4963-a3c5-50e54475b1ef",
      "number": 2,
      "content": "Thậm chí chúng còn trở thành kẻ thù của nàng.",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.282Z",
      "updatedAt": "2024-09-24T02:31:35.282Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "d9792607-6f7b-4d95-92a7-b96106b3790f",
      "number": 3,
      "content": "Giu-đa đã bị lưu đày,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.284Z",
      "updatedAt": "2024-09-24T02:31:35.284Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "1018cdbf-f44b-49fd-8c67-63b3381e6a40",
      "number": 3,
      "content": "Nàng phải chịu biết bao đau đớn và phải làm lao động khổ sai;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.286Z",
      "updatedAt": "2024-09-24T02:31:35.286Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "5bdeaf11-2bdd-4647-94ee-a4b5ddb74eba",
      "number": 3,
      "content": "Bây giờ nàng phải sống rải rác giữa các nước,",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.289Z",
      "updatedAt": "2024-09-24T02:31:35.289Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "43fd240b-942c-40c5-9b19-69d723df2198",
      "number": 3,
      "content": "Nàng chẳng tìm được nơi nào để nghỉ ngơi;",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.291Z",
      "updatedAt": "2024-09-24T02:31:35.291Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "c13d62cf-2c95-48b5-979b-a9aa23b80c0e",
      "number": 3,
      "content": "Mọi kẻ truy đuổi nàng đều bắt kịp nàng trong lúc nàng cùng đường.",
      "order": 4,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.293Z",
      "updatedAt": "2024-09-24T02:31:35.293Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "7a9c2d0b-fe73-4b73-9340-c5eb4b5c3f8a",
      "number": 4,
      "content": "Những con đường dẫn đến Si-ôn âu sầu ảm đạm,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.295Z",
      "updatedAt": "2024-09-24T02:31:35.295Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "4ba6be0c-a209-40a3-9276-1e406280033b",
      "number": 4,
      "content": "Vì không ai đến dự các đại lễ như xưa;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.297Z",
      "updatedAt": "2024-09-24T02:31:35.297Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "22369e41-bf7f-492a-9c8d-c57c5f6b0299",
      "number": 4,
      "content": "Tất cả các cổng thành đều đìu hiu quạnh quẽ,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.300Z",
      "updatedAt": "2024-09-24T02:31:35.300Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "a8dc547c-67a9-4b6b-bdb5-39b424a400c0",
      "number": 4,
      "content": "Các tư tế đều rầu rĩ thở than,",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.303Z",
      "updatedAt": "2024-09-24T02:31:35.303Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "ee3ea679-3174-4115-8f0f-7972dbb1ece7",
      "number": 4,
      "content": "Các thiếu nữ của nàng buồn da diết,",
      "order": 4,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.306Z",
      "updatedAt": "2024-09-24T02:31:35.306Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "ff2b8759-a305-4ace-a1e7-6d1962647200",
      "number": 4,
      "content": "Số phận nàng thật đau khổ đắng cay.",
      "order": 5,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.308Z",
      "updatedAt": "2024-09-24T02:31:35.308Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "467cd7e4-df00-43ed-882b-98de24fb4928",
      "number": 5,
      "content": "Những kẻ thù ghét nàng đã trở thành các chủ nhân nàng,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.311Z",
      "updatedAt": "2024-09-24T02:31:35.311Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "18f64f90-2c66-40f6-b141-f1436eea29cb",
      "number": 5,
      "content": "Những kẻ thù của nàng đã phát đạt.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.313Z",
      "updatedAt": "2024-09-24T02:31:35.313Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "a2fb4b6f-5920-4f6b-a42d-9ee48a9ea6f8",
      "number": 5,
      "content": "Vì vô số tội lỗi của nàng,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.319Z",
      "updatedAt": "2024-09-24T02:31:35.319Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "4063e0d4-d172-45e6-9417-d42f1d7da18e",
      "number": 5,
      "content": "Chúa bắt nàng phải chịu đau khổ;",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.327Z",
      "updatedAt": "2024-09-24T02:31:35.327Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "f02c96d0-88f5-4e2a-aa7e-717b034f74fd",
      "number": 5,
      "content": "Con cái nàng phải sống tha phương,",
      "order": 4,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.329Z",
      "updatedAt": "2024-09-24T02:31:35.329Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "eeae01ca-d7f7-4cbb-a074-eeed682bbb37",
      "number": 5,
      "content": "Vì bị quân thù đem lưu đày biệt xứ.",
      "order": 5,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.331Z",
      "updatedAt": "2024-09-24T02:31:35.331Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "f83365ec-cb1d-4863-b4ed-c529af4abfe5",
      "number": 6,
      "content": "Ái Nữ của Si-ôn đã hết thời,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.334Z",
      "updatedAt": "2024-09-24T02:31:35.334Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "53646bb9-690c-43a3-b2ab-2f4b971e373f",
      "number": 6,
      "content": "Vẻ cao sang quý phái của nàng không còn nữa;",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.337Z",
      "updatedAt": "2024-09-24T02:31:35.337Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "7ccde753-d217-4175-aafb-10144087b290",
      "number": 6,
      "content": "Những kẻ cầm quyền của nàng như nai đói không tìm ra đồng cỏ;",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.341Z",
      "updatedAt": "2024-09-24T02:31:35.341Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "ce93ac1a-4eb7-432a-8ecd-69b5a662485d",
      "number": 6,
      "content": "Họ bạc nhược bỏ chạy trước mặt kẻ truy kích họ.",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.344Z",
      "updatedAt": "2024-09-24T02:31:35.344Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "987f7942-0238-4cff-a9eb-632d6aa0dbc1",
      "number": 7,
      "content": "Giữa những ngày nàng đau khổ và lang thang,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.346Z",
      "updatedAt": "2024-09-24T02:31:35.346Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "cdce9b99-5963-4c22-b729-e0a15fa6e648",
      "number": 7,
      "content": "Giê-ru-sa-lem nhớ lại mọi phước hạnh nàng đã hưởng ngày xưa.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.349Z",
      "updatedAt": "2024-09-24T02:31:35.349Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "d4f06f6e-6e93-4994-85b5-b17a643a27f0",
      "number": 7,
      "content": "Khi dân nàng sa vào tay quân thù, chẳng ai đến tiếp cứu nàng;",
      "order": 2,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.352Z",
      "updatedAt": "2024-09-24T02:31:35.352Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "80bd5f5b-803c-46c3-add8-38ad7958db0e",
      "number": 7,
      "content": "Thấy tình cảnh của nàng cô đơn như thế, kẻ thù của nàng cười hả hê trước sự sụp đổ của nàng.",
      "order": 3,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.354Z",
      "updatedAt": "2024-09-24T02:31:35.354Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "0de5c6dc-2d31-4c9e-9829-fa4ccd0c7ce2",
      "number": 8,
      "content": "Giê-ru-sa-lem đã phạm quá nhiều tội lỗi,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.356Z",
      "updatedAt": "2024-09-24T02:31:35.356Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "9c17a2e7-6e70-4cef-827a-ca873e27c842",
      "number": 8,
      "content": "Nên nàng thành cớ thiên hạ cười chê;",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.358Z",
      "updatedAt": "2024-09-24T02:31:35.358Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "4bc99b8c-0ebf-4754-8eac-47d994f99515",
      "number": 8,
      "content": "Tất cả những ai từng kính nể nàng đều khinh bỉ nàng,",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.361Z",
      "updatedAt": "2024-09-24T02:31:35.361Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "02f3ff86-29a6-499f-a2cf-80bb0ce37423",
      "number": 8,
      "content": "Vì chúng đã thấy nàng bị trần trụi lõa lồ.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.363Z",
      "updatedAt": "2024-09-24T02:31:35.363Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "2177c01a-0da2-4862-b1b1-4231c1a62336",
      "number": 8,
      "content": "Phải, chính nàng cũng rên rỉ thở than,",
      "order": 4,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.365Z",
      "updatedAt": "2024-09-24T02:31:35.365Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "7cbf8211-0dd6-4847-a571-7b16f507fb4b",
      "number": 8,
      "content": "Và quay mặt đi để khỏi bị ai nhìn thấy.",
      "order": 5,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.367Z",
      "updatedAt": "2024-09-24T02:31:35.367Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "fd8a6537-4219-4ef4-9b12-0c1e38374a74",
      "number": 9,
      "content": "Vết ô uế vẫn còn dính trên váy nàng,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.369Z",
      "updatedAt": "2024-09-24T02:31:35.369Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "5749dc50-f5ac-4ef6-a099-ed16ee595cd9",
      "number": 9,
      "content": "Khi phạm tội nàng không nghĩ đến hậu quả về sau;",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.371Z",
      "updatedAt": "2024-09-24T02:31:35.371Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "6e4319f2-8d40-4615-a0a7-46c3bb8b7092",
      "number": 9,
      "content": "Vì thế nàng bị sụp đổ kinh hoàng;",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.375Z",
      "updatedAt": "2024-09-24T02:31:35.375Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "990b2b9a-6a10-4494-9bea-ef169a7343f5",
      "number": 9,
      "content": "Không ai an ủi nàng nửa lời.",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.377Z",
      "updatedAt": "2024-09-24T02:31:35.377Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "e7852bb7-6e3a-422c-b430-4728efb33d47",
      "number": 9,
      "content": "“Lạy Chúa, xin đoái xem nỗi đau khổ của con,",
      "order": 4,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.379Z",
      "updatedAt": "2024-09-24T02:31:35.379Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "2a540cc8-c6e1-4930-93b1-5c2c297d01c5",
      "number": 9,
      "content": "Vì quân thù của con thắng hơn con.”",
      "order": 5,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.381Z",
      "updatedAt": "2024-09-24T02:31:35.381Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "9015008f-34cc-45c2-a9fc-ec5e8dd0dab6",
      "number": 10,
      "content": "Quân thù đã đưa tay hốt sạch của cải quý báu của nàng;",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.383Z",
      "updatedAt": "2024-09-24T02:31:35.383Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "974ee303-39fd-465e-b924-903f5053bdd7",
      "number": 10,
      "content": "Nàng đã thấy các dân ngoại nghênh ngang vào đền thánh,",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.385Z",
      "updatedAt": "2024-09-24T02:31:35.385Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "3c38d157-be77-4fef-a143-632ae7c1ab17",
      "number": 10,
      "content": "Ðó là những kẻ Ngài cấm vào nơi nhóm họp của dân Ngài.",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.387Z",
      "updatedAt": "2024-09-24T02:31:35.387Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "4a253b40-0735-47d5-a801-b20aab8dc28d",
      "number": 11,
      "content": "Toàn dân nàng than thở khi tìm thực phẩm đỡ lòng;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.389Z",
      "updatedAt": "2024-09-24T02:31:35.389Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "467b47dd-1222-4453-94e4-f784b9e8e616",
      "number": 11,
      "content": "Họ đánh đổi của báu cho thức ăn đạm bạc,",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.391Z",
      "updatedAt": "2024-09-24T02:31:35.391Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "552963f0-ed5d-4bec-9911-80e647a18895",
      "number": 11,
      "content": "Ðể mong được phục sức.",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.393Z",
      "updatedAt": "2024-09-24T02:31:35.393Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "2902ffc7-88d3-41bd-b1b3-b3d8dc914777",
      "number": 11,
      "content": "“Lạy Chúa, xin đoái xem,",
      "order": 3,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.396Z",
      "updatedAt": "2024-09-24T02:31:35.396Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "72800a6e-a975-40d4-b7ed-e0a766b370e8",
      "number": 11,
      "content": "Con bị khinh bỉ biết bao!”",
      "order": 4,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.401Z",
      "updatedAt": "2024-09-24T02:31:35.401Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "3d1bca14-df56-489e-bb72-c1ab3957623d",
      "number": 12,
      "content": "Hỡi tất cả những ai qua lại,",
      "order": 0,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.406Z",
      "updatedAt": "2024-09-24T02:31:35.406Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "edd2230f-e9a3-4174-ac64-78182414d1a0",
      "number": 12,
      "content": "Quý vị thấy sự kiện này chẳng có ý nghĩa gì sao?",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.410Z",
      "updatedAt": "2024-09-24T02:31:35.410Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "132fbf33-d4da-41b7-8e0f-b0b9a190cb75",
      "number": 12,
      "content": "Hãy nhìn và xem,",
      "order": 2,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.415Z",
      "updatedAt": "2024-09-24T02:31:35.415Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "c4ba8592-4195-4484-947a-79303002110e",
      "number": 12,
      "content": "Có nỗi sầu khổ nào sánh bằng nỗi sầu khổ của tôi chăng?",
      "order": 3,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.420Z",
      "updatedAt": "2024-09-24T02:31:35.420Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "c28f91e3-92fc-4a48-993d-e12504727f4e",
      "number": 12,
      "content": "Ðó là điều đã xảy đến với tôi,",
      "order": 4,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.425Z",
      "updatedAt": "2024-09-24T02:31:35.425Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "adc98966-b72b-46a4-85f0-7cff1f3c7203",
      "number": 12,
      "content": "Mà Chúa đã giáng trên tôi trong ngày Ngài bừng bừng nổi giận.",
      "order": 5,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.430Z",
      "updatedAt": "2024-09-24T02:31:35.430Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "ef0c04ac-3ad2-4f51-97dc-a57172754c16",
      "number": 13,
      "content": "Từ trời cao Ngài phóng lửa hừng vụt xuống,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.435Z",
      "updatedAt": "2024-09-24T02:31:35.435Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "48c65817-2bd4-4c5c-a974-fbc757a0e812",
      "number": 13,
      "content": "Khiến lửa hừng thiêu đốt xương tôi;",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.439Z",
      "updatedAt": "2024-09-24T02:31:35.439Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "94ab7674-1e3a-4451-b0ca-c57503666fe8",
      "number": 13,
      "content": "Ngài giăng lưới để chân tôi vướng mắc,",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.444Z",
      "updatedAt": "2024-09-24T02:31:35.444Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "9961907d-57bb-4e8a-888d-d22eeb014823",
      "number": 13,
      "content": "Ngài bắt tôi phải quay trở lại;",
      "order": 3,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.450Z",
      "updatedAt": "2024-09-24T02:31:35.450Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "0ac095e1-254b-47d7-9c06-0d3ad27ea260",
      "number": 13,
      "content": "Ngài để tôi quạnh hiu và ngất xỉu suốt ngày.",
      "order": 4,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.455Z",
      "updatedAt": "2024-09-24T02:31:35.455Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "d29df521-c1a4-41e1-8b77-2b52d79e2269",
      "number": 14,
      "content": "Ngài buộc những vi phạm của tôi vào một cái ách,",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.460Z",
      "updatedAt": "2024-09-24T02:31:35.460Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "fad24cee-5fad-4311-a778-f23207a8f9f5",
      "number": 14,
      "content": "Ngài dùng tay Ngài cột chặt chúng lại với nhau,",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.465Z",
      "updatedAt": "2024-09-24T02:31:35.465Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "b65c5985-c10f-4c4f-8b10-5ba9a550c9b2",
      "number": 14,
      "content": "Và đặt chúng đè nặng trên cổ tôi,",
      "order": 2,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.469Z",
      "updatedAt": "2024-09-24T02:31:35.469Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "a5eb87d1-37c0-4c4b-8f38-4a245f2fd92e",
      "number": 14,
      "content": "Khiến sức lực tôi tiêu hao cạn kiệt;",
      "order": 3,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.472Z",
      "updatedAt": "2024-09-24T02:31:35.472Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "38afcc0a-d9f4-48a7-9d07-e375607dc027",
      "number": 14,
      "content": "Chúa đã trao tôi vào tay những kẻ tôi không có khả năng chống cự.",
      "order": 4,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.475Z",
      "updatedAt": "2024-09-24T02:31:35.475Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "0ad44e77-9405-4821-88ac-eabd58f25607",
      "number": 15,
      "content": "Chúa loại bỏ tất cả các dũng sĩ ở giữa tôi;",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.477Z",
      "updatedAt": "2024-09-24T02:31:35.477Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "64259868-6181-4254-a08f-97fb5045411e",
      "number": 15,
      "content": "Ngài triệu tập một đội quân đông đảo chống lại tôi,",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.480Z",
      "updatedAt": "2024-09-24T02:31:35.480Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "babe67b8-49ed-49fd-9383-27be54bcb895",
      "number": 15,
      "content": "Ðể tiêu diệt các chiến sĩ trẻ của tôi;",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.484Z",
      "updatedAt": "2024-09-24T02:31:35.484Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "e0925173-bc4a-4f69-91cd-8b429807df82",
      "number": 15,
      "content": "Chúa chà đạp tôi như người ta đạp nho trong bồn ép nho làm rượu;",
      "order": 3,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.487Z",
      "updatedAt": "2024-09-24T02:31:35.487Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "9172d62e-c920-44f4-95b1-60069be116ee",
      "number": 15,
      "content": "Ngài chà đạp người trinh nữ là Ái Nữ của Giu-đa.",
      "order": 4,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.489Z",
      "updatedAt": "2024-09-24T02:31:35.489Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "6c059cf1-6392-43d5-bf78-0d6d0bd8dab7",
      "number": 16,
      "content": "Vì những điều ấy nên tôi khóc,",
      "order": 0,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.491Z",
      "updatedAt": "2024-09-24T02:31:35.491Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "352588ed-12a3-4209-8530-18670d097048",
      "number": 16,
      "content": "Mắt tôi đổ lệ dầm dề;",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.494Z",
      "updatedAt": "2024-09-24T02:31:35.494Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "15eb1815-6a6c-4b48-a360-6b217a93f2de",
      "number": 16,
      "content": "Vì Ðấng an ủi, Ðấng làm cho tinh thần tôi phục hồi, đã lìa xa tôi;",
      "order": 2,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.496Z",
      "updatedAt": "2024-09-24T02:31:35.496Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "8df10843-6703-4afb-beef-b3340837f735",
      "number": 16,
      "content": "Các con cái tôi bơ vơ đói khổ,",
      "order": 3,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.498Z",
      "updatedAt": "2024-09-24T02:31:35.498Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "64a0f492-1214-4ab6-8876-555859f332b2",
      "number": 16,
      "content": "Vì quân thù đã thắng hơn tôi.",
      "order": 4,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.500Z",
      "updatedAt": "2024-09-24T02:31:35.500Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "8a2a67e8-021f-4340-8dbc-d63cbe12d7a9",
      "number": 17,
      "content": "Si-ôn đưa tay ra van xin giúp đỡ,",
      "order": 0,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.502Z",
      "updatedAt": "2024-09-24T02:31:35.502Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "729bef65-7d64-49c0-b2e5-37d54e51ff69",
      "number": 17,
      "content": "Nhưng không ai an ủi nàng;",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.504Z",
      "updatedAt": "2024-09-24T02:31:35.504Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "198f9005-17ab-4536-bfd5-9b8db8685214",
      "number": 17,
      "content": "Chúa đã ra lịnh cho các láng giềng của Gia-cốp rằng,",
      "order": 2,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.506Z",
      "updatedAt": "2024-09-24T02:31:35.506Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "fa8b1ca1-3a55-4b0e-a9a9-757aa2c75176",
      "number": 17,
      "content": "Chúng phải trở nên kẻ thù của ông;",
      "order": 3,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.509Z",
      "updatedAt": "2024-09-24T02:31:35.509Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "20e00cd2-dd2e-4d92-aabd-a5e91b2a2ad1",
      "number": 17,
      "content": "Giê-ru-sa-lem đã trở thành một vật ô uế giữa các láng giềng của nàng.",
      "order": 4,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.511Z",
      "updatedAt": "2024-09-24T02:31:35.511Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "30974b96-92f2-4683-9a78-9a1de9e19226",
      "number": 18,
      "content": "Chúa thật đúng, vì tôi đã nổi loạn chống lại mạng lịnh Ngài;",
      "order": 0,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.513Z",
      "updatedAt": "2024-09-24T02:31:35.513Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "16623f0a-3c09-4857-9c1d-1df833bb55e1",
      "number": 18,
      "content": "Hỡi muôn dân, hãy lắng nghe,",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.514Z",
      "updatedAt": "2024-09-24T02:31:35.514Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "3365b725-bf77-4a4d-913a-68baa25ec5db",
      "number": 18,
      "content": "Và hãy nhìn xem những sầu thảm tôi đang phải chịu;",
      "order": 2,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.516Z",
      "updatedAt": "2024-09-24T02:31:35.516Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "17da3dad-b5d4-4f93-837b-661a7dbc20c2",
      "number": 18,
      "content": "Các thiếu nữ và các thanh niên của tôi đã bị bắt đem lưu đày.",
      "order": 3,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.518Z",
      "updatedAt": "2024-09-24T02:31:35.518Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "8a98137b-0849-4f90-acec-596ebd0f5192",
      "number": 19,
      "content": "Tôi gọi những người yêu của tôi, nhưng họ đã gạt tôi;",
      "order": 0,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.520Z",
      "updatedAt": "2024-09-24T02:31:35.520Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "a2e9dc2b-1784-4a4e-8cef-e82e69ee2e8b",
      "number": 19,
      "content": "Các tư tế và các trưởng lão của tôi chết thảm trong thành,",
      "order": 1,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.523Z",
      "updatedAt": "2024-09-24T02:31:35.523Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "2749a40a-95dd-46df-bda6-36888f5701bc",
      "number": 19,
      "content": "Khi họ cố tìm chút gì để ăn đỡ đói, hầu phục hồi sinh lực của họ.",
      "order": 2,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.525Z",
      "updatedAt": "2024-09-24T02:31:35.525Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "ad844a03-7e94-4e8c-875d-e2417171620d",
      "number": 20,
      "content": "Lạy Chúa, xin Ngài xem, con tuyệt vọng biết bao!",
      "order": 0,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.527Z",
      "updatedAt": "2024-09-24T02:31:35.527Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "fd6aa3d9-621c-46d0-bf2a-cadd5972e5bc",
      "number": 20,
      "content": "Bụng con phập phồng lo sợ,",
      "order": 1,
      "parNumber": 95,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.529Z",
      "updatedAt": "2024-09-24T02:31:35.529Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "fe339d29-fbfc-455f-a8a0-262a448ae268",
      "number": 20,
      "content": "Tim con hồi hộp âu lo.",
      "order": 2,
      "parNumber": 96,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.531Z",
      "updatedAt": "2024-09-24T02:31:35.531Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "fd2ec98d-540b-4053-8a9b-9f1df17d8ce2",
      "number": 20,
      "content": "Vì con đã quyết tâm chống lại Ngài,",
      "order": 3,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.533Z",
      "updatedAt": "2024-09-24T02:31:35.533Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "960934b0-03b0-430c-af43-1258c8db6f37",
      "number": 20,
      "content": "Nên ngoài đường gươm đao đua nhau lấy mạng,",
      "order": 4,
      "parNumber": 98,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.536Z",
      "updatedAt": "2024-09-24T02:31:35.536Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "6e5417e2-05ca-4d87-b452-5129c3fbbabb",
      "number": 20,
      "content": "Còn trong nhà thần chết hoành hành.",
      "order": 5,
      "parNumber": 99,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.538Z",
      "updatedAt": "2024-09-24T02:31:35.538Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "2cefe13a-f027-49a5-a593-8104ee553e45",
      "number": 21,
      "content": "Người ta nghe tiếng con rên rỉ, nhưng chẳng ai an ủi con.",
      "order": 0,
      "parNumber": 100,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.540Z",
      "updatedAt": "2024-09-24T02:31:35.540Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "d7c010ab-9aea-4315-afab-073e32ccc54c",
      "number": 21,
      "content": "Tất cả kẻ thù của con nghe tin con gặp nạn,",
      "order": 1,
      "parNumber": 101,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.543Z",
      "updatedAt": "2024-09-24T02:31:35.543Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "b0a6c647-7646-46f7-b334-b541de66e7b3",
      "number": 21,
      "content": "Chúng vui mừng vì Ngài làm việc ấy cho con.",
      "order": 2,
      "parNumber": 102,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.545Z",
      "updatedAt": "2024-09-24T02:31:35.545Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "6f5ef19a-ebb5-4b3e-8b34-83079b2c88be",
      "number": 21,
      "content": "Xin Ngài sớm thực hiện ngày đoán phạt mà Ngài đã phán,",
      "order": 3,
      "parNumber": 103,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.548Z",
      "updatedAt": "2024-09-24T02:31:35.548Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "ae3d4c0e-2595-492b-b75a-67478b372a27",
      "number": 21,
      "content": "Ðể chúng biết mùi đau khổ như con.",
      "order": 4,
      "parNumber": 104,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.550Z",
      "updatedAt": "2024-09-24T02:31:35.550Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "021df75d-ca9a-4454-8de8-4ffc26482ded",
      "number": 22,
      "content": "Nguyện các việc ác của chúng bị phơi bày trước thánh nhan Ngài;",
      "order": 0,
      "parNumber": 105,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.552Z",
      "updatedAt": "2024-09-24T02:31:35.552Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "9d94a048-8fc3-4a60-90d1-ae1d4f31bbd3",
      "number": 22,
      "content": "Xin đối xử với chúng như Ngài đã đối xử với con,",
      "order": 1,
      "parNumber": 106,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.554Z",
      "updatedAt": "2024-09-24T02:31:35.554Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "61ba67eb-8a5c-4834-858d-107e80cfd1e1",
      "number": 22,
      "content": "Vì mọi vi phạm của con,",
      "order": 2,
      "parNumber": 107,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.557Z",
      "updatedAt": "2024-09-24T02:31:35.557Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "51406e5e-fcc5-43ae-ad2a-109962ad5afa",
      "number": 22,
      "content": "Vì con than thở quá nhiều,",
      "order": 3,
      "parNumber": 108,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.559Z",
      "updatedAt": "2024-09-24T02:31:35.559Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    },
    {
      "id": "0b83b01c-19a1-4ffd-b454-0aaa927f374d",
      "number": 22,
      "content": "Và lòng con giờ đã nao sờn.",
      "order": 4,
      "parNumber": 109,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:35.562Z",
      "updatedAt": "2024-09-24T02:31:35.562Z",
      "chapterId": "4133a260-d8dd-4e92-a7f3-1dfd2671f5a4"
    }
  ],
