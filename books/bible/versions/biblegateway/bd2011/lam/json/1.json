{
  "chapter": {
    "id": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Ai%20Ca%201&version=BD2011",
    "createdAt": "2025-04-22T07:37:53.586Z",
    "updatedAt": "2025-04-22T07:37:53.586Z",
    "bookId": "1a450e41-7151-4559-8401-d82ee41eafef"
  },
  "verses": [
    {
      "id": "44cb3373-969d-4239-b3cb-4ee16515d111",
      "number": 1,
      "content": "Kìa thành phố một thời đông người nhộn nhịp,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.345Z",
      "updatedAt": "2025-04-22T08:53:50.345Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "7e8e9c4f-95b1-4250-af88-610ec336f46c",
      "number": 1,
      "content": "Nay thưa người đơn chiếc đìu hiu!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.372Z",
      "updatedAt": "2025-04-22T08:53:50.372Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "5c53ca35-7a4b-4d25-8e15-8c1b8c3aec84",
      "number": 1,
      "content": "Kìa nàng vốn đại đô các nước,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.383Z",
      "updatedAt": "2025-04-22T08:53:50.383Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "66148114-a9c5-4995-932a-70bc8791b57c",
      "number": 1,
      "content": "Giờ trở thành góa phụ cô đơn!",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.394Z",
      "updatedAt": "2025-04-22T08:53:50.394Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "8ae96548-76fb-4833-8ec8-dafba2105a43",
      "number": 1,
      "content": "Kìa nàng vốn nữ hoàng các tiểu quốc,",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.407Z",
      "updatedAt": "2025-04-22T08:53:50.407Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "86525ada-9989-488d-9cd3-87d9edbda9fd",
      "number": 1,
      "content": "Nay biến thành nữ nô lệ phục tùng!",
      "order": 5,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.420Z",
      "updatedAt": "2025-04-22T08:53:50.420Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "04c480ed-237c-480e-aa7f-9af05e3daa57",
      "number": 2,
      "content": "Giữa đêm khuya nàng khóc than cay đắng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.433Z",
      "updatedAt": "2025-04-22T08:53:50.433Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "d34fe855-1179-48f6-b47b-ec42545a2303",
      "number": 2,
      "content": "Lệ tuôn tràn trên đôi má gầy gò;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.446Z",
      "updatedAt": "2025-04-22T08:53:50.446Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "a73ba5fd-25c3-4827-9b5c-718276f5cc0e",
      "number": 2,
      "content": "Trong số tất cả tình nhân của nàng, không ai an ủi nàng;",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.459Z",
      "updatedAt": "2025-04-22T08:53:50.459Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "a7305f15-67cf-4e91-a2f6-5d593c10a288",
      "number": 2,
      "content": "Tất cả các bạn nàng đã phụ rẫy nàng,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.473Z",
      "updatedAt": "2025-04-22T08:53:50.473Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "5903512e-4916-49fe-ab7c-9a643180a93a",
      "number": 2,
      "content": "Thậm chí chúng còn trở thành kẻ thù của nàng.",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.486Z",
      "updatedAt": "2025-04-22T08:53:50.486Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "1405971d-8fb2-4173-935e-ed0b72dbadde",
      "number": 3,
      "content": "Giu-đa đã bị lưu đày,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.502Z",
      "updatedAt": "2025-04-22T08:53:50.502Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "ac0da0ea-75ca-4152-a631-285606d1327d",
      "number": 3,
      "content": "Nàng phải chịu biết bao đau đớn và phải làm lao động khổ sai;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.515Z",
      "updatedAt": "2025-04-22T08:53:50.515Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "180e2aa8-2e59-4b9f-9ccb-b26e0b96c52a",
      "number": 3,
      "content": "Bây giờ nàng phải sống rải rác giữa các nước,",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.528Z",
      "updatedAt": "2025-04-22T08:53:50.528Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "e9093d0a-c56d-4fea-955a-e2721d99ade2",
      "number": 3,
      "content": "Nàng chẳng tìm được nơi nào để nghỉ ngơi;",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.541Z",
      "updatedAt": "2025-04-22T08:53:50.541Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "26da2c1b-e806-4663-b41f-af10bff9046d",
      "number": 3,
      "content": "Mọi kẻ truy đuổi nàng đều bắt kịp nàng trong lúc nàng cùng đường.",
      "order": 4,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.556Z",
      "updatedAt": "2025-04-22T08:53:50.556Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "2e67caab-ece5-45a8-a8cf-12b61911d6a0",
      "number": 4,
      "content": "Những con đường dẫn đến Si-ôn âu sầu ảm đạm,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.569Z",
      "updatedAt": "2025-04-22T08:53:50.569Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "b952da4e-c02d-4b25-a471-50a6811fb784",
      "number": 4,
      "content": "Vì không ai đến dự các đại lễ như xưa;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.582Z",
      "updatedAt": "2025-04-22T08:53:50.582Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "f2788ffb-239b-4a1b-9da9-7b3f23b13217",
      "number": 4,
      "content": "Tất cả các cổng thành đều đìu hiu quạnh quẽ,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.595Z",
      "updatedAt": "2025-04-22T08:53:50.595Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "c922652b-2a46-4afb-85e0-bee4d33390d6",
      "number": 4,
      "content": "Các tư tế đều rầu rĩ thở than,",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.608Z",
      "updatedAt": "2025-04-22T08:53:50.608Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "2f809e42-e1b3-4b79-b3bb-5661d7e63e4b",
      "number": 4,
      "content": "Các thiếu nữ của nàng buồn da diết,",
      "order": 4,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.622Z",
      "updatedAt": "2025-04-22T08:53:50.622Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "9bfbfae6-55fb-4f87-891f-9ecff11cf88a",
      "number": 4,
      "content": "Số phận nàng thật đau khổ đắng cay.",
      "order": 5,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.635Z",
      "updatedAt": "2025-04-22T08:53:50.635Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "57da667a-7a2f-4bd4-8922-eda6d64bb6e2",
      "number": 5,
      "content": "Những kẻ thù ghét nàng đã trở thành các chủ nhân nàng,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.648Z",
      "updatedAt": "2025-04-22T08:53:50.648Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "8a9c15aa-9e81-4835-8dab-53f8f34b0f8e",
      "number": 5,
      "content": "Những kẻ thù của nàng đã phát đạt.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.667Z",
      "updatedAt": "2025-04-22T08:53:50.667Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "c8dea578-c93c-46c8-878c-73c83fa792eb",
      "number": 5,
      "content": "Vì vô số tội lỗi của nàng,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.680Z",
      "updatedAt": "2025-04-22T08:53:50.680Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "ca5e9608-9d1c-436d-a6f0-96e95f47256c",
      "number": 5,
      "content": "Chúa bắt nàng phải chịu đau khổ;",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.693Z",
      "updatedAt": "2025-04-22T08:53:50.693Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "2c5303cb-1380-49b1-9ed4-9b5806366c6d",
      "number": 5,
      "content": "Con cái nàng phải sống tha phương,",
      "order": 4,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.706Z",
      "updatedAt": "2025-04-22T08:53:50.706Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "9f80c475-5507-407e-a1ee-565320869249",
      "number": 5,
      "content": "Vì bị quân thù đem lưu đày biệt xứ.",
      "order": 5,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.722Z",
      "updatedAt": "2025-04-22T08:53:50.722Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "309e0ae4-44f6-4ca5-8404-92ba75448644",
      "number": 6,
      "content": "Ái Nữ của Si-ôn đã hết thời,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.736Z",
      "updatedAt": "2025-04-22T08:53:50.736Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "8a4a602a-b257-4bfc-ad4f-f9c4f50b1769",
      "number": 6,
      "content": "Vẻ cao sang quý phái của nàng không còn nữa;",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.749Z",
      "updatedAt": "2025-04-22T08:53:50.749Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "bdcec187-0cce-4bc0-ae63-a03ca9249e52",
      "number": 6,
      "content": "Những kẻ cầm quyền của nàng như nai đói không tìm ra đồng cỏ;",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.762Z",
      "updatedAt": "2025-04-22T08:53:50.762Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "d85bb37e-2077-42e8-8942-18d5775736c9",
      "number": 6,
      "content": "Họ bạc nhược bỏ chạy trước mặt kẻ truy kích họ.",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.776Z",
      "updatedAt": "2025-04-22T08:53:50.776Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "8896fed9-baaa-4cbb-a96e-364e68c7c3e8",
      "number": 7,
      "content": "Giữa những ngày nàng đau khổ và lang thang,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.789Z",
      "updatedAt": "2025-04-22T08:53:50.789Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "035e4ee9-6938-4b03-979d-041d197c041f",
      "number": 7,
      "content": "Giê-ru-sa-lem nhớ lại mọi phước hạnh nàng đã hưởng ngày xưa.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.802Z",
      "updatedAt": "2025-04-22T08:53:50.802Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "9a3488da-086b-4947-9805-133ee41addc1",
      "number": 7,
      "content": "Khi dân nàng sa vào tay quân thù, chẳng ai đến tiếp cứu nàng;",
      "order": 2,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.815Z",
      "updatedAt": "2025-04-22T08:53:50.815Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "197f19cf-7190-4a31-b970-1cee1b89a10a",
      "number": 7,
      "content": "Thấy tình cảnh của nàng cô đơn như thế, kẻ thù của nàng cười hả hê trước sự sụp đổ của nàng.",
      "order": 3,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.829Z",
      "updatedAt": "2025-04-22T08:53:50.829Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "ccbf213f-226d-4b0d-8bea-2ea1e2bb9292",
      "number": 8,
      "content": "Giê-ru-sa-lem đã phạm quá nhiều tội lỗi,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.842Z",
      "updatedAt": "2025-04-22T08:53:50.842Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "977f8c89-62f0-45df-8318-feadcae6730c",
      "number": 8,
      "content": "Nên nàng thành cớ thiên hạ cười chê;",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.855Z",
      "updatedAt": "2025-04-22T08:53:50.855Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "b6a80693-3260-4613-b65e-4a557d204952",
      "number": 8,
      "content": "Tất cả những ai từng kính nể nàng đều khinh bỉ nàng,",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.867Z",
      "updatedAt": "2025-04-22T08:53:50.867Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "7680ca61-7135-44b4-b3f3-1bfa0f7e4914",
      "number": 8,
      "content": "Vì chúng đã thấy nàng bị trần trụi lõa lồ.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.881Z",
      "updatedAt": "2025-04-22T08:53:50.881Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "c50f90e8-8157-422c-9b1d-8cd29fbb30b5",
      "number": 8,
      "content": "Phải, chính nàng cũng rên rỉ thở than,",
      "order": 4,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.894Z",
      "updatedAt": "2025-04-22T08:53:50.894Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "fe0833fb-a64d-41be-8365-f228f4d75764",
      "number": 8,
      "content": "Và quay mặt đi để khỏi bị ai nhìn thấy.",
      "order": 5,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.907Z",
      "updatedAt": "2025-04-22T08:53:50.907Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "ccd628b2-3921-48cb-a2f3-e22a10162b22",
      "number": 9,
      "content": "Vết ô uế vẫn còn dính trên váy nàng,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.920Z",
      "updatedAt": "2025-04-22T08:53:50.920Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "d58c55a3-d28d-4395-8766-ed4968bfb383",
      "number": 9,
      "content": "Khi phạm tội nàng không nghĩ đến hậu quả về sau;",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.933Z",
      "updatedAt": "2025-04-22T08:53:50.933Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "187ca8a0-540e-484b-b511-e09fc36b4060",
      "number": 9,
      "content": "Vì thế nàng bị sụp đổ kinh hoàng;",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.947Z",
      "updatedAt": "2025-04-22T08:53:50.947Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "5d0250b4-c259-4c35-bb56-6c977a17277c",
      "number": 9,
      "content": "Không ai an ủi nàng nửa lời.",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.960Z",
      "updatedAt": "2025-04-22T08:53:50.960Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "4b12f818-a396-44c0-8a99-5dd148c8a443",
      "number": 9,
      "content": "“Lạy Chúa, xin đoái xem nỗi đau khổ của con,",
      "order": 4,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.973Z",
      "updatedAt": "2025-04-22T08:53:50.973Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "cd1dca9a-ae8c-40f8-a616-e35558cb060e",
      "number": 9,
      "content": "Vì quân thù của con thắng hơn con.”",
      "order": 5,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:50.986Z",
      "updatedAt": "2025-04-22T08:53:50.986Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "41c67508-3758-4ab0-91c9-dc10fc4be7a1",
      "number": 10,
      "content": "Quân thù đã đưa tay hốt sạch của cải quý báu của nàng;",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.002Z",
      "updatedAt": "2025-04-22T08:53:51.002Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "8d564283-d287-4586-bbcf-b2c9d5b68630",
      "number": 10,
      "content": "Nàng đã thấy các dân ngoại nghênh ngang vào đền thánh,",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.015Z",
      "updatedAt": "2025-04-22T08:53:51.015Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "764f7520-6ad4-4780-8a1c-6a01d78ef7a1",
      "number": 10,
      "content": "Ðó là những kẻ Ngài cấm vào nơi nhóm họp của dân Ngài.",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.028Z",
      "updatedAt": "2025-04-22T08:53:51.028Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "4422c8ab-6874-47be-b729-354eff42c16b",
      "number": 11,
      "content": "Toàn dân nàng than thở khi tìm thực phẩm đỡ lòng;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.042Z",
      "updatedAt": "2025-04-22T08:53:51.042Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "a5923c20-0766-44b7-a647-6dd4c9347c79",
      "number": 11,
      "content": "Họ đánh đổi của báu cho thức ăn đạm bạc,",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.056Z",
      "updatedAt": "2025-04-22T08:53:51.056Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "7e76a589-252e-46ad-9fd8-7ac550d80571",
      "number": 11,
      "content": "Ðể mong được phục sức.",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.072Z",
      "updatedAt": "2025-04-22T08:53:51.072Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "375439ac-df03-4b18-994e-75ff33fc232b",
      "number": 11,
      "content": "“Lạy Chúa, xin đoái xem,",
      "order": 3,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.085Z",
      "updatedAt": "2025-04-22T08:53:51.085Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "398bc93a-a296-4228-9a7f-5ed67548fc71",
      "number": 11,
      "content": "Con bị khinh bỉ biết bao!”",
      "order": 4,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.099Z",
      "updatedAt": "2025-04-22T08:53:51.099Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "04cb97cc-eb11-4032-8e41-49a562946261",
      "number": 12,
      "content": "Hỡi tất cả những ai qua lại,",
      "order": 0,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.112Z",
      "updatedAt": "2025-04-22T08:53:51.112Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "81c307ec-465c-480a-8f0f-0bf9e0853cdf",
      "number": 12,
      "content": "Quý vị thấy sự kiện này chẳng có ý nghĩa gì sao?",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.125Z",
      "updatedAt": "2025-04-22T08:53:51.125Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "9b015e99-8768-4871-b185-13a0f26e4411",
      "number": 12,
      "content": "Hãy nhìn và xem,",
      "order": 2,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.138Z",
      "updatedAt": "2025-04-22T08:53:51.138Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "fcb1d7f0-2f12-464d-ad04-aed82285f92b",
      "number": 12,
      "content": "Có nỗi sầu khổ nào sánh bằng nỗi sầu khổ của tôi chăng?",
      "order": 3,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.155Z",
      "updatedAt": "2025-04-22T08:53:51.155Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "60f9b16e-3f5c-4cb6-b671-21be8660e3f6",
      "number": 12,
      "content": "Ðó là điều đã xảy đến với tôi,",
      "order": 4,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.168Z",
      "updatedAt": "2025-04-22T08:53:51.168Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "c6146c0a-caae-4e90-92d3-df40ce7fb6ed",
      "number": 12,
      "content": "Mà Chúa đã giáng trên tôi trong ngày Ngài bừng bừng nổi giận.",
      "order": 5,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.181Z",
      "updatedAt": "2025-04-22T08:53:51.181Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "054ecf6c-5017-4c6c-9788-83bc559c79c7",
      "number": 13,
      "content": "Từ trời cao Ngài phóng lửa hừng vụt xuống,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.194Z",
      "updatedAt": "2025-04-22T08:53:51.194Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "2a48c769-e178-4bc5-8289-4e44891847d0",
      "number": 13,
      "content": "Khiến lửa hừng thiêu đốt xương tôi;",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.207Z",
      "updatedAt": "2025-04-22T08:53:51.207Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "b24842c2-175c-49e9-b8f2-658f4f21f7eb",
      "number": 13,
      "content": "Ngài giăng lưới để chân tôi vướng mắc,",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.220Z",
      "updatedAt": "2025-04-22T08:53:51.220Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "9e1cebab-f837-42a7-8b87-fdf912c27cbf",
      "number": 13,
      "content": "Ngài bắt tôi phải quay trở lại;",
      "order": 3,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.233Z",
      "updatedAt": "2025-04-22T08:53:51.233Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "3f83455d-3a12-4d1c-93dd-d010a73d9e40",
      "number": 13,
      "content": "Ngài để tôi quạnh hiu và ngất xỉu suốt ngày.",
      "order": 4,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.249Z",
      "updatedAt": "2025-04-22T08:53:51.249Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "8e6aac40-0db3-4ea1-bf04-f376acdcc178",
      "number": 14,
      "content": "Ngài buộc những vi phạm của tôi vào một cái ách,",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.265Z",
      "updatedAt": "2025-04-22T08:53:51.265Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "ca81e6a0-f072-41e9-ad21-33311140a16e",
      "number": 14,
      "content": "Ngài dùng tay Ngài cột chặt chúng lại với nhau,",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.278Z",
      "updatedAt": "2025-04-22T08:53:51.278Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "2ae382f6-9697-4783-88a8-2d5eef2e5c76",
      "number": 14,
      "content": "Và đặt chúng đè nặng trên cổ tôi,",
      "order": 2,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.292Z",
      "updatedAt": "2025-04-22T08:53:51.292Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "fa63ad18-51f0-4029-8e9b-898160842f65",
      "number": 14,
      "content": "Khiến sức lực tôi tiêu hao cạn kiệt;",
      "order": 3,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.305Z",
      "updatedAt": "2025-04-22T08:53:51.305Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "69868ce2-cef5-41a4-96ce-7b5aa51cae6d",
      "number": 14,
      "content": "Chúa đã trao tôi vào tay những kẻ tôi không có khả năng chống cự.",
      "order": 4,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.318Z",
      "updatedAt": "2025-04-22T08:53:51.318Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "5ac2d1e2-ee4f-4e5e-99d0-72bbb334717d",
      "number": 15,
      "content": "Chúa loại bỏ tất cả các dũng sĩ ở giữa tôi;",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.331Z",
      "updatedAt": "2025-04-22T08:53:51.331Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "a578381e-f77d-4c1f-908e-6af8383886a4",
      "number": 15,
      "content": "Ngài triệu tập một đội quân đông đảo chống lại tôi,",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.344Z",
      "updatedAt": "2025-04-22T08:53:51.344Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "3d7c1bda-5e5b-43dd-8dc8-421d9a23a2b8",
      "number": 15,
      "content": "Ðể tiêu diệt các chiến sĩ trẻ của tôi;",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.357Z",
      "updatedAt": "2025-04-22T08:53:51.357Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "6af34838-499e-48b3-86fc-988d3773579f",
      "number": 15,
      "content": "Chúa chà đạp tôi như người ta đạp nho trong bồn ép nho làm rượu;",
      "order": 3,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.370Z",
      "updatedAt": "2025-04-22T08:53:51.370Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "51446c70-883d-4325-8e8e-1a019df884aa",
      "number": 15,
      "content": "Ngài chà đạp người trinh nữ là Ái Nữ của Giu-đa.",
      "order": 4,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.383Z",
      "updatedAt": "2025-04-22T08:53:51.383Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "65c28aee-51ba-4abf-8e3c-72d5b1e500c6",
      "number": 16,
      "content": "Vì những điều ấy nên tôi khóc,",
      "order": 0,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.396Z",
      "updatedAt": "2025-04-22T08:53:51.396Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "a2a4f9fe-eaf1-4357-910c-618bdc0f8867",
      "number": 16,
      "content": "Mắt tôi đổ lệ dầm dề;",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.409Z",
      "updatedAt": "2025-04-22T08:53:51.409Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "428422af-3815-4bd5-ac21-7e6cc4fdac41",
      "number": 16,
      "content": "Vì Ðấng an ủi, Ðấng làm cho tinh thần tôi phục hồi, đã lìa xa tôi;",
      "order": 2,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.422Z",
      "updatedAt": "2025-04-22T08:53:51.422Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "667ce561-4956-4f90-87ea-6d174eb30616",
      "number": 16,
      "content": "Các con cái tôi bơ vơ đói khổ,",
      "order": 3,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.435Z",
      "updatedAt": "2025-04-22T08:53:51.435Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "1e03174e-035b-4e70-bd3d-1dc331a56018",
      "number": 16,
      "content": "Vì quân thù đã thắng hơn tôi.",
      "order": 4,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.448Z",
      "updatedAt": "2025-04-22T08:53:51.448Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "55f4dd39-f11f-417f-93c6-f0da91b1c7ca",
      "number": 17,
      "content": "Si-ôn đưa tay ra van xin giúp đỡ,",
      "order": 0,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.461Z",
      "updatedAt": "2025-04-22T08:53:51.461Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "0c6c360e-6318-429d-b862-cb376220ad9e",
      "number": 17,
      "content": "Nhưng không ai an ủi nàng;",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.474Z",
      "updatedAt": "2025-04-22T08:53:51.474Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "3181a9fb-5c27-4f6a-9671-b23675445dce",
      "number": 17,
      "content": "Chúa đã ra lịnh cho các láng giềng của Gia-cốp rằng,",
      "order": 2,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.488Z",
      "updatedAt": "2025-04-22T08:53:51.488Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "01ea8906-a614-41d8-b9ce-6414e270e596",
      "number": 17,
      "content": "Chúng phải trở nên kẻ thù của ông;",
      "order": 3,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.504Z",
      "updatedAt": "2025-04-22T08:53:51.504Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "e35c864d-12c0-44a1-bbf5-27761d35f182",
      "number": 17,
      "content": "Giê-ru-sa-lem đã trở thành một vật ô uế giữa các láng giềng của nàng.",
      "order": 4,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.518Z",
      "updatedAt": "2025-04-22T08:53:51.518Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "0c915e0f-4edc-4937-9647-2c033a4c00bf",
      "number": 18,
      "content": "Chúa thật đúng, vì tôi đã nổi loạn chống lại mạng lịnh Ngài;",
      "order": 0,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.531Z",
      "updatedAt": "2025-04-22T08:53:51.531Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "7ee87bac-48dc-4ae1-a8ef-713c78f19d53",
      "number": 18,
      "content": "Hỡi muôn dân, hãy lắng nghe,",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.544Z",
      "updatedAt": "2025-04-22T08:53:51.544Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "93e5258f-d312-4ef0-8526-2d279e6a85fb",
      "number": 18,
      "content": "Và hãy nhìn xem những sầu thảm tôi đang phải chịu;",
      "order": 2,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.557Z",
      "updatedAt": "2025-04-22T08:53:51.557Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "808970a8-f224-4cc2-a0c7-750372b8cb33",
      "number": 18,
      "content": "Các thiếu nữ và các thanh niên của tôi đã bị bắt đem lưu đày.",
      "order": 3,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.574Z",
      "updatedAt": "2025-04-22T08:53:51.574Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "ae6574ef-2bfa-4929-9cde-854c12dfc845",
      "number": 19,
      "content": "Tôi gọi những người yêu của tôi, nhưng họ đã gạt tôi;",
      "order": 0,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.587Z",
      "updatedAt": "2025-04-22T08:53:51.587Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "dfdfc0ff-02bf-4f56-9e97-09a612db4b6c",
      "number": 19,
      "content": "Các tư tế và các trưởng lão của tôi chết thảm trong thành,",
      "order": 1,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.601Z",
      "updatedAt": "2025-04-22T08:53:51.601Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "b2f03000-e33f-4c36-81b4-c665ecd82abd",
      "number": 19,
      "content": "Khi họ cố tìm chút gì để ăn đỡ đói, hầu phục hồi sinh lực của họ.",
      "order": 2,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.614Z",
      "updatedAt": "2025-04-22T08:53:51.614Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "1b2850d9-60d9-459c-805e-26290cf050a1",
      "number": 20,
      "content": "Lạy Chúa, xin Ngài xem, con tuyệt vọng biết bao!",
      "order": 0,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.627Z",
      "updatedAt": "2025-04-22T08:53:51.627Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "adfff02d-cd2c-43f3-bb39-11cae304d229",
      "number": 20,
      "content": "Bụng con phập phồng lo sợ,",
      "order": 1,
      "parNumber": 95,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.640Z",
      "updatedAt": "2025-04-22T08:53:51.640Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "7aba2b36-dda6-4d3b-91e8-137ae776bbd7",
      "number": 20,
      "content": "Tim con hồi hộp âu lo.",
      "order": 2,
      "parNumber": 96,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.653Z",
      "updatedAt": "2025-04-22T08:53:51.653Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "38c5ab6f-8572-4cf2-9558-50e2259a0d6d",
      "number": 20,
      "content": "Vì con đã quyết tâm chống lại Ngài,",
      "order": 3,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.667Z",
      "updatedAt": "2025-04-22T08:53:51.667Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "93f6e91e-626d-4e3d-ba55-d7adb033a5bb",
      "number": 20,
      "content": "Nên ngoài đường gươm đao đua nhau lấy mạng,",
      "order": 4,
      "parNumber": 98,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.680Z",
      "updatedAt": "2025-04-22T08:53:51.680Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "ae842bb5-c648-473b-b37d-cb465009d256",
      "number": 20,
      "content": "Còn trong nhà thần chết hoành hành.",
      "order": 5,
      "parNumber": 99,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.693Z",
      "updatedAt": "2025-04-22T08:53:51.693Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "0dcbcd99-c050-46c9-8ca2-cce7e416fdc8",
      "number": 21,
      "content": "Người ta nghe tiếng con rên rỉ, nhưng chẳng ai an ủi con.",
      "order": 0,
      "parNumber": 100,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.706Z",
      "updatedAt": "2025-04-22T08:53:51.706Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "4614c665-5741-437a-a972-9feddb286042",
      "number": 21,
      "content": "Tất cả kẻ thù của con nghe tin con gặp nạn,",
      "order": 1,
      "parNumber": 101,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.719Z",
      "updatedAt": "2025-04-22T08:53:51.719Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "53614345-e71d-4d50-b3b8-b157c4c59486",
      "number": 21,
      "content": "Chúng vui mừng vì Ngài làm việc ấy cho con.",
      "order": 2,
      "parNumber": 102,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.732Z",
      "updatedAt": "2025-04-22T08:53:51.732Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "6f859871-b72e-4cd5-8d2e-cb0065e753b8",
      "number": 21,
      "content": "Xin Ngài sớm thực hiện ngày đoán phạt mà Ngài đã phán,",
      "order": 3,
      "parNumber": 103,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.745Z",
      "updatedAt": "2025-04-22T08:53:51.745Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "deefc1e8-e157-4ccc-a987-bdaed62ba6eb",
      "number": 21,
      "content": "Ðể chúng biết mùi đau khổ như con.",
      "order": 4,
      "parNumber": 104,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.758Z",
      "updatedAt": "2025-04-22T08:53:51.758Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "25a29af5-5d83-4e13-b1cd-39f243618844",
      "number": 22,
      "content": "Nguyện các việc ác của chúng bị phơi bày trước thánh nhan Ngài;",
      "order": 0,
      "parNumber": 105,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.771Z",
      "updatedAt": "2025-04-22T08:53:51.771Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "dd255594-0661-4efb-a979-b315290801af",
      "number": 22,
      "content": "Xin đối xử với chúng như Ngài đã đối xử với con,",
      "order": 1,
      "parNumber": 106,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.784Z",
      "updatedAt": "2025-04-22T08:53:51.784Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "5c40884b-981f-4d8f-b1fa-bda56eb2f4e6",
      "number": 22,
      "content": "Vì mọi vi phạm của con,",
      "order": 2,
      "parNumber": 107,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.797Z",
      "updatedAt": "2025-04-22T08:53:51.797Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "3fee2c8a-0eb7-40a3-ab4e-af9f1817594e",
      "number": 22,
      "content": "Vì con than thở quá nhiều,",
      "order": 3,
      "parNumber": 108,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.810Z",
      "updatedAt": "2025-04-22T08:53:51.810Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    },
    {
      "id": "e8cff374-3ad9-4d18-906a-63f8397d80be",
      "number": 22,
      "content": "Và lòng con giờ đã nao sờn.",
      "order": 4,
      "parNumber": 109,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:51.824Z",
      "updatedAt": "2025-04-22T08:53:51.824Z",
      "chapterId": "18f8b6c9-88ed-413c-b59b-2c8661b6c14f"
    }
  ],
