{
  "chapter": {
    "id": "83361345-6b7a-4e84-9e79-851a001cd929",
    "number": 2,
    "ref": "https://www.biblegateway.com/passage/?search=Ai%20Ca%202&version=BD2011",
    "createdAt": "2025-04-24T03:03:23.221Z",
    "updatedAt": "2025-04-24T03:03:23.221Z",
    "bookId": "d2db2063-7c78-49fd-a278-7ac331ad9d55"
  },
  "verses": [
    {
      "id": "3ae5a599-c574-47d7-8814-24a858a83a70",
      "number": 1,
      "content": "Sao Chúa nỡ bao phủ Ái Nữ của Si-ôn bằng cơn giận?",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:56.936Z",
      "updatedAt": "2025-04-24T04:22:56.936Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "7df1dcb4-b218-43ba-bc2a-24dfc676601c",
      "number": 1,
      "content": "Từ trời cao Ngài ném xuống đất vẻ đẹp của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:56.963Z",
      "updatedAt": "2025-04-24T04:22:56.963Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "6f5ae9cd-caad-4308-8300-46bddf7eee87",
      "number": 1,
      "content": "Và không nhớ đến bệ chân Ngài trong ngày Ngài nổi giận.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:56.974Z",
      "updatedAt": "2025-04-24T04:22:56.974Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "a1e878d7-bf78-48a9-a56f-ea32f01bda9a",
      "number": 2,
      "content": "Chúa tiêu diệt không chút xót thương mọi nơi cư ngụ của Gia-cốp;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:56.984Z",
      "updatedAt": "2025-04-24T04:22:56.984Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "d09c1910-d75b-46ec-a9a6-28a3335695f8",
      "number": 2,
      "content": "Trong cơn giận Ngài phá tan các thành lũy Ái Nữ của Giu-đa;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:56.997Z",
      "updatedAt": "2025-04-24T04:22:56.997Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "138f63fe-3b03-4bfe-86be-ed0eddfb2157",
      "number": 2,
      "content": "Ngài khiến chúng đổ xuống đất;",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.007Z",
      "updatedAt": "2025-04-24T04:22:57.007Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "46b4af5b-257f-4d26-800f-5dbecb851ddc",
      "number": 2,
      "content": "Ðể làm nhục vương quốc và những người lãnh đạo của nó.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.018Z",
      "updatedAt": "2025-04-24T04:22:57.018Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "c3a74b4e-c7d7-40d5-990b-d576966914e7",
      "number": 3,
      "content": "Trong cơn cực kỳ phẫn nộ Ngài chặt đứt tất cả các sừng của I-sơ-ra-ên;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.030Z",
      "updatedAt": "2025-04-24T04:22:57.030Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "cf210203-87bb-4586-9d0f-19114a04def0",
      "number": 3,
      "content": "Ngài rút lại cánh tay phải Ngài trước mặt quân thù của họ;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.042Z",
      "updatedAt": "2025-04-24T04:22:57.042Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "37dc996e-c5f3-4efc-8c53-e1182d0e69e2",
      "number": 3,
      "content": "Ngài đốt rụi trong xứ của Gia-cốp,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.055Z",
      "updatedAt": "2025-04-24T04:22:57.055Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "8e2d5b22-3883-4081-b955-83ea2b29d4af",
      "number": 3,
      "content": "Như ngọn lửa hừng thiêu rụi mọi thứ xung quanh.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.069Z",
      "updatedAt": "2025-04-24T04:22:57.069Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "4c2147f8-82b7-4db5-bfb7-d49c38416693",
      "number": 4,
      "content": "Ngài đã giương cung nhắm bắn như một xạ thủ của quân thù,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.084Z",
      "updatedAt": "2025-04-24T04:22:57.084Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "af934e8e-dcb4-4372-99e0-228d97b71211",
      "number": 4,
      "content": "Tay phải Ngài sẵn sàng buông tên;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.097Z",
      "updatedAt": "2025-04-24T04:22:57.097Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "25ba18be-fc1d-4ad0-91ea-cef939d339d1",
      "number": 4,
      "content": "Như kẻ thù, Ngài giết sạch kẻ nào mắt Ngài muốn giết,",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.108Z",
      "updatedAt": "2025-04-24T04:22:57.108Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "47fc2054-3a4e-4f8e-b37d-ae7df63247fc",
      "number": 5,
      "content": "Chúa đã trở thành như kẻ thù;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.118Z",
      "updatedAt": "2025-04-24T04:22:57.118Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "2002250f-3016-4699-b9ea-c62277c4adbf",
      "number": 5,
      "content": "Ngài nuốt chửng I-sơ-ra-ên;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.128Z",
      "updatedAt": "2025-04-24T04:22:57.128Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "0a9f9a82-349c-4772-bfb5-8f126d85c21f",
      "number": 5,
      "content": "Ngài nuốt trộng tất cả cung điện nó,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.139Z",
      "updatedAt": "2025-04-24T04:22:57.139Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "12e9a483-3f73-4f6e-802c-e571ae300b7c",
      "number": 5,
      "content": "Ngài biến các thành trì nó ra hoang phế,",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.149Z",
      "updatedAt": "2025-04-24T04:22:57.149Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "e3ed2a2d-1f70-49f1-967b-836b7b23dfd1",
      "number": 5,
      "content": "Ngài làm gia tăng nỗi sầu thảm và bi ai cho Ái Nữ của Giu-đa.",
      "order": 4,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.159Z",
      "updatedAt": "2025-04-24T04:22:57.159Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "c3c05e31-a19d-4e64-8859-4b22c6b615d3",
      "number": 6,
      "content": "Ngài phá tan Lều Tạm của Ngài như người ta phá bỏ một mảnh vườn;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.170Z",
      "updatedAt": "2025-04-24T04:22:57.170Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "8e580e30-e711-4a01-80c9-b5df45beca06",
      "number": 6,
      "content": "Ngài phá hủy nơi nhóm lại của Ngài.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.180Z",
      "updatedAt": "2025-04-24T04:22:57.180Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "573b37c5-0df9-4546-b994-67ca6af7af7f",
      "number": 6,
      "content": "Chúa làm cho những ngày đại lễ và những ngày Sa-bát ở Si-ôn không được nhớ đến nữa;",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.190Z",
      "updatedAt": "2025-04-24T04:22:57.190Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "160be0d5-f039-412f-83c2-28e76e89e35a",
      "number": 6,
      "content": "Trong cơn cực kỳ phẫn nộ Ngài ghê tởm cả vua lẫn tư tế ở đó.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.201Z",
      "updatedAt": "2025-04-24T04:22:57.201Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "3cec217b-8416-4a53-8bc8-acf94fec708b",
      "number": 7,
      "content": "Chúa loại bỏ bàn thờ của Ngài;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.211Z",
      "updatedAt": "2025-04-24T04:22:57.211Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "041858b6-c883-4423-9658-f65c8bbd7c9e",
      "number": 7,
      "content": "Ngài ghê tởm nơi thánh của Ngài;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.221Z",
      "updatedAt": "2025-04-24T04:22:57.221Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "055a4791-62aa-4bd1-86d1-8a3eb307882f",
      "number": 7,
      "content": "Ngài trao vào tay quân thù các bức tường cung điện của nàng;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.234Z",
      "updatedAt": "2025-04-24T04:22:57.234Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "8205cef0-2476-4c01-b9c4-a4609aabccdb",
      "number": 7,
      "content": "Chúng làm ồn trong nhà Chúa như tiếng ồn trong ngày đại lễ.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.245Z",
      "updatedAt": "2025-04-24T04:22:57.245Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "46921422-7c51-4ab5-b7a8-6e633d39e738",
      "number": 8,
      "content": "Chúa quyết định phá đổ tường thành Ái Nữ của Si-ôn;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.257Z",
      "updatedAt": "2025-04-24T04:22:57.257Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "a0a906d9-a76b-4391-8614-8b9405b97222",
      "number": 8,
      "content": "Ngài giăng dây ra đo và không rút tay Ngài lại khi hủy diệt;",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.268Z",
      "updatedAt": "2025-04-24T04:22:57.268Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "04ca3ef1-256c-44c9-b95b-4ad9f89fb1a2",
      "number": 8,
      "content": "Ngài khiến các chiến lũy và tường thành đều buồn thảm;",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.279Z",
      "updatedAt": "2025-04-24T04:22:57.279Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "da4a3537-5648-4a19-8020-ff413355e265",
      "number": 8,
      "content": "Chúng cùng nhau suy tàn.",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.289Z",
      "updatedAt": "2025-04-24T04:22:57.289Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "1ca4d15d-b8ae-4c9b-9caa-c3bd50e467eb",
      "number": 9,
      "content": "Các cổng thành của nàng lún sâu dưới đất;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.299Z",
      "updatedAt": "2025-04-24T04:22:57.299Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "7d1c27b2-d8ad-4cfb-9d0a-ed7a2e6d6b8c",
      "number": 9,
      "content": "Ngài phá tan và bẻ gãy các thanh ngang;",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.313Z",
      "updatedAt": "2025-04-24T04:22:57.313Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "db0290ba-c7d8-4d1e-a296-d0ceaca2d417",
      "number": 9,
      "content": "Vua và các quan nàng bị lưu đày nơi xứ lạ,",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.324Z",
      "updatedAt": "2025-04-24T04:22:57.324Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "067d43f2-6f68-444b-b4b7-3fcf93f693b7",
      "number": 9,
      "content": "Luật Pháp không còn nữa,",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.338Z",
      "updatedAt": "2025-04-24T04:22:57.338Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "8fe06179-9601-4263-9828-32a5e02b02aa",
      "number": 9,
      "content": "Các tiên tri nàng chẳng thấy khải tượng nào từ Chúa cả.",
      "order": 4,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.348Z",
      "updatedAt": "2025-04-24T04:22:57.348Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "cebc4e93-620b-41b3-907f-e63dd814cd92",
      "number": 10,
      "content": "Các trưởng lão Ái Nữ của Si-ôn ngồi im lìm dưới đất,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.358Z",
      "updatedAt": "2025-04-24T04:22:57.358Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "e9cc7170-a9f0-44b8-9585-9cdf0099434f",
      "number": 10,
      "content": "Họ hốt bụi rải trên đầu,",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.368Z",
      "updatedAt": "2025-04-24T04:22:57.368Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "a0eda525-f9b3-4498-aaf0-a04329146ecf",
      "number": 10,
      "content": "Họ mặc vải thô;",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.380Z",
      "updatedAt": "2025-04-24T04:22:57.380Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "0653cce1-7e35-4eab-bf5e-b4060aabc205",
      "number": 10,
      "content": "Các trinh nữ của Giê-ru-sa-lem cúi gục đầu xuống đất.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.391Z",
      "updatedAt": "2025-04-24T04:22:57.391Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "f59c75f8-a6bb-4da5-81f0-d314766bfb7b",
      "number": 11,
      "content": "Mắt tôi hốc hác vì khóc khô dòng lệ;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.401Z",
      "updatedAt": "2025-04-24T04:22:57.401Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "6c9b5ff5-256b-4a17-9326-a2ee8e345b2c",
      "number": 11,
      "content": "Lòng dạ tôi tan nát từng cơn,",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.411Z",
      "updatedAt": "2025-04-24T04:22:57.411Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "905c9700-483a-4bf5-8086-66dff73aa700",
      "number": 11,
      "content": "Mật tôi đổ ra trên đất vì sự hủy diệt Ái Nữ của dân tôi,",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.421Z",
      "updatedAt": "2025-04-24T04:22:57.421Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "1a2856d4-ea6c-4474-8a63-6955267ff9ae",
      "number": 11,
      "content": "Bởi vì các trẻ thơ và các ấu nhi ngất xỉu trên các đường phố.",
      "order": 3,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.431Z",
      "updatedAt": "2025-04-24T04:22:57.431Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "fdd76445-be49-414b-88cf-0ef4646859a8",
      "number": 12,
      "content": "Chúng khóc xin với mẹ chúng,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.441Z",
      "updatedAt": "2025-04-24T04:22:57.441Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "d7920d47-47a8-49ab-adb4-58ebb4601b5f",
      "number": 12,
      "content": "“Cơm bánh và thức uống đâu?”",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.458Z",
      "updatedAt": "2025-04-24T04:22:57.458Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "a12bb67a-91ca-4bec-b80a-6f393f11fe32",
      "number": 12,
      "content": "Chúng ngất xỉu như người bị tử thương trên các đường phố,",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.470Z",
      "updatedAt": "2025-04-24T04:22:57.470Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "fb399a29-49f9-42c6-a203-ad976a16a477",
      "number": 12,
      "content": "Rồi chúng từ từ tắt thở trong vòng tay bên lòng mẹ.",
      "order": 3,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.480Z",
      "updatedAt": "2025-04-24T04:22:57.480Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "fb1992cf-d902-4c12-89bd-d21c4f5f3d5d",
      "number": 13,
      "content": "Hỡi Ái Nữ của Giê-ru-sa-lem, ta có thể nói gì cho ngươi nữa?",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.491Z",
      "updatedAt": "2025-04-24T04:22:57.491Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "690d5dd3-7695-4ac9-8b8d-77549502c96a",
      "number": 13,
      "content": "Ta có thể lấy gì so sánh với ngươi?",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.502Z",
      "updatedAt": "2025-04-24T04:22:57.502Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "93b0b0eb-4af7-4f15-80a4-2bacb0576756",
      "number": 13,
      "content": "Hỡi nàng trinh nữ, Ái Nữ của Si-ôn, ta có thể so sánh ngươi với ai,",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.512Z",
      "updatedAt": "2025-04-24T04:22:57.512Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "8cf5661e-a1f9-44f4-9059-43b9d1bba5ab",
      "number": 13,
      "content": "Ðể ta có thể an ủi ngươi?",
      "order": 3,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.522Z",
      "updatedAt": "2025-04-24T04:22:57.522Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "1c45d944-f041-4623-9e07-1442465cc992",
      "number": 13,
      "content": "Vì nỗi đau thương của ngươi lớn như biển cả,",
      "order": 4,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.532Z",
      "updatedAt": "2025-04-24T04:22:57.532Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "9d9813bb-66e3-4ea0-b4e5-19233446e5ab",
      "number": 13,
      "content": "Ai có thể chữa cho ngươi được lành?",
      "order": 5,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.542Z",
      "updatedAt": "2025-04-24T04:22:57.542Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "2036f8b6-5c5d-48a1-826b-4abf8eead97f",
      "number": 14,
      "content": "Các tiên tri của ngươi đã thấy toàn những điều giả dối và tầm phào;",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.553Z",
      "updatedAt": "2025-04-24T04:22:57.553Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "6dc6583a-e389-4380-b1a6-047df8af8cff",
      "number": 14,
      "content": "Chúng không vạch rõ các tội của ngươi ra,",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.563Z",
      "updatedAt": "2025-04-24T04:22:57.563Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "64f286bf-2e8d-42df-9904-df9afc5e732c",
      "number": 14,
      "content": "Hầu mong đảo ngược vận mạng của ngươi,",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.575Z",
      "updatedAt": "2025-04-24T04:22:57.575Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "835096d0-bc3e-40db-8993-68fbcaaa72cf",
      "number": 14,
      "content": "Nhưng chúng chỉ thấy và bảo ngươi toàn những sứ điệp giả dối và ảo giác hư không.",
      "order": 3,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.598Z",
      "updatedAt": "2025-04-24T04:22:57.598Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "2eb305a4-9f7e-49df-afd6-480eaa605db9",
      "number": 15,
      "content": "Tất cả những kẻ đi ngang qua thấy ngươi đều đập tay tiếc rẻ,",
      "order": 0,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.609Z",
      "updatedAt": "2025-04-24T04:22:57.609Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "b86c9eee-5150-4c57-83b3-2d75bd2765a0",
      "number": 15,
      "content": "Chúng xầm xì chế nhạo và lắc đầu khinh bỉ Ái Nữ của Giê-ru-sa-lem,",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.622Z",
      "updatedAt": "2025-04-24T04:22:57.622Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "e4761eee-53ef-4a51-8cec-08acbdfded2d",
      "number": 15,
      "content": "“Phải chăng đây vốn là thành mang danh tuyệt mỹ,",
      "order": 2,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.632Z",
      "updatedAt": "2025-04-24T04:22:57.632Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "3c353642-5540-4c53-9339-f0f8b6ab1363",
      "number": 15,
      "content": "Và niềm vui của cả thế gian?”",
      "order": 3,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.645Z",
      "updatedAt": "2025-04-24T04:22:57.645Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "bc59135e-dfbf-4f9b-90f9-4cb905d9d106",
      "number": 16,
      "content": "Tất cả kẻ thù của ngươi mở miệng chê bai ngươi;",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.658Z",
      "updatedAt": "2025-04-24T04:22:57.658Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "0eda3e18-5641-4e2f-b27a-080d61c808d6",
      "number": 16,
      "content": "Chúng xầm xì chế nhạo, nghiến răng, và đắc ý nói vang,",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.667Z",
      "updatedAt": "2025-04-24T04:22:57.667Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "432ff4ff-1ade-41db-a18e-a92877610a75",
      "number": 16,
      "content": "“Phen này chúng ta nuốt trộng nó được rồi!",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.680Z",
      "updatedAt": "2025-04-24T04:22:57.680Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "bf313563-7add-42f4-bd6a-6e5cfe30b62d",
      "number": 16,
      "content": "Chúng ta đã đợi ngày này lâu lắm rồi;",
      "order": 3,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.695Z",
      "updatedAt": "2025-04-24T04:22:57.695Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "7f990f56-1d0f-4a00-8ce6-82320961b15e",
      "number": 16,
      "content": "Cuối cùng chúng ta đã thấy được tận mắt!”",
      "order": 4,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.705Z",
      "updatedAt": "2025-04-24T04:22:57.705Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "1cac15e3-c240-4d5a-b9ed-93310d053729",
      "number": 17,
      "content": "Chúa thực hiện những gì Ngài đã quyết định;",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.717Z",
      "updatedAt": "2025-04-24T04:22:57.717Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "afbab81c-d5b0-4955-b4a8-bbe368630be4",
      "number": 17,
      "content": "Ngài làm ứng nghiệm lời Ngài đã cảnh cáo từ lâu.",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.730Z",
      "updatedAt": "2025-04-24T04:22:57.730Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "92e8ace5-3d05-412a-a34c-73b5ca05ceb3",
      "number": 17,
      "content": "Ngài lật đổ chẳng mảy may thương xót;",
      "order": 2,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.743Z",
      "updatedAt": "2025-04-24T04:22:57.743Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "bceb5102-fc0a-4f67-98e6-09f14b792ce5",
      "number": 17,
      "content": "Ngài làm cho kẻ thù của ngươi vui mừng vì ngươi,",
      "order": 3,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.755Z",
      "updatedAt": "2025-04-24T04:22:57.755Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "61a582bd-3d5f-4cd0-aa1a-5345942be8dd",
      "number": 17,
      "content": "Và làm cho sừng của kẻ thù ngươi thêm cứng mạnh.",
      "order": 4,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.768Z",
      "updatedAt": "2025-04-24T04:22:57.768Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "44213bac-769a-4bbf-9348-43e33fd793cf",
      "number": 18,
      "content": "Hỡi tường thành Ái Nữ của Si-ôn, hãy dốc đổ lòng ngươi kêu cầu với Chúa;",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.781Z",
      "updatedAt": "2025-04-24T04:22:57.781Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "302c7ea3-9720-4067-8a31-47b9d54ccd34",
      "number": 18,
      "content": "Hãy đổ lệ ngươi như suối suốt ngày đêm.",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.794Z",
      "updatedAt": "2025-04-24T04:22:57.794Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "f96edd84-d9a5-46ca-bd9a-fa45e13749fc",
      "number": 18,
      "content": "Chớ để cho ngươi được nghỉ ngơi thơi thả.",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.806Z",
      "updatedAt": "2025-04-24T04:22:57.806Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "812dc317-58cb-47fd-b6c5-b8058351a617",
      "number": 18,
      "content": "Ðừng để cho mắt ngươi ngưng đổ lệ phút nào.",
      "order": 3,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.819Z",
      "updatedAt": "2025-04-24T04:22:57.819Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "85e378fd-cb16-4cbe-9d67-a1606388d4ff",
      "number": 19,
      "content": "Hãy thức dậy giữa đêm khuya để kêu cầu thống thiết,",
      "order": 0,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.831Z",
      "updatedAt": "2025-04-24T04:22:57.831Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "1f359e83-f718-47ce-8154-71f4b3988749",
      "number": 19,
      "content": "Mỗi đầu canh hãy cất tiếng van nài;",
      "order": 1,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.844Z",
      "updatedAt": "2025-04-24T04:22:57.844Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "452c9a8c-90d1-4473-b7ed-4b6c9c7d0385",
      "number": 19,
      "content": "Hãy dốc đổ lòng ngươi trước mặt Chúa như nước tuôn lai láng,",
      "order": 2,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.857Z",
      "updatedAt": "2025-04-24T04:22:57.857Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "b6867fcb-7c48-48fc-b44e-2ddd490f64a9",
      "number": 19,
      "content": "Hãy đưa đôi tay ngươi lên hướng về Ngài cầu nguyện cho sinh mạng đàn con dại của ngươi,",
      "order": 3,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.869Z",
      "updatedAt": "2025-04-24T04:22:57.869Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "1628f375-7f39-4d35-aa63-edb2d25eaa00",
      "number": 19,
      "content": "Vì chúng đói khát đến ngất xỉu nơi mỗi đầu đường góc phố.",
      "order": 4,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.888Z",
      "updatedAt": "2025-04-24T04:22:57.888Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "a2418d4d-3450-41f8-9785-c04d923432e9",
      "number": 20,
      "content": "Lạy Chúa, xin Ngài đoái xem và nhìn thấy!",
      "order": 0,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.899Z",
      "updatedAt": "2025-04-24T04:22:57.899Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "3f2100e6-78cd-49f5-b1fb-742ab859018c",
      "number": 20,
      "content": "Ðể phạt ai mà Ngài làm những điều này?",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.911Z",
      "updatedAt": "2025-04-24T04:22:57.911Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "5dfe0053-4b4a-469c-bf7b-1112e40d068d",
      "number": 20,
      "content": "Chẳng lẽ những người mẹ phải ăn thịt con mình để sống,",
      "order": 2,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.922Z",
      "updatedAt": "2025-04-24T04:22:57.922Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "bebd0959-0757-4e68-bbbe-74d2ce574d94",
      "number": 20,
      "content": "Những đứa con mà họ mang nặng đẻ đau?",
      "order": 3,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.934Z",
      "updatedAt": "2025-04-24T04:22:57.934Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "6c6d223a-a2d8-4646-a39a-7ca00a9582a6",
      "number": 20,
      "content": "Chẳng lẽ tư tế và tiên tri phải bị giết trong nơi thánh của Chúa sao?",
      "order": 4,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.946Z",
      "updatedAt": "2025-04-24T04:22:57.946Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "25d54d59-b428-4fb7-823f-abd3bafb0f3f",
      "number": 21,
      "content": "Xác trai trẻ và người già nằm la liệt trên đất khắp các đường phố;",
      "order": 0,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.958Z",
      "updatedAt": "2025-04-24T04:22:57.958Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "3b2eecdf-4a40-4275-86d7-2abef99b0adc",
      "number": 21,
      "content": "Các thiếu nữ và các thanh niên bị ngã chết vì gươm.",
      "order": 1,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.970Z",
      "updatedAt": "2025-04-24T04:22:57.970Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "c0bc4d6e-b0ec-4fef-a43e-a29657b96001",
      "number": 21,
      "content": "Ngài giết họ trong ngày Ngài nổi giận,",
      "order": 2,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:57.985Z",
      "updatedAt": "2025-04-24T04:22:57.985Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "25f04a21-ab7c-4d05-829b-0d213183834e",
      "number": 21,
      "content": "Ngài tàn sát họ không một chút xót thương.",
      "order": 3,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:58.001Z",
      "updatedAt": "2025-04-24T04:22:58.001Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "7b3126ad-d78b-4fe8-a768-65e92d08f9c6",
      "number": 22,
      "content": "Ngài mời quân thù của con từ khắp nơi kéo đến,",
      "order": 0,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:58.016Z",
      "updatedAt": "2025-04-24T04:22:58.016Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "ae5df795-78b9-483a-8fe7-9f1028837689",
      "number": 22,
      "content": "Như thể chúng được mời đến dự một ngày đại lễ tưng bừng,",
      "order": 1,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:58.028Z",
      "updatedAt": "2025-04-24T04:22:58.028Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "9dcdc721-3a5c-45af-8992-e1fbabaa1a8a",
      "number": 22,
      "content": "Ðể trong ngày thịnh nộ của Chúa không người nào trốn thoát hoặc sống sót,",
      "order": 2,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:58.040Z",
      "updatedAt": "2025-04-24T04:22:58.040Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    },
    {
      "id": "ab53b801-c9f3-4aff-b962-94dc648b519c",
      "number": 22,
      "content": "Quân thù giết cả những con thơ mà con bồng ẵm và dưỡng nuôi.",
      "order": 3,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:22:58.052Z",
      "updatedAt": "2025-04-24T04:22:58.052Z",
      "chapterId": "83361345-6b7a-4e84-9e79-851a001cd929"
    }
  ],
