{
  "chapter": {
    "id": "8c487d74-4677-437c-a722-72d68205121b",
    "number": 2,
    "ref": "https://www.biblegateway.com/passage/?search=Ai%20Ca%202&version=BD2011",
    "createdAt": "2025-04-22T07:37:53.610Z",
    "updatedAt": "2025-04-22T07:37:53.610Z",
    "bookId": "1a450e41-7151-4559-8401-d82ee41eafef"
  },
  "verses": [
    {
      "id": "8db0826e-058c-4f05-87d9-885af0ca0bce",
      "number": 1,
      "content": "Sao Chúa nỡ bao phủ Ái Nữ của Si-ôn bằng cơn giận?",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.103Z",
      "updatedAt": "2025-04-22T08:53:58.103Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "3034e9dd-b031-4481-b713-04e8421f30a8",
      "number": 1,
      "content": "Từ trời cao Ngài ném xuống đất vẻ đẹp của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.131Z",
      "updatedAt": "2025-04-22T08:53:58.131Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "4b5d049d-7c27-4131-8e0a-b6c9d50eb10a",
      "number": 1,
      "content": "Và không nhớ đến bệ chân Ngài trong ngày Ngài nổi giận.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.145Z",
      "updatedAt": "2025-04-22T08:53:58.145Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "fa0b72c7-f58e-4524-aef7-f87f7f3d1f90",
      "number": 2,
      "content": "Chúa tiêu diệt không chút xót thương mọi nơi cư ngụ của Gia-cốp;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.155Z",
      "updatedAt": "2025-04-22T08:53:58.155Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "bb4bfa0f-acef-4677-bc07-110d454c4f56",
      "number": 2,
      "content": "Trong cơn giận Ngài phá tan các thành lũy Ái Nữ của Giu-đa;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.166Z",
      "updatedAt": "2025-04-22T08:53:58.166Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "55ecea68-4e5b-4734-baf6-aa229047fe7f",
      "number": 2,
      "content": "Ngài khiến chúng đổ xuống đất;",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.177Z",
      "updatedAt": "2025-04-22T08:53:58.177Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "939fc9bf-3337-4f7e-b661-2d0e15ff95ec",
      "number": 2,
      "content": "Ðể làm nhục vương quốc và những người lãnh đạo của nó.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.188Z",
      "updatedAt": "2025-04-22T08:53:58.188Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "76580d50-6648-4e6c-a085-7085d074ea81",
      "number": 3,
      "content": "Trong cơn cực kỳ phẫn nộ Ngài chặt đứt tất cả các sừng của I-sơ-ra-ên;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.202Z",
      "updatedAt": "2025-04-22T08:53:58.202Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "3e44f1c6-faa3-4ae5-98ef-bf1b4d92da33",
      "number": 3,
      "content": "Ngài rút lại cánh tay phải Ngài trước mặt quân thù của họ;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.213Z",
      "updatedAt": "2025-04-22T08:53:58.213Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "1bd8344d-7554-4d8d-9d1f-79c15ce1f42f",
      "number": 3,
      "content": "Ngài đốt rụi trong xứ của Gia-cốp,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.224Z",
      "updatedAt": "2025-04-22T08:53:58.224Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "8fba3ba6-1ede-4860-879a-0910144d08df",
      "number": 3,
      "content": "Như ngọn lửa hừng thiêu rụi mọi thứ xung quanh.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.234Z",
      "updatedAt": "2025-04-22T08:53:58.234Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "6f7892b0-00e4-4232-8bb6-172fc677aad4",
      "number": 4,
      "content": "Ngài đã giương cung nhắm bắn như một xạ thủ của quân thù,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.245Z",
      "updatedAt": "2025-04-22T08:53:58.245Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "4734910b-71b5-4275-adc6-c15cbfadfb56",
      "number": 4,
      "content": "Tay phải Ngài sẵn sàng buông tên;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.256Z",
      "updatedAt": "2025-04-22T08:53:58.256Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "64a3e194-e5a2-4973-9b84-e55d2fb01480",
      "number": 4,
      "content": "Như kẻ thù, Ngài giết sạch kẻ nào mắt Ngài muốn giết,",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.269Z",
      "updatedAt": "2025-04-22T08:53:58.269Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "c31e342b-ac24-41ea-b2b0-ab59ec3710e3",
      "number": 5,
      "content": "Chúa đã trở thành như kẻ thù;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.282Z",
      "updatedAt": "2025-04-22T08:53:58.282Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "9a7e14d3-0f22-4bcf-81da-448520fd5b20",
      "number": 5,
      "content": "Ngài nuốt chửng I-sơ-ra-ên;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.295Z",
      "updatedAt": "2025-04-22T08:53:58.295Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "ba9cd8cf-1a19-46af-82b2-22ab419a517f",
      "number": 5,
      "content": "Ngài nuốt trộng tất cả cung điện nó,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.307Z",
      "updatedAt": "2025-04-22T08:53:58.307Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "921d42cc-6f82-4070-ab7e-613cae91e300",
      "number": 5,
      "content": "Ngài biến các thành trì nó ra hoang phế,",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.318Z",
      "updatedAt": "2025-04-22T08:53:58.318Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "ddf92dcb-894d-48a4-9f72-3b407fab650c",
      "number": 5,
      "content": "Ngài làm gia tăng nỗi sầu thảm và bi ai cho Ái Nữ của Giu-đa.",
      "order": 4,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.329Z",
      "updatedAt": "2025-04-22T08:53:58.329Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "4b0c90f1-3a08-43e1-9ef1-a03461a9acad",
      "number": 6,
      "content": "Ngài phá tan Lều Tạm của Ngài như người ta phá bỏ một mảnh vườn;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.348Z",
      "updatedAt": "2025-04-22T08:53:58.348Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "57b27c35-0764-4bc4-b775-e41f361300da",
      "number": 6,
      "content": "Ngài phá hủy nơi nhóm lại của Ngài.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.361Z",
      "updatedAt": "2025-04-22T08:53:58.361Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "85faa04e-4815-492d-9d20-e4f7178bd172",
      "number": 6,
      "content": "Chúa làm cho những ngày đại lễ và những ngày Sa-bát ở Si-ôn không được nhớ đến nữa;",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.374Z",
      "updatedAt": "2025-04-22T08:53:58.374Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "d0b860a8-80c0-4392-959e-3f6515944b29",
      "number": 6,
      "content": "Trong cơn cực kỳ phẫn nộ Ngài ghê tởm cả vua lẫn tư tế ở đó.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.388Z",
      "updatedAt": "2025-04-22T08:53:58.388Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "930c7894-988b-4d45-a14d-5a7d910a039e",
      "number": 7,
      "content": "Chúa loại bỏ bàn thờ của Ngài;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.401Z",
      "updatedAt": "2025-04-22T08:53:58.401Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "c432828f-11a8-4dea-a18c-be9689ac2b4a",
      "number": 7,
      "content": "Ngài ghê tởm nơi thánh của Ngài;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.414Z",
      "updatedAt": "2025-04-22T08:53:58.414Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "cd086616-a056-4ec7-910c-86d851db8b01",
      "number": 7,
      "content": "Ngài trao vào tay quân thù các bức tường cung điện của nàng;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.428Z",
      "updatedAt": "2025-04-22T08:53:58.428Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "c7c30999-3759-4cab-bb80-98f56d4a19d6",
      "number": 7,
      "content": "Chúng làm ồn trong nhà Chúa như tiếng ồn trong ngày đại lễ.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.441Z",
      "updatedAt": "2025-04-22T08:53:58.441Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "5ae58da8-26f7-4244-9bf5-fe33ba867209",
      "number": 8,
      "content": "Chúa quyết định phá đổ tường thành Ái Nữ của Si-ôn;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.454Z",
      "updatedAt": "2025-04-22T08:53:58.454Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "ce1b7bca-37b3-4e9f-bee1-50713edebd20",
      "number": 8,
      "content": "Ngài giăng dây ra đo và không rút tay Ngài lại khi hủy diệt;",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.467Z",
      "updatedAt": "2025-04-22T08:53:58.467Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "da0c4565-18e8-4ce3-9487-71cef522b461",
      "number": 8,
      "content": "Ngài khiến các chiến lũy và tường thành đều buồn thảm;",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.481Z",
      "updatedAt": "2025-04-22T08:53:58.481Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "63d8183c-e2af-49f1-93ff-f6411a073077",
      "number": 8,
      "content": "Chúng cùng nhau suy tàn.",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.494Z",
      "updatedAt": "2025-04-22T08:53:58.494Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "193b5d9a-ac70-4f5b-934f-5498b95c9495",
      "number": 9,
      "content": "Các cổng thành của nàng lún sâu dưới đất;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.508Z",
      "updatedAt": "2025-04-22T08:53:58.508Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "4c80b4c6-3bce-4ede-a039-e6aab1ee82f5",
      "number": 9,
      "content": "Ngài phá tan và bẻ gãy các thanh ngang;",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.521Z",
      "updatedAt": "2025-04-22T08:53:58.521Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "85099fda-372f-4d9c-9d60-157d5b6fdf25",
      "number": 9,
      "content": "Vua và các quan nàng bị lưu đày nơi xứ lạ,",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.535Z",
      "updatedAt": "2025-04-22T08:53:58.535Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "aa44528d-71df-4e0f-addd-e477cd9e3a80",
      "number": 9,
      "content": "Luật Pháp không còn nữa,",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.551Z",
      "updatedAt": "2025-04-22T08:53:58.551Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "d51524f5-3304-452a-82a3-d026446f98c4",
      "number": 9,
      "content": "Các tiên tri nàng chẳng thấy khải tượng nào từ Chúa cả.",
      "order": 4,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.564Z",
      "updatedAt": "2025-04-22T08:53:58.564Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "7471b28f-e1e1-443c-8c98-b4b5eaf58a59",
      "number": 10,
      "content": "Các trưởng lão Ái Nữ của Si-ôn ngồi im lìm dưới đất,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.577Z",
      "updatedAt": "2025-04-22T08:53:58.577Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "2068f8d9-657a-48cb-afdf-d1384eb47dfe",
      "number": 10,
      "content": "Họ hốt bụi rải trên đầu,",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.589Z",
      "updatedAt": "2025-04-22T08:53:58.589Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "8253d70d-6f21-4c39-930e-470ffd24e750",
      "number": 10,
      "content": "Họ mặc vải thô;",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.599Z",
      "updatedAt": "2025-04-22T08:53:58.599Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "9cbdc1b1-ffdc-466a-8f4d-d840af916e39",
      "number": 10,
      "content": "Các trinh nữ của Giê-ru-sa-lem cúi gục đầu xuống đất.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.610Z",
      "updatedAt": "2025-04-22T08:53:58.610Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "d2f2b7ab-a9bc-40d6-a9d1-633119bc105b",
      "number": 11,
      "content": "Mắt tôi hốc hác vì khóc khô dòng lệ;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.621Z",
      "updatedAt": "2025-04-22T08:53:58.621Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "0b7b92fc-139e-4782-91db-ef1d7a1f0c9b",
      "number": 11,
      "content": "Lòng dạ tôi tan nát từng cơn,",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.631Z",
      "updatedAt": "2025-04-22T08:53:58.631Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "e0e1e99e-7728-4431-8f95-694c68fb28f6",
      "number": 11,
      "content": "Mật tôi đổ ra trên đất vì sự hủy diệt Ái Nữ của dân tôi,",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.643Z",
      "updatedAt": "2025-04-22T08:53:58.643Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "1b5a49d9-eca5-4c23-b32c-38795e6cb13e",
      "number": 11,
      "content": "Bởi vì các trẻ thơ và các ấu nhi ngất xỉu trên các đường phố.",
      "order": 3,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.656Z",
      "updatedAt": "2025-04-22T08:53:58.656Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "cde22a97-4a24-4a87-b16f-c23865811aa4",
      "number": 12,
      "content": "Chúng khóc xin với mẹ chúng,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.672Z",
      "updatedAt": "2025-04-22T08:53:58.672Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "89503005-44bd-4182-b601-e7c84e9ac17e",
      "number": 12,
      "content": "“Cơm bánh và thức uống đâu?”",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.685Z",
      "updatedAt": "2025-04-22T08:53:58.685Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "8a030681-4dca-4553-ae54-a96e4cdf94b8",
      "number": 12,
      "content": "Chúng ngất xỉu như người bị tử thương trên các đường phố,",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.699Z",
      "updatedAt": "2025-04-22T08:53:58.699Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "8921cd69-1661-4ee8-841f-e36acd37c856",
      "number": 12,
      "content": "Rồi chúng từ từ tắt thở trong vòng tay bên lòng mẹ.",
      "order": 3,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.712Z",
      "updatedAt": "2025-04-22T08:53:58.712Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "c03702ab-b035-4ea2-9677-3251664e0df9",
      "number": 13,
      "content": "Hỡi Ái Nữ của Giê-ru-sa-lem, ta có thể nói gì cho ngươi nữa?",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.728Z",
      "updatedAt": "2025-04-22T08:53:58.728Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "34b9e12b-d325-4bf8-9a4f-73c1384c4489",
      "number": 13,
      "content": "Ta có thể lấy gì so sánh với ngươi?",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.741Z",
      "updatedAt": "2025-04-22T08:53:58.741Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "26c18b6a-f7fb-406d-b313-6dcdd9e9032c",
      "number": 13,
      "content": "Hỡi nàng trinh nữ, Ái Nữ của Si-ôn, ta có thể so sánh ngươi với ai,",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.755Z",
      "updatedAt": "2025-04-22T08:53:58.755Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "f0a0ee7b-50fc-4fdd-bd4e-b6d86e1ebfbf",
      "number": 13,
      "content": "Ðể ta có thể an ủi ngươi?",
      "order": 3,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.774Z",
      "updatedAt": "2025-04-22T08:53:58.774Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "12189d46-9fce-4290-9758-a8c9ac9b3711",
      "number": 13,
      "content": "Vì nỗi đau thương của ngươi lớn như biển cả,",
      "order": 4,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.787Z",
      "updatedAt": "2025-04-22T08:53:58.787Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "8287f6aa-7a86-48b0-8391-2a48bd82921b",
      "number": 13,
      "content": "Ai có thể chữa cho ngươi được lành?",
      "order": 5,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.804Z",
      "updatedAt": "2025-04-22T08:53:58.804Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "f9c4cf1e-ab66-42fb-b624-7eb813c211e6",
      "number": 14,
      "content": "Các tiên tri của ngươi đã thấy toàn những điều giả dối và tầm phào;",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.817Z",
      "updatedAt": "2025-04-22T08:53:58.817Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "eb51f727-e477-4ad9-b746-79f24ff6f456",
      "number": 14,
      "content": "Chúng không vạch rõ các tội của ngươi ra,",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.830Z",
      "updatedAt": "2025-04-22T08:53:58.830Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "c7ed8644-bf44-41bb-90d0-8b7c2f8a0ac1",
      "number": 14,
      "content": "Hầu mong đảo ngược vận mạng của ngươi,",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.844Z",
      "updatedAt": "2025-04-22T08:53:58.844Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "9063a2bd-736c-4051-a1e0-574eb28a92d2",
      "number": 14,
      "content": "Nhưng chúng chỉ thấy và bảo ngươi toàn những sứ điệp giả dối và ảo giác hư không.",
      "order": 3,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.870Z",
      "updatedAt": "2025-04-22T08:53:58.870Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "b0f13984-d952-4679-80f3-541daf1be6a3",
      "number": 15,
      "content": "Tất cả những kẻ đi ngang qua thấy ngươi đều đập tay tiếc rẻ,",
      "order": 0,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.884Z",
      "updatedAt": "2025-04-22T08:53:58.884Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "1bd582a0-eb80-4bbe-bdf2-42afcabbf42d",
      "number": 15,
      "content": "Chúng xầm xì chế nhạo và lắc đầu khinh bỉ Ái Nữ của Giê-ru-sa-lem,",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.897Z",
      "updatedAt": "2025-04-22T08:53:58.897Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "291d182b-61c1-4c98-ac17-0a7e57392af9",
      "number": 15,
      "content": "“Phải chăng đây vốn là thành mang danh tuyệt mỹ,",
      "order": 2,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.910Z",
      "updatedAt": "2025-04-22T08:53:58.910Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "2d1eeff4-3f53-46d8-91b4-5758e291a302",
      "number": 15,
      "content": "Và niềm vui của cả thế gian?”",
      "order": 3,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.926Z",
      "updatedAt": "2025-04-22T08:53:58.926Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "fc404370-be94-449d-b47f-d850d533ead2",
      "number": 16,
      "content": "Tất cả kẻ thù của ngươi mở miệng chê bai ngươi;",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.940Z",
      "updatedAt": "2025-04-22T08:53:58.940Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "3879536b-6bd3-4789-ae00-cbd9884313c2",
      "number": 16,
      "content": "Chúng xầm xì chế nhạo, nghiến răng, và đắc ý nói vang,",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.953Z",
      "updatedAt": "2025-04-22T08:53:58.953Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "b9f2bfa1-45ac-4e4f-a781-9f138892b78e",
      "number": 16,
      "content": "“Phen này chúng ta nuốt trộng nó được rồi!",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.966Z",
      "updatedAt": "2025-04-22T08:53:58.966Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "5b1a16b4-dd55-48d8-ac8c-7bc46f4e2848",
      "number": 16,
      "content": "Chúng ta đã đợi ngày này lâu lắm rồi;",
      "order": 3,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.979Z",
      "updatedAt": "2025-04-22T08:53:58.979Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "8d667c16-d451-408b-8221-66b3b3b83cd3",
      "number": 16,
      "content": "Cuối cùng chúng ta đã thấy được tận mắt!”",
      "order": 4,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:58.993Z",
      "updatedAt": "2025-04-22T08:53:58.993Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "5ee876fa-b8f2-4477-a3c8-7dc55427fc94",
      "number": 17,
      "content": "Chúa thực hiện những gì Ngài đã quyết định;",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.009Z",
      "updatedAt": "2025-04-22T08:53:59.009Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "dc12c46c-6906-4e0d-a4ee-631861bb85eb",
      "number": 17,
      "content": "Ngài làm ứng nghiệm lời Ngài đã cảnh cáo từ lâu.",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.022Z",
      "updatedAt": "2025-04-22T08:53:59.022Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "58c9e16a-19c7-4e1c-b64a-2ecf64fa4a3d",
      "number": 17,
      "content": "Ngài lật đổ chẳng mảy may thương xót;",
      "order": 2,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.035Z",
      "updatedAt": "2025-04-22T08:53:59.035Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "0904e220-88d5-4378-b316-34481009670b",
      "number": 17,
      "content": "Ngài làm cho kẻ thù của ngươi vui mừng vì ngươi,",
      "order": 3,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.048Z",
      "updatedAt": "2025-04-22T08:53:59.048Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "17192cec-8eb0-4175-a9d6-f4865083c795",
      "number": 17,
      "content": "Và làm cho sừng của kẻ thù ngươi thêm cứng mạnh.",
      "order": 4,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.062Z",
      "updatedAt": "2025-04-22T08:53:59.062Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "f3c0ecf2-66e3-4d19-b5ad-16ae1801bb6d",
      "number": 18,
      "content": "Hỡi tường thành Ái Nữ của Si-ôn, hãy dốc đổ lòng ngươi kêu cầu với Chúa;",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.075Z",
      "updatedAt": "2025-04-22T08:53:59.075Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "fb21f7d1-031e-4275-97e1-8e96d5d15beb",
      "number": 18,
      "content": "Hãy đổ lệ ngươi như suối suốt ngày đêm.",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.088Z",
      "updatedAt": "2025-04-22T08:53:59.088Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "11bf9efb-5bbd-4626-8b0d-6fd62bba6fe0",
      "number": 18,
      "content": "Chớ để cho ngươi được nghỉ ngơi thơi thả.",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.101Z",
      "updatedAt": "2025-04-22T08:53:59.101Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "2791430e-5777-4b99-b61a-23b95104a19b",
      "number": 18,
      "content": "Ðừng để cho mắt ngươi ngưng đổ lệ phút nào.",
      "order": 3,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.115Z",
      "updatedAt": "2025-04-22T08:53:59.115Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "a5b4ea97-9496-49aa-8694-e36c9a7c9388",
      "number": 19,
      "content": "Hãy thức dậy giữa đêm khuya để kêu cầu thống thiết,",
      "order": 0,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.128Z",
      "updatedAt": "2025-04-22T08:53:59.128Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "ad353a69-96b4-463c-b617-43a14f289627",
      "number": 19,
      "content": "Mỗi đầu canh hãy cất tiếng van nài;",
      "order": 1,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.141Z",
      "updatedAt": "2025-04-22T08:53:59.141Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "4d3f7e09-dff1-4938-ad50-a3d1da70076e",
      "number": 19,
      "content": "Hãy dốc đổ lòng ngươi trước mặt Chúa như nước tuôn lai láng,",
      "order": 2,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.154Z",
      "updatedAt": "2025-04-22T08:53:59.154Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "899c52a1-1cb2-4d9d-91de-526a7bee3077",
      "number": 19,
      "content": "Hãy đưa đôi tay ngươi lên hướng về Ngài cầu nguyện cho sinh mạng đàn con dại của ngươi,",
      "order": 3,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.167Z",
      "updatedAt": "2025-04-22T08:53:59.167Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "96b8f72b-90c2-48d3-b2db-f7564d8d2b0e",
      "number": 19,
      "content": "Vì chúng đói khát đến ngất xỉu nơi mỗi đầu đường góc phố.",
      "order": 4,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.180Z",
      "updatedAt": "2025-04-22T08:53:59.180Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "9df55c19-16f4-4ac6-a326-475c2f83ad67",
      "number": 20,
      "content": "Lạy Chúa, xin Ngài đoái xem và nhìn thấy!",
      "order": 0,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.194Z",
      "updatedAt": "2025-04-22T08:53:59.194Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "319095cd-b1ac-4a7f-8529-3d1c002618cf",
      "number": 20,
      "content": "Ðể phạt ai mà Ngài làm những điều này?",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.207Z",
      "updatedAt": "2025-04-22T08:53:59.207Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "65a1561d-00a2-43f2-a712-f6665622966c",
      "number": 20,
      "content": "Chẳng lẽ những người mẹ phải ăn thịt con mình để sống,",
      "order": 2,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.220Z",
      "updatedAt": "2025-04-22T08:53:59.220Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "a4d18923-cd58-4df5-91f0-20f06b11721b",
      "number": 20,
      "content": "Những đứa con mà họ mang nặng đẻ đau?",
      "order": 3,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.236Z",
      "updatedAt": "2025-04-22T08:53:59.236Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "571c9ab4-0d35-4eb0-aaf5-679a5bf97805",
      "number": 20,
      "content": "Chẳng lẽ tư tế và tiên tri phải bị giết trong nơi thánh của Chúa sao?",
      "order": 4,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.249Z",
      "updatedAt": "2025-04-22T08:53:59.249Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "8d01b7eb-aac2-415a-8089-6a214fe5505e",
      "number": 21,
      "content": "Xác trai trẻ và người già nằm la liệt trên đất khắp các đường phố;",
      "order": 0,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.262Z",
      "updatedAt": "2025-04-22T08:53:59.262Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "064823e3-739b-4049-811e-6b9aa9515e26",
      "number": 21,
      "content": "Các thiếu nữ và các thanh niên bị ngã chết vì gươm.",
      "order": 1,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.275Z",
      "updatedAt": "2025-04-22T08:53:59.275Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "1942d28f-dba8-4324-ae88-0257bfec563d",
      "number": 21,
      "content": "Ngài giết họ trong ngày Ngài nổi giận,",
      "order": 2,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.288Z",
      "updatedAt": "2025-04-22T08:53:59.288Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "553c44f2-500c-43b6-bacd-8cb9a4b1c2c7",
      "number": 21,
      "content": "Ngài tàn sát họ không một chút xót thương.",
      "order": 3,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.301Z",
      "updatedAt": "2025-04-22T08:53:59.301Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "b91613d9-d9db-4326-aa33-d178869bcb69",
      "number": 22,
      "content": "Ngài mời quân thù của con từ khắp nơi kéo đến,",
      "order": 0,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.315Z",
      "updatedAt": "2025-04-22T08:53:59.315Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "04441e01-2e10-4a3c-a334-f62670ad0c80",
      "number": 22,
      "content": "Như thể chúng được mời đến dự một ngày đại lễ tưng bừng,",
      "order": 1,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.328Z",
      "updatedAt": "2025-04-22T08:53:59.328Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "0a0b0ce1-e177-40b1-b4e5-0259eb93cceb",
      "number": 22,
      "content": "Ðể trong ngày thịnh nộ của Chúa không người nào trốn thoát hoặc sống sót,",
      "order": 2,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.341Z",
      "updatedAt": "2025-04-22T08:53:59.341Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    },
    {
      "id": "f655c5ce-7ac1-4f16-8515-ec60367771fe",
      "number": 22,
      "content": "Quân thù giết cả những con thơ mà con bồng ẵm và dưỡng nuôi.",
      "order": 3,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:53:59.354Z",
      "updatedAt": "2025-04-22T08:53:59.354Z",
      "chapterId": "8c487d74-4677-437c-a722-72d68205121b"
    }
  ],
