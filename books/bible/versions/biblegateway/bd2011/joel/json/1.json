{
  "chapter": {
    "id": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Gi%C3%B4-%C3%AAn%201&version=BD2011",
    "createdAt": "2025-04-24T03:03:25.170Z",
    "updatedAt": "2025-04-24T03:03:25.170Z",
    "bookId": "38be7a06-1a70-4e75-80e3-b8f0ccf70a64"
  },
  "verses": [
    {
      "id": "c2c5b7aa-033e-41c1-846c-f95372cd0f3c",
      "number": 1,
      "content": "Lời của Chúa phán với Giô-ên con trai Pê-thu-ên:",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:30:19.046Z",
      "updatedAt": "2025-04-24T04:30:19.046Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "643b8660-7fff-4bce-9590-da5c8483e073",
      "number": 2,
      "content": "Hỡi các trưởng lão, hãy lắng nghe những điều nầy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.080Z",
      "updatedAt": "2025-04-24T04:30:19.080Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "7885a2a9-8f2f-4faf-8f2b-24cc03df89a1",
      "number": 2,
      "content": "Hỡi tất cả dân cư trong xứ, hãy lắng tai nghe.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.103Z",
      "updatedAt": "2025-04-24T04:30:19.103Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "98d9a72a-e0bd-4abb-836a-ff74fc64b105",
      "number": 2,
      "content": "Có bao giờ việc như thế nầy đã xảy ra trong thời của các ngươi,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.115Z",
      "updatedAt": "2025-04-24T04:30:19.115Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "636f708b-a32f-436c-8146-a7ee3b6a7572",
      "number": 2,
      "content": "Hoặc trong thời của tổ tiên các ngươi chăng?",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.126Z",
      "updatedAt": "2025-04-24T04:30:19.126Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "a59b561b-a23f-4882-bddc-fb004aa79de7",
      "number": 3,
      "content": "Hãy kể lại cho con cái các ngươi để chúng biết,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.138Z",
      "updatedAt": "2025-04-24T04:30:19.138Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "3a5e6b0f-7990-4790-8003-9ace5abbbbc3",
      "number": 3,
      "content": "Hầu con cái các ngươi sẽ kể lại cho con cái của chúng,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.150Z",
      "updatedAt": "2025-04-24T04:30:19.150Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "c0b32575-f9c7-4629-9808-2e27fb97eb94",
      "number": 3,
      "content": "Con cái của chúng sẽ kể lại cho thế hệ tiếp theo:",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.165Z",
      "updatedAt": "2025-04-24T04:30:19.165Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "c45719b9-de51-48ad-9544-ad4aa3624cfd",
      "number": 4,
      "content": "Vật gì cào cào đầu nhỏ bỏ lại, cào cào đầu lớn ăn.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.179Z",
      "updatedAt": "2025-04-24T04:30:19.179Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "256119e5-1d14-4c19-82d8-51a322c0a17a",
      "number": 4,
      "content": "Vật gì cào cào đầu lớn bỏ lại, cào cào cánh ngắn ăn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.191Z",
      "updatedAt": "2025-04-24T04:30:19.191Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "d964e8d8-9fcd-408b-a0be-5ac67a32b80e",
      "number": 4,
      "content": "Vật gì cào cào cánh ngắn bỏ lại, cào cào cánh dài ăn.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.203Z",
      "updatedAt": "2025-04-24T04:30:19.203Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "74c7235e-6cad-4079-9cb9-8823bd0ee38f",
      "number": 5,
      "content": "Hỡi những kẻ say sưa, hãy tỉnh dậy và khóc lóc;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.214Z",
      "updatedAt": "2025-04-24T04:30:19.214Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "d26fbedb-65cb-4c91-af98-1481f7530b76",
      "number": 5,
      "content": "Hỡi mọi kẻ ghiền rượu, hãy mếu máo than van,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.231Z",
      "updatedAt": "2025-04-24T04:30:19.231Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "b994e482-f1f2-40bc-a6ed-e92cd7c23db3",
      "number": 5,
      "content": "Vì rượu mới chưa kịp uống đã bị cướp mất;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.243Z",
      "updatedAt": "2025-04-24T04:30:19.243Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "68e46545-9adc-498a-b269-20daafc682d1",
      "number": 6,
      "content": "Vì một dân hùng mạnh và đông vô số đã xâm lăng xứ sở chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.254Z",
      "updatedAt": "2025-04-24T04:30:19.254Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "8db449f9-76f7-48c9-a6c3-f2e92a84eb5f",
      "number": 6,
      "content": "Răng của nó như răng sư tử;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.281Z",
      "updatedAt": "2025-04-24T04:30:19.281Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "abcec1c5-c15f-4d80-89f3-289c9ae60ee5",
      "number": 6,
      "content": "Nanh của nó như nanh sư tử cái.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.294Z",
      "updatedAt": "2025-04-24T04:30:19.294Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "0722353b-4ed1-4d65-9e08-09bf7ca2cdeb",
      "number": 7,
      "content": "Nó đã phá tan hoang cây nho của chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.306Z",
      "updatedAt": "2025-04-24T04:30:19.306Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "fba4128d-5eee-45b9-979e-43211de0646b",
      "number": 7,
      "content": "Nó đã biến cây vả của chúng ta thành đống củi lộn.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.319Z",
      "updatedAt": "2025-04-24T04:30:19.319Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "85d0cdf3-e3fc-4740-bceb-903a8d7a62dd",
      "number": 7,
      "content": "Nó tước sạch vỏ rồi quăng xuống;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.331Z",
      "updatedAt": "2025-04-24T04:30:19.331Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "c97d4626-152d-4532-919b-fe2565730fc5",
      "number": 7,
      "content": "Nó lột sạch các cành, khiến chúng trắng hếu trụi trơ.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.347Z",
      "updatedAt": "2025-04-24T04:30:19.347Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "6e9b31db-8b08-4109-be2e-1aeeb8cee3a0",
      "number": 8,
      "content": "Hãy than khóc như người trinh nữ trẻ mặc tang phục khóc thương cho hôn phu nàng vừa mới mất.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.366Z",
      "updatedAt": "2025-04-24T04:30:19.366Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "8de2cfb2-48ff-4e99-ab92-69b29905b56e",
      "number": 9,
      "content": "Trong nhà Chúa, của lễ chay và của lễ quán không còn dâng nữa.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.381Z",
      "updatedAt": "2025-04-24T04:30:19.381Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "8491d61d-494f-405f-abf3-22c6e1302cdb",
      "number": 9,
      "content": "Các tư tế, tức các tôi tớ Chúa, buồn thảm như thể đang thọ tang.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.394Z",
      "updatedAt": "2025-04-24T04:30:19.394Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "e1cd2d29-a51d-451d-afd9-e5ecfac09fc2",
      "number": 10,
      "content": "Ruộng đồng bị bỏ hoang, đất đai tiêu điều,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.408Z",
      "updatedAt": "2025-04-24T04:30:19.408Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "033e8f42-9971-4d20-84d9-ff28ad48c785",
      "number": 10,
      "content": "Lúa gạo hư hại, rượu mới chẳng còn, dầu ô-liu khan hiếm.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.423Z",
      "updatedAt": "2025-04-24T04:30:19.423Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "be3d806a-92ad-4f86-89a5-07cc89eab1b2",
      "number": 11,
      "content": "Hỡi các nông phu, hãy thẹn thùng xấu hổ,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.434Z",
      "updatedAt": "2025-04-24T04:30:19.434Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "7c1fff59-7299-4ae2-9aff-33996d949979",
      "number": 11,
      "content": "Hỡi những kẻ trồng nho, hãy khóc lóc than van,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.445Z",
      "updatedAt": "2025-04-24T04:30:19.445Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "622cd2f9-5954-4bd4-9f2b-6131d3c008df",
      "number": 11,
      "content": "Vì lúa mì và lúa mạch không còn nữa,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.456Z",
      "updatedAt": "2025-04-24T04:30:19.456Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "20af945e-63b5-49a5-a362-ef0b6b636fc5",
      "number": 11,
      "content": "Bởi ruộng nương bị phá hủy cả rồi.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.466Z",
      "updatedAt": "2025-04-24T04:30:19.466Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "b33a3a28-ebeb-4411-a2ec-7f210122e60b",
      "number": 12,
      "content": "Cây nho cằn cỗi, cây vả héo khô;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.476Z",
      "updatedAt": "2025-04-24T04:30:19.476Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "57730cdb-ee4b-4aef-a663-6aadc06eb44d",
      "number": 12,
      "content": "Cây lựu, cây chà là, và cây táo cùng chung số phận;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.486Z",
      "updatedAt": "2025-04-24T04:30:19.486Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "a531ca4d-371e-4f45-8680-d075134d5e31",
      "number": 12,
      "content": "Tất cả cây cối ngoài đồng khô héo,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.496Z",
      "updatedAt": "2025-04-24T04:30:19.496Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "bdd15a91-d72b-4739-8488-23ba3be27ede",
      "number": 12,
      "content": "Và như thế niềm vui của con cái loài người cũng khô héo theo.”",
      "order": 3,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.506Z",
      "updatedAt": "2025-04-24T04:30:19.506Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "89edfd42-59f5-409c-b383-e19497324769",
      "number": 13,
      "content": "Hỡi các tư tế, hãy mặc tang phục và than khóc;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.516Z",
      "updatedAt": "2025-04-24T04:30:19.516Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "f59fb1ed-b6d8-40fe-ba4d-015ec428191c",
      "number": 13,
      "content": "Hỡi những người phục vụ nơi bàn thờ, hãy khóc lóc thở than;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.535Z",
      "updatedAt": "2025-04-24T04:30:19.535Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "7bde5d47-41e7-42ce-8827-8f88b0280109",
      "number": 13,
      "content": "Hỡi các tôi tớ của Ðức Chúa Trời, hãy đến,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.545Z",
      "updatedAt": "2025-04-24T04:30:19.545Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "77621d0a-90da-4b9f-afd2-8653b4bcf57c",
      "number": 13,
      "content": "Hãy mặc vải thô và nằm than thở suốt đêm,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.555Z",
      "updatedAt": "2025-04-24T04:30:19.555Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "73f31804-a553-4b82-9445-ade98099f188",
      "number": 13,
      "content": "Vì nơi nhà Ðức Chúa Trời của lễ chay và của lễ quán đã bị cắt.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.566Z",
      "updatedAt": "2025-04-24T04:30:19.566Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "89c33f3d-d25c-4781-94cb-ac5514a9d214",
      "number": 14,
      "content": "Hãy biệt riêng ra một cuộc kiêng ăn;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.575Z",
      "updatedAt": "2025-04-24T04:30:19.575Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "ea17e3ca-ad33-4a8b-8c25-3d68141975f1",
      "number": 14,
      "content": "Hãy triệu tập một đại hội trọng thể;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.585Z",
      "updatedAt": "2025-04-24T04:30:19.585Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "9586cf5c-461f-406e-850c-2c256ac10ccd",
      "number": 14,
      "content": "Hãy mời các trưởng lão và toàn dân trong xứ đến nhà Chúa, Ðức Chúa Trời của anh chị em,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.595Z",
      "updatedAt": "2025-04-24T04:30:19.595Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "c590b47e-b3d1-4fc0-a563-7b33b91ea378",
      "number": 14,
      "content": "Rồi hãy cùng nhau kêu cầu Chúa.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.605Z",
      "updatedAt": "2025-04-24T04:30:19.605Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "e45866c1-27fb-49bb-805c-c2689b4e5b7d",
      "number": 15,
      "content": "Than ôi, ngày ấy đang đến!",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.615Z",
      "updatedAt": "2025-04-24T04:30:19.615Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "d42ea73a-651d-4540-87bf-3e18e07545c8",
      "number": 15,
      "content": "Ngày của Chúa đang đến gần;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.626Z",
      "updatedAt": "2025-04-24T04:30:19.626Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "cf5cafc9-09a0-4203-b566-47f179e6b147",
      "number": 15,
      "content": "Nó đến như một cơn hủy diệt từ Ðấng Toàn Năng.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.636Z",
      "updatedAt": "2025-04-24T04:30:19.636Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "baafc11f-f2c3-44eb-8555-8bee7db0f328",
      "number": 16,
      "content": "Há chẳng phải thực phẩm bị cắt trước mắt chúng ta sao?",
      "order": 0,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.645Z",
      "updatedAt": "2025-04-24T04:30:19.645Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "18b5b5ae-b092-49b9-8d27-3628acb3328c",
      "number": 16,
      "content": "Sự hân hoan và vui vẻ cũng chẳng còn thấy nơi nhà Ðức Chúa Trời chúng ta sao?",
      "order": 1,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.655Z",
      "updatedAt": "2025-04-24T04:30:19.655Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "487d2c79-a3a7-49cd-b6df-d8238f3cd2f7",
      "number": 17,
      "content": "Hạt giống chết khô dưới lớp đất,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.670Z",
      "updatedAt": "2025-04-24T04:30:19.670Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "c867ecd4-d3ff-4f5d-8d20-40f3307cf265",
      "number": 17,
      "content": "Các vựa lúa trở thành nhà hoang,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.684Z",
      "updatedAt": "2025-04-24T04:30:19.684Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "4aaf91ff-7bdc-4773-be63-0cbe1de20c79",
      "number": 17,
      "content": "Các kho gạo trống không đổ nát,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.697Z",
      "updatedAt": "2025-04-24T04:30:19.697Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "85834279-d73b-40ed-a4b5-8b8eb6d75757",
      "number": 17,
      "content": "Bởi vì chẳng còn lúa gạo.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.710Z",
      "updatedAt": "2025-04-24T04:30:19.710Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "fe752125-cf26-4970-96e1-e0d1d70c8110",
      "number": 18,
      "content": "Súc vật rên rỉ thảm thương,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.723Z",
      "updatedAt": "2025-04-24T04:30:19.723Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "dff134a8-37ef-4300-9922-82eaf5eb6b77",
      "number": 18,
      "content": "Trâu bò lang thang đói khát,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.737Z",
      "updatedAt": "2025-04-24T04:30:19.737Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "c7830291-eace-4db0-8bcd-f917f3e2b343",
      "number": 18,
      "content": "Vì chẳng có đồng cỏ cho chúng;",
      "order": 2,
      "parNumber": 7,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.750Z",
      "updatedAt": "2025-04-24T04:30:19.750Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "6f79b93a-7e72-4da8-8509-68be60bd67fd",
      "number": 18,
      "content": "Ngay cả các đàn chiên cũng cùng chung số phận.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.763Z",
      "updatedAt": "2025-04-24T04:30:19.763Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "c7d1f701-1ce3-4c48-b7d9-9bc806d50a0f",
      "number": 19,
      "content": "Lạy Chúa, con cầu xin Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.777Z",
      "updatedAt": "2025-04-24T04:30:19.777Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "321047bc-8cc0-49a5-b297-87592dc321d5",
      "number": 19,
      "content": "Vì lửa đã thiêu rụi những đồng cỏ trong đồng hoang,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.790Z",
      "updatedAt": "2025-04-24T04:30:19.790Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "d766711f-4d8d-43ef-8673-c0139406ca01",
      "number": 19,
      "content": "Và ngọn lửa đã đốt cháy mọi cây cối ngoài đồng.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.804Z",
      "updatedAt": "2025-04-24T04:30:19.804Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "9e746a5b-7098-428a-b900-3658589b2700",
      "number": 20,
      "content": "Ngay cả các thú hoang cũng van xin Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.816Z",
      "updatedAt": "2025-04-24T04:30:19.816Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "03bffa77-6368-4eaa-9fe8-18b6a9f7f838",
      "number": 20,
      "content": "Vì các dòng nước đều khô cạn,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.832Z",
      "updatedAt": "2025-04-24T04:30:19.832Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    },
    {
      "id": "3cba63c5-99b9-4c49-8f3b-d6e2ee99e929",
      "number": 20,
      "content": "Và lửa đã thiêu rụi những đồng cỏ trong đồng hoang.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:30:19.845Z",
      "updatedAt": "2025-04-24T04:30:19.845Z",
      "chapterId": "880ed3c5-b6d6-45b9-b43d-9aaea0584d2a"
    }
  ],
