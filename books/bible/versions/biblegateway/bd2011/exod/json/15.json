    {
      "id": "88cccaa8-a6de-4803-9d94-85d611c66e34",
      "content": "Ma-ra *nghĩa là* đắng",
      "position": 108,
      "order": 4,
      "createdAt": "2025-04-22T07:43:40.645Z",
