{
  "chapter": {
    "id": "b7e4274e-003d-4508-bd77-9aa650db9cac",
    "number": 138,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20138&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.988Z",
    "updatedAt": "2025-04-22T07:37:48.988Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "a4f6230e-2428-44f9-871c-41a0097db277",
      "number": 1,
      "content": "Con sẽ hết lòng ca ngợi Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.847Z",
      "updatedAt": "2025-04-22T08:36:07.847Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "47373fb3-f9e3-43cf-a2a2-68232dbba2cc",
      "number": 1,
      "content": "Con sẽ ca ngợi Ngài trước mặt các thần.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.875Z",
      "updatedAt": "2025-04-22T08:36:07.875Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "e5449725-64f9-4734-a092-41fda935bb77",
      "number": 2,
      "content": "Con sẽ hướng về đền thánh Ngài sấp mình thờ lạy và ca ngợi danh Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.886Z",
      "updatedAt": "2025-04-22T08:36:07.886Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "2620bfcc-ba84-4feb-8eb3-ffe02c97fb66",
      "number": 2,
      "content": "Vì tình thương và đức thành tín của Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.897Z",
      "updatedAt": "2025-04-22T08:36:07.897Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "6dcd3495-23df-4ff4-86f3-7a2b92cfc1cc",
      "number": 2,
      "content": "Vì Ngài đã làm cho lời Ngài và danh Ngài được tôn cao trên mọi sự.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.908Z",
      "updatedAt": "2025-04-22T08:36:07.908Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "d3ac575e-8d99-4aaa-83b8-48de99e3412c",
      "number": 3,
      "content": "Vì trong ngày con kêu cầu Ngài đã đáp lời con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.919Z",
      "updatedAt": "2025-04-22T08:36:07.919Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "e40a5670-24e1-4ef4-aa9d-d95316692493",
      "number": 3,
      "content": "Ngài khiến con dũng cảm với sức mạnh trong tâm hồn.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.931Z",
      "updatedAt": "2025-04-22T08:36:07.931Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "1899a942-004a-4391-9c5d-07e7d473a16d",
      "number": 4,
      "content": "Chúa ôi, tất cả các vua trên đất sẽ cảm tạ Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.945Z",
      "updatedAt": "2025-04-22T08:36:07.945Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "32769579-415f-4972-87c4-17a831351c25",
      "number": 4,
      "content": "Khi họ được nghe những lời từ miệng Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.958Z",
      "updatedAt": "2025-04-22T08:36:07.958Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "275bff33-6795-461a-bad5-b71e29cd4433",
      "number": 5,
      "content": "Thật vậy họ sẽ ca ngợi các đường lối Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.971Z",
      "updatedAt": "2025-04-22T08:36:07.971Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "1c2db164-70b7-41d1-9605-7c095b939c49",
      "number": 5,
      "content": "Vì vinh hiển Chúa thật lớn lao thay.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.984Z",
      "updatedAt": "2025-04-22T08:36:07.984Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "4edd565c-c125-453f-ab7c-d78af1972ed5",
      "number": 6,
      "content": "Vì dù Chúa ngự trên trời cao thăm thẳm,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:07.997Z",
      "updatedAt": "2025-04-22T08:36:07.997Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "c621f3cd-1895-4710-9192-f5377b42d226",
      "number": 6,
      "content": "Ngài vẫn đoái thương đến những kẻ thấp hèn;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:08.010Z",
      "updatedAt": "2025-04-22T08:36:08.010Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "b8e54f14-f3b3-411d-a3c0-d32ae3aa9867",
      "number": 6,
      "content": "Còn những kẻ kiêu ngạo Ngài đã biết rõ từ xa.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:08.026Z",
      "updatedAt": "2025-04-22T08:36:08.026Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "fd317c69-1111-4345-8b63-f361fba9640e",
      "number": 7,
      "content": "Dù con phải bước đi giữa cơn nguy khốn,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:08.039Z",
      "updatedAt": "2025-04-22T08:36:08.039Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "72624c12-256c-432a-9244-9f15ee473572",
      "number": 7,
      "content": "Ngài sẽ giữ gìn mạng sống con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:08.052Z",
      "updatedAt": "2025-04-22T08:36:08.052Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "29a6019d-8eba-4a55-ae1b-d1162616b4f7",
      "number": 7,
      "content": "Ngài sẽ đưa tay ra đối phó với cơn giận của những kẻ thù ghét con;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:08.068Z",
      "updatedAt": "2025-04-22T08:36:08.068Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "b264ac12-229f-45e4-a174-f1f8c174849b",
      "number": 7,
      "content": "Tay phải Ngài sẽ cứu con.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:08.081Z",
      "updatedAt": "2025-04-22T08:36:08.081Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "fd4f12e1-c2ec-445c-982b-a4fe10bfa539",
      "number": 8,
      "content": "Chúa sẽ thực hiện những gì Ngài muốn làm cho đời sống con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:08.097Z",
      "updatedAt": "2025-04-22T08:36:08.097Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "b672b569-f69a-4bda-b117-8bffae4ffa90",
      "number": 8,
      "content": "Chúa ôi, lòng thương xót của Ngài còn đến đời đời;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:08.110Z",
      "updatedAt": "2025-04-22T08:36:08.110Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    },
    {
      "id": "87e9f17b-815b-4286-8e36-7a163140b6c2",
      "number": 8,
      "content": "Xin đừng bỏ công việc của tay Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:36:08.123Z",
      "updatedAt": "2025-04-22T08:36:08.123Z",
      "chapterId": "b7e4274e-003d-4508-bd77-9aa650db9cac"
    }
  ],
