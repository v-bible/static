{
  "chapter": {
    "id": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5",
    "number": 125,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20125&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.632Z",
    "updatedAt": "2025-04-22T07:37:48.632Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "3195cbc4-d356-40bc-ae9b-acb8e76fd95a",
      "number": 1,
      "content": "Phàm ai tin cậy Chúa sẽ như Núi Si-ôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.698Z",
      "updatedAt": "2025-04-22T08:34:39.698Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    },
    {
      "id": "dcf53a5b-24d1-4c18-a6b8-610751ab0e4c",
      "number": 1,
      "content": "Chẳng hề rúng động nhưng cứ bền vững muôn đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.726Z",
      "updatedAt": "2025-04-22T08:34:39.726Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    },
    {
      "id": "554b5c71-57a5-4728-98fa-79019a4e7bf2",
      "number": 2,
      "content": "Như các rặng núi bao quanh Giê-ru-sa-lem thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.744Z",
      "updatedAt": "2025-04-22T08:34:39.744Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    },
    {
      "id": "acf65e21-e88a-43d6-8fd7-b8aedc378121",
      "number": 2,
      "content": "Chúa bao quanh dân Ngài thể ấy,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.757Z",
      "updatedAt": "2025-04-22T08:34:39.757Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    },
    {
      "id": "fc628c06-0cb9-4f56-95f1-6778723909f7",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.770Z",
      "updatedAt": "2025-04-22T08:34:39.770Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    },
    {
      "id": "b2606c5d-eaa2-48ff-8199-f26114b3bb22",
      "number": 3,
      "content": "Vì gậy cầm quyền của kẻ ác không được phép hành sử trên địa phận của người ngay lành,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.784Z",
      "updatedAt": "2025-04-22T08:34:39.784Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    },
    {
      "id": "cd237f5b-b9e2-4183-8b80-7b73696d3244",
      "number": 3,
      "content": "Nên tay người ngay lành không bị buộc phải tham dự vào việc ác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.798Z",
      "updatedAt": "2025-04-22T08:34:39.798Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    },
    {
      "id": "825ac4f9-a12c-4b02-bfa4-31251153a433",
      "number": 4,
      "content": "Chúa ôi, xin ban phước cho những người tốt,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.811Z",
      "updatedAt": "2025-04-22T08:34:39.811Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    },
    {
      "id": "3e76e48d-d641-4951-a187-ab8feb321a0a",
      "number": 4,
      "content": "Và cho những người có lòng ngay thẳng thật thà.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.824Z",
      "updatedAt": "2025-04-22T08:34:39.824Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    },
    {
      "id": "50ed5d5a-347a-4954-8088-ae7adee4a3fd",
      "number": 5,
      "content": "Nhưng đối với những kẻ bỏ Ngài để đi theo con đường lươn lẹo,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.837Z",
      "updatedAt": "2025-04-22T08:34:39.837Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    },
    {
      "id": "b599744a-bec3-4e3e-8800-cb3dd8002276",
      "number": 5,
      "content": "Xin Chúa trục xuất chúng chung với những kẻ làm điều ác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.850Z",
      "updatedAt": "2025-04-22T08:34:39.850Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    },
    {
      "id": "dbba4573-b601-4b24-8cf3-5230b28c9099",
      "number": 5,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:39.863Z",
      "updatedAt": "2025-04-22T08:34:39.863Z",
      "chapterId": "b53b49f2-a092-4152-b8c1-b0c7d5bf4ca5"
    }
  ],
