{
  "chapter": {
    "id": "5466e3d5-fafd-40a5-b6c7-b11b4febb866",
    "number": 125,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20125&version=BD2011",
    "createdAt": "2025-04-24T03:03:18.047Z",
    "updatedAt": "2025-04-24T03:03:18.047Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "f65ccc10-b33a-43ce-9ab6-d7c0945537da",
      "number": 1,
      "content": "Phàm ai tin cậy Chúa sẽ như Núi Si-ôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.124Z",
      "updatedAt": "2025-04-24T04:02:55.124Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    },
    {
      "id": "67235c6e-3f36-43c7-9ca4-4eeab8eaa9a3",
      "number": 1,
      "content": "Chẳng hề rúng động nhưng cứ bền vững muôn đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.157Z",
      "updatedAt": "2025-04-24T04:02:55.157Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    },
    {
      "id": "478a88fa-3699-43ce-af30-cab7e34c7685",
      "number": 2,
      "content": "Như các rặng núi bao quanh Giê-ru-sa-lem thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.171Z",
      "updatedAt": "2025-04-24T04:02:55.171Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    },
    {
      "id": "42ae4f1e-c60b-4b64-abf2-366c759c88b8",
      "number": 2,
      "content": "Chúa bao quanh dân Ngài thể ấy,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.184Z",
      "updatedAt": "2025-04-24T04:02:55.184Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    },
    {
      "id": "ac40da6d-2668-46f8-adc1-1b23564c62fd",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.197Z",
      "updatedAt": "2025-04-24T04:02:55.197Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    },
    {
      "id": "7bc2ede5-c115-44fa-b98e-f85d8ee2af67",
      "number": 3,
      "content": "Vì gậy cầm quyền của kẻ ác không được phép hành sử trên địa phận của người ngay lành,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.211Z",
      "updatedAt": "2025-04-24T04:02:55.211Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    },
    {
      "id": "b4203ccc-568f-4c7f-9921-8a9faef85aa5",
      "number": 3,
      "content": "Nên tay người ngay lành không bị buộc phải tham dự vào việc ác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.224Z",
      "updatedAt": "2025-04-24T04:02:55.224Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    },
    {
      "id": "053cada8-1dcf-44aa-ae6c-20406b1beebe",
      "number": 4,
      "content": "Chúa ôi, xin ban phước cho những người tốt,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.237Z",
      "updatedAt": "2025-04-24T04:02:55.237Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    },
    {
      "id": "a3d07977-995f-4967-8b05-f0e687576f6c",
      "number": 4,
      "content": "Và cho những người có lòng ngay thẳng thật thà.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.250Z",
      "updatedAt": "2025-04-24T04:02:55.250Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    },
    {
      "id": "b4ca9d8f-be15-4c1f-9275-0e1b32c59c5a",
      "number": 5,
      "content": "Nhưng đối với những kẻ bỏ Ngài để đi theo con đường lươn lẹo,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.263Z",
      "updatedAt": "2025-04-24T04:02:55.263Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    },
    {
      "id": "74d7c149-6622-47b4-82b5-2ca1d6ca4f97",
      "number": 5,
      "content": "Xin Chúa trục xuất chúng chung với những kẻ làm điều ác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.277Z",
      "updatedAt": "2025-04-24T04:02:55.277Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    },
    {
      "id": "ad54261e-4a84-4139-8f1b-dff7c7cd96e4",
      "number": 5,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:55.293Z",
      "updatedAt": "2025-04-24T04:02:55.293Z",
      "chapterId": "5466e3d5-fafd-40a5-b6c7-b11b4febb866"
    }
  ],
