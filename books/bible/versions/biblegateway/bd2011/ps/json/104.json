{
  "chapter": {
    "id": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39",
    "number": 104,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20104&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.521Z",
    "updatedAt": "2025-04-24T03:03:17.521Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "f32885dd-6cb5-43b6-aa97-b586a40c9075",
      "number": 1,
      "content": "Hãy ca ngợi Chúa, hỡi linh hồn ta!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.166Z",
      "updatedAt": "2025-04-24T04:00:05.166Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "f96ec9ed-53c6-452a-8db4-f524db139c99",
      "number": 1,
      "content": "Lạy Chúa, Ðức Chúa Trời của con, Ngài thật vĩ đại thay!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.181Z",
      "updatedAt": "2025-04-24T04:00:05.181Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "9df5a260-411f-4b1c-97ef-3c3e7085f5c8",
      "number": 1,
      "content": "Ngài mặc bằng sự huy hoàng và sự uy nghi.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.194Z",
      "updatedAt": "2025-04-24T04:00:05.194Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "52efa5eb-653b-40d4-a48d-489abf89c3f2",
      "number": 2,
      "content": "Ngài bao phủ mình bằng ánh sáng như chiếc áo choàng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.208Z",
      "updatedAt": "2025-04-24T04:00:05.208Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "a7a95465-174f-4a0a-bad5-0153f1643d9d",
      "number": 2,
      "content": "Ngài giăng các tầng trời ra như căng một cái trại.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.226Z",
      "updatedAt": "2025-04-24T04:00:05.226Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "f561f4fc-8356-476b-ada4-2318e78762a9",
      "number": 3,
      "content": "Ngài đặt các đà ngang cho các thiên cung của Ngài trên nước;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.238Z",
      "updatedAt": "2025-04-24T04:00:05.238Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "e7e754a7-38c0-4193-82ab-7f36407840d6",
      "number": 3,
      "content": "Ngài dùng mây làm xa giá;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.254Z",
      "updatedAt": "2025-04-24T04:00:05.254Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "60642c67-552a-4275-9322-ce92978f6912",
      "number": 3,
      "content": "Ngài bước đi trên cánh gió;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.270Z",
      "updatedAt": "2025-04-24T04:00:05.270Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "353a68ef-9b6f-4c02-b88a-c33726b5bafb",
      "number": 4,
      "content": "Ngài dùng gió làm các sứ giả Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.287Z",
      "updatedAt": "2025-04-24T04:00:05.287Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "82ae242f-e109-4914-a871-0919b7c97488",
      "number": 4,
      "content": "Lửa hừng làm bầy tôi hầu hạ Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.304Z",
      "updatedAt": "2025-04-24T04:00:05.304Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "4a46c2d6-635c-488d-b488-0390a65354c6",
      "number": 5,
      "content": "Ngài đặt trái đất trên nền của nó,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.322Z",
      "updatedAt": "2025-04-24T04:00:05.322Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "156c3d2b-2b76-46ea-a92f-eeb59e8508b7",
      "number": 5,
      "content": "Ðể đến đời đời nó chẳng chuyển lay.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.333Z",
      "updatedAt": "2025-04-24T04:00:05.333Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "29b27b48-1918-4272-b7e5-c0c4242bcbbb",
      "number": 6,
      "content": "Ngài bao phủ đất bằng biển sâu như phủ một chiếc áo;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.344Z",
      "updatedAt": "2025-04-24T04:00:05.344Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "031e4ab3-7caa-4eb5-b3ed-f5dec3ea0b47",
      "number": 6,
      "content": "Các nguồn nước lại đứng trên các ngọn núi cao.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.360Z",
      "updatedAt": "2025-04-24T04:00:05.360Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "47bbc6eb-703b-4de7-bc89-c3f7e6f954ea",
      "number": 7,
      "content": "Khi Ngài quở trách, chúng thất kinh chạy trốn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.371Z",
      "updatedAt": "2025-04-24T04:00:05.371Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "c1039b8c-1402-432f-a3d9-8e51abefac82",
      "number": 7,
      "content": "Nghe tiếng sấm sét của Ngài, chúng vụt chạy cho xa.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.384Z",
      "updatedAt": "2025-04-24T04:00:05.384Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "0a8b6e1b-5bd9-4b34-b695-9878c25b5474",
      "number": 8,
      "content": "Núi đồi trồi lên, thung lũng sụp xuống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.398Z",
      "updatedAt": "2025-04-24T04:00:05.398Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "df8d8e5f-6431-449a-a806-26169d22d4be",
      "number": 8,
      "content": "Ðúng vào nơi Ngài định cho chúng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.408Z",
      "updatedAt": "2025-04-24T04:00:05.408Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "ec53e8dd-8b3b-4b74-843e-4cdc4e3df75b",
      "number": 9,
      "content": "Ngài định giới hạn để nước không thể vượt qua,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.419Z",
      "updatedAt": "2025-04-24T04:00:05.419Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "1c8591b0-f403-4fac-970a-5ad4e1deeb8a",
      "number": 9,
      "content": "Hầu không được phủ ngập mặt đất nữa.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.429Z",
      "updatedAt": "2025-04-24T04:00:05.429Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "fdf4722b-cdbe-43aa-8317-5dda2e0d40aa",
      "number": 10,
      "content": "Trong các thung lũng Ngài ban các dòng nước ngọt;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.442Z",
      "updatedAt": "2025-04-24T04:00:05.442Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "049892f6-e6ef-44b7-93e6-6d7d44a15e7a",
      "number": 10,
      "content": "Chúng lượn quanh tuôn chảy giữa núi đồi;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.455Z",
      "updatedAt": "2025-04-24T04:00:05.455Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "42535592-a151-4b78-9609-551cb5f94aab",
      "number": 11,
      "content": "Chúng cung cấp thức uống cho mọi sinh vật trong đồng;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.468Z",
      "updatedAt": "2025-04-24T04:00:05.468Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "cb606702-689b-4c4f-8bcc-0a81db20808e",
      "number": 11,
      "content": "Nhờ chúng những lừa rừng được thỏa mãn cơn khát.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.478Z",
      "updatedAt": "2025-04-24T04:00:05.478Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "6f254b28-75de-4b4e-b4bf-be687ba87d7f",
      "number": 12,
      "content": "Bên cạnh chúng chim trời làm tổ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.491Z",
      "updatedAt": "2025-04-24T04:00:05.491Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "1cf612a5-cb52-44e9-b776-1b9825d5202f",
      "number": 12,
      "content": "Cất tiếng hót ca giữa các cành cây.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.501Z",
      "updatedAt": "2025-04-24T04:00:05.501Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "1a064d20-91a7-4a8c-9a10-22900b15745e",
      "number": 13,
      "content": "Từ thiên cung Ngài đổ nước xuống tưới các núi rừng;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.511Z",
      "updatedAt": "2025-04-24T04:00:05.511Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "a6645372-155d-43cd-a319-fd95c7a8ec38",
      "number": 13,
      "content": "Trái đất được no nê nhờ bông trái của công việc Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.522Z",
      "updatedAt": "2025-04-24T04:00:05.522Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "31c0dbc9-8315-4d7b-a962-4627558b8cce",
      "number": 14,
      "content": "Ngài khiến cỏ xanh mọc lên cho súc vật,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.532Z",
      "updatedAt": "2025-04-24T04:00:05.532Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "380abf64-bc7c-4383-8443-ae8b65797a20",
      "number": 14,
      "content": "Và rau trái sinh ra để phục vụ loài người;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.543Z",
      "updatedAt": "2025-04-24T04:00:05.543Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "3e18e48a-f01d-4b32-afb9-ddd17480d0f1",
      "number": 14,
      "content": "Từ ruộng đất người ta có thể đem về cho mình thực phẩm,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.554Z",
      "updatedAt": "2025-04-24T04:00:05.554Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "b4bc5ec6-9e3a-4492-9093-ca302eec6012",
      "number": 15,
      "content": "Rồi chế thành rượu để uống cho lòng vui vẻ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.565Z",
      "updatedAt": "2025-04-24T04:00:05.565Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "10d94fa1-a477-40b1-b24c-e767508cdc05",
      "number": 15,
      "content": "Chế thành dầu để xức cho mặt mày tươi sáng,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.578Z",
      "updatedAt": "2025-04-24T04:00:05.578Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "470bab8e-ed35-4ddf-8be2-d1f7ae40f44e",
      "number": 15,
      "content": "Và làm cơm bánh để ăn cho vững mạnh lòng người.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.591Z",
      "updatedAt": "2025-04-24T04:00:05.591Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "22b72e5a-e8f4-4e98-9d64-3940dc562418",
      "number": 16,
      "content": "Những cây cối của Chúa đều được nước uống no nê,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.603Z",
      "updatedAt": "2025-04-24T04:00:05.603Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "6a37ead5-701b-4855-aeb0-c539c29436cb",
      "number": 16,
      "content": "Kể cả những cây bá hương Ngài đã trồng ở Li-băng,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.616Z",
      "updatedAt": "2025-04-24T04:00:05.616Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "a815d089-d27d-4bab-9ac4-3237f9812f8b",
      "number": 17,
      "content": "Cùng những cây cối khác, nơi các chim trời làm tổ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.630Z",
      "updatedAt": "2025-04-24T04:00:05.630Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "ef72f040-40a8-4389-b59a-cafb5a07c9c9",
      "number": 17,
      "content": "Luôn cả những cây bách, nơi chim hạc chọn làm nhà.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.642Z",
      "updatedAt": "2025-04-24T04:00:05.642Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "606693c7-8798-4452-b479-450754ef9126",
      "number": 18,
      "content": "Ngài dựng nên những núi cao cho các dê rừng làm chỗ ở;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.662Z",
      "updatedAt": "2025-04-24T04:00:05.662Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "b32be295-ad2e-4d74-aab5-b17023331576",
      "number": 18,
      "content": "Các gành đá cheo leo cho các chuột núi nương thân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.675Z",
      "updatedAt": "2025-04-24T04:00:05.675Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "fe35c5e6-9d83-4002-809b-6c3439893b68",
      "number": 19,
      "content": "Ngài dựng nên mặt trăng để ấn định các mùa;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.688Z",
      "updatedAt": "2025-04-24T04:00:05.688Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "6a4e9483-ec64-4521-b6de-fad1aa61bd96",
      "number": 19,
      "content": "Còn mặt trời tự biết khi nào lặn xuống.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.701Z",
      "updatedAt": "2025-04-24T04:00:05.701Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "4a163cbe-7921-4dd7-a7fe-062d6a02d248",
      "number": 20,
      "content": "Ngài dựng nên bóng tối, và nó trở thành ban đêm;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.716Z",
      "updatedAt": "2025-04-24T04:00:05.716Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "602a81e8-8817-4056-b021-7f023163d61d",
      "number": 20,
      "content": "Ấy là khi các thú rừng đi ra săn mồi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.729Z",
      "updatedAt": "2025-04-24T04:00:05.729Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "51d1c252-7aa7-4403-8ad7-76a498efad58",
      "number": 21,
      "content": "Các sư tử tơ gầm rống tìm mồi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.749Z",
      "updatedAt": "2025-04-24T04:00:05.749Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "57da9e9a-69d6-4019-8efa-ce7f2eb9fb70",
      "number": 21,
      "content": "Và tìm kiếm thức ăn từ Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.762Z",
      "updatedAt": "2025-04-24T04:00:05.762Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "0c3b9b53-d79f-4538-8e03-41210b7d0f42",
      "number": 22,
      "content": "Ðến khi mặt trời mọc lên chúng lại rút về,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.775Z",
      "updatedAt": "2025-04-24T04:00:05.775Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "f485bbdf-fe86-4b2b-93a2-cb18089b288e",
      "number": 22,
      "content": "Và nằm xuống trong hang.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.788Z",
      "updatedAt": "2025-04-24T04:00:05.788Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "d9059eab-38c3-4a0e-ad86-7e6877577e63",
      "number": 23,
      "content": "Người ta đi ra làm việc,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.801Z",
      "updatedAt": "2025-04-24T04:00:05.801Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "050ba43c-0139-4b60-987b-c9713b8255bf",
      "number": 23,
      "content": "Và làm lụng cho đến hoàng hôn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.814Z",
      "updatedAt": "2025-04-24T04:00:05.814Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "17880c14-8862-4929-a273-ba0c4a00cca4",
      "number": 24,
      "content": "Chúa ôi, công việc của Ngài thật nhiều thay!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.827Z",
      "updatedAt": "2025-04-24T04:00:05.827Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "1b6134eb-74ed-4142-a890-8c54d8b8affd",
      "number": 24,
      "content": "Ngài đã dựng nên tất cả bằng sự khôn ngoan của Ngài;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.840Z",
      "updatedAt": "2025-04-24T04:00:05.840Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "250c3940-7911-41b3-bb86-1a6af7fa1a79",
      "number": 24,
      "content": "Trái đất đầy dẫy muôn vật do Ngài dựng nên.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.853Z",
      "updatedAt": "2025-04-24T04:00:05.853Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "a629f28e-b644-45aa-8598-c996284d4323",
      "number": 25,
      "content": "Kìa là đại dương sâu rộng mênh mông,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.865Z",
      "updatedAt": "2025-04-24T04:00:05.865Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "2c4c9bc5-b8ed-4897-a711-8fe910ba6962",
      "number": 25,
      "content": "Trong đó chứa nhung nhúc biết bao sinh vật;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.879Z",
      "updatedAt": "2025-04-24T04:00:05.879Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "c4252343-429f-47ca-9d21-4a5c7369a173",
      "number": 25,
      "content": "Thật là vô sốsinh vật lớn và nhỏ.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.892Z",
      "updatedAt": "2025-04-24T04:00:05.892Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "18622275-1b51-4978-8341-ea14c7a02eaa",
      "number": 26,
      "content": "Rồi cũng có các ghe thuyền tàu bè qua lại,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.905Z",
      "updatedAt": "2025-04-24T04:00:05.905Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "0376fea3-d53e-4dc1-a2d1-3a388f9b53ac",
      "number": 26,
      "content": "Và con quái vật Ngài đã dựng nên đùa chơi trong đó.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.918Z",
      "updatedAt": "2025-04-24T04:00:05.918Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "b43b878c-d7b4-40c6-aef1-8def726acaf1",
      "number": 27,
      "content": "Tất cả đều trông chờ nơi Ngài,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.947Z",
      "updatedAt": "2025-04-24T04:00:05.947Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "4ce0dfbb-23fb-4714-a45b-7c099dbbbba7",
      "number": 27,
      "content": "Ðể Ngài ban cho chúng vật thực phải thì.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.960Z",
      "updatedAt": "2025-04-24T04:00:05.960Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "5961d043-d263-4789-84aa-3d0802698de9",
      "number": 28,
      "content": "Khi Ngài cho ăn, chúng liền tụ lại;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.972Z",
      "updatedAt": "2025-04-24T04:00:05.972Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "2d9de438-9249-4f52-a8aa-8adb1dac9415",
      "number": 28,
      "content": "Khi Ngài sè tay ra, chúng được ăn uống no nê.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:05.985Z",
      "updatedAt": "2025-04-24T04:00:05.985Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "d7a6d805-3b9e-4017-a6f0-7792dd829503",
      "number": 29,
      "content": "Khi Ngài ẩn mặt, chúng hoảng kinh bối rối;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.001Z",
      "updatedAt": "2025-04-24T04:00:06.001Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "5767e5f7-afb7-4477-b8be-f5fd5947a013",
      "number": 29,
      "content": "Khi Ngài lấy hơi thở chúng đi, chúng liền chết và trở về cát bụi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.014Z",
      "updatedAt": "2025-04-24T04:00:06.014Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "ea490d9f-d513-4a5c-a1df-838d63001e0b",
      "number": 30,
      "content": "Khi Ngài ban Thần Khí Ngài ra, chúng được dựng nên,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.027Z",
      "updatedAt": "2025-04-24T04:00:06.027Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "d9b37f98-bc68-4740-bf35-cf55a60c96ff",
      "number": 30,
      "content": "Và Ngài làm cho mặt đất mới lại.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.040Z",
      "updatedAt": "2025-04-24T04:00:06.040Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "eaf5b647-283b-47f9-ba17-19fa1399a3bc",
      "number": 31,
      "content": "Nguyện vinh hiển của Chúa còn đến đời đời.",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.053Z",
      "updatedAt": "2025-04-24T04:00:06.053Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "7a039c5d-3a1a-4f60-a12d-c7f9af6f380c",
      "number": 31,
      "content": "Nguyện Chúa vui thích về các công việc Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.069Z",
      "updatedAt": "2025-04-24T04:00:06.069Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "8905b019-a7f6-4a37-9093-368356c2c494",
      "number": 32,
      "content": "Ngài nhìn vào trái đất, nó run lên lẩy bẩy;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.085Z",
      "updatedAt": "2025-04-24T04:00:06.085Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "564c7f1c-dcc9-46b6-b765-e7c644e927e6",
      "number": 32,
      "content": "Ngài đụng đến núi non, chúng bốc khói mịt mù.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.098Z",
      "updatedAt": "2025-04-24T04:00:06.098Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "e6a356c5-f8de-4be0-ab09-72286c0fb864",
      "number": 33,
      "content": "Hễ tôi còn sống bao lâu, tôi sẽ ca tụng Chúa bấy lâu;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.114Z",
      "updatedAt": "2025-04-24T04:00:06.114Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "c0aed8ea-ff30-469c-a075-4aac7e43e6d6",
      "number": 33,
      "content": "Hễ tôi còn sống chừng nào, tôi sẽ ca ngợi Ðức Chúa Trời tôi chừng nấy.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.127Z",
      "updatedAt": "2025-04-24T04:00:06.127Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "66404ed5-f2d2-4f83-8a5e-111edc355080",
      "number": 34,
      "content": "Nguyện những suy nghĩ của tôi được đẹp lòng Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.140Z",
      "updatedAt": "2025-04-24T04:00:06.140Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "a7f73637-8366-4991-a7e9-d8f5e41c9cf6",
      "number": 34,
      "content": "Về phần tôi, tôi sẽ cứ vui mừng trong Chúa.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.153Z",
      "updatedAt": "2025-04-24T04:00:06.153Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "54a9dfbc-0026-40ef-99e9-04ae4817bf46",
      "number": 35,
      "content": "Nguyện những kẻ tội lỗi bị diệt trừ khỏi mặt đất;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.166Z",
      "updatedAt": "2025-04-24T04:00:06.166Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "a6cba3fb-54cd-4457-b009-aa5902076057",
      "number": 35,
      "content": "Nguyện những kẻ ác không còn nữa.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.179Z",
      "updatedAt": "2025-04-24T04:00:06.179Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "5682fcb3-ceb9-4f85-ae45-c4271621746f",
      "number": 35,
      "content": "Hãy chúc tụng Chúa, hỡi linh hồn ta,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.192Z",
      "updatedAt": "2025-04-24T04:00:06.192Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    },
    {
      "id": "919284c4-7778-4ece-a13b-9da5ad4130f2",
      "number": 35,
      "content": "Hãy ca ngợi Chúa!",
      "order": 3,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:06.205Z",
      "updatedAt": "2025-04-24T04:00:06.205Z",
      "chapterId": "8ef782b0-9afb-4c1b-bb98-1b09313f7b39"
    }
  ],
