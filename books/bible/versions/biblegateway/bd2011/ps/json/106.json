{
  "chapter": {
    "id": "4d22aca9-f27a-46c3-ae9c-8d56cc016464",
    "number": 106,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20106&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.125Z",
    "updatedAt": "2025-04-22T07:37:48.125Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "8a60489a-c85f-4372-bd27-9dca080bd933",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.358Z",
      "updatedAt": "2025-04-22T08:32:19.358Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "b8173feb-9a49-4b8a-bbef-2baf6eb0fc69",
      "number": 48,
      "content": "Hãy ca ngợi Chúa!",
      "order": 3,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.826Z",
      "updatedAt": "2025-04-22T08:32:20.826Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "fdc0ca7c-2670-465c-b47b-8deceb97fd5f",
      "number": 1,
      "content": "Hãy cảm tạ Chúa, vì Ngài thật tốt,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.385Z",
      "updatedAt": "2025-04-22T08:32:19.385Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "3ab810a5-3aa2-46a2-9f8c-bda8bc819fba",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.396Z",
      "updatedAt": "2025-04-22T08:32:19.396Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "fb0e378e-5efe-4060-aee6-d4b7320d1c15",
      "number": 2,
      "content": "Ai có thể thuật lại những việc quyền năng của Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.406Z",
      "updatedAt": "2025-04-22T08:32:19.406Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "e90c8d1f-3691-4607-bd9f-1811fa9d06fb",
      "number": 2,
      "content": "Hoặc có thể rao ra tất cả lời ca ngợi về Ngài?",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.417Z",
      "updatedAt": "2025-04-22T08:32:19.417Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "cf7fa963-4a1d-4b3c-9c68-584e22e72ba9",
      "number": 3,
      "content": "Phước hạnh thay cho những người duy trì công lý,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.430Z",
      "updatedAt": "2025-04-22T08:32:19.430Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "8f84c7f9-9ae4-4344-8eaa-9eb158b7fadc",
      "number": 3,
      "content": "Tức những người làm điều công chính ở mọi lúc mọi nơi!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.443Z",
      "updatedAt": "2025-04-22T08:32:19.443Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "c65bfd9a-c783-4fc9-af5a-5dcb7030a224",
      "number": 4,
      "content": "Chúa ôi, xin nhớ đến con khi Ngài ban ơn cho dân Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.456Z",
      "updatedAt": "2025-04-22T08:32:19.456Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "a5980633-1e8e-482d-8b1f-62c62f87125f",
      "number": 4,
      "content": "Xin đoái đến con khi Ngài ban ơn cứu rỗi của Ngài,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.470Z",
      "updatedAt": "2025-04-22T08:32:19.470Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "a2243528-c2ad-41d3-a180-2cd4533f7152",
      "number": 5,
      "content": "Ðể con được thấy sự thịnh vượng của người Ngài chọn,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.483Z",
      "updatedAt": "2025-04-22T08:32:19.483Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "55018e54-1ae8-451c-96ca-c24d03f34821",
      "number": 5,
      "content": "Ðược hân hoan trong niềm vui của người dân trong nước Ngài,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.497Z",
      "updatedAt": "2025-04-22T08:32:19.497Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "d41a1ae5-24ac-4aab-b3f3-2685f1089834",
      "number": 5,
      "content": "Và được vinh hiển với những người hưởng cơ nghiệp Ngài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.510Z",
      "updatedAt": "2025-04-22T08:32:19.510Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "1ad61a09-19e3-4c46-875b-8db1ee82b9c0",
      "number": 6,
      "content": "Giống như tổ tiên chúng con, chúng con đã phạm tội;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.523Z",
      "updatedAt": "2025-04-22T08:32:19.523Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "b4555ef9-c072-4796-9535-8bca6534debd",
      "number": 6,
      "content": "Chúng con đã làm những điều xấu xa tội lỗi;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.537Z",
      "updatedAt": "2025-04-22T08:32:19.537Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "873d061f-5c40-40e2-ad87-588303765d57",
      "number": 6,
      "content": "Chúng con đã hành động cách gian tà.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.550Z",
      "updatedAt": "2025-04-22T08:32:19.550Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "362402ed-2996-4d81-b10f-b9a58f7560ec",
      "number": 7,
      "content": "Khi ở Ai-cập tổ tiên chúng con đã không nghĩ đến những việc diệu kỳ của Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.564Z",
      "updatedAt": "2025-04-22T08:32:19.564Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "c813a309-ed17-4b0e-934c-217b49a560e9",
      "number": 7,
      "content": "Họ đã chẳng nhớ đến lòng thương xót lớn lao của Ngài;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.577Z",
      "updatedAt": "2025-04-22T08:32:19.577Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "b5098651-b307-466f-a55e-d3b344038f42",
      "number": 7,
      "content": "Họ đã chọc giận Ngài bên bờ biển, tức bên bờ Hồng Hải.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.591Z",
      "updatedAt": "2025-04-22T08:32:19.591Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "33f3fdd8-efaf-4cda-9eca-859aa631ade5",
      "number": 8,
      "content": "Dầu vậy vì cớ danh Ngài, Ngài đã cứu họ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.604Z",
      "updatedAt": "2025-04-22T08:32:19.604Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "fd79d219-2f57-4ddb-8e06-6080987b6632",
      "number": 8,
      "content": "Ðể quyền năng Ngài được biết đến khắp nơi.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.618Z",
      "updatedAt": "2025-04-22T08:32:19.618Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "59ae5e1a-9178-488d-920d-5d5c960bd8cc",
      "number": 9,
      "content": "Ngài quở trách Hồng Hải, và nó liền khô cạn;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.632Z",
      "updatedAt": "2025-04-22T08:32:19.632Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "cdba2fba-1bb7-478b-96d4-ceb91cc58cd8",
      "number": 9,
      "content": "Ngài dẫn họ qua biển sâu như qua một sa mạc khô khan.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.645Z",
      "updatedAt": "2025-04-22T08:32:19.645Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "3acb310f-d399-419f-a89b-5286b5c7c4eb",
      "number": 10,
      "content": "Ngài giải cứu họ khỏi tay những kẻ ghét họ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.658Z",
      "updatedAt": "2025-04-22T08:32:19.658Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "c8b53c5e-a6e7-487b-be8d-f68b50b2a2c3",
      "number": 10,
      "content": "Và cứu chuộc họ khỏi tay những kẻ thù của họ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.672Z",
      "updatedAt": "2025-04-22T08:32:19.672Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "829fe611-b837-409c-9c10-64c025b86ac3",
      "number": 11,
      "content": "Nước biển đã bao phủ những kẻ thù của họ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.685Z",
      "updatedAt": "2025-04-22T08:32:19.685Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "fd64157e-5fc4-4363-8600-66b4b6c0b052",
      "number": 11,
      "content": "Không chừa lại ai, dù chỉ một người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.698Z",
      "updatedAt": "2025-04-22T08:32:19.698Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "813233fc-2282-46bc-a882-24351f2ab099",
      "number": 12,
      "content": "Bấy giờ họ mới chịu tin lời Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.715Z",
      "updatedAt": "2025-04-22T08:32:19.715Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "f74ccf1b-e6a3-4826-9c18-e452d96f13d9",
      "number": 12,
      "content": "Họ mới cất tiếng chúc tụng Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.728Z",
      "updatedAt": "2025-04-22T08:32:19.728Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "ef9603eb-2794-4545-a4b2-31879dbdbb6d",
      "number": 13,
      "content": "Nhưng chẳng bao lâu họ lại quên các công việc Ngài;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.741Z",
      "updatedAt": "2025-04-22T08:32:19.741Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "3f46856b-26f4-4606-b428-ef845e856295",
      "number": 13,
      "content": "Họ chẳng màng đến việc chờ đợi để cầu hỏi ý Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.755Z",
      "updatedAt": "2025-04-22T08:32:19.755Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "7c55af70-4a38-4023-85be-e54e09e22599",
      "number": 14,
      "content": "Họ chiều theo dục vọng mình quá đỗi trong đồng hoang;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.768Z",
      "updatedAt": "2025-04-22T08:32:19.768Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "cf4a3cc1-508e-495b-84f7-3903abd2530a",
      "number": 14,
      "content": "Và dám thách đố cả đến Ðức Chúa Trời trong sa mạc.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.782Z",
      "updatedAt": "2025-04-22T08:32:19.782Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "50bfdfba-1d04-4769-a5c0-522b2afcd4c4",
      "number": 15,
      "content": "Ngài cho họ những gì họ đòi hỏi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.795Z",
      "updatedAt": "2025-04-22T08:32:19.795Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "7c394396-b0f0-4586-9964-4e06a81a4bd3",
      "number": 15,
      "content": "Nhưng cũng sai đến một ôn dịch chết người.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.808Z",
      "updatedAt": "2025-04-22T08:32:19.808Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "d7a7a9a2-3c42-483b-bc08-f4e43034232e",
      "number": 16,
      "content": "Trong doanh trại họ ganh tị với Môi-se,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.821Z",
      "updatedAt": "2025-04-22T08:32:19.821Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "90c16443-037b-4efb-9f60-3bef533780d8",
      "number": 16,
      "content": "Và với A-rôn người thánh của Chúa.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.834Z",
      "updatedAt": "2025-04-22T08:32:19.834Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "0dbde57a-bdd8-40db-b425-54c85af9eb54",
      "number": 17,
      "content": "Ðất mở miệng ra và nuốt chửng Ða-than,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.848Z",
      "updatedAt": "2025-04-22T08:32:19.848Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "af8f9cee-c567-45d6-94cf-875facfed366",
      "number": 17,
      "content": "Và ngoạm lấy đồng bọn của A-bi-ram.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.861Z",
      "updatedAt": "2025-04-22T08:32:19.861Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "8648143b-8de5-4766-85ed-c010cf209c53",
      "number": 18,
      "content": "Một ngọn lửa đã bùng lên giữa đám người phản loạn;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.875Z",
      "updatedAt": "2025-04-22T08:32:19.875Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "6d168b86-7a04-44e3-8328-b42be23f6066",
      "number": 18,
      "content": "Ngọn lửa hừng đã thiêu rụi những kẻ gian tà.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.888Z",
      "updatedAt": "2025-04-22T08:32:19.888Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "65ba588a-9358-4c20-9019-3d9c4b488afd",
      "number": 19,
      "content": "Họ đã đúc một tượng bò con ở Hô-rếp,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.901Z",
      "updatedAt": "2025-04-22T08:32:19.901Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "c0553844-4799-433c-981c-d80497a7e401",
      "number": 19,
      "content": "Rồi quỳ mọp xuống thờ lạy tượng vàng họ đúc.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.918Z",
      "updatedAt": "2025-04-22T08:32:19.918Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "dd132e71-8680-4fba-9b10-e7be648cc14a",
      "number": 20,
      "content": "Họ đã đánh đổi vinh hiển mình cho hình tượng một con bò,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.932Z",
      "updatedAt": "2025-04-22T08:32:19.932Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "d44826d4-9843-4818-97cb-80cc568c3fe6",
      "number": 20,
      "content": "Một con vật ăn cỏ.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.945Z",
      "updatedAt": "2025-04-22T08:32:19.945Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "cf836e62-df09-4956-b335-7165d39233e1",
      "number": 21,
      "content": "Họ quên rằng Ðức Chúa Trời là Ðấng Giải Cứu họ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.958Z",
      "updatedAt": "2025-04-22T08:32:19.958Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "7e40ea5b-a64a-4edf-a68d-3daab377d3c3",
      "number": 21,
      "content": "Ðấng làm những việc lớn lao ở Ai-cập,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.972Z",
      "updatedAt": "2025-04-22T08:32:19.972Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "306fecd1-a07a-494f-940e-7103a5ba61ae",
      "number": 22,
      "content": "Những việc lạ lùng trong xứ của Cham,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.985Z",
      "updatedAt": "2025-04-22T08:32:19.985Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "a324f94b-cbf4-4db5-9da3-feb2cd3c5b6c",
      "number": 22,
      "content": "Những việc kinh hoàng bên bờ Hồng Hải.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:19.998Z",
      "updatedAt": "2025-04-22T08:32:19.998Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "918c3bc4-d0fb-4a80-8308-27fdf4b584b5",
      "number": 23,
      "content": "Vì thế Ngài phán rằng Ngài sẽ tiêu diệt họ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.011Z",
      "updatedAt": "2025-04-22T08:32:20.011Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "c3525c4b-a50b-4108-b4f7-e18d04b1e6ef",
      "number": 23,
      "content": "May nhờ có Môi-se, người Ngài đã chọn, đứng mũi chịu sào,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.024Z",
      "updatedAt": "2025-04-22T08:32:20.024Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "f5456d8d-5038-4748-b7c5-aa3b9935eafe",
      "number": 23,
      "content": "Ðể van nài Ngài nguôi ngoai cơn giận mà không diệt họ đi.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.037Z",
      "updatedAt": "2025-04-22T08:32:20.037Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "66eabe31-237a-4786-9afa-7b8a1abfcd12",
      "number": 24,
      "content": "Kế đến họ khinh chê đất hứa;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.054Z",
      "updatedAt": "2025-04-22T08:32:20.054Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "6cd28f38-d0b1-4d10-b5bb-a7aea5251207",
      "number": 24,
      "content": "Họ không tin vào lời hứa của Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.067Z",
      "updatedAt": "2025-04-22T08:32:20.067Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "69987c39-6f9b-4a45-9332-feb7bbd2cc9a",
      "number": 25,
      "content": "Trong lều họ, họ lầm bầm oán trách;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.080Z",
      "updatedAt": "2025-04-22T08:32:20.080Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "e7944724-a1f1-4cf3-881d-46c778763d6b",
      "number": 25,
      "content": "Chẳng muốn nghe tiếng Chúa phán truyền.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.093Z",
      "updatedAt": "2025-04-22T08:32:20.093Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "bc07616a-9bf2-400f-be18-e5504ddc2878",
      "number": 26,
      "content": "Vì thế Ngài đã đưa tay lên và thề rằng,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.106Z",
      "updatedAt": "2025-04-22T08:32:20.106Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "79579f0d-6d15-4eb0-b59a-5fbdd0ff1497",
      "number": 26,
      "content": "Ngài sẽ cho họ bỏ thây trong sa mạc,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.119Z",
      "updatedAt": "2025-04-22T08:32:20.119Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "6d12b179-5a9a-4db1-aa60-a08f81672ee0",
      "number": 27,
      "content": "Rải con cháu họ ra trong các quốc gia,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.138Z",
      "updatedAt": "2025-04-22T08:32:20.138Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "5859186a-c9f6-4804-9f3a-ba6b6c2c1f19",
      "number": 27,
      "content": "Và làm cho họ tản lạc khắp nơi trong các nước.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.152Z",
      "updatedAt": "2025-04-22T08:32:20.152Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "af4c1835-e4e0-4da6-8384-504c29e089a0",
      "number": 28,
      "content": "Ðã thế họ còn thờ lạy Ba-anh Pê-ô;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.165Z",
      "updatedAt": "2025-04-22T08:32:20.165Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "f1d0b585-90a4-4f76-9378-feefd3d04743",
      "number": 28,
      "content": "Và ăn đồ cúng cho người chết.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.178Z",
      "updatedAt": "2025-04-22T08:32:20.178Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "02a58b37-6686-4269-82c4-a994c4878a70",
      "number": 29,
      "content": "Họ đã dùng các hành động mình để chọc giận Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.191Z",
      "updatedAt": "2025-04-22T08:32:20.191Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "6b00ac35-498a-4097-8981-8196a5c8ba1a",
      "number": 29,
      "content": "Nên ôn dịch đã bộc phát ở giữa họ.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.204Z",
      "updatedAt": "2025-04-22T08:32:20.204Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "ae5f77e0-1345-4bb8-afc9-99b97fa544e9",
      "number": 30,
      "content": "May nhờ có Phi-nê-a đứng dậy và cầu thay,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.217Z",
      "updatedAt": "2025-04-22T08:32:20.217Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "293615d9-467c-4a57-801b-d36a7c1e2f8e",
      "number": 30,
      "content": "Nên bệnh dịch mới ngừng ngay tại chỗ.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.231Z",
      "updatedAt": "2025-04-22T08:32:20.231Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "fb878ea6-cec1-42cf-9ded-dffabd45ca6f",
      "number": 31,
      "content": "Ðiều ấy được xem là công đức của ông ta,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.244Z",
      "updatedAt": "2025-04-22T08:32:20.244Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "9bd1daeb-f1c9-4bb9-86c1-9dca10dff9b5",
      "number": 31,
      "content": "Và được ghi nhớ từ đời này sang đời khác.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.258Z",
      "updatedAt": "2025-04-22T08:32:20.258Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "f3aca52a-36fc-4917-b2f7-c5ae06735db2",
      "number": 32,
      "content": "Họ đã chọc giận Ngài trong vụ nước ở Mê-ri-ba,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.271Z",
      "updatedAt": "2025-04-22T08:32:20.271Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "67984b52-a047-4a77-8960-607f67b71003",
      "number": 32,
      "content": "Cũng tại vì họ mà Môi-se đã mang lụy vào thân.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.284Z",
      "updatedAt": "2025-04-22T08:32:20.284Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "888e0f46-2201-4717-a1ce-a297198d13eb",
      "number": 33,
      "content": "Bởi họ đã làm cho tâm thần ông bực dọc,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.298Z",
      "updatedAt": "2025-04-22T08:32:20.298Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "be85ec95-1857-4b3b-8bc8-541cc513d5de",
      "number": 33,
      "content": "Nên môi ông đã nói những lời thiếu đắn đo suy nghĩ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.311Z",
      "updatedAt": "2025-04-22T08:32:20.311Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "f4850084-6462-4023-a892-03c5b139d03e",
      "number": 34,
      "content": "Họ cũng chẳng tiêu diệt các dân trong xứ,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.325Z",
      "updatedAt": "2025-04-22T08:32:20.325Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "e0987eee-643b-404a-823a-dc3f3cc12564",
      "number": 34,
      "content": "Ðể làm đúng theo mạng lịnh Chúa đã truyền,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.338Z",
      "updatedAt": "2025-04-22T08:32:20.338Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "9ca4a715-b218-4295-8625-2f2f91c8024b",
      "number": 35,
      "content": "Nhưng họ đã sống chung lộn với các dân tộc ấy,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.353Z",
      "updatedAt": "2025-04-22T08:32:20.353Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "0e2b5d33-456b-4759-a240-151043c7befe",
      "number": 35,
      "content": "Và học theo các thói tục của chúng.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.367Z",
      "updatedAt": "2025-04-22T08:32:20.367Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "caeabebc-1f0c-4f7c-871a-8a9c7403d2d4",
      "number": 36,
      "content": "Ðã thế họ còn thờ lạy các thần tượng của chúng;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.381Z",
      "updatedAt": "2025-04-22T08:32:20.381Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "ec84972a-ba24-4cee-b56e-1fb2216b627f",
      "number": 36,
      "content": "Các thần tượng ấy đã trở thành cái bẫy cho họ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.395Z",
      "updatedAt": "2025-04-22T08:32:20.395Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "a0c91414-2169-40fe-9164-5ecce30c460b",
      "number": 37,
      "content": "Thậm chí họ còn sát tế các con trai và các con gái mình,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.408Z",
      "updatedAt": "2025-04-22T08:32:20.408Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "ce582f03-dd30-486c-a16e-8b3deedc9a98",
      "number": 37,
      "content": "Ðể cúng tế cho các tà linh ma quỷ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.425Z",
      "updatedAt": "2025-04-22T08:32:20.425Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "36fd1a01-da45-4a6b-b9f3-6954b3acd0bf",
      "number": 38,
      "content": "Họ đã làm đổ máu vô tội ra,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.438Z",
      "updatedAt": "2025-04-22T08:32:20.438Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "dacf8adc-7c5f-4822-bdf9-2931d572b460",
      "number": 38,
      "content": "Tức máu của các con trai và các con gái họ,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.451Z",
      "updatedAt": "2025-04-22T08:32:20.451Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "70c9b893-83f0-4689-b4c8-6ffffe3fcb63",
      "number": 38,
      "content": "Những trẻ thơ đã bị làm vật tế thần cho các thần tượng xứ Ca-na-an;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.464Z",
      "updatedAt": "2025-04-22T08:32:20.464Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "83f50f6d-3aec-463b-b6a9-e8b54d4e51e2",
      "number": 38,
      "content": "Và đất đã bị ô nhiễm vì máu các trẻ thơ ấy.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.477Z",
      "updatedAt": "2025-04-22T08:32:20.477Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "35d74a16-7dc3-452c-b453-832c856729f9",
      "number": 39,
      "content": "Như vậy họ đã làm cho mình ra ô uế vì các hành động của họ;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.493Z",
      "updatedAt": "2025-04-22T08:32:20.493Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "b23c8e56-fc90-45ce-9507-69e06db81bca",
      "number": 39,
      "content": "Họ tự biến mình thành những đĩ điếm thuộc linh qua các hành vi của họ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.506Z",
      "updatedAt": "2025-04-22T08:32:20.506Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "fcc736a4-e2f1-47b3-a283-5b9777bb18c0",
      "number": 40,
      "content": "Vì thế cơn giận của Chúa đã nổi lên nghịch lại dân Ngài;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.519Z",
      "updatedAt": "2025-04-22T08:32:20.519Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "74c12aa1-e89f-496b-9e32-919454d0eab4",
      "number": 40,
      "content": "Và Ngài ghê tởm cơ nghiệp Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.539Z",
      "updatedAt": "2025-04-22T08:32:20.539Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "a8622e82-c40a-4765-abf8-67b862c8c1a1",
      "number": 41,
      "content": "Ngài phó họ vào tay các dân ngoại,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.552Z",
      "updatedAt": "2025-04-22T08:32:20.552Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "aac16375-6c9c-4087-9e17-ea024f63032a",
      "number": 41,
      "content": "Để cho những kẻ ghét họ cai trị họ.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.569Z",
      "updatedAt": "2025-04-22T08:32:20.569Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "d02fc664-9c41-487b-ba75-d35b60633e7c",
      "number": 42,
      "content": "Những kẻ thù của họ hà hiếp họ;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.582Z",
      "updatedAt": "2025-04-22T08:32:20.582Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "030bb20c-3752-41a2-a182-cdbbb378a228",
      "number": 42,
      "content": "Họ bị khuất phục dưới tay chúng.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.596Z",
      "updatedAt": "2025-04-22T08:32:20.596Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "b174dce4-7f66-46ca-8d34-eaed1904bf65",
      "number": 43,
      "content": "Dù nhiều lần Ngài đã giải cứu họ,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.615Z",
      "updatedAt": "2025-04-22T08:32:20.615Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "0aa3a959-878d-47ef-b27f-f4b2a9972783",
      "number": 43,
      "content": "Nhưng họ vẫn cứ bàn với nhau để tính kế phản bội Ngài,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.629Z",
      "updatedAt": "2025-04-22T08:32:20.629Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "b682c0f8-ddb3-406e-a797-a72e0526cb9c",
      "number": 43,
      "content": "Và cứ thế họ càng ngày càng lún sâu trong tội lỗi.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.642Z",
      "updatedAt": "2025-04-22T08:32:20.642Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "54eeabf7-17ff-41b1-83b9-37e66da4635d",
      "number": 44,
      "content": "Dầu vậy Ngài đã đoái xem cảnh khốn cùng của họ,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.655Z",
      "updatedAt": "2025-04-22T08:32:20.655Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "2a1127c7-5ace-4b68-95aa-8f5a2331a863",
      "number": 44,
      "content": "Khi Ngài nghe tiếng họ kêu cầu.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.668Z",
      "updatedAt": "2025-04-22T08:32:20.668Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "6b7512b1-82ca-4368-9433-cb46dafc4346",
      "number": 45,
      "content": "Vì cớ họ, Ngài nhớ lại giao ước Ngài;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.682Z",
      "updatedAt": "2025-04-22T08:32:20.682Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "5d05f2bd-4e51-4131-ad0b-1b4692a435bf",
      "number": 45,
      "content": "Và đoái thương họ vì lòng thương xót lớn lao của Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.696Z",
      "updatedAt": "2025-04-22T08:32:20.696Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "5af318f8-b708-402e-9391-086164404c63",
      "number": 46,
      "content": "Ngài làm cho họ được người ta thấy tội nghiệp;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.709Z",
      "updatedAt": "2025-04-22T08:32:20.709Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "4b02b7d2-0e2b-4135-a952-93dd6faa5819",
      "number": 46,
      "content": "Ngài khiến cho những kẻ đem họ đi lưu đày thương hại họ.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.723Z",
      "updatedAt": "2025-04-22T08:32:20.723Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "56d570c9-6f2a-4d98-83ba-d79b26b319f1",
      "number": 47,
      "content": "Lạy Chúa, Ðức Chúa Trời của chúng con, xin giải cứu chúng con,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.740Z",
      "updatedAt": "2025-04-22T08:32:20.740Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "f086a2c9-3a83-4706-b3b1-afb1ac8498c4",
      "number": 47,
      "content": "Xin đem chúng con từ giữa các dân ngoại trở về,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.753Z",
      "updatedAt": "2025-04-22T08:32:20.753Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "cdd6fcca-3f98-4d3c-9d01-064579575883",
      "number": 47,
      "content": "Ðể chúng con cảm tạ danh thánh Ngài,",
      "order": 2,
      "parNumber": 11,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.766Z",
      "updatedAt": "2025-04-22T08:32:20.766Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "a6ce9960-be7b-4014-a6fd-5e1972ac1364",
      "number": 47,
      "content": "Và tôn vinh ca ngợi Ngài.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.778Z",
      "updatedAt": "2025-04-22T08:32:20.778Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "c0162c17-7c46-4e77-a588-d5a8c8c3c299",
      "number": 48,
      "content": "Chúc tụng Chúa, Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.790Z",
      "updatedAt": "2025-04-22T08:32:20.790Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "15b39bcf-b1b3-4e97-a44c-870a1c846299",
      "number": 48,
      "content": "Từ đời đời cho đến đời đời.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.802Z",
      "updatedAt": "2025-04-22T08:32:20.802Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    },
    {
      "id": "119f364a-f4a4-45ac-8057-1b0e0d2a5631",
      "number": 48,
      "content": "Xin mọi người hãy nói, “A-men!”",
      "order": 2,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:20.814Z",
      "updatedAt": "2025-04-22T08:32:20.814Z",
      "chapterId": "4d22aca9-f27a-46c3-ae9c-8d56cc016464"
    }
  ],
