{
  "chapter": {
    "id": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a",
    "number": 15,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2015&version=BD2011",
    "createdAt": "2025-04-24T03:03:15.191Z",
    "updatedAt": "2025-04-24T03:03:15.191Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "1c270742-a05f-4a75-8283-c9388031f77b",
      "number": 1,
      "content": "Chúa ôi, ai sẽ được ở trong đền tạm Ngài?",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.496Z",
      "updatedAt": "2025-04-24T03:48:59.496Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "05ec0bc5-179f-4cd2-b826-e07818098208",
      "number": 1,
      "content": "Ai sẽ được cư ngụ trên núi thánh Ngài?",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.527Z",
      "updatedAt": "2025-04-24T03:48:59.527Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "71d6254c-17c0-42a7-a7e9-2910ee8f6a1e",
      "number": 2,
      "content": "Ấy là người sống ngay thẳng, làm những điều theo lẽ phải,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.539Z",
      "updatedAt": "2025-04-24T03:48:59.539Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "0877b662-1e80-4506-ae65-3513c0784dda",
      "number": 2,
      "content": "Và nói sự thật trong lòng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.550Z",
      "updatedAt": "2025-04-24T03:48:59.550Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "81f77993-f975-4d47-b33b-126d4bb7ca3b",
      "number": 3,
      "content": "Người ấy không dùng lưỡi mình vu khống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.560Z",
      "updatedAt": "2025-04-24T03:48:59.560Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "3abdd9fd-ed2e-41f1-a82b-b9502faf4f18",
      "number": 3,
      "content": "Không làm hại người khác,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.573Z",
      "updatedAt": "2025-04-24T03:48:59.573Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "79783652-911e-488d-8173-8f1691eea2ef",
      "number": 3,
      "content": "Và không gây sỉ nhục cho bạn bè.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.586Z",
      "updatedAt": "2025-04-24T03:48:59.586Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "8c4f368f-547b-4256-87a1-e9a922c74b38",
      "number": 4,
      "content": "Trong mắt người ấy, kẻ gian ác bị khinh rẻ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.596Z",
      "updatedAt": "2025-04-24T03:48:59.596Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "5cbcc5c1-58e7-452c-90da-aa121633b234",
      "number": 4,
      "content": "Nhưng người kính sợ Chúa được tôn trọng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.606Z",
      "updatedAt": "2025-04-24T03:48:59.606Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "5c89196c-d637-449f-b9af-fd95fde807d2",
      "number": 4,
      "content": "Người ấy giữ lời thệ hứa, dù bị thiệt hại cũng không nuốt lời.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.616Z",
      "updatedAt": "2025-04-24T03:48:59.616Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "66c20b7d-7f7d-4307-8077-74568d351925",
      "number": 5,
      "content": "Người ấy cho mượn tiền chẳng để lấy lời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.628Z",
      "updatedAt": "2025-04-24T03:48:59.628Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "2a450964-0309-466a-a4c4-4ae228cc526e",
      "number": 5,
      "content": "Người ấy cũng không nhận của hối lộ để hại người vô tội.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.640Z",
      "updatedAt": "2025-04-24T03:48:59.640Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    },
    {
      "id": "90f894b1-1fec-4510-b875-3e4a89ab47f0",
      "number": 5,
      "content": "Phàm ai làm những điều ấy sẽ không bao giờ bị rúng động.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:59.654Z",
      "updatedAt": "2025-04-24T03:48:59.654Z",
      "chapterId": "5edb2adb-10c0-4cf2-b1fd-cd7bfe49925a"
    }
  ],
