{
  "chapter": {
    "id": "f0685eff-5c25-4bcd-a914-aa85798324bb",
    "number": 51,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2051&version=BD2011",
    "createdAt": "2024-09-24T01:00:43.400Z",
    "updatedAt": "2024-09-24T01:00:43.400Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "264a462d-abdd-4bc4-a5b8-3fbe32637bdd",
      "number": 1,
      "content": "Ðức Chúa Trời ôi, xin thương xót con theo tình thương của Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:23.998Z",
      "updatedAt": "2024-09-24T01:59:23.998Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "0bcc927d-701a-4e14-b516-26eaaaf4c6d7",
      "number": 1,
      "content": "Xin xóa bỏ các vi phạm con theo lòng thương xót lớn lao của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.014Z",
      "updatedAt": "2024-09-24T01:59:24.014Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "61e969d1-3754-4aeb-ae2f-ec8917c4995b",
      "number": 2,
      "content": "Xin rửa sạch con khỏi mọi gian ác của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.017Z",
      "updatedAt": "2024-09-24T01:59:24.017Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "4fd933cc-c7c0-43ab-956b-08c4f9697e73",
      "number": 2,
      "content": "Xin tẩy sạch con khỏi mọi tội lỗi của con,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.019Z",
      "updatedAt": "2024-09-24T01:59:24.019Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "59a14908-f234-4feb-85ae-59f72f7b1b02",
      "number": 3,
      "content": "Vì con nhận biết các vi phạm con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.021Z",
      "updatedAt": "2024-09-24T01:59:24.021Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "e81a1da9-0214-41dc-90ee-045185bc0cae",
      "number": 3,
      "content": "Tội lỗi con hằng ở trước mặt con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.024Z",
      "updatedAt": "2024-09-24T01:59:24.024Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "ced705f8-89b6-425a-b3d9-1f114cbbbb74",
      "number": 4,
      "content": "Con đã phạm tội đối với Ngài và chỉ đối với Ngài mà thôi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.026Z",
      "updatedAt": "2024-09-24T01:59:24.026Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "bfc908bf-4223-422c-9ea5-6705c4399cb7",
      "number": 4,
      "content": "Con đã làm điều ác trước mặt Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.028Z",
      "updatedAt": "2024-09-24T01:59:24.028Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "8cf4c05f-c8f2-4ac4-a5ba-ece7d644b90f",
      "number": 4,
      "content": "Vì thế Ngài thật đúng khi Ngài phán xét;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.030Z",
      "updatedAt": "2024-09-24T01:59:24.030Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "98d1f5f4-7036-4ff8-94d4-2c091e63fd26",
      "number": 4,
      "content": "Khi Ngài tuyên án, con không thể trách Ngài được.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.034Z",
      "updatedAt": "2024-09-24T01:59:24.034Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "e0e43f8f-788f-477c-8935-bcc8b73d004d",
      "number": 5,
      "content": "Kìa, vừa mới sinh ra, con đã là người có tội;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.038Z",
      "updatedAt": "2024-09-24T01:59:24.038Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "765772ab-698d-488a-8011-04bdf69d9553",
      "number": 5,
      "content": "Con đã là tội nhân từ lúc con được thụ thai trong lòng mẹ con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.040Z",
      "updatedAt": "2024-09-24T01:59:24.040Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "c3f2af6c-063e-4e9e-bcfa-99c9d42db6eb",
      "number": 6,
      "content": "Kìa, Ngài muốn sự thật trong đáy lòng con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.042Z",
      "updatedAt": "2024-09-24T01:59:24.042Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "7924304a-9058-4822-b085-5304fd0e2ebd",
      "number": 6,
      "content": "Trong nơi sâu thẳm nhất của tâm hồn, Ngài dạy con biết lẽ khôn ngoan.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.044Z",
      "updatedAt": "2024-09-24T01:59:24.044Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "6c0adbda-abde-4ba8-af7c-201a23d12749",
      "number": 7,
      "content": "Xin dùng nhánh bài hương tẩy rửa con, để con được sạch;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.047Z",
      "updatedAt": "2024-09-24T01:59:24.047Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "1e54c9cf-c5f7-402f-9788-84dfcb8c38dc",
      "number": 7,
      "content": "Xin thanh tẩy con, để con trở nên trắng hơn tuyết.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.049Z",
      "updatedAt": "2024-09-24T01:59:24.049Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "1a9fb631-6fde-46e8-a357-fe1ca4699078",
      "number": 8,
      "content": "Xin cho con được hưởng niềm vui và niềm hoan hỉ của người được tha thứ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.052Z",
      "updatedAt": "2024-09-24T01:59:24.052Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "c113660a-1ed1-4c12-86e0-7d234ec0edb8",
      "number": 8,
      "content": "Ðể các xương con đã bị Ngài bẻ gãy được vui mừng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.054Z",
      "updatedAt": "2024-09-24T01:59:24.054Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "559d3388-7b93-4ed9-9be4-9d138e16d46b",
      "number": 9,
      "content": "Xin che mặt Ngài để khỏi nhìn thấy các tội lỗi con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.056Z",
      "updatedAt": "2024-09-24T01:59:24.056Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "3ecc2b8e-0a07-4e0b-8c3b-2e282c48e7f3",
      "number": 9,
      "content": "Xin xóa sạch mọi gian ác của con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.059Z",
      "updatedAt": "2024-09-24T01:59:24.059Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "7cae378f-a6a3-4929-bb08-db2ce0917d58",
      "number": 10,
      "content": "Ðức Chúa Trời ôi, xin dựng nên trong con một lòng trong sạch,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.062Z",
      "updatedAt": "2024-09-24T01:59:24.062Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "97a5d506-a5c5-46a0-969e-042a12a279d8",
      "number": 10,
      "content": "Và làm mới lại trong con một tâm linh ngay thẳng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.070Z",
      "updatedAt": "2024-09-24T01:59:24.070Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "85adb3ae-c1c3-4696-956e-dabc6bd94950",
      "number": 11,
      "content": "Xin đừng quăng con ra khỏi thánh nhan Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.076Z",
      "updatedAt": "2024-09-24T01:59:24.076Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "a82c6cad-3d5f-41bc-8a68-9ce11db481fd",
      "number": 11,
      "content": "Xin đừng cất Ðức Thánh Linh của Ngài khỏi con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.081Z",
      "updatedAt": "2024-09-24T01:59:24.081Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "498e889c-fb14-430c-8e8e-30056791e95a",
      "number": 12,
      "content": "Xin khôi phục cho con niềm vui của ơn cứu rỗi Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.087Z",
      "updatedAt": "2024-09-24T01:59:24.087Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "3942574f-9ae9-4844-bc5d-11f718287c4e",
      "number": 12,
      "content": "Và xin giữ con luôn có tinh thần đầu phục,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.092Z",
      "updatedAt": "2024-09-24T01:59:24.092Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "07974806-ce81-43b8-b934-08e4a03dc33a",
      "number": 13,
      "content": "Ðể con sẽ chỉ cho những ai vi phạm biết đường lối Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.099Z",
      "updatedAt": "2024-09-24T01:59:24.099Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "e61bc887-c061-42b4-8880-1f2624575313",
      "number": 13,
      "content": "Rồi những kẻ tội lỗi sẽ quay về với Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.106Z",
      "updatedAt": "2024-09-24T01:59:24.106Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "7843521b-59e7-4ab3-9b22-785ed7cf896c",
      "number": 14,
      "content": "Ðức Chúa Trời ôi, xin giải cứu con khỏi tội làm đổ máu,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.111Z",
      "updatedAt": "2024-09-24T01:59:24.111Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "e91055ec-7a19-460c-94bb-ba5197882ffa",
      "number": 14,
      "content": "Lạy Ðức Chúa Trời, Ðấng Giải Cứu của con,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.116Z",
      "updatedAt": "2024-09-24T01:59:24.116Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "dbd02dc4-5d70-4fca-a293-69731e4d30d7",
      "number": 14,
      "content": "Ðể lưỡi con sẽ vui mừng ca ngợi ơn cứu rỗi của Ngài.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.122Z",
      "updatedAt": "2024-09-24T01:59:24.122Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "bdda66f6-e8b3-46a1-8a4b-f320745ad78a",
      "number": 15,
      "content": "Chúa ôi, xin mở môi con,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.133Z",
      "updatedAt": "2024-09-24T01:59:24.133Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "9ecd56e7-0b99-4650-b41b-dcc92c09c2c6",
      "number": 15,
      "content": "Ðể miệng con truyền ra những lời ca tụng Ngài,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.140Z",
      "updatedAt": "2024-09-24T01:59:24.140Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "27f74f10-976d-4f1d-b2ea-38a5708e7c1d",
      "number": 16,
      "content": "Vì Ngài không vui thích của tế lễ,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.145Z",
      "updatedAt": "2024-09-24T01:59:24.145Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "b47f8b57-3b35-4534-9fcb-6939de1e095d",
      "number": 16,
      "content": "Bởi nếu Ngài thích, con đã dâng rồi;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.151Z",
      "updatedAt": "2024-09-24T01:59:24.151Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "3f634db8-cdf7-437a-9af1-c6a2b8b77b2b",
      "number": 16,
      "content": "Dù con dâng của lễ thiêu, Ngài cũng chẳng vui lòng.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.156Z",
      "updatedAt": "2024-09-24T01:59:24.156Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "ae73e637-d770-44b8-819c-a369c6ada15b",
      "number": 17,
      "content": "Của lễ đẹp lòng Ðức Chúa Trời là một tâm linh tan vỡ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.162Z",
      "updatedAt": "2024-09-24T01:59:24.162Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "2d42ad41-7fe7-40f3-890b-15d46269a238",
      "number": 17,
      "content": "Ðức Chúa Trời ôi, một tấm lòng tan vỡ và thống hối Ngài sẽ chẳng khinh thường.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.170Z",
      "updatedAt": "2024-09-24T01:59:24.170Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "45e4b42b-4cdc-4b6f-bae0-477bcffb69dd",
      "number": 18,
      "content": "Cầu xin Ngài làm điều tốt cho Si-ôn theo ý tốt của Ngài;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.175Z",
      "updatedAt": "2024-09-24T01:59:24.175Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "3acede33-0d69-49f5-ab7b-b1ccc3fe5014",
      "number": 18,
      "content": "Cầu xin Ngài dựng lại các tường thành của Giê-ru-sa-lem.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.180Z",
      "updatedAt": "2024-09-24T01:59:24.180Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "f99ad73a-94ce-4931-95e6-cbb53fd67131",
      "number": 19,
      "content": "Bấy giờ Ngài sẽ vui lòng về các của lễ dâng lên để được xưng công chính,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.186Z",
      "updatedAt": "2024-09-24T01:59:24.186Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "ab8f406a-a15a-4e44-9c4c-d2a1c697a1a7",
      "number": 19,
      "content": "Tức các của lễ thiêu và các của lễ toàn thiêu;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.192Z",
      "updatedAt": "2024-09-24T01:59:24.192Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    },
    {
      "id": "638a8df8-a2a2-4947-9480-d46c92b91059",
      "number": 19,
      "content": "Rồi các bò đực tơ sẽ được hiến dâng trên bàn thờ Ngài.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:24.197Z",
      "updatedAt": "2024-09-24T01:59:24.197Z",
      "chapterId": "f0685eff-5c25-4bcd-a914-aa85798324bb"
    }
  ],
