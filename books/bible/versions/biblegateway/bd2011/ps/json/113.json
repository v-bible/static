{
  "chapter": {
    "id": "6c162acd-d896-4d0f-a50b-3c2ce10c487e",
    "number": 113,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20113&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.745Z",
    "updatedAt": "2025-04-24T03:03:17.745Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "dc559c96-7689-415d-9798-06d94010acd0",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.580Z",
      "updatedAt": "2025-04-24T04:01:20.580Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "3cd4f81e-57e0-423d-8daf-ae7474dbd1be",
      "number": 1,
      "content": "Hỡi các tôi tớ Chúa, hãy ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.607Z",
      "updatedAt": "2025-04-24T04:01:20.607Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "84d68812-a0ad-47b0-9afa-39d41daed206",
      "number": 1,
      "content": "Hãy ca ngợi danh Chúa.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.617Z",
      "updatedAt": "2025-04-24T04:01:20.617Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "ad35c506-ac80-49fb-96a1-58aa8be1dcd8",
      "number": 2,
      "content": "Hãy chúc tụng danh Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.627Z",
      "updatedAt": "2025-04-24T04:01:20.627Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "160ddaba-add0-4efd-b21f-d4698919abb2",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.640Z",
      "updatedAt": "2025-04-24T04:01:20.640Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "ab89c745-d94d-4f17-acec-506379c3daf0",
      "number": 3,
      "content": "Từ bình minh cho đến hoàng hôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.651Z",
      "updatedAt": "2025-04-24T04:01:20.651Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "27b2ab4b-8d80-4123-a337-1afb39d567ad",
      "number": 3,
      "content": "Danh Chúa phải được tôn vinh ca ngợi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.662Z",
      "updatedAt": "2025-04-24T04:01:20.662Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "3d7c5dde-2dfd-49d7-b572-86ecb5adc6c6",
      "number": 4,
      "content": "Chúa là Ðấng siêu việt hơn tất cả các dân các nước;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.673Z",
      "updatedAt": "2025-04-24T04:01:20.673Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "3ed092a6-c154-4ed5-9992-e61d482871b7",
      "number": 4,
      "content": "Vinh quang Ngài trỗi cao hơn các tầng trời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.685Z",
      "updatedAt": "2025-04-24T04:01:20.685Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "f6a69b26-d0c9-4136-87c7-ed25a3d437e4",
      "number": 5,
      "content": "Có ai giống như Chúa, Ðức Chúa Trời chúng ta,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.698Z",
      "updatedAt": "2025-04-24T04:01:20.698Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "66c36095-407e-4644-8d5f-f9dfaefabc11",
      "number": 5,
      "content": "Ðấng ngự trên ngai cao cả,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.711Z",
      "updatedAt": "2025-04-24T04:01:20.711Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "5ad7a8ff-fbfd-43fe-9c26-61e5210aba95",
      "number": 6,
      "content": "Lại tự hạ mình xuống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.724Z",
      "updatedAt": "2025-04-24T04:01:20.724Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "6c0c27d9-814f-442b-a439-f8f4638eb74d",
      "number": 6,
      "content": "Ðể đoái xem mọi vật trong bầu trời và trên mặt đất chăng?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.737Z",
      "updatedAt": "2025-04-24T04:01:20.737Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "fb2aee6d-c165-4dbf-9313-9a6f8abce0d2",
      "number": 7,
      "content": "Ngài nâng đỡ kẻ nghèo khổ ra khỏi chốn bụi tro,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.750Z",
      "updatedAt": "2025-04-24T04:01:20.750Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "cfd1ef54-e9ec-428c-b399-d7e0701a4a68",
      "number": 7,
      "content": "Và nhấc đưa kẻ khó nghèo ra khỏi gò rác rến.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.763Z",
      "updatedAt": "2025-04-24T04:01:20.763Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "34423838-022e-436b-8aae-bc57b8090058",
      "number": 8,
      "content": "Ngài đặt họ ngồi ngang hàng với các bậc quan quyền,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.776Z",
      "updatedAt": "2025-04-24T04:01:20.776Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "3ade81ac-cfd9-4155-a0af-f8ef23b000de",
      "number": 8,
      "content": "Tức với những người quyền thế của con dân Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.789Z",
      "updatedAt": "2025-04-24T04:01:20.789Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "a5ef5dd1-0cfc-47b6-95c4-3e5ca72b596d",
      "number": 9,
      "content": "Ngài làm cho người đàn bà son sẻ sống quạnh quẽ trong nhà",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.802Z",
      "updatedAt": "2025-04-24T04:01:20.802Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "bb480f68-7de9-497c-80d7-9e41533b8eb7",
      "number": 9,
      "content": "Thành người mẹ hiền vui vẻ của một đàn con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.819Z",
      "updatedAt": "2025-04-24T04:01:20.819Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    },
    {
      "id": "f4e62322-0678-4352-8009-265147dbba2d",
      "number": 9,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:20.832Z",
      "updatedAt": "2025-04-24T04:01:20.832Z",
      "chapterId": "6c162acd-d896-4d0f-a50b-3c2ce10c487e"
    }
  ],
