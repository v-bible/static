{
  "chapter": {
    "id": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f",
    "number": 113,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20113&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.294Z",
    "updatedAt": "2025-04-22T07:37:48.294Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "075bf7f7-a095-4671-8e6e-fe03ae84fed6",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:10.886Z",
      "updatedAt": "2025-04-22T08:33:10.886Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "f4273c08-ab77-4538-ba64-f4023a090bf8",
      "number": 9,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.156Z",
      "updatedAt": "2025-04-22T08:33:11.156Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "475b5c43-7190-4927-9516-098c3f3eb6fd",
      "number": 1,
      "content": "Hỡi các tôi tớ Chúa, hãy ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:10.915Z",
      "updatedAt": "2025-04-22T08:33:10.915Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "69ad1ffd-d081-43a7-a94b-306af63effb1",
      "number": 1,
      "content": "Hãy ca ngợi danh Chúa.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:10.926Z",
      "updatedAt": "2025-04-22T08:33:10.926Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "1a0befed-b7b9-4bba-bd12-5983871f1b05",
      "number": 2,
      "content": "Hãy chúc tụng danh Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:10.939Z",
      "updatedAt": "2025-04-22T08:33:10.939Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "e494a712-8e10-42d4-b68d-65b994f854dc",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:10.952Z",
      "updatedAt": "2025-04-22T08:33:10.952Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "3e6583fa-bf43-4f4d-bfde-3348f8d5c15f",
      "number": 3,
      "content": "Từ bình minh cho đến hoàng hôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:10.966Z",
      "updatedAt": "2025-04-22T08:33:10.966Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "e60e3c73-6b3b-448d-880a-2b6834103813",
      "number": 3,
      "content": "Danh Chúa phải được tôn vinh ca ngợi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:10.979Z",
      "updatedAt": "2025-04-22T08:33:10.979Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "90916d7c-3fd3-4308-b9b8-f50b136821d4",
      "number": 4,
      "content": "Chúa là Ðấng siêu việt hơn tất cả các dân các nước;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:10.995Z",
      "updatedAt": "2025-04-22T08:33:10.995Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "07bde7bf-bc05-41b1-846c-1d6420af029e",
      "number": 4,
      "content": "Vinh quang Ngài trỗi cao hơn các tầng trời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.009Z",
      "updatedAt": "2025-04-22T08:33:11.009Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "82581879-022a-4ae2-82cb-657f876293e1",
      "number": 5,
      "content": "Có ai giống như Chúa, Ðức Chúa Trời chúng ta,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.022Z",
      "updatedAt": "2025-04-22T08:33:11.022Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "9c93f0bf-1c7d-4ad4-8352-22e2e6736582",
      "number": 5,
      "content": "Ðấng ngự trên ngai cao cả,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.035Z",
      "updatedAt": "2025-04-22T08:33:11.035Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "9524b7a2-ccec-4c8e-9dd7-b7c3233d0258",
      "number": 6,
      "content": "Lại tự hạ mình xuống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.048Z",
      "updatedAt": "2025-04-22T08:33:11.048Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "673e1e14-2452-47f9-9eaa-56078ffe471a",
      "number": 6,
      "content": "Ðể đoái xem mọi vật trong bầu trời và trên mặt đất chăng?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.061Z",
      "updatedAt": "2025-04-22T08:33:11.061Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "e46dab6c-adad-4648-a1c3-3f3ecf4701fb",
      "number": 7,
      "content": "Ngài nâng đỡ kẻ nghèo khổ ra khỏi chốn bụi tro,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.074Z",
      "updatedAt": "2025-04-22T08:33:11.074Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "668e2368-1ca3-4293-acd7-8c5c37e4bda9",
      "number": 7,
      "content": "Và nhấc đưa kẻ khó nghèo ra khỏi gò rác rến.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.087Z",
      "updatedAt": "2025-04-22T08:33:11.087Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "8fe4e57f-99b6-4358-a2d2-338a8fb947aa",
      "number": 8,
      "content": "Ngài đặt họ ngồi ngang hàng với các bậc quan quyền,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.103Z",
      "updatedAt": "2025-04-22T08:33:11.103Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "419faf57-26f5-4b36-a0e3-79802c2d363e",
      "number": 8,
      "content": "Tức với những người quyền thế của con dân Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.116Z",
      "updatedAt": "2025-04-22T08:33:11.116Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "664004ab-02f3-4a16-8cea-c8fbf253b866",
      "number": 9,
      "content": "Ngài làm cho người đàn bà son sẻ sống quạnh quẽ trong nhà",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.129Z",
      "updatedAt": "2025-04-22T08:33:11.129Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    },
    {
      "id": "f7e00eb3-4915-4e9b-bfa1-d9fb2b9c52aa",
      "number": 9,
      "content": "Thành người mẹ hiền vui vẻ của một đàn con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:11.142Z",
      "updatedAt": "2025-04-22T08:33:11.142Z",
      "chapterId": "f1b0ae6e-10f1-40b9-ad13-62d8f371331f"
    }
  ],
