{
  "chapter": {
    "id": "f1969444-a6e3-472d-9b0f-0ecccef21574",
    "number": 110,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20110&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.664Z",
    "updatedAt": "2025-04-24T03:03:17.664Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "0f52ca0a-97b3-4eaa-84f3-ffb2f9ddfaa0",
      "number": 1,
      "content": "Chúa phán với Chúa của tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.587Z",
      "updatedAt": "2025-04-24T04:01:00.587Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "ef31c440-bbc5-44b7-b6cb-6fce1643cd1e",
      "number": 1,
      "content": "“Con hãy ngồi bên phải Ta,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.608Z",
      "updatedAt": "2025-04-24T04:01:00.608Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "3ade034c-46da-44c0-acb7-2fa8d60c95df",
      "number": 1,
      "content": "Cho đến khi Ta đặt những kẻ thù của Con làm bệ chân Con.”",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.619Z",
      "updatedAt": "2025-04-24T04:01:00.619Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "3c611d9e-6eb4-43a4-bef5-11a17a0c131b",
      "number": 2,
      "content": "“Từ Si-ôn Chúa sẽ đưa vương trượng uy quyền của Con ra:",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.629Z",
      "updatedAt": "2025-04-24T04:01:00.629Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "d1236115-8d5a-44d5-bf96-d9568944eed0",
      "number": 2,
      "content": "Hãy trị vì những kẻ thù của Con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.640Z",
      "updatedAt": "2025-04-24T04:01:00.640Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "b46a2074-97a0-496e-9060-aebe13c9b1b6",
      "number": 3,
      "content": "Trong ngày Con biểu dương quyền lực, dân tộc Con sẽ hăng hái xung phong;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.652Z",
      "updatedAt": "2025-04-24T04:01:00.652Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "1097305f-6ff9-4b90-b460-13dc678ff8ff",
      "number": 3,
      "content": "Họ sẽ tập họp thành đội ngũ chỉnh tề khi hừng đông ló dạng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.666Z",
      "updatedAt": "2025-04-24T04:01:00.666Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "c7048a14-522b-4593-92d9-e807bf7c9c50",
      "number": 3,
      "content": "Những trai trẻ sẽ quy tụ quanh Con như sương phủ ban mai.”",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.679Z",
      "updatedAt": "2025-04-24T04:01:00.679Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "54521b10-8f0c-4302-b434-b8fb9acfe52f",
      "number": 4,
      "content": "Chúa đã thề và sẽ không đổi ý của Ngài rằng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.692Z",
      "updatedAt": "2025-04-24T04:01:00.692Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "feab57b2-0aab-4261-aa70-874b2b37d25b",
      "number": 4,
      "content": "“Con là tư tế đời đời theo Dòng Mên-chi-xê-đéc.”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.705Z",
      "updatedAt": "2025-04-24T04:01:00.705Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "967ebb5b-0489-4dd4-a688-bb8059401f6c",
      "number": 5,
      "content": "Chúa ở bên phải Con.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.718Z",
      "updatedAt": "2025-04-24T04:01:00.718Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "3cd8cd44-2867-4a38-9f22-2912a86b797e",
      "number": 5,
      "content": "Ngài sẽ đập tan các vua trong ngày Ngài nổi giận.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.731Z",
      "updatedAt": "2025-04-24T04:01:00.731Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "7f4e5dba-fd53-457d-b85e-3f421404a6a9",
      "number": 6,
      "content": "Ngài sẽ đoán phạt các dân;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.744Z",
      "updatedAt": "2025-04-24T04:01:00.744Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "151907d9-8b4b-40f2-a279-3636da977db8",
      "number": 6,
      "content": "Ngài sẽ làm cho thây những kẻ ác chất cao thành đống;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.758Z",
      "updatedAt": "2025-04-24T04:01:00.758Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "066c4156-1eda-42e3-b94f-70ff279160e4",
      "number": 6,
      "content": "Ngài sẽ đập tan các lãnh tụ của thế gian.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.771Z",
      "updatedAt": "2025-04-24T04:01:00.771Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "adca40f6-360b-42df-8be2-8d65dbd39445",
      "number": 7,
      "content": "Ngài sẽ uống nước suối bên đường,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.787Z",
      "updatedAt": "2025-04-24T04:01:00.787Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "99918887-b343-4113-90e8-2d1e3ed51291",
      "number": 7,
      "content": "Rồi Ngài sẽ ngẩng đầu lên cách hiên ngang đắc thắng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:00.800Z",
      "updatedAt": "2025-04-24T04:01:00.800Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    },
    {
      "id": "07e3db8f-2239-4ab1-90aa-9995dc46bdca",
      "number": 7,
      "content": "*(Theo mẫu tự Hê-bơ-rơ)*",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:01:00.817Z",
      "updatedAt": "2025-04-24T04:01:00.817Z",
      "chapterId": "f1969444-a6e3-472d-9b0f-0ecccef21574"
    }
  ],
