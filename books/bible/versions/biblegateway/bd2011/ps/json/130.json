{
  "chapter": {
    "id": "36d5d84c-e43e-474f-bdb0-272606b50af2",
    "number": 130,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20130&version=BD2011",
    "createdAt": "2025-04-24T03:03:18.176Z",
    "updatedAt": "2025-04-24T03:03:18.176Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "4d739abb-febd-4bef-a9a0-a9954616bd7c",
      "number": 1,
      "content": "Lạy Chúa, từ chốn vực sâu con kêu cầu Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:29.846Z",
      "updatedAt": "2025-04-24T04:03:29.846Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "42d4fd2d-1714-456f-af37-5edfe2397827",
      "number": 2,
      "content": "Chúa ôi, xin nghe tiếng cầu xin của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:29.874Z",
      "updatedAt": "2025-04-24T04:03:29.874Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "7ecc8ac9-bf60-41f3-af3f-f5f3ec39e7be",
      "number": 2,
      "content": "Xin lắng tai nghe lời cầu nguyện của con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:29.884Z",
      "updatedAt": "2025-04-24T04:03:29.884Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "4599207c-839f-421f-8848-c159d26cc06a",
      "number": 3,
      "content": "Lạy Chúa, nếu Ngài ghi nhớ mọi tội lỗi của chúng con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:29.895Z",
      "updatedAt": "2025-04-24T04:03:29.895Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "373a5493-da63-4b41-b234-8be7572fae9c",
      "number": 3,
      "content": "Chúa ôi, còn ai đứng nổi trước mặt Ngài?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:29.907Z",
      "updatedAt": "2025-04-24T04:03:29.907Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "a3973879-678d-4c63-9c80-5856feb33c71",
      "number": 4,
      "content": "Nhưng vì Ngài sẵn lòng ban ơn tha thứ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:29.920Z",
      "updatedAt": "2025-04-24T04:03:29.920Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "c5659133-554b-478f-895f-3a541364d581",
      "number": 4,
      "content": "Nên chúng con càng kính sợ Ngài hơn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:29.933Z",
      "updatedAt": "2025-04-24T04:03:29.933Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "ada3dd43-2b63-4b16-a81a-1363bdf6df6a",
      "number": 5,
      "content": "Tôi trông đợi Chúa, linh hồn tôi trông đợi Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:29.946Z",
      "updatedAt": "2025-04-24T04:03:29.946Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "3d505db9-45db-459a-9500-c8249d3b75c3",
      "number": 5,
      "content": "Tôi đặt trọn hy vọng nơi lời Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:29.960Z",
      "updatedAt": "2025-04-24T04:03:29.960Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "e1625274-c5e1-4192-b49a-282dae80a109",
      "number": 6,
      "content": "Linh hồn tôi trông đợi Chúa hơn người lính canh trông đợi sáng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:29.973Z",
      "updatedAt": "2025-04-24T04:03:29.973Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "67506395-8b8b-478e-9e74-61a5f66afec6",
      "number": 6,
      "content": "Thật hơn người lính canh trông đợi sáng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:29.992Z",
      "updatedAt": "2025-04-24T04:03:29.992Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "9fda9865-bc92-405b-8736-d09e8ff1be42",
      "number": 7,
      "content": "Hỡi I-sơ-ra-ên, hãy đặt trọn hy vọng nơi Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:30.005Z",
      "updatedAt": "2025-04-24T04:03:30.005Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "f569e2b6-e610-4ee1-a395-59e6bd3420b8",
      "number": 7,
      "content": "Vì nơi Chúa có tình thương;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:30.018Z",
      "updatedAt": "2025-04-24T04:03:30.018Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "b1d5892b-f33c-4f55-9a3e-7e6aa08f4613",
      "number": 7,
      "content": "Ngài ban ơn cứu chuộc cách dồi dào.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:30.031Z",
      "updatedAt": "2025-04-24T04:03:30.031Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    },
    {
      "id": "b4089697-fb06-4a70-9d67-94dd7e58ade3",
      "number": 8,
      "content": "Ngài sẽ cứu chuộc I-sơ-ra-ên khỏi mọi tội lỗi của họ.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:30.043Z",
      "updatedAt": "2025-04-24T04:03:30.043Z",
      "chapterId": "36d5d84c-e43e-474f-bdb0-272606b50af2"
    }
  ],
