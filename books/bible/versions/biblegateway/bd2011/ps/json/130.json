{
  "chapter": {
    "id": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f",
    "number": 130,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20130&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.760Z",
    "updatedAt": "2025-04-22T07:37:48.760Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "f15ce8f6-1bb9-4eb6-8f5a-2c09d84f9731",
      "number": 1,
      "content": "Lạy Chúa, từ chốn vực sâu con kêu cầu Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.295Z",
      "updatedAt": "2025-04-22T08:35:13.295Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "b676f308-8c2e-468e-9c82-106380120373",
      "number": 2,
      "content": "Chúa ôi, xin nghe tiếng cầu xin của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.329Z",
      "updatedAt": "2025-04-22T08:35:13.329Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "8716491e-a144-477a-95a5-5ea532d5ef7d",
      "number": 2,
      "content": "Xin lắng tai nghe lời cầu nguyện của con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.340Z",
      "updatedAt": "2025-04-22T08:35:13.340Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "9a21e854-e22a-42b9-9ad8-a59fc0585231",
      "number": 3,
      "content": "Lạy Chúa, nếu Ngài ghi nhớ mọi tội lỗi của chúng con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.350Z",
      "updatedAt": "2025-04-22T08:35:13.350Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "2d74e41b-385f-499e-90dc-5b097435c58a",
      "number": 3,
      "content": "Chúa ôi, còn ai đứng nổi trước mặt Ngài?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.361Z",
      "updatedAt": "2025-04-22T08:35:13.361Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "32bb3019-db62-47f9-b909-a35570fb052d",
      "number": 4,
      "content": "Nhưng vì Ngài sẵn lòng ban ơn tha thứ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.372Z",
      "updatedAt": "2025-04-22T08:35:13.372Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "7d8ad77a-0f06-4afc-84a7-d00eaf2b6a02",
      "number": 4,
      "content": "Nên chúng con càng kính sợ Ngài hơn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.382Z",
      "updatedAt": "2025-04-22T08:35:13.382Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "150307d3-7654-4380-8392-f186504f83d3",
      "number": 5,
      "content": "Tôi trông đợi Chúa, linh hồn tôi trông đợi Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.393Z",
      "updatedAt": "2025-04-22T08:35:13.393Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "776fe44f-d938-415d-8477-f99c8795e8fd",
      "number": 5,
      "content": "Tôi đặt trọn hy vọng nơi lời Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.404Z",
      "updatedAt": "2025-04-22T08:35:13.404Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "cc3764cc-56e0-4f9c-9701-be7cb1f4ba60",
      "number": 6,
      "content": "Linh hồn tôi trông đợi Chúa hơn người lính canh trông đợi sáng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.414Z",
      "updatedAt": "2025-04-22T08:35:13.414Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "764eb71c-2078-4f08-94fb-eb032ff8d78c",
      "number": 6,
      "content": "Thật hơn người lính canh trông đợi sáng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.425Z",
      "updatedAt": "2025-04-22T08:35:13.425Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "244a776e-d797-44d1-bca1-65c673c04bd0",
      "number": 7,
      "content": "Hỡi I-sơ-ra-ên, hãy đặt trọn hy vọng nơi Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.437Z",
      "updatedAt": "2025-04-22T08:35:13.437Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "76b0ead2-7800-467c-87a1-a3387411c938",
      "number": 7,
      "content": "Vì nơi Chúa có tình thương;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.450Z",
      "updatedAt": "2025-04-22T08:35:13.450Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "d10608af-8afa-4fed-be53-bd24852dbb55",
      "number": 7,
      "content": "Ngài ban ơn cứu chuộc cách dồi dào.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.463Z",
      "updatedAt": "2025-04-22T08:35:13.463Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    },
    {
      "id": "b0769873-6cbc-417a-b09a-236dc57f3224",
      "number": 8,
      "content": "Ngài sẽ cứu chuộc I-sơ-ra-ên khỏi mọi tội lỗi của họ.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:13.477Z",
      "updatedAt": "2025-04-22T08:35:13.477Z",
      "chapterId": "cf8e263f-3bf2-49fd-ae9a-ef4ec5efd67f"
    }
  ],
