{
  "chapter": {
    "id": "c1b3b793-2279-4784-a72e-1ca782857b9e",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%201&version=BD2011",
    "createdAt": "2025-04-24T03:03:14.822Z",
    "updatedAt": "2025-04-24T03:03:14.822Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "279af7a0-3558-46a5-8a7e-08b437e57561",
      "number": 1,
      "content": "Phước cho người không đi theo kế kẻ gian ác,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.112Z",
      "updatedAt": "2025-04-24T03:47:06.112Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "8aabb2bf-4ea7-43df-8286-237155946e8e",
      "number": 1,
      "content": "Không đứng chung đường với phường tội lỗi,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.160Z",
      "updatedAt": "2025-04-24T03:47:06.160Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "40de7e73-3ea6-4b90-a1d0-f69840ba61b5",
      "number": 1,
      "content": "Không ngồi cùng chỗ với đồ nhạo báng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.173Z",
      "updatedAt": "2025-04-24T03:47:06.173Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "54fe331b-86ed-4b08-a191-130dd16abd7a",
      "number": 2,
      "content": "Nhưng vui thích về luật pháp của Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.186Z",
      "updatedAt": "2025-04-24T03:47:06.186Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "ad0f2c6a-af85-4670-8ce7-927d6327da02",
      "number": 2,
      "content": "Và suy gẫm luật pháp ấy ngày và đêm.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.199Z",
      "updatedAt": "2025-04-24T03:47:06.199Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "258ff238-8d53-4d3c-a652-68446df02535",
      "number": 3,
      "content": "Người ấy sẽ như cây trồng gần dòng nước,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.212Z",
      "updatedAt": "2025-04-24T03:47:06.212Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "16da8216-58c5-4482-88ea-2da0ceda21f1",
      "number": 3,
      "content": "Cứ đến mùa được hoa quả đầy cành,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.226Z",
      "updatedAt": "2025-04-24T03:47:06.226Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "feae32d0-7c6d-4d58-8aa1-ecf1e17462e0",
      "number": 3,
      "content": "Lá vẫn xanh chứ chẳng hề héo úa;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.239Z",
      "updatedAt": "2025-04-24T03:47:06.239Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "b9c08783-c812-47cb-b775-ad088b5bf72e",
      "number": 3,
      "content": "Mọi sự người ấy làm đều sẽ thành công.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.253Z",
      "updatedAt": "2025-04-24T03:47:06.253Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "8f6862e1-9c73-46e4-9b53-3a46d81a2456",
      "number": 4,
      "content": "Những kẻ gian ác sẽ không được như vậy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.269Z",
      "updatedAt": "2025-04-24T03:47:06.269Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "51b811a4-eaa7-4abc-b0cc-f6d1413bba0d",
      "number": 4,
      "content": "Nhưng chúng chẳng khác gì rác rến bị gió đùa đi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.282Z",
      "updatedAt": "2025-04-24T03:47:06.282Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "091edc72-2d84-41fc-b353-a6b5144bf87d",
      "number": 5,
      "content": "Vì thế những kẻ gian ác sẽ chẳng đứng nổi trong ngày phán xét;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.299Z",
      "updatedAt": "2025-04-24T03:47:06.299Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "b57c7ffb-bda3-4070-b2b1-0c5a48835c78",
      "number": 5,
      "content": "Những kẻ tội lỗi sẽ không được vào hội người ngay lành;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.318Z",
      "updatedAt": "2025-04-24T03:47:06.318Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "e04442b9-9456-4cb1-86f6-6a7ad15704ea",
      "number": 6,
      "content": "Vì Chúa biết rõ đường lối người ngay lành,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.332Z",
      "updatedAt": "2025-04-24T03:47:06.332Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    },
    {
      "id": "6b1a88dd-1f63-4d60-a389-082652e4d563",
      "number": 6,
      "content": "Còn đường lối những kẻ gian ác sẽ dẫn đến diệt vong.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:47:06.345Z",
      "updatedAt": "2025-04-24T03:47:06.345Z",
      "chapterId": "c1b3b793-2279-4784-a72e-1ca782857b9e"
    }
  ],
