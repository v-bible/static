{
  "chapter": {
    "id": "6cf68c80-531b-4579-bfe0-354ce8b5894c",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%201&version=BD2011",
    "createdAt": "2025-04-22T07:37:45.406Z",
    "updatedAt": "2025-04-22T07:37:45.406Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "f7da2bf1-eaa1-4538-96cd-86b9b72e7b47",
      "number": 1,
      "content": "Phước cho người không đi theo kế kẻ gian ác,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.781Z",
      "updatedAt": "2025-04-22T08:19:37.781Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "142427e1-86d3-4236-bdad-7a1461e1e286",
      "number": 1,
      "content": "Không đứng chung đường với phường tội lỗi,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.829Z",
      "updatedAt": "2025-04-22T08:19:37.829Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "25c4883f-9446-4370-965f-49b721e9b45a",
      "number": 1,
      "content": "Không ngồi cùng chỗ với đồ nhạo báng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.844Z",
      "updatedAt": "2025-04-22T08:19:37.844Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "0a370c25-8cfb-44a9-911a-11ff2ffe70c7",
      "number": 2,
      "content": "Nhưng vui thích về luật pháp của Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.857Z",
      "updatedAt": "2025-04-22T08:19:37.857Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "92ca4c6a-ab4f-40b6-8d8b-9bff3334a109",
      "number": 2,
      "content": "Và suy gẫm luật pháp ấy ngày và đêm.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.873Z",
      "updatedAt": "2025-04-22T08:19:37.873Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "a0db7bd7-005f-43f6-92ea-14df6f1c574a",
      "number": 3,
      "content": "Người ấy sẽ như cây trồng gần dòng nước,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.887Z",
      "updatedAt": "2025-04-22T08:19:37.887Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "c567d1e2-223b-4e2a-91c4-b6c617d91f58",
      "number": 3,
      "content": "Cứ đến mùa được hoa quả đầy cành,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.900Z",
      "updatedAt": "2025-04-22T08:19:37.900Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "6bf24633-5972-4b9b-805b-7d1bbcc8f225",
      "number": 3,
      "content": "Lá vẫn xanh chứ chẳng hề héo úa;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.912Z",
      "updatedAt": "2025-04-22T08:19:37.912Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "b4524875-8961-4377-af77-1a3eb7c0d91a",
      "number": 3,
      "content": "Mọi sự người ấy làm đều sẽ thành công.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.924Z",
      "updatedAt": "2025-04-22T08:19:37.924Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "c4a768f2-8bc9-4459-8dca-76de455efce6",
      "number": 4,
      "content": "Những kẻ gian ác sẽ không được như vậy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.934Z",
      "updatedAt": "2025-04-22T08:19:37.934Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "bf9f6334-d8c2-46f2-8f64-c59777006873",
      "number": 4,
      "content": "Nhưng chúng chẳng khác gì rác rến bị gió đùa đi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.945Z",
      "updatedAt": "2025-04-22T08:19:37.945Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "a3da2620-23f8-4439-b72d-ae2244ace83d",
      "number": 5,
      "content": "Vì thế những kẻ gian ác sẽ chẳng đứng nổi trong ngày phán xét;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.956Z",
      "updatedAt": "2025-04-22T08:19:37.956Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "79188740-bfb3-4442-9541-958c9fb948ad",
      "number": 5,
      "content": "Những kẻ tội lỗi sẽ không được vào hội người ngay lành;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.967Z",
      "updatedAt": "2025-04-22T08:19:37.967Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "e0d33f48-6c73-42a9-aca9-15b955341718",
      "number": 6,
      "content": "Vì Chúa biết rõ đường lối người ngay lành,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.977Z",
      "updatedAt": "2025-04-22T08:19:37.977Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    },
    {
      "id": "012cee0d-3a0f-40a6-a54d-77ab9ac064ea",
      "number": 6,
      "content": "Còn đường lối những kẻ gian ác sẽ dẫn đến diệt vong.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:37.989Z",
      "updatedAt": "2025-04-22T08:19:37.989Z",
      "chapterId": "6cf68c80-531b-4579-bfe0-354ce8b5894c"
    }
  ],
