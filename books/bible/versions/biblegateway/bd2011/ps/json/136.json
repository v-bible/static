      "id": "e6d131d4-96de-4a4d-91de-e5e2145f732f",
      "number": 14,
      "content": "Ðấng đem dân I-sơ-ra-ên đi qua giữa biển ấy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.504Z",
      "updatedAt": "2025-04-22T08:35:53.504Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "ba8d3a74-0eab-4ad9-a0d2-bf6e4824f9b2",
      "number": 15,
      "content": "Nhưng Ngài lật đổ Pha-ra-ôn và đạo quân vua ấy xuống Hồng Hải,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.534Z",
      "updatedAt": "2025-04-22T08:35:53.534Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "3fb46f44-ebcd-42fd-a999-4bc57b9e81c3",
      "number": 16,
      "content": "Ðấng dẫn dắt dân Ngài đi qua đồng hoang,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 24,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.560Z",
      "updatedAt": "2025-04-22T08:35:53.560Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "5d4e554f-6f01-4522-96f4-f5c94ddedabc",
      "number": 17,
      "content": "Ðấng đánh bại các vua lớn,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 26,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.587Z",
      "updatedAt": "2025-04-22T08:35:53.587Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "f3b02ab1-d7fd-4d76-b1f7-d5acb4bf9cf5",
      "number": 18,
      "content": "Và tiêu diệt những vua mạnh,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 28,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.613Z",
      "updatedAt": "2025-04-22T08:35:53.613Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "d527ff38-a9af-44ac-aa4d-348b324dd73e",
      "number": 19,
      "content": "Ðó là Si-hôn vua dân A-mô-ri,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 30,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.640Z",
      "updatedAt": "2025-04-22T08:35:53.640Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "3f539635-67ae-4f3b-804d-2dcc0b84dd13",
      "number": 20,
      "content": "Và Óc vua Ba-san,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 32,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.666Z",
      "updatedAt": "2025-04-22T08:35:53.666Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "962d1ab8-296b-4d7f-98c2-10e2a2e9ed8e",
      "number": 21,
      "content": "Rồi ban xứ của họ để làm sản nghiệp,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 34,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.693Z",
      "updatedAt": "2025-04-22T08:35:53.693Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "9ecff844-da2e-48c5-9bab-256c4ab17998",
      "number": 22,
      "content": "Tức làm sản nghiệp cho I-sơ-ra-ên tôi tớ Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 36,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.720Z",
      "updatedAt": "2025-04-22T08:35:53.720Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "138b352f-351e-4a79-a5fc-62bccd03e7d7",
      "number": 23,
      "content": "Ðấng nhớ đến thân phận thấp hèn của chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.747Z",
      "updatedAt": "2025-04-22T08:35:53.747Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "c27f31f6-e5ad-46d6-bd57-7b53e6daaaaa",
      "number": 24,
      "content": "Và giải cứu chúng ta khỏi những kẻ thù của chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.777Z",
      "updatedAt": "2025-04-22T08:35:53.777Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "ad26f48b-2741-4822-88ca-e42371bd3f00",
      "number": 25,
      "content": "Ðấng ban thực phẩm cho mọi loài xác thịt,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.804Z",
      "updatedAt": "2025-04-22T08:35:53.804Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    },
    {
      "id": "386262c1-d329-4e67-a907-27f1e8a53e00",
      "number": 26,
      "content": "Hãy cảm tạ Ðức Chúa Trời của các tầng trời,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:53.830Z",
      "updatedAt": "2025-04-22T08:35:53.830Z",
      "chapterId": "09263bc6-5c17-4de0-97a7-e5c581b2e502"
    }
  ],
