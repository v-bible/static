{
  "chapter": {
    "id": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04",
    "number": 8,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%208&version=BD2011",
    "createdAt": "2025-04-24T03:03:15.004Z",
    "updatedAt": "2025-04-24T03:03:15.004Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "041c1ca1-005d-46b3-a718-3c6e23dfbed6",
      "number": 1,
      "content": "Lạy Chúa, Chúa của chúng con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.067Z",
      "updatedAt": "2025-04-24T03:48:04.067Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "c663febe-886d-4e7e-8ee0-d5ea1c38070a",
      "number": 1,
      "content": "Danh Ngài thật oai nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.100Z",
      "updatedAt": "2025-04-24T03:48:04.100Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "9e6fc21e-8b94-4732-92df-4e8596b43d2c",
      "number": 1,
      "content": "Ngài đã đặt vinh quang Ngài trên các tầng trời!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.113Z",
      "updatedAt": "2025-04-24T03:48:04.113Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "84deb9bc-0b3a-448e-9139-0d2aa31ec6cc",
      "number": 2,
      "content": "Từ miệng các trẻ thơ và các ấu nhi Ngài đã biểu lộ quyền năng, để quân chống lại Ngài thấy rõ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.126Z",
      "updatedAt": "2025-04-24T03:48:04.126Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "46e8c646-9240-4e30-8dff-9174fee72a80",
      "number": 2,
      "content": "Ðể kẻ thù và kẻ báo thù phải im hơi bỏ cuộc.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.139Z",
      "updatedAt": "2025-04-24T03:48:04.139Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "c95d63aa-ba05-46ee-ac12-28631d57e8bd",
      "number": 3,
      "content": "Khi con lặng ngắm cõi trời cao, công việc của ngón tay Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.151Z",
      "updatedAt": "2025-04-24T03:48:04.151Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "04742d47-740e-4a67-a7c3-11d9935e4600",
      "number": 3,
      "content": "Mặt trăng và các ngôi sao mà Ngài đã đặt trên không trung;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.164Z",
      "updatedAt": "2025-04-24T03:48:04.164Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "a7042391-a82b-4d1b-ac71-0e5f0fc6b5a8",
      "number": 4,
      "content": "Loài người là gì mà Ngài phải quan tâm đến?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.177Z",
      "updatedAt": "2025-04-24T03:48:04.177Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "2de4e59a-11bf-46d6-9230-58be3675bbad",
      "number": 4,
      "content": "Con của loài người là chi mà Ngài phải bận lòng?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.190Z",
      "updatedAt": "2025-04-24T03:48:04.190Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "0e3a5462-7fb3-4b13-a5c5-8992f33c203f",
      "number": 5,
      "content": "Ngài đã dựng nên loài người thấp hơn các vị thần một chút;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.203Z",
      "updatedAt": "2025-04-24T03:48:04.203Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "1462b377-2506-4434-94b3-eacc05883678",
      "number": 5,
      "content": "Ngài đội trên đầu loài người vinh hiển và tôn trọng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.228Z",
      "updatedAt": "2025-04-24T03:48:04.228Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "9fe7b7b9-66e4-46b7-832a-bbf106127eb8",
      "number": 6,
      "content": "Ngài cho loài người quản trị mọi vật do tay Ngài dựng nên;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.241Z",
      "updatedAt": "2025-04-24T03:48:04.241Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "22556b28-ed2b-4d30-9d00-cd18dbc2fa88",
      "number": 6,
      "content": "Ngài bắt muôn vật phục dưới chân loài người,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.254Z",
      "updatedAt": "2025-04-24T03:48:04.254Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "eb6c9a76-c529-497b-a66b-00a99fa74580",
      "number": 7,
      "content": "Tức tất cả chiên và bò,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.273Z",
      "updatedAt": "2025-04-24T03:48:04.273Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "95636725-bc3e-4eb1-8a10-4322001fad77",
      "number": 7,
      "content": "Cùng các thú vật ngoài đồng,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.283Z",
      "updatedAt": "2025-04-24T03:48:04.283Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "64355227-2191-472b-8d2d-dfcf4feb5945",
      "number": 8,
      "content": "Các loài chim trên trời và mọi loài cá dưới biển,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.295Z",
      "updatedAt": "2025-04-24T03:48:04.295Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "d53e9b2f-2bbc-4374-a92f-83fd6fa1e803",
      "number": 8,
      "content": "Tức mọi vật bơi lội trong các dòng nước biển.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.308Z",
      "updatedAt": "2025-04-24T03:48:04.308Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "46d845e1-338c-469d-a852-d4cce9e8564d",
      "number": 9,
      "content": "Lạy Chúa, Chúa của chúng con,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.321Z",
      "updatedAt": "2025-04-24T03:48:04.321Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    },
    {
      "id": "e29a788a-efa0-4896-8901-f6b9d50f2a81",
      "number": 9,
      "content": "Danh Ngài thật uy nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:04.333Z",
      "updatedAt": "2025-04-24T03:48:04.333Z",
      "chapterId": "d8a6e62a-d509-4976-a2aa-6fe20bbaed04"
    }
  ],
