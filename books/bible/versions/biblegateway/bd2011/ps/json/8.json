{
  "chapter": {
    "id": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88",
    "number": 8,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%208&version=BD2011",
    "createdAt": "2025-04-22T07:37:45.574Z",
    "updatedAt": "2025-04-22T07:37:45.574Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "ea2bd52c-9842-40a6-b340-57f34fc036b3",
      "number": 1,
      "content": "Lạy Chúa, Chúa của chúng con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.708Z",
      "updatedAt": "2025-04-22T08:20:26.708Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "db8fc450-e66d-41df-b617-181f23c6884b",
      "number": 9,
      "content": "Lạy Chúa, Chúa của chúng con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.956Z",
      "updatedAt": "2025-04-22T08:20:26.956Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "7c2509c0-efca-4347-bff7-3d20d5a794d1",
      "number": 1,
      "content": "Danh Ngài thật oai nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.743Z",
      "updatedAt": "2025-04-22T08:20:26.743Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "38fc9d82-5ff2-4544-b7d8-accfe32ce5ef",
      "number": 1,
      "content": "Ngài đã đặt vinh quang Ngài trên các tầng trời!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.754Z",
      "updatedAt": "2025-04-22T08:20:26.754Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "bd1a569d-e450-4e99-a3f9-d8d9d8ffa4b4",
      "number": 2,
      "content": "Từ miệng các trẻ thơ và các ấu nhi Ngài đã biểu lộ quyền năng, để quân chống lại Ngài thấy rõ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.767Z",
      "updatedAt": "2025-04-22T08:20:26.767Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "a6dcfffb-8546-4df9-b6cd-4ebe87098825",
      "number": 2,
      "content": "Ðể kẻ thù và kẻ báo thù phải im hơi bỏ cuộc.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.780Z",
      "updatedAt": "2025-04-22T08:20:26.780Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "c64b63f9-db06-4d40-b450-40ce8fbedf91",
      "number": 3,
      "content": "Khi con lặng ngắm cõi trời cao, công việc của ngón tay Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.793Z",
      "updatedAt": "2025-04-22T08:20:26.793Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "51501212-15fd-4400-8234-38e78cc238b8",
      "number": 3,
      "content": "Mặt trăng và các ngôi sao mà Ngài đã đặt trên không trung;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.810Z",
      "updatedAt": "2025-04-22T08:20:26.810Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "82b65fa5-ca04-4eb3-bfc8-e33887bb0a6c",
      "number": 4,
      "content": "Loài người là gì mà Ngài phải quan tâm đến?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.826Z",
      "updatedAt": "2025-04-22T08:20:26.826Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "c69ebc2d-bab6-45f7-88ab-b3b4f3540108",
      "number": 4,
      "content": "Con của loài người là chi mà Ngài phải bận lòng?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.844Z",
      "updatedAt": "2025-04-22T08:20:26.844Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "422864c0-eacc-4212-9ed1-0c1736241638",
      "number": 5,
      "content": "Ngài đã dựng nên loài người thấp hơn các vị thần một chút;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.857Z",
      "updatedAt": "2025-04-22T08:20:26.857Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "628d7432-5053-425d-be42-70fb8a17a218",
      "number": 5,
      "content": "Ngài đội trên đầu loài người vinh hiển và tôn trọng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.883Z",
      "updatedAt": "2025-04-22T08:20:26.883Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "dfb42456-da07-42e1-bcaf-0046126295a3",
      "number": 6,
      "content": "Ngài cho loài người quản trị mọi vật do tay Ngài dựng nên;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.893Z",
      "updatedAt": "2025-04-22T08:20:26.893Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "f08c95c0-a9bf-4246-8ab3-2eaee0dab422",
      "number": 6,
      "content": "Ngài bắt muôn vật phục dưới chân loài người,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.903Z",
      "updatedAt": "2025-04-22T08:20:26.903Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "e01db817-3d86-41e4-8689-10d31576c370",
      "number": 7,
      "content": "Tức tất cả chiên và bò,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.913Z",
      "updatedAt": "2025-04-22T08:20:26.913Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "91b7fa4d-0671-424a-97f2-f81d9f78d891",
      "number": 7,
      "content": "Cùng các thú vật ngoài đồng,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.926Z",
      "updatedAt": "2025-04-22T08:20:26.926Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "fe0e56cf-87de-42c8-a33b-83c4b89adf2c",
      "number": 8,
      "content": "Các loài chim trên trời và mọi loài cá dưới biển,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.937Z",
      "updatedAt": "2025-04-22T08:20:26.937Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "2d305f17-15e5-4dd4-9e46-d0cc14920230",
      "number": 8,
      "content": "Tức mọi vật bơi lội trong các dòng nước biển.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.946Z",
      "updatedAt": "2025-04-22T08:20:26.946Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    },
    {
      "id": "14eebddc-81ca-40e3-b2d5-858541f78ffe",
      "number": 9,
      "content": "Danh Ngài thật uy nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:20:26.967Z",
      "updatedAt": "2025-04-22T08:20:26.967Z",
      "chapterId": "9cd7afa9-c844-4c3f-8a8d-97d96f8fef88"
    }
  ],
