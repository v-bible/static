{
  "chapter": {
    "id": "632fd545-a066-4479-a0e1-a9677541a4ae",
    "number": 61,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2061&version=BD2011",
    "createdAt": "2025-04-22T07:37:46.974Z",
    "updatedAt": "2025-04-22T07:37:46.974Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "3474fcd4-c6ac-49de-a540-9cfb99173e9b",
      "number": 1,
      "content": "Ðức Chúa Trời ôi, xin nghe tiếng kêu cầu của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:50.861Z",
      "updatedAt": "2025-04-22T08:26:50.861Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "2a5e4af9-9cfe-478d-ad80-1e7c06de35f3",
      "number": 1,
      "content": "Xin lắng nghe lời cầu nguyện của con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:50.889Z",
      "updatedAt": "2025-04-22T08:26:50.889Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "13203e78-6e61-48cc-996d-ba1369d86427",
      "number": 2,
      "content": "Từ nơi xa xôi ở tận cùng trái đất con kêu cầu Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:50.900Z",
      "updatedAt": "2025-04-22T08:26:50.900Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "3db0bd40-161a-4f0d-ab93-997933c91aef",
      "number": 2,
      "content": "Lòng con bị choáng ngợp vì biết bao nỗi lo sợ,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:50.911Z",
      "updatedAt": "2025-04-22T08:26:50.911Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "848e28cc-7b02-402c-9af7-3261cec89a96",
      "number": 2,
      "content": "Xin dẫn đưa con đến một vầng đá cao lớn hơn con;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:50.923Z",
      "updatedAt": "2025-04-22T08:26:50.923Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "52ef08f0-1e7e-43d0-bf4a-bdd8b30c97fa",
      "number": 3,
      "content": "Vì Ngài là nơi ẩn náu của con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:50.936Z",
      "updatedAt": "2025-04-22T08:26:50.936Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "650b3d13-de45-4f16-8312-e80a3a689b12",
      "number": 3,
      "content": "Một pháo tháp vững chắc để con đối phó với quân thù.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:50.956Z",
      "updatedAt": "2025-04-22T08:26:50.956Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "80217246-699e-44d9-94c8-792454d372ce",
      "number": 4,
      "content": "Xin cho con ở trong nhà Ngài mãi mãi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:50.969Z",
      "updatedAt": "2025-04-22T08:26:50.969Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "ab7533a4-8a94-46b4-b4c8-407c6a393141",
      "number": 4,
      "content": "Xin cho con nương náu dưới sự che chở của bóng cánh Ngài. *(Sê-la)*",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:50.983Z",
      "updatedAt": "2025-04-22T08:26:50.983Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "9b454311-8340-4bb3-8d57-ffa712356fe8",
      "number": 5,
      "content": "Ðức Chúa Trời ôi, vì Ngài đã nghe lời hứa nguyện của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:50.995Z",
      "updatedAt": "2025-04-22T08:26:50.995Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "bc1420e2-a9e6-43e4-8ce1-cfc8e5922585",
      "number": 5,
      "content": "Ngài ban cho con cơ nghiệp dành cho những người kính sợ danh Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:51.009Z",
      "updatedAt": "2025-04-22T08:26:51.009Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "e0302ff0-ef25-4402-977d-6a6d80809174",
      "number": 6,
      "content": "Cầu xin Ngài gia tăng tuổi thọ cho vua;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:51.022Z",
      "updatedAt": "2025-04-22T08:26:51.022Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "d587829d-86e7-476c-ae07-12045d08fd24",
      "number": 6,
      "content": "Xin cho những năm của đời người kéo dài qua nhiều thế hệ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:51.035Z",
      "updatedAt": "2025-04-22T08:26:51.035Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "82185cb8-f6a3-4ced-a66d-466b2c22c939",
      "number": 7,
      "content": "Nguyện người trị vì trước mặt Ðức Chúa Trời mãi mãi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:51.048Z",
      "updatedAt": "2025-04-22T08:26:51.048Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "b7c2bc8a-c3fe-471b-b504-2db1a2c8828a",
      "number": 7,
      "content": "Cầu xin Ngài ban tình thương và đức thành tín bảo vệ người.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:51.061Z",
      "updatedAt": "2025-04-22T08:26:51.061Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "b84f77f0-a7d7-4b23-8d8b-171747b003ca",
      "number": 8,
      "content": "Như vậy con sẽ ca ngợi danh Ngài mãi mãi;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:51.074Z",
      "updatedAt": "2025-04-22T08:26:51.074Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    },
    {
      "id": "6b57e943-0243-4380-b43e-6beb6b3307a5",
      "number": 8,
      "content": "Con sẽ giữ trọn những gì con đã hứa nguyện mỗi ngày.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:26:51.087Z",
      "updatedAt": "2025-04-22T08:26:51.087Z",
      "chapterId": "632fd545-a066-4479-a0e1-a9677541a4ae"
    }
  ],
