{
  "chapter": {
    "id": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94",
    "number": 112,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20112&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.721Z",
    "updatedAt": "2025-04-24T03:03:17.721Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "2be40be8-22b0-4c5a-8f6f-fb184ab2589f",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.217Z",
      "updatedAt": "2025-04-24T04:01:14.217Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "593a4495-8f05-4a36-80c0-41411076e2bc",
      "number": 1,
      "content": "Phước cho người nào kính sợ Chúa,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.235Z",
      "updatedAt": "2025-04-24T04:01:14.235Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "0bd888d8-46f6-402b-8204-3f85a674454a",
      "number": 1,
      "content": "Người thích thú vô vàn các mạng lịnh Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.247Z",
      "updatedAt": "2025-04-24T04:01:14.247Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "89cd668d-bed1-4fd7-bcc4-92af9f198fab",
      "number": 2,
      "content": "Con cháu người ấy sẽ được cường thịnh trên đất.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.258Z",
      "updatedAt": "2025-04-24T04:01:14.258Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "99249a9d-ef86-4764-8cfc-34ef413ccaae",
      "number": 2,
      "content": "Dòng dõi người chính trực sẽ được phước.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.270Z",
      "updatedAt": "2025-04-24T04:01:14.270Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "267776de-2c13-4417-a2d8-c3bdf9c6c517",
      "number": 3,
      "content": "Giàu sang và phú quý sẽ ở trong nhà người ấy.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.283Z",
      "updatedAt": "2025-04-24T04:01:14.283Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "103d1ea3-33f9-42c0-8c6f-02e1efc053fa",
      "number": 3,
      "content": "Ðức công chính của người ấy sẽ còn lại đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.296Z",
      "updatedAt": "2025-04-24T04:01:14.296Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "fb6ae572-812c-4fcf-aa02-2e63fab254e1",
      "number": 4,
      "content": "Ánh sáng bừng lên trong tăm tối cho người chính trực,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.312Z",
      "updatedAt": "2025-04-24T04:01:14.312Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "1f57fe90-404a-44a7-b9b6-0e08a546a2d0",
      "number": 4,
      "content": "Tức cho người nhân từ, thương xót, và ngay lành;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.325Z",
      "updatedAt": "2025-04-24T04:01:14.325Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "3649b22a-11de-43d3-a338-17de92290982",
      "number": 5,
      "content": "Phước cho người hay làm ơn và cho vay bất vụ lợi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.338Z",
      "updatedAt": "2025-04-24T04:01:14.338Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "0a702557-21d8-4264-a977-5a1866ce6b1d",
      "number": 5,
      "content": "Một người điều hành công việc của mình theo lẽ phải.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.352Z",
      "updatedAt": "2025-04-24T04:01:14.352Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "15b45c60-be0e-437c-b732-09da488283dd",
      "number": 6,
      "content": "Chắc chắn người ấy sẽ không bao giờ bị lung lay.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.365Z",
      "updatedAt": "2025-04-24T04:01:14.365Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "95a6b89b-6835-424d-a811-d9ff5614d706",
      "number": 6,
      "content": "Người ngay lành sẽ được tưởng nhớ đến muôn đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.379Z",
      "updatedAt": "2025-04-24T04:01:14.379Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "4420f4c9-c32a-4d24-9a00-e2387a17f633",
      "number": 7,
      "content": "Người ấy sẽ không sợ các hung tin.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.393Z",
      "updatedAt": "2025-04-24T04:01:14.393Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "18286943-ba53-4f2a-be96-8246f4202290",
      "number": 7,
      "content": "Người ấy cứ vững lòng tin cậy Chúa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.406Z",
      "updatedAt": "2025-04-24T04:01:14.406Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "2d8ff6b2-81a7-4cbd-9dd8-11d916208c07",
      "number": 8,
      "content": "Người ấy luôn an tâm và chẳng sợ hãi gì,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.421Z",
      "updatedAt": "2025-04-24T04:01:14.421Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "cdd3fc80-24eb-4939-81cf-b55623bd56d8",
      "number": 8,
      "content": "Cho đến khi người ấy nhìn kẻ thù mình một cách đắc thắng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.435Z",
      "updatedAt": "2025-04-24T04:01:14.435Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "45549359-15ca-433e-9827-d2153f7fb2ce",
      "number": 9,
      "content": "Người ấy rải của mình ra giúp người nghèo khó.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.450Z",
      "updatedAt": "2025-04-24T04:01:14.450Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "440cf73b-0ca1-40b7-8268-1f5ed80cc04c",
      "number": 9,
      "content": "Ðức công chính của người ấy trường tồn mãi mãi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.461Z",
      "updatedAt": "2025-04-24T04:01:14.461Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "5d929e75-a833-4c7d-953d-b867e618f344",
      "number": 9,
      "content": "Sừng người ấy sẽ ngước cao trong vinh hiển.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.471Z",
      "updatedAt": "2025-04-24T04:01:14.471Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "f647c5c7-726f-4942-b772-c77fbee36458",
      "number": 10,
      "content": "Kẻ ác thấy thế bèn nổi điên tức giận.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.481Z",
      "updatedAt": "2025-04-24T04:01:14.481Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "3ec4634a-92c2-4f68-9394-be0fa822a281",
      "number": 10,
      "content": "Nó nghiến răng tức tối rồi bị tiêu tan.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.491Z",
      "updatedAt": "2025-04-24T04:01:14.491Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    },
    {
      "id": "597f1d61-938d-41ae-abc5-293be5fdefce",
      "number": 10,
      "content": "Ước vọng của kẻ ác sẽ hoàn toàn mất tiệt.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:14.501Z",
      "updatedAt": "2025-04-24T04:01:14.501Z",
      "chapterId": "8b4a7b8d-6bd8-4c9d-875f-b83f3a721e94"
    }
  ],
