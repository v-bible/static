{
  "chapter": {
    "id": "1c87ffd6-9957-4b76-b403-fa9eed49f37d",
    "number": 123,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20123&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.581Z",
    "updatedAt": "2025-04-22T07:37:48.581Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "342ed265-e283-496f-9b77-3b22f447a3b1",
      "number": 1,
      "content": "Lạy Ðấng ngự trên các tầng trời,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:24.691Z",
      "updatedAt": "2025-04-22T08:34:24.691Z",
      "chapterId": "1c87ffd6-9957-4b76-b403-fa9eed49f37d"
    },
    {
      "id": "ff5b1e9e-0f89-4390-b051-da864b1d358a",
      "number": 1,
      "content": "Con ngước mắt lên hướng về Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:24.724Z",
      "updatedAt": "2025-04-22T08:34:24.724Z",
      "chapterId": "1c87ffd6-9957-4b76-b403-fa9eed49f37d"
    },
    {
      "id": "9e416fb8-c990-4ab9-8926-5c926072942d",
      "number": 2,
      "content": "Kìa, như mắt các đầy tớ trông nơi tay ông chủ họ thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:24.738Z",
      "updatedAt": "2025-04-22T08:34:24.738Z",
      "chapterId": "1c87ffd6-9957-4b76-b403-fa9eed49f37d"
    },
    {
      "id": "0f81ef9a-bbbe-44f5-b37c-c01a18795cfe",
      "number": 2,
      "content": "Như mắt người tớ gái trông nơi tay bà chủ nàng thể nào,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:24.751Z",
      "updatedAt": "2025-04-22T08:34:24.751Z",
      "chapterId": "1c87ffd6-9957-4b76-b403-fa9eed49f37d"
    },
    {
      "id": "6d57e8ce-5daa-42f5-8136-4465cd8900a1",
      "number": 2,
      "content": "Mắt chúng con cũng vọng trông nơi Chúa, Ðức Chúa Trời chúng con, thể ấy,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:24.764Z",
      "updatedAt": "2025-04-22T08:34:24.764Z",
      "chapterId": "1c87ffd6-9957-4b76-b403-fa9eed49f37d"
    },
    {
      "id": "688b1784-02b7-4e8e-a92a-156149ff3dba",
      "number": 2,
      "content": "Cho đến khi Ngài bày tỏ ơn thương xót đối với chúng con.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:24.777Z",
      "updatedAt": "2025-04-22T08:34:24.777Z",
      "chapterId": "1c87ffd6-9957-4b76-b403-fa9eed49f37d"
    },
    {
      "id": "a7fd32db-9069-489e-b024-7bba7b084805",
      "number": 3,
      "content": "Xin thương xót chúng con, Chúa ôi, xin thương xót chúng con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:24.791Z",
      "updatedAt": "2025-04-22T08:34:24.791Z",
      "chapterId": "1c87ffd6-9957-4b76-b403-fa9eed49f37d"
    },
    {
      "id": "7ccd20e2-6694-4464-aaa0-cbcc3ca53c08",
      "number": 3,
      "content": "Vì chúng con bị khinh khi quá đỗi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:24.804Z",
      "updatedAt": "2025-04-22T08:34:24.804Z",
      "chapterId": "1c87ffd6-9957-4b76-b403-fa9eed49f37d"
    },
    {
      "id": "15c5f91e-f2b7-4faf-8b11-dcd6f05683f1",
      "number": 4,
      "content": "Linh hồn chúng con quá ngao ngán những lời nhạo báng của đám người kiêu ngạo,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:24.818Z",
      "updatedAt": "2025-04-22T08:34:24.818Z",
      "chapterId": "1c87ffd6-9957-4b76-b403-fa9eed49f37d"
    },
    {
      "id": "fda782b6-983d-49af-bd67-a5056d036eb1",
      "number": 4,
      "content": "Cùng với giọng khinh người của những kẻ kiêu căng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:24.832Z",
      "updatedAt": "2025-04-22T08:34:24.832Z",
      "chapterId": "1c87ffd6-9957-4b76-b403-fa9eed49f37d"
    }
  ],
