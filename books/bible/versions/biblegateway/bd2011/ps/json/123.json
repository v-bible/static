{
  "chapter": {
    "id": "89a85225-d79a-4c5b-aa3b-e80df5146f28",
    "number": 123,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20123&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.996Z",
    "updatedAt": "2025-04-24T03:03:17.996Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "8d1cdf95-336f-4e38-be10-6c05a592c632",
      "number": 1,
      "content": "Lạy Ðấng ngự trên các tầng trời,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:41.666Z",
      "updatedAt": "2025-04-24T04:02:41.666Z",
      "chapterId": "89a85225-d79a-4c5b-aa3b-e80df5146f28"
    },
    {
      "id": "44717b29-bd96-47b6-a988-07c74aa3942f",
      "number": 1,
      "content": "Con ngước mắt lên hướng về Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:41.697Z",
      "updatedAt": "2025-04-24T04:02:41.697Z",
      "chapterId": "89a85225-d79a-4c5b-aa3b-e80df5146f28"
    },
    {
      "id": "2f59411e-b6c8-442c-af13-dc36919ed525",
      "number": 2,
      "content": "Kìa, như mắt các đầy tớ trông nơi tay ông chủ họ thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:41.709Z",
      "updatedAt": "2025-04-24T04:02:41.709Z",
      "chapterId": "89a85225-d79a-4c5b-aa3b-e80df5146f28"
    },
    {
      "id": "d30eca93-0a36-4a7f-a8a3-e73d4df939d7",
      "number": 2,
      "content": "Như mắt người tớ gái trông nơi tay bà chủ nàng thể nào,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:41.720Z",
      "updatedAt": "2025-04-24T04:02:41.720Z",
      "chapterId": "89a85225-d79a-4c5b-aa3b-e80df5146f28"
    },
    {
      "id": "972b67dc-8772-44dc-bf71-839d5fa2d4fc",
      "number": 2,
      "content": "Mắt chúng con cũng vọng trông nơi Chúa, Ðức Chúa Trời chúng con, thể ấy,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:41.732Z",
      "updatedAt": "2025-04-24T04:02:41.732Z",
      "chapterId": "89a85225-d79a-4c5b-aa3b-e80df5146f28"
    },
    {
      "id": "49140ecc-7b6a-470d-936f-ed6f3b104035",
      "number": 2,
      "content": "Cho đến khi Ngài bày tỏ ơn thương xót đối với chúng con.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:41.744Z",
      "updatedAt": "2025-04-24T04:02:41.744Z",
      "chapterId": "89a85225-d79a-4c5b-aa3b-e80df5146f28"
    },
    {
      "id": "1fd51475-3e58-4720-a7e0-4a48c5a7e957",
      "number": 3,
      "content": "Xin thương xót chúng con, Chúa ôi, xin thương xót chúng con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:41.755Z",
      "updatedAt": "2025-04-24T04:02:41.755Z",
      "chapterId": "89a85225-d79a-4c5b-aa3b-e80df5146f28"
    },
    {
      "id": "f5507485-23ab-44cb-bdfe-bcc1f82cbd2f",
      "number": 3,
      "content": "Vì chúng con bị khinh khi quá đỗi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:41.773Z",
      "updatedAt": "2025-04-24T04:02:41.773Z",
      "chapterId": "89a85225-d79a-4c5b-aa3b-e80df5146f28"
    },
    {
      "id": "f986436f-238f-4209-950c-66c5e5038a48",
      "number": 4,
      "content": "Linh hồn chúng con quá ngao ngán những lời nhạo báng của đám người kiêu ngạo,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:41.785Z",
      "updatedAt": "2025-04-24T04:02:41.785Z",
      "chapterId": "89a85225-d79a-4c5b-aa3b-e80df5146f28"
    },
    {
      "id": "bb66ae47-0dbe-452a-8199-1a8da862b3d5",
      "number": 4,
      "content": "Cùng với giọng khinh người của những kẻ kiêu căng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:41.802Z",
      "updatedAt": "2025-04-24T04:02:41.802Z",
      "chapterId": "89a85225-d79a-4c5b-aa3b-e80df5146f28"
    }
  ],
