  "verses": [
    {
      "id": "92b977e8-20a3-4d11-887d-7bcbb2eb9c5e",
      "number": 1,
      "content": "Hỡi tất cả tôi tớ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:57.688Z",
      "updatedAt": "2025-04-24T04:03:57.688Z",
      "chapterId": "4a3b8597-f665-4801-9f42-756550cd2925"
    },
    {
      "id": "6e387d55-9519-48a8-93d7-40168e56787a",
      "number": 1,
      "content": "Những người phục vụ trong nhà Chúa ban đêm,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:57.716Z",
      "updatedAt": "2025-04-24T04:03:57.716Z",
      "chapterId": "4a3b8597-f665-4801-9f42-756550cd2925"
    },
    {
      "id": "d35dab5d-2262-4988-a161-b1340e2b4f88",
      "number": 1,
      "content": "Xin hãy chúc tụng Chúa.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:57.727Z",
      "updatedAt": "2025-04-24T04:03:57.727Z",
      "chapterId": "4a3b8597-f665-4801-9f42-756550cd2925"
    },
    {
      "id": "3d78b225-3180-4a8f-85b8-7297691e9378",
      "number": 2,
      "content": "Xin đưa cao đôi tay quý vị lên trong nơi thánh và chúc tụng Chúa.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:57.741Z",
      "updatedAt": "2025-04-24T04:03:57.741Z",
      "chapterId": "4a3b8597-f665-4801-9f42-756550cd2925"
    },
    {
      "id": "88ddef89-9441-4cfd-8d4f-48d9d7e2b846",
      "number": 3,
      "content": "Nguyện Chúa, Ðấng dựng nên trời và đất, ban phước cho quý vị từ Si-ôn.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:57.754Z",
      "updatedAt": "2025-04-24T04:03:57.754Z",
      "chapterId": "4a3b8597-f665-4801-9f42-756550cd2925"
    }
  ],
