  "verses": [
    {
      "id": "c2e17d89-4d37-4454-9038-2e494bf9a90f",
      "number": 1,
      "content": "Hỡi tất cả tôi tớ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:43.136Z",
      "updatedAt": "2024-09-24T02:11:43.136Z",
      "chapterId": "f4540c77-ab2d-432a-8ce6-51b9c8d427b6"
    },
    {
      "id": "b3487407-dd59-487f-b9ed-62c1fba204b1",
      "number": 1,
      "content": "Những người phục vụ trong nhà Chúa ban đêm,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:43.150Z",
      "updatedAt": "2024-09-24T02:11:43.150Z",
      "chapterId": "f4540c77-ab2d-432a-8ce6-51b9c8d427b6"
    },
    {
      "id": "60785fa5-8b9a-4e6d-84c9-308b73792558",
      "number": 1,
      "content": "Xin hãy chúc tụng Chúa.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:43.153Z",
      "updatedAt": "2024-09-24T02:11:43.153Z",
      "chapterId": "f4540c77-ab2d-432a-8ce6-51b9c8d427b6"
    },
    {
      "id": "6d3c4c8a-14f5-413f-be0f-afee0d72e526",
      "number": 2,
      "content": "Xin đưa cao đôi tay quý vị lên trong nơi thánh và chúc tụng Chúa.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:43.156Z",
      "updatedAt": "2024-09-24T02:11:43.156Z",
      "chapterId": "f4540c77-ab2d-432a-8ce6-51b9c8d427b6"
    },
    {
      "id": "8af7300d-0012-4b7b-8743-6b33aed4f6bc",
      "number": 3,
      "content": "Nguyện Chúa, Ðấng dựng nên trời và đất, ban phước cho quý vị từ Si-ôn.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:43.159Z",
      "updatedAt": "2024-09-24T02:11:43.159Z",
      "chapterId": "f4540c77-ab2d-432a-8ce6-51b9c8d427b6"
    }
  ],
