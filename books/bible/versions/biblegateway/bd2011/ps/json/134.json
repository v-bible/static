  "verses": [
    {
      "id": "d2e11df5-83a9-497a-a067-83b541822375",
      "number": 1,
      "content": "Hỡi tất cả tôi tớ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:40.224Z",
      "updatedAt": "2025-04-22T08:35:40.224Z",
      "chapterId": "7a9986d7-cc57-4038-8bb8-806073dce7c2"
    },
    {
      "id": "fdde7072-b622-49f1-b288-56af5eee243d",
      "number": 1,
      "content": "Những người phục vụ trong nhà Chúa ban đêm,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:40.249Z",
      "updatedAt": "2025-04-22T08:35:40.249Z",
      "chapterId": "7a9986d7-cc57-4038-8bb8-806073dce7c2"
    },
    {
      "id": "c5f74e48-f20e-4810-b8d0-5aadb0f45e41",
      "number": 1,
      "content": "Xin hãy chúc tụng Chúa.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:40.262Z",
      "updatedAt": "2025-04-22T08:35:40.262Z",
      "chapterId": "7a9986d7-cc57-4038-8bb8-806073dce7c2"
    },
    {
      "id": "59bd9896-2125-4ff4-97a4-730795d11140",
      "number": 2,
      "content": "Xin đưa cao đôi tay quý vị lên trong nơi thánh và chúc tụng Chúa.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:40.272Z",
      "updatedAt": "2025-04-22T08:35:40.272Z",
      "chapterId": "7a9986d7-cc57-4038-8bb8-806073dce7c2"
    },
    {
      "id": "5f1d0f72-de9e-43d6-8a77-721b71feaccb",
      "number": 3,
      "content": "Nguyện Chúa, Ðấng dựng nên trời và đất, ban phước cho quý vị từ Si-ôn.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:40.282Z",
      "updatedAt": "2025-04-22T08:35:40.282Z",
      "chapterId": "7a9986d7-cc57-4038-8bb8-806073dce7c2"
    }
  ],
