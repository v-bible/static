{
  "chapter": {
    "id": "e11d3bb7-2b56-4b70-8dc7-f326d6806789",
    "number": 146,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20146&version=BD2011",
    "createdAt": "2025-04-24T03:03:18.592Z",
    "updatedAt": "2025-04-24T03:03:18.592Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "55b7bd3f-d27e-4e82-8284-9d6835d04e2b",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.747Z",
      "updatedAt": "2025-04-24T04:05:23.747Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "ac5e4bde-41ab-4880-94c4-59427b53269a",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.774Z",
      "updatedAt": "2025-04-24T04:05:23.774Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "db9e8761-23e3-49bc-983e-e716873a7649",
      "number": 2,
      "content": "Tôi sẽ ca ngợi Chúa trọn đời mình;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.784Z",
      "updatedAt": "2025-04-24T04:05:23.784Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "f1aefda3-5322-48e2-94f2-92d3b02f7583",
      "number": 2,
      "content": "Hễ tôi còn sống bao lâu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.796Z",
      "updatedAt": "2025-04-24T04:05:23.796Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "2981b4de-ee52-45bc-8b4b-e995d014d5ef",
      "number": 2,
      "content": "Tôi sẽ ca hát tôn ngợi Ðức Chúa Trời của tôi bấy lâu.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.809Z",
      "updatedAt": "2025-04-24T04:05:23.809Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "83b8a6fe-71ba-4851-aa13-69bfbfee0cbb",
      "number": 3,
      "content": "Ðừng để lòng tin cậy vào những người quyền thế;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.826Z",
      "updatedAt": "2025-04-24T04:05:23.826Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "d0e6731d-9a5e-4316-be32-fe7ff18e48a9",
      "number": 3,
      "content": "Ðừng tin cậy loài người là những kẻ không đủ khả năng cứu giúp.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.843Z",
      "updatedAt": "2025-04-24T04:05:23.843Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "5062d9da-c21d-46bb-8544-b52f02a2a4ea",
      "number": 4,
      "content": "Hơi thở tắt đi, thân xác nó liền trở về cát bụi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.856Z",
      "updatedAt": "2025-04-24T04:05:23.856Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "c621aeae-81c7-42d4-943a-ff2d822f8e25",
      "number": 4,
      "content": "Trong chính ngày đó các toan tính của nó đều tiêu tan.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.869Z",
      "updatedAt": "2025-04-24T04:05:23.869Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "98855d97-ad2c-42ed-8ff4-1e67a4d0c632",
      "number": 5,
      "content": "Phước cho người được Ðức Chúa Trời của Gia-cốp giúp đỡ mình,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.882Z",
      "updatedAt": "2025-04-24T04:05:23.882Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "2720dada-5736-45f4-bc8d-525e67d93879",
      "number": 5,
      "content": "Người đặt hy vọng nơi Chúa, Ðức Chúa Trời mình.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.896Z",
      "updatedAt": "2025-04-24T04:05:23.896Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "f2a7beb8-0dfc-4ab6-ba7b-f778b76b4397",
      "number": 6,
      "content": "Ngài là Ðấng dựng nên trời và đất,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.909Z",
      "updatedAt": "2025-04-24T04:05:23.909Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "7cca5a40-ef15-4255-88ba-be47580662ca",
      "number": 6,
      "content": "Biển và mọi vật trong đó.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.922Z",
      "updatedAt": "2025-04-24T04:05:23.922Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "0d098627-9554-4f69-9707-601fdf5e99c0",
      "number": 6,
      "content": "Ngài giữ sự thành tín đến đời đời.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.935Z",
      "updatedAt": "2025-04-24T04:05:23.935Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "dfd09d1c-d4d1-48a7-accf-e8bb91dd8e41",
      "number": 7,
      "content": "Ngài thực thi công lý cho người bị áp bức;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.949Z",
      "updatedAt": "2025-04-24T04:05:23.949Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "1cab36c7-9967-42b4-9979-3a213b58d23b",
      "number": 7,
      "content": "Ngài ban bánh cho người đói.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.962Z",
      "updatedAt": "2025-04-24T04:05:23.962Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "dbe870e9-6062-438b-afe7-6c0856d69d15",
      "number": 7,
      "content": "Chúa giải thoát người bị giam cầm.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.975Z",
      "updatedAt": "2025-04-24T04:05:23.975Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "cd8d263d-aefb-49ca-a03d-7ef50c13458b",
      "number": 8,
      "content": "Chúa mở mắt người bị mù;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:23.988Z",
      "updatedAt": "2025-04-24T04:05:23.988Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "8ed1a572-d92d-40a4-a99a-6694ecb1c358",
      "number": 8,
      "content": "Chúa đỡ dậy những ai bị bắt phải sấp mình;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:24.002Z",
      "updatedAt": "2025-04-24T04:05:24.002Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "d4847615-b5f5-4b82-a11a-7390077b4a7d",
      "number": 8,
      "content": "Chúa yêu mến người ngay lành;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:24.015Z",
      "updatedAt": "2025-04-24T04:05:24.015Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "5f9925cc-2b6e-4882-a1da-9dfd779e7112",
      "number": 9,
      "content": "Chúa bảo vệ những kiều dân;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:24.029Z",
      "updatedAt": "2025-04-24T04:05:24.029Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "dbd2673c-e458-4660-b94c-0832aceeb83b",
      "number": 9,
      "content": "Ngài nâng đỡ cô nhi và quả phụ;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:24.042Z",
      "updatedAt": "2025-04-24T04:05:24.042Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "5adcbf45-025e-4b37-b618-cd2da9f049e4",
      "number": 9,
      "content": "Nhưng Ngài phá hỏng đường lối của những kẻ ác.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:24.055Z",
      "updatedAt": "2025-04-24T04:05:24.055Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "4db6da08-a4e5-4a45-bba5-27fb125a51b8",
      "number": 10,
      "content": "Chúa sẽ trị vì đời đời;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:24.072Z",
      "updatedAt": "2025-04-24T04:05:24.072Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "eb97ce5d-5c74-4c79-8a63-fc3b04e88d08",
      "number": 10,
      "content": "Hỡi Si-ôn, Ðức Chúa Trời của ngươi sẽ trị vì đời đời.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:24.085Z",
      "updatedAt": "2025-04-24T04:05:24.085Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    },
    {
      "id": "85eb05c9-dfe4-4ac6-aa1d-8aca8ca3cd37",
      "number": 10,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:24.098Z",
      "updatedAt": "2025-04-24T04:05:24.098Z",
      "chapterId": "e11d3bb7-2b56-4b70-8dc7-f326d6806789"
    }
  ],
