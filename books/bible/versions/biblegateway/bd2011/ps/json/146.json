{
  "chapter": {
    "id": "8fee083e-eaa1-49c5-8c27-acd608e59a33",
    "number": 146,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20146&version=BD2011",
    "createdAt": "2025-04-22T07:37:49.194Z",
    "updatedAt": "2025-04-22T07:37:49.194Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "7b6cc18b-3b55-4012-a837-1b777176e411",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.645Z",
      "updatedAt": "2025-04-22T08:37:02.645Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "3647ad26-d296-482b-bf7b-ecf82520846c",
      "number": 10,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.950Z",
      "updatedAt": "2025-04-22T08:37:02.950Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "3b351af8-367e-4d94-8d19-b3b0c1553d15",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.680Z",
      "updatedAt": "2025-04-22T08:37:02.680Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "7100e392-0dd7-459b-9cc3-42490c277b3b",
      "number": 2,
      "content": "Tôi sẽ ca ngợi Chúa trọn đời mình;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.692Z",
      "updatedAt": "2025-04-22T08:37:02.692Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "9b09489b-45eb-4414-b051-151c73c201d6",
      "number": 2,
      "content": "Hễ tôi còn sống bao lâu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.702Z",
      "updatedAt": "2025-04-22T08:37:02.702Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "c47a8751-5696-46f1-bfd5-041b578f4440",
      "number": 2,
      "content": "Tôi sẽ ca hát tôn ngợi Ðức Chúa Trời của tôi bấy lâu.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.712Z",
      "updatedAt": "2025-04-22T08:37:02.712Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "5f5814e9-d57c-4210-92f7-e5c1472e24f0",
      "number": 3,
      "content": "Ðừng để lòng tin cậy vào những người quyền thế;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.724Z",
      "updatedAt": "2025-04-22T08:37:02.724Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "85c38054-4864-4987-9b65-70fbb5c92d66",
      "number": 3,
      "content": "Ðừng tin cậy loài người là những kẻ không đủ khả năng cứu giúp.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.739Z",
      "updatedAt": "2025-04-22T08:37:02.739Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "bca2c8ff-0f79-4d24-bbe3-f04a00152a2f",
      "number": 4,
      "content": "Hơi thở tắt đi, thân xác nó liền trở về cát bụi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.752Z",
      "updatedAt": "2025-04-22T08:37:02.752Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "7568b1f2-af7c-4cf8-8039-a86145ef7cd1",
      "number": 4,
      "content": "Trong chính ngày đó các toan tính của nó đều tiêu tan.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.762Z",
      "updatedAt": "2025-04-22T08:37:02.762Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "b46e2c7a-06ea-4ee5-be3b-aa8f0b96029b",
      "number": 5,
      "content": "Phước cho người được Ðức Chúa Trời của Gia-cốp giúp đỡ mình,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.772Z",
      "updatedAt": "2025-04-22T08:37:02.772Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "52088165-9113-4736-be19-0ec672db5922",
      "number": 5,
      "content": "Người đặt hy vọng nơi Chúa, Ðức Chúa Trời mình.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.783Z",
      "updatedAt": "2025-04-22T08:37:02.783Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "67a4b17a-1635-42c4-bbb5-b13b9a115440",
      "number": 6,
      "content": "Ngài là Ðấng dựng nên trời và đất,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.793Z",
      "updatedAt": "2025-04-22T08:37:02.793Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "f119bc76-bfa0-41ab-b67e-307bb54d1b6c",
      "number": 6,
      "content": "Biển và mọi vật trong đó.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.803Z",
      "updatedAt": "2025-04-22T08:37:02.803Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "4c775a7d-9d68-439d-aad0-1e9a206d969d",
      "number": 6,
      "content": "Ngài giữ sự thành tín đến đời đời.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.816Z",
      "updatedAt": "2025-04-22T08:37:02.816Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "0269050f-4103-459c-baf7-41b6461bb527",
      "number": 7,
      "content": "Ngài thực thi công lý cho người bị áp bức;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.827Z",
      "updatedAt": "2025-04-22T08:37:02.827Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "0fdea230-602e-49bf-8d55-e6523022caf5",
      "number": 7,
      "content": "Ngài ban bánh cho người đói.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.837Z",
      "updatedAt": "2025-04-22T08:37:02.837Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "5809a54d-54f1-460f-a329-e3210d704224",
      "number": 7,
      "content": "Chúa giải thoát người bị giam cầm.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.851Z",
      "updatedAt": "2025-04-22T08:37:02.851Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "83bb9223-aa0b-45fb-a00b-b45d99737728",
      "number": 8,
      "content": "Chúa mở mắt người bị mù;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.861Z",
      "updatedAt": "2025-04-22T08:37:02.861Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "513eb4e3-6573-4d7b-a898-99eaf9ddffd9",
      "number": 8,
      "content": "Chúa đỡ dậy những ai bị bắt phải sấp mình;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.874Z",
      "updatedAt": "2025-04-22T08:37:02.874Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "0eb9a8e8-34fd-46eb-a270-cc520be7ea74",
      "number": 8,
      "content": "Chúa yêu mến người ngay lành;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.889Z",
      "updatedAt": "2025-04-22T08:37:02.889Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "db81c1c2-04d9-4998-8403-1e89fc127744",
      "number": 9,
      "content": "Chúa bảo vệ những kiều dân;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.900Z",
      "updatedAt": "2025-04-22T08:37:02.900Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "a4aae5d9-ce1b-4544-95fa-830cbf9aa785",
      "number": 9,
      "content": "Ngài nâng đỡ cô nhi và quả phụ;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.910Z",
      "updatedAt": "2025-04-22T08:37:02.910Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "7b229eed-de6a-475e-a9cb-a26a38bc18ba",
      "number": 9,
      "content": "Nhưng Ngài phá hỏng đường lối của những kẻ ác.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.920Z",
      "updatedAt": "2025-04-22T08:37:02.920Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "72ac0791-1d09-48c5-8edc-ac4197625d74",
      "number": 10,
      "content": "Chúa sẽ trị vì đời đời;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.930Z",
      "updatedAt": "2025-04-22T08:37:02.930Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    },
    {
      "id": "d75dc4c5-f22a-49fb-801c-07d518806636",
      "number": 10,
      "content": "Hỡi Si-ôn, Ðức Chúa Trời của ngươi sẽ trị vì đời đời.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:02.940Z",
      "updatedAt": "2025-04-22T08:37:02.940Z",
      "chapterId": "8fee083e-eaa1-49c5-8c27-acd608e59a33"
    }
  ],
