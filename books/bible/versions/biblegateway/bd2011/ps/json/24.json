{
  "chapter": {
    "id": "297812e4-c2b4-44c8-a97a-c34808f1c1ae",
    "number": 24,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2024&version=BD2011",
    "createdAt": "2025-04-24T03:03:15.428Z",
    "updatedAt": "2025-04-24T03:03:15.428Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "5c96ef2b-922f-457e-9971-7275dd033958",
      "number": 1,
      "content": "Trái đất và những vật trên đất đều thuộc về Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.366Z",
      "updatedAt": "2025-04-24T03:50:07.366Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "a33d3b1c-33c2-48ba-8b50-62546ade97fb",
      "number": 1,
      "content": "Thế giới và mọi người sống trong đó đều thuộc về Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.387Z",
      "updatedAt": "2025-04-24T03:50:07.387Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "c8fde808-2093-44e3-a781-1d9646a8b85e",
      "number": 2,
      "content": "Vì Ngài đã đặt nền nó trên các đại dương,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.398Z",
      "updatedAt": "2025-04-24T03:50:07.398Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "aba9b25c-5b42-4fed-8ade-bd4f149d4cf8",
      "number": 2,
      "content": "Và Ngài lập nó vững vàng trên các luồng nước.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.410Z",
      "updatedAt": "2025-04-24T03:50:07.410Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "57dffcf2-a304-453b-b786-95f73b36bc25",
      "number": 3,
      "content": "Ai có thể lên núi của Chúa?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.423Z",
      "updatedAt": "2025-04-24T03:50:07.423Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "47fb2bd1-0b76-4790-9400-3e333acf24f5",
      "number": 3,
      "content": "Ai có thể đứng trong nơi thánh của Ngài?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.436Z",
      "updatedAt": "2025-04-24T03:50:07.436Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "ee953392-3855-4c57-9fa9-99d1497c9b0a",
      "number": 4,
      "content": "Ấy là người có tay tinh sạch và lòng trong trắng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.449Z",
      "updatedAt": "2025-04-24T03:50:07.449Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "e6defc58-6c2c-43a0-8ae5-eedd16cf2fba",
      "number": 4,
      "content": "Ấy là người chẳng hướng lòng mình về thần tượng hư không,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.463Z",
      "updatedAt": "2025-04-24T03:50:07.463Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "e5467c56-8f3f-45c8-9b0f-8e83b4e79d61",
      "number": 4,
      "content": "Và chẳng thốt ra những lời thề nguyện giả dối.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.476Z",
      "updatedAt": "2025-04-24T03:50:07.476Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "4c739274-5f4c-4142-9d27-0f13db08257b",
      "number": 5,
      "content": "Người ấy sẽ nhận được phước hạnh của Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.489Z",
      "updatedAt": "2025-04-24T03:50:07.489Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "29b7ddeb-9f7a-4715-a303-46c428ee58f8",
      "number": 5,
      "content": "Và sẽ được Ðức Chúa Trời, Ðấng Giải Cứu mình, kể là công chính.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.503Z",
      "updatedAt": "2025-04-24T03:50:07.503Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "0cfbcdc8-56fb-4428-85eb-72f664171c04",
      "number": 6,
      "content": "Ấy là dòng dõi của những người tìm kiếm Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.516Z",
      "updatedAt": "2025-04-24T03:50:07.516Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "ca45f397-fb56-41b8-aae1-984a7df80ce5",
      "number": 6,
      "content": "Những người tìm kiếm mặt Ðức Chúa Trời của Gia-cốp. *(Sê-la)*",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.529Z",
      "updatedAt": "2025-04-24T03:50:07.529Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "567b1155-6fa8-438f-8402-0bf89d2f72ce",
      "number": 7,
      "content": "Hỡi các cổng, hãy ngẩng đầu lên;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.543Z",
      "updatedAt": "2025-04-24T03:50:07.543Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "76ef24bf-6322-42e1-a016-551a37934b84",
      "number": 7,
      "content": "Hỡi các cổng vạn cổ, hãy ngẩng cao lên,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.556Z",
      "updatedAt": "2025-04-24T03:50:07.556Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "c5857a25-fa65-4cae-95f8-11cbcdce4441",
      "number": 7,
      "content": "Ðể Vua vinh hiển sẽ vào.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.570Z",
      "updatedAt": "2025-04-24T03:50:07.570Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "548fa55a-fe1e-4149-a3ac-c67deb2b417b",
      "number": 8,
      "content": "Vua vinh hiển này là ai?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.583Z",
      "updatedAt": "2025-04-24T03:50:07.583Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "3aa2b552-680c-4f6e-84e4-80d91338d232",
      "number": 8,
      "content": "Ấy là Chúa đại quyền đại năng,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.597Z",
      "updatedAt": "2025-04-24T03:50:07.597Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "1d61823c-1748-432d-aa7b-17993358f69f",
      "number": 8,
      "content": "Ngài là Chúa vạn năng ở giữa chiến trận.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.610Z",
      "updatedAt": "2025-04-24T03:50:07.610Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "f938d548-8560-4db5-94ad-dd45cca332f7",
      "number": 9,
      "content": "Hỡi các cổng, hãy ngẩng đầu lên;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.623Z",
      "updatedAt": "2025-04-24T03:50:07.623Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "ef21afbb-3310-4a20-8d5c-6a4cc8ff9b79",
      "number": 9,
      "content": "Hỡi các cổng vạn cổ, hãy ngẩng cao lên,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.637Z",
      "updatedAt": "2025-04-24T03:50:07.637Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "72680554-db47-4681-ac72-9f23d3c33210",
      "number": 9,
      "content": "Ðể Vua vinh hiển sẽ vào.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.650Z",
      "updatedAt": "2025-04-24T03:50:07.650Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "103d2d38-cdf2-408e-8c85-9be8a21fe798",
      "number": 10,
      "content": "Vua vinh hiển này là ai?",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.664Z",
      "updatedAt": "2025-04-24T03:50:07.664Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "9291be00-62b5-4faf-8cc6-2266e33d53cf",
      "number": 10,
      "content": "Ấy là Chúa các đạo quân,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.677Z",
      "updatedAt": "2025-04-24T03:50:07.677Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    },
    {
      "id": "d771113a-2231-4426-8349-633e52b3a139",
      "number": 10,
      "content": "Chính Ngài là Vua vinh hiển. *(Sê-la)*",
      "order": 2,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:07.690Z",
      "updatedAt": "2025-04-24T03:50:07.690Z",
      "chapterId": "297812e4-c2b4-44c8-a97a-c34808f1c1ae"
    }
  ],
