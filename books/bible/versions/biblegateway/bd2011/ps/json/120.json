{
  "chapter": {
    "id": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6",
    "number": 120,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20120&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.923Z",
    "updatedAt": "2025-04-24T03:03:17.923Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "73f958a8-8b30-4a7b-9e48-e26d8d09d72c",
      "number": 1,
      "content": "Trong cơn hoạn nạn tôi kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.396Z",
      "updatedAt": "2025-04-24T04:02:20.396Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "43b32509-d763-4788-bdd0-9c7cecbbb6f3",
      "number": 1,
      "content": "Và Ngài đã đáp lời tôi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.432Z",
      "updatedAt": "2025-04-24T04:02:20.432Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "02cf6f65-81f7-4661-9b90-428c9c8e2682",
      "number": 2,
      "content": "Chúa ôi, xin giải cứu con khỏi môi dối trá,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.449Z",
      "updatedAt": "2025-04-24T04:02:20.449Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "346c9646-d918-42d6-9bd5-caf27041ac9a",
      "number": 2,
      "content": "Và khỏi lưỡi dối lừa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.462Z",
      "updatedAt": "2025-04-24T04:02:20.462Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "92a7ce97-1e7f-406e-a92d-3abbe781ce2d",
      "number": 3,
      "content": "Hỡi lưỡi dối trá, Ngài sẽ làm gì cho ngươi?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.474Z",
      "updatedAt": "2025-04-24T04:02:20.474Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "3c7fee96-d3db-4635-8579-ec803c588d42",
      "number": 3,
      "content": "Ngài sẽ làm gì nữa cho ngươi?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.489Z",
      "updatedAt": "2025-04-24T04:02:20.489Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "9a0554d0-2618-415f-bf97-9fa5b8e55e47",
      "number": 4,
      "content": "Ngài sẽ dùng những tên nhọn của người dũng sĩ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.502Z",
      "updatedAt": "2025-04-24T04:02:20.502Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "148be852-a7dc-4a2e-9e73-6920f0e77061",
      "number": 4,
      "content": "Vàn hững than lửa hừng của bụi cây chổi cho ngươi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.518Z",
      "updatedAt": "2025-04-24T04:02:20.518Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "dfbf2977-536c-494f-901f-85396c35cdd6",
      "number": 5,
      "content": "Khốn thay cho tôi vì tôi trú ngụ ở Mê-séc,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.528Z",
      "updatedAt": "2025-04-24T04:02:20.528Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "a3f07b2a-023a-4ecf-bb70-7f693f99835e",
      "number": 5,
      "content": "Vì tôi cư ngụ giữa các trại của Kê-đa.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.545Z",
      "updatedAt": "2025-04-24T04:02:20.545Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "c7bac345-767e-44d1-83cd-3c3e8c2daaee",
      "number": 6,
      "content": "Linh hồn tôi đã sống quá lâu",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.554Z",
      "updatedAt": "2025-04-24T04:02:20.554Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "605f1750-04e2-4301-81d2-73d1cd07bbe1",
      "number": 6,
      "content": "với những kẻ ghét hòa bình.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.567Z",
      "updatedAt": "2025-04-24T04:02:20.567Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "4e7e1921-86c2-454f-bd77-d2ba5d060461",
      "number": 7,
      "content": "Tôi yêu chuộng hòa bình,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.581Z",
      "updatedAt": "2025-04-24T04:02:20.581Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    },
    {
      "id": "b1deeaa8-a74d-4669-a258-36dce331952f",
      "number": 7,
      "content": "Nhưng khi tôi nói đến hòa bình, chúng chỉ muốn chiến tranh.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:20.596Z",
      "updatedAt": "2025-04-24T04:02:20.596Z",
      "chapterId": "a96fbaf2-49d0-40f2-ae90-c8d2c4c7b3a6"
    }
  ],
