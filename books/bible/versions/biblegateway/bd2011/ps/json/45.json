{
  "chapter": {
    "id": "72d6af61-9419-48f1-8c32-6b8f2793edba",
    "number": 45,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2045&version=BD2011",
    "createdAt": "2025-04-24T03:03:15.945Z",
    "updatedAt": "2025-04-24T03:03:15.945Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "f3623fe6-29de-4830-9910-939ff19e29ce",
      "number": 1,
      "content": "Lòng tôi tràn đầy những ý tưởng cao đẹp;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.629Z",
      "updatedAt": "2025-04-24T03:52:41.629Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "9978ec41-3dfe-4baa-b620-8939ca324f6f",
      "number": 1,
      "content": "Tôi xin ngâm cho vua nghe những lời thơ tao nhã;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.658Z",
      "updatedAt": "2025-04-24T03:52:41.658Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "caa6a23f-18ca-4f4e-8483-4d5937f40eb6",
      "number": 1,
      "content": "Lưỡi tôi như ngòi viết của một văn sĩ tài hoa.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.669Z",
      "updatedAt": "2025-04-24T03:52:41.669Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "78960544-70ad-4055-8478-42ce1a06d58a",
      "number": 2,
      "content": "Ðức vua tuấn tú hơn hẳn các con trai của loài người;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.684Z",
      "updatedAt": "2025-04-24T03:52:41.684Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "bfc0c03e-f413-40de-ac12-9722a0108030",
      "number": 2,
      "content": "Ân điển thoát ra từ môi người;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.700Z",
      "updatedAt": "2025-04-24T03:52:41.700Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "355760f0-a9ea-4cc1-8833-cb28c59b8cc1",
      "number": 2,
      "content": "Cho nên Ðức Chúa Trời ban phước cho người mãi mãi.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.715Z",
      "updatedAt": "2025-04-24T03:52:41.715Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "3e81a492-cb5c-462c-89d1-5603b896ea9f",
      "number": 3,
      "content": "Hỡi người dũng mãnh, xin nịt gươm người bên hông;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.728Z",
      "updatedAt": "2025-04-24T03:52:41.728Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "d2d5c918-f72c-4fa3-a14b-4199c4a29aa7",
      "number": 3,
      "content": "Xin mặc lấy sự huy hoàng và uy nghi!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.741Z",
      "updatedAt": "2025-04-24T03:52:41.741Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "c060fc82-aa2b-4ef8-8ed3-18c2605c7c05",
      "number": 4,
      "content": "Vì cớ chân lý, khiêm nhu, và công chính,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.754Z",
      "updatedAt": "2025-04-24T03:52:41.754Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "866c21fd-cfd1-430a-a28d-ae312808eb6d",
      "number": 4,
      "content": "Người oai hùng cỡi ngựa ra đi cách hiên ngang;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.767Z",
      "updatedAt": "2025-04-24T03:52:41.767Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "fe74e9b8-0d3e-4f02-8e29-b63f7fa32387",
      "number": 4,
      "content": "Nguyện tay phải người thực hiện những việc đáng sợ.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.780Z",
      "updatedAt": "2025-04-24T03:52:41.780Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "a7b054e3-2acc-41d4-a862-5b22e6bb40c9",
      "number": 5,
      "content": "Nguyện những mũi tên của người thật bén nhọn,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.793Z",
      "updatedAt": "2025-04-24T03:52:41.793Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "94aaf601-53a2-4b27-b966-d954067a7db3",
      "number": 5,
      "content": "Ghim thấu vào tim những kẻ thù của người;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.806Z",
      "updatedAt": "2025-04-24T03:52:41.806Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "4066e8f6-bf57-4962-8680-dabd75eaec9b",
      "number": 5,
      "content": "Nguyện các dân ngã rạp dưới chân người.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.821Z",
      "updatedAt": "2025-04-24T03:52:41.821Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "2b546929-f23e-4d82-bcce-d7773bd720e7",
      "number": 6,
      "content": "Ðức Chúa Trời ôi, nguyện ngôi người còn đời đời vô cùng;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.833Z",
      "updatedAt": "2025-04-24T03:52:41.833Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "ac72e012-0afa-4572-9ac4-6f5ce7e7ce78",
      "number": 6,
      "content": "Nguyện vương trượng của người là vương trượng của công lý.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.846Z",
      "updatedAt": "2025-04-24T03:52:41.846Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "ca818fa4-e928-4ef4-ad99-14c106e86654",
      "number": 7,
      "content": "Người yêu công chính và ghét gian tà,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.859Z",
      "updatedAt": "2025-04-24T03:52:41.859Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "51790239-1cfb-484c-8e28-3ca71c99a5f0",
      "number": 7,
      "content": "Nên Ðức Chúa Trời, Thần của người, đã xức dầu cho người bằng dầu vui mừng để lập người trên các đồng bạn của người.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.871Z",
      "updatedAt": "2025-04-24T03:52:41.871Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "b14e8b19-8632-4ab3-a907-d1d7eb35c7f7",
      "number": 8,
      "content": "Vương bào của người tỏa ra mùi thơm của mộc dược, trầm hương, và nhục quế;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.881Z",
      "updatedAt": "2025-04-24T03:52:41.881Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "fb67b965-9f8d-478d-b5e4-6d1efc0cb207",
      "number": 8,
      "content": "Trong cung điện bằng ngà, những nhạc khí bằng dây làm cho người vui vẻ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.900Z",
      "updatedAt": "2025-04-24T03:52:41.900Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "4964bceb-e926-4640-81c3-944a47cb9ed4",
      "number": 9,
      "content": "Các công chúa của các vua là những thiếu nữ giữa vòng những người nữ quý phái của người;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.912Z",
      "updatedAt": "2025-04-24T03:52:41.912Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "0aa3bca7-3b6d-4592-86ac-9189c2a89eb4",
      "number": 9,
      "content": "Hoàng hậu đứng bên phải người thật diễm lệ trong y phục bằng vàng Ô-phia.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.924Z",
      "updatedAt": "2025-04-24T03:52:41.924Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "1f5d5ece-c848-4cae-bb85-c5709c20fcae",
      "number": 10,
      "content": "Hỡi ái nữ, hãy lắng nghe, hãy chú tâm và nghe cho kỹ:",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.937Z",
      "updatedAt": "2025-04-24T03:52:41.937Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "ebab4dd2-5ac4-46c0-91b8-af1b48833af0",
      "number": 10,
      "content": "Hãy quên dân tộc con và nhà cha con,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.950Z",
      "updatedAt": "2025-04-24T03:52:41.950Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "0ed3eaa7-bf75-4fdd-a0eb-214dd7318bf1",
      "number": 11,
      "content": "Ðể đức vua sẽ ái mộ nhan sắc của con,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.962Z",
      "updatedAt": "2025-04-24T03:52:41.962Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "7d390d67-1a78-496f-850f-15782e5df8cf",
      "number": 11,
      "content": "Vì người là chúa của con, con hãy phủ phục trước mặt người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.975Z",
      "updatedAt": "2025-04-24T03:52:41.975Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "c64bdf59-1f48-4c2f-a48c-3738c5ee8036",
      "number": 12,
      "content": "Bấy giờ Ái Nữ của Ty-rơ sẽ đem lễ vật đến cho con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:41.988Z",
      "updatedAt": "2025-04-24T03:52:41.988Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "68a7c654-9e08-4212-9fe5-baeacd31f513",
      "number": 12,
      "content": "Những người giàu sang trong dân sẽ đến cầu ơn con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:42.001Z",
      "updatedAt": "2025-04-24T03:52:42.001Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "736b0b81-e11b-43d7-aa03-16a36726f4d9",
      "number": 13,
      "content": "Ái nữ của đức vua thật lộng lẫy trong cung điện của nàng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:42.013Z",
      "updatedAt": "2025-04-24T03:52:42.013Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "b3933982-c77a-40c9-8e15-7e3155d3abff",
      "number": 13,
      "content": "Áo nàng được dệt bằng vàng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:42.026Z",
      "updatedAt": "2025-04-24T03:52:42.026Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "8b33a6b2-c89d-4b4e-b510-922df4f8fc6a",
      "number": 14,
      "content": "Khi nàng được đưa đến chầu đức vua, nàng mặc áo thêu nhiều màu tuyệt đẹp;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:42.039Z",
      "updatedAt": "2025-04-24T03:52:42.039Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "1372e8d5-b70f-4930-b0bb-8c8455c2d6f0",
      "number": 14,
      "content": "Các trinh nữ theo hầu nàng cũng được diện kiến đức vua.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:42.050Z",
      "updatedAt": "2025-04-24T03:52:42.050Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "51c4fc55-c7c4-42d0-b622-c2861deb7add",
      "number": 15,
      "content": "Họ được đưa đi với lòng vui mừng hớn hở,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:42.060Z",
      "updatedAt": "2025-04-24T03:52:42.060Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "46526b20-647b-4cc4-94ec-43ff14025f7b",
      "number": 15,
      "content": "Khi họ được vào chầu trong cung điện của đức vua.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:42.072Z",
      "updatedAt": "2025-04-24T03:52:42.072Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "1898d8e2-100d-4860-970a-52654382f927",
      "number": 16,
      "content": "Các con trai của đức vua sẽ kế vị tổ tiên họ;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:42.084Z",
      "updatedAt": "2025-04-24T03:52:42.084Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "9ca7f11b-6eff-4160-97a0-b922daefa7ee",
      "number": 16,
      "content": "Ðức vua lập họ làm những lãnh tụ trên khắp nước.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:42.097Z",
      "updatedAt": "2025-04-24T03:52:42.097Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "92119640-f8e9-4c0b-a56a-395836459046",
      "number": 17,
      "content": "Tôi sẽ làm cho danh người được ghi nhớ trải qua các đời,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:42.110Z",
      "updatedAt": "2025-04-24T03:52:42.110Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    },
    {
      "id": "c77346ca-80ca-4494-b74a-101d061c5aab",
      "number": 17,
      "content": "Ðể các dân cảm tạ người đời đời mãi mãi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:42.122Z",
      "updatedAt": "2025-04-24T03:52:42.122Z",
      "chapterId": "72d6af61-9419-48f1-8c32-6b8f2793edba"
    }
