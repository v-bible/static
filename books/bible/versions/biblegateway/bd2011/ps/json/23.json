{
  "chapter": {
    "id": "0c1461b9-9d63-4b26-ac6f-ad0f70764844",
    "number": 23,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2023&version=BD2011",
    "createdAt": "2025-04-22T07:37:45.975Z",
    "updatedAt": "2025-04-22T07:37:45.975Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "66a99faf-9083-4f8f-82ee-5864814f517d",
      "number": 1,
      "content": "Chúa là Ðấng chăn giữ tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.695Z",
      "updatedAt": "2025-04-22T08:22:14.695Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "c4d07e9a-1279-4480-b261-be200f372cb9",
      "number": 1,
      "content": "Tôi sẽ chẳng thiếu thốn gì.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.723Z",
      "updatedAt": "2025-04-22T08:22:14.723Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "77bc6ac2-3dd1-499f-abb1-4ee4933d8a0f",
      "number": 2,
      "content": "Ngài cho tôi nằm nghỉ trong đồng cỏ xanh tươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.734Z",
      "updatedAt": "2025-04-22T08:22:14.734Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "702eb438-a844-4777-8a07-66e8dcd68a0b",
      "number": 2,
      "content": "Ngài dẫn tôi đến mé nước bình tịnh.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.745Z",
      "updatedAt": "2025-04-22T08:22:14.745Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "4d6d1403-815e-41c9-b8b2-8e2344440525",
      "number": 3,
      "content": "Ngài phục hồi sức lực cho linh hồn tôi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.757Z",
      "updatedAt": "2025-04-22T08:22:14.757Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "d3007bbc-a427-4c3b-958e-2b8e431e2cd1",
      "number": 3,
      "content": "Ngài dẫn tôi vào các lối công chính, vì cớ danh Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.770Z",
      "updatedAt": "2025-04-22T08:22:14.770Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "2ddf82fd-988d-4725-983d-4a74a01ddc7d",
      "number": 4,
      "content": "Thật vậy, dù khi con đi qua thung lũng rợp bóng tử thần,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.783Z",
      "updatedAt": "2025-04-22T08:22:14.783Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "ac1fb285-5c96-41dd-9651-4e8365ddc037",
      "number": 4,
      "content": "Con sẽ chẳng sợ tai họa nào,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.796Z",
      "updatedAt": "2025-04-22T08:22:14.796Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "188fe1ee-3541-4080-964d-9f640d4bdb81",
      "number": 4,
      "content": "Vì Ngài ở với con;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.810Z",
      "updatedAt": "2025-04-22T08:22:14.810Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "f2511efe-32dd-44db-b2dd-bf392f074467",
      "number": 4,
      "content": "Cây trượng và cây gậy của Ngài an ủi con.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.823Z",
      "updatedAt": "2025-04-22T08:22:14.823Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "1bdcac81-fb12-439e-8ed1-5fbe271a0c07",
      "number": 5,
      "content": "Ngài dọn bàn cho con trước mặt những kẻ thù của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.836Z",
      "updatedAt": "2025-04-22T08:22:14.836Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "1ff6ad7e-1a02-43d1-9d3a-068d14989781",
      "number": 5,
      "content": "Ngài xức dầu cho đầu con;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.849Z",
      "updatedAt": "2025-04-22T08:22:14.849Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "aed44a92-2364-4e5f-8266-f050b8fe79d5",
      "number": 5,
      "content": "Chén con đầy tràn.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.863Z",
      "updatedAt": "2025-04-22T08:22:14.863Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "8cd4e536-47aa-4555-a666-44382700f0c9",
      "number": 6,
      "content": "Quả thật, trọn đời tôi, phước hạnh và ơn thương xót sẽ theo tôi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.876Z",
      "updatedAt": "2025-04-22T08:22:14.876Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    },
    {
      "id": "9ff4c6f0-e4f4-42df-b796-c03af3e956a8",
      "number": 6,
      "content": "Tôi sẽ ở trong nhà của Chúa cho đến đời đời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:22:14.892Z",
      "updatedAt": "2025-04-22T08:22:14.892Z",
      "chapterId": "0c1461b9-9d63-4b26-ac6f-ad0f70764844"
    }
  ],
