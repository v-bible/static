{
  "chapter": {
    "id": "ec15b66b-2071-4998-8383-65509fbf71f5",
    "number": 23,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2023&version=BD2011",
    "createdAt": "2025-04-24T03:03:15.402Z",
    "updatedAt": "2025-04-24T03:03:15.402Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "59d0d8ba-3c27-482b-bc6a-47b7a6630ffb",
      "number": 1,
      "content": "Chúa là Ðấng chăn giữ tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.733Z",
      "updatedAt": "2025-04-24T03:50:00.733Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "38926a96-fcf3-4dda-9cf7-eb5e90234b57",
      "number": 1,
      "content": "Tôi sẽ chẳng thiếu thốn gì.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.761Z",
      "updatedAt": "2025-04-24T03:50:00.761Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "3fedb98c-2c9a-4618-8d6a-88a3c38264fe",
      "number": 2,
      "content": "Ngài cho tôi nằm nghỉ trong đồng cỏ xanh tươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.771Z",
      "updatedAt": "2025-04-24T03:50:00.771Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "752cb2ba-35a8-4c08-9e60-5c36dcacad84",
      "number": 2,
      "content": "Ngài dẫn tôi đến mé nước bình tịnh.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.782Z",
      "updatedAt": "2025-04-24T03:50:00.782Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "e95c77b6-9e7c-4838-bea1-cbef76a0c10f",
      "number": 3,
      "content": "Ngài phục hồi sức lực cho linh hồn tôi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.793Z",
      "updatedAt": "2025-04-24T03:50:00.793Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "471216d1-9d19-43e8-8010-22d982ce9e20",
      "number": 3,
      "content": "Ngài dẫn tôi vào các lối công chính, vì cớ danh Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.807Z",
      "updatedAt": "2025-04-24T03:50:00.807Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "101ad42d-9c0c-4ef6-9a3e-e1bd18627187",
      "number": 4,
      "content": "Thật vậy, dù khi con đi qua thung lũng rợp bóng tử thần,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.819Z",
      "updatedAt": "2025-04-24T03:50:00.819Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "53d9c99b-6b8e-404a-900a-e6939d5bd1a8",
      "number": 4,
      "content": "Con sẽ chẳng sợ tai họa nào,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.836Z",
      "updatedAt": "2025-04-24T03:50:00.836Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "8eb440f4-9e05-4dfb-8c70-550261836fd9",
      "number": 4,
      "content": "Vì Ngài ở với con;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.847Z",
      "updatedAt": "2025-04-24T03:50:00.847Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "e9eab3f4-2dc3-4b92-bdcc-11eed8f69f64",
      "number": 4,
      "content": "Cây trượng và cây gậy của Ngài an ủi con.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.858Z",
      "updatedAt": "2025-04-24T03:50:00.858Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "e1468830-ef01-4288-a6c4-9674f0462cd4",
      "number": 5,
      "content": "Ngài dọn bàn cho con trước mặt những kẻ thù của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.869Z",
      "updatedAt": "2025-04-24T03:50:00.869Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "19880eab-f1d3-4254-ad28-949f44f593b5",
      "number": 5,
      "content": "Ngài xức dầu cho đầu con;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.880Z",
      "updatedAt": "2025-04-24T03:50:00.880Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "21d73eca-c42c-40bd-b588-4d9ed66d5a12",
      "number": 5,
      "content": "Chén con đầy tràn.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.895Z",
      "updatedAt": "2025-04-24T03:50:00.895Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "45bdcf72-d467-4d95-a209-ea3a4b9ad0db",
      "number": 6,
      "content": "Quả thật, trọn đời tôi, phước hạnh và ơn thương xót sẽ theo tôi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.906Z",
      "updatedAt": "2025-04-24T03:50:00.906Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    },
    {
      "id": "a97caf68-9898-4daf-810c-77ef279f4513",
      "number": 6,
      "content": "Tôi sẽ ở trong nhà của Chúa cho đến đời đời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:50:00.916Z",
      "updatedAt": "2025-04-24T03:50:00.916Z",
      "chapterId": "ec15b66b-2071-4998-8383-65509fbf71f5"
    }
  ],
