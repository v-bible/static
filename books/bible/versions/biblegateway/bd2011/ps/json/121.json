{
  "chapter": {
    "id": "e16efaf8-8837-48d7-b66d-b7580b034862",
    "number": 121,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20121&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.948Z",
    "updatedAt": "2025-04-24T03:03:17.948Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "5c0c7d9b-61cd-4d76-a263-c9e4bc042816",
      "number": 1,
      "content": "Tôi ngước mắt nhìn lên đồi núi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.419Z",
      "updatedAt": "2025-04-24T04:02:27.419Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "06fe31a8-81f7-451d-b9ac-107d90cb7604",
      "number": 1,
      "content": "Sự giúp đỡ tôi đến từ đâu?",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.446Z",
      "updatedAt": "2025-04-24T04:02:27.446Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "77b24e76-1d45-4803-92b7-110d387e2eaf",
      "number": 2,
      "content": "Sự giúp đỡ tôi đến từ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.456Z",
      "updatedAt": "2025-04-24T04:02:27.456Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "b7f8d7d5-32cc-4d00-bb46-2c3c8c061677",
      "number": 2,
      "content": "Ðấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.466Z",
      "updatedAt": "2025-04-24T04:02:27.466Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "e0569eed-35c1-4489-9460-ddc1546ec418",
      "number": 3,
      "content": "Ngài sẽ không để cho chân bạn trợt ngã;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.476Z",
      "updatedAt": "2025-04-24T04:02:27.476Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "e2e50ebc-d096-4cb1-844f-5330d630f787",
      "number": 3,
      "content": "Ðấng gìn giữ bạn không hề buồn ngủ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.490Z",
      "updatedAt": "2025-04-24T04:02:27.490Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "435c2eac-d7da-4287-92f4-b1f0f330ddc4",
      "number": 4,
      "content": "Thật vậy Ðấng gìn giữ I-sơ-ra-ên không hề buồn ngủ và không bao giờ ngủ.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.502Z",
      "updatedAt": "2025-04-24T04:02:27.502Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "52332a45-3d57-4d64-9ffc-a4adf93c8972",
      "number": 5,
      "content": "Chúa là Ðấng gìn giữ bạn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.516Z",
      "updatedAt": "2025-04-24T04:02:27.516Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "d7f937d3-4318-469b-a57d-2f51545e2d69",
      "number": 5,
      "content": "Chúa là bóng mát bên phải bạn.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.529Z",
      "updatedAt": "2025-04-24T04:02:27.529Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "ec3c03d1-a357-44a9-9938-eb8d43ba57d8",
      "number": 6,
      "content": "Mặt trời sẽ không hại bạn ban ngày;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.542Z",
      "updatedAt": "2025-04-24T04:02:27.542Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "e7af62c8-0037-4dfa-a918-0b6f7c6cc1dd",
      "number": 6,
      "content": "Mặt trăng sẽ không hại bạn ban đêm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.555Z",
      "updatedAt": "2025-04-24T04:02:27.555Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "188ffb48-2a30-4138-9cf1-34fa46d19946",
      "number": 7,
      "content": "Chúa sẽ gìn giữ bạn khỏi mọi tai họa;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.568Z",
      "updatedAt": "2025-04-24T04:02:27.568Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "6e1168d1-7f3c-4399-bacf-3b0d794b14db",
      "number": 7,
      "content": "Ngài sẽ gìn giữ mạng sống bạn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.581Z",
      "updatedAt": "2025-04-24T04:02:27.581Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "1661e58c-33da-43f0-97bf-71d1e572fdbf",
      "number": 8,
      "content": "Chúa sẽ gìn giữ bạn khi ra khi vào,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.593Z",
      "updatedAt": "2025-04-24T04:02:27.593Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    },
    {
      "id": "4ed55595-439c-4647-921a-bb9b5feee226",
      "number": 8,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:27.607Z",
      "updatedAt": "2025-04-24T04:02:27.607Z",
      "chapterId": "e16efaf8-8837-48d7-b66d-b7580b034862"
    }
  ],
