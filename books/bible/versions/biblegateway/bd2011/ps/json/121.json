{
  "chapter": {
    "id": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0",
    "number": 121,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20121&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.530Z",
    "updatedAt": "2025-04-22T07:37:48.530Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "e526f9c3-262d-4e1e-95eb-0191648cb498",
      "number": 1,
      "content": "Tôi ngước mắt nhìn lên đồi núi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.765Z",
      "updatedAt": "2025-04-22T08:34:09.765Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "c21e6b0f-0f84-435f-bed7-69f19068b1d5",
      "number": 1,
      "content": "Sự giúp đỡ tôi đến từ đâu?",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.795Z",
      "updatedAt": "2025-04-22T08:34:09.795Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "7df0c221-277c-4fdd-bd71-f61359828511",
      "number": 2,
      "content": "Sự giúp đỡ tôi đến từ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.807Z",
      "updatedAt": "2025-04-22T08:34:09.807Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "62af683e-eff3-4382-a45d-e6f74c94898c",
      "number": 2,
      "content": "Ðấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.819Z",
      "updatedAt": "2025-04-22T08:34:09.819Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "2b17179e-d3c3-4453-afdb-505a48b346c8",
      "number": 3,
      "content": "Ngài sẽ không để cho chân bạn trợt ngã;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.832Z",
      "updatedAt": "2025-04-22T08:34:09.832Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "c935e6bb-7eef-4dc7-8026-687c7535edde",
      "number": 3,
      "content": "Ðấng gìn giữ bạn không hề buồn ngủ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.846Z",
      "updatedAt": "2025-04-22T08:34:09.846Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "3bf28870-ea30-4500-aeab-33b9c6f2d9b6",
      "number": 4,
      "content": "Thật vậy Ðấng gìn giữ I-sơ-ra-ên không hề buồn ngủ và không bao giờ ngủ.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.859Z",
      "updatedAt": "2025-04-22T08:34:09.859Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "09ddb5f4-dac9-46e3-a28e-35e0c84f117c",
      "number": 5,
      "content": "Chúa là Ðấng gìn giữ bạn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.872Z",
      "updatedAt": "2025-04-22T08:34:09.872Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "c283f781-bd28-4aa1-8679-39e5193c49a8",
      "number": 5,
      "content": "Chúa là bóng mát bên phải bạn.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.886Z",
      "updatedAt": "2025-04-22T08:34:09.886Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "e419c051-9a91-401f-ae3e-b78fa8814bea",
      "number": 6,
      "content": "Mặt trời sẽ không hại bạn ban ngày;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.902Z",
      "updatedAt": "2025-04-22T08:34:09.902Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "27005be9-7d0b-49e8-b708-289805f6cb44",
      "number": 6,
      "content": "Mặt trăng sẽ không hại bạn ban đêm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.915Z",
      "updatedAt": "2025-04-22T08:34:09.915Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "251ff793-712b-4fa3-9854-88c1cd808892",
      "number": 7,
      "content": "Chúa sẽ gìn giữ bạn khỏi mọi tai họa;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.928Z",
      "updatedAt": "2025-04-22T08:34:09.928Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "a7b39e75-2fc0-4e5d-abda-8837a17e8182",
      "number": 7,
      "content": "Ngài sẽ gìn giữ mạng sống bạn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.940Z",
      "updatedAt": "2025-04-22T08:34:09.940Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "d40d5660-54cc-4647-be54-bbec897d3b21",
      "number": 8,
      "content": "Chúa sẽ gìn giữ bạn khi ra khi vào,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.952Z",
      "updatedAt": "2025-04-22T08:34:09.952Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    },
    {
      "id": "8217dfd7-c1b8-4ca9-b029-ba28f8f336ec",
      "number": 8,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:09.964Z",
      "updatedAt": "2025-04-22T08:34:09.964Z",
      "chapterId": "0dc3f56d-05e2-4706-9044-bd1d0a75aff0"
    }
  ],
