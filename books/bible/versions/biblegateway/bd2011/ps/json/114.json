{
  "chapter": {
    "id": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7",
    "number": 114,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20114&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.318Z",
    "updatedAt": "2025-04-22T07:37:48.318Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "257862a3-7127-4bf2-8a7a-476dd8a65734",
      "number": 1,
      "content": "Khi dân I-sơ-ra-ên ra khỏi Ai-cập,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.496Z",
      "updatedAt": "2025-04-22T08:33:17.496Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "a970014a-8b44-4356-b5a0-552da6f06994",
      "number": 1,
      "content": "Nhà Gia-cốp ra khỏi một dân nói một ngôn ngữ lạ,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.523Z",
      "updatedAt": "2025-04-22T08:33:17.523Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "f09ba187-b48d-4560-b411-745df0822cdb",
      "number": 2,
      "content": "Giu-đa trở thành nơi thánh của Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.533Z",
      "updatedAt": "2025-04-22T08:33:17.533Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "f0d3552d-785a-4863-a188-a88ee7b2c222",
      "number": 2,
      "content": "I-sơ-ra-ên trở nên vương quốc của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.543Z",
      "updatedAt": "2025-04-22T08:33:17.543Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "2d782b43-3475-4c84-b04b-aa93a7c2be99",
      "number": 3,
      "content": "Nước đại dương trông thấy thế liền hoảng kinh chạy trốn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.553Z",
      "updatedAt": "2025-04-22T08:33:17.553Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "d866a387-4934-4972-8459-da07e4b604aa",
      "number": 3,
      "content": "Nước Sông Giô-đanh đã hỗn loạn chảy ngược về nguồn;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.563Z",
      "updatedAt": "2025-04-22T08:33:17.563Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "1e39f94d-baaf-4ff2-a9ec-760b98e1a45c",
      "number": 4,
      "content": "Các rặng núi nhảy lên như chiên đực;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.576Z",
      "updatedAt": "2025-04-22T08:33:17.576Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "039ec41b-af7a-479c-951f-cabe52d1c7da",
      "number": 4,
      "content": "Các dãy đồi nhảy cỡn như chiên con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.590Z",
      "updatedAt": "2025-04-22T08:33:17.590Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "19ebfff6-8812-46b6-9dd1-f53e02b2d2c3",
      "number": 5,
      "content": "Ðiều gì đã khiến các ngươi làm như thế?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.600Z",
      "updatedAt": "2025-04-22T08:33:17.600Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "4295c443-cdbb-4469-8e1d-6851432e0a74",
      "number": 5,
      "content": "Hỡi nước đại dương, sao ngươi chạy trốn?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.611Z",
      "updatedAt": "2025-04-22T08:33:17.611Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "147d220a-d042-4ffe-8d47-819ed4906f24",
      "number": 5,
      "content": "Hỡi nước Sông Giô-đanh, sao ngươi chảy ngược về nguồn?",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.625Z",
      "updatedAt": "2025-04-22T08:33:17.625Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "ef6a6246-7d28-4db0-8fee-d708cd43d922",
      "number": 6,
      "content": "Hỡi các rặng núi, sao các ngươi nhảy lên như chiên đực?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.639Z",
      "updatedAt": "2025-04-22T08:33:17.639Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "b17a6f87-3a1b-47d2-acdb-700a2ae5e6ca",
      "number": 6,
      "content": "Hỡi các dãy đồi, sao các ngươi nhảy cỡn như chiên con?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.650Z",
      "updatedAt": "2025-04-22T08:33:17.650Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "78b6a4ae-d43c-4c92-91a6-9ee0be809685",
      "number": 7,
      "content": "Hỡi trái đất, khá run rẩy trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.660Z",
      "updatedAt": "2025-04-22T08:33:17.660Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "45b4271b-a140-4ae5-b4e5-044a9fecc519",
      "number": 7,
      "content": "Tức trước thánh nhan Ðức Chúa Trời của Gia-cốp,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.670Z",
      "updatedAt": "2025-04-22T08:33:17.670Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "1166c9a1-d2bb-4684-ba1c-b905f186f87a",
      "number": 8,
      "content": "Ðấng khiến nước từ một vầng đá tuôn ra thành ao hồ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.680Z",
      "updatedAt": "2025-04-22T08:33:17.680Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    },
    {
      "id": "59821452-71fe-49bd-91bf-c2bef2087e04",
      "number": 8,
      "content": "Từ tảng đá hoa cương, dòng nước ngọt lai láng trào ra.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:17.690Z",
      "updatedAt": "2025-04-22T08:33:17.690Z",
      "chapterId": "297f6d44-e3c9-4d4a-a1e0-978f16c18cb7"
    }
  ],
