{
  "chapter": {
    "id": "ea7626c9-992a-4f2e-88b5-3c44d681cb44",
    "number": 114,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20114&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.771Z",
    "updatedAt": "2025-04-24T03:03:17.771Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "c74b2eef-bf41-483e-9445-86716f24227c",
      "number": 1,
      "content": "Khi dân I-sơ-ra-ên ra khỏi Ai-cập,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.437Z",
      "updatedAt": "2025-04-24T04:01:27.437Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "7f77668d-7ba5-44ce-af1c-3425da22994a",
      "number": 1,
      "content": "Nhà Gia-cốp ra khỏi một dân nói một ngôn ngữ lạ,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.464Z",
      "updatedAt": "2025-04-24T04:01:27.464Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "ec1afc7e-0549-43db-8a6f-6ce9e9aa61c9",
      "number": 2,
      "content": "Giu-đa trở thành nơi thánh của Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.474Z",
      "updatedAt": "2025-04-24T04:01:27.474Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "2ddffba6-e139-42ac-9c52-54cd3e363a0e",
      "number": 2,
      "content": "I-sơ-ra-ên trở nên vương quốc của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.484Z",
      "updatedAt": "2025-04-24T04:01:27.484Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "4cb92d13-e0fb-424a-a632-231e8b53c2d9",
      "number": 3,
      "content": "Nước đại dương trông thấy thế liền hoảng kinh chạy trốn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.494Z",
      "updatedAt": "2025-04-24T04:01:27.494Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "2e9681ac-85a1-4fba-8b96-1c6dddf3b4f1",
      "number": 3,
      "content": "Nước Sông Giô-đanh đã hỗn loạn chảy ngược về nguồn;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.505Z",
      "updatedAt": "2025-04-24T04:01:27.505Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "5a2ad784-4e31-4a4f-8c52-fd0992eafcfd",
      "number": 4,
      "content": "Các rặng núi nhảy lên như chiên đực;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.517Z",
      "updatedAt": "2025-04-24T04:01:27.517Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "5116ab43-b289-431c-bd20-29298e6f8436",
      "number": 4,
      "content": "Các dãy đồi nhảy cỡn như chiên con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.530Z",
      "updatedAt": "2025-04-24T04:01:27.530Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "2ce934e7-ebb9-487a-8e76-b41c16b7b819",
      "number": 5,
      "content": "Ðiều gì đã khiến các ngươi làm như thế?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.544Z",
      "updatedAt": "2025-04-24T04:01:27.544Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "bbee9337-cb57-4fe1-8d29-7071a90c1f8a",
      "number": 5,
      "content": "Hỡi nước đại dương, sao ngươi chạy trốn?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.557Z",
      "updatedAt": "2025-04-24T04:01:27.557Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "8c0da4bb-b851-492f-9637-843963b7e9b8",
      "number": 5,
      "content": "Hỡi nước Sông Giô-đanh, sao ngươi chảy ngược về nguồn?",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.570Z",
      "updatedAt": "2025-04-24T04:01:27.570Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "e2a5008d-7943-418a-96b6-31b822397ed3",
      "number": 6,
      "content": "Hỡi các rặng núi, sao các ngươi nhảy lên như chiên đực?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.583Z",
      "updatedAt": "2025-04-24T04:01:27.583Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "8125ea12-c3db-4071-98d4-f7523b99ef32",
      "number": 6,
      "content": "Hỡi các dãy đồi, sao các ngươi nhảy cỡn như chiên con?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.597Z",
      "updatedAt": "2025-04-24T04:01:27.597Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "64e3590b-1100-42ed-818e-7a7c939542e3",
      "number": 7,
      "content": "Hỡi trái đất, khá run rẩy trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.610Z",
      "updatedAt": "2025-04-24T04:01:27.610Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "cda109bd-6b43-4086-b270-e12582cfcae5",
      "number": 7,
      "content": "Tức trước thánh nhan Ðức Chúa Trời của Gia-cốp,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.623Z",
      "updatedAt": "2025-04-24T04:01:27.623Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "07a383b7-70f3-4632-9555-869904ebc051",
      "number": 8,
      "content": "Ðấng khiến nước từ một vầng đá tuôn ra thành ao hồ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.639Z",
      "updatedAt": "2025-04-24T04:01:27.639Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    },
    {
      "id": "92641d99-6256-44c8-b97c-fda0f03ad74a",
      "number": 8,
      "content": "Từ tảng đá hoa cương, dòng nước ngọt lai láng trào ra.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:27.652Z",
      "updatedAt": "2025-04-24T04:01:27.652Z",
      "chapterId": "ea7626c9-992a-4f2e-88b5-3c44d681cb44"
    }
  ],
