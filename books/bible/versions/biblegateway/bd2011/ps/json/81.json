  "headings": [
    {
      "id": "7215322c-8838-4087-ac61-cfb6c860baa2",
      "content": "Lòng Tốt của Ðức Chúa Trời Ðối Với Sự Lầm Lạc của Dân I-sơ-ra-ên",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T03:57:10.088Z",
