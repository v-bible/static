  "headings": [
    {
      "id": "6b1d0ec3-60cd-4184-8845-ece8e006c1db",
      "content": "Lòng Tốt của Ðức Chúa Trời Ðối Với Sự Lầm Lạc của Dân I-sơ-ra-ên",
      "order": 0,
      "createdAt": "2024-09-24T02:04:00.952Z",
