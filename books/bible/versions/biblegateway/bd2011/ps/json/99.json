{
  "chapter": {
    "id": "90193e33-4ddb-4f88-93bf-6afa893b919b",
    "number": 99,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2099&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.360Z",
    "updatedAt": "2025-04-24T03:03:17.360Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "ffd939b4-4eb7-46f6-a821-e649f4eb1d99",
      "number": 1,
      "content": "Chúa trị vì, muôn dân hãy run sợ!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:24.978Z",
      "updatedAt": "2025-04-24T03:59:24.978Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "64379eb5-b87e-4431-98cb-3f9ff6f36126",
      "number": 1,
      "content": "Ngài ngự giữa các chê-ru-bim, trái đất khá rung rinh.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.008Z",
      "updatedAt": "2025-04-24T03:59:25.008Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "fc455be2-6cb5-4fd5-bc83-56af078f29aa",
      "number": 2,
      "content": "Chúa thật vĩ đại ở Si-ôn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.017Z",
      "updatedAt": "2025-04-24T03:59:25.017Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "bb32a54b-d7f8-4fcc-ab3d-b8f953832aaa",
      "number": 2,
      "content": "Ngài được tôn cao hơn tất cả các dân các nước.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.031Z",
      "updatedAt": "2025-04-24T03:59:25.031Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "6bc46d8d-e2ef-4ff6-93e0-e36eeff55852",
      "number": 3,
      "content": "Nguyện họ ca tụng danh lớn lao và đáng sợ của Ngài.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.043Z",
      "updatedAt": "2025-04-24T03:59:25.043Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "b38c1a29-aeaf-474a-a6c8-b6ebe3e4d46a",
      "number": 3,
      "content": "Ngài là Thánh!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.055Z",
      "updatedAt": "2025-04-24T03:59:25.055Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "520074f6-7137-4364-944c-2e794ab12785",
      "number": 4,
      "content": "Lạy Vua quyền năng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.068Z",
      "updatedAt": "2025-04-24T03:59:25.068Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "1f331977-2901-4a1f-abb9-7fb7faedcd8c",
      "number": 4,
      "content": "Ðấng yêu công lý;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.078Z",
      "updatedAt": "2025-04-24T03:59:25.078Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "e93ef989-d3af-41e3-81d8-dc572b7e4b8f",
      "number": 4,
      "content": "Ngài lập sự công bằng;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.088Z",
      "updatedAt": "2025-04-24T03:59:25.088Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "beed0083-928a-4954-9fd0-1eeed2d70e70",
      "number": 4,
      "content": "Ngài thi hành công lý và công chính trong Gia-cốp.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.098Z",
      "updatedAt": "2025-04-24T03:59:25.098Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "57ff337f-a118-401a-897f-90ae097ab51c",
      "number": 5,
      "content": "Hãy tôn cao Chúa, Ðức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.108Z",
      "updatedAt": "2025-04-24T03:59:25.108Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "a0abd42f-437c-4c17-b661-24b7f95bd038",
      "number": 5,
      "content": "Hãy thờ lạy nơi bệ chân Ngài;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.119Z",
      "updatedAt": "2025-04-24T03:59:25.119Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "bef5ebb3-0147-464f-ba23-8577e768ddc7",
      "number": 5,
      "content": "Ngài là Thánh!",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.136Z",
      "updatedAt": "2025-04-24T03:59:25.136Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "68c63ce1-e9dd-4cd9-a8ea-45e99f4e083e",
      "number": 6,
      "content": "Môi-se và A-rôn là những người trong vòng các tư tế;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.149Z",
      "updatedAt": "2025-04-24T03:59:25.149Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "6113105b-56de-482e-a1d3-d88e333f3eff",
      "number": 6,
      "content": "Sa-mu-ên ở trong số những người cầu khẩn danh Ngài;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.161Z",
      "updatedAt": "2025-04-24T03:59:25.161Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "7e5b2428-1527-4d55-b91f-b2c356f16ab1",
      "number": 6,
      "content": "Họ kêu cầu Chúa, và Ngài đã đáp lời họ.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.173Z",
      "updatedAt": "2025-04-24T03:59:25.173Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "395c88b8-7405-48dc-8912-0da48bb238b1",
      "number": 7,
      "content": "Từ trong đám mây, Ngài phán với họ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.185Z",
      "updatedAt": "2025-04-24T03:59:25.185Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "026d18c6-e192-49cf-9333-496283e7e313",
      "number": 7,
      "content": "Họ tuân giữ các mạng lịnh Ngài và vâng theo các luật lệ Ngài ban cho họ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.202Z",
      "updatedAt": "2025-04-24T03:59:25.202Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "9693e1e3-3a4d-4178-9e7e-54ca10c6fdd5",
      "number": 8,
      "content": "Lạy Chúa, Ðức Chúa Trời chúng con, Ngài đã đáp lời họ;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.213Z",
      "updatedAt": "2025-04-24T03:59:25.213Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "bdbcfac8-8299-45af-ae28-32693cb08b54",
      "number": 8,
      "content": "Khi đối xử với họ Ngài thật là Ðức Chúa Trời hay dung thứ,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.224Z",
      "updatedAt": "2025-04-24T03:59:25.224Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "7feed020-5808-44e3-9fea-51b121d09c6d",
      "number": 8,
      "content": "Dẫu Ngài có sửa phạt họ vì những sai phạm của họ.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.236Z",
      "updatedAt": "2025-04-24T03:59:25.236Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "7b98db46-f9bd-44d7-9320-af7e882bc22d",
      "number": 9,
      "content": "Hãy tôn cao Chúa, Ðức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.248Z",
      "updatedAt": "2025-04-24T03:59:25.248Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "5fa68731-8ed3-4e71-ac27-bffbbf6b7917",
      "number": 9,
      "content": "Hãy thờ phượng Ngài trên núi thánh của Ngài,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.262Z",
      "updatedAt": "2025-04-24T03:59:25.262Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    },
    {
      "id": "d28ca1df-9f27-4446-8645-910b89211188",
      "number": 9,
      "content": "Vì Chúa, Ðức Chúa Trời chúng ta, là thánh.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:25.276Z",
      "updatedAt": "2025-04-24T03:59:25.276Z",
      "chapterId": "90193e33-4ddb-4f88-93bf-6afa893b919b"
    }
  ],
