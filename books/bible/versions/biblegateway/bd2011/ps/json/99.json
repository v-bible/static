    {
      "id": "2a0a1ad7-ea97-4b9e-adb6-08299426ffe5",
      "content": "Ca Tụng Chúa Vì Sự Gắn Bó của Ngài với I-sơ-ra-ên",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T03:59:24.995Z",
      "updatedAt": "2025-04-24T03:59:24.995Z",
