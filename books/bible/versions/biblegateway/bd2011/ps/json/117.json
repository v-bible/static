  "verses": [
    {
      "id": "85cd8337-94bc-4dc5-902e-ad611d8110b9",
      "number": 1,
      "content": "Hỡi các nước, hãy ca ngợi Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:51.425Z",
      "updatedAt": "2025-04-24T04:01:51.425Z",
      "chapterId": "97127375-07c7-4e04-b2fa-a18d7cd913ee"
    },
    {
      "id": "50564789-c0e8-44e8-9b2f-37f674d1a2d5",
      "number": 1,
      "content": "Hỡi muôn dân, khá tôn ngợi Ngài!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:51.445Z",
      "updatedAt": "2025-04-24T04:01:51.445Z",
      "chapterId": "97127375-07c7-4e04-b2fa-a18d7cd913ee"
    },
    {
      "id": "243a49ce-95b3-471e-8945-e05b2154c753",
      "number": 2,
      "content": "Vì tình thương của Ngài đối với chúng ta rất lớn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:51.455Z",
      "updatedAt": "2025-04-24T04:01:51.455Z",
      "chapterId": "97127375-07c7-4e04-b2fa-a18d7cd913ee"
    },
    {
      "id": "31addf0a-0e8c-4e86-93fc-c8412c09b950",
      "number": 2,
      "content": "Và đức thành tín của Chúa hằng còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:51.465Z",
      "updatedAt": "2025-04-24T04:01:51.465Z",
      "chapterId": "97127375-07c7-4e04-b2fa-a18d7cd913ee"
    },
    {
      "id": "18741683-ac91-4ca5-8d31-7463e29ae909",
      "number": 2,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:01:51.475Z",
      "updatedAt": "2025-04-24T04:01:51.475Z",
