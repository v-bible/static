  "verses": [
    {
      "id": "c0110e94-0868-4baa-b8c7-d7303fb86621",
      "number": 1,
      "content": "Hỡi các nước, hãy ca ngợi Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:09:28.162Z",
      "updatedAt": "2024-09-24T02:09:28.162Z",
      "chapterId": "b30aa256-4dd8-4c70-a4bb-46889e027b6f"
    },
    {
      "id": "f6d6b33a-3cb5-4b85-b106-f928a2d33ad1",
      "number": 1,
      "content": "Hỡi muôn dân, khá tôn ngợi Ngài!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:09:28.169Z",
      "updatedAt": "2024-09-24T02:09:28.169Z",
      "chapterId": "b30aa256-4dd8-4c70-a4bb-46889e027b6f"
    },
    {
      "id": "00fa3686-0e3c-4d68-a187-55fa3b572dac",
      "number": 2,
      "content": "Vì tình thương của Ngài đối với chúng ta rất lớn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:09:28.172Z",
      "updatedAt": "2024-09-24T02:09:28.172Z",
      "chapterId": "b30aa256-4dd8-4c70-a4bb-46889e027b6f"
    },
    {
      "id": "2b1f317a-cc0d-4baa-895e-20d67dc4eba1",
      "number": 2,
      "content": "Và đức thành tín của Chúa hằng còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:09:28.175Z",
      "updatedAt": "2024-09-24T02:09:28.175Z",
      "chapterId": "b30aa256-4dd8-4c70-a4bb-46889e027b6f"
    },
    {
      "id": "7f04b5e0-cb12-4f39-8c00-3175a7701a6f",
      "number": 2,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:09:28.178Z",
      "updatedAt": "2024-09-24T02:09:28.178Z",
