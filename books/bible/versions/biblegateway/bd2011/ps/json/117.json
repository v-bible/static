  "verses": [
    {
      "id": "0ebf9d80-9233-421b-966d-b9192b8541c8",
      "number": 1,
      "content": "Hỡi các nước, hãy ca ngợi Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:37.751Z",
      "updatedAt": "2025-04-22T08:33:37.751Z",
      "chapterId": "18ec52bc-318a-4a01-b623-e8868f63a60d"
    },
    {
      "id": "bf3abe86-1e4f-41c6-a0a9-d80fe49401a3",
      "number": 1,
      "content": "Hỡi muôn dân, khá tôn ngợi Ngài!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:37.779Z",
      "updatedAt": "2025-04-22T08:33:37.779Z",
      "chapterId": "18ec52bc-318a-4a01-b623-e8868f63a60d"
    },
    {
      "id": "a5fbcc36-d40a-425d-8bd7-f7b4f7627100",
      "number": 2,
      "content": "Vì tình thương của Ngài đối với chúng ta rất lớn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:37.790Z",
      "updatedAt": "2025-04-22T08:33:37.790Z",
      "chapterId": "18ec52bc-318a-4a01-b623-e8868f63a60d"
    },
    {
      "id": "d7e1cd3e-f00e-45bb-8f87-f833bb61dd08",
      "number": 2,
      "content": "Và đức thành tín của Chúa hằng còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:37.801Z",
      "updatedAt": "2025-04-22T08:33:37.801Z",
      "chapterId": "18ec52bc-318a-4a01-b623-e8868f63a60d"
    },
    {
      "id": "226b6200-4ea3-4ea6-8aad-a6ccf8ee8c28",
      "number": 2,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:33:37.812Z",
      "updatedAt": "2025-04-22T08:33:37.812Z",
