{
  "chapter": {
    "id": "3ae186ca-446c-4280-afb9-e9038ecb3b70",
    "number": 66,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2066&version=BD2011",
    "createdAt": "2025-04-22T07:37:47.096Z",
    "updatedAt": "2025-04-22T07:37:47.096Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "f08a7029-02e2-4b78-a180-16d3955805a3",
      "number": 1,
      "content": "Hỡi muôn loài trên đất,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.265Z",
      "updatedAt": "2025-04-22T08:27:27.265Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "251ecfb4-d504-4f57-bc23-0a68f01f66de",
      "number": 1,
      "content": "Hãy cất tiếng reo mừng dâng lên Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.303Z",
      "updatedAt": "2025-04-22T08:27:27.303Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "44e57a04-4130-4415-a5a9-ea6f7e908323",
      "number": 2,
      "content": "Hãy ca ngợi danh vinh hiển của Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.316Z",
      "updatedAt": "2025-04-22T08:27:27.316Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "7e4a4d86-5794-4e69-ba42-af2b11e0b36e",
      "number": 2,
      "content": "Hãy tôn vinh ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.329Z",
      "updatedAt": "2025-04-22T08:27:27.329Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "5ca5f05d-a9e6-476e-9362-eceed8db4c2b",
      "number": 3,
      "content": "Hãy thưa cùng Ðức Chúa Trời rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.345Z",
      "updatedAt": "2025-04-22T08:27:27.345Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "0071c54d-3b38-4659-aff8-d3afe7d71e13",
      "number": 3,
      "content": "“Những công việc của Ngài thật đáng kinh hãi thay!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.362Z",
      "updatedAt": "2025-04-22T08:27:27.362Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "730f29df-0a51-4961-a39b-8674cc13a794",
      "number": 3,
      "content": "Bởi quyền năng lớn lao của Ngài,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.376Z",
      "updatedAt": "2025-04-22T08:27:27.376Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "736479a2-e180-4084-a55a-179703f595c7",
      "number": 3,
      "content": "Những kẻ thù của Ngài phải phủ phục trước mặt Ngài.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.389Z",
      "updatedAt": "2025-04-22T08:27:27.389Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "94047bc4-77ba-45d3-b2f7-0064bfc90943",
      "number": 4,
      "content": "Mọi loài trên đất sẽ thờ phượng Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.402Z",
      "updatedAt": "2025-04-22T08:27:27.402Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "d7c0ff90-aeca-4af5-aaa0-30f215f8332e",
      "number": 4,
      "content": "Chúng sẽ hát ca tôn ngợi Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.415Z",
      "updatedAt": "2025-04-22T08:27:27.415Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "496c9471-97a7-456c-99f4-d2b231d538d5",
      "number": 4,
      "content": "Chúng sẽ ca hát tôn vinh danh Ngài.” *(Sê-la)*",
      "order": 2,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.429Z",
      "updatedAt": "2025-04-22T08:27:27.429Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "cad181d4-cdf7-493f-a05c-c7198111c431",
      "number": 5,
      "content": "Hãy đến và nhìn xem những công việc của Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.442Z",
      "updatedAt": "2025-04-22T08:27:27.442Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "3cb053d9-11ce-43af-b28a-4b502f6ac6d1",
      "number": 5,
      "content": "Những gì Ngài làm cho loài người thật đáng kinh hãi thay!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.459Z",
      "updatedAt": "2025-04-22T08:27:27.459Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "11f4c245-1d3e-409a-8dcb-039eb3d1b64b",
      "number": 6,
      "content": "Ngài biến đại dương thành đất khô;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.472Z",
      "updatedAt": "2025-04-22T08:27:27.472Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "e4225c96-65e9-41ab-bfb3-0cefd0210753",
      "number": 6,
      "content": "Ngài cho họ đi bộ vượt qua dòng nước mạnh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.485Z",
      "updatedAt": "2025-04-22T08:27:27.485Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "0b10963f-06a7-491c-b8e9-2059813d35b0",
      "number": 6,
      "content": "Hãy đến, chúng ta hãy vui mừng trong Ngài!",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.498Z",
      "updatedAt": "2025-04-22T08:27:27.498Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "d5497517-f186-43e5-838d-e42a4d6eebbc",
      "number": 7,
      "content": "Ngài dùng quyền năng của Ngài trị vì đời đời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.511Z",
      "updatedAt": "2025-04-22T08:27:27.511Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "1cfc585a-064a-44c3-8d5c-1a5b2fa8b9bc",
      "number": 7,
      "content": "Mắt Ngài nhìn xem các nước,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.525Z",
      "updatedAt": "2025-04-22T08:27:27.525Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "0e04936c-8e3e-48a7-9dea-7a2378befa99",
      "number": 7,
      "content": "Kẻo những kẻ phản loạn nổi lên gây rối chăng. *(Sê-la)*",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.538Z",
      "updatedAt": "2025-04-22T08:27:27.538Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "f4ed8edb-c5f9-4c46-8567-945c04639e92",
      "number": 8,
      "content": "Hỡi các dân, hãy ca ngợi Ðức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.554Z",
      "updatedAt": "2025-04-22T08:27:27.554Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "18c1706a-4129-4040-9e8d-46e8f45f2a83",
      "number": 8,
      "content": "Hãy làm cho âm thanh ca ngợi Ngài vang ra khắp chốn,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.568Z",
      "updatedAt": "2025-04-22T08:27:27.568Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "8889d295-67f6-4792-bd95-3b0d4647211f",
      "number": 9,
      "content": "Vì Ngài giữ gìn mạng sống chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.582Z",
      "updatedAt": "2025-04-22T08:27:27.582Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "3ce40232-8c8f-4f5f-aac9-bab8affbd7dc",
      "number": 9,
      "content": "Ngài không để cho chân chúng ta trợt ngã.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.595Z",
      "updatedAt": "2025-04-22T08:27:27.595Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "601efc27-0660-40b2-a3f7-5a77c4b4f133",
      "number": 10,
      "content": "Thật vậy Ðức Chúa Trời ôi, Ngài đã thử thách chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.609Z",
      "updatedAt": "2025-04-22T08:27:27.609Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "89d8950e-ca31-48be-80b5-be8a812cbaca",
      "number": 10,
      "content": "Ngài đã rèn luyện chúng con như người ta luyện bạc.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.622Z",
      "updatedAt": "2025-04-22T08:27:27.622Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "f2d19bc3-95cd-45e5-91e0-b930a9a6bf83",
      "number": 11,
      "content": "Ngài đã để cho chúng con bị sa vào lưới;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.635Z",
      "updatedAt": "2025-04-22T08:27:27.635Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "81239a08-e841-4774-80b6-491983add253",
      "number": 11,
      "content": "Ngài đã để gánh nặng bị áp bức đè nặng trên lưng chúng con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.648Z",
      "updatedAt": "2025-04-22T08:27:27.648Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "4347f00e-bdca-429b-9bb8-6cfb46497b67",
      "number": 12,
      "content": "Ngài đã để người ta cỡi trên đầu trên cổ chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.661Z",
      "updatedAt": "2025-04-22T08:27:27.661Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "2bced9b1-e7eb-44d9-b211-c7605d06ea42",
      "number": 12,
      "content": "Chúng con phải đi qua lửa và qua nước;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.678Z",
      "updatedAt": "2025-04-22T08:27:27.678Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "bc44fd30-5754-41e7-94e4-213fb4b8fa5a",
      "number": 12,
      "content": "Nhưng rồi Ngài đã đem chúng con ra khỏi,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.691Z",
      "updatedAt": "2025-04-22T08:27:27.691Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "67e4bf98-c7d8-4200-8604-3e26ec35a72e",
      "number": 12,
      "content": "Ðể đưa chúng con vào một nơi trù phú.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.705Z",
      "updatedAt": "2025-04-22T08:27:27.705Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "d29c5619-f069-49d0-9505-47b3336bac00",
      "number": 13,
      "content": "Con sẽ vào nhà Ngài với những của lễ thiêu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.718Z",
      "updatedAt": "2025-04-22T08:27:27.718Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "a709eec6-fab6-4cb5-a949-d716e7b4ecc7",
      "number": 13,
      "content": "Con sẽ trả xong cho Ngài những gì con đã hứa nguyện,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.731Z",
      "updatedAt": "2025-04-22T08:27:27.731Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "d8f09c2f-3da5-4ad7-a1fa-ead9662becc8",
      "number": 14,
      "content": "Những gì môi con đã hứa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.744Z",
      "updatedAt": "2025-04-22T08:27:27.744Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "96e4c9ee-d1ee-49f4-b53a-006bf4bddc92",
      "number": 14,
      "content": "Và miệng con đã nói ra trong lúc bị khốn khổ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.757Z",
      "updatedAt": "2025-04-22T08:27:27.757Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "b72ef384-ba27-4815-abc3-f42c8ab57a22",
      "number": 15,
      "content": "Con sẽ dâng lên Ngài những con vật hiến tế béo tốt để làm của lễ thiêu, chung với mỡ của các chiên đực;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.770Z",
      "updatedAt": "2025-04-22T08:27:27.770Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "98b6dd2f-d2d2-47f7-aa83-c20e3ec982f5",
      "number": 15,
      "content": "Con sẽ dâng các bò đực và các dê đực. *(Sê-la)*",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.784Z",
      "updatedAt": "2025-04-22T08:27:27.784Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "b955a9d8-245d-492c-9560-c0fb79706e75",
      "number": 16,
      "content": "Hỡi mọi người kính sợ Ðức Chúa Trời, xin đến nghe,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.797Z",
      "updatedAt": "2025-04-22T08:27:27.797Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "162321bc-d64b-46b5-b642-9fdf74edce25",
      "number": 16,
      "content": "Tôi sẽ thuật lại những gì Ngài đã làm cho tôi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.811Z",
      "updatedAt": "2025-04-22T08:27:27.811Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "d48eb4ab-cd7f-483e-bc88-0171b7a24e97",
      "number": 17,
      "content": "Miệng tôi đã kêu cầu Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.824Z",
      "updatedAt": "2025-04-22T08:27:27.824Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "f3f0aa26-f3c4-43b6-a136-6120f85aff53",
      "number": 17,
      "content": "Và lưỡi tôi đã tôn vinh Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.837Z",
      "updatedAt": "2025-04-22T08:27:27.837Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "6e43a7b2-2389-44cd-bd4e-e9ba404159e3",
      "number": 18,
      "content": "Nếu lòng tôi chú về tội ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.851Z",
      "updatedAt": "2025-04-22T08:27:27.851Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "87b9e6a7-d195-4c65-8a0a-474e013b0098",
      "number": 18,
      "content": "Ắt Chúa chẳng nghe tôi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.864Z",
      "updatedAt": "2025-04-22T08:27:27.864Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "135226e6-c4cc-4ed7-a246-844bfd76260e",
      "number": 19,
      "content": "Nhưng Ðức Chúa Trời thật có nghe;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.878Z",
      "updatedAt": "2025-04-22T08:27:27.878Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "0a0a1101-12b5-4422-835a-ba4ad34dc4ca",
      "number": 19,
      "content": "Ngài đã lắng nghe tiếng cầu nguyện của tôi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.891Z",
      "updatedAt": "2025-04-22T08:27:27.891Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "ec8cab11-b045-4719-82ba-df8b3cd743a3",
      "number": 20,
      "content": "Tôn ngợi Ðức Chúa Trời, Ðấng chẳng bỏ qua lời cầu nguyện của tôi,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.905Z",
      "updatedAt": "2025-04-22T08:27:27.905Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    },
    {
      "id": "feff0018-2554-42a7-93d7-58bcea0ad5f1",
      "number": 20,
      "content": "Và không rút lại tình thương của Ngài đối với tôi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:27.918Z",
      "updatedAt": "2025-04-22T08:27:27.918Z",
      "chapterId": "3ae186ca-446c-4280-afb9-e9038ecb3b70"
    }
  ],
