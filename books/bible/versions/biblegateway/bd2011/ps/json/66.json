{
  "chapter": {
    "id": "00a2a838-7945-476c-b6ba-edb67700af11",
    "number": 66,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2066&version=BD2011",
    "createdAt": "2025-04-24T03:03:16.507Z",
    "updatedAt": "2025-04-24T03:03:16.507Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "ef5927a9-4257-4700-989d-dd3ef4634191",
      "number": 1,
      "content": "Hỡi muôn loài trên đất,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.379Z",
      "updatedAt": "2025-04-24T03:55:15.379Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "5bb2e3d6-80c6-48ca-a7dc-d794220a5d7d",
      "number": 1,
      "content": "Hãy cất tiếng reo mừng dâng lên Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.417Z",
      "updatedAt": "2025-04-24T03:55:15.417Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "e8bd6cfd-ebb2-4ccb-ac85-62a33e6d4557",
      "number": 2,
      "content": "Hãy ca ngợi danh vinh hiển của Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.433Z",
      "updatedAt": "2025-04-24T03:55:15.433Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "59bab037-63a0-4961-88ee-a59cadfd1d96",
      "number": 2,
      "content": "Hãy tôn vinh ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.442Z",
      "updatedAt": "2025-04-24T03:55:15.442Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "e9c0da73-7b0f-4a8d-a68f-49161190ff6f",
      "number": 3,
      "content": "Hãy thưa cùng Ðức Chúa Trời rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.452Z",
      "updatedAt": "2025-04-24T03:55:15.452Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "bad0fdd2-c303-44f2-9b28-1402e40a5d3a",
      "number": 3,
      "content": "“Những công việc của Ngài thật đáng kinh hãi thay!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.462Z",
      "updatedAt": "2025-04-24T03:55:15.462Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "4365a697-2dec-42c9-9572-a2666219c573",
      "number": 3,
      "content": "Bởi quyền năng lớn lao của Ngài,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.472Z",
      "updatedAt": "2025-04-24T03:55:15.472Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "529666a8-8bc9-4ebe-893a-41af363bb777",
      "number": 3,
      "content": "Những kẻ thù của Ngài phải phủ phục trước mặt Ngài.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.482Z",
      "updatedAt": "2025-04-24T03:55:15.482Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "21fac9fd-4fa6-4a0d-9f7f-3e1e61896f04",
      "number": 4,
      "content": "Mọi loài trên đất sẽ thờ phượng Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.493Z",
      "updatedAt": "2025-04-24T03:55:15.493Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "e2c4d4b1-50c2-4c52-9743-37d4d788f2e0",
      "number": 4,
      "content": "Chúng sẽ hát ca tôn ngợi Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.503Z",
      "updatedAt": "2025-04-24T03:55:15.503Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "eda56fa8-4a59-4a8a-be2e-c2b27ee5e2d8",
      "number": 4,
      "content": "Chúng sẽ ca hát tôn vinh danh Ngài.” *(Sê-la)*",
      "order": 2,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.512Z",
      "updatedAt": "2025-04-24T03:55:15.512Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "fe1fe0db-4a50-4c59-9b53-0ad8ccd62032",
      "number": 5,
      "content": "Hãy đến và nhìn xem những công việc của Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.523Z",
      "updatedAt": "2025-04-24T03:55:15.523Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "d7a8d8d1-f1e1-458c-94a7-33c48fe516ae",
      "number": 5,
      "content": "Những gì Ngài làm cho loài người thật đáng kinh hãi thay!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.533Z",
      "updatedAt": "2025-04-24T03:55:15.533Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "52be672b-507b-4e29-8e29-66833c4ebad6",
      "number": 6,
      "content": "Ngài biến đại dương thành đất khô;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.543Z",
      "updatedAt": "2025-04-24T03:55:15.543Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "f8a8ea58-c6f1-4783-93da-eeaff1316200",
      "number": 6,
      "content": "Ngài cho họ đi bộ vượt qua dòng nước mạnh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.553Z",
      "updatedAt": "2025-04-24T03:55:15.553Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "20a8b213-6d12-4a06-9c7e-b7bb35afc791",
      "number": 6,
      "content": "Hãy đến, chúng ta hãy vui mừng trong Ngài!",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.563Z",
      "updatedAt": "2025-04-24T03:55:15.563Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "9c163e2b-4b10-4294-89cb-4a19de261e11",
      "number": 7,
      "content": "Ngài dùng quyền năng của Ngài trị vì đời đời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.573Z",
      "updatedAt": "2025-04-24T03:55:15.573Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "ace54b99-203b-43bd-88a2-87a3dd9ca90a",
      "number": 7,
      "content": "Mắt Ngài nhìn xem các nước,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.584Z",
      "updatedAt": "2025-04-24T03:55:15.584Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "29067256-7384-48e1-93f5-616186715901",
      "number": 7,
      "content": "Kẻo những kẻ phản loạn nổi lên gây rối chăng. *(Sê-la)*",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.595Z",
      "updatedAt": "2025-04-24T03:55:15.595Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "ea21e4d0-ee03-462f-94cc-81a2aaa0040f",
      "number": 8,
      "content": "Hỡi các dân, hãy ca ngợi Ðức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.605Z",
      "updatedAt": "2025-04-24T03:55:15.605Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "c01f9a4e-7efb-4bf2-ac61-2c6eaaf9c219",
      "number": 8,
      "content": "Hãy làm cho âm thanh ca ngợi Ngài vang ra khắp chốn,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.615Z",
      "updatedAt": "2025-04-24T03:55:15.615Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "387ada76-4006-475d-99c7-13002818a17a",
      "number": 9,
      "content": "Vì Ngài giữ gìn mạng sống chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.627Z",
      "updatedAt": "2025-04-24T03:55:15.627Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "b9393c4f-e466-46e3-b58b-3cd72246e837",
      "number": 9,
      "content": "Ngài không để cho chân chúng ta trợt ngã.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.637Z",
      "updatedAt": "2025-04-24T03:55:15.637Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "7c96d5d5-9558-4270-9b92-1411e662168a",
      "number": 10,
      "content": "Thật vậy Ðức Chúa Trời ôi, Ngài đã thử thách chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.647Z",
      "updatedAt": "2025-04-24T03:55:15.647Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "98a9a8ab-fdf1-4b3a-b77e-51d8926883a8",
      "number": 10,
      "content": "Ngài đã rèn luyện chúng con như người ta luyện bạc.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.657Z",
      "updatedAt": "2025-04-24T03:55:15.657Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "e07741d8-7374-4731-bc15-8ef89f5e5e7d",
      "number": 11,
      "content": "Ngài đã để cho chúng con bị sa vào lưới;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.667Z",
      "updatedAt": "2025-04-24T03:55:15.667Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "c72624f9-3ce0-46b8-b4bf-c79f058ee4bd",
      "number": 11,
      "content": "Ngài đã để gánh nặng bị áp bức đè nặng trên lưng chúng con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.677Z",
      "updatedAt": "2025-04-24T03:55:15.677Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "5ed33d78-a033-4f20-8f42-46cb81d33223",
      "number": 12,
      "content": "Ngài đã để người ta cỡi trên đầu trên cổ chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.687Z",
      "updatedAt": "2025-04-24T03:55:15.687Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "1024709f-5467-4f3a-874b-96de3a25df74",
      "number": 12,
      "content": "Chúng con phải đi qua lửa và qua nước;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.697Z",
      "updatedAt": "2025-04-24T03:55:15.697Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "cc196c64-b129-4f2d-a79a-5e3394eabf33",
      "number": 12,
      "content": "Nhưng rồi Ngài đã đem chúng con ra khỏi,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.707Z",
      "updatedAt": "2025-04-24T03:55:15.707Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "b2232e9c-cbbe-4162-accc-35800e77f629",
      "number": 12,
      "content": "Ðể đưa chúng con vào một nơi trù phú.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.717Z",
      "updatedAt": "2025-04-24T03:55:15.717Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "246721d3-a615-40bf-9b72-b50b3b42335d",
      "number": 13,
      "content": "Con sẽ vào nhà Ngài với những của lễ thiêu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.727Z",
      "updatedAt": "2025-04-24T03:55:15.727Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "8cc21661-eff6-45d8-8ba3-5d8b0a5fcb89",
      "number": 13,
      "content": "Con sẽ trả xong cho Ngài những gì con đã hứa nguyện,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.737Z",
      "updatedAt": "2025-04-24T03:55:15.737Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "2df11bd9-f684-4689-89b3-92f839e89fe7",
      "number": 14,
      "content": "Những gì môi con đã hứa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.747Z",
      "updatedAt": "2025-04-24T03:55:15.747Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "cd72454c-e023-4300-ad76-0ba387c05a49",
      "number": 14,
      "content": "Và miệng con đã nói ra trong lúc bị khốn khổ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.757Z",
      "updatedAt": "2025-04-24T03:55:15.757Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "069b9257-f5ac-49ac-a13b-cd44e13b2ff5",
      "number": 15,
      "content": "Con sẽ dâng lên Ngài những con vật hiến tế béo tốt để làm của lễ thiêu, chung với mỡ của các chiên đực;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.770Z",
      "updatedAt": "2025-04-24T03:55:15.770Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "fd013963-b09a-49ac-a0fa-001b976fd2ba",
      "number": 15,
      "content": "Con sẽ dâng các bò đực và các dê đực. *(Sê-la)*",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.780Z",
      "updatedAt": "2025-04-24T03:55:15.780Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "26530e6c-19ed-428d-9ec9-1b77fa3e49f7",
      "number": 16,
      "content": "Hỡi mọi người kính sợ Ðức Chúa Trời, xin đến nghe,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.790Z",
      "updatedAt": "2025-04-24T03:55:15.790Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "d2c9e7a8-5806-479e-954c-43daf316b6f8",
      "number": 16,
      "content": "Tôi sẽ thuật lại những gì Ngài đã làm cho tôi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.803Z",
      "updatedAt": "2025-04-24T03:55:15.803Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "72727058-d693-488e-8842-5c62304dc37c",
      "number": 17,
      "content": "Miệng tôi đã kêu cầu Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.813Z",
      "updatedAt": "2025-04-24T03:55:15.813Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "f69e7451-dd0d-4fb3-a7fe-0e61da8dfa83",
      "number": 17,
      "content": "Và lưỡi tôi đã tôn vinh Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.823Z",
      "updatedAt": "2025-04-24T03:55:15.823Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "6091a661-6cfb-4f47-a70a-c0939e83cd50",
      "number": 18,
      "content": "Nếu lòng tôi chú về tội ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.833Z",
      "updatedAt": "2025-04-24T03:55:15.833Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "6b886926-4d81-4547-8b88-51d5cde2ede1",
      "number": 18,
      "content": "Ắt Chúa chẳng nghe tôi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.843Z",
      "updatedAt": "2025-04-24T03:55:15.843Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "ec6bd456-5008-4579-b8e0-e469dadd0abb",
      "number": 19,
      "content": "Nhưng Ðức Chúa Trời thật có nghe;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.860Z",
      "updatedAt": "2025-04-24T03:55:15.860Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "c8919d62-91d8-42a4-a6d5-48afba5b34e4",
      "number": 19,
      "content": "Ngài đã lắng nghe tiếng cầu nguyện của tôi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.873Z",
      "updatedAt": "2025-04-24T03:55:15.873Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "6b1a2d0a-bd18-4ac1-abb2-dbb8fd5182e9",
      "number": 20,
      "content": "Tôn ngợi Ðức Chúa Trời, Ðấng chẳng bỏ qua lời cầu nguyện của tôi,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.887Z",
      "updatedAt": "2025-04-24T03:55:15.887Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    },
    {
      "id": "412ad644-f62b-4892-bf3a-7969e7c4c071",
      "number": 20,
      "content": "Và không rút lại tình thương của Ngài đối với tôi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:15.900Z",
      "updatedAt": "2025-04-24T03:55:15.900Z",
      "chapterId": "00a2a838-7945-476c-b6ba-edb67700af11"
    }
  ],
