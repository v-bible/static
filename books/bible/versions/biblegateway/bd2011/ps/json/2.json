    {
      "id": "23d1c3d4-c318-40ee-ad58-db08568161a9",
      "content": "Sự Trị Vì của Ðấng Ðược Xức Dầu",
      "order": 0,
      "createdAt": "2024-09-24T01:51:25.004Z",
      "updatedAt": "2024-09-24T01:51:25.004Z",
