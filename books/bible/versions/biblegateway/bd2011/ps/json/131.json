  },
  "verses": [
    {
      "id": "ff94cd34-2a9c-4736-b190-500c7fba2f79",
      "number": 1,
      "content": "Chúa ôi, lòng con không dám tự cao;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:36.973Z",
      "updatedAt": "2025-04-24T04:03:36.973Z",
      "chapterId": "aa2f4630-87c7-4570-91d7-2fcfb953b2e5"
    },
    {
      "id": "9f04f0c2-6dca-4b69-aa40-d69ed64582bf",
      "number": 1,
      "content": "Mắt con chẳng dám kiêu kỳ;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:37.006Z",
      "updatedAt": "2025-04-24T04:03:37.006Z",
      "chapterId": "aa2f4630-87c7-4570-91d7-2fcfb953b2e5"
    },
    {
      "id": "8e0f41fe-5991-4e06-adaf-a1bdcce4a33d",
      "number": 1,
      "content": "Con không dám dự vào những việc lớn lao quá sức,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:37.019Z",
      "updatedAt": "2025-04-24T04:03:37.019Z",
      "chapterId": "aa2f4630-87c7-4570-91d7-2fcfb953b2e5"
    },
    {
      "id": "84f32921-5c77-43c2-ae3d-13e5ca176e2c",
      "number": 1,
      "content": "Hay tham dự vào những việc vượt quá khả năng mình.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:37.031Z",
      "updatedAt": "2025-04-24T04:03:37.031Z",
      "chapterId": "aa2f4630-87c7-4570-91d7-2fcfb953b2e5"
    },
    {
      "id": "ec96872b-cb90-4e95-bebb-b0268df86876",
      "number": 2,
      "content": "Nhưng con đã tự trấn tĩnh và làm cho linh hồn mình lắng dịu,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:37.043Z",
      "updatedAt": "2025-04-24T04:03:37.043Z",
      "chapterId": "aa2f4630-87c7-4570-91d7-2fcfb953b2e5"
    },
    {
      "id": "8a784c27-a18c-487b-ac2c-349151747be5",
      "number": 2,
      "content": "Như trẻ thơ no sữa nép mình bên lòng mẹ;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:37.056Z",
      "updatedAt": "2025-04-24T04:03:37.056Z",
      "chapterId": "aa2f4630-87c7-4570-91d7-2fcfb953b2e5"
    },
    {
      "id": "2676f44f-842c-42b5-8289-6532d84f122f",
      "number": 2,
      "content": "Linh hồn con như một em bé vừa no sữa ở trong con.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:37.068Z",
      "updatedAt": "2025-04-24T04:03:37.068Z",
      "chapterId": "aa2f4630-87c7-4570-91d7-2fcfb953b2e5"
    },
    {
      "id": "a349623c-49cb-4c8a-98ce-04d516b4e5c5",
      "number": 3,
      "content": "Hỡi I-sơ-ra-ên, hãy hy vọng nơi Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:37.080Z",
      "updatedAt": "2025-04-24T04:03:37.080Z",
      "chapterId": "aa2f4630-87c7-4570-91d7-2fcfb953b2e5"
    },
    {
      "id": "07eafc56-24bf-4a1f-937c-a00fd8a454a7",
      "number": 3,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:37.092Z",
      "updatedAt": "2025-04-24T04:03:37.092Z",
      "chapterId": "aa2f4630-87c7-4570-91d7-2fcfb953b2e5"
    }
  ],
