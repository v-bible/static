  },
  "verses": [
    {
      "id": "99534bec-1d5d-4e53-805c-2edb750e38a1",
      "number": 1,
      "content": "Chúa ôi, lòng con không dám tự cao;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:19.943Z",
      "updatedAt": "2025-04-22T08:35:19.943Z",
      "chapterId": "d7c33865-e61c-4881-85d8-ffb79db135aa"
    },
    {
      "id": "58627794-864a-45b8-9581-3d28f55e9c13",
      "number": 1,
      "content": "Mắt con chẳng dám kiêu kỳ;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:19.963Z",
      "updatedAt": "2025-04-22T08:35:19.963Z",
      "chapterId": "d7c33865-e61c-4881-85d8-ffb79db135aa"
    },
    {
      "id": "f73cd4f3-4f89-4566-86bc-45b5f6f45e82",
      "number": 1,
      "content": "Con không dám dự vào những việc lớn lao quá sức,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:19.974Z",
      "updatedAt": "2025-04-22T08:35:19.974Z",
      "chapterId": "d7c33865-e61c-4881-85d8-ffb79db135aa"
    },
    {
      "id": "21ebbda8-fa3f-472b-a43d-1ce068af3edb",
      "number": 1,
      "content": "Hay tham dự vào những việc vượt quá khả năng mình.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:19.985Z",
      "updatedAt": "2025-04-22T08:35:19.985Z",
      "chapterId": "d7c33865-e61c-4881-85d8-ffb79db135aa"
    },
    {
      "id": "f4c1e020-226c-4472-91e8-f63b0da9d0fe",
      "number": 2,
      "content": "Nhưng con đã tự trấn tĩnh và làm cho linh hồn mình lắng dịu,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:19.996Z",
      "updatedAt": "2025-04-22T08:35:19.996Z",
      "chapterId": "d7c33865-e61c-4881-85d8-ffb79db135aa"
    },
    {
      "id": "23886d0f-1255-4cdb-ab75-135e35aa696a",
      "number": 2,
      "content": "Như trẻ thơ no sữa nép mình bên lòng mẹ;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:20.009Z",
      "updatedAt": "2025-04-22T08:35:20.009Z",
      "chapterId": "d7c33865-e61c-4881-85d8-ffb79db135aa"
    },
    {
      "id": "1e316c3b-c8cd-4cbf-8e75-f4230ee99044",
      "number": 2,
      "content": "Linh hồn con như một em bé vừa no sữa ở trong con.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:20.022Z",
      "updatedAt": "2025-04-22T08:35:20.022Z",
      "chapterId": "d7c33865-e61c-4881-85d8-ffb79db135aa"
    },
    {
      "id": "af7d6638-df3f-4caf-8503-1149514e494a",
      "number": 3,
      "content": "Hỡi I-sơ-ra-ên, hãy hy vọng nơi Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:20.035Z",
      "updatedAt": "2025-04-22T08:35:20.035Z",
      "chapterId": "d7c33865-e61c-4881-85d8-ffb79db135aa"
    },
    {
      "id": "5eaa019a-9700-42e0-a8e7-2628000a2329",
      "number": 3,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:20.048Z",
      "updatedAt": "2025-04-22T08:35:20.048Z",
      "chapterId": "d7c33865-e61c-4881-85d8-ffb79db135aa"
    }
  ],
