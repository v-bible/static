{
  "chapter": {
    "id": "aa2f4630-87c7-4570-91d7-2fcfb953b2e5",
    "number": 131,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20131&version=BD2011",
    "createdAt": "2025-04-24T03:03:18.202Z",
