      "chapterId": "ff0e35fc-3ea7-44c1-9cc3-50cb7ea2c894"
    },
    {
      "id": "8ca0494f-6097-43c8-9100-0efa5f83ab7f",
      "content": "ctd: tên bay đạn réo ban ngày",
      "position": 22,
      "order": 2,
      "createdAt": "2025-04-24T03:58:23.126Z",
      "updatedAt": "2025-04-24T03:58:23.126Z",
      "verseId": "bdc3e04b-ab80-4b83-b866-6c6b25bf87c4",
      "chapterId": "ff0e35fc-3ea7-44c1-9cc3-50cb7ea2c894"
    },
