      "position": 47,
      "order": 1,
      "createdAt": "2025-04-22T08:30:33.547Z",
      "updatedAt": "2025-04-22T08:30:33.547Z",
      "verseId": "f1e636ad-fc49-45a3-ae2b-1cf6fed8545e",
      "chapterId": "646f6d05-3a43-4e91-b08b-a02b61189101"
    },
    {
      "id": "1c9ac6c9-898d-4a26-968f-2462d1307409",
      "content": "ctd: tên bay đạn réo ban ngày",
      "position": 22,
      "order": 2,
      "createdAt": "2025-04-22T08:30:33.576Z",
      "updatedAt": "2025-04-22T08:30:33.576Z",
      "verseId": "ab5c35db-fe6c-492c-ae2c-d249f34bd05a",
      "chapterId": "646f6d05-3a43-4e91-b08b-a02b61189101"
    },
    {
      "id": "4cc7e96f-ebca-4b76-9936-f42b6234f55f",
