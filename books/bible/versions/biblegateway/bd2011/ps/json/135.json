{
  "chapter": {
    "id": "58f07aa0-943e-4523-bad5-158c8f251e28",
    "number": 135,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20135&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.883Z",
    "updatedAt": "2025-04-22T07:37:48.883Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "f0144946-4b78-4aea-bbc7-959986e485d5",
      "number": 1,
      "content": "Hãy ca ngợi Chúa! Hãy ca ngợi danh Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.565Z",
      "updatedAt": "2025-04-22T08:35:46.565Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "0fa0292f-47f5-4739-9b62-e010901c80a8",
      "number": 1,
      "content": "Hỡi các tôi tớ Chúa, hãy ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.592Z",
      "updatedAt": "2025-04-22T08:35:46.592Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "7944b130-b2ea-4083-bfba-b8377ff5615d",
      "number": 2,
      "content": "Hỡi các bạn, những người đứng trong nhà Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.602Z",
      "updatedAt": "2025-04-22T08:35:46.602Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "b49b8244-be98-49f2-9940-75c1363a2bbb",
      "number": 2,
      "content": "Những người đứng trong sân đền thờ Ðức Chúa Trời chúng ta,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.613Z",
      "updatedAt": "2025-04-22T08:35:46.613Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "66263a5d-0d0c-4c16-bcc2-de9dcdde47e7",
      "number": 3,
      "content": "Hãy ca ngợi Chúa, vì Chúa thật tốt;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.623Z",
      "updatedAt": "2025-04-22T08:35:46.623Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "f5f1dac4-00b7-4dd3-aacf-26dae1e4a8c2",
      "number": 3,
      "content": "Hãy ca tụng danh Ngài, vì danh Ngài đáng kính yêu;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.635Z",
      "updatedAt": "2025-04-22T08:35:46.635Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "206b9cf6-7490-4357-ac86-13f479bf3326",
      "number": 4,
      "content": "Vì Chúa đã chọn Gia-cốp cho Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.648Z",
      "updatedAt": "2025-04-22T08:35:46.648Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "20cf5d75-a33a-4efb-9d70-b73d0c89de52",
      "number": 4,
      "content": "I-sơ-ra-ên làm tài sản riêng của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.662Z",
      "updatedAt": "2025-04-22T08:35:46.662Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "a8aba65d-340d-4960-a0ad-cd1622efde8c",
      "number": 5,
      "content": "Vì tôi biết rằng Chúa thật vĩ đại;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.678Z",
      "updatedAt": "2025-04-22T08:35:46.678Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "6e325009-c4f7-4294-8f39-2186002e3ff1",
      "number": 5,
      "content": "Chúa của chúng ta vĩ đại hơn tất cả thần thánh của loài người.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.695Z",
      "updatedAt": "2025-04-22T08:35:46.695Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "577e0871-193c-4427-9927-bd0b73f4c67d",
      "number": 6,
      "content": "Bất cứ điều gì Chúa muốn, Ngài làm điều ấy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.709Z",
      "updatedAt": "2025-04-22T08:35:46.709Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "f0e94c68-443e-41a5-ab1f-b36f8951cb61",
      "number": 6,
      "content": "Bất kể trên trời hay dưới đất, trong lòng biển hay trong các vực sâu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.722Z",
      "updatedAt": "2025-04-22T08:35:46.722Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "024a9b7a-4899-49bf-b55a-079769896d57",
      "number": 7,
      "content": "Ngài khiến mây trồi lên từ những nơi tận cùng trái đất;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.735Z",
      "updatedAt": "2025-04-22T08:35:46.735Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "28f5851f-1a4c-497b-b8ec-89aca2da9a35",
      "number": 7,
      "content": "Ngài làm cho sấm sét nổ vang khi trời đổ cơn mưa;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.747Z",
      "updatedAt": "2025-04-22T08:35:46.747Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "2a04c197-19be-440d-8bfb-bea8950f008c",
      "number": 7,
      "content": "Ngài sai gió bay ra từ các kho lẫm của Ngài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.763Z",
      "updatedAt": "2025-04-22T08:35:46.763Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "48f80408-d9b3-4f73-8e60-27f458777867",
      "number": 8,
      "content": "Ngài đánh hạ các con đầu lòng của Ai-cập,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.773Z",
      "updatedAt": "2025-04-22T08:35:46.773Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "c3db1a1b-80ec-4124-b88e-084071ae8a0f",
      "number": 8,
      "content": "Con của loài người và con của súc vật.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.784Z",
      "updatedAt": "2025-04-22T08:35:46.784Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "047a8408-75c3-461b-a4ec-c4f10ec6ca10",
      "number": 9,
      "content": "Hỡi Ai-cập, Ngài sai các dấu kỳ và phép lạ đến giữa ngươi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.796Z",
      "updatedAt": "2025-04-22T08:35:46.796Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "c1aa16e8-5151-4dda-9506-ec4ffa6dbeb5",
      "number": 9,
      "content": "Ðể trừng phạt Pha-ra-ôn và các tôi tớ của ông ta.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.810Z",
      "updatedAt": "2025-04-22T08:35:46.810Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "22398d10-ece3-421e-963d-de38ed4b63f2",
      "number": 10,
      "content": "Ngài đánh bại nhiều dân tộc,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.823Z",
      "updatedAt": "2025-04-22T08:35:46.823Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "160e1271-ae09-45db-b74a-1e956f2461fe",
      "number": 10,
      "content": "Và tiêu diệt các vua mạnh mẽ;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.839Z",
      "updatedAt": "2025-04-22T08:35:46.839Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "682710be-3a77-4c63-a2e1-a77bfd75159e",
      "number": 11,
      "content": "Si-hôn vua dân A-mô-ri, Óc vua Ba-san,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.850Z",
      "updatedAt": "2025-04-22T08:35:46.850Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "25a042a6-ce4b-428c-8dd0-bdbaedde0154",
      "number": 11,
      "content": "Và tất cả các vương quốc ở Ca-na-an.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.869Z",
      "updatedAt": "2025-04-22T08:35:46.869Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "0ca5dfec-be25-4709-a894-de70378e065f",
      "number": 12,
      "content": "Ngài ban xứ của họ để làm sản nghiệp,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.879Z",
      "updatedAt": "2025-04-22T08:35:46.879Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "fed71823-efd5-4c92-bde9-3810354157b6",
      "number": 12,
      "content": "Một sản nghiệp cho dân I-sơ-ra-ên của Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.889Z",
      "updatedAt": "2025-04-22T08:35:46.889Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "16ccc9c9-7c82-42db-acb7-9af7ff79325c",
      "number": 13,
      "content": "Chúa ôi, danh Ngài sẽ trường tồn mãi mãi;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.899Z",
      "updatedAt": "2025-04-22T08:35:46.899Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "a88e07c5-c682-462f-bcab-98bfeaeb3590",
      "number": 13,
      "content": "Chúa ôi, công ơn Ngài sẽ được ghi nhớ từ đời nọ đến đời kia,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.912Z",
      "updatedAt": "2025-04-22T08:35:46.912Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "ac5420fc-8e49-4e14-8f54-4c6eec5cc666",
      "number": 14,
      "content": "Vì Chúa sẽ xét đoán dân Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.925Z",
      "updatedAt": "2025-04-22T08:35:46.925Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "97a3f939-e793-47b0-b402-5d894de9cfa3",
      "number": 14,
      "content": "Ngài sẽ tỏ lòng thương xót đối với các tôi tớ Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.938Z",
      "updatedAt": "2025-04-22T08:35:46.938Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "ba2a01d6-891e-49b9-a853-dde4e6cd56ba",
      "number": 15,
      "content": "Các thần tượng của các dân chẳng qua là bạc và vàng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.951Z",
      "updatedAt": "2025-04-22T08:35:46.951Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "20701765-3640-498f-9c29-9900d78cb11c",
      "number": 15,
      "content": "Chúng chỉ là sản phẩm do tay người làm ra.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.965Z",
      "updatedAt": "2025-04-22T08:35:46.965Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "bae9cde7-9187-43b3-9675-ad41a5163161",
      "number": 16,
      "content": "Chúng có miệng mà chẳng nói;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.979Z",
      "updatedAt": "2025-04-22T08:35:46.979Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "1ca51a96-25e0-44aa-8745-9716905113c5",
      "number": 16,
      "content": "Chúng có mắt mà chẳng thấy;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:46.992Z",
      "updatedAt": "2025-04-22T08:35:46.992Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "7596bfde-1f88-4a7b-9698-616ebd9b24bb",
      "number": 17,
      "content": "Chúng có tai mà chẳng nghe;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:47.006Z",
      "updatedAt": "2025-04-22T08:35:47.006Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "3cb4d17d-7ff7-423c-9908-b2d2349db393",
      "number": 17,
      "content": "Không có chút hơi thở nào trong miệng chúng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:47.022Z",
      "updatedAt": "2025-04-22T08:35:47.022Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "ac36f472-dce9-459b-9e1a-f737d3970a42",
      "number": 18,
      "content": "Nguyện những kẻ làm thần tượng sẽ trở nên như chúng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:47.036Z",
      "updatedAt": "2025-04-22T08:35:47.036Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "410691d3-6d07-4851-b93f-3757a751f802",
      "number": 18,
      "content": "Và những ai tin cậy vào thần tượng cũng giống như vậy.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:47.052Z",
      "updatedAt": "2025-04-22T08:35:47.052Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "1266c280-446e-4410-8cfc-0e4adf00d973",
      "number": 19,
      "content": "Hỡi nhà I-sơ-ra-ên, hãy chúc tụng Chúa!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:47.066Z",
      "updatedAt": "2025-04-22T08:35:47.066Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "6de1b974-51b8-47f2-af70-b25c0aa8f871",
      "number": 19,
      "content": "Hỡi nhà A-rôn, hãy chúc tụng Chúa!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:47.079Z",
      "updatedAt": "2025-04-22T08:35:47.079Z",
      "chapterId": "58f07aa0-943e-4523-bad5-158c8f251e28"
    },
    {
      "id": "f2f4c971-2e73-44ce-bfae-009350ff0ca8",
      "number": 20,
      "content": "Hỡi nhà Lê-vi, hãy chúc tụng Chúa!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:47.096Z",
