{
  "chapter": {
    "id": "03f58947-7efb-48b0-87c9-841bb5bc1548",
    "number": 103,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20103&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.496Z",
    "updatedAt": "2025-04-24T03:03:17.496Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "dcef0b81-332c-48f3-8426-c19a267dadd0",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.404Z",
      "updatedAt": "2025-04-24T03:59:55.404Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "de63f9e7-d5d0-4aef-bb38-b055a3d14d57",
      "number": 1,
      "content": "Hỡi tất cả những gì ở trong ta, hãy ca tụng danh thánh của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.432Z",
      "updatedAt": "2025-04-24T03:59:55.432Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "b4433ac4-5030-420b-8dd8-3cd82e19d28c",
      "number": 2,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.445Z",
      "updatedAt": "2025-04-24T03:59:55.445Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "d1522cca-7b73-48de-939c-24a370929db7",
      "number": 2,
      "content": "Chớ quên mọi phước hạnh của Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.455Z",
      "updatedAt": "2025-04-24T03:59:55.455Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "79349f17-f801-438e-9a2d-a3eee2b7e946",
      "number": 3,
      "content": "Ðấng tha thứ mọi tội lỗi ngươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.464Z",
      "updatedAt": "2025-04-24T03:59:55.464Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "bea5380f-2d81-465f-b3d1-bfac4b0e2e76",
      "number": 3,
      "content": "Ðấng chữa lành mọi bịnh tật ngươi;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.475Z",
      "updatedAt": "2025-04-24T03:59:55.475Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "b18346c0-0014-47db-85d6-a4c1ce7efe21",
      "number": 4,
      "content": "Ðấng cứu chuộc mạng sống ngươi thoát khỏi mộ phần;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.486Z",
      "updatedAt": "2025-04-24T03:59:55.486Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "2369ffc5-849f-4a53-9447-f2587de3006c",
      "number": 4,
      "content": "Ðấng đội trên đầu ngươi tình thương và lòng thương xót;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.505Z",
      "updatedAt": "2025-04-24T03:59:55.505Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "b4750c62-5946-4c03-a66c-1f8ece7667aa",
      "number": 5,
      "content": "Ðấng cho ngươi no thỏa với thức ăn ngon,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.518Z",
      "updatedAt": "2025-04-24T03:59:55.518Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "58ca7c78-2338-40c8-817d-2ffe75d55b94",
      "number": 5,
      "content": "Nhờ đó sức lực ngươi trẻ lại như sức của phượng hoàng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.532Z",
      "updatedAt": "2025-04-24T03:59:55.532Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "fe4fb2e9-3cfb-433c-a7df-6213f7abb013",
      "number": 6,
      "content": "Chúa thi hành sự công chính",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.545Z",
      "updatedAt": "2025-04-24T03:59:55.545Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "e32b2bfa-d28c-4eec-97fd-96c20f09d6b0",
      "number": 6,
      "content": "Và công lý cho mọi người bị áp bức.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.558Z",
      "updatedAt": "2025-04-24T03:59:55.558Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "dbaca123-c11c-4bfb-9bf9-e68a695f8d9a",
      "number": 7,
      "content": "Ngài bày tỏ cho Môi-se các đường lối Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.570Z",
      "updatedAt": "2025-04-24T03:59:55.570Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "573f4343-4a60-4425-91a5-b7620fd224bf",
      "number": 7,
      "content": "Cho dân I-sơ-ra-ên các công việc Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.582Z",
      "updatedAt": "2025-04-24T03:59:55.582Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "60d5f914-5ea3-4dc6-9655-f43080de420f",
      "number": 8,
      "content": "Chúa hay thương xót và khoan dung độ lượng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.598Z",
      "updatedAt": "2025-04-24T03:59:55.598Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "33022d2b-f9b5-48ab-a0f5-489b6fbee44a",
      "number": 8,
      "content": "Chậm nóng giận và đầy dẫy tình thương.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.614Z",
      "updatedAt": "2025-04-24T03:59:55.614Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "19e3bca3-66d4-48a4-83cf-d0c65e7cdbb3",
      "number": 9,
      "content": "Ngài không bắt tội luôn luôn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.626Z",
      "updatedAt": "2025-04-24T03:59:55.626Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "96d8f0d7-24f2-4cb4-ba96-6d45cf6a74e2",
      "number": 9,
      "content": "Ngài cũng chẳng giữ lòng giận đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.639Z",
      "updatedAt": "2025-04-24T03:59:55.639Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "5e314a52-1efc-4a86-85b1-8532775956e8",
      "number": 10,
      "content": "Ngài không đối xử với chúng ta theo các tội của chúng ta;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.652Z",
      "updatedAt": "2025-04-24T03:59:55.652Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "515f6637-de78-4873-86af-301749fa95ad",
      "number": 10,
      "content": "Ngài cũng không báo trả chúng ta theo các lỗi của chúng ta.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.664Z",
      "updatedAt": "2025-04-24T03:59:55.664Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "8b758d21-8449-4d16-88c2-48becb0f7b7c",
      "number": 11,
      "content": "Vì các tầng trời cao hơn đất bao nhiêu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.677Z",
      "updatedAt": "2025-04-24T03:59:55.677Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "ed6feeee-1ae3-44a9-9998-96d91c2ec677",
      "number": 11,
      "content": "Tình thương của Ngài đối với những người kính sợ Ngài cũng cao lớn bấy nhiêu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.690Z",
      "updatedAt": "2025-04-24T03:59:55.690Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "e45e4d72-2bfe-4e3b-9c97-0900cc2c70f7",
      "number": 12,
      "content": "Phương đông xa cách phương tây bao nhiêu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.703Z",
      "updatedAt": "2025-04-24T03:59:55.703Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "c4635a10-fd86-4e69-a3ec-c3dcd0e75aff",
      "number": 12,
      "content": "Ngài đem những vi phạm của chúng ta xa khỏi chúng ta cũng bấy nhiêu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.715Z",
      "updatedAt": "2025-04-24T03:59:55.715Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "5d189895-64f7-4048-a116-10c45fe1bc3e",
      "number": 13,
      "content": "Như người cha thương xót các con mình thể nào,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.727Z",
      "updatedAt": "2025-04-24T03:59:55.727Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "dc13540e-79c3-4289-820f-47e4c97abc11",
      "number": 13,
      "content": "Chúa thương xót những người kính sợ Ngài cũng thể ấy.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.739Z",
      "updatedAt": "2025-04-24T03:59:55.739Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "417c350e-1d5c-47c7-82c9-51b73184a182",
      "number": 14,
      "content": "Vì Ngài biết rõ chúng ta được dựng nên bằng những gì;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.752Z",
      "updatedAt": "2025-04-24T03:59:55.752Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "33d57eb0-ec2e-47c6-b96b-65055b7ed2ca",
      "number": 14,
      "content": "Ngài nhớ lại rằng chúng ta là bụi đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.765Z",
      "updatedAt": "2025-04-24T03:59:55.765Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "572761bd-e5fc-4c55-825e-2061cd6430b0",
      "number": 15,
      "content": "Về loài người, đời người như ngọn cỏ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.778Z",
      "updatedAt": "2025-04-24T03:59:55.778Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "a586b89b-c375-4002-acd9-17856ec40cf4",
      "number": 15,
      "content": "Nó thắm tươi như hoa dại ngoài đồng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.790Z",
      "updatedAt": "2025-04-24T03:59:55.790Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "09c402fa-311d-4a95-9b0f-990e23587e4a",
      "number": 16,
      "content": "Một cơn gió thổi qua, kìa, nó chẳng còn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.806Z",
      "updatedAt": "2025-04-24T03:59:55.806Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "191d6401-8b8c-4f1a-9358-7e4f9313d362",
      "number": 16,
      "content": "Chỗ nó mọc lên cũng không còn biết nó nữa.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.819Z",
      "updatedAt": "2025-04-24T03:59:55.819Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "62f26fe9-6880-4d8b-bddf-78368886fb73",
      "number": 17,
      "content": "Nhưng tình thương của Chúa thì còn mãi mãi cho những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.832Z",
      "updatedAt": "2025-04-24T03:59:55.832Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "794ee0f5-5323-472a-8437-6cd182bf01a8",
      "number": 17,
      "content": "Và đức công chính Ngài dành cho con cháu chắt chít họ,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.844Z",
      "updatedAt": "2025-04-24T03:59:55.844Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "a5c03043-a361-4daf-b5b9-c94c20a302ad",
      "number": 18,
      "content": "Tức cho những người giữ giao ước Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.857Z",
      "updatedAt": "2025-04-24T03:59:55.857Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "8a0ac086-ce4f-4b73-9aa1-cfe7acdd8d8b",
      "number": 18,
      "content": "Và cho những người nhớ đến các điều răn Ngài mà làm theo.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.869Z",
      "updatedAt": "2025-04-24T03:59:55.869Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "c1c75d0e-c62d-484a-83cd-bffab950ebb3",
      "number": 19,
      "content": "Chúa thiết lập ngai Ngài trên các tầng trời;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.882Z",
      "updatedAt": "2025-04-24T03:59:55.882Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "84dd0449-198b-43f7-a1b8-8252f23babac",
      "number": 19,
      "content": "Vương quyền Ngài bao trùm cả vũ trụ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.894Z",
      "updatedAt": "2025-04-24T03:59:55.894Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "0e627c01-f99d-4c41-917e-4d2299722d7b",
      "number": 20,
      "content": "Hãy ca ngợi Chúa, hỡi các thiên sứ của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.907Z",
      "updatedAt": "2025-04-24T03:59:55.907Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "808a5764-787e-479a-a59d-4c2ca9c750ad",
      "number": 20,
      "content": "Tức những đấng dũng mãnh siêu phàm,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.923Z",
      "updatedAt": "2025-04-24T03:59:55.923Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "b57704c7-e8c9-4598-8ada-0e8c4a009c13",
      "number": 20,
      "content": "Những đấng thi hành mệnh lệnh Ngài,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.936Z",
      "updatedAt": "2025-04-24T03:59:55.936Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "754c50fa-58de-4f21-8dfe-21f080685fe2",
      "number": 20,
      "content": "Những đấng vâng theo lời Ngài!",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.949Z",
      "updatedAt": "2025-04-24T03:59:55.949Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "5fa74bf1-d132-4f62-a10e-6b71347c3391",
      "number": 21,
      "content": "Hãy ca ngợi Chúa, hỡi tất cả các đạo quân của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.965Z",
      "updatedAt": "2025-04-24T03:59:55.965Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "d984ee48-497f-4596-a91c-039ad6b0f959",
      "number": 21,
      "content": "Tức các đấng phục vụ Ngài và làm theo thánh ý Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.978Z",
      "updatedAt": "2025-04-24T03:59:55.978Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "0f79f9a9-2085-478d-939e-0d05a6eba5fd",
      "number": 22,
      "content": "Hãy ca ngợi Chúa, hỡi tất cả các tạo vật của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:55.990Z",
      "updatedAt": "2025-04-24T03:59:55.990Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "bc216399-db0b-4c87-b2d7-68d86f9ea53d",
      "number": 22,
      "content": "Ở mọi nơi dưới quyền trị vì của Ngài!",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:56.009Z",
      "updatedAt": "2025-04-24T03:59:56.009Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    },
    {
      "id": "129be1d4-a4e9-449b-912b-cd92cca279dc",
      "number": 22,
      "content": "Hãy ca ngợi Chúa, hỡi linh hồn ta!",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:56.025Z",
      "updatedAt": "2025-04-24T03:59:56.025Z",
      "chapterId": "03f58947-7efb-48b0-87c9-841bb5bc1548"
    }
  ],
