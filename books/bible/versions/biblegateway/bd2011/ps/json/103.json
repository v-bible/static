{
  "chapter": {
    "id": "9bc689aa-57fb-4dfc-8a84-114d210d04e5",
    "number": 103,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20103&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.054Z",
    "updatedAt": "2025-04-22T07:37:48.054Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "67b2bb5d-b55a-4c04-954a-d1b4ca525228",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.799Z",
      "updatedAt": "2025-04-22T08:31:57.799Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "c2273818-7b32-4174-b153-d02cf47386d0",
      "number": 1,
      "content": "Hỡi tất cả những gì ở trong ta, hãy ca tụng danh thánh của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.822Z",
      "updatedAt": "2025-04-22T08:31:57.822Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "d68518b4-2f54-4b08-a8ae-1e39b9da7d22",
      "number": 2,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.832Z",
      "updatedAt": "2025-04-22T08:31:57.832Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "136f31d1-7a73-47d6-bcd0-fcdef36eddf7",
      "number": 2,
      "content": "Chớ quên mọi phước hạnh của Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.842Z",
      "updatedAt": "2025-04-22T08:31:57.842Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "3386167a-189b-44e8-b5be-820a23cdeae7",
      "number": 3,
      "content": "Ðấng tha thứ mọi tội lỗi ngươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.852Z",
      "updatedAt": "2025-04-22T08:31:57.852Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "0f4fb046-29d4-4a36-8610-cccd2e287f62",
      "number": 3,
      "content": "Ðấng chữa lành mọi bịnh tật ngươi;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.862Z",
      "updatedAt": "2025-04-22T08:31:57.862Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "477a25bd-b1b6-4164-b770-b17d7fae6466",
      "number": 4,
      "content": "Ðấng cứu chuộc mạng sống ngươi thoát khỏi mộ phần;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.872Z",
      "updatedAt": "2025-04-22T08:31:57.872Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "09a1c492-4879-4049-8eba-97b13a4b1caf",
      "number": 4,
      "content": "Ðấng đội trên đầu ngươi tình thương và lòng thương xót;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.882Z",
      "updatedAt": "2025-04-22T08:31:57.882Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "1eec8db3-40d9-4f32-892b-53561cb29f38",
      "number": 5,
      "content": "Ðấng cho ngươi no thỏa với thức ăn ngon,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.892Z",
      "updatedAt": "2025-04-22T08:31:57.892Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "1bdcfd73-2a15-4d5f-898d-313dee63bc98",
      "number": 5,
      "content": "Nhờ đó sức lực ngươi trẻ lại như sức của phượng hoàng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.905Z",
      "updatedAt": "2025-04-22T08:31:57.905Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "7efd0166-dbfa-4d7f-8275-859b8258566b",
      "number": 6,
      "content": "Chúa thi hành sự công chính",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.914Z",
      "updatedAt": "2025-04-22T08:31:57.914Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "f42dea4f-4e49-4e38-afbc-2f5a122299bf",
      "number": 6,
      "content": "Và công lý cho mọi người bị áp bức.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.926Z",
      "updatedAt": "2025-04-22T08:31:57.926Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "8485b1e0-7fbd-4979-a0f9-e34c3bec8983",
      "number": 7,
      "content": "Ngài bày tỏ cho Môi-se các đường lối Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.939Z",
      "updatedAt": "2025-04-22T08:31:57.939Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "f9f32963-aa55-40a8-a39f-d0cdbca08922",
      "number": 7,
      "content": "Cho dân I-sơ-ra-ên các công việc Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.949Z",
      "updatedAt": "2025-04-22T08:31:57.949Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "a0c81e12-8b65-47b5-9b62-6a2d58992293",
      "number": 8,
      "content": "Chúa hay thương xót và khoan dung độ lượng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.960Z",
      "updatedAt": "2025-04-22T08:31:57.960Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "7407149e-a56f-4cce-8789-6f1093fb3401",
      "number": 8,
      "content": "Chậm nóng giận và đầy dẫy tình thương.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.970Z",
      "updatedAt": "2025-04-22T08:31:57.970Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "db640b17-9c84-429f-94df-30c98359a296",
      "number": 9,
      "content": "Ngài không bắt tội luôn luôn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.983Z",
      "updatedAt": "2025-04-22T08:31:57.983Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "b0493e83-7ac2-40eb-99ed-58b4ceb16e8c",
      "number": 9,
      "content": "Ngài cũng chẳng giữ lòng giận đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:57.993Z",
      "updatedAt": "2025-04-22T08:31:57.993Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "dfe03c0e-c589-4c65-a8ff-95a758c4f3f6",
      "number": 10,
      "content": "Ngài không đối xử với chúng ta theo các tội của chúng ta;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.007Z",
      "updatedAt": "2025-04-22T08:31:58.007Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "b1064e33-f368-4e8d-8f4b-7ef631875914",
      "number": 10,
      "content": "Ngài cũng không báo trả chúng ta theo các lỗi của chúng ta.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.017Z",
      "updatedAt": "2025-04-22T08:31:58.017Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "c28a3642-d3be-4a2c-87e8-427c8a6a6d5d",
      "number": 11,
      "content": "Vì các tầng trời cao hơn đất bao nhiêu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.027Z",
      "updatedAt": "2025-04-22T08:31:58.027Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "29246442-a1d4-48d5-acab-245babec8e98",
      "number": 11,
      "content": "Tình thương của Ngài đối với những người kính sợ Ngài cũng cao lớn bấy nhiêu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.040Z",
      "updatedAt": "2025-04-22T08:31:58.040Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "a0d3451b-facb-4a4b-9f5a-e42bb7736cc8",
      "number": 12,
      "content": "Phương đông xa cách phương tây bao nhiêu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.057Z",
      "updatedAt": "2025-04-22T08:31:58.057Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "5c155525-d24f-455c-b0f0-2e90643b6b7e",
      "number": 12,
      "content": "Ngài đem những vi phạm của chúng ta xa khỏi chúng ta cũng bấy nhiêu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.070Z",
      "updatedAt": "2025-04-22T08:31:58.070Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "7190af8b-7f19-4dfe-a042-a06beda2db00",
      "number": 13,
      "content": "Như người cha thương xót các con mình thể nào,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.083Z",
      "updatedAt": "2025-04-22T08:31:58.083Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "dec022f7-f187-47a0-bd1c-c148bba76bca",
      "number": 13,
      "content": "Chúa thương xót những người kính sợ Ngài cũng thể ấy.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.093Z",
      "updatedAt": "2025-04-22T08:31:58.093Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "eeb33af2-2daa-42c9-9ce5-cc72e4ca041f",
      "number": 14,
      "content": "Vì Ngài biết rõ chúng ta được dựng nên bằng những gì;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.103Z",
      "updatedAt": "2025-04-22T08:31:58.103Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "d4daed97-6ea4-4046-941c-9e5c0a951c15",
      "number": 14,
      "content": "Ngài nhớ lại rằng chúng ta là bụi đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.117Z",
      "updatedAt": "2025-04-22T08:31:58.117Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "fc65dd48-bea2-4af0-a14a-c3e11c65e915",
      "number": 15,
      "content": "Về loài người, đời người như ngọn cỏ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.127Z",
      "updatedAt": "2025-04-22T08:31:58.127Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "b2f78fce-bd0e-4285-81eb-dfe2dd10bce1",
      "number": 15,
      "content": "Nó thắm tươi như hoa dại ngoài đồng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.140Z",
      "updatedAt": "2025-04-22T08:31:58.140Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "3d1389c9-be35-4c8e-82c1-109b3564de08",
      "number": 16,
      "content": "Một cơn gió thổi qua, kìa, nó chẳng còn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.150Z",
      "updatedAt": "2025-04-22T08:31:58.150Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "913c24ce-7557-4a66-8f62-e41d653c02eb",
      "number": 16,
      "content": "Chỗ nó mọc lên cũng không còn biết nó nữa.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.160Z",
      "updatedAt": "2025-04-22T08:31:58.160Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "49431da6-97b4-405c-8dbd-aa10624cef93",
      "number": 17,
      "content": "Nhưng tình thương của Chúa thì còn mãi mãi cho những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.174Z",
      "updatedAt": "2025-04-22T08:31:58.174Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "5605a9cf-c558-4a75-9ae5-d4a007b12850",
      "number": 17,
      "content": "Và đức công chính Ngài dành cho con cháu chắt chít họ,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.183Z",
      "updatedAt": "2025-04-22T08:31:58.183Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "8dc96f84-3889-45d2-ad32-67b495e439a8",
      "number": 18,
      "content": "Tức cho những người giữ giao ước Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.193Z",
      "updatedAt": "2025-04-22T08:31:58.193Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "272bd2a9-ddd9-4f04-8aa8-e8cc1f674171",
      "number": 18,
      "content": "Và cho những người nhớ đến các điều răn Ngài mà làm theo.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.204Z",
      "updatedAt": "2025-04-22T08:31:58.204Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "dd73a5fa-7c83-4a07-a58a-d615aa442d66",
      "number": 19,
      "content": "Chúa thiết lập ngai Ngài trên các tầng trời;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.214Z",
      "updatedAt": "2025-04-22T08:31:58.214Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "116a2d5e-5cbb-49cc-b1bd-68c8d40ee614",
      "number": 19,
      "content": "Vương quyền Ngài bao trùm cả vũ trụ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.224Z",
      "updatedAt": "2025-04-22T08:31:58.224Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "99677bcf-8280-45bb-8f62-94b60732bf3a",
      "number": 20,
      "content": "Hãy ca ngợi Chúa, hỡi các thiên sứ của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.234Z",
      "updatedAt": "2025-04-22T08:31:58.234Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "05ffb260-784a-4471-8588-dd19a6c0ba24",
      "number": 20,
      "content": "Tức những đấng dũng mãnh siêu phàm,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.250Z",
      "updatedAt": "2025-04-22T08:31:58.250Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "2e6ea870-a49c-4bc8-b391-d258d05cbf45",
      "number": 20,
      "content": "Những đấng thi hành mệnh lệnh Ngài,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.259Z",
      "updatedAt": "2025-04-22T08:31:58.259Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "da0b5ae0-31d2-4060-b0fd-7d1b09fd1a9a",
      "number": 20,
      "content": "Những đấng vâng theo lời Ngài!",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.272Z",
      "updatedAt": "2025-04-22T08:31:58.272Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "4f1493a4-4f67-4cda-a347-d1efdc03d775",
      "number": 21,
      "content": "Hãy ca ngợi Chúa, hỡi tất cả các đạo quân của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.282Z",
      "updatedAt": "2025-04-22T08:31:58.282Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "bb9cd60c-a747-495f-a2f5-7c0c1d9b9d66",
      "number": 21,
      "content": "Tức các đấng phục vụ Ngài và làm theo thánh ý Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.292Z",
      "updatedAt": "2025-04-22T08:31:58.292Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "13302ec7-bf62-4841-af44-45240da8648a",
      "number": 22,
      "content": "Hãy ca ngợi Chúa, hỡi tất cả các tạo vật của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.302Z",
      "updatedAt": "2025-04-22T08:31:58.302Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "4906d1ab-a8a9-4648-9b75-57b0ff3be3ea",
      "number": 22,
      "content": "Ở mọi nơi dưới quyền trị vì của Ngài!",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.312Z",
      "updatedAt": "2025-04-22T08:31:58.312Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    },
    {
      "id": "049e0693-c0fd-435d-96a7-7069d74edcd4",
      "number": 22,
      "content": "Hãy ca ngợi Chúa, hỡi linh hồn ta!",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:58.322Z",
      "updatedAt": "2025-04-22T08:31:58.322Z",
      "chapterId": "9bc689aa-57fb-4dfc-8a84-114d210d04e5"
    }
  ],
