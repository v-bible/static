{
  "chapter": {
    "id": "bd283c31-ea6a-4fe1-8574-ae9f40542d70",
    "number": 103,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20103&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.457Z",
    "updatedAt": "2024-09-24T01:00:44.457Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "f27ad679-f4f5-487e-baa9-e22e79ede4f9",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.885Z",
      "updatedAt": "2024-09-24T02:07:03.885Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "a7df5334-f824-4272-82e7-2f1a8b3aa020",
      "number": 1,
      "content": "Hỡi tất cả những gì ở trong ta, hãy ca tụng danh thánh của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.889Z",
      "updatedAt": "2024-09-24T02:07:03.889Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "34ce8f21-c96c-46b5-a1d7-ccaeb0483da5",
      "number": 2,
      "content": "Hỡi linh hồn ta, hãy ca ngợi Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.891Z",
      "updatedAt": "2024-09-24T02:07:03.891Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "0afe01a2-df90-49ca-88b9-2dd2e657440f",
      "number": 2,
      "content": "Chớ quên mọi phước hạnh của Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.893Z",
      "updatedAt": "2024-09-24T02:07:03.893Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "01ae7baa-f0ab-48d7-93f2-6d7af1d7a1aa",
      "number": 3,
      "content": "Ðấng tha thứ mọi tội lỗi ngươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.895Z",
      "updatedAt": "2024-09-24T02:07:03.895Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "761e0f08-a24c-4350-abe5-8258b0405c3e",
      "number": 3,
      "content": "Ðấng chữa lành mọi bịnh tật ngươi;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.897Z",
      "updatedAt": "2024-09-24T02:07:03.897Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "e30ef6fa-c947-4493-81fe-0c6a48bdefef",
      "number": 4,
      "content": "Ðấng cứu chuộc mạng sống ngươi thoát khỏi mộ phần;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.898Z",
      "updatedAt": "2024-09-24T02:07:03.898Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "6729ee0f-6f41-4cb7-8aa9-56c96d347725",
      "number": 4,
      "content": "Ðấng đội trên đầu ngươi tình thương và lòng thương xót;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.900Z",
      "updatedAt": "2024-09-24T02:07:03.900Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "47b52c96-0f4a-4b4c-b758-9df7273d1b63",
      "number": 5,
      "content": "Ðấng cho ngươi no thỏa với thức ăn ngon,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.902Z",
      "updatedAt": "2024-09-24T02:07:03.902Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "7728af64-f81f-4c98-b6a0-679317d2cb80",
      "number": 5,
      "content": "Nhờ đó sức lực ngươi trẻ lại như sức của phượng hoàng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.904Z",
      "updatedAt": "2024-09-24T02:07:03.904Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "3a6de702-ebbc-4f45-ab70-4c0a512efcba",
      "number": 6,
      "content": "Chúa thi hành sự công chính",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.906Z",
      "updatedAt": "2024-09-24T02:07:03.906Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "cb98d009-dca9-4874-a894-ed5209f690c1",
      "number": 6,
      "content": "Và công lý cho mọi người bị áp bức.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.910Z",
      "updatedAt": "2024-09-24T02:07:03.910Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "7b32e89c-5f2c-400a-89bb-cf450a94fa9e",
      "number": 7,
      "content": "Ngài bày tỏ cho Môi-se các đường lối Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.912Z",
      "updatedAt": "2024-09-24T02:07:03.912Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "a1f17e7a-02be-42ba-acf7-ae9c46f01f98",
      "number": 7,
      "content": "Cho dân I-sơ-ra-ên các công việc Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.914Z",
      "updatedAt": "2024-09-24T02:07:03.914Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "86900e4c-1f69-49e9-acba-dbe65d0231dc",
      "number": 8,
      "content": "Chúa hay thương xót và khoan dung độ lượng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.916Z",
      "updatedAt": "2024-09-24T02:07:03.916Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "521959cb-3f5a-43c1-a4fb-338f0427bf90",
      "number": 8,
      "content": "Chậm nóng giận và đầy dẫy tình thương.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.918Z",
      "updatedAt": "2024-09-24T02:07:03.918Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "c9b2ac98-ef02-4a94-b383-c3b78cd819a2",
      "number": 9,
      "content": "Ngài không bắt tội luôn luôn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.920Z",
      "updatedAt": "2024-09-24T02:07:03.920Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "688c6f22-837c-4090-a8ea-9264a16fdc0f",
      "number": 9,
      "content": "Ngài cũng chẳng giữ lòng giận đến đời đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.922Z",
      "updatedAt": "2024-09-24T02:07:03.922Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "94acf930-18d5-4a93-bec2-31003361c839",
      "number": 10,
      "content": "Ngài không đối xử với chúng ta theo các tội của chúng ta;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.925Z",
      "updatedAt": "2024-09-24T02:07:03.925Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "26539ffc-acc7-4ea4-856c-786b84de0bfe",
      "number": 10,
      "content": "Ngài cũng không báo trả chúng ta theo các lỗi của chúng ta.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.930Z",
      "updatedAt": "2024-09-24T02:07:03.930Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "df394c7a-e1ad-4b4d-b475-cbac0cc4bdeb",
      "number": 11,
      "content": "Vì các tầng trời cao hơn đất bao nhiêu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.935Z",
      "updatedAt": "2024-09-24T02:07:03.935Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "46ad202b-67cb-486b-b10d-43330ef212ad",
      "number": 11,
      "content": "Tình thương của Ngài đối với những người kính sợ Ngài cũng cao lớn bấy nhiêu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.940Z",
      "updatedAt": "2024-09-24T02:07:03.940Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "acd80507-b8f9-4ad3-8848-560448521d2c",
      "number": 12,
      "content": "Phương đông xa cách phương tây bao nhiêu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.945Z",
      "updatedAt": "2024-09-24T02:07:03.945Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "4734c03a-ab03-463e-8ca3-493b200e51ea",
      "number": 12,
      "content": "Ngài đem những vi phạm của chúng ta xa khỏi chúng ta cũng bấy nhiêu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.950Z",
      "updatedAt": "2024-09-24T02:07:03.950Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "207a4641-6c47-4f8f-b8fe-a03ef0bef932",
      "number": 13,
      "content": "Như người cha thương xót các con mình thể nào,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.955Z",
      "updatedAt": "2024-09-24T02:07:03.955Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "800b79b3-23d2-4f90-9449-8f79944f8201",
      "number": 13,
      "content": "Chúa thương xót những người kính sợ Ngài cũng thể ấy.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.960Z",
      "updatedAt": "2024-09-24T02:07:03.960Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "94aea4e5-1ff4-44a4-943c-f407b23de347",
      "number": 14,
      "content": "Vì Ngài biết rõ chúng ta được dựng nên bằng những gì;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.964Z",
      "updatedAt": "2024-09-24T02:07:03.964Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "5bd5d37f-bec1-4337-addc-64770cf0a40e",
      "number": 14,
      "content": "Ngài nhớ lại rằng chúng ta là bụi đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.969Z",
      "updatedAt": "2024-09-24T02:07:03.969Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "1b7999c1-d6c9-4ac2-b320-d22dea7a3c06",
      "number": 15,
      "content": "Về loài người, đời người như ngọn cỏ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.974Z",
      "updatedAt": "2024-09-24T02:07:03.974Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "0ef0db42-2dc6-4ea2-adef-19571e96a2f5",
      "number": 15,
      "content": "Nó thắm tươi như hoa dại ngoài đồng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.979Z",
      "updatedAt": "2024-09-24T02:07:03.979Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "f5b0808a-c365-42de-b88a-061bd50d0a9d",
      "number": 16,
      "content": "Một cơn gió thổi qua, kìa, nó chẳng còn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.984Z",
      "updatedAt": "2024-09-24T02:07:03.984Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "d1245da4-4143-49f1-8a9c-6eaf9e3f72e3",
      "number": 16,
      "content": "Chỗ nó mọc lên cũng không còn biết nó nữa.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.988Z",
      "updatedAt": "2024-09-24T02:07:03.988Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "8db07d08-935b-4f6c-8663-3d174c18bbd3",
      "number": 17,
      "content": "Nhưng tình thương của Chúa thì còn mãi mãi cho những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.993Z",
      "updatedAt": "2024-09-24T02:07:03.993Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "e2fb52dd-0eb5-4b2a-a450-5beeafb0cdf8",
      "number": 17,
      "content": "Và đức công chính Ngài dành cho con cháu chắt chít họ,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:03.998Z",
      "updatedAt": "2024-09-24T02:07:03.998Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "81c46079-6692-4b8a-bab7-e6719281068f",
      "number": 18,
      "content": "Tức cho những người giữ giao ước Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.003Z",
      "updatedAt": "2024-09-24T02:07:04.003Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "1fb85886-ebe4-4514-a238-2f99230be252",
      "number": 18,
      "content": "Và cho những người nhớ đến các điều răn Ngài mà làm theo.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.008Z",
      "updatedAt": "2024-09-24T02:07:04.008Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "f3c1335e-76f6-438b-b4f7-e3b4483616bc",
      "number": 19,
      "content": "Chúa thiết lập ngai Ngài trên các tầng trời;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.012Z",
      "updatedAt": "2024-09-24T02:07:04.012Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "36762b1a-5a87-442c-870c-201d29ea6c2b",
      "number": 19,
      "content": "Vương quyền Ngài bao trùm cả vũ trụ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.017Z",
      "updatedAt": "2024-09-24T02:07:04.017Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "5d38cf0e-0041-48aa-b14a-c44185d865e8",
      "number": 20,
      "content": "Hãy ca ngợi Chúa, hỡi các thiên sứ của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.024Z",
      "updatedAt": "2024-09-24T02:07:04.024Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "0d113b92-ef52-4674-a408-8fc8c150db8f",
      "number": 20,
      "content": "Tức những đấng dũng mãnh siêu phàm,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.028Z",
      "updatedAt": "2024-09-24T02:07:04.028Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "ea44574c-d398-4c88-a6a7-7cd657ee7f9c",
      "number": 20,
      "content": "Những đấng thi hành mệnh lệnh Ngài,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.033Z",
      "updatedAt": "2024-09-24T02:07:04.033Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "c05922b3-8eec-407a-b29f-cc5d50394b8a",
      "number": 20,
      "content": "Những đấng vâng theo lời Ngài!",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.039Z",
      "updatedAt": "2024-09-24T02:07:04.039Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "fec753f7-3680-4149-b88f-430aaaeaf5fd",
      "number": 21,
      "content": "Hãy ca ngợi Chúa, hỡi tất cả các đạo quân của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.043Z",
      "updatedAt": "2024-09-24T02:07:04.043Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "e280bcf5-c033-4235-b4de-cc64c51cf688",
      "number": 21,
      "content": "Tức các đấng phục vụ Ngài và làm theo thánh ý Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.048Z",
      "updatedAt": "2024-09-24T02:07:04.048Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "5163d200-0c10-41b3-8c28-b69c24586e39",
      "number": 22,
      "content": "Hãy ca ngợi Chúa, hỡi tất cả các tạo vật của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.053Z",
      "updatedAt": "2024-09-24T02:07:04.053Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "d1738b04-3bf0-4378-866b-da4084d0ad61",
      "number": 22,
      "content": "Ở mọi nơi dưới quyền trị vì của Ngài!",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.059Z",
      "updatedAt": "2024-09-24T02:07:04.059Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    },
    {
      "id": "28081684-2175-4d2e-a622-c8fefd4881c7",
      "number": 22,
      "content": "Hãy ca ngợi Chúa, hỡi linh hồn ta!",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:07:04.063Z",
      "updatedAt": "2024-09-24T02:07:04.063Z",
      "chapterId": "bd283c31-ea6a-4fe1-8574-ae9f40542d70"
    }
  ],
