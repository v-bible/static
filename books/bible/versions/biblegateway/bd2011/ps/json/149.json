{
  "chapter": {
    "id": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5",
    "number": 149,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20149&version=BD2011",
    "createdAt": "2025-04-22T07:37:49.269Z",
    "updatedAt": "2025-04-22T07:37:49.269Z",
