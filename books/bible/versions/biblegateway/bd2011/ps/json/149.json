{
  "chapter": {
    "id": "09e206d8-3df1-456f-ab80-aa5c9774c42a",
    "number": 149,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20149&version=BD2011",
    "createdAt": "2025-04-24T03:03:18.670Z",
    "updatedAt": "2025-04-24T03:03:18.670Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "7c03f563-8572-4177-92bd-22e811f63fc1",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.232Z",
      "updatedAt": "2025-04-24T04:05:45.232Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "954a8100-4040-49b9-b733-80808a8e2013",
      "number": 1,
      "content": "Hãy ca ngợi Chúa một bài ca mới.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.260Z",
      "updatedAt": "2025-04-24T04:05:45.260Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "f19de247-156c-44ad-b80e-5f8704867af6",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong đại hội của các thánh dân.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.270Z",
      "updatedAt": "2025-04-24T04:05:45.270Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "cf0c5c43-8bbc-4448-8e9d-2880d0dcfa9f",
      "number": 2,
      "content": "Nguyện I-sơ-ra-ên vui mừng nơi Ðấng tạo nên mình.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.280Z",
      "updatedAt": "2025-04-24T04:05:45.280Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "2e9bbe0b-a26e-48fc-90d9-e1b9fb3f7576",
      "number": 2,
      "content": "Nguyện dân cư của Si-ôn vui vẻ nơi Vua mình.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.290Z",
      "updatedAt": "2025-04-24T04:05:45.290Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "0b6381b7-558b-4c4e-89d1-54f9bfdb91a7",
      "number": 3,
      "content": "Nguyện họ nhảy múa ca ngợi danh Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.302Z",
      "updatedAt": "2025-04-24T04:05:45.302Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "04aef60b-38f5-4d93-8bc9-c60cf10153ba",
      "number": 3,
      "content": "Nguyện họ dùng trống và đàn ca tụng Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.315Z",
      "updatedAt": "2025-04-24T04:05:45.315Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "9a7d1e1d-3957-4c6f-a068-f3e3dfa0889b",
      "number": 4,
      "content": "Vì Chúa vui nơi con dân Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.329Z",
      "updatedAt": "2025-04-24T04:05:45.329Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "bb8f5d43-c936-46e4-a76f-5ea932b103ee",
      "number": 4,
      "content": "Ngài điểm tô cho người khiêm nhường bằng ơn cứu rỗi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.342Z",
      "updatedAt": "2025-04-24T04:05:45.342Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "6b771ba5-995a-4bad-83f6-aa5df66da253",
      "number": 5,
      "content": "Nguyện các thánh đồ vui mừng trong vinh hạnh ấy.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.355Z",
      "updatedAt": "2025-04-24T04:05:45.355Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "af0b32d1-c72f-45a7-8bfb-ad3836beb500",
      "number": 5,
      "content": "Nguyện họ hát ca ngay cả khi ở trên giường.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.368Z",
      "updatedAt": "2025-04-24T04:05:45.368Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "4e4b355e-c222-4e41-a20d-9de8c40dba80",
      "number": 6,
      "content": "Nguyện những lời ca ngợi Ðức Chúa Trời ở nơi miệng họ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.384Z",
      "updatedAt": "2025-04-24T04:05:45.384Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "00eaae9d-f588-4f9d-a195-d7f4428e9bae",
      "number": 6,
      "content": "Và nguyện một thanh gươm hai lưỡi ở trong tay họ,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.400Z",
      "updatedAt": "2025-04-24T04:05:45.400Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "602353cd-32b4-4ea6-9a62-c5529d961f35",
      "number": 7,
      "content": "Ðể họ báo thù các nước,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.413Z",
      "updatedAt": "2025-04-24T04:05:45.413Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "cb0ab2ff-55f4-4ae3-b953-bdcc0fd5e819",
      "number": 7,
      "content": "Và trừng phạt các dân,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.423Z",
      "updatedAt": "2025-04-24T04:05:45.423Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "d5970071-b3d3-455d-b288-a0ed79054dfd",
      "number": 8,
      "content": "Ðể trói các vua của chúng bằng xích,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.434Z",
      "updatedAt": "2025-04-24T04:05:45.434Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "28d1bd92-b0f0-4695-9932-34ff90b2629c",
      "number": 8,
      "content": "Và những kẻ quyền quý của chúng bằng xiềng sắt,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.444Z",
      "updatedAt": "2025-04-24T04:05:45.444Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "8ecae20c-5931-4f7c-84cc-1be3bafa5da5",
      "number": 9,
      "content": "Ðể thi hành án phạt đã ghi sẵn cho chúng.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.458Z",
      "updatedAt": "2025-04-24T04:05:45.458Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "505c7bf9-9222-4f41-8589-e5f697388dc7",
      "number": 9,
      "content": "Ðó là niềm vinh dự cho tất cả thánh đồ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.468Z",
      "updatedAt": "2025-04-24T04:05:45.468Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    },
    {
      "id": "c24cb48e-9eaa-4ef7-94be-5cdc2d64aaa3",
      "number": 9,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:45.478Z",
      "updatedAt": "2025-04-24T04:05:45.478Z",
      "chapterId": "09e206d8-3df1-456f-ab80-aa5c9774c42a"
    }
  ],
