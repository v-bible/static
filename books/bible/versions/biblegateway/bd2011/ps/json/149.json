  },
  "verses": [
    {
      "id": "abe63889-0cee-480d-84cf-413734051a11",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.302Z",
      "updatedAt": "2025-04-22T08:37:24.302Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "a2190b40-3773-484f-9a31-3a4bd95ea3ac",
      "number": 9,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.564Z",
      "updatedAt": "2025-04-22T08:37:24.564Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "5957e6f0-c70a-48b2-8bf7-5461258b2054",
      "number": 1,
      "content": "Hãy ca ngợi Chúa một bài ca mới.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.329Z",
      "updatedAt": "2025-04-22T08:37:24.329Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "e56e18e7-978f-4be3-a124-30c4d37c656f",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong đại hội của các thánh dân.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.339Z",
      "updatedAt": "2025-04-22T08:37:24.339Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "fe18cb57-be01-4bc5-870c-9c72fdb32540",
      "number": 2,
      "content": "Nguyện I-sơ-ra-ên vui mừng nơi Ðấng tạo nên mình.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.350Z",
      "updatedAt": "2025-04-22T08:37:24.350Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "dcdd22fb-ab45-42de-a0e6-99c8defe7c47",
      "number": 2,
      "content": "Nguyện dân cư của Si-ôn vui vẻ nơi Vua mình.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.360Z",
      "updatedAt": "2025-04-22T08:37:24.360Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "3ec9e705-dc0c-4cbd-a76c-77fd74321084",
      "number": 3,
      "content": "Nguyện họ nhảy múa ca ngợi danh Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.373Z",
      "updatedAt": "2025-04-22T08:37:24.373Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "19a5edc2-46df-4e34-9042-e10c7dfb699c",
      "number": 3,
      "content": "Nguyện họ dùng trống và đàn ca tụng Ngài,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.392Z",
      "updatedAt": "2025-04-22T08:37:24.392Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "0016a720-b3f9-459c-a1f0-68ae43cfc24b",
      "number": 4,
      "content": "Vì Chúa vui nơi con dân Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.405Z",
      "updatedAt": "2025-04-22T08:37:24.405Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "c6461c74-a34c-479d-84fb-1ccba49c5480",
      "number": 4,
      "content": "Ngài điểm tô cho người khiêm nhường bằng ơn cứu rỗi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.418Z",
      "updatedAt": "2025-04-22T08:37:24.418Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "fc39087c-a6d6-4e0e-b1f2-766abba697ce",
      "number": 5,
      "content": "Nguyện các thánh đồ vui mừng trong vinh hạnh ấy.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.431Z",
      "updatedAt": "2025-04-22T08:37:24.431Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "4389f36c-5355-4b7f-b374-bdaf205f6b5b",
      "number": 5,
      "content": "Nguyện họ hát ca ngay cả khi ở trên giường.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.444Z",
      "updatedAt": "2025-04-22T08:37:24.444Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "cff4f748-c536-49ee-8bdb-73da5ca235fd",
      "number": 6,
      "content": "Nguyện những lời ca ngợi Ðức Chúa Trời ở nơi miệng họ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.458Z",
      "updatedAt": "2025-04-22T08:37:24.458Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "b64dd27c-9a05-41dc-b3e5-84efa556abf7",
      "number": 6,
      "content": "Và nguyện một thanh gươm hai lưỡi ở trong tay họ,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.471Z",
      "updatedAt": "2025-04-22T08:37:24.471Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "ca2203b5-3d30-4481-8c9d-a8bfc0d40fca",
      "number": 7,
      "content": "Ðể họ báo thù các nước,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.484Z",
      "updatedAt": "2025-04-22T08:37:24.484Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "7c41308f-e5bd-4cb7-a56c-383557ea9d69",
      "number": 7,
      "content": "Và trừng phạt các dân,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.497Z",
      "updatedAt": "2025-04-22T08:37:24.497Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "6bf2415a-82a9-44d2-bb57-3315df60e776",
      "number": 8,
      "content": "Ðể trói các vua của chúng bằng xích,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.511Z",
      "updatedAt": "2025-04-22T08:37:24.511Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "9d70ac3c-67c2-4e38-926b-bedb9976229c",
      "number": 8,
      "content": "Và những kẻ quyền quý của chúng bằng xiềng sắt,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.524Z",
      "updatedAt": "2025-04-22T08:37:24.524Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "56addd42-fe49-45b3-9568-fc320e2f11f9",
      "number": 9,
      "content": "Ðể thi hành án phạt đã ghi sẵn cho chúng.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.537Z",
      "updatedAt": "2025-04-22T08:37:24.537Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    },
    {
      "id": "784ffbf6-a529-4c19-80ef-b54d3bb55acb",
      "number": 9,
      "content": "Ðó là niềm vinh dự cho tất cả thánh đồ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:24.550Z",
      "updatedAt": "2025-04-22T08:37:24.550Z",
      "chapterId": "cd9f76bc-5ed3-4ae2-87b9-7f9771e737a5"
    }
  ],
