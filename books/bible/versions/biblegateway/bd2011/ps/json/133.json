  },
  "verses": [
    {
      "id": "df228487-2ecc-4922-8d87-d0576d525891",
      "number": 1,
      "content": "Kìa, tốt đẹp thay và phước hạnh thay khi anh chị em sống hòa thuận với nhau!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:33.387Z",
      "updatedAt": "2025-04-22T08:35:33.387Z",
      "chapterId": "f441941e-60bd-44a0-97f9-ee99b2db5f80"
    },
    {
      "id": "72b2eae2-b87c-4353-9895-d80b614bb3f3",
      "number": 2,
      "content": "Ðiều ấy chẳng khác gì dầu quý báu từ trên đầu chảy xuống,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:33.425Z",
      "updatedAt": "2025-04-22T08:35:33.425Z",
      "chapterId": "f441941e-60bd-44a0-97f9-ee99b2db5f80"
    },
    {
      "id": "cb5be68d-66f8-4537-b3b9-552af9d2cd66",
      "number": 2,
      "content": "Chảy xuống râu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:33.435Z",
      "updatedAt": "2025-04-22T08:35:33.435Z",
      "chapterId": "f441941e-60bd-44a0-97f9-ee99b2db5f80"
    },
    {
      "id": "5868f90a-df71-43c5-825e-5471e776646a",
      "number": 2,
      "content": "Chảy xuống râu của A-rôn,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:33.446Z",
      "updatedAt": "2025-04-22T08:35:33.446Z",
      "chapterId": "f441941e-60bd-44a0-97f9-ee99b2db5f80"
    },
    {
      "id": "cf327f54-6925-4089-a8da-b884b1a45856",
      "number": 2,
      "content": "Rồi tiếp tục chảy xuống vạt áo ông.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:33.459Z",
      "updatedAt": "2025-04-22T08:35:33.459Z",
      "chapterId": "f441941e-60bd-44a0-97f9-ee99b2db5f80"
    },
    {
      "id": "37f143f3-92ed-4789-91e0-c375c8649c2a",
      "number": 3,
      "content": "Ðiều ấy giống như sương móc từ Núi Hẹt-môn sa xuống các rặng núi Si-ôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:33.472Z",
      "updatedAt": "2025-04-22T08:35:33.472Z",
      "chapterId": "f441941e-60bd-44a0-97f9-ee99b2db5f80"
    },
    {
      "id": "35515156-198a-4c34-8e85-0641a6a315bb",
      "number": 3,
      "content": "Vì tại đó Chúa ban phước,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:33.488Z",
      "updatedAt": "2025-04-22T08:35:33.488Z",
      "chapterId": "f441941e-60bd-44a0-97f9-ee99b2db5f80"
    },
    {
      "id": "5cb3e3cb-4eac-49f0-9a60-1fd9e80f38cb",
      "number": 3,
      "content": "Phước của sự sống đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:35:33.502Z",
      "updatedAt": "2025-04-22T08:35:33.502Z",
      "chapterId": "f441941e-60bd-44a0-97f9-ee99b2db5f80"
    }
  ],
