  },
  "verses": [
    {
      "id": "6bdbe067-7bf6-41cd-912e-d036fce41a7e",
      "number": 1,
      "content": "Kìa, tốt đẹp thay và phước hạnh thay khi anh chị em sống hòa thuận với nhau!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:51.279Z",
      "updatedAt": "2025-04-24T04:03:51.279Z",
      "chapterId": "7959afc3-dba5-4486-b6ca-a07abdcd8c2b"
    },
    {
      "id": "0b8b5170-f9e0-4b57-b885-047cc2abb132",
      "number": 2,
      "content": "Ðiều ấy chẳng khác gì dầu quý báu từ trên đầu chảy xuống,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:51.320Z",
      "updatedAt": "2025-04-24T04:03:51.320Z",
      "chapterId": "7959afc3-dba5-4486-b6ca-a07abdcd8c2b"
    },
    {
      "id": "67423e1b-07da-487a-a542-e413158c3272",
      "number": 2,
      "content": "Chảy xuống râu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:51.331Z",
      "updatedAt": "2025-04-24T04:03:51.331Z",
      "chapterId": "7959afc3-dba5-4486-b6ca-a07abdcd8c2b"
    },
    {
      "id": "1591f91b-d2ed-4b3c-85d8-bd8eb2ebb56c",
      "number": 2,
      "content": "Chảy xuống râu của A-rôn,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:51.341Z",
      "updatedAt": "2025-04-24T04:03:51.341Z",
      "chapterId": "7959afc3-dba5-4486-b6ca-a07abdcd8c2b"
    },
    {
      "id": "34b4eb69-0f65-4384-bf75-ec6198fa343e",
      "number": 2,
      "content": "Rồi tiếp tục chảy xuống vạt áo ông.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:51.353Z",
      "updatedAt": "2025-04-24T04:03:51.353Z",
      "chapterId": "7959afc3-dba5-4486-b6ca-a07abdcd8c2b"
    },
    {
      "id": "16d48c32-87c1-4c98-b478-5c3a33610a27",
      "number": 3,
      "content": "Ðiều ấy giống như sương móc từ Núi Hẹt-môn sa xuống các rặng núi Si-ôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:51.367Z",
      "updatedAt": "2025-04-24T04:03:51.367Z",
      "chapterId": "7959afc3-dba5-4486-b6ca-a07abdcd8c2b"
    },
    {
      "id": "7fbb76be-c703-4508-a3cd-a992319556b3",
      "number": 3,
      "content": "Vì tại đó Chúa ban phước,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:51.381Z",
      "updatedAt": "2025-04-24T04:03:51.381Z",
      "chapterId": "7959afc3-dba5-4486-b6ca-a07abdcd8c2b"
    },
    {
      "id": "e9eca9e6-2f30-4a47-bc73-04d7e4954a91",
      "number": 3,
      "content": "Phước của sự sống đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:51.394Z",
      "updatedAt": "2025-04-24T04:03:51.394Z",
      "chapterId": "7959afc3-dba5-4486-b6ca-a07abdcd8c2b"
    }
  ],
