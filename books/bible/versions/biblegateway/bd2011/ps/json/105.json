{
  "chapter": {
    "id": "4f943e58-d2eb-4f63-aa55-4e7a410820b5",
    "number": 105,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20105&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.545Z",
    "updatedAt": "2025-04-24T03:03:17.545Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "9c836a60-0019-4193-b5ca-e8ffcb2c7441",
      "number": 1,
      "content": "Hãy cảm tạ Chúa, hãy cầu khẩn danh Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.124Z",
      "updatedAt": "2025-04-24T04:00:16.124Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "5e23a145-623c-4c98-8cc8-5a9df5fa0402",
      "number": 1,
      "content": "Hãy rao truyền cho các dân biết các công việc Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.141Z",
      "updatedAt": "2025-04-24T04:00:16.141Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "7aac3eb1-d3bb-46d4-84bb-6f8e1bba9697",
      "number": 2,
      "content": "Hãy ca ngợi Ngài, hãy ca hát chúc tụng Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.154Z",
      "updatedAt": "2025-04-24T04:00:16.154Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "ad3697d8-9d6e-42f3-8cd5-60d278041f6a",
      "number": 2,
      "content": "Hãy kể ra tất cả việc lạ lùng của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.164Z",
      "updatedAt": "2025-04-24T04:00:16.164Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "a60e213f-d4b8-469f-81bf-f28b1ac718d9",
      "number": 3,
      "content": "Vinh hiển thay cho ai được ở trong danh thánh Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.175Z",
      "updatedAt": "2025-04-24T04:00:16.175Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "a9ad4bd3-d415-47f7-9191-dd84e812a168",
      "number": 3,
      "content": "Nguyện lòng những người tìm kiếm Chúa được vui mừng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.185Z",
      "updatedAt": "2025-04-24T04:00:16.185Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "4c314b2c-fac4-4a0c-ad6f-610d60f2a24a",
      "number": 4,
      "content": "Hãy tìm kiếm Chúa và quyền năng Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.196Z",
      "updatedAt": "2025-04-24T04:00:16.196Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "644520ab-e359-4132-8177-d8c80d733eb9",
      "number": 4,
      "content": "Hãy tìm kiếm mặt Ngài luôn luôn.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.212Z",
      "updatedAt": "2025-04-24T04:00:16.212Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "f78e4a2d-b80b-4900-91bf-4afe2535a5f4",
      "number": 5,
      "content": "Hãy nhớ mãi những việc lạ lùng Ngài làm,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.225Z",
      "updatedAt": "2025-04-24T04:00:16.225Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "2916b2e4-ba96-4d67-9fcf-a3e37b693976",
      "number": 5,
      "content": "Tức những việc diệu kỳ và những phán quyết miệng Ngài đã phán,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.238Z",
      "updatedAt": "2025-04-24T04:00:16.238Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "88964215-5974-4ec4-b387-c26c26118a82",
      "number": 6,
      "content": "Hỡi dòng dõi của Áp-ra-ham, tôi tớ Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.251Z",
      "updatedAt": "2025-04-24T04:00:16.251Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "de9e203e-ea2e-4111-8372-e6df635f16f3",
      "number": 6,
      "content": "Hỡi con cháu của Gia-cốp, những người Ngài chọn.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.269Z",
      "updatedAt": "2025-04-24T04:00:16.269Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "c019da8b-ae64-4994-972b-f22edca163c0",
      "number": 7,
      "content": "Ngài là Chúa, Ðức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.283Z",
      "updatedAt": "2025-04-24T04:00:16.283Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "b6f69834-7b31-4811-b8cf-5caa43036ae7",
      "number": 7,
      "content": "Các phán quyết của Ngài là định luật cho cả địa cầu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.296Z",
      "updatedAt": "2025-04-24T04:00:16.296Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "44e9b8bb-0fb5-4930-9eea-3db4a1365bf7",
      "number": 8,
      "content": "Ngài ghi nhớ giao ước Ngài mãi mãi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.310Z",
      "updatedAt": "2025-04-24T04:00:16.310Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "f7d39355-0deb-4bd3-b099-b1b68cdc8e30",
      "number": 8,
      "content": "Lời Ngài truyền có hiệu lực đến ngàn thế hệ về sau;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.323Z",
      "updatedAt": "2025-04-24T04:00:16.323Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "dfc3f760-275f-4772-a10f-4cbe0fb92bf9",
      "number": 9,
      "content": "Ấy là giao ước Ngài đã lập với Áp-ra-ham,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.337Z",
      "updatedAt": "2025-04-24T04:00:16.337Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "e38ca21b-64ac-4495-bc5d-d80e5d687614",
      "number": 9,
      "content": "Và lời thề Ngài đã thề cùng I-sác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.350Z",
      "updatedAt": "2025-04-24T04:00:16.350Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "3d5fef12-6948-4377-a876-59b1e667d0c2",
      "number": 10,
      "content": "Sau đó Ngài đã xác nhận với Gia-cốp như một đạo luật,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.363Z",
      "updatedAt": "2025-04-24T04:00:16.363Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "abda4c20-b7d3-4c2f-8d23-6c9746702ec0",
      "number": 10,
      "content": "Với I-sơ-ra-ên như một giao ước đời đời,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.376Z",
      "updatedAt": "2025-04-24T04:00:16.376Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "a0f1fc87-f317-47ab-8e01-72b511376c5a",
      "number": 11,
      "content": "Rằng, “Ta sẽ ban xứ Ca-na-an cho các ngươi để làm sản nghiệp,”",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.390Z",
      "updatedAt": "2025-04-24T04:00:16.390Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "54ce01f7-ff72-4595-ad8b-e124acaa8d68",
      "number": 12,
      "content": "Trong khi họ chỉ là một số ít người,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.403Z",
      "updatedAt": "2025-04-24T04:00:16.403Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "ae466828-376d-4a9d-9f0b-0a96e37a5465",
      "number": 12,
      "content": "Một nhóm thiểu số và kiều dân trong xứ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.416Z",
      "updatedAt": "2025-04-24T04:00:16.416Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "e344d5e2-03c3-4ede-b70b-54c0d6b0b788",
      "number": 13,
      "content": "Họ đã đi lang thang từ nước này sang nước khác,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.429Z",
      "updatedAt": "2025-04-24T04:00:16.429Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "cfd98493-25f2-4177-acc9-a401decabbee",
      "number": 13,
      "content": "Từ vương quốc nọ đến vương quốc kia.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.443Z",
      "updatedAt": "2025-04-24T04:00:16.443Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "0ec3caa9-1a2c-43e8-a8b0-8408b5879777",
      "number": 14,
      "content": "Nhưng Ngài đã không cho phép ai hà hiếp họ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.456Z",
      "updatedAt": "2025-04-24T04:00:16.456Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "6f1412f0-7c4d-405c-9611-f1ff5f96bfa2",
      "number": 14,
      "content": "Vì cớ họ Ngài đã quở trách các vua,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.469Z",
      "updatedAt": "2025-04-24T04:00:16.469Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "36c0dd05-5f51-4870-a8dc-1d03af60e09a",
      "number": 15,
      "content": "“Ðừng đụng đến những người được xức dầu của Ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.486Z",
      "updatedAt": "2025-04-24T04:00:16.486Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "9570b918-72c3-4f9a-8c0f-9fe646b6aec6",
      "number": 15,
      "content": "Chớ làm hại các tiên tri của Ta.”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.499Z",
      "updatedAt": "2025-04-24T04:00:16.499Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "c3e5d645-a147-41ce-a48d-b2db9857afb4",
      "number": 16,
      "content": "Khi Ngài sai nạn đói đến trong xứ;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.512Z",
      "updatedAt": "2025-04-24T04:00:16.512Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "5c373fab-1279-4f38-a32c-edcc0c88214e",
      "number": 16,
      "content": "Và dẹp hết mọi nguồn lương thực của họ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.526Z",
      "updatedAt": "2025-04-24T04:00:16.526Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "e33b406e-196d-4672-980c-381abd1adaee",
      "number": 17,
      "content": "Thì Ngài đã sai một người ra đi trước họ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.543Z",
      "updatedAt": "2025-04-24T04:00:16.543Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "69140c43-aba0-4e71-8113-4a0cbb5597e1",
      "number": 17,
      "content": "Ấy là Giô-sép, một người bị bán làm nô lệ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.556Z",
      "updatedAt": "2025-04-24T04:00:16.556Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "2fd910ac-acdd-4d53-a9b3-263c9ae4df09",
      "number": 18,
      "content": "Họ tra chân ông vào cùm, khiến ông bị đau đớn;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.569Z",
      "updatedAt": "2025-04-24T04:00:16.569Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "843e651a-bf1e-45be-9e5e-a074d2fce222",
      "number": 18,
      "content": "Họ bắt cổ ông phải mang xích sắt nặng nề,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.583Z",
      "updatedAt": "2025-04-24T04:00:16.583Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "26466811-9c38-4aa6-87d0-916858e72338",
      "number": 19,
      "content": "Cho đến khi những gì ông nói được ứng nghiệm;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.596Z",
      "updatedAt": "2025-04-24T04:00:16.596Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "a43ed14a-6747-43ad-bfbc-72ba5995222f",
      "number": 19,
      "content": "Lời Chúa minh xác rằng lời ông đúng hoàn toàn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.613Z",
      "updatedAt": "2025-04-24T04:00:16.613Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "7fd3ff65-256e-4383-8555-81606c499613",
      "number": 20,
      "content": "Do đó vua phải sai người đến thả ông ra;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.626Z",
      "updatedAt": "2025-04-24T04:00:16.626Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "6669588c-e604-4c95-b656-edfbfe9d39d9",
      "number": 20,
      "content": "Kẻ cai trị các dân đã cho ông được tự do.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.639Z",
      "updatedAt": "2025-04-24T04:00:16.639Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "e2f6ad24-2c50-4e1f-8604-46cc624c8401",
      "number": 21,
      "content": "Rồi vua ấy lập ông làm tể tướng của vương quốc mình,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.652Z",
      "updatedAt": "2025-04-24T04:00:16.652Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "17b5af7f-6195-4387-9fd1-11ac7a5e0415",
      "number": 21,
      "content": "Làm người cai trị trên tất cả cơ nghiệp mình,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.692Z",
      "updatedAt": "2025-04-24T04:00:16.692Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "a5567cec-18af-4ef6-8ecd-e0298a4f517e",
      "number": 22,
      "content": "Ðể ông chỉ huy những người quyền thế mình,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.705Z",
      "updatedAt": "2025-04-24T04:00:16.705Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "a16b3ee3-7f74-4eb0-a5ed-f843ce7015aa",
      "number": 22,
      "content": "Và bày diệu kế cho các thượng quan mình.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.718Z",
      "updatedAt": "2025-04-24T04:00:16.718Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "1128d621-e9c9-4358-92b9-9f9444746f6e",
      "number": 23,
      "content": "Sau đó I-sơ-ra-ên di cư vào Ai-cập;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.731Z",
      "updatedAt": "2025-04-24T04:00:16.731Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "6b6453e7-3f1d-46b4-b27b-78f208cc18bb",
      "number": 23,
      "content": "Gia-cốp đến kiều ngụ trong xứ của Cham.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.744Z",
      "updatedAt": "2025-04-24T04:00:16.744Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "7713ffa2-db00-4c82-9376-f346ac1422ff",
      "number": 24,
      "content": "Tại đó Chúa làm cho dân Ngài gia tăng đông đúc,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.761Z",
      "updatedAt": "2025-04-24T04:00:16.761Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "697299fa-46e5-43fa-a88a-99b506601683",
      "number": 24,
      "content": "Khiến họ mạnh hơn những kẻ thù của họ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.774Z",
      "updatedAt": "2025-04-24T04:00:16.774Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "9e8fef13-0989-4d99-908b-71131ec754c6",
      "number": 25,
      "content": "Ngài để cho chúng thay lòng đổi dạ ghen ghét dân Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.787Z",
      "updatedAt": "2025-04-24T04:00:16.787Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "b933a35e-b90a-4d30-9b5d-cdc14f846ec9",
      "number": 25,
      "content": "Và lập mưu hãm hại các tôi tớ Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.801Z",
      "updatedAt": "2025-04-24T04:00:16.801Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "0c46d1e3-d5d4-4d09-a1cf-30cd26db93b8",
      "number": 26,
      "content": "Ngài sai Môi-se đầy tớ Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.814Z",
      "updatedAt": "2025-04-24T04:00:16.814Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "1044e0ac-da41-4648-86d6-99997cd08856",
      "number": 26,
      "content": "Và A-rôn người Ngài đã chọn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.830Z",
      "updatedAt": "2025-04-24T04:00:16.830Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "323f862c-3261-4e8a-83c0-ab9a4077c3c3",
      "number": 27,
      "content": "Họ thực hiện những việc diệu kỳ của Ngài giữa chúng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.844Z",
      "updatedAt": "2025-04-24T04:00:16.844Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "e57a4e7c-6eb1-4a64-ae96-bc123e9a7bb4",
      "number": 27,
      "content": "Và tỏ những phép lạ của Ngài trong xứ của Cham.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.857Z",
      "updatedAt": "2025-04-24T04:00:16.857Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "9071ac40-5586-42a9-b1cb-021ab02dc2df",
      "number": 28,
      "content": "Ngài giáng tối tăm, khiến cả xứ trở nên đen kịt,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.870Z",
      "updatedAt": "2025-04-24T04:00:16.870Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "20dd02c7-1e4e-45a2-95e1-44e5384a38b5",
      "number": 28,
      "content": "Ðể chúng không nghịch lại lời Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.883Z",
      "updatedAt": "2025-04-24T04:00:16.883Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "82732bb1-a896-4ac9-83af-ddca911f478a",
      "number": 29,
      "content": "Ngài làm cho nước của chúng trở nên như máu,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.897Z",
      "updatedAt": "2025-04-24T04:00:16.897Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "d2aea78e-0c9b-4586-8a23-062ab1b41662",
      "number": 29,
      "content": "Và khiến cá của chúng đều phải chết toi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.910Z",
      "updatedAt": "2025-04-24T04:00:16.910Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "c0623a28-d232-475a-b5b2-c604046aa254",
      "number": 30,
      "content": "Ếch nhái nhung nhúc tràn lan khắp xứ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.923Z",
      "updatedAt": "2025-04-24T04:00:16.923Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "f2d5b576-bca6-4908-a5ac-9241d75bc962",
      "number": 30,
      "content": "Chúng tràn tận vào cung thất của nhà vua.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.936Z",
      "updatedAt": "2025-04-24T04:00:16.936Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "76a42cc0-9427-48ee-8b21-a9d2f573e81d",
      "number": 31,
      "content": "Ngài vừa phán, vô số ruồi mòng từ đâu bay đến;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.950Z",
      "updatedAt": "2025-04-24T04:00:16.950Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "a48155c7-3c38-4aaf-9fda-bde69be76242",
      "number": 31,
      "content": "Muỗi vo ve tràn khắp cõi bờ.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.967Z",
      "updatedAt": "2025-04-24T04:00:16.967Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "f0c172a1-f511-4265-9ab3-bbade1f761a8",
      "number": 32,
      "content": "Ngài cho trận mưa nhưng là mưa đá;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:16.980Z",
      "updatedAt": "2025-04-24T04:00:16.980Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "7996a5a1-f5b0-49ad-94bb-ca517970b569",
      "number": 32,
      "content": "Sấm sét nổ vang, gây hỏa hoạn khắp nơi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17Z",
      "updatedAt": "2025-04-24T04:00:17Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "7b8e9818-90a5-4a3b-bc17-c7020389fff9",
      "number": 33,
      "content": "Ngài quật ngã cây nho và cây vả của chúng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.013Z",
      "updatedAt": "2025-04-24T04:00:17.013Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "20a63403-df4f-4fd0-acc3-7d204ea2ec30",
      "number": 33,
      "content": "Khiến cây cối của chúng ngã rạp khắp xứ.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.026Z",
      "updatedAt": "2025-04-24T04:00:17.026Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "7ea25f91-a811-48ec-81ce-b734797e3104",
      "number": 34,
      "content": "Ngài phán nữa, vô số cào cào từ đâu bay đến;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.039Z",
      "updatedAt": "2025-04-24T04:00:17.039Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "3a06a6db-f8b8-4d97-9246-48b7f719462e",
      "number": 34,
      "content": "Chấu chấu phủ tràn lan, không thể đếm cho cùng.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.052Z",
      "updatedAt": "2025-04-24T04:00:17.052Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "3cdf9846-c1e7-432c-bd57-70f9642d1922",
      "number": 35,
      "content": "Chúng cắn phá mọi cỏ cây trong xứ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.064Z",
      "updatedAt": "2025-04-24T04:00:17.064Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "fcae624f-39ba-4d08-8de9-566b4e251d79",
      "number": 35,
      "content": "Chúng cắn ăn mọi sản vật của đất đai.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.075Z",
      "updatedAt": "2025-04-24T04:00:17.075Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "7e90c499-dc81-4a2b-8f3b-e4fb63bc5d21",
      "number": 36,
      "content": "Ngài cũng đánh hạ các con đầu lòng trong xứ chúng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.086Z",
      "updatedAt": "2025-04-24T04:00:17.086Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "4a2aeca1-56c6-4a24-818b-bb6c29856b55",
      "number": 36,
      "content": "Những đứa con sinh ra khi sức lực chúng dồi dào.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.097Z",
      "updatedAt": "2025-04-24T04:00:17.097Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "64288318-6ef1-4f28-8acd-0f6028fccd26",
      "number": 37,
      "content": "Sau đó Ngài đem dân I-sơ-ra-ên ra đi với biết bao vàng bạc;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.108Z",
      "updatedAt": "2025-04-24T04:00:17.108Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "d4c46b1a-ac8f-4590-a73e-25ec29149be4",
      "number": 37,
      "content": "Chẳng có ai chùn chân trong các chi tộc của Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.118Z",
      "updatedAt": "2025-04-24T04:00:17.118Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "a8e1341e-5ba5-495f-8a00-4e563a1cf9c7",
      "number": 38,
      "content": "Ai-cập vui mừng khi họ lìa khỏi xứ,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.128Z",
      "updatedAt": "2025-04-24T04:00:17.128Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "c9c99616-9a11-4342-bd63-45aec9ac43d8",
      "number": 38,
      "content": "Vì nỗi sợ hãi họ giáng trên chúng khắp nơi.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.139Z",
      "updatedAt": "2025-04-24T04:00:17.139Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "891c086a-0f6c-4e2d-95a6-0d686b227556",
      "number": 39,
      "content": "Ngài giăng đám mây che mát trên đầu họ,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.150Z",
      "updatedAt": "2025-04-24T04:00:17.150Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "1b404205-89b1-453f-8c77-ec8b8d2f0dcf",
      "number": 39,
      "content": "Và ban lửa hồng soi sáng lúc đêm về.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.161Z",
      "updatedAt": "2025-04-24T04:00:17.161Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "af01a900-9b10-48b6-b661-dda7a2568292",
      "number": 40,
      "content": "Họ đòi thịt, Ngài mang đến họ chim cút nhiều vô kể;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.174Z",
      "updatedAt": "2025-04-24T04:00:17.174Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "1d2f4420-e4ea-40c3-b1af-2b16e1870997",
      "number": 40,
      "content": "Và họ được no nê bằng bánh đến từ trời.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.187Z",
      "updatedAt": "2025-04-24T04:00:17.187Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "fb605241-9213-4dc4-b95d-437f9758cd84",
      "number": 41,
      "content": "Ngài mở vầng đá, và nước ngọt tuôn ra lai láng;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.200Z",
      "updatedAt": "2025-04-24T04:00:17.200Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "d68570a0-a310-48b5-8f56-6d23c6aadbf1",
      "number": 41,
      "content": "Nước phun trào giữa nơi khô hạn tựa dòng sông.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.214Z",
      "updatedAt": "2025-04-24T04:00:17.214Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "6512635b-585a-4303-9775-970a112fc4b2",
      "number": 42,
      "content": "Vì Ngài nhớ lại lời hứa thánh của Ngài,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.227Z",
      "updatedAt": "2025-04-24T04:00:17.227Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "d84c7c89-26f3-48d8-893c-3a142a243c94",
      "number": 42,
      "content": "Tức lời hứa với Áp-ra-ham tôi tớ Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.240Z",
      "updatedAt": "2025-04-24T04:00:17.240Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "f6ac399e-8256-41d9-a220-a44855d63c47",
      "number": 43,
      "content": "Vậy Ngài đã đem dân Ngài ra đi trong vui mừng hớn hở;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.253Z",
      "updatedAt": "2025-04-24T04:00:17.253Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "55ddda42-d0de-4e3c-a937-3ab06e969f5d",
      "number": 43,
      "content": "Ðem những người Ngài chọn xuất hành với tiếng vui vẻ reo hò.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.266Z",
      "updatedAt": "2025-04-24T04:00:17.266Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "fdf2b3c6-59c3-436d-991e-45706eac09ff",
      "number": 44,
      "content": "Ngài ban xứ sở của các dân cho họ làm sản nghiệp;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.279Z",
      "updatedAt": "2025-04-24T04:00:17.279Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "67ec5ed0-8436-4b73-8a7c-b5d79b003a7b",
      "number": 44,
      "content": "Và cho họ hưởng công lao làm lụng của các dân,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.292Z",
      "updatedAt": "2025-04-24T04:00:17.292Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "8d898a7c-28f9-4993-a238-9b65088cf2b0",
      "number": 45,
      "content": "Ðể họ vâng giữ luật lệ Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.305Z",
      "updatedAt": "2025-04-24T04:00:17.305Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "13a74205-ef44-4f00-951f-6bbbf64a4661",
      "number": 45,
      "content": "Và tuân theo luật pháp Ngài.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.319Z",
      "updatedAt": "2025-04-24T04:00:17.319Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    },
    {
      "id": "bfcb51cc-efb3-4e1a-ad7b-d488182d39a7",
      "number": 45,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:17.332Z",
      "updatedAt": "2025-04-24T04:00:17.332Z",
      "chapterId": "4f943e58-d2eb-4f63-aa55-4e7a410820b5"
    }
  ],
