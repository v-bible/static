{
  "chapter": {
    "id": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14",
    "number": 100,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20100&version=BD2011",
    "createdAt": "2025-04-22T07:37:47.982Z",
    "updatedAt": "2025-04-22T07:37:47.982Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "1d893681-db05-4646-a01e-26f941efa0f9",
      "number": 1,
      "content": "Hỡi cả thế giới,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.519Z",
      "updatedAt": "2025-04-22T08:31:36.519Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "051fcd76-909b-46ba-857b-33774d79376c",
      "number": 1,
      "content": "Hãy lớn tiếng reo mừng tung hô Chúa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.547Z",
      "updatedAt": "2025-04-22T08:31:36.547Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "df305b86-c519-4652-8ed0-4addb3a09847",
      "number": 2,
      "content": "Hãy thờ phượng Chúa cách vui mừng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.557Z",
      "updatedAt": "2025-04-22T08:31:36.557Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "affee875-f28c-4317-969e-01aa58eb51d3",
      "number": 2,
      "content": "Hãy đến trước thánh nhan Ngài với tiếng hoan ca.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.579Z",
      "updatedAt": "2025-04-22T08:31:36.579Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "f18e97c4-938a-4f66-b6bc-8cbfdad519e3",
      "number": 3,
      "content": "Khá biết rằng Chúa là Ðức Chúa Trời.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.591Z",
      "updatedAt": "2025-04-22T08:31:36.591Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "db17e92a-d9f5-4c68-bdf3-4bd4ef4160fc",
      "number": 3,
      "content": "Ấy là Ngài, Đấng dựng nên chúng ta;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.604Z",
      "updatedAt": "2025-04-22T08:31:36.604Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "b513e72a-3b82-4092-add9-45085fda0b7e",
      "number": 3,
      "content": "Chúng ta thuộc về Ngài;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.617Z",
      "updatedAt": "2025-04-22T08:31:36.617Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "089ffd6a-083d-4384-8e2a-339b5a98df67",
      "number": 3,
      "content": "Chúng ta là con dân Ngài và bầy chiên của đồng cỏ Ngài.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.630Z",
      "updatedAt": "2025-04-22T08:31:36.630Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "26e9635d-f359-40b4-bd26-0eb6a61e0a5e",
      "number": 4,
      "content": "Hãy cảm tạ mà vào các cổng Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.643Z",
      "updatedAt": "2025-04-22T08:31:36.643Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "e25ec469-3f90-4729-b01c-2677d65cca2e",
      "number": 4,
      "content": "Khá ngợi ca mà vào các sân Ngài;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.657Z",
      "updatedAt": "2025-04-22T08:31:36.657Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "cbcac474-7fe7-41f7-9252-7372bb965902",
      "number": 4,
      "content": "Hãy tạ ơn Ngài, khá chúc tụng danh Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.670Z",
      "updatedAt": "2025-04-22T08:31:36.670Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "cba3e3cf-6dca-4e67-972e-b45833d05256",
      "number": 5,
      "content": "Vì Chúa thật tốt;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.683Z",
      "updatedAt": "2025-04-22T08:31:36.683Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "c321d49e-ec1d-4a1d-bb94-3fb17d57c007",
      "number": 5,
      "content": "Tình thương của Ngài hằng còn mãi mãi;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.696Z",
      "updatedAt": "2025-04-22T08:31:36.696Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    },
    {
      "id": "e9041869-ec0b-4cd2-9e73-ea2f8ac5280d",
      "number": 5,
      "content": "Ðức thành tín của Ngài còn đến muôn đời.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:36.712Z",
      "updatedAt": "2025-04-22T08:31:36.712Z",
      "chapterId": "886c6a87-ddfc-4a95-ba61-d16ad83b1e14"
    }
  ],
