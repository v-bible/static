{
  "chapter": {
    "id": "772b45ed-db37-47c2-a902-0e0a97027450",
    "number": 100,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20100&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.398Z",
    "updatedAt": "2025-04-24T03:03:17.398Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "18e5959c-80e2-4f3c-b1c8-c21b418b77e1",
      "number": 1,
      "content": "Hỡi cả thế giới,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.209Z",
      "updatedAt": "2025-04-24T03:59:32.209Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "d1093218-9d00-4358-9449-c99d5f29ffff",
      "number": 1,
      "content": "Hãy lớn tiếng reo mừng tung hô Chúa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.241Z",
      "updatedAt": "2025-04-24T03:59:32.241Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "7d60f220-bea7-40c8-b510-cbee63780eeb",
      "number": 2,
      "content": "Hãy thờ phượng Chúa cách vui mừng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.252Z",
      "updatedAt": "2025-04-24T03:59:32.252Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "7c77eaa2-3136-4047-a9e7-c5f5153b93ac",
      "number": 2,
      "content": "Hãy đến trước thánh nhan Ngài với tiếng hoan ca.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.273Z",
      "updatedAt": "2025-04-24T03:59:32.273Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "0b883074-b968-4257-a6ff-43787aaebc38",
      "number": 3,
      "content": "Khá biết rằng Chúa là Ðức Chúa Trời.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.284Z",
      "updatedAt": "2025-04-24T03:59:32.284Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "bd5b850c-9907-4d39-9707-e86841810d0b",
      "number": 3,
      "content": "Ấy là Ngài, Đấng dựng nên chúng ta;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.295Z",
      "updatedAt": "2025-04-24T03:59:32.295Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "519b566a-c74d-48e0-b59e-68e700f58081",
      "number": 3,
      "content": "Chúng ta thuộc về Ngài;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.305Z",
      "updatedAt": "2025-04-24T03:59:32.305Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "9d19df03-08f2-4a9d-8499-7697e3e3582f",
      "number": 3,
      "content": "Chúng ta là con dân Ngài và bầy chiên của đồng cỏ Ngài.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.316Z",
      "updatedAt": "2025-04-24T03:59:32.316Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "50318244-3ea6-4860-837d-543994ff5514",
      "number": 4,
      "content": "Hãy cảm tạ mà vào các cổng Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.326Z",
      "updatedAt": "2025-04-24T03:59:32.326Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "5a359508-7537-4dd5-9893-892d68a6f78e",
      "number": 4,
      "content": "Khá ngợi ca mà vào các sân Ngài;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.337Z",
      "updatedAt": "2025-04-24T03:59:32.337Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "89650ad7-9ab1-41e8-a453-b9a643d0c27b",
      "number": 4,
      "content": "Hãy tạ ơn Ngài, khá chúc tụng danh Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.347Z",
      "updatedAt": "2025-04-24T03:59:32.347Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "b37c69ba-1840-4e22-bacc-cfe49c9b880c",
      "number": 5,
      "content": "Vì Chúa thật tốt;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.357Z",
      "updatedAt": "2025-04-24T03:59:32.357Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "4ef0e810-fe0d-421c-95a4-f9b2f930affe",
      "number": 5,
      "content": "Tình thương của Ngài hằng còn mãi mãi;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.368Z",
      "updatedAt": "2025-04-24T03:59:32.368Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    },
    {
      "id": "ec8b045a-6cab-4dc2-971b-1fca85fb354c",
      "number": 5,
      "content": "Ðức thành tín của Ngài còn đến muôn đời.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:32.379Z",
      "updatedAt": "2025-04-24T03:59:32.379Z",
      "chapterId": "772b45ed-db37-47c2-a902-0e0a97027450"
    }
  ],
