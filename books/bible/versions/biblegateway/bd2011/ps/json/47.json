{
  "chapter": {
    "id": "2fc700cc-55e7-4482-8c3b-3813575864a0",
    "number": 47,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2047&version=BD2011",
    "createdAt": "2025-04-24T03:03:15.997Z",
    "updatedAt": "2025-04-24T03:03:15.997Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "47fc06c4-b668-430e-be02-a5830bdfc027",
      "number": 1,
      "content": "Hãy vỗ tay, hỡi mọi người!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.203Z",
      "updatedAt": "2025-04-24T03:52:55.203Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "dace0416-6031-4741-befe-d70165a49825",
      "number": 1,
      "content": "Hãy lớn tiếng reo mừng ca ngợi Ðức Chúa Trời!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.220Z",
      "updatedAt": "2025-04-24T03:52:55.220Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "138d9766-1c52-4a74-938d-377841b7ba3f",
      "number": 2,
      "content": "Vì Chúa, Ðấng Tối Cao, thật đáng kính sợ.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.230Z",
      "updatedAt": "2025-04-24T03:52:55.230Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "42c14637-573a-47a7-862c-b46e900dbc04",
      "number": 2,
      "content": "Ngài là Vua cao cả trên toàn thế giới.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.240Z",
      "updatedAt": "2025-04-24T03:52:55.240Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "a58a9295-d375-49e2-b200-bed081edda11",
      "number": 3,
      "content": "Ngài làm cho các dân bị khuất phục dưới chúng tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.251Z",
      "updatedAt": "2025-04-24T03:52:55.251Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "d7a86097-d535-4ac3-ae07-aebf76411b75",
      "number": 3,
      "content": "Và các nước ở dưới chân chúng tôi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.262Z",
      "updatedAt": "2025-04-24T03:52:55.262Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "996e8836-e7ad-438a-a37a-e2478b3db0fe",
      "number": 4,
      "content": "Ngài chọn cơ nghiệp cho chúng tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.275Z",
      "updatedAt": "2025-04-24T03:52:55.275Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "f4d8114f-d47c-42f5-99b4-ec7ec11d3700",
      "number": 4,
      "content": "Vinh hiển của Gia-cốp, người Ngài yêu mến. *(Sê-la)*",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.288Z",
      "updatedAt": "2025-04-24T03:52:55.288Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "4b260d11-61b0-4248-9edd-ebb66379cd52",
      "number": 5,
      "content": "Ðức Chúa Trời ngự lên giữa tiếng reo mừng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.302Z",
      "updatedAt": "2025-04-24T03:52:55.302Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "519ebffa-f041-4084-bc2c-8812274399ec",
      "number": 5,
      "content": "Chúa ngự lên giữa tiếng kèn trỗi lên vang lừng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.316Z",
      "updatedAt": "2025-04-24T03:52:55.316Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "1902f629-0483-49c0-b55f-ab6fa8d9a5c3",
      "number": 6,
      "content": "Hãy ca ngợi Ðức Chúa Trời; khá hát lên những bài chúc tụng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.335Z",
      "updatedAt": "2025-04-24T03:52:55.335Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "5a2c61aa-49e8-4494-9e92-a0841d451804",
      "number": 6,
      "content": "Hãy ca ngợi Vua chúng ta; khá hát lên những bài chúc tụng,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.349Z",
      "updatedAt": "2025-04-24T03:52:55.349Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "94dcd53e-7c06-46a8-bbeb-2087d2855f05",
      "number": 7,
      "content": "Vì Ðức Chúa Trời là Vua của toàn thế giới;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.362Z",
      "updatedAt": "2025-04-24T03:52:55.362Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "f237cd82-dbd0-4425-a2f9-20112efde524",
      "number": 7,
      "content": "Hãy ca ngợi Ngài bằng những bài thánh thi chúc tụng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.375Z",
      "updatedAt": "2025-04-24T03:52:55.375Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "db778176-29a4-46c5-8cef-f4c3736b68ba",
      "number": 8,
      "content": "Ðức Chúa Trời trị vì trên các dân các nước;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.389Z",
      "updatedAt": "2025-04-24T03:52:55.389Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "35114b13-b676-4aa8-9ba5-5abe4f0420a3",
      "number": 8,
      "content": "Ðức Chúa Trời ngự trên ngôi thánh của Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.402Z",
      "updatedAt": "2025-04-24T03:52:55.402Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "ed7ab471-77a7-40ae-8112-428942ba2292",
      "number": 9,
      "content": "Các lãnh tụ thế giới họp nhau lại làm dân của Ðức Chúa Trời của Áp-ra-ham,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.416Z",
      "updatedAt": "2025-04-24T03:52:55.416Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "17f3661a-4d8f-47c4-ae41-c58d8a03c30d",
      "number": 9,
      "content": "Bởi vì các thủ lãnh trên thế gian đều thuộc về Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.432Z",
      "updatedAt": "2025-04-24T03:52:55.432Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    },
    {
      "id": "cfec12d4-db72-45eb-9f67-0e3539843b5d",
      "number": 9,
      "content": "Ngài được tôn chí cao!",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:52:55.465Z",
      "updatedAt": "2025-04-24T03:52:55.465Z",
      "chapterId": "2fc700cc-55e7-4482-8c3b-3813575864a0"
    }
  ],
