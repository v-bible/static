{
  "chapter": {
    "id": "2aa02063-d7f9-4be7-83f9-ce89d9e62844",
    "number": 47,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2047&version=BD2011",
    "createdAt": "2024-09-24T01:00:43.319Z",
    "updatedAt": "2024-09-24T01:00:43.319Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "9633d132-0de3-4223-8367-b6b0ec8df2cd",
      "number": 1,
      "content": "Hãy vỗ tay, hỡi mọi người!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.695Z",
      "updatedAt": "2024-09-24T01:58:42.695Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "50424899-9591-47a4-a687-226e32eeff80",
      "number": 1,
      "content": "Hãy lớn tiếng reo mừng ca ngợi Ðức Chúa Trời!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.705Z",
      "updatedAt": "2024-09-24T01:58:42.705Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "aaad372a-f27a-4645-8775-a7cced878595",
      "number": 2,
      "content": "Vì Chúa, Ðấng Tối Cao, thật đáng kính sợ.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.707Z",
      "updatedAt": "2024-09-24T01:58:42.707Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "d40df320-deb0-4d35-98d1-13126075bdb5",
      "number": 2,
      "content": "Ngài là Vua cao cả trên toàn thế giới.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.710Z",
      "updatedAt": "2024-09-24T01:58:42.710Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "0d129e15-29c2-4358-8d2e-6f25ce6b8088",
      "number": 3,
      "content": "Ngài làm cho các dân bị khuất phục dưới chúng tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.712Z",
      "updatedAt": "2024-09-24T01:58:42.712Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "2e067793-2378-43c7-b363-834e2fd44a57",
      "number": 3,
      "content": "Và các nước ở dưới chân chúng tôi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.719Z",
      "updatedAt": "2024-09-24T01:58:42.719Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "749668f4-6830-4f88-b993-f5ec90601dc9",
      "number": 4,
      "content": "Ngài chọn cơ nghiệp cho chúng tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.722Z",
      "updatedAt": "2024-09-24T01:58:42.722Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "e16b74a1-5790-47e3-8824-fadc6df1ea67",
      "number": 4,
      "content": "Vinh hiển của Gia-cốp, người Ngài yêu mến. *(Sê-la)*",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.724Z",
      "updatedAt": "2024-09-24T01:58:42.724Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "0b6fc56b-8b51-4a22-a376-275bd2c39978",
      "number": 5,
      "content": "Ðức Chúa Trời ngự lên giữa tiếng reo mừng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.727Z",
      "updatedAt": "2024-09-24T01:58:42.727Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "290a06e7-2fa2-42d8-a034-ad50daf6223a",
      "number": 5,
      "content": "Chúa ngự lên giữa tiếng kèn trỗi lên vang lừng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.729Z",
      "updatedAt": "2024-09-24T01:58:42.729Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "e02987fc-adfd-4976-9168-429794221808",
      "number": 6,
      "content": "Hãy ca ngợi Ðức Chúa Trời; khá hát lên những bài chúc tụng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.731Z",
      "updatedAt": "2024-09-24T01:58:42.731Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "da78ecc0-65bd-45b9-8058-e16e6d8ff269",
      "number": 6,
      "content": "Hãy ca ngợi Vua chúng ta; khá hát lên những bài chúc tụng,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.735Z",
      "updatedAt": "2024-09-24T01:58:42.735Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "f7456624-ebdb-4c1d-a1b8-3813ccddc0b6",
      "number": 7,
      "content": "Vì Ðức Chúa Trời là Vua của toàn thế giới;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.737Z",
      "updatedAt": "2024-09-24T01:58:42.737Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "04e751dc-5663-43fd-976b-159999c3f950",
      "number": 7,
      "content": "Hãy ca ngợi Ngài bằng những bài thánh thi chúc tụng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.739Z",
      "updatedAt": "2024-09-24T01:58:42.739Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "e26d428c-819a-4527-b6bc-8e87f61e562e",
      "number": 8,
      "content": "Ðức Chúa Trời trị vì trên các dân các nước;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.741Z",
      "updatedAt": "2024-09-24T01:58:42.741Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "b8b937b3-7fc5-48e7-8d7c-a7d7e145f669",
      "number": 8,
      "content": "Ðức Chúa Trời ngự trên ngôi thánh của Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.744Z",
      "updatedAt": "2024-09-24T01:58:42.744Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "fd384f12-1a0b-4401-abb1-0a2048c88037",
      "number": 9,
      "content": "Các lãnh tụ thế giới họp nhau lại làm dân của Ðức Chúa Trời của Áp-ra-ham,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.746Z",
      "updatedAt": "2024-09-24T01:58:42.746Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "58a899f6-0e3a-4d93-b3e8-cf0ac3a1d95d",
      "number": 9,
      "content": "Bởi vì các thủ lãnh trên thế gian đều thuộc về Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.749Z",
      "updatedAt": "2024-09-24T01:58:42.749Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    },
    {
      "id": "45836434-c90f-499e-9b79-db3807c35aaf",
      "number": 9,
      "content": "Ngài được tôn chí cao!",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:58:42.754Z",
      "updatedAt": "2024-09-24T01:58:42.754Z",
      "chapterId": "2aa02063-d7f9-4be7-83f9-ce89d9e62844"
    }
  ],
