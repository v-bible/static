{
  "chapter": {
    "id": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a",
    "number": 107,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20107&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.149Z",
    "updatedAt": "2025-04-22T07:37:48.149Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "9b569b4e-16d0-4aae-854d-dc54fabd6312",
      "number": 1,
      "content": "Hãy cảm tạ Chúa, vì Ngài thật tốt,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.747Z",
      "updatedAt": "2025-04-22T08:32:27.747Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "2534bdad-cb4c-4534-9b05-c4abd6e59673",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.789Z",
      "updatedAt": "2025-04-22T08:32:27.789Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "09ad309f-35ff-4135-a799-5f4f6746a9f0",
      "number": 2,
      "content": "Nguyện những người được cứu chuộc của Chúa nói như vậy,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.799Z",
      "updatedAt": "2025-04-22T08:32:27.799Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "f4f67044-3475-4375-a072-e27d826952d0",
      "number": 2,
      "content": "Những người được Ngài cứu khỏi tay kẻ thù,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.813Z",
      "updatedAt": "2025-04-22T08:32:27.813Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "2a778816-5e51-411b-951e-eaab57eeb902",
      "number": 3,
      "content": "Rồi gom nhóm họ từ các nước trở về;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.824Z",
      "updatedAt": "2025-04-22T08:32:27.824Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "75a8fe0d-3093-4124-bb7f-2f10751229d2",
      "number": 3,
      "content": "Từ phương đông và phương tây, phương bắc và phương nam.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.836Z",
      "updatedAt": "2025-04-22T08:32:27.836Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "8eae9622-d474-4c3c-9cdd-895791b40dd5",
      "number": 4,
      "content": "Họ lang thang trong đồng hoang, trong nơi hoang vắng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.849Z",
      "updatedAt": "2025-04-22T08:32:27.849Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "a8476df2-4642-4131-acee-2a7995e79e5d",
      "number": 4,
      "content": "Họ chẳng tìm được đường về một thành để ở.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.862Z",
      "updatedAt": "2025-04-22T08:32:27.862Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "63cd7ce9-9178-4e17-bf7e-4732db27fc4b",
      "number": 5,
      "content": "Họ đói khát;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.875Z",
      "updatedAt": "2025-04-22T08:32:27.875Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "c694f842-dce1-4660-9aef-a37f048410b3",
      "number": 5,
      "content": "Linh hồn họ mòn mỏi nao sờn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.886Z",
      "updatedAt": "2025-04-22T08:32:27.886Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "4446fcff-25b3-4c07-978e-6a7a744d8cc6",
      "number": 6,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.896Z",
      "updatedAt": "2025-04-22T08:32:27.896Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "8dea233f-4b99-4807-8185-826060f3cbe2",
      "number": 13,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.089Z",
      "updatedAt": "2025-04-22T08:32:28.089Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "34925254-9a9a-412c-b3aa-db61236f4929",
      "number": 19,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.232Z",
      "updatedAt": "2025-04-22T08:32:28.232Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "0b099bfa-df60-4c27-8adf-9ac79fad61ba",
      "number": 28,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.463Z",
      "updatedAt": "2025-04-22T08:32:28.463Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "ef866c3a-ff9c-4f48-93ce-be65d699df47",
      "number": 6,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.909Z",
      "updatedAt": "2025-04-22T08:32:27.909Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "41da6134-3a27-43b1-8886-be7a31f05a71",
      "number": 13,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.100Z",
      "updatedAt": "2025-04-22T08:32:28.100Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "98a7b2c3-dcf7-4a03-87a3-b4162a7a230f",
      "number": 19,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.245Z",
      "updatedAt": "2025-04-22T08:32:28.245Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "2da3f77b-f8d8-401e-a5aa-f875bdc9b968",
      "number": 28,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.476Z",
      "updatedAt": "2025-04-22T08:32:28.476Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "53ef04c1-e62e-491d-a671-8f64233fadec",
      "number": 7,
      "content": "Ngài dẫn đưa họ đúng vào con đường phải đi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.919Z",
      "updatedAt": "2025-04-22T08:32:27.919Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "4e400d64-91fd-4236-a7ae-7e0ebac18157",
      "number": 7,
      "content": "Ðể họ đến một thành mà ở.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.929Z",
      "updatedAt": "2025-04-22T08:32:27.929Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "6d2cfe91-0e21-4bc4-9ed3-0a20776c39fc",
      "number": 8,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.943Z",
      "updatedAt": "2025-04-22T08:32:27.943Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "fa73be77-4913-474d-b791-64dd73bf7fc3",
      "number": 15,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.135Z",
      "updatedAt": "2025-04-22T08:32:28.135Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "2a50a344-fdbf-4278-a01c-d180ce85a830",
      "number": 21,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.288Z",
      "updatedAt": "2025-04-22T08:32:28.288Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "7f9bc8a2-550f-4f84-a426-f8487fb8449c",
      "number": 31,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.550Z",
      "updatedAt": "2025-04-22T08:32:28.550Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "7f276f57-7ef1-487d-93eb-57adb9906838",
      "number": 8,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.956Z",
      "updatedAt": "2025-04-22T08:32:27.956Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "6c8de222-b77e-4a42-b8d6-6184b2425b2c",
      "number": 9,
      "content": "Vì Ngài làm thỏa mãn linh hồn khao khát,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.970Z",
      "updatedAt": "2025-04-22T08:32:27.970Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "2529178a-84f4-4c53-a67e-970a3f8c7ab5",
      "number": 9,
      "content": "Và làm no nê linh hồn đói lả bằng các thức ăn ngon.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:27.986Z",
      "updatedAt": "2025-04-22T08:32:27.986Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "0ab2ae59-b2d1-4f2b-8dad-cd73fdcfd191",
      "number": 10,
      "content": "Có những kẻ sống trong tối tăm và trong bóng tử thần,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.003Z",
      "updatedAt": "2025-04-22T08:32:28.003Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "5f48e408-bea6-4d7f-b205-f99cd1ec5786",
      "number": 10,
      "content": "Bị giam cầm trong khổ đau và xiềng xích,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.016Z",
      "updatedAt": "2025-04-22T08:32:28.016Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "92c135ea-c373-4c25-86ef-ff1b0862bb2a",
      "number": 11,
      "content": "Bởi vì họ chống lại những lời của Ðức Chúa Trời,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.033Z",
      "updatedAt": "2025-04-22T08:32:28.033Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "5dd94e54-2447-438a-a3b8-0c91e33b8386",
      "number": 11,
      "content": "Và khinh bỉ lời chỉ bảo của Ðấng Chí Cao.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.046Z",
      "updatedAt": "2025-04-22T08:32:28.046Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "2423ac6a-529a-4792-b490-3f98517531d0",
      "number": 12,
      "content": "Vì thế Ngài lấy gian lao hạ lòng họ xuống;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.061Z",
      "updatedAt": "2025-04-22T08:32:28.061Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "49a62907-5eec-4123-b739-366af48547e5",
      "number": 12,
      "content": "Họ bị ngã nhào và chẳng ai giúp họ đứng lên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.073Z",
      "updatedAt": "2025-04-22T08:32:28.073Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "5a90bca9-8e2c-48c6-8b74-d44f003b14b6",
      "number": 14,
      "content": "Ngài đem họ ra khỏi cảnh tối tăm và bóng tử thần,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.112Z",
      "updatedAt": "2025-04-22T08:32:28.112Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "f3597120-4562-40ef-8b9f-6e32a06f78c1",
      "number": 14,
      "content": "Và đập tan xiềng xích gông cùm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.124Z",
      "updatedAt": "2025-04-22T08:32:28.124Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "9480d501-3af5-4f68-9fdd-5f917dd49a2a",
      "number": 15,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.149Z",
      "updatedAt": "2025-04-22T08:32:28.149Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "e2f3b0ac-f1d5-4535-a7e6-8166ad9002a3",
      "number": 16,
      "content": "Vì Ngài phá tan các cổng bằng đồng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.159Z",
      "updatedAt": "2025-04-22T08:32:28.159Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "823c2077-5b28-4d10-bd0c-7af988ef3a5c",
      "number": 16,
      "content": "Và Ngài cắt rời những song bằng sắt.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.172Z",
      "updatedAt": "2025-04-22T08:32:28.172Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "378eb256-8859-4ea3-a9b1-07761b3b182b",
      "number": 17,
      "content": "Có những kẻ cứ bội nghịch nên hóa ra điên dại,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.182Z",
      "updatedAt": "2025-04-22T08:32:28.182Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "856ab524-5d0d-42c4-9165-4684f6146be6",
      "number": 17,
      "content": "Và vì tội lỗi mình nên phải chuốc lấy khổ đau.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.193Z",
      "updatedAt": "2025-04-22T08:32:28.193Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "0ebef40c-e545-4692-986a-24dfb9012cc0",
      "number": 18,
      "content": "Linh hồn họ gớm ghê mọi thực phẩm,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.207Z",
      "updatedAt": "2025-04-22T08:32:28.207Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "f806bce0-07fd-4a22-96a8-a4601812a253",
      "number": 18,
      "content": "Và họ bị kéo gần đến cổng của tử thần.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.219Z",
      "updatedAt": "2025-04-22T08:32:28.219Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "35f7c31b-c608-452f-9b6f-46143369f6bb",
      "number": 20,
      "content": "Ngài ban lời Ngài và chữa lành họ;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.258Z",
      "updatedAt": "2025-04-22T08:32:28.258Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "16e908fe-c20e-43ff-94d2-cfba5fde7d4d",
      "number": 20,
      "content": "Ngài giải cứu họ khỏi miệng của phần mộ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.271Z",
      "updatedAt": "2025-04-22T08:32:28.271Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "27eb7199-36db-4baf-b256-0557a66027c7",
      "number": 21,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.301Z",
      "updatedAt": "2025-04-22T08:32:28.301Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "167de9bb-fae8-4813-a7ff-964b6131f407",
      "number": 31,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.562Z",
      "updatedAt": "2025-04-22T08:32:28.562Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "1c3b6ce3-8a00-456b-bd03-a8f152ffa6bb",
      "number": 22,
      "content": "Cũng hãy để họ dâng những của lễ cảm tạ lên Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.320Z",
      "updatedAt": "2025-04-22T08:32:28.320Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "225da136-1ece-41ca-afeb-312e108ddae5",
      "number": 22,
      "content": "Và dùng những lời ca ngợi vui mừng thuật lại các công việc Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.333Z",
      "updatedAt": "2025-04-22T08:32:28.333Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "f8c8afb4-fb06-4e79-b4d3-3ca3f05f46db",
      "number": 23,
      "content": "Có những kẻ xuống tàu vượt đại dương,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.344Z",
      "updatedAt": "2025-04-22T08:32:28.344Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "d14d6bbb-a55d-48ec-a58d-217e94524729",
      "number": 23,
      "content": "Ấy là những người làm ăn trên biển cả;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.361Z",
      "updatedAt": "2025-04-22T08:32:28.361Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "e12f9f1c-45db-4dee-8817-5e0f5337e346",
      "number": 24,
      "content": "Họ trông thấy những việc làm của Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.371Z",
      "updatedAt": "2025-04-22T08:32:28.371Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "f5ca970c-df41-4568-a08f-ec04073b5963",
      "number": 24,
      "content": "Và những việc lạ lùng của Ngài ở giữa biển sâu.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.381Z",
      "updatedAt": "2025-04-22T08:32:28.381Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "1d88b1e2-0c01-45b0-a2c8-5dc3d97b036f",
      "number": 25,
      "content": "Vì Ngài phán một tiếng, bão tố liền nổi lên cuồn cuộn,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.392Z",
      "updatedAt": "2025-04-22T08:32:28.392Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "17e8e433-1e2d-4d71-8863-c68c8992e9b3",
      "number": 25,
      "content": "Cuồng phong nâng cao những ngọn sóng kinh hồn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.402Z",
      "updatedAt": "2025-04-22T08:32:28.402Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "f3012b8a-37ca-4574-a57e-96e1a27fcf69",
      "number": 26,
      "content": "Sóng cả dâng cao đến tận trời, rồi trụt sâu xuống đáy vực thẳm;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.412Z",
      "updatedAt": "2025-04-22T08:32:28.412Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "898916a5-eb2c-4b75-a86e-787e7ae9ac7e",
      "number": 26,
      "content": "Giữa cảnh khốn cùng họ hồn phi phách tán.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.423Z",
      "updatedAt": "2025-04-22T08:32:28.423Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "49f21e01-7836-426d-bd89-fb0abbe896e9",
      "number": 27,
      "content": "Họ bị quay cuồng đảo điên như người say rượu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.434Z",
      "updatedAt": "2025-04-22T08:32:28.434Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "0f6db1aa-4844-47a3-914b-fb212c30d196",
      "number": 27,
      "content": "Mọi tài trí khôn ngoan đều biến mất.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.447Z",
      "updatedAt": "2025-04-22T08:32:28.447Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "0a1c4243-1c3a-41d0-a469-b7925ef6cd5c",
      "number": 29,
      "content": "Ngài khiến bão tố yên lặng như tờ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.492Z",
      "updatedAt": "2025-04-22T08:32:28.492Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "1df50aa2-555f-456c-9954-2deaa3b8a26d",
      "number": 29,
      "content": "Những ngọn sóng lớn lặng im.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.508Z",
      "updatedAt": "2025-04-22T08:32:28.508Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "f76563a9-fdad-478c-8d14-1f8540f252ab",
      "number": 30,
      "content": "Họ rất đỗi vui mừng vì biển êm gió lặng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.521Z",
      "updatedAt": "2025-04-22T08:32:28.521Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "85171b9a-5fff-4de6-9f7d-49cd8ab57442",
      "number": 30,
      "content": "Rồi Ngài đưa họ đến bến bờ họ mong ước.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.537Z",
      "updatedAt": "2025-04-22T08:32:28.537Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "a1d78a49-14db-4127-9f03-21502c18a9af",
      "number": 32,
      "content": "Hãy để họ tôn cao Ngài giữa đại hội,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.575Z",
      "updatedAt": "2025-04-22T08:32:28.575Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "a3cc703d-ab1f-4619-b29c-64f4afb587c9",
      "number": 32,
      "content": "Và ca tụng Ngài giữa sảnh đường của hội đồng trưởng lão.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.591Z",
      "updatedAt": "2025-04-22T08:32:28.591Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "b9894253-5921-46ff-9d5c-4baf2506485d",
      "number": 33,
      "content": "Ngài biến các dòng sông thành sa mạc hoang vu,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.604Z",
      "updatedAt": "2025-04-22T08:32:28.604Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "31b7cb37-3e54-45e6-b10d-f72595f52e15",
      "number": 33,
      "content": "Các dòng suối thành đất khô khan cằn cỗi,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.619Z",
      "updatedAt": "2025-04-22T08:32:28.619Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "9ea7d9d8-2284-41c6-96b0-e13fa3640bdc",
      "number": 34,
      "content": "Ðất đai màu mỡ thành đồng chua nước mặn,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.631Z",
      "updatedAt": "2025-04-22T08:32:28.631Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "7baa581d-c173-40d4-837c-96fbe0606310",
      "number": 34,
      "content": "Vì sự gian ác của dân cư ở đó.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.652Z",
      "updatedAt": "2025-04-22T08:32:28.652Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "1f3cfcfc-aa52-4dad-ba90-45f20403c15f",
      "number": 35,
      "content": "Ngài biến đồng hoang thành đất có ao hồ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.664Z",
      "updatedAt": "2025-04-22T08:32:28.664Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "12b8cc8d-062d-4968-adc5-b6beaf09474b",
      "number": 35,
      "content": "Và xứ khô cằn thành nơi có dồi dào các dòng nước ngọt;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.675Z",
      "updatedAt": "2025-04-22T08:32:28.675Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "9f560211-cb21-40e4-a1b9-ff126264c6ba",
      "number": 36,
      "content": "Ngài đã cho những kẻ đói khát đến cư ngụ tại đó,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.688Z",
      "updatedAt": "2025-04-22T08:32:28.688Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "871cd52e-4806-4423-a4d4-21f6a35f2a6e",
      "number": 36,
      "content": "Ðể họ xây dựng một thành mà an cư lạc nghiệp;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.701Z",
      "updatedAt": "2025-04-22T08:32:28.701Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "283a06c6-921e-47d4-a520-9396cd2a01c3",
      "number": 37,
      "content": "Họ gieo mạ ngoài đồng và trồng tỉa vườn nho,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.714Z",
      "updatedAt": "2025-04-22T08:32:28.714Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "24ff6f9c-c6e3-4e0b-8eba-f90ca720cf51",
      "number": 37,
      "content": "Rồi thu hoạch hoa màu sung túc.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.731Z",
      "updatedAt": "2025-04-22T08:32:28.731Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "db90cc84-f0de-4099-a8e5-2f868777baff",
      "number": 38,
      "content": "Ngài ban phước cho họ, và dân số họ gia tăng;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.747Z",
      "updatedAt": "2025-04-22T08:32:28.747Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "70067d54-8f98-4318-93e7-723fdead3c5c",
      "number": 38,
      "content": "Ngài chẳng để cho bầy súc vật của họ bị giảm thiểu.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.761Z",
      "updatedAt": "2025-04-22T08:32:28.761Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "f479d51b-993a-4159-9161-e4e286ae20ad",
      "number": 39,
      "content": "Có lúc họ bị tiêu hao lụn bại, phải cúi đầu khuất phục ngoại bang,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.774Z",
      "updatedAt": "2025-04-22T08:32:28.774Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "3ad1320c-974c-43ec-9f8a-1f4d82ace4d8",
      "number": 39,
      "content": "Bị áp bức, khổ đau, và sầu não.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.787Z",
      "updatedAt": "2025-04-22T08:32:28.787Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "48a64fca-d59f-4feb-9652-3dcf0e06fbce",
      "number": 40,
      "content": "Ngài trút đổ ô nhục trên những người quyền quý của họ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.804Z",
      "updatedAt": "2025-04-22T08:32:28.804Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "f34519be-f33a-4be1-95c0-9d443742ce71",
      "number": 40,
      "content": "Khiến họ lang thang trong nơi hoang vắng không đường.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.817Z",
      "updatedAt": "2025-04-22T08:32:28.817Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "6714961a-33d8-48e0-8d7c-e50321e1cbdd",
      "number": 41,
      "content": "Nhưng Ngài lại cất những người nghèo ra khỏi cảnh lầm than,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.831Z",
      "updatedAt": "2025-04-22T08:32:28.831Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "25fe6831-ba2f-486d-9453-310e2653bf94",
      "number": 41,
      "content": "Và cho con cháu họ được gia tăng đông đúc.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.844Z",
      "updatedAt": "2025-04-22T08:32:28.844Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "160f50a2-ec81-49a3-86d3-2b165ea4621b",
      "number": 42,
      "content": "Những người ngay lành thấy thế bèn vui mừng;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.857Z",
      "updatedAt": "2025-04-22T08:32:28.857Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "2ca10749-3194-4426-92af-2eff7fa6f26a",
      "number": 42,
      "content": "Còn những kẻ gian ác thảy đều câm miệng lại.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.870Z",
      "updatedAt": "2025-04-22T08:32:28.870Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "c9255b29-c384-4505-91ba-30afec42dce5",
      "number": 43,
      "content": "Hỡi ai là người khôn ngoan, hãy suy gẫm những điều ấy,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.887Z",
      "updatedAt": "2025-04-22T08:32:28.887Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    },
    {
      "id": "aaf69063-98cd-4cf8-b047-ae469aef4dcf",
      "number": 43,
      "content": "Rồi sẽ hiểu được thế nào là tình thương của Chúa.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:32:28.901Z",
      "updatedAt": "2025-04-22T08:32:28.901Z",
      "chapterId": "c64ce309-bc37-47b8-a1f5-b7190ee44f1a"
    }
  ],
