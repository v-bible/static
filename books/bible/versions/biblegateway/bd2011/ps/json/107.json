{
  "chapter": {
    "id": "c4893943-7d6a-4653-8035-2a2c85e164c6",
    "number": 107,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20107&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.591Z",
    "updatedAt": "2025-04-24T03:03:17.591Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "4610bdf8-d42f-4262-aba5-bef9e136cdc8",
      "number": 1,
      "content": "Hãy cảm tạ Chúa, vì Ngài thật tốt,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.655Z",
      "updatedAt": "2025-04-24T04:00:34.655Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "71f981b1-f0ee-4dd8-855d-49ddda194dd8",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.696Z",
      "updatedAt": "2025-04-24T04:00:34.696Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "417138db-d9c9-4ef6-a698-89031c133e2d",
      "number": 2,
      "content": "Nguyện những người được cứu chuộc của Chúa nói như vậy,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.706Z",
      "updatedAt": "2025-04-24T04:00:34.706Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "9858a6da-c021-45db-9259-cee864a722d3",
      "number": 2,
      "content": "Những người được Ngài cứu khỏi tay kẻ thù,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.717Z",
      "updatedAt": "2025-04-24T04:00:34.717Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "104e741a-a6bb-4683-bd7f-b8f4fdb642b2",
      "number": 3,
      "content": "Rồi gom nhóm họ từ các nước trở về;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.728Z",
      "updatedAt": "2025-04-24T04:00:34.728Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "93336d8d-0913-49a5-80f8-58fa8e4ff518",
      "number": 3,
      "content": "Từ phương đông và phương tây, phương bắc và phương nam.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.741Z",
      "updatedAt": "2025-04-24T04:00:34.741Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "3720a71e-41ad-470c-afad-f43ba294d398",
      "number": 4,
      "content": "Họ lang thang trong đồng hoang, trong nơi hoang vắng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.754Z",
      "updatedAt": "2025-04-24T04:00:34.754Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "bdabc8c7-a90b-4944-bfa0-f7b1f9371de4",
      "number": 4,
      "content": "Họ chẳng tìm được đường về một thành để ở.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.768Z",
      "updatedAt": "2025-04-24T04:00:34.768Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "821609ee-3fcf-4b6a-9de0-5fae07da6773",
      "number": 5,
      "content": "Họ đói khát;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.781Z",
      "updatedAt": "2025-04-24T04:00:34.781Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "6e17cae5-5047-419d-9a12-e52cc3675f4b",
      "number": 5,
      "content": "Linh hồn họ mòn mỏi nao sờn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.795Z",
      "updatedAt": "2025-04-24T04:00:34.795Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "35941aa7-1807-4991-8f8f-762b8457e625",
      "number": 6,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.808Z",
      "updatedAt": "2025-04-24T04:00:34.808Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "bb115290-7099-4057-ad22-f0154c4c5734",
      "number": 6,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.821Z",
      "updatedAt": "2025-04-24T04:00:34.821Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "bf109c3e-abab-4a65-bcdb-792fe632316c",
      "number": 7,
      "content": "Ngài dẫn đưa họ đúng vào con đường phải đi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.834Z",
      "updatedAt": "2025-04-24T04:00:34.834Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "82d1a793-90ee-44d3-845a-6c109f080a55",
      "number": 7,
      "content": "Ðể họ đến một thành mà ở.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.853Z",
      "updatedAt": "2025-04-24T04:00:34.853Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "456f1efb-4a42-4522-8dd9-1a482ab6bffc",
      "number": 8,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.866Z",
      "updatedAt": "2025-04-24T04:00:34.866Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "b232699b-c070-44b1-b191-e83c9ff8ce88",
      "number": 8,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.880Z",
      "updatedAt": "2025-04-24T04:00:34.880Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "c1f227dc-5585-4bf9-b957-8a9cab05218f",
      "number": 9,
      "content": "Vì Ngài làm thỏa mãn linh hồn khao khát,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.893Z",
      "updatedAt": "2025-04-24T04:00:34.893Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "470e6fe4-2f11-46b5-86f6-c9bc3c10e556",
      "number": 9,
      "content": "Và làm no nê linh hồn đói lả bằng các thức ăn ngon.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.906Z",
      "updatedAt": "2025-04-24T04:00:34.906Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "fa789b6c-4d09-4fd7-af0b-77a1aa50f452",
      "number": 10,
      "content": "Có những kẻ sống trong tối tăm và trong bóng tử thần,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.920Z",
      "updatedAt": "2025-04-24T04:00:34.920Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "d53b8e35-3ee1-439d-8394-2d3f66aa4f1c",
      "number": 10,
      "content": "Bị giam cầm trong khổ đau và xiềng xích,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.933Z",
      "updatedAt": "2025-04-24T04:00:34.933Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "0de6e332-9ca7-4be1-9870-4b938c51391b",
      "number": 11,
      "content": "Bởi vì họ chống lại những lời của Ðức Chúa Trời,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.946Z",
      "updatedAt": "2025-04-24T04:00:34.946Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "fb5acf42-2543-44bd-be8e-cc79d24964e9",
      "number": 11,
      "content": "Và khinh bỉ lời chỉ bảo của Ðấng Chí Cao.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.959Z",
      "updatedAt": "2025-04-24T04:00:34.959Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "0cb617b2-a793-4e39-93c1-9d9ab86b894d",
      "number": 12,
      "content": "Vì thế Ngài lấy gian lao hạ lòng họ xuống;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.972Z",
      "updatedAt": "2025-04-24T04:00:34.972Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "1ab337fc-3983-4cab-bc84-e6037b739266",
      "number": 12,
      "content": "Họ bị ngã nhào và chẳng ai giúp họ đứng lên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.985Z",
      "updatedAt": "2025-04-24T04:00:34.985Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "a0ab3b34-bf2f-46cc-80e4-a8d36df63010",
      "number": 13,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:34.998Z",
      "updatedAt": "2025-04-24T04:00:34.998Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "c670c688-58fd-4273-9806-38a42e2a358b",
      "number": 13,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.011Z",
      "updatedAt": "2025-04-24T04:00:35.011Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "f720759a-7cdc-41c6-a446-5bf15b4ec28d",
      "number": 14,
      "content": "Ngài đem họ ra khỏi cảnh tối tăm và bóng tử thần,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.025Z",
      "updatedAt": "2025-04-24T04:00:35.025Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "3f0a0af9-06f4-4c2a-9f11-f7e962bee7c9",
      "number": 14,
      "content": "Và đập tan xiềng xích gông cùm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.038Z",
      "updatedAt": "2025-04-24T04:00:35.038Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "4f979548-70e0-4f7e-b939-3d180d450902",
      "number": 15,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.051Z",
      "updatedAt": "2025-04-24T04:00:35.051Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "62c5ee26-89b3-4e2d-b961-9846432c8577",
      "number": 15,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.064Z",
      "updatedAt": "2025-04-24T04:00:35.064Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "d3a3b6f2-5acb-4cd3-9457-a9bd7f24e475",
      "number": 16,
      "content": "Vì Ngài phá tan các cổng bằng đồng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.077Z",
      "updatedAt": "2025-04-24T04:00:35.077Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "f1751c9a-091a-47cb-813d-36e39e62dfdb",
      "number": 16,
      "content": "Và Ngài cắt rời những song bằng sắt.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.090Z",
      "updatedAt": "2025-04-24T04:00:35.090Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "9682d469-8f3a-45ac-ba6e-f96502d7acd4",
      "number": 17,
      "content": "Có những kẻ cứ bội nghịch nên hóa ra điên dại,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.104Z",
      "updatedAt": "2025-04-24T04:00:35.104Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "ffe9e85a-7d30-4385-8d36-3547998a6dd8",
      "number": 17,
      "content": "Và vì tội lỗi mình nên phải chuốc lấy khổ đau.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.117Z",
      "updatedAt": "2025-04-24T04:00:35.117Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "5dd83072-7241-4c83-9925-15d07d8f1019",
      "number": 18,
      "content": "Linh hồn họ gớm ghê mọi thực phẩm,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.131Z",
      "updatedAt": "2025-04-24T04:00:35.131Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "da1d87fa-4890-45dc-a930-295ebe530f24",
      "number": 18,
      "content": "Và họ bị kéo gần đến cổng của tử thần.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.147Z",
      "updatedAt": "2025-04-24T04:00:35.147Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "ff3a48f1-3587-4df3-97ee-ec90937e5643",
      "number": 19,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.161Z",
      "updatedAt": "2025-04-24T04:00:35.161Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "3cd0bed6-746c-4b76-9713-ec2897b58402",
      "number": 19,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.174Z",
      "updatedAt": "2025-04-24T04:00:35.174Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "040ae654-b154-4568-bcdf-db458d9f1f90",
      "number": 20,
      "content": "Ngài ban lời Ngài và chữa lành họ;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.187Z",
      "updatedAt": "2025-04-24T04:00:35.187Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "30f63e5f-d7c1-4419-878c-d1828f644a60",
      "number": 20,
      "content": "Ngài giải cứu họ khỏi miệng của phần mộ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.201Z",
      "updatedAt": "2025-04-24T04:00:35.201Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "b11bdc0c-3bbb-4a3a-80e0-62a78ff7ccfd",
      "number": 21,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.214Z",
      "updatedAt": "2025-04-24T04:00:35.214Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "55c2d354-daaf-4090-9ac6-e3c672c52d83",
      "number": 21,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.228Z",
      "updatedAt": "2025-04-24T04:00:35.228Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "ec73b8e0-9367-4e2a-8379-01c7ee639fd0",
      "number": 22,
      "content": "Cũng hãy để họ dâng những của lễ cảm tạ lên Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.241Z",
      "updatedAt": "2025-04-24T04:00:35.241Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "5af7da2e-e423-4d47-a1c1-df84e011390d",
      "number": 22,
      "content": "Và dùng những lời ca ngợi vui mừng thuật lại các công việc Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.254Z",
      "updatedAt": "2025-04-24T04:00:35.254Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "6f6ed794-2722-4b9c-974c-ecef9351a1ea",
      "number": 23,
      "content": "Có những kẻ xuống tàu vượt đại dương,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.267Z",
      "updatedAt": "2025-04-24T04:00:35.267Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "1e47cf2b-406a-4577-bfc8-b216c62c9e98",
      "number": 23,
      "content": "Ấy là những người làm ăn trên biển cả;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.280Z",
      "updatedAt": "2025-04-24T04:00:35.280Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "998edc77-f98c-4645-91a3-6f632e3d7510",
      "number": 24,
      "content": "Họ trông thấy những việc làm của Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.294Z",
      "updatedAt": "2025-04-24T04:00:35.294Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "2bbeea0c-2729-4105-96aa-d815540477c1",
      "number": 24,
      "content": "Và những việc lạ lùng của Ngài ở giữa biển sâu.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.306Z",
      "updatedAt": "2025-04-24T04:00:35.306Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "2b2640df-004c-4796-a18d-84ac772b47ae",
      "number": 25,
      "content": "Vì Ngài phán một tiếng, bão tố liền nổi lên cuồn cuộn,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.317Z",
      "updatedAt": "2025-04-24T04:00:35.317Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "968127c1-4976-4ec2-b26c-eeff85b321dc",
      "number": 25,
      "content": "Cuồng phong nâng cao những ngọn sóng kinh hồn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.328Z",
      "updatedAt": "2025-04-24T04:00:35.328Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "338f1f9f-3d45-45e3-b325-26b5a894741a",
      "number": 26,
      "content": "Sóng cả dâng cao đến tận trời, rồi trụt sâu xuống đáy vực thẳm;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.338Z",
      "updatedAt": "2025-04-24T04:00:35.338Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "cd7c3f27-4b3e-476d-8562-bd402a28046f",
      "number": 26,
      "content": "Giữa cảnh khốn cùng họ hồn phi phách tán.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.350Z",
      "updatedAt": "2025-04-24T04:00:35.350Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "3cff719c-9a7b-4811-b8d3-0feca3ab2805",
      "number": 27,
      "content": "Họ bị quay cuồng đảo điên như người say rượu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.363Z",
      "updatedAt": "2025-04-24T04:00:35.363Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "384fe12b-1106-49c4-a7ec-6be0a1e5c81e",
      "number": 27,
      "content": "Mọi tài trí khôn ngoan đều biến mất.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.376Z",
      "updatedAt": "2025-04-24T04:00:35.376Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "dbc01cd6-7a19-4969-8069-0924ce8889b8",
      "number": 28,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.393Z",
      "updatedAt": "2025-04-24T04:00:35.393Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "f2f8f05c-a055-4193-ac5c-c5bb378f08bb",
      "number": 28,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.407Z",
      "updatedAt": "2025-04-24T04:00:35.407Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "554163e5-b839-4bbb-9fb8-8d447a07daf8",
      "number": 29,
      "content": "Ngài khiến bão tố yên lặng như tờ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.420Z",
      "updatedAt": "2025-04-24T04:00:35.420Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "f2744833-3ac8-45a7-b32d-7eab70f24cd9",
      "number": 29,
      "content": "Những ngọn sóng lớn lặng im.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.433Z",
      "updatedAt": "2025-04-24T04:00:35.433Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "954ea35a-977f-4b50-9b4a-73a8c7f8c37d",
      "number": 30,
      "content": "Họ rất đỗi vui mừng vì biển êm gió lặng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.446Z",
      "updatedAt": "2025-04-24T04:00:35.446Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "6ed451f6-3023-4c6e-8512-82d531838b31",
      "number": 30,
      "content": "Rồi Ngài đưa họ đến bến bờ họ mong ước.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.458Z",
      "updatedAt": "2025-04-24T04:00:35.458Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "a4718ed6-48eb-4c2d-b5d7-2d4f2e2be05c",
      "number": 31,
      "content": "Hãy để họ cảm tạ Chúa vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.469Z",
      "updatedAt": "2025-04-24T04:00:35.469Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "0d86aa44-56bc-40bb-b00a-e6f1d152d249",
      "number": 31,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.480Z",
      "updatedAt": "2025-04-24T04:00:35.480Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "4e9c09f9-a1b3-4873-a05d-8c481977f406",
      "number": 32,
      "content": "Hãy để họ tôn cao Ngài giữa đại hội,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.497Z",
      "updatedAt": "2025-04-24T04:00:35.497Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "5fa4a3d4-49ba-48b1-8822-1cb4e339e9d4",
      "number": 32,
      "content": "Và ca tụng Ngài giữa sảnh đường của hội đồng trưởng lão.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.507Z",
      "updatedAt": "2025-04-24T04:00:35.507Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "03905085-93c6-4110-a2e4-46ee71a69bfe",
      "number": 33,
      "content": "Ngài biến các dòng sông thành sa mạc hoang vu,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.520Z",
      "updatedAt": "2025-04-24T04:00:35.520Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "c5c7999a-f7e1-4234-b105-93eac5d44c40",
      "number": 33,
      "content": "Các dòng suối thành đất khô khan cằn cỗi,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.533Z",
      "updatedAt": "2025-04-24T04:00:35.533Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "954d2ede-c8b5-4716-9979-61a7f02a55d7",
      "number": 34,
      "content": "Ðất đai màu mỡ thành đồng chua nước mặn,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.546Z",
      "updatedAt": "2025-04-24T04:00:35.546Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "47467955-6206-4f29-b3ce-2aeafb0b46e2",
      "number": 34,
      "content": "Vì sự gian ác của dân cư ở đó.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.560Z",
      "updatedAt": "2025-04-24T04:00:35.560Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "79cb7faf-6165-4dd3-8ae8-6414102ccf22",
      "number": 35,
      "content": "Ngài biến đồng hoang thành đất có ao hồ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.573Z",
      "updatedAt": "2025-04-24T04:00:35.573Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "382c59a9-2b13-4ce3-9d22-ef7bfc5a71d2",
      "number": 35,
      "content": "Và xứ khô cằn thành nơi có dồi dào các dòng nước ngọt;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.586Z",
      "updatedAt": "2025-04-24T04:00:35.586Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "02b0b99a-ab21-4abc-be3f-0310098ee033",
      "number": 36,
      "content": "Ngài đã cho những kẻ đói khát đến cư ngụ tại đó,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.600Z",
      "updatedAt": "2025-04-24T04:00:35.600Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "46849afa-0d2a-48d2-b1d0-e29adcec84d2",
      "number": 36,
      "content": "Ðể họ xây dựng một thành mà an cư lạc nghiệp;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.613Z",
      "updatedAt": "2025-04-24T04:00:35.613Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "31a8af92-5ca2-4789-be5f-057d965038bf",
      "number": 37,
      "content": "Họ gieo mạ ngoài đồng và trồng tỉa vườn nho,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.626Z",
      "updatedAt": "2025-04-24T04:00:35.626Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "dee1dc60-3622-469a-a102-7284e1af13ca",
      "number": 37,
      "content": "Rồi thu hoạch hoa màu sung túc.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.639Z",
      "updatedAt": "2025-04-24T04:00:35.639Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "8b5d150c-3b3b-4a66-bec4-fa4a1287b2e7",
      "number": 38,
      "content": "Ngài ban phước cho họ, và dân số họ gia tăng;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.652Z",
      "updatedAt": "2025-04-24T04:00:35.652Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "368a7d6f-0e7e-4e7d-9230-a0c3ce083e0e",
      "number": 38,
      "content": "Ngài chẳng để cho bầy súc vật của họ bị giảm thiểu.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.666Z",
      "updatedAt": "2025-04-24T04:00:35.666Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "f3e1daed-1e15-424d-9d00-4a1ed1612152",
      "number": 39,
      "content": "Có lúc họ bị tiêu hao lụn bại, phải cúi đầu khuất phục ngoại bang,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.679Z",
      "updatedAt": "2025-04-24T04:00:35.679Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "12d94638-f8ea-4051-94d0-17dff9c8e054",
      "number": 39,
      "content": "Bị áp bức, khổ đau, và sầu não.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.693Z",
      "updatedAt": "2025-04-24T04:00:35.693Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "881b06f6-93a4-4d13-b1a4-6be0db1ca3fc",
      "number": 40,
      "content": "Ngài trút đổ ô nhục trên những người quyền quý của họ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.706Z",
      "updatedAt": "2025-04-24T04:00:35.706Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "beaf4126-62df-49b8-bd75-a77696478aed",
      "number": 40,
      "content": "Khiến họ lang thang trong nơi hoang vắng không đường.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.720Z",
      "updatedAt": "2025-04-24T04:00:35.720Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "63238a1a-ee59-4bb7-802d-51704df11c04",
      "number": 41,
      "content": "Nhưng Ngài lại cất những người nghèo ra khỏi cảnh lầm than,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.733Z",
      "updatedAt": "2025-04-24T04:00:35.733Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "77316ba8-c5aa-4227-a6fa-9ca067c7e878",
      "number": 41,
      "content": "Và cho con cháu họ được gia tăng đông đúc.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.746Z",
      "updatedAt": "2025-04-24T04:00:35.746Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "06916071-c781-4f4e-8018-9a48cdf51a0a",
      "number": 42,
      "content": "Những người ngay lành thấy thế bèn vui mừng;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.760Z",
      "updatedAt": "2025-04-24T04:00:35.760Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "71cd2172-f019-4dd2-b5bd-673718589353",
      "number": 42,
      "content": "Còn những kẻ gian ác thảy đều câm miệng lại.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.773Z",
      "updatedAt": "2025-04-24T04:00:35.773Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "47ae856c-2cc8-4e39-ab19-5d446f093633",
      "number": 43,
      "content": "Hỡi ai là người khôn ngoan, hãy suy gẫm những điều ấy,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.786Z",
      "updatedAt": "2025-04-24T04:00:35.786Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    },
    {
      "id": "44120b7b-b587-4468-9f4f-9d00f7a236d9",
      "number": 43,
      "content": "Rồi sẽ hiểu được thế nào là tình thương của Chúa.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:00:35.803Z",
      "updatedAt": "2025-04-24T04:00:35.803Z",
      "chapterId": "c4893943-7d6a-4653-8035-2a2c85e164c6"
    }
  ],
