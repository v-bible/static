{
  "chapter": {
    "id": "2f67040d-0af3-4b92-b0cd-86faf33c2654",
    "number": 148,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20148&version=BD2011",
    "createdAt": "2025-04-24T03:03:18.645Z",
    "updatedAt": "2025-04-24T03:03:18.645Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "5476c33c-566c-460f-b8aa-ba42bb1bf3ac",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.291Z",
      "updatedAt": "2025-04-24T04:05:38.291Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "274ac3ef-4261-49d9-84d2-bf83eb417038",
      "number": 1,
      "content": "Hãy ca ngợi Chúa trên các tầng trời!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.319Z",
      "updatedAt": "2025-04-24T04:05:38.319Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "e40288ef-90e6-4787-a02d-5f12c045835c",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong cõi trời cao!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.329Z",
      "updatedAt": "2025-04-24T04:05:38.329Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "a53d98fb-54ca-4375-b3a7-6095c78c78af",
      "number": 2,
      "content": "Hãy ca ngợi Ngài, hỡi tất cả các thiên sứ của Ngài!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.341Z",
      "updatedAt": "2025-04-24T04:05:38.341Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "c5104c6e-2969-40b7-b1e1-1da7833da516",
      "number": 2,
      "content": "Hãy ca ngợi Ngài, hỡi tất cả các thiên binh của Ngài!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.351Z",
      "updatedAt": "2025-04-24T04:05:38.351Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "78d8cf10-5ea6-40be-a15f-872a14083a3d",
      "number": 3,
      "content": "Hãy ca ngợi Ngài, hỡi mặt trời và mặt trăng!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.361Z",
      "updatedAt": "2025-04-24T04:05:38.361Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "7f84946c-9cdc-4fa4-a819-6660ddbdf582",
      "number": 3,
      "content": "Hãy ca ngợi Ngài, hỡi tất cả các ngươi, những vì sao sáng!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.371Z",
      "updatedAt": "2025-04-24T04:05:38.371Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "780e0ed9-4794-415a-8a88-fbbd42eeaa25",
      "number": 4,
      "content": "Hãy ca ngợi Ngài, hỡi các ngươi, các tầng trời cao thăm thẳm!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.381Z",
      "updatedAt": "2025-04-24T04:05:38.381Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "748cf926-97f6-4b5b-936c-ab291eb927f4",
      "number": 4,
      "content": "Và hỡi các ngươi, các nguồn nước trên các tầng trời!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.391Z",
      "updatedAt": "2025-04-24T04:05:38.391Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "6a144911-db27-4188-9220-1666ef4967cf",
      "number": 5,
      "content": "Tất cả hãy ca ngợi danh Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.401Z",
      "updatedAt": "2025-04-24T04:05:38.401Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "19d3b642-299a-45bc-9380-50c4c66c1f44",
      "number": 5,
      "content": "Vì Ngài ra lịnh, mọi vật được dựng nên.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.411Z",
      "updatedAt": "2025-04-24T04:05:38.411Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "501ae365-ec16-4dda-9026-b3076c75744d",
      "number": 6,
      "content": "Ngài thiết lập chúng, để chúng trường tồn mãi mãi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.421Z",
      "updatedAt": "2025-04-24T04:05:38.421Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "eab30dd4-f9d9-44c4-b63f-d8b7d5ec4044",
      "number": 6,
      "content": "Ngài ban một định luật, và chúng không bao giờ bị đào thải.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.431Z",
      "updatedAt": "2025-04-24T04:05:38.431Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "50a0643b-24ab-4129-8ca5-18d168e60180",
      "number": 7,
      "content": "Hãy ca ngợi Chúa ở dưới đất,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.442Z",
      "updatedAt": "2025-04-24T04:05:38.442Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "17cba6c0-5f9e-4cd8-a69d-6737d32490e3",
      "number": 7,
      "content": "Hỡi các thủy quái và các vực sâu,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.452Z",
      "updatedAt": "2025-04-24T04:05:38.452Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "9f6b02cc-8195-4d0e-805f-9a7abf9ba0cb",
      "number": 8,
      "content": "Hỡi lửa và mưa đá, hỡi tuyết và mây,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.472Z",
      "updatedAt": "2025-04-24T04:05:38.472Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "6bee1861-7f9e-44bd-b83d-dfcda830794a",
      "number": 8,
      "content": "Hỡi gió bão luôn vâng lời Ngài,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.482Z",
      "updatedAt": "2025-04-24T04:05:38.482Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "04407821-1cd1-42d0-b1f1-962a9f7f6422",
      "number": 9,
      "content": "Hỡi các núi non và mọi đồi nổng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.492Z",
      "updatedAt": "2025-04-24T04:05:38.492Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "9b7932f6-50da-45b4-8996-f8eed8d32070",
      "number": 9,
      "content": "Hỡi những cây ra trái và những cây bá hương,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.502Z",
      "updatedAt": "2025-04-24T04:05:38.502Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "f105e2c8-9593-471d-b423-68bc25ef3c3f",
      "number": 10,
      "content": "Hỡi những thú rừng và mọi loài súc vật,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.512Z",
      "updatedAt": "2025-04-24T04:05:38.512Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "873f1003-cbc5-4df7-afc5-fee20319dbba",
      "number": 10,
      "content": "Hỡi những côn trùng và những điểu thú,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.522Z",
      "updatedAt": "2025-04-24T04:05:38.522Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "27d0f878-1bc4-4d4d-a61c-ed4eea267685",
      "number": 11,
      "content": "Hỡi các vua và mọi dân trên đất,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.531Z",
      "updatedAt": "2025-04-24T04:05:38.531Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "9de29e47-bada-451c-a514-5df885e987a1",
      "number": 11,
      "content": "Hỡi các lãnh chúa và các lãnh tụ trên thế gian,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.541Z",
      "updatedAt": "2025-04-24T04:05:38.541Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "7cdcdadd-c54b-42d9-8b6d-d6ba51fda273",
      "number": 12,
      "content": "Hỡi tất cả thanh thiếu niên nam nữ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.551Z",
      "updatedAt": "2025-04-24T04:05:38.551Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "ec2ca267-8ac4-4791-b5c0-6017997ae56c",
      "number": 12,
      "content": "Hỡi quý cụ lão thành và hỡi những trẻ còn thơ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.561Z",
      "updatedAt": "2025-04-24T04:05:38.561Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "897ea3b1-3ed5-4f90-9c9f-e77ef3593930",
      "number": 13,
      "content": "Tất cả hãy ca ngợi danh Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.572Z",
      "updatedAt": "2025-04-24T04:05:38.572Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "21367213-d007-4978-86f3-17a73cf51c0b",
      "number": 13,
      "content": "Vì chỉ có danh Ngài mới đáng được tôn cao;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.582Z",
      "updatedAt": "2025-04-24T04:05:38.582Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "6747a25f-bc1c-471f-ab76-200a38dfad3a",
      "number": 13,
      "content": "Vinh quang Ngài vượt trỗi trên cả đất và trời.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.592Z",
      "updatedAt": "2025-04-24T04:05:38.592Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "c2738e9c-88f4-416d-9e2b-696d6e4c4a4b",
      "number": 14,
      "content": "Ngài nhấc cao sừng của dân Ngài lên,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.602Z",
      "updatedAt": "2025-04-24T04:05:38.602Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "363cd024-0511-4e02-a689-17b778c78eaa",
      "number": 14,
      "content": "Nên hãy ca ngợi Ngài, hỡi tất cả thánh dân Ngài,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.612Z",
      "updatedAt": "2025-04-24T04:05:38.612Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "2d033890-a3b0-4053-a120-5211cbbba508",
      "number": 14,
      "content": "Con cháu của I-sơ-ra-ên, một dân gần gũi bên Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.622Z",
      "updatedAt": "2025-04-24T04:05:38.622Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    },
    {
      "id": "5db09a13-60c7-4875-81d7-7de7f8c0e68d",
      "number": 14,
      "content": "Hãy ca ngợi Chúa!",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:38.632Z",
      "updatedAt": "2025-04-24T04:05:38.632Z",
      "chapterId": "2f67040d-0af3-4b92-b0cd-86faf33c2654"
    }
  ],
