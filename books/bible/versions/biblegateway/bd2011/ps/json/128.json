  },
  "verses": [
    {
      "id": "ff2c95cc-0683-4170-ba41-c6ec3e14a217",
      "number": 1,
      "content": "Phước cho người kính sợ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:59.717Z",
      "updatedAt": "2025-04-22T08:34:59.717Z",
      "chapterId": "f40404a2-1a44-4789-836f-0170da5267b1"
    },
    {
      "id": "f90be547-9846-4189-a5c9-c53d1bceee29",
      "number": 1,
      "content": "Người đi trong đường lối Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:59.746Z",
      "updatedAt": "2025-04-22T08:34:59.746Z",
      "chapterId": "f40404a2-1a44-4789-836f-0170da5267b1"
    },
    {
      "id": "9083df3a-2152-482b-a6a6-9de9898109da",
      "number": 2,
      "content": "Bạn sẽ hưởng hoa lợi của tay mình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:59.759Z",
      "updatedAt": "2025-04-22T08:34:59.759Z",
      "chapterId": "f40404a2-1a44-4789-836f-0170da5267b1"
    },
    {
      "id": "3f59e23b-0835-4390-9752-f040c06d3f80",
      "number": 2,
      "content": "Bạn sẽ được phước và thịnh vượng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:59.772Z",
      "updatedAt": "2025-04-22T08:34:59.772Z",
      "chapterId": "f40404a2-1a44-4789-836f-0170da5267b1"
    },
    {
      "id": "4967fc63-aa86-4cb4-8ea0-ceb61b3245cf",
      "number": 3,
      "content": "Vợ bạn sẽ như cây nho sai trái trong nhà bạn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:59.782Z",
      "updatedAt": "2025-04-22T08:34:59.782Z",
      "chapterId": "f40404a2-1a44-4789-836f-0170da5267b1"
    },
    {
      "id": "6dd22348-1068-4060-adc7-b03273cdf961",
      "number": 3,
      "content": "Con cái bạn sẽ như các chồi ô-liu quanh bàn bạn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:59.792Z",
      "updatedAt": "2025-04-22T08:34:59.792Z",
      "chapterId": "f40404a2-1a44-4789-836f-0170da5267b1"
    },
    {
      "id": "20e22076-ad00-4631-92e8-36ceace03dcf",
      "number": 4,
      "content": "Kìa, người nào kính sợ Chúa sẽ được phước như vậy.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:59.802Z",
      "updatedAt": "2025-04-22T08:34:59.802Z",
      "chapterId": "f40404a2-1a44-4789-836f-0170da5267b1"
    },
    {
      "id": "f958380f-0671-4ec8-b188-567c4d14ceeb",
      "number": 5,
      "content": "Nguyện Chúa ban phước cho bạn từ Si-ôn.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:59.812Z",
      "updatedAt": "2025-04-22T08:34:59.812Z",
      "chapterId": "f40404a2-1a44-4789-836f-0170da5267b1"
    },
    {
      "id": "fd49dd81-7296-41fb-8c25-7d2099e4b304",
      "number": 5,
      "content": "Nguyện suốt đời bạn chứng kiến những phước hạnh của Giê-ru-sa-lem.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:59.822Z",
      "updatedAt": "2025-04-22T08:34:59.822Z",
      "chapterId": "f40404a2-1a44-4789-836f-0170da5267b1"
    },
    {
      "id": "9ca7b15e-870f-4b9c-a879-a0a0a0f98c8e",
      "number": 6,
      "content": "Nguyện bạn được nhìn thấy con cháu của con cái bạn.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:59.832Z",
      "updatedAt": "2025-04-22T08:34:59.832Z",
      "chapterId": "f40404a2-1a44-4789-836f-0170da5267b1"
    },
    {
      "id": "52f9d23c-da4a-43eb-a80a-ea3a8fd392bf",
      "number": 6,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:59.842Z",
      "updatedAt": "2025-04-22T08:34:59.842Z",
      "chapterId": "f40404a2-1a44-4789-836f-0170da5267b1"
    }
  ],
