{
  "chapter": {
    "id": "f40404a2-1a44-4789-836f-0170da5267b1",
    "number": 128,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20128&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.710Z",
