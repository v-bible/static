  },
  "verses": [
    {
      "id": "dd20ffe1-a1e7-48bd-9482-f5377260b70a",
      "number": 1,
      "content": "Phước cho người kính sợ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:15.330Z",
      "updatedAt": "2025-04-24T04:03:15.330Z",
      "chapterId": "7e898e2d-55ce-440d-82e4-132162c9172d"
    },
    {
      "id": "f98430b0-6d97-4c04-8cbe-591afeec228f",
      "number": 1,
      "content": "Người đi trong đường lối Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:15.358Z",
      "updatedAt": "2025-04-24T04:03:15.358Z",
      "chapterId": "7e898e2d-55ce-440d-82e4-132162c9172d"
    },
    {
      "id": "f74d1063-435e-4f86-b499-c34c5ccd2c2e",
      "number": 2,
      "content": "Bạn sẽ hưởng hoa lợi của tay mình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:15.368Z",
      "updatedAt": "2025-04-24T04:03:15.368Z",
      "chapterId": "7e898e2d-55ce-440d-82e4-132162c9172d"
    },
    {
      "id": "740ef00e-d1ac-435d-bc50-b45bfa9e0893",
      "number": 2,
      "content": "Bạn sẽ được phước và thịnh vượng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:15.378Z",
      "updatedAt": "2025-04-24T04:03:15.378Z",
      "chapterId": "7e898e2d-55ce-440d-82e4-132162c9172d"
    },
    {
      "id": "da9c1908-715c-49d9-9622-988fe441defb",
      "number": 3,
      "content": "Vợ bạn sẽ như cây nho sai trái trong nhà bạn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:15.388Z",
      "updatedAt": "2025-04-24T04:03:15.388Z",
      "chapterId": "7e898e2d-55ce-440d-82e4-132162c9172d"
    },
    {
      "id": "18605709-6e13-44e6-b940-410ee037c549",
      "number": 3,
      "content": "Con cái bạn sẽ như các chồi ô-liu quanh bàn bạn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:15.401Z",
      "updatedAt": "2025-04-24T04:03:15.401Z",
      "chapterId": "7e898e2d-55ce-440d-82e4-132162c9172d"
    },
    {
      "id": "72c7e22d-4bf4-4a3a-89e8-b5303bb22af0",
      "number": 4,
      "content": "Kìa, người nào kính sợ Chúa sẽ được phước như vậy.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:15.412Z",
      "updatedAt": "2025-04-24T04:03:15.412Z",
      "chapterId": "7e898e2d-55ce-440d-82e4-132162c9172d"
    },
    {
      "id": "bd6e138d-2d80-4310-b75e-bd31ab561533",
      "number": 5,
      "content": "Nguyện Chúa ban phước cho bạn từ Si-ôn.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:15.422Z",
      "updatedAt": "2025-04-24T04:03:15.422Z",
      "chapterId": "7e898e2d-55ce-440d-82e4-132162c9172d"
    },
    {
      "id": "1db670fd-9d2d-4121-aa72-1730425f92ce",
      "number": 5,
      "content": "Nguyện suốt đời bạn chứng kiến những phước hạnh của Giê-ru-sa-lem.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:15.433Z",
      "updatedAt": "2025-04-24T04:03:15.433Z",
      "chapterId": "7e898e2d-55ce-440d-82e4-132162c9172d"
    },
    {
      "id": "1a2ea547-15ec-4795-a6e6-5ecfe52b1655",
      "number": 6,
      "content": "Nguyện bạn được nhìn thấy con cháu của con cái bạn.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:15.444Z",
      "updatedAt": "2025-04-24T04:03:15.444Z",
      "chapterId": "7e898e2d-55ce-440d-82e4-132162c9172d"
    },
    {
      "id": "8dd08ecd-44a5-4ee7-a57e-acc2afbe18a1",
      "number": 6,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:03:15.454Z",
      "updatedAt": "2025-04-24T04:03:15.454Z",
      "chapterId": "7e898e2d-55ce-440d-82e4-132162c9172d"
    }
  ],
