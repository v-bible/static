{
  "chapter": {
    "id": "7e898e2d-55ce-440d-82e4-132162c9172d",
    "number": 128,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20128&version=BD2011",
    "createdAt": "2025-04-24T03:03:18.130Z",
