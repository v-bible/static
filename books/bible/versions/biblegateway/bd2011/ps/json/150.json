  },
  "verses": [
    {
      "id": "ef2da276-f808-40d7-b740-68ac1f3b5f94",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.083Z",
      "updatedAt": "2025-04-24T04:05:52.083Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "77ad3149-c7e6-402d-804b-132c2c73a6f6",
      "number": 1,
      "content": "Hãy ca ngợi Ðức Chúa Trời trong nơi thánh Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.113Z",
      "updatedAt": "2025-04-24T04:05:52.113Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "d99a689f-5e11-451c-b529-cbc74c7a387c",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong bầu trời lớn lao vô tận của Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.124Z",
      "updatedAt": "2025-04-24T04:05:52.124Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "c9bcac7e-aa87-4281-8017-2e16e0d604c1",
      "number": 2,
      "content": "Hãy ca ngợi Ngài về những việc quyền năng Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.134Z",
      "updatedAt": "2025-04-24T04:05:52.134Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "35216dc7-d2e5-4414-9379-131125c8a860",
      "number": 2,
      "content": "Hãy ca ngợi Ngài theo sự lớn lao vĩ đại của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.145Z",
      "updatedAt": "2025-04-24T04:05:52.145Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "83a38a57-5a3a-4e26-be98-647517cdfceb",
      "number": 3,
      "content": "Hãy dùng tiếng kèn ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.157Z",
      "updatedAt": "2025-04-24T04:05:52.157Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "ea967e02-bc75-4758-9987-fbd79e787179",
      "number": 3,
      "content": "Hãy dùng đàn lia và hạc cầm ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.170Z",
      "updatedAt": "2025-04-24T04:05:52.170Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "8c3b1d54-8ac3-41ef-8a4c-8a73eda79436",
      "number": 4,
      "content": "Hãy dùng trống lục lạc và điệu vũ ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.183Z",
      "updatedAt": "2025-04-24T04:05:52.183Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "dc77f22f-2ff7-44a2-a1bf-5248afbeaea5",
      "number": 4,
      "content": "Hãy dùng các đàn dây và ống sáo ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.199Z",
      "updatedAt": "2025-04-24T04:05:52.199Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "fc7b0d81-9462-440e-a975-86b7090c52fb",
      "number": 5,
      "content": "Hãy dùng các phèng la vang tiếng ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.212Z",
      "updatedAt": "2025-04-24T04:05:52.212Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "e2e68aa3-4f83-40f0-9ff8-ccabdaa38337",
      "number": 5,
      "content": "Hãy dùng các khánh chiêng vang rền ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.225Z",
      "updatedAt": "2025-04-24T04:05:52.225Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "bb2da2fd-83e1-4f30-96e4-1d80957cce1b",
      "number": 6,
      "content": "Phàm vật chi thở, hãy ca ngợi Chúa.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:52.238Z",
      "updatedAt": "2025-04-24T04:05:52.238Z",
      "chapterId": "a2856d97-1592-4cf2-8c31-d3a4f1c4de72"
    },
    {
      "id": "44f51064-0487-4cf6-ac72-097a58d8df49",
      "number": 6,
      "content": "Hãy ca ngợi Chúa!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
