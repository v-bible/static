  "verses": [
    {
      "id": "49893e50-0a11-4498-be3e-5e8e2a2707f8",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.295Z",
      "updatedAt": "2025-04-22T08:37:31.295Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "956482e9-2b1e-4019-bae9-67f46ba0a1f6",
      "number": 6,
      "content": "Hãy ca ngợi Chúa!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.468Z",
      "updatedAt": "2025-04-22T08:37:31.468Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "e32d00ec-1201-4a69-aa0c-c5a20944d9fa",
      "number": 1,
      "content": "Hãy ca ngợi Ðức Chúa Trời trong nơi thánh Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.323Z",
      "updatedAt": "2025-04-22T08:37:31.323Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "6bba76ef-9b6e-4b27-b6e0-651d9da6ed46",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong bầu trời lớn lao vô tận của Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.333Z",
      "updatedAt": "2025-04-22T08:37:31.333Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "47d57ca9-36fa-4ed0-b2a7-705aac3d3442",
      "number": 2,
      "content": "Hãy ca ngợi Ngài về những việc quyền năng Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.345Z",
      "updatedAt": "2025-04-22T08:37:31.345Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "a22e3532-c4aa-41ce-8d6c-f286c61c120b",
      "number": 2,
      "content": "Hãy ca ngợi Ngài theo sự lớn lao vĩ đại của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.358Z",
      "updatedAt": "2025-04-22T08:37:31.358Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "6f973eea-29e3-4265-92ea-49bcb2a426af",
      "number": 3,
      "content": "Hãy dùng tiếng kèn ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.371Z",
      "updatedAt": "2025-04-22T08:37:31.371Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "dd3bd1f1-4471-4111-a7d2-9f9b65d878d0",
      "number": 3,
      "content": "Hãy dùng đàn lia và hạc cầm ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.385Z",
      "updatedAt": "2025-04-22T08:37:31.385Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "392becc0-580c-4529-9ef0-15ba00a8aa2e",
      "number": 4,
      "content": "Hãy dùng trống lục lạc và điệu vũ ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.401Z",
      "updatedAt": "2025-04-22T08:37:31.401Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "f0f6ba77-6d07-4929-b3ff-8134c7ab8081",
      "number": 4,
      "content": "Hãy dùng các đàn dây và ống sáo ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.415Z",
      "updatedAt": "2025-04-22T08:37:31.415Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "f83642d8-d51a-4408-b955-20bfad580054",
      "number": 5,
      "content": "Hãy dùng các phèng la vang tiếng ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.428Z",
      "updatedAt": "2025-04-22T08:37:31.428Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "6418ff4c-501d-4133-a653-1279d0609519",
      "number": 5,
      "content": "Hãy dùng các khánh chiêng vang rền ca ngợi Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.441Z",
      "updatedAt": "2025-04-22T08:37:31.441Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    },
    {
      "id": "7d8d17e4-51a8-4bc3-995a-7fa3dbc841d3",
      "number": 6,
      "content": "Phàm vật chi thở, hãy ca ngợi Chúa.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:31.454Z",
      "updatedAt": "2025-04-22T08:37:31.454Z",
      "chapterId": "db6e37be-61b0-4ac3-90f0-afc89d460de3"
    }
  ],
