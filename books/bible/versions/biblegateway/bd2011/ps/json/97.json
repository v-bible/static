{
  "chapter": {
    "id": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c",
    "number": 97,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2097&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.287Z",
    "updatedAt": "2025-04-24T03:03:17.287Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "961b3373-854e-4fc4-a582-724a3071312e",
      "number": 1,
      "content": "Chúa trị vì!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.359Z",
      "updatedAt": "2025-04-24T03:59:11.359Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "9b47f1bc-d4f5-44db-b2f1-653c51dcf775",
      "number": 1,
      "content": "Hỡi trái đất, hãy vui mừng;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.385Z",
      "updatedAt": "2025-04-24T03:59:11.385Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "2d7da784-25ca-4425-ae72-6e827ace9df6",
      "number": 1,
      "content": "Hỡi muôn ngàn hải đảo, hãy hân hoan.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.396Z",
      "updatedAt": "2025-04-24T03:59:11.396Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "be167a36-700b-43b6-8b7c-bcb3a5949f51",
      "number": 2,
      "content": "Mây dày và bóng tối mù mịt bao quanh Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.406Z",
      "updatedAt": "2025-04-24T03:59:11.406Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "56a6b195-1a20-4979-995f-ae4d2ace2232",
      "number": 2,
      "content": "Công chính và công lý là nền tảng của ngai Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.417Z",
      "updatedAt": "2025-04-24T03:59:11.417Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "f21f9948-969b-4ecc-9721-da0750f1862e",
      "number": 3,
      "content": "Lửa hừng đi trước mặt Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.427Z",
      "updatedAt": "2025-04-24T03:59:11.427Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "4d51d0ea-5de4-49cd-8c80-ef19175d194a",
      "number": 3,
      "content": "Thiêu hủy mọi kẻ chống nghịch chung quanh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.440Z",
      "updatedAt": "2025-04-24T03:59:11.440Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "3fbb910e-5b6e-4d3f-a1a2-05ba91d5a528",
      "number": 4,
      "content": "Các tia chớp của Ngài lóe lên rực sáng khắp thế gian;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.450Z",
      "updatedAt": "2025-04-24T03:59:11.450Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "cec33376-1232-4569-9cd4-88e95893b793",
      "number": 4,
      "content": "Ðất thấy vậy run lên lẩy bẩy.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.461Z",
      "updatedAt": "2025-04-24T03:59:11.461Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "dcf15957-1dd4-4561-b5ed-db91b022c782",
      "number": 5,
      "content": "Các núi đồi tan chảy như sáp trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.471Z",
      "updatedAt": "2025-04-24T03:59:11.471Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "a25942ba-bc08-40ae-8376-60661d735819",
      "number": 5,
      "content": "Tức trước thánh nhan của Ðấng Chúa Tể toàn cầu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.481Z",
      "updatedAt": "2025-04-24T03:59:11.481Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "660caad2-9606-4329-8049-994c82ef6229",
      "number": 6,
      "content": "Các tầng trời rao truyền đức công chính Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.492Z",
      "updatedAt": "2025-04-24T03:59:11.492Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "723481c0-cbd8-469a-af3f-5684b811db09",
      "number": 6,
      "content": "Tất cả các dân đều thấy vinh quang Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.502Z",
      "updatedAt": "2025-04-24T03:59:11.502Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "a744b5c0-c96b-4fd6-b14b-165d7f8a819f",
      "number": 7,
      "content": "Hãy để mọi kẻ thờ lạy các hình tượng bị xấu hổ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.513Z",
      "updatedAt": "2025-04-24T03:59:11.513Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "e4ab27b2-e1f5-4479-b4cb-a8cd0ef6492b",
      "number": 7,
      "content": "Tức những kẻ khoe mình về các hình tượng vô tri của chúng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.524Z",
      "updatedAt": "2025-04-24T03:59:11.524Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "656400e4-84d9-4280-bd3c-b4693144e564",
      "number": 7,
      "content": "Hỡi tất cả các thần, hãy sấp mình xuống thờ lạy Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.534Z",
      "updatedAt": "2025-04-24T03:59:11.534Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "3b366fba-85cc-44f2-b5c1-94812a825039",
      "number": 8,
      "content": "Si-ôn nghe thế bèn vui mừng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.547Z",
      "updatedAt": "2025-04-24T03:59:11.547Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "eeaaa8a2-4731-4098-9fc8-040b97e951bd",
      "number": 8,
      "content": "Các ái nữ của Giu-đa đều vui vẻ,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.558Z",
      "updatedAt": "2025-04-24T03:59:11.558Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "da58df8f-9b76-4565-97b3-71f831326861",
      "number": 8,
      "content": "Vì những phán quyết của Ngài, Chúa ôi;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.569Z",
      "updatedAt": "2025-04-24T03:59:11.569Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "97775c5c-ee4f-4d2b-a6b0-62bdd36aa4c7",
      "number": 9,
      "content": "Vì Ngài, Chúa ôi, là Ðấng Tối Cao trên khắp đất;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.579Z",
      "updatedAt": "2025-04-24T03:59:11.579Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "d1ed6bc4-b457-46eb-a6b8-b5d8537846d7",
      "number": 9,
      "content": "Ngài vượt trỗi bỏ xa tất cả các thần.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.592Z",
      "updatedAt": "2025-04-24T03:59:11.592Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "2786c581-7d3c-40fd-adb8-958898b8a04e",
      "number": 10,
      "content": "Hỡi những người yêu mến Chúa, hãy ghét điều ác;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.601Z",
      "updatedAt": "2025-04-24T03:59:11.601Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "ee04ddfb-e894-4c27-bb4f-67e104187112",
      "number": 10,
      "content": "Ngài bảo vệ sinh mạng các thánh đồ của Ngài khỏi tay kẻ ác.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.612Z",
      "updatedAt": "2025-04-24T03:59:11.612Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "6f1149cd-e767-4fd6-8c01-67b51c7b193c",
      "number": 11,
      "content": "Ánh sáng tỏa rạng trên người ngay lành,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.622Z",
      "updatedAt": "2025-04-24T03:59:11.622Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "18e4ecdf-1fac-45a3-8032-0d02c987e07c",
      "number": 11,
      "content": "Và niềm vui cho người có lòng ngay thẳng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.632Z",
      "updatedAt": "2025-04-24T03:59:11.632Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "0a79cac2-ee22-46bf-97f4-5b40d5cbff24",
      "number": 12,
      "content": "Hỡi những người ngay lành, hãy vui mừng trong Chúa;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.643Z",
      "updatedAt": "2025-04-24T03:59:11.643Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    },
    {
      "id": "414da424-2bb3-4808-94b8-d2849b48bf77",
      "number": 12,
      "content": "Hãy dâng lòng biết ơn lên danh thánh của Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:59:11.653Z",
      "updatedAt": "2025-04-24T03:59:11.653Z",
      "chapterId": "3a6ba12d-1c4f-4f76-9654-5e6409864a8c"
    }
  ],
