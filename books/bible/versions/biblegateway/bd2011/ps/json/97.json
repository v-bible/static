{
  "chapter": {
    "id": "0c52e926-b99d-4569-86a8-1e9dd397311c",
    "number": 97,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2097&version=BD2011",
    "createdAt": "2025-04-22T07:37:47.911Z",
    "updatedAt": "2025-04-22T07:37:47.911Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "5df5639a-5de2-4d0b-823e-6a48643b7dde",
      "number": 1,
      "content": "Chúa trị vì!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.571Z",
      "updatedAt": "2025-04-22T08:31:16.571Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "f0c31a8a-7aba-495a-8d54-36f6ab71d3dc",
      "number": 1,
      "content": "Hỡi trái đất, hãy vui mừng;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.598Z",
      "updatedAt": "2025-04-22T08:31:16.598Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "285820ad-5b09-442e-882b-5b0416615eae",
      "number": 1,
      "content": "Hỡi muôn ngàn hải đảo, hãy hân hoan.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.609Z",
      "updatedAt": "2025-04-22T08:31:16.609Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "0cd3feed-196c-4ff6-a691-3f7ac9896fa9",
      "number": 2,
      "content": "Mây dày và bóng tối mù mịt bao quanh Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.623Z",
      "updatedAt": "2025-04-22T08:31:16.623Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "ced7f5c7-6f92-49a7-9f04-5c87f9b570a9",
      "number": 2,
      "content": "Công chính và công lý là nền tảng của ngai Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.636Z",
      "updatedAt": "2025-04-22T08:31:16.636Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "8d29044b-6c96-4dc8-b219-4a775cf08a78",
      "number": 3,
      "content": "Lửa hừng đi trước mặt Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.648Z",
      "updatedAt": "2025-04-22T08:31:16.648Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "fa18a17a-e0be-4d2a-8c44-5e4ef2b983fa",
      "number": 3,
      "content": "Thiêu hủy mọi kẻ chống nghịch chung quanh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.662Z",
      "updatedAt": "2025-04-22T08:31:16.662Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "34bca986-2a16-4208-898a-d6b51ce9592f",
      "number": 4,
      "content": "Các tia chớp của Ngài lóe lên rực sáng khắp thế gian;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.675Z",
      "updatedAt": "2025-04-22T08:31:16.675Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "e965d139-9b55-458d-ae3b-7b8d911355a1",
      "number": 4,
      "content": "Ðất thấy vậy run lên lẩy bẩy.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.688Z",
      "updatedAt": "2025-04-22T08:31:16.688Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "56088aec-520e-4b2c-bd14-1f802551e036",
      "number": 5,
      "content": "Các núi đồi tan chảy như sáp trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.701Z",
      "updatedAt": "2025-04-22T08:31:16.701Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "944f3110-7a29-44a1-a211-ffc6c2b4448f",
      "number": 5,
      "content": "Tức trước thánh nhan của Ðấng Chúa Tể toàn cầu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.714Z",
      "updatedAt": "2025-04-22T08:31:16.714Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "f2c8afae-f5d3-4526-a1ac-804ec9058b58",
      "number": 6,
      "content": "Các tầng trời rao truyền đức công chính Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.728Z",
      "updatedAt": "2025-04-22T08:31:16.728Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "23b89498-63dc-4105-8cad-d20c94725198",
      "number": 6,
      "content": "Tất cả các dân đều thấy vinh quang Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.741Z",
      "updatedAt": "2025-04-22T08:31:16.741Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "78decf52-0711-4020-b70c-e5f62b69b139",
      "number": 7,
      "content": "Hãy để mọi kẻ thờ lạy các hình tượng bị xấu hổ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.754Z",
      "updatedAt": "2025-04-22T08:31:16.754Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "df466452-b2cf-4ea8-9cec-9e0d34be8270",
      "number": 7,
      "content": "Tức những kẻ khoe mình về các hình tượng vô tri của chúng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.767Z",
      "updatedAt": "2025-04-22T08:31:16.767Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "d7976d25-6d56-4c35-8374-2eadeeae550e",
      "number": 7,
      "content": "Hỡi tất cả các thần, hãy sấp mình xuống thờ lạy Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.781Z",
      "updatedAt": "2025-04-22T08:31:16.781Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "40caed1a-85df-4ab4-bbb0-d678c7639cd9",
      "number": 8,
      "content": "Si-ôn nghe thế bèn vui mừng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.794Z",
      "updatedAt": "2025-04-22T08:31:16.794Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "f694a8e6-3bb4-4cb9-8f45-56c95afd4208",
      "number": 8,
      "content": "Các ái nữ của Giu-đa đều vui vẻ,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.811Z",
      "updatedAt": "2025-04-22T08:31:16.811Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "5915ed46-c9eb-41d5-b668-9c87fccb11c4",
      "number": 8,
      "content": "Vì những phán quyết của Ngài, Chúa ôi;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.827Z",
      "updatedAt": "2025-04-22T08:31:16.827Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "31eddfdc-95d3-4f16-9e0a-8453aeadce0e",
      "number": 9,
      "content": "Vì Ngài, Chúa ôi, là Ðấng Tối Cao trên khắp đất;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.840Z",
      "updatedAt": "2025-04-22T08:31:16.840Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "7ae1ab48-85c9-4b91-a255-d0dae157b6a1",
      "number": 9,
      "content": "Ngài vượt trỗi bỏ xa tất cả các thần.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.853Z",
      "updatedAt": "2025-04-22T08:31:16.853Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "77ef9304-924c-4a51-98c3-6b4c249b8081",
      "number": 10,
      "content": "Hỡi những người yêu mến Chúa, hãy ghét điều ác;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.863Z",
      "updatedAt": "2025-04-22T08:31:16.863Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "95939e22-87b0-461c-9240-59ceebb86854",
      "number": 10,
      "content": "Ngài bảo vệ sinh mạng các thánh đồ của Ngài khỏi tay kẻ ác.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.873Z",
      "updatedAt": "2025-04-22T08:31:16.873Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "78769ada-4a22-4970-9085-60c5e97771d6",
      "number": 11,
      "content": "Ánh sáng tỏa rạng trên người ngay lành,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.883Z",
      "updatedAt": "2025-04-22T08:31:16.883Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "3bed7752-f187-4237-8499-291ec750f7ac",
      "number": 11,
      "content": "Và niềm vui cho người có lòng ngay thẳng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.893Z",
      "updatedAt": "2025-04-22T08:31:16.893Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "0996166d-371c-446b-bac8-a706f1b082bd",
      "number": 12,
      "content": "Hỡi những người ngay lành, hãy vui mừng trong Chúa;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.904Z",
      "updatedAt": "2025-04-22T08:31:16.904Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    },
    {
      "id": "4c557433-21bb-4c2f-8533-20d8eed1036d",
      "number": 12,
      "content": "Hãy dâng lòng biết ơn lên danh thánh của Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:31:16.915Z",
      "updatedAt": "2025-04-22T08:31:16.915Z",
      "chapterId": "0c52e926-b99d-4569-86a8-1e9dd397311c"
    }
  ],
