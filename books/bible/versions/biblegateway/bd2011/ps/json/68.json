{
  "chapter": {
    "id": "99755d60-f6f2-4bc0-985e-cab13ddc904a",
    "number": 68,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2068&version=BD2011",
    "createdAt": "2025-04-22T07:37:47.155Z",
    "updatedAt": "2025-04-22T07:37:47.155Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "b7c89ad7-b0ad-4c10-91b9-1846161d3ce5",
      "number": 1,
      "content": "Nguyện Ðức Chúa Trời trỗi dậy;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.096Z",
      "updatedAt": "2025-04-22T08:27:42.096Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "e318d01b-a439-430d-be8b-bdcb9ade95ee",
      "number": 1,
      "content": "Nguyện những kẻ chống nghịch Ngài tản lạc;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.117Z",
      "updatedAt": "2025-04-22T08:27:42.117Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "e8c3a71a-c707-4e15-a6ce-67f788529c2e",
      "number": 1,
      "content": "Nguyện những kẻ ghét Ngài chạy trốn trước mặt Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.128Z",
      "updatedAt": "2025-04-22T08:27:42.128Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "f45a2897-fdbf-4346-b8e7-97b361395dc5",
      "number": 2,
      "content": "Như khói bị tan biến thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.142Z",
      "updatedAt": "2025-04-22T08:27:42.142Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "e6fe9a33-b790-45f3-8281-d838c8752e41",
      "number": 2,
      "content": "Nguyện chúng bị tan đi thể ấy;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.158Z",
      "updatedAt": "2025-04-22T08:27:42.158Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "599633b3-56da-478b-920a-af0d3446f253",
      "number": 2,
      "content": "Như sáp bị tan chảy trước ngọn lửa thể nào,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.172Z",
      "updatedAt": "2025-04-22T08:27:42.172Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "05fdf8df-6891-4966-bfc2-3e9edba7eaa8",
      "number": 2,
      "content": "Nguyện những kẻ ác bị diệt mất trước mặt Ðức Chúa Trời thể ấy.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.186Z",
      "updatedAt": "2025-04-22T08:27:42.186Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "346382e9-dfef-428c-a26b-300cb993b890",
      "number": 3,
      "content": "Nguyện những người ngay lành hân hoan,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.199Z",
      "updatedAt": "2025-04-22T08:27:42.199Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "c25f7639-89db-41ed-8643-a41f1469d9b2",
      "number": 3,
      "content": "Nguyện họ vui vẻ trước mặt Ðức Chúa Trời;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.212Z",
      "updatedAt": "2025-04-22T08:27:42.212Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "9a03b126-6eb0-420c-aacc-3b35c7b7db7c",
      "number": 3,
      "content": "Thật vậy nguyện họ hớn hở và vui mừng.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.225Z",
      "updatedAt": "2025-04-22T08:27:42.225Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "ab9765e1-baa9-40f3-aef6-865e6d0a2b46",
      "number": 4,
      "content": "Hãy ca ngợi Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.237Z",
      "updatedAt": "2025-04-22T08:27:42.237Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "9d5dfbbc-d7f7-4cff-b728-7721295bbcb6",
      "number": 4,
      "content": "Hãy hát ca chúc tụng danh Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.249Z",
      "updatedAt": "2025-04-22T08:27:42.249Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "cdd6f7b9-7c8a-4cad-84e8-c25ea7f27850",
      "number": 4,
      "content": "Khá tôn ngợi Ðấng ngự trên các tầng mây;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.261Z",
      "updatedAt": "2025-04-22T08:27:42.261Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "36603cfd-99fd-4d80-9bfc-8023475871db",
      "number": 4,
      "content": "Danh Ngài là Chúa;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.291Z",
      "updatedAt": "2025-04-22T08:27:42.291Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "a768a21c-a9ca-47da-8d54-4237b836f305",
      "number": 4,
      "content": "Khá reo vui trước mặt Ngài.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.303Z",
      "updatedAt": "2025-04-22T08:27:42.303Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "27184477-fb2f-4e29-bffa-4183b21b62d9",
      "number": 5,
      "content": "Cha kẻ mồ côi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.315Z",
      "updatedAt": "2025-04-22T08:27:42.315Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "a8cd9f91-03d4-4748-a852-ee6db67fa961",
      "number": 5,
      "content": "Ðấng binh vực người góa bụa,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.327Z",
      "updatedAt": "2025-04-22T08:27:42.327Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "20183152-efdb-4c87-898c-54cc5e1535c8",
      "number": 5,
      "content": "Chính là Ðức Chúa Trời,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.339Z",
      "updatedAt": "2025-04-22T08:27:42.339Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "d514d482-aaa8-40c3-a570-2eb5e23decfa",
      "number": 5,
      "content": "Ðấng ngự trong nơi thánh của Ngài.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.351Z",
      "updatedAt": "2025-04-22T08:27:42.351Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "c7d99024-dd66-4ca8-8097-8bfaa973c6e6",
      "number": 6,
      "content": "Ðức Chúa Trời là Ðấng làm cho kẻ cô đơn có mái ấm gia đình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.363Z",
      "updatedAt": "2025-04-22T08:27:42.363Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "c074e4bd-db1c-48d1-a97a-a248b31eb167",
      "number": 6,
      "content": "Ngài dẫn đưa những kẻ bị tù đày đến nơi sung mãn;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.376Z",
      "updatedAt": "2025-04-22T08:27:42.376Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "33aa74dd-a933-426e-9023-09d540b8bf85",
      "number": 6,
      "content": "Còn những kẻ bội nghịch phải ở trong xứ khô khan.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.388Z",
      "updatedAt": "2025-04-22T08:27:42.388Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "91153613-7ee1-4301-99f3-2dbc316663c5",
      "number": 7,
      "content": "Ðức Chúa Trời ôi, khi Ngài đi ra trước mặt con dân Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.400Z",
      "updatedAt": "2025-04-22T08:27:42.400Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "83944efb-2ae0-40ef-9e9c-da10d9c94e34",
      "number": 7,
      "content": "Khi Ngài đi qua sa mạc, *(Sê-la)*",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.413Z",
      "updatedAt": "2025-04-22T08:27:42.413Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "eadd1924-3d67-407c-b79c-24fc64d5f481",
      "number": 8,
      "content": "Ðất rúng động, các tầng trời ào ạt đổ cơn mưa,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.425Z",
      "updatedAt": "2025-04-22T08:27:42.425Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "afe64639-66f3-45a0-8271-db3ce30e9e18",
      "number": 8,
      "content": "Trước mặt Ðức Chúa Trời, Thần của Núi Si-nai,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.437Z",
      "updatedAt": "2025-04-22T08:27:42.437Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "52a04aec-b64c-4cc8-b087-2ba1333dc3c8",
      "number": 8,
      "content": "Trước mặt Ðức Chúa Trời, Thần của I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.464Z",
      "updatedAt": "2025-04-22T08:27:42.464Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "d3410c3e-fb29-4106-bd46-5d306b0ba4a9",
      "number": 9,
      "content": "Ðức Chúa Trời ôi, Ngài đã ban mưa xuống cách dồi dào,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.478Z",
      "updatedAt": "2025-04-22T08:27:42.478Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "098d2bc1-a044-4e93-8012-be8142cafac8",
      "number": 9,
      "content": "Khi sản nghiệp của Ngài bị khô khan, Ngài đã bồi bổ nó lại.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.491Z",
      "updatedAt": "2025-04-22T08:27:42.491Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "f79e2c39-be5e-47bd-976e-415f144497d8",
      "number": 10,
      "content": "Con dân Ngài đã định cư trong sản nghiệp đó;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.504Z",
      "updatedAt": "2025-04-22T08:27:42.504Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "6d41c6e1-6601-4fbe-8998-884d9a455b84",
      "number": 10,
      "content": "Ðức Chúa Trời ôi, do lòng tốt của Ngài, Ngài đã chu cấp cho những người nghèo khó.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.518Z",
      "updatedAt": "2025-04-22T08:27:42.518Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "a784d2bd-4559-403b-85a4-f03959199797",
      "number": 11,
      "content": "Khi Chúa ban lệnh,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.531Z",
      "updatedAt": "2025-04-22T08:27:42.531Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "3b1adcbb-442f-41a1-bccd-e1ca143dfa65",
      "number": 11,
      "content": "Những kẻ loan truyền tin mừng ra đi đông vô số;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.545Z",
      "updatedAt": "2025-04-22T08:27:42.545Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "7bcd36ed-6605-4c13-a6ef-5efe10f299dc",
      "number": 11,
      "content": "Họ báo rằng,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.558Z",
      "updatedAt": "2025-04-22T08:27:42.558Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "e7227f54-c809-4f34-89a7-dc0ea14a164c",
      "number": 12,
      "content": "“Các vua của các đạo quân đã bỏ chạy. Tất cả đã chạy trốn.”",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.571Z",
      "updatedAt": "2025-04-22T08:27:42.571Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "97fa68dc-15b1-4486-a894-e7415bb4a86b",
      "number": 12,
      "content": "Những phụ nữ ở nhà đều sẽ được chia các chiến lợi phẩm,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.585Z",
      "updatedAt": "2025-04-22T08:27:42.585Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "afd79625-a238-46b6-9d4d-5280112232a2",
      "number": 13,
      "content": "Dù họ chỉ nằm nghỉ ở giữa các trại quân.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.598Z",
      "updatedAt": "2025-04-22T08:27:42.598Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "e0872698-8aa4-4fba-9107-a2e2fdc5e42b",
      "number": 13,
      "content": "Kìa họ trông giống như những cánh bồ câu bọc bạc,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.625Z",
      "updatedAt": "2025-04-22T08:27:42.625Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "cbf7a792-8ac5-4571-9869-331db59160dc",
      "number": 13,
      "content": "Với bộ lông như vàng ròng lóng lánh.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.648Z",
      "updatedAt": "2025-04-22T08:27:42.648Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "4f3f2fc2-01f0-4325-94f6-7a19a45ff372",
      "number": 14,
      "content": "Khi Ðấng Toàn Năng đánh văng các vua trong xứ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.659Z",
      "updatedAt": "2025-04-22T08:27:42.659Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "c7785c32-dae7-440e-8cd1-b365524c73bf",
      "number": 14,
      "content": "Họ như tuyết rơi tơi tả ở Xanh-môn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.672Z",
      "updatedAt": "2025-04-22T08:27:42.672Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "9ea9aaa6-3b4c-4a4e-a67b-38122a25d196",
      "number": 15,
      "content": "Núi Ba-san là núi của Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.685Z",
      "updatedAt": "2025-04-22T08:27:42.685Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "3040bbae-4bf3-48b8-9c2d-03bc27feff36",
      "number": 15,
      "content": "Rặng Núi Ba-san quả có nhiều đỉnh cao chót vót.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.697Z",
      "updatedAt": "2025-04-22T08:27:42.697Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "ec5293a5-d8f7-46ff-9a9d-b5c634cbbd3b",
      "number": 16,
      "content": "Hỡi những rặng núi cao có nhiều đỉnh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.710Z",
      "updatedAt": "2025-04-22T08:27:42.710Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "eef4d448-1a71-4bde-af1a-1ef1caef6cc6",
      "number": 16,
      "content": "Tại sao các ngươi ganh tị với ngọn núi Ðức Chúa Trời đã chọn để làm nơi Ngài ngự?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.720Z",
      "updatedAt": "2025-04-22T08:27:42.720Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "9d66d300-4eb3-44ac-b8f9-449251b6511f",
      "number": 16,
      "content": "Thật vậy Chúa sẽ ngự tại đó đời đời.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.733Z",
      "updatedAt": "2025-04-22T08:27:42.733Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "53122418-bda1-49c9-8293-9deef9b9fd2b",
      "number": 17,
      "content": "Các xe chiến mã của Ðức Chúa Trời nhiều đến hằng vạn, thật là hằng ngàn và hằng vạn;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.746Z",
      "updatedAt": "2025-04-22T08:27:42.746Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "ce649d0e-43da-434a-9034-4c92f92fee02",
      "number": 17,
      "content": "Chúa ngự giữa chúng như Ngài ngự trong nơi thánh của Ngài ở Si-nai.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.759Z",
      "updatedAt": "2025-04-22T08:27:42.759Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "dcf19380-adc9-4af3-b544-ad340fc25d67",
      "number": 18,
      "content": "Ngài đã lên nơi cao,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.772Z",
      "updatedAt": "2025-04-22T08:27:42.772Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "c16bfb94-363e-4399-bd5a-78e97079df5a",
      "number": 18,
      "content": "Ngài dẫn theo đoàn tù binh bị bắt đem đi lưu đày;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.782Z",
      "updatedAt": "2025-04-22T08:27:42.782Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "e8072ceb-955d-4bef-a076-d60d0861121d",
      "number": 18,
      "content": "Ngài đã nhận những lễ vật của loài người,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.792Z",
      "updatedAt": "2025-04-22T08:27:42.792Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "57d2be97-54f5-4682-a511-dfb8a8b2ffa1",
      "number": 18,
      "content": "Ngay cả những lễ vật của những kẻ bội nghịch,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.802Z",
      "updatedAt": "2025-04-22T08:27:42.802Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "2327b9b6-c603-4e60-9d9c-a6e6840f3e10",
      "number": 18,
      "content": "Ðể chứng tỏ rằng Chúa Ðức Chúa Trời cũng ở tại đó.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.813Z",
      "updatedAt": "2025-04-22T08:27:42.813Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "bd573253-df7a-4d33-bd6f-ee0354b4bc9d",
      "number": 19,
      "content": "Tôn ngợi Chúa, Ðấng hằng ngày cưu mang chúng ta;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.840Z",
      "updatedAt": "2025-04-22T08:27:42.840Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "c3122bb5-aec8-4db8-83c7-0ff22c21000d",
      "number": 19,
      "content": "Ðức Chúa Trời là Ðấng Giải Cứu của chúng ta. *(Sê-la)*",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.853Z",
      "updatedAt": "2025-04-22T08:27:42.853Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "75a4351a-3edb-4ee6-b037-7c9aa0fb6e23",
      "number": 20,
      "content": "Ðức Chúa Trời chúng ta là Ðức Chúa Trời giải cứu;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.867Z",
      "updatedAt": "2025-04-22T08:27:42.867Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "1cd35559-5957-4fca-bea7-ca0c15673dc8",
      "number": 20,
      "content": "Nhờ Chúa Hằng Hữu, chúng ta được thoát chết.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.880Z",
      "updatedAt": "2025-04-22T08:27:42.880Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "6674a577-6916-4b95-a259-9e28e2c3656e",
      "number": 21,
      "content": "Chắc chắn Ðức Chúa Trời sẽ đạp nát đầu kẻ thù của Ngài,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.910Z",
      "updatedAt": "2025-04-22T08:27:42.910Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "de8b0596-59af-4fe6-90e1-91ad088c5f69",
      "number": 21,
      "content": "Tức đầu tóc của những kẻ cứ miệt mài trong tội ác.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.923Z",
      "updatedAt": "2025-04-22T08:27:42.923Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "c7f77a39-43e0-4450-8859-ddefbfb6cd14",
      "number": 22,
      "content": "Chúa phán, “Từ Ba-san, Ta sẽ đem chúng về;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.938Z",
      "updatedAt": "2025-04-22T08:27:42.938Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "49757ebc-6177-45ce-a042-741b79c8dd94",
      "number": 22,
      "content": "Từ các đại dương sâu thẳm, Ta sẽ mang chúng về,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.955Z",
      "updatedAt": "2025-04-22T08:27:42.955Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "4f9c4b5f-2c60-4334-8786-bb55869d9d75",
      "number": 23,
      "content": "Ðể các ngươi có thể giẫm chân trên máu kẻ thù của mình,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.969Z",
      "updatedAt": "2025-04-22T08:27:42.969Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "4984ee58-d79a-4b3f-93ad-faab2c464022",
      "number": 23,
      "content": "Ngay cả lưỡi của chó các ngươi cũng sẽ được phần trên quân thù của các ngươi.”",
      "order": 1,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.983Z",
      "updatedAt": "2025-04-22T08:27:42.983Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "d0a641a7-2bd1-4e3d-a0d0-bbf1f3cbc445",
      "number": 24,
      "content": "Ðức Chúa Trời ôi, người ta đã chứng kiến lễ nghinh rước Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:42.999Z",
      "updatedAt": "2025-04-22T08:27:42.999Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "297718ff-498f-4fd1-8b83-b2a456feee9d",
      "number": 24,
      "content": "Tức lễ nghinh rước Ðức Chúa Trời của con, Vua của con, vào đền thánh.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.012Z",
      "updatedAt": "2025-04-22T08:27:43.012Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "dce3d48a-7d6c-495c-9da5-c6a34680495a",
      "number": 25,
      "content": "Ca đoàn đi trước ngợi ca, ban nhạc theo sau trỗi nhạc,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.025Z",
      "updatedAt": "2025-04-22T08:27:43.025Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "7d23fe3a-2e77-4231-b300-e9aeecaec815",
      "number": 25,
      "content": "Ở giữa là đoàn thiếu nữ đánh trống lục lạc nhịp nhàng.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.040Z",
      "updatedAt": "2025-04-22T08:27:43.040Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "b986cb90-2c6e-4017-9aa8-e4ccee0001b2",
      "number": 26,
      "content": "Hãy chúc tụng Ðức Chúa Trời giữa những đoàn người đông đúc dự lễ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.057Z",
      "updatedAt": "2025-04-22T08:27:43.057Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "2aa90c04-4be4-43f7-97e2-a3ee96c26221",
      "number": 26,
      "content": "Hãy tôn ngợi Chúa, nguồn cội của I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.072Z",
      "updatedAt": "2025-04-22T08:27:43.072Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "93d04bca-0509-48fa-a462-e71013081991",
      "number": 27,
      "content": "Kìa là Bên-gia-min, chi tộc trẻ nhất, đang dẫn đầu;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.085Z",
      "updatedAt": "2025-04-22T08:27:43.085Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "fba09fe5-10b4-418f-b9d9-5f59b0ccd2e6",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Giu-đa và phái đoàn của họ;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.099Z",
      "updatedAt": "2025-04-22T08:27:43.099Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "f58e1e37-3324-46e7-96b3-698898946933",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Xê-bu-lun và các nhà quyền quý của Náp-ta-li.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.113Z",
      "updatedAt": "2025-04-22T08:27:43.113Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "6b4da215-78e0-4db8-82f8-2eedea8ab268",
      "number": 28,
      "content": "Ðức Chúa Trời ôi, xin thể hiện quyền năng Ngài;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.126Z",
      "updatedAt": "2025-04-22T08:27:43.126Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "08a0a3eb-9f1a-419f-ace4-c8a2317a80d4",
      "number": 28,
      "content": "Ðức Chúa Trời ôi, xin bày tỏ sức mạnh Ngài,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.153Z",
      "updatedAt": "2025-04-22T08:27:43.153Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "9cf83d80-6e20-49cc-9e57-c735bccdd566",
      "number": 28,
      "content": "Như Ngài đã từng làm cho chúng con khi trước.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.166Z",
      "updatedAt": "2025-04-22T08:27:43.166Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "d13eb582-9651-4703-b85b-4a53a563aa85",
      "number": 29,
      "content": "Vì đền thờ Ngài tọa lạc tại Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.179Z",
      "updatedAt": "2025-04-22T08:27:43.179Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "31c60490-d085-4eee-b682-66378bf52e6e",
      "number": 29,
      "content": "Nên các vua sẽ đem các lễ vật đến dâng lên Ngài tại đó.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.192Z",
      "updatedAt": "2025-04-22T08:27:43.192Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "17c396aa-4e9e-4319-aa0c-21d5043a8620",
      "number": 30,
      "content": "Cầu xin Ngài trách phạt bầy thú hoang trong đám lau sậy,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.206Z",
      "updatedAt": "2025-04-22T08:27:43.206Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "94513f9d-8137-424d-9c8b-51993998e508",
      "number": 30,
      "content": "Cùng đám bò đực ở giữa đàn bò con của các dân.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.222Z",
      "updatedAt": "2025-04-22T08:27:43.222Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "c014cb7c-7434-4da8-9572-09dc1b1e67e9",
      "number": 30,
      "content": "Xin Ngài bắt chúng phải đem bạc nén đến quỳ dâng;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.236Z",
      "updatedAt": "2025-04-22T08:27:43.236Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "0276c2ae-5746-4a9b-a1ef-eeb74f3400da",
      "number": 30,
      "content": "Còn những dân hiếu chiến, xin Ngài làm cho chúng bị tản lạc khắp nơi trên đất.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.249Z",
      "updatedAt": "2025-04-22T08:27:43.249Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "7423a718-0cef-4c0e-8192-3d666f8f9a32",
      "number": 31,
      "content": "Những đoàn người quyền thế sẽ từ Ai-cập đến thờ phượng;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.266Z",
      "updatedAt": "2025-04-22T08:27:43.266Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "ae118779-8340-4363-8f7d-73a05fa2fb16",
      "number": 31,
      "content": "Người Ê-thi-ô-pi sẽ sớm đưa tay lên hướng về Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.279Z",
      "updatedAt": "2025-04-22T08:27:43.279Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "49aa6194-5dbf-4ea5-bf8c-155e084c79db",
      "number": 32,
      "content": "Hỡi tất cả quốc gia trên thế giới, hãy ca tụng Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.306Z",
      "updatedAt": "2025-04-22T08:27:43.306Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "c4a238a8-a8b6-4983-b7c7-f90acb264d82",
      "number": 32,
      "content": "Hãy ca ngợi Chúa, *(Sê-la)*",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.319Z",
      "updatedAt": "2025-04-22T08:27:43.319Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "b2c89377-0242-4aed-8986-19fdbe3da5a4",
      "number": 33,
      "content": "Ðấng ngự trên nơi chí cao của các tầng trời,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.333Z",
      "updatedAt": "2025-04-22T08:27:43.333Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "b86ad41d-6bca-4fb4-9515-b819965ff43f",
      "number": 33,
      "content": "Tức các tầng trời đã được dựng nên từ thời thái cổ;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.346Z",
      "updatedAt": "2025-04-22T08:27:43.346Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "45ba1bcf-0911-4c01-96d2-250a976ff6ab",
      "number": 33,
      "content": "Kìa, Ngài cất tiếng nói,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.359Z",
      "updatedAt": "2025-04-22T08:27:43.359Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "35d1d4cb-daeb-4341-bc32-747375641745",
      "number": 33,
      "content": "Giọng Ngài thật oai hùng.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.372Z",
      "updatedAt": "2025-04-22T08:27:43.372Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "78bc6af9-fc67-4adf-a442-b33d8cf8a53a",
      "number": 34,
      "content": "Khá biết rằng mọi quyền lực đều thuộc về Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.389Z",
      "updatedAt": "2025-04-22T08:27:43.389Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "c87c78dd-cc7c-45c2-a9f2-0cebbeccd379",
      "number": 34,
      "content": "Nguyện uy nghi Ngài tỏa rạng trên I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.408Z",
      "updatedAt": "2025-04-22T08:27:43.408Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "9cd7be5f-49c3-4c32-8a9d-a3217113a2e5",
      "number": 34,
      "content": "Và quyền năng Ngài đầy dẫy khắp không trung.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.425Z",
      "updatedAt": "2025-04-22T08:27:43.425Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "8c7e5f6e-933b-44fe-b5d4-d322a646603b",
      "number": 35,
      "content": "Ðức Chúa Trời ôi, trong đền thánh Ngài, Ngài đáng kinh hãi biết bao!",
      "order": 0,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.438Z",
      "updatedAt": "2025-04-22T08:27:43.438Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "1fd44c04-94bf-4fe0-aa74-937b9a513392",
      "number": 35,
      "content": "Chính Ðức Chúa Trời của I-sơ-ra-ên đã ban sức mạnh và quyền lực cho con dân Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.452Z",
      "updatedAt": "2025-04-22T08:27:43.452Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    },
    {
      "id": "2b0cba78-a3ef-49c7-9b58-81fe53530aec",
      "number": 35,
      "content": "Tôn ngợi Ðức Chúa Trời!",
      "order": 2,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:27:43.465Z",
      "updatedAt": "2025-04-22T08:27:43.465Z",
      "chapterId": "99755d60-f6f2-4bc0-985e-cab13ddc904a"
    }
  ],
