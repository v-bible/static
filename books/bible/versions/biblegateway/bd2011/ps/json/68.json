{
  "chapter": {
    "id": "a534a79b-86c1-416f-8a84-35123f3df822",
    "number": 68,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2068&version=BD2011",
    "createdAt": "2025-04-24T03:03:16.559Z",
    "updatedAt": "2025-04-24T03:03:16.559Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "9a30306d-2ac7-4a9a-941d-7619ed4fcd07",
      "number": 1,
      "content": "Nguyện Ðức Chúa Trời trỗi dậy;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:30.859Z",
      "updatedAt": "2025-04-24T03:55:30.859Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "eec0f8b9-cbe0-4b29-8253-72666c6742bc",
      "number": 1,
      "content": "Nguyện những kẻ chống nghịch Ngài tản lạc;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:30.888Z",
      "updatedAt": "2025-04-24T03:55:30.888Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "e82dde6f-8765-4bfc-8e8e-136145f0cacb",
      "number": 1,
      "content": "Nguyện những kẻ ghét Ngài chạy trốn trước mặt Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:30.899Z",
      "updatedAt": "2025-04-24T03:55:30.899Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "72f19622-f000-45d8-973a-2c72cf1e85c1",
      "number": 2,
      "content": "Như khói bị tan biến thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:30.912Z",
      "updatedAt": "2025-04-24T03:55:30.912Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "efdebf02-886d-4cdc-8f6b-756bb85adaef",
      "number": 2,
      "content": "Nguyện chúng bị tan đi thể ấy;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:30.925Z",
      "updatedAt": "2025-04-24T03:55:30.925Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "c79c7eb2-20dc-4f4e-a70a-9533b43eaa35",
      "number": 2,
      "content": "Như sáp bị tan chảy trước ngọn lửa thể nào,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:30.939Z",
      "updatedAt": "2025-04-24T03:55:30.939Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "aec37d74-4db7-4450-bf0f-724855fdfdef",
      "number": 2,
      "content": "Nguyện những kẻ ác bị diệt mất trước mặt Ðức Chúa Trời thể ấy.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:30.952Z",
      "updatedAt": "2025-04-24T03:55:30.952Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "7b6d83bd-0314-4bc4-9e5a-e449ef5f7713",
      "number": 3,
      "content": "Nguyện những người ngay lành hân hoan,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:30.965Z",
      "updatedAt": "2025-04-24T03:55:30.965Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "582b3344-58d1-4bd1-b7e2-a1b3a8c688dc",
      "number": 3,
      "content": "Nguyện họ vui vẻ trước mặt Ðức Chúa Trời;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:30.978Z",
      "updatedAt": "2025-04-24T03:55:30.978Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "cd2024bb-91f7-452c-92a1-7085ae538a8d",
      "number": 3,
      "content": "Thật vậy nguyện họ hớn hở và vui mừng.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:30.992Z",
      "updatedAt": "2025-04-24T03:55:30.992Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "3ba4f0ba-671d-40ae-81d9-3de1543064fa",
      "number": 4,
      "content": "Hãy ca ngợi Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.005Z",
      "updatedAt": "2025-04-24T03:55:31.005Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "f71be0c7-85f8-442c-a59e-db814aeb5fb1",
      "number": 4,
      "content": "Hãy hát ca chúc tụng danh Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.019Z",
      "updatedAt": "2025-04-24T03:55:31.019Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "6511e733-a5ee-47b3-bf44-409d8e4dc825",
      "number": 4,
      "content": "Khá tôn ngợi Ðấng ngự trên các tầng mây;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.032Z",
      "updatedAt": "2025-04-24T03:55:31.032Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "a60fe548-c150-403d-9b18-180694165d4b",
      "number": 4,
      "content": "Danh Ngài là Chúa;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.059Z",
      "updatedAt": "2025-04-24T03:55:31.059Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "f64816de-c40c-451e-b2b6-0caf6701c5c8",
      "number": 4,
      "content": "Khá reo vui trước mặt Ngài.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.073Z",
      "updatedAt": "2025-04-24T03:55:31.073Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "ed62d128-b17d-40f2-8f06-7e6865c4473b",
      "number": 5,
      "content": "Cha kẻ mồ côi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.090Z",
      "updatedAt": "2025-04-24T03:55:31.090Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "1a9c9454-0f2c-46af-9a35-f6bbb977dd85",
      "number": 5,
      "content": "Ðấng binh vực người góa bụa,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.103Z",
      "updatedAt": "2025-04-24T03:55:31.103Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "a1d89cbc-9d13-4085-99f2-9d5f061e4a08",
      "number": 5,
      "content": "Chính là Ðức Chúa Trời,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.121Z",
      "updatedAt": "2025-04-24T03:55:31.121Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "0580969a-6211-485e-a616-24deb5979a0e",
      "number": 5,
      "content": "Ðấng ngự trong nơi thánh của Ngài.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.134Z",
      "updatedAt": "2025-04-24T03:55:31.134Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "d8cdce90-5c2a-4186-b77a-01d110c0eb01",
      "number": 6,
      "content": "Ðức Chúa Trời là Ðấng làm cho kẻ cô đơn có mái ấm gia đình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.147Z",
      "updatedAt": "2025-04-24T03:55:31.147Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "874fa004-fee5-495e-b0b2-866a934a9ac1",
      "number": 6,
      "content": "Ngài dẫn đưa những kẻ bị tù đày đến nơi sung mãn;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.160Z",
      "updatedAt": "2025-04-24T03:55:31.160Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "91f5df8e-ff19-438d-9ece-f5a6644a6aff",
      "number": 6,
      "content": "Còn những kẻ bội nghịch phải ở trong xứ khô khan.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.174Z",
      "updatedAt": "2025-04-24T03:55:31.174Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "9aa32f8e-03c2-47fd-8962-5c58869dc895",
      "number": 7,
      "content": "Ðức Chúa Trời ôi, khi Ngài đi ra trước mặt con dân Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.187Z",
      "updatedAt": "2025-04-24T03:55:31.187Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "013af271-7bcc-4485-aad8-7e3e4c965fca",
      "number": 7,
      "content": "Khi Ngài đi qua sa mạc, *(Sê-la)*",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.200Z",
      "updatedAt": "2025-04-24T03:55:31.200Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "82e2f1aa-7bc1-49b8-ad74-62c3fbf58bbb",
      "number": 8,
      "content": "Ðất rúng động, các tầng trời ào ạt đổ cơn mưa,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.213Z",
      "updatedAt": "2025-04-24T03:55:31.213Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "7aeefc6e-ead2-4fc6-a0c1-1887a07d398a",
      "number": 8,
      "content": "Trước mặt Ðức Chúa Trời, Thần của Núi Si-nai,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.226Z",
      "updatedAt": "2025-04-24T03:55:31.226Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "d70ac2fb-a05b-400e-9407-8b4667572130",
      "number": 8,
      "content": "Trước mặt Ðức Chúa Trời, Thần của I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.253Z",
      "updatedAt": "2025-04-24T03:55:31.253Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "0b73bf46-afea-403b-a0f8-c054e1df4b38",
      "number": 9,
      "content": "Ðức Chúa Trời ôi, Ngài đã ban mưa xuống cách dồi dào,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.266Z",
      "updatedAt": "2025-04-24T03:55:31.266Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "ec63f703-c346-4dae-81fd-a354f60889c4",
      "number": 9,
      "content": "Khi sản nghiệp của Ngài bị khô khan, Ngài đã bồi bổ nó lại.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.289Z",
      "updatedAt": "2025-04-24T03:55:31.289Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "ffc3f87d-ab25-4bb1-9a1e-98a2f58a724a",
      "number": 10,
      "content": "Con dân Ngài đã định cư trong sản nghiệp đó;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.302Z",
      "updatedAt": "2025-04-24T03:55:31.302Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "63ff5d60-cf92-4dbe-9190-8b026f1c5506",
      "number": 10,
      "content": "Ðức Chúa Trời ôi, do lòng tốt của Ngài, Ngài đã chu cấp cho những người nghèo khó.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.315Z",
      "updatedAt": "2025-04-24T03:55:31.315Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "cb2470d4-4cff-4075-9a44-511b8a3a3303",
      "number": 11,
      "content": "Khi Chúa ban lệnh,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.332Z",
      "updatedAt": "2025-04-24T03:55:31.332Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "3975ef14-b076-4e27-b324-0799714b31d1",
      "number": 11,
      "content": "Những kẻ loan truyền tin mừng ra đi đông vô số;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.345Z",
      "updatedAt": "2025-04-24T03:55:31.345Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "3e763b5b-a6a1-483b-8a42-b6bf5396bf8e",
      "number": 11,
      "content": "Họ báo rằng,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.364Z",
      "updatedAt": "2025-04-24T03:55:31.364Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "67950ae8-1727-4663-8e1b-880d16a38259",
      "number": 12,
      "content": "“Các vua của các đạo quân đã bỏ chạy. Tất cả đã chạy trốn.”",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.378Z",
      "updatedAt": "2025-04-24T03:55:31.378Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "65660a8a-9f84-48ff-982c-acc82dcaf3ec",
      "number": 12,
      "content": "Những phụ nữ ở nhà đều sẽ được chia các chiến lợi phẩm,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.391Z",
      "updatedAt": "2025-04-24T03:55:31.391Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "8dbff833-7f02-4eff-9bc6-b44502cdf523",
      "number": 13,
      "content": "Dù họ chỉ nằm nghỉ ở giữa các trại quân.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.404Z",
      "updatedAt": "2025-04-24T03:55:31.404Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "5a3655f9-fafb-44d3-a0d5-25ca7775999e",
      "number": 13,
      "content": "Kìa họ trông giống như những cánh bồ câu bọc bạc,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.433Z",
      "updatedAt": "2025-04-24T03:55:31.433Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "fc5e18e2-5a8b-4307-9a5c-5b2d69717e61",
      "number": 13,
      "content": "Với bộ lông như vàng ròng lóng lánh.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.460Z",
      "updatedAt": "2025-04-24T03:55:31.460Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "f1bd042d-9767-4f39-b24c-b21377f2bb9f",
      "number": 14,
      "content": "Khi Ðấng Toàn Năng đánh văng các vua trong xứ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.473Z",
      "updatedAt": "2025-04-24T03:55:31.473Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "7a4cfcda-356a-4f0d-8ea5-90691046ac6e",
      "number": 14,
      "content": "Họ như tuyết rơi tơi tả ở Xanh-môn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.487Z",
      "updatedAt": "2025-04-24T03:55:31.487Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "be43fb8f-0830-4c80-99ce-0808adbd42a9",
      "number": 15,
      "content": "Núi Ba-san là núi của Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.499Z",
      "updatedAt": "2025-04-24T03:55:31.499Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "f45844c4-816a-42a8-aa7f-4bb1f92e012f",
      "number": 15,
      "content": "Rặng Núi Ba-san quả có nhiều đỉnh cao chót vót.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.513Z",
      "updatedAt": "2025-04-24T03:55:31.513Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "df38022a-cfa8-46b7-bd44-bb3c2fd8cdc2",
      "number": 16,
      "content": "Hỡi những rặng núi cao có nhiều đỉnh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.526Z",
      "updatedAt": "2025-04-24T03:55:31.526Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "bc4c49df-842a-424a-b3dd-fb83a19c6921",
      "number": 16,
      "content": "Tại sao các ngươi ganh tị với ngọn núi Ðức Chúa Trời đã chọn để làm nơi Ngài ngự?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.539Z",
      "updatedAt": "2025-04-24T03:55:31.539Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "bbd311ec-f019-4f2b-80f6-b68cf88412cb",
      "number": 16,
      "content": "Thật vậy Chúa sẽ ngự tại đó đời đời.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.552Z",
      "updatedAt": "2025-04-24T03:55:31.552Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "b697f674-1e75-4ea7-987e-f269d661ccdb",
      "number": 17,
      "content": "Các xe chiến mã của Ðức Chúa Trời nhiều đến hằng vạn, thật là hằng ngàn và hằng vạn;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.565Z",
      "updatedAt": "2025-04-24T03:55:31.565Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "8140bc1f-bd05-4fc6-8f07-ef845f818734",
      "number": 17,
      "content": "Chúa ngự giữa chúng như Ngài ngự trong nơi thánh của Ngài ở Si-nai.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.578Z",
      "updatedAt": "2025-04-24T03:55:31.578Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "8623935d-505b-4520-9c21-02178174ea85",
      "number": 18,
      "content": "Ngài đã lên nơi cao,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.591Z",
      "updatedAt": "2025-04-24T03:55:31.591Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "8b04563f-abd1-461c-9669-d87ce57e6242",
      "number": 18,
      "content": "Ngài dẫn theo đoàn tù binh bị bắt đem đi lưu đày;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.605Z",
      "updatedAt": "2025-04-24T03:55:31.605Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "f55b299f-7705-41af-a817-fca64a237253",
      "number": 18,
      "content": "Ngài đã nhận những lễ vật của loài người,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.618Z",
      "updatedAt": "2025-04-24T03:55:31.618Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "b7159acf-8153-46ce-80e1-ade368458c80",
      "number": 18,
      "content": "Ngay cả những lễ vật của những kẻ bội nghịch,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.632Z",
      "updatedAt": "2025-04-24T03:55:31.632Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "6f92294a-8f48-41ab-8b58-55fe84900b47",
      "number": 18,
      "content": "Ðể chứng tỏ rằng Chúa Ðức Chúa Trời cũng ở tại đó.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.645Z",
      "updatedAt": "2025-04-24T03:55:31.645Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "f4713ab8-3e0e-4c4e-a6f7-d2c6cb13a825",
      "number": 19,
      "content": "Tôn ngợi Chúa, Ðấng hằng ngày cưu mang chúng ta;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.671Z",
      "updatedAt": "2025-04-24T03:55:31.671Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "0989c9f0-3b43-4cb4-83d6-1963717b7f11",
      "number": 19,
      "content": "Ðức Chúa Trời là Ðấng Giải Cứu của chúng ta. *(Sê-la)*",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.684Z",
      "updatedAt": "2025-04-24T03:55:31.684Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "f1ac4578-5840-4417-a5a3-b1fee07a7642",
      "number": 20,
      "content": "Ðức Chúa Trời chúng ta là Ðức Chúa Trời giải cứu;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.697Z",
      "updatedAt": "2025-04-24T03:55:31.697Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "dd07e9cf-5693-4267-9532-9a939c436f6c",
      "number": 20,
      "content": "Nhờ Chúa Hằng Hữu, chúng ta được thoát chết.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.710Z",
      "updatedAt": "2025-04-24T03:55:31.710Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "6251bcba-90ac-41f6-996f-f0a0a0ad1c6b",
      "number": 21,
      "content": "Chắc chắn Ðức Chúa Trời sẽ đạp nát đầu kẻ thù của Ngài,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.737Z",
      "updatedAt": "2025-04-24T03:55:31.737Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "76964b79-102d-4a0f-a158-890f8d41d7cc",
      "number": 21,
      "content": "Tức đầu tóc của những kẻ cứ miệt mài trong tội ác.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.751Z",
      "updatedAt": "2025-04-24T03:55:31.751Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "ea4c8777-95cc-43a4-b613-ae2995fd087b",
      "number": 22,
      "content": "Chúa phán, “Từ Ba-san, Ta sẽ đem chúng về;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.764Z",
      "updatedAt": "2025-04-24T03:55:31.764Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "7262d71a-ec6f-49cc-8456-b6cbf6dd5bb3",
      "number": 22,
      "content": "Từ các đại dương sâu thẳm, Ta sẽ mang chúng về,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.777Z",
      "updatedAt": "2025-04-24T03:55:31.777Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "c81dfec7-6588-4e2e-a23a-3219753bdb72",
      "number": 23,
      "content": "Ðể các ngươi có thể giẫm chân trên máu kẻ thù của mình,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.790Z",
      "updatedAt": "2025-04-24T03:55:31.790Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "653fc1b3-a6f6-4ab5-95cf-5166c9d6656e",
      "number": 23,
      "content": "Ngay cả lưỡi của chó các ngươi cũng sẽ được phần trên quân thù của các ngươi.”",
      "order": 1,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.804Z",
      "updatedAt": "2025-04-24T03:55:31.804Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "5f281676-1ab5-4428-80f8-52d43d136e62",
      "number": 24,
      "content": "Ðức Chúa Trời ôi, người ta đã chứng kiến lễ nghinh rước Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.817Z",
      "updatedAt": "2025-04-24T03:55:31.817Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "fcd45bc3-5027-4444-9d70-c5e6d3a5a7bf",
      "number": 24,
      "content": "Tức lễ nghinh rước Ðức Chúa Trời của con, Vua của con, vào đền thánh.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.830Z",
      "updatedAt": "2025-04-24T03:55:31.830Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "733fb9cf-7738-468f-95e1-e3b2fca9ebfa",
      "number": 25,
      "content": "Ca đoàn đi trước ngợi ca, ban nhạc theo sau trỗi nhạc,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.844Z",
      "updatedAt": "2025-04-24T03:55:31.844Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "0b4986eb-1a41-4b6e-b3b9-5726a2561f9b",
      "number": 25,
      "content": "Ở giữa là đoàn thiếu nữ đánh trống lục lạc nhịp nhàng.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.857Z",
      "updatedAt": "2025-04-24T03:55:31.857Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "7ff3edd7-b123-4cb7-8674-e2d40d5e0dfa",
      "number": 26,
      "content": "Hãy chúc tụng Ðức Chúa Trời giữa những đoàn người đông đúc dự lễ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.871Z",
      "updatedAt": "2025-04-24T03:55:31.871Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "5d51045d-c0b5-4b72-a31a-d6e446081ec0",
      "number": 26,
      "content": "Hãy tôn ngợi Chúa, nguồn cội của I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.884Z",
      "updatedAt": "2025-04-24T03:55:31.884Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "9f7009d3-42d0-488c-acb0-debd784f9293",
      "number": 27,
      "content": "Kìa là Bên-gia-min, chi tộc trẻ nhất, đang dẫn đầu;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.898Z",
      "updatedAt": "2025-04-24T03:55:31.898Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "759ed7d7-4c0d-4403-89e3-72cdcf65df5f",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Giu-đa và phái đoàn của họ;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.911Z",
      "updatedAt": "2025-04-24T03:55:31.911Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "fc055cfa-672d-4750-aa4a-66e4fadcee97",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Xê-bu-lun và các nhà quyền quý của Náp-ta-li.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.924Z",
      "updatedAt": "2025-04-24T03:55:31.924Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "29b4e7a1-96ac-4167-b574-134f3c8291c8",
      "number": 28,
      "content": "Ðức Chúa Trời ôi, xin thể hiện quyền năng Ngài;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.944Z",
      "updatedAt": "2025-04-24T03:55:31.944Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "33cde942-1b30-4b08-8581-b4eefa1bb5f6",
      "number": 28,
      "content": "Ðức Chúa Trời ôi, xin bày tỏ sức mạnh Ngài,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.970Z",
      "updatedAt": "2025-04-24T03:55:31.970Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "7a2444fc-b9ec-4ccc-9e49-570bde7415aa",
      "number": 28,
      "content": "Như Ngài đã từng làm cho chúng con khi trước.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.983Z",
      "updatedAt": "2025-04-24T03:55:31.983Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "7744e7e4-9dcf-42cb-9e52-f67d695b6385",
      "number": 29,
      "content": "Vì đền thờ Ngài tọa lạc tại Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:31.997Z",
      "updatedAt": "2025-04-24T03:55:31.997Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "51321147-8641-438e-a0c9-4f18ff7f191e",
      "number": 29,
      "content": "Nên các vua sẽ đem các lễ vật đến dâng lên Ngài tại đó.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.010Z",
      "updatedAt": "2025-04-24T03:55:32.010Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "ef349edb-fc21-45f8-9c06-5bd80161d938",
      "number": 30,
      "content": "Cầu xin Ngài trách phạt bầy thú hoang trong đám lau sậy,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.022Z",
      "updatedAt": "2025-04-24T03:55:32.022Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "c8fd0f15-7eea-494a-8de7-393d37929344",
      "number": 30,
      "content": "Cùng đám bò đực ở giữa đàn bò con của các dân.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.035Z",
      "updatedAt": "2025-04-24T03:55:32.035Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "68540e7c-bd27-4a77-9d4c-16c4118255fc",
      "number": 30,
      "content": "Xin Ngài bắt chúng phải đem bạc nén đến quỳ dâng;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.051Z",
      "updatedAt": "2025-04-24T03:55:32.051Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "6d333a45-7255-400a-b014-42432791e693",
      "number": 30,
      "content": "Còn những dân hiếu chiến, xin Ngài làm cho chúng bị tản lạc khắp nơi trên đất.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.067Z",
      "updatedAt": "2025-04-24T03:55:32.067Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "0f06f1f7-9f0b-49b1-b3bc-8d32dc2c34f5",
      "number": 31,
      "content": "Những đoàn người quyền thế sẽ từ Ai-cập đến thờ phượng;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.081Z",
      "updatedAt": "2025-04-24T03:55:32.081Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "f4d9e985-2346-43d3-9870-9ffdce3ee04a",
      "number": 31,
      "content": "Người Ê-thi-ô-pi sẽ sớm đưa tay lên hướng về Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.094Z",
      "updatedAt": "2025-04-24T03:55:32.094Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "24f084c7-ce9c-4402-9f34-e021e83273d3",
      "number": 32,
      "content": "Hỡi tất cả quốc gia trên thế giới, hãy ca tụng Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.120Z",
      "updatedAt": "2025-04-24T03:55:32.120Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "b7a7b7ca-59f9-4525-8b29-66e286b7b6f2",
      "number": 32,
      "content": "Hãy ca ngợi Chúa, *(Sê-la)*",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.134Z",
      "updatedAt": "2025-04-24T03:55:32.134Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "33605125-415e-46fa-b123-131efcce51f7",
      "number": 33,
      "content": "Ðấng ngự trên nơi chí cao của các tầng trời,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.147Z",
      "updatedAt": "2025-04-24T03:55:32.147Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "2aaf7642-7fcc-4dd7-af1b-a997c6e2ac63",
      "number": 33,
      "content": "Tức các tầng trời đã được dựng nên từ thời thái cổ;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.160Z",
      "updatedAt": "2025-04-24T03:55:32.160Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "031194ce-d03d-46db-9569-8573e51a4d73",
      "number": 33,
      "content": "Kìa, Ngài cất tiếng nói,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.173Z",
      "updatedAt": "2025-04-24T03:55:32.173Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "448ab767-2bf7-495d-ba52-9fb987e8e102",
      "number": 33,
      "content": "Giọng Ngài thật oai hùng.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.187Z",
      "updatedAt": "2025-04-24T03:55:32.187Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "7a439cc8-ccee-4a95-b0ff-a54bcffbe997",
      "number": 34,
      "content": "Khá biết rằng mọi quyền lực đều thuộc về Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.202Z",
      "updatedAt": "2025-04-24T03:55:32.202Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "3408146e-d5af-488a-842d-6c64c29468ca",
      "number": 34,
      "content": "Nguyện uy nghi Ngài tỏa rạng trên I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.213Z",
      "updatedAt": "2025-04-24T03:55:32.213Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "0879f767-e126-4b25-9342-1ecc6accde0b",
      "number": 34,
      "content": "Và quyền năng Ngài đầy dẫy khắp không trung.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.224Z",
      "updatedAt": "2025-04-24T03:55:32.224Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "22330ef6-0829-48b4-b5d1-b910c616a668",
      "number": 35,
      "content": "Ðức Chúa Trời ôi, trong đền thánh Ngài, Ngài đáng kinh hãi biết bao!",
      "order": 0,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.234Z",
      "updatedAt": "2025-04-24T03:55:32.234Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "590ff1e8-7085-49d8-94a5-a0e2b634d6ae",
      "number": 35,
      "content": "Chính Ðức Chúa Trời của I-sơ-ra-ên đã ban sức mạnh và quyền lực cho con dân Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.245Z",
      "updatedAt": "2025-04-24T03:55:32.245Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    },
    {
      "id": "dc81e136-f9cc-41c0-9ac1-ca76115abba3",
      "number": 35,
      "content": "Tôn ngợi Ðức Chúa Trời!",
      "order": 2,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:55:32.255Z",
      "updatedAt": "2025-04-24T03:55:32.255Z",
      "chapterId": "a534a79b-86c1-416f-8a84-35123f3df822"
    }
  ],
