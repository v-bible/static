{
  "chapter": {
    "id": "35b64aa8-c8fa-42af-816b-2aee72079c09",
    "number": 80,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2080&version=BD2011",
    "createdAt": "2025-04-24T03:03:16.873Z",
    "updatedAt": "2025-04-24T03:03:16.873Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "a921e841-55af-4174-b41d-d2086061bdba",
      "number": 1,
      "content": "Lạy Ðấng Chăn Giữ I-sơ-ra-ên, xin lắng nghe chúng con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.335Z",
      "updatedAt": "2025-04-24T03:57:02.335Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "c07e8e66-e284-43a9-9f5d-e27365a9efa9",
      "number": 1,
      "content": "Ngài là Ðấng dẫn dắt Gia-cốp như dẫn dắt một đàn chiên;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.361Z",
      "updatedAt": "2025-04-24T03:57:02.361Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "417fb8bd-d2c8-4bc8-acf7-a92e37e164b9",
      "number": 1,
      "content": "Ngài là Ðấng đang ngự trị giữa các chê-ru-bim,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.373Z",
      "updatedAt": "2025-04-24T03:57:02.373Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "48906cb8-013c-4373-a26a-1d3c8942cff1",
      "number": 1,
      "content": "Xin chiếu rạng vinh quang Ngài ra!",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.387Z",
      "updatedAt": "2025-04-24T03:57:02.387Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "c12d95e1-672d-4084-8610-82dd23c1b4ae",
      "number": 2,
      "content": "Xin khởi động quyền năng Ngài trước mặt Ép-ra-im, Bên-gia-min, và Ma-na-se,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.402Z",
      "updatedAt": "2025-04-24T03:57:02.402Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "ea9c5a27-5ea6-494e-aba6-c5c602f91b98",
      "number": 2,
      "content": "Và xin đến cứu chúng con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.416Z",
      "updatedAt": "2025-04-24T03:57:02.416Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "f14e6938-63ae-4061-a28c-9149f22a2b11",
      "number": 3,
      "content": "Ðức Chúa Trời ôi, xin khôi phục chúng con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.432Z",
      "updatedAt": "2025-04-24T03:57:02.432Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "42b87a39-688d-4f2c-82fd-5eacd59f21f1",
      "number": 3,
      "content": "Xin cho thánh nhan Ngài tỏa rạng trên chúng con, để chúng con được cứu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.446Z",
      "updatedAt": "2025-04-24T03:57:02.446Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "27823e93-00ac-4b46-8089-ed4db211d321",
      "number": 4,
      "content": "Lạy Chúa, Ðức Chúa Trời của các đạo quân,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.460Z",
      "updatedAt": "2025-04-24T03:57:02.460Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "5eca16f6-37d3-4875-a38a-d8b0a2b88c75",
      "number": 4,
      "content": "Ngài còn giận mà không muốn nghe lời cầu nguyện của con dân Ngài đến bao lâu nữa?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.475Z",
      "updatedAt": "2025-04-24T03:57:02.475Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "93437f46-7aba-4b48-aed0-9a95f3dd0722",
      "number": 5,
      "content": "Ngài đã cho họ ăn bánh chan hòa những giọt lệ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.489Z",
      "updatedAt": "2025-04-24T03:57:02.489Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "ad8408fc-32a1-4971-8c0f-326da4c48555",
      "number": 5,
      "content": "Ngài đã để họ uống nước pha với nước mắt.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.503Z",
      "updatedAt": "2025-04-24T03:57:02.503Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "d2c86a06-6a9b-4a21-8a9a-360f765d0d48",
      "number": 6,
      "content": "Ngài đã làm cho chúng con thành đề tài giễu cợt cho các lân bang của chúng con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.516Z",
      "updatedAt": "2025-04-24T03:57:02.516Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "c13809f8-0d76-4c52-84b1-1d8d2d9a098d",
      "number": 6,
      "content": "Những kẻ thù của chúng con đã chế nhạo chúng con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.534Z",
      "updatedAt": "2025-04-24T03:57:02.534Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "dc629856-971f-4668-808b-4e32296c165c",
      "number": 7,
      "content": "Ðức Chúa Trời của các đạo quân ôi, xin khôi phục chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.548Z",
      "updatedAt": "2025-04-24T03:57:02.548Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "7b4412a0-269d-4096-94b8-c6adda219710",
      "number": 7,
      "content": "Xin cho thánh nhan Ngài tỏa rạng trên chúng con, để chúng con được cứu.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.563Z",
      "updatedAt": "2025-04-24T03:57:02.563Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "653020fe-07f1-4081-8588-2ac8aee39f2c",
      "number": 8,
      "content": "Từ Ai-cập, Ngài đã bứng lên một cây nho;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.582Z",
      "updatedAt": "2025-04-24T03:57:02.582Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "0fe7983c-3424-4ea5-8c49-637e34d6139f",
      "number": 8,
      "content": "Ngài đuổi các dân tộc kia đi, rồi trồng cây nho ấy xuống.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.598Z",
      "updatedAt": "2025-04-24T03:57:02.598Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "b0bc4b1c-2407-4497-b7ec-81ae068c6ae6",
      "number": 9,
      "content": "Ngài đã làm sạch mảnh đất cho nó;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.615Z",
      "updatedAt": "2025-04-24T03:57:02.615Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "60149066-9c95-465b-a81a-8f5b09c62540",
      "number": 9,
      "content": "Nó đâm rễ và tràn ra khắp xứ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.630Z",
      "updatedAt": "2025-04-24T03:57:02.630Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "09c3c907-72cf-47e8-9ba3-c734221eed85",
      "number": 10,
      "content": "Các núi rừng bị bóng nó phủ che;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.643Z",
      "updatedAt": "2025-04-24T03:57:02.643Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "34960692-aa5d-4567-b498-9ba705031e64",
      "number": 10,
      "content": "Các nhánh nó cứng mạnh như cây bá hương tươi tốt.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.660Z",
      "updatedAt": "2025-04-24T03:57:02.660Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "4249254c-92af-4a1c-82e0-9c88d9e6684e",
      "number": 11,
      "content": "Các cành nó nhô ra đến tận đại dương;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.675Z",
      "updatedAt": "2025-04-24T03:57:02.675Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "438a6e49-b301-4b51-8e59-8bf62f8cfce3",
      "number": 11,
      "content": "Các chồi nó vươn dài đến bờ Sông Lớn.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.697Z",
      "updatedAt": "2025-04-24T03:57:02.697Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "0613e23f-d96f-4967-a153-2ea90d2e1a73",
      "number": 12,
      "content": "Nhưng sao Ngài nỡ dỡ rào nó xuống,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.720Z",
      "updatedAt": "2025-04-24T03:57:02.720Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "e002fae1-375d-4107-9afc-68fc7b9d5e3c",
      "number": 12,
      "content": "Ðể ai qua lại cũng có thể ngắt bứt nó thỏa tay,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.739Z",
      "updatedAt": "2025-04-24T03:57:02.739Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "40948f30-7306-46e4-9804-0312975b7999",
      "number": 13,
      "content": "Ðể các heo rừng cùng nhau cắn phá nó,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.749Z",
      "updatedAt": "2025-04-24T03:57:02.749Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "3ab9f227-2243-435e-bfc2-87b2b9d9cf1f",
      "number": 13,
      "content": "Và những thú hoang cấu xé nó tan tành?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.760Z",
      "updatedAt": "2025-04-24T03:57:02.760Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "6ca44502-340f-44e1-a4a9-698ef9f58e65",
      "number": 14,
      "content": "Ðức Chúa Trời của các đạo quân ôi, xin Ngài quay trở lại;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.771Z",
      "updatedAt": "2025-04-24T03:57:02.771Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "85e312bc-1a47-404f-93ab-d05aebd32ea9",
      "number": 14,
      "content": "Xin Ngài từ trời nhìn xuống và đoái xem chúng con;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.784Z",
      "updatedAt": "2025-04-24T03:57:02.784Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "e7f9e50e-f206-4a95-8a1e-601d5c1c52b0",
      "number": 14,
      "content": "Xin Ngài thăm viếng cây nho này,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.795Z",
      "updatedAt": "2025-04-24T03:57:02.795Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "fad1018f-957b-4fe4-a696-53ad8a437e89",
      "number": 15,
      "content": "Tức gốc nho do cánh tay phải Ngài đã trồng,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.808Z",
      "updatedAt": "2025-04-24T03:57:02.808Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "109fa82e-e571-4a28-96f1-741a7774dfeb",
      "number": 15,
      "content": "Là đứa con Ngài đã gây dựng cho mình.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.818Z",
      "updatedAt": "2025-04-24T03:57:02.818Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "e6af5185-d829-4ccb-9427-f37bb3af3d4f",
      "number": 16,
      "content": "Bây giờ cây nho ấy đang bị đốn chặt và bị đốt cháy;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.831Z",
      "updatedAt": "2025-04-24T03:57:02.831Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "b6e6fd49-19cb-4cdc-8c2b-a7b74e517e8c",
      "number": 16,
      "content": "Vì khi Ngài nghiêm mặt quở trách, họ liền bị diệt vong.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.841Z",
      "updatedAt": "2025-04-24T03:57:02.841Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "a1d3486e-67c1-449b-a401-e68e933ebc61",
      "number": 17,
      "content": "Xin đặt cánh tay Ngài trên người bên phải Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.852Z",
      "updatedAt": "2025-04-24T03:57:02.852Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "ee265bfa-f1d2-4505-a0dd-d00326becd06",
      "number": 17,
      "content": "Tức trên người Ngài đã gây dựng cho mình;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.863Z",
      "updatedAt": "2025-04-24T03:57:02.863Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "d26da4e4-ea1d-43b2-a6fd-800cff70a7c0",
      "number": 18,
      "content": "Ðể chúng con không quay lưng lìa bỏ Ngài.",
      "order": 0,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.875Z",
      "updatedAt": "2025-04-24T03:57:02.875Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "15fcfbd0-28a3-4767-b63e-c06294e4e225",
      "number": 18,
      "content": "Xin phục hưng chúng con, để chúng con cầu khẩn danh Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:57:02.891Z",
      "updatedAt": "2025-04-24T03:57:02.891Z",
      "chapterId": "35b64aa8-c8fa-42af-816b-2aee72079c09"
    },
    {
      "id": "b930b357-9249-47a9-800b-1ce4d5be789f",
      "number": 19,
      "content": "Lạy Chúa, Ðức Chúa Trời của các đạo quân, xin khôi phục chúng con;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
