{
  "chapter": {
    "id": "e1649899-5be7-499a-8c75-474e0aacb5c0",
    "number": 147,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20147&version=BD2011",
    "createdAt": "2025-04-22T07:37:49.218Z",
    "updatedAt": "2025-04-22T07:37:49.218Z",
    "bookId": "7a44f33e-f512-4e76-bac1-934bef7677e1"
  },
  "verses": [
    {
      "id": "c40b9123-0638-4cc9-9189-78497953112e",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:09.955Z",
      "updatedAt": "2025-04-22T08:37:09.955Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "9bd2f6f7-15af-4da1-9d3e-4d0ee533b4ba",
      "number": 20,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.466Z",
      "updatedAt": "2025-04-22T08:37:10.466Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "8690a3a2-938d-4d03-91c5-9629f39ee2e6",
      "number": 1,
      "content": "Ca hát chúc tụng Ðức Chúa Trời chúng ta là điều tốt đẹp;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:09.977Z",
      "updatedAt": "2025-04-22T08:37:09.977Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "843082a0-d78b-41f2-a256-6714af3e9500",
      "number": 1,
      "content": "Vì tôn ngợi Ngài là điều vui thỏa và hợp lý dường nào!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:09.987Z",
      "updatedAt": "2025-04-22T08:37:09.987Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "d03457c8-1af3-419b-bc9f-325f9af0dcce",
      "number": 2,
      "content": "Chúa xây dựng lại Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:09.997Z",
      "updatedAt": "2025-04-22T08:37:09.997Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "b1ed053b-350f-445d-979e-315e9ae15389",
      "number": 2,
      "content": "Ngài gom nhóm những người bị tản lạc của I-sơ-ra-ên trở về.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.011Z",
      "updatedAt": "2025-04-22T08:37:10.011Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "4c43e928-4c8f-4a24-b5e3-a35ad9397fce",
      "number": 3,
      "content": "Ngài rịt lành tâm hồn tan vỡ;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.022Z",
      "updatedAt": "2025-04-22T08:37:10.022Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "dad1ff26-4d77-4d17-b1e2-7db5a817414f",
      "number": 3,
      "content": "Và băng bó các vết thương của họ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.035Z",
      "updatedAt": "2025-04-22T08:37:10.035Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "63c878d6-237a-4c57-86ff-fda7a90d2ee5",
      "number": 4,
      "content": "Ngài định số lượng các vì sao;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.048Z",
      "updatedAt": "2025-04-22T08:37:10.048Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "bee04d6d-ecdf-415b-ad9a-a7b47a8d3840",
      "number": 4,
      "content": "Ngài gọi chúng theo tên riêng của từng vì sao một.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.061Z",
      "updatedAt": "2025-04-22T08:37:10.061Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "d83fa163-2b99-4b31-bb05-095bb0857c07",
      "number": 5,
      "content": "Chúa chúng ta vĩ đại biết bao!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.075Z",
      "updatedAt": "2025-04-22T08:37:10.075Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "a499cc15-7d77-46df-8411-16ba2a98685d",
      "number": 5,
      "content": "Quyền năng Ngài thật là vô hạn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.088Z",
      "updatedAt": "2025-04-22T08:37:10.088Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "7400c5f1-b629-4f9f-897f-2f1e3d4315f8",
      "number": 5,
      "content": "Kiến thức Ngài quả thật vô cùng.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.101Z",
      "updatedAt": "2025-04-22T08:37:10.101Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "2e63d71b-0f96-4411-a88b-fb62e4389e43",
      "number": 6,
      "content": "Chúa nhấc đưa những người nghèo khó lên cao,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.114Z",
      "updatedAt": "2025-04-22T08:37:10.114Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "b216174e-3ca4-4c75-8cac-2b03f1e6d044",
      "number": 6,
      "content": "Nhưng Ngài đánh đổ những kẻ gian ác ngã nhào xuống đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.130Z",
      "updatedAt": "2025-04-22T08:37:10.130Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "44d0ef76-3ac6-40ed-acb2-7106c231a60a",
      "number": 7,
      "content": "Hãy dâng lên Chúa những lời cảm tạ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.146Z",
      "updatedAt": "2025-04-22T08:37:10.146Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "49e74ebe-10fa-44ea-a9bc-d7def2b35d06",
      "number": 7,
      "content": "Hãy dùng nhạc khí ca ngợi Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.160Z",
      "updatedAt": "2025-04-22T08:37:10.160Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "82164636-95e7-4aa0-8f3e-c751e4de5e13",
      "number": 8,
      "content": "Ngài bao phủ các tầng trời bằng mây;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.187Z",
      "updatedAt": "2025-04-22T08:37:10.187Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "49cc1052-8ac0-4547-9ca8-ae1d6f275972",
      "number": 8,
      "content": "Ngài chuẩn bị các trận mưa cho đất;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.201Z",
      "updatedAt": "2025-04-22T08:37:10.201Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "45dadbc4-a6c5-4123-ae20-be9d07173999",
      "number": 8,
      "content": "Ngài khiến cỏ mọc trên các núi đồi.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.213Z",
      "updatedAt": "2025-04-22T08:37:10.213Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "9e95eb46-671a-4bf1-90ee-d55f02f56b86",
      "number": 9,
      "content": "Ngài ban thực phẩm cho muôn loài cầm thú;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.224Z",
      "updatedAt": "2025-04-22T08:37:10.224Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "d33f0b3e-a5fc-4db3-8ff3-9b3e51df05df",
      "number": 9,
      "content": "Ngài ban thức ăn cho các quạ con đói bụng kêu gào.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.234Z",
      "updatedAt": "2025-04-22T08:37:10.234Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "5dca8f1e-5c85-44c5-be7d-84da0b5e72bc",
      "number": 10,
      "content": "Ngài chẳng hài lòng về sức lực của ngựa chiến;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.245Z",
      "updatedAt": "2025-04-22T08:37:10.245Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "c9dedd09-23c1-433f-b44d-c1fb484c9385",
      "number": 10,
      "content": "Ngài không vui về đôi chân nhanh nhẹn của loài người;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.255Z",
      "updatedAt": "2025-04-22T08:37:10.255Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "460a95df-54dc-49ad-a1d2-6440891fdcfc",
      "number": 11,
      "content": "Nhưng Chúa rất vui lòng về những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.265Z",
      "updatedAt": "2025-04-22T08:37:10.265Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "fb15f268-3118-4dd2-ba21-3418bf3d880b",
      "number": 11,
      "content": "Những người đặt hy vọng vào tình thương của Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.277Z",
      "updatedAt": "2025-04-22T08:37:10.277Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "255c54b9-06a1-4afa-b777-c9d23293250f",
      "number": 12,
      "content": "Hỡi Giê-ru-sa-lem, hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.287Z",
      "updatedAt": "2025-04-22T08:37:10.287Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "dae582fc-5a7c-4085-938a-cb3e7180f130",
      "number": 12,
      "content": "Hỡi Si-ôn, hãy tôn ngợi Ðức Chúa Trời!",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.297Z",
      "updatedAt": "2025-04-22T08:37:10.297Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "ae77ac9d-95bd-4df0-b363-bbb3515478fe",
      "number": 13,
      "content": "Vì Ngài làm vững mạnh các song sắt ở các cổng thành của ngươi;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.307Z",
      "updatedAt": "2025-04-22T08:37:10.307Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "b11227c5-3b3c-4ecc-8067-aff5eddec591",
      "number": 13,
      "content": "Ngài ban phước cho con dân ngươi sống trong ngươi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.317Z",
      "updatedAt": "2025-04-22T08:37:10.317Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "4a67df8f-c908-4821-bc97-329dedf2a896",
      "number": 14,
      "content": "Ngài ban bình an nơi các biên giới ngươi;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.327Z",
      "updatedAt": "2025-04-22T08:37:10.327Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "7eea8137-4f82-42a7-b60e-525d0b198a74",
      "number": 14,
      "content": "Ngài cho ngươi no thỏa bằng gạo ngon hảo hạng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.337Z",
      "updatedAt": "2025-04-22T08:37:10.337Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "39ab8a44-827c-44ca-8dc0-966e05c48de4",
      "number": 15,
      "content": "Ngài truyền lịnh của Ngài cho trái đất;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.347Z",
      "updatedAt": "2025-04-22T08:37:10.347Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "d21cafad-0f7b-4f26-a390-ac3087abeff5",
      "number": 15,
      "content": "Lời Ngài phán ra được cấp tốc truyền đi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.357Z",
      "updatedAt": "2025-04-22T08:37:10.357Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "cca4328f-018f-48e3-a6d5-28261e7c9e6d",
      "number": 16,
      "content": "Ngài ban tuyết xuống phủ trắng như lông chiên;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.367Z",
      "updatedAt": "2025-04-22T08:37:10.367Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "2fe9d4f0-3c48-4e47-880f-7d9b4ff7b37e",
      "number": 16,
      "content": "Ngài rải sương giá ra như tro bụi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.377Z",
      "updatedAt": "2025-04-22T08:37:10.377Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "b38ddeeb-3a21-43a3-9a92-9787754447ba",
      "number": 17,
      "content": "Ngài quăng băng giá ra từng mảng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.387Z",
      "updatedAt": "2025-04-22T08:37:10.387Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "8ee07035-84fe-4878-9f7f-339246a744d9",
      "number": 17,
      "content": "Ai có thể chịu nổi cơn buốt giá của Ngài?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.396Z",
      "updatedAt": "2025-04-22T08:37:10.396Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "1de26e85-f029-4f03-8dae-c18a581af4e0",
      "number": 18,
      "content": "Ngài ban lịnh, chúng liền tan chảy;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.406Z",
      "updatedAt": "2025-04-22T08:37:10.406Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "8e1fcc68-b205-4081-8d4e-216a23e8c7cb",
      "number": 18,
      "content": "Ngài khiến gió thổi qua, nước chảy xuống thành dòng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
