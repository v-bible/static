{
  "chapter": {
    "id": "6e2da041-8c01-4640-a20f-1a2447b9ca6b",
    "number": 147,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20147&version=BD2011",
    "createdAt": "2025-04-24T03:03:18.620Z",
    "updatedAt": "2025-04-24T03:03:18.620Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "4965f468-5098-41e9-b4c4-df895cb24ead",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:30.839Z",
      "updatedAt": "2025-04-24T04:05:30.839Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "cc58e6c9-81e9-4173-8ce2-dce719fec8b8",
      "number": 1,
      "content": "Ca hát chúc tụng Ðức Chúa Trời chúng ta là điều tốt đẹp;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:30.869Z",
      "updatedAt": "2025-04-24T04:05:30.869Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "0482b14b-0df5-4ce4-9944-1e6a3149bb67",
      "number": 1,
      "content": "Vì tôn ngợi Ngài là điều vui thỏa và hợp lý dường nào!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:30.883Z",
      "updatedAt": "2025-04-24T04:05:30.883Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "3f6d18a1-7898-4bb9-ad43-89d606904a0d",
      "number": 2,
      "content": "Chúa xây dựng lại Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:30.896Z",
      "updatedAt": "2025-04-24T04:05:30.896Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "dbe83cce-8787-4be3-9204-181229c33b95",
      "number": 2,
      "content": "Ngài gom nhóm những người bị tản lạc của I-sơ-ra-ên trở về.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:30.909Z",
      "updatedAt": "2025-04-24T04:05:30.909Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "b36aafad-3cd0-4132-8749-2dad728911f1",
      "number": 3,
      "content": "Ngài rịt lành tâm hồn tan vỡ;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:30.923Z",
      "updatedAt": "2025-04-24T04:05:30.923Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "5f94db65-31e0-4442-ab41-d76e772cb91e",
      "number": 3,
      "content": "Và băng bó các vết thương của họ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:30.936Z",
      "updatedAt": "2025-04-24T04:05:30.936Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "e988d0a2-c767-4a8a-88a8-c175aeea2b57",
      "number": 4,
      "content": "Ngài định số lượng các vì sao;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:30.950Z",
      "updatedAt": "2025-04-24T04:05:30.950Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "92b16251-a79b-4f6e-a360-2273396a4af9",
      "number": 4,
      "content": "Ngài gọi chúng theo tên riêng của từng vì sao một.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:30.963Z",
      "updatedAt": "2025-04-24T04:05:30.963Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "369f7037-55fe-4a85-8781-ec1b37ea3a40",
      "number": 5,
      "content": "Chúa chúng ta vĩ đại biết bao!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:30.976Z",
      "updatedAt": "2025-04-24T04:05:30.976Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "d9d13f74-574c-4d27-96c4-65bff659c848",
      "number": 5,
      "content": "Quyền năng Ngài thật là vô hạn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:30.989Z",
      "updatedAt": "2025-04-24T04:05:30.989Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "fb67c5ad-92e1-475b-b2c5-fcde47d1a0b5",
      "number": 5,
      "content": "Kiến thức Ngài quả thật vô cùng.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.002Z",
      "updatedAt": "2025-04-24T04:05:31.002Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "363a0267-18b5-4931-a3b6-326b5466229c",
      "number": 6,
      "content": "Chúa nhấc đưa những người nghèo khó lên cao,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.015Z",
      "updatedAt": "2025-04-24T04:05:31.015Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "79a378d3-96cc-47bf-a1c6-e636eed097ba",
      "number": 6,
      "content": "Nhưng Ngài đánh đổ những kẻ gian ác ngã nhào xuống đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.028Z",
      "updatedAt": "2025-04-24T04:05:31.028Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "2cb7557b-cdc0-43a3-baed-3e99f45f3ad1",
      "number": 7,
      "content": "Hãy dâng lên Chúa những lời cảm tạ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.042Z",
      "updatedAt": "2025-04-24T04:05:31.042Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "14808224-2e85-43c7-8d83-4bc4b1f1efaf",
      "number": 7,
      "content": "Hãy dùng nhạc khí ca ngợi Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.055Z",
      "updatedAt": "2025-04-24T04:05:31.055Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "b9473080-6234-4664-9939-770fc90034ee",
      "number": 8,
      "content": "Ngài bao phủ các tầng trời bằng mây;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.085Z",
      "updatedAt": "2025-04-24T04:05:31.085Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "5098a4d3-c04d-45e6-a4b4-707a7c7a94c0",
      "number": 8,
      "content": "Ngài chuẩn bị các trận mưa cho đất;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.098Z",
      "updatedAt": "2025-04-24T04:05:31.098Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "d31d764a-812d-418c-813e-611493dd0664",
      "number": 8,
      "content": "Ngài khiến cỏ mọc trên các núi đồi.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.111Z",
      "updatedAt": "2025-04-24T04:05:31.111Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "b4c054c9-54b8-497f-8a94-b981843f0095",
      "number": 9,
      "content": "Ngài ban thực phẩm cho muôn loài cầm thú;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.125Z",
      "updatedAt": "2025-04-24T04:05:31.125Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "bd93977f-6cf6-49f1-b80a-ce3e5f3ad2c0",
      "number": 9,
      "content": "Ngài ban thức ăn cho các quạ con đói bụng kêu gào.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.138Z",
      "updatedAt": "2025-04-24T04:05:31.138Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "1cc5dd53-a628-42ba-b21f-832efdb96902",
      "number": 10,
      "content": "Ngài chẳng hài lòng về sức lực của ngựa chiến;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.151Z",
      "updatedAt": "2025-04-24T04:05:31.151Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "2af9d244-ec75-4ae8-a4a7-7c2664312ccf",
      "number": 10,
      "content": "Ngài không vui về đôi chân nhanh nhẹn của loài người;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.165Z",
      "updatedAt": "2025-04-24T04:05:31.165Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "0d4c2ad9-a67d-4127-a764-3596deaac4d2",
      "number": 11,
      "content": "Nhưng Chúa rất vui lòng về những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.181Z",
      "updatedAt": "2025-04-24T04:05:31.181Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "8c98500f-8fcb-4137-b584-1a7368c7687b",
      "number": 11,
      "content": "Những người đặt hy vọng vào tình thương của Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.193Z",
      "updatedAt": "2025-04-24T04:05:31.193Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "0c27f57c-2014-49e8-804f-f8668312dab3",
      "number": 12,
      "content": "Hỡi Giê-ru-sa-lem, hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.204Z",
      "updatedAt": "2025-04-24T04:05:31.204Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "4bd93650-3266-4302-8010-a50b4f3d5d40",
      "number": 12,
      "content": "Hỡi Si-ôn, hãy tôn ngợi Ðức Chúa Trời!",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.215Z",
      "updatedAt": "2025-04-24T04:05:31.215Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "901f2e6b-5a71-49ce-bf6a-db2a6ebe9d3a",
      "number": 13,
      "content": "Vì Ngài làm vững mạnh các song sắt ở các cổng thành của ngươi;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.229Z",
      "updatedAt": "2025-04-24T04:05:31.229Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "b0d7de2a-a88f-4118-934c-801728643044",
      "number": 13,
      "content": "Ngài ban phước cho con dân ngươi sống trong ngươi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.240Z",
      "updatedAt": "2025-04-24T04:05:31.240Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "2e97aa4d-ad8c-4727-961e-7e6e164db076",
      "number": 14,
      "content": "Ngài ban bình an nơi các biên giới ngươi;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.251Z",
      "updatedAt": "2025-04-24T04:05:31.251Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "8fa3f635-87e0-452a-bd27-aa83ecc0447a",
      "number": 14,
      "content": "Ngài cho ngươi no thỏa bằng gạo ngon hảo hạng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.263Z",
      "updatedAt": "2025-04-24T04:05:31.263Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "3adab551-618e-43e9-bd91-f8cd7e23b587",
      "number": 15,
      "content": "Ngài truyền lịnh của Ngài cho trái đất;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.274Z",
      "updatedAt": "2025-04-24T04:05:31.274Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "7191560e-1fa6-4bfa-ab7b-0a7ba668b09a",
      "number": 15,
      "content": "Lời Ngài phán ra được cấp tốc truyền đi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.285Z",
      "updatedAt": "2025-04-24T04:05:31.285Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "616e3160-1639-4799-b1cf-ceab28d35f0b",
      "number": 16,
      "content": "Ngài ban tuyết xuống phủ trắng như lông chiên;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.297Z",
      "updatedAt": "2025-04-24T04:05:31.297Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "5c309e02-413a-4773-b3dd-d043478f640c",
      "number": 16,
      "content": "Ngài rải sương giá ra như tro bụi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.310Z",
      "updatedAt": "2025-04-24T04:05:31.310Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "6d4d702d-f4ff-4897-8357-4f7fb7722aeb",
      "number": 17,
      "content": "Ngài quăng băng giá ra từng mảng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.323Z",
      "updatedAt": "2025-04-24T04:05:31.323Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "4120f879-c069-450c-adde-035f82def18b",
      "number": 17,
      "content": "Ai có thể chịu nổi cơn buốt giá của Ngài?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.336Z",
      "updatedAt": "2025-04-24T04:05:31.336Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "d069a07b-f418-4dc8-aa73-44b5d46c2422",
      "number": 18,
      "content": "Ngài ban lịnh, chúng liền tan chảy;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.350Z",
      "updatedAt": "2025-04-24T04:05:31.350Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "e028a545-e17e-4eb0-a055-98559f425566",
      "number": 18,
      "content": "Ngài khiến gió thổi qua, nước chảy xuống thành dòng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.363Z",
      "updatedAt": "2025-04-24T04:05:31.363Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "ac24f069-6206-49c8-83f9-5605d1877c1b",
      "number": 19,
      "content": "Ngài tuyên bố lời Ngài cho Gia-cốp,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.386Z",
      "updatedAt": "2025-04-24T04:05:31.386Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "20762f08-123d-4c39-89d1-a1e24380eb65",
      "number": 19,
      "content": "Luật lệ Ngài và mạng lịnh Ngài cho I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.403Z",
      "updatedAt": "2025-04-24T04:05:31.403Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "7299f4ef-b0d2-4e09-b853-65acad8ed4f7",
      "number": 20,
      "content": "Ngài chẳng làm như vậy cho một dân nào khác;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.416Z",
      "updatedAt": "2025-04-24T04:05:31.416Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "a68af316-384b-4497-bd88-553b11766148",
      "number": 20,
      "content": "Họ chẳng biết gì về các mạng lịnh của Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.432Z",
      "updatedAt": "2025-04-24T04:05:31.432Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    },
    {
      "id": "1e921b47-0127-4d16-87c3-3d8a1a789e3a",
      "number": 20,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:05:31.445Z",
      "updatedAt": "2025-04-24T04:05:31.445Z",
      "chapterId": "6e2da041-8c01-4640-a20f-1a2447b9ca6b"
    }
  ],
