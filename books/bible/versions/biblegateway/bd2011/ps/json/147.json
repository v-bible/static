    },
    {
      "id": "b727ead4-6cd3-4d94-8550-4e4be536d761",
      "number": 19,
      "content": "Ngài tuyên bố lời Ngài cho Gia-cốp,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.426Z",
      "updatedAt": "2025-04-22T08:37:10.426Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "ac064b2c-cf6e-4e5e-b589-791992148fea",
      "number": 19,
      "content": "Luật lệ Ngài và mạng lịnh Ngài cho I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.436Z",
      "updatedAt": "2025-04-22T08:37:10.436Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "9d9c4302-aa25-4e92-ba65-fd8b09f00bbc",
      "number": 20,
      "content": "Ngài chẳng làm như vậy cho một dân nào khác;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.446Z",
      "updatedAt": "2025-04-22T08:37:10.446Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    },
    {
      "id": "99d92dba-8a84-4bc4-89fe-e26d4c386347",
      "number": 20,
      "content": "Họ chẳng biết gì về các mạng lịnh của Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:37:10.456Z",
      "updatedAt": "2025-04-22T08:37:10.456Z",
      "chapterId": "e1649899-5be7-499a-8c75-474e0aacb5c0"
    }
