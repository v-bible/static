{
  "chapter": {
    "id": "67905655-ca42-4d4d-b87e-de563eedf3bb",
    "number": 122,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20122&version=BD2011",
    "createdAt": "2025-04-24T03:03:17.972Z",
    "updatedAt": "2025-04-24T03:03:17.972Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "5f7d935e-52a0-4417-89e5-dc5479779e34",
      "number": 1,
      "content": "Tôi vui mừng khi người ta nói với tôi rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.605Z",
      "updatedAt": "2025-04-24T04:02:34.605Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "dedd46bd-e193-4d1f-80ff-170be4aaf4b8",
      "number": 1,
      "content": "“Chúng ta hãy đi đến nhà Chúa.”",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.640Z",
      "updatedAt": "2025-04-24T04:02:34.640Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "2f5e2b64-db48-4d5c-bb3e-fd2bfa2297db",
      "number": 2,
      "content": "Hỡi Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.651Z",
      "updatedAt": "2025-04-24T04:02:34.651Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "43555745-49e5-4a0e-8106-11c2d4323978",
      "number": 2,
      "content": "Chân chúng ta đang đứng bên trong cổng thành ngươi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.662Z",
      "updatedAt": "2025-04-24T04:02:34.662Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "4a97fdc4-47e1-4b31-bea4-359ff1fae021",
      "number": 3,
      "content": "Giê-ru-sa-lem đã được xây dựng thành một kinh đô,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.673Z",
      "updatedAt": "2025-04-24T04:02:34.673Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "ffaecf6c-21e3-424e-b4f1-40107a3b6e04",
      "number": 3,
      "content": "Các tòa nhà trong thành nối kết nhau chặt chẽ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.684Z",
      "updatedAt": "2025-04-24T04:02:34.684Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "fe7936d0-3b0b-40fe-a76e-a2f74d36d50b",
      "number": 4,
      "content": "Các chi tộc đi lên thành ấy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.696Z",
      "updatedAt": "2025-04-24T04:02:34.696Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "5d8e737f-0b71-4a6b-8455-bd13b0775070",
      "number": 4,
      "content": "Tức các chi tộc của Chúa,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.709Z",
      "updatedAt": "2025-04-24T04:02:34.709Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "de872b52-5049-4610-ac6a-690efdce75d4",
      "number": 4,
      "content": "Theo như luật định cho I-sơ-ra-ên,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.722Z",
      "updatedAt": "2025-04-24T04:02:34.722Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "9b27053f-0dfd-40c8-9424-5e16dd3de6d1",
      "number": 4,
      "content": "Ðể cảm tạ danh Chúa,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.735Z",
      "updatedAt": "2025-04-24T04:02:34.735Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "91ada93b-ec2e-4f74-9835-35a36c3bb693",
      "number": 5,
      "content": "Vì tại đó các ngai xét đoán được thiết lập,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.748Z",
      "updatedAt": "2025-04-24T04:02:34.748Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "9db8db53-3819-4215-85ad-62835baa53d6",
      "number": 5,
      "content": "Ðó là các ngai của nhà Ða-vít.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:34.761Z",
      "updatedAt": "2025-04-24T04:02:34.761Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "e6bf55be-67d0-43c9-b414-f663ca59679d",
      "number": 6,
      "content": "Hãy cầu hòa bình cho Giê-ru-sa-lem:",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:02:34.774Z",
      "updatedAt": "2025-04-24T04:02:34.774Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "8b49a32b-0382-4f0f-905e-c842438e819f",
      "number": 7,
      "content": "Nguyện bình an ở phía trong tường ngươi.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:02:34.790Z",
      "updatedAt": "2025-04-24T04:02:34.790Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "85c119d6-b987-4094-ad3a-c6e6ce77fd81",
      "number": 8,
      "content": "Nay vì cớ anh chị em ta và các bạn hữu ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:02:34.803Z",
      "updatedAt": "2025-04-24T04:02:34.803Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    },
    {
      "id": "5ff062ac-f01d-4611-ab90-de06feebc224",
      "number": 9,
      "content": "Vì cớ nhà Chúa, Ðức Chúa Trời chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:02:34.829Z",
      "updatedAt": "2025-04-24T04:02:34.829Z",
      "chapterId": "67905655-ca42-4d4d-b87e-de563eedf3bb"
    }
  ],
