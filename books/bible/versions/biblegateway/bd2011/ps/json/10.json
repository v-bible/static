    },
    {
      "id": "61ec207b-6296-4b9a-ba7f-add2d9003393",
      "number": 18,
      "content": "Hầu những kẻ ra từ bụi đất sẽ không còn gây kinh hãi nữa.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 4,
