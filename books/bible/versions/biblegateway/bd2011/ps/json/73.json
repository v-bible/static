    },
    {
      "id": "034b563b-f511-4035-809c-8719c1c649ba",
      "content": "Cuối Ðời của Người Ngay Lành và Kẻ Gian Ác",
      "order": 1,
      "createdAt": "2024-09-24T02:03:02.141Z",
      "updatedAt": "2024-09-24T02:03:02.141Z",
      "verseId": "c8e2457e-74bd-44c0-899a-ce103f0d13ea",
      "chapterId": "2722c481-9bee-4eca-9a38-2f3183af9f58"
    }
  ],
