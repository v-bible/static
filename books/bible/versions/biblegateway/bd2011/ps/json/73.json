    {
      "id": "e3e3b391-4409-42b0-aad2-aa846b6f7e5e",
      "content": "Cuối Ðời của Người Ngay Lành và Kẻ Gian Ác",
      "level": 2,
      "order": 1,
      "createdAt": "2025-04-22T08:28:19.813Z",
      "updatedAt": "2025-04-22T08:28:19.813Z",
