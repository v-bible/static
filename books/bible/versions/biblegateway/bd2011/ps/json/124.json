{
  "chapter": {
    "id": "2dfebab3-c577-427b-b7de-87b2bba1e9d8",
    "number": 124,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20124&version=BD2011",
    "createdAt": "2025-04-22T07:37:48.607Z",
