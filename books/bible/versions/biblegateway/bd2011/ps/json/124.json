{
  "chapter": {
    "id": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c",
    "number": 124,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20124&version=BD2011",
    "createdAt": "2025-04-24T03:03:18.022Z",
    "updatedAt": "2025-04-24T03:03:18.022Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "98070afb-a756-4e92-a85f-e9a442605959",
      "number": 1,
      "content": "Nếu chẳng phải Chúa bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.005Z",
      "updatedAt": "2025-04-24T04:02:48.005Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "9b609e18-9c9f-4bee-87a6-5329435041c5",
      "number": 1,
      "content": "Hỡi I-sơ-ra-ên, hãy nói đi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.045Z",
      "updatedAt": "2025-04-24T04:02:48.045Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "b4e3e3ff-c58f-4544-8f80-9117aca743ae",
      "number": 2,
      "content": "Nếu chẳng phải Chúa bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.060Z",
      "updatedAt": "2025-04-24T04:02:48.060Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "f1e055e5-3420-40a9-91ae-8a903510e434",
      "number": 2,
      "content": "Thì khi người ta nổi lên chống lại chúng ta,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.077Z",
      "updatedAt": "2025-04-24T04:02:48.077Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "de03607a-b867-46dc-b39b-44259c1d924c",
      "number": 3,
      "content": "Khi cơn cuồng nộ của chúng bừng lên nghịch lại chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.090Z",
      "updatedAt": "2025-04-24T04:02:48.090Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "c33d98c3-b92d-43ec-8b53-2cdd9a2d09a2",
      "number": 3,
      "content": "Chắc hẳn chúng đã nuốt chửng chúng ta rồi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.104Z",
      "updatedAt": "2025-04-24T04:02:48.104Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "63af1bad-d791-4102-9639-26b266e75043",
      "number": 4,
      "content": "Các dòng nước lũ chắc đã phủ ngập chúng ta;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.117Z",
      "updatedAt": "2025-04-24T04:02:48.117Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "952ccfd9-792b-421f-b4f9-2d94977ae973",
      "number": 4,
      "content": "Luồng nước mạnh chắc đã cuốn trôi linh hồn chúng ta;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.131Z",
      "updatedAt": "2025-04-24T04:02:48.131Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "aecb2302-3ca7-43cb-9a40-accccd18d07a",
      "number": 5,
      "content": "Các đợt sóng thần dữ dội chắc đã cuốn mất linh hồn chúng ta rồi.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.144Z",
      "updatedAt": "2025-04-24T04:02:48.144Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "21c26b1d-baee-4970-a8f3-a47ce1592bb8",
      "number": 6,
      "content": "Chúc tụng Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.158Z",
      "updatedAt": "2025-04-24T04:02:48.158Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "c1d54abe-8757-403a-8b0d-b1d2a1177d93",
      "number": 6,
      "content": "Ðấng không cho phép chúng ta làm mồi cho răng chúng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.171Z",
      "updatedAt": "2025-04-24T04:02:48.171Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "91e17338-2938-47b3-aa26-9a2e09196c5c",
      "number": 7,
      "content": "Linh hồn chúng ta được giải thoát,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.185Z",
      "updatedAt": "2025-04-24T04:02:48.185Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "6db883a2-2933-4258-ab76-18002127deb3",
      "number": 7,
      "content": "Như con chim thoát khỏi bẫy của kẻ bẫy chim;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.198Z",
      "updatedAt": "2025-04-24T04:02:48.198Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "ebe230f7-f8c8-4896-a950-bbf5c722354c",
      "number": 7,
      "content": "Cái bẫy bị bẻ gãy, và chúng ta thoát ra ngoài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.211Z",
      "updatedAt": "2025-04-24T04:02:48.211Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "0c35ea46-9cdd-47cb-b9c2-b07ca5eff7b2",
      "number": 8,
      "content": "Chúng ta được giúp đỡ nhờ danh Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.228Z",
      "updatedAt": "2025-04-24T04:02:48.228Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    },
    {
      "id": "375b386b-ca73-4005-ba72-088d1fcec97b",
      "number": 8,
      "content": "Ðấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:02:48.244Z",
      "updatedAt": "2025-04-24T04:02:48.244Z",
      "chapterId": "b5f2f779-8e41-49d8-b9f8-97aeeafdf15c"
    }
  ],
