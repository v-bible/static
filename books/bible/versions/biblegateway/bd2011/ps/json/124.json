  },
  "verses": [
    {
      "id": "10ddb56a-2f0d-443c-9cc5-d428a3749e83",
      "number": 1,
      "content": "Nếu chẳng phải Chúa bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.176Z",
      "updatedAt": "2025-04-22T08:34:32.176Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "43453401-387d-4e75-be17-e604ab051e15",
      "number": 2,
      "content": "Nếu chẳng phải Chúa bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.225Z",
      "updatedAt": "2025-04-22T08:34:32.225Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "323872e3-5aa5-434d-b472-8880de04f552",
      "number": 1,
      "content": "Hỡi I-sơ-ra-ên, hãy nói đi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.214Z",
      "updatedAt": "2025-04-22T08:34:32.214Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "b49e47e6-ff27-4d3f-b74a-54a451910d72",
      "number": 2,
      "content": "Thì khi người ta nổi lên chống lại chúng ta,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.237Z",
      "updatedAt": "2025-04-22T08:34:32.237Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "49cd059f-9f29-4a94-b1f4-1a11509d90be",
      "number": 3,
      "content": "Khi cơn cuồng nộ của chúng bừng lên nghịch lại chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.251Z",
      "updatedAt": "2025-04-22T08:34:32.251Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "7639ca1a-6200-4fb1-b3e8-4daab0b40ed5",
      "number": 3,
      "content": "Chắc hẳn chúng đã nuốt chửng chúng ta rồi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.265Z",
      "updatedAt": "2025-04-22T08:34:32.265Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "d0493f35-14a6-4d63-b389-f32e2d5f859c",
      "number": 4,
      "content": "Các dòng nước lũ chắc đã phủ ngập chúng ta;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.278Z",
      "updatedAt": "2025-04-22T08:34:32.278Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "a6b1f778-6d7a-4c11-8a17-7399bf0b81b6",
      "number": 4,
      "content": "Luồng nước mạnh chắc đã cuốn trôi linh hồn chúng ta;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.292Z",
      "updatedAt": "2025-04-22T08:34:32.292Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "a5a63584-2059-4a12-8d7b-32323fcc40b4",
      "number": 5,
      "content": "Các đợt sóng thần dữ dội chắc đã cuốn mất linh hồn chúng ta rồi.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.308Z",
      "updatedAt": "2025-04-22T08:34:32.308Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "9cbfd84e-2f03-4dda-86fc-40e0f729ba0d",
      "number": 6,
      "content": "Chúc tụng Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.321Z",
      "updatedAt": "2025-04-22T08:34:32.321Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "7223e026-325d-462e-925f-fd9fc3613ff0",
      "number": 6,
      "content": "Ðấng không cho phép chúng ta làm mồi cho răng chúng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.335Z",
      "updatedAt": "2025-04-22T08:34:32.335Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "0dd550a2-92df-48dd-9e83-5a9164881f37",
      "number": 7,
      "content": "Linh hồn chúng ta được giải thoát,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.348Z",
      "updatedAt": "2025-04-22T08:34:32.348Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "aba2010e-7aff-47e3-b53b-e519552b7bf4",
      "number": 7,
      "content": "Như con chim thoát khỏi bẫy của kẻ bẫy chim;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.361Z",
      "updatedAt": "2025-04-22T08:34:32.361Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "17630435-cce8-4b13-876c-c7365e22bb01",
      "number": 7,
      "content": "Cái bẫy bị bẻ gãy, và chúng ta thoát ra ngoài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.374Z",
      "updatedAt": "2025-04-22T08:34:32.374Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "672ebaf9-5cc0-42a9-90d4-77257519e40f",
      "number": 8,
      "content": "Chúng ta được giúp đỡ nhờ danh Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.387Z",
      "updatedAt": "2025-04-22T08:34:32.387Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    },
    {
      "id": "566ce2c9-8a03-4914-b220-8f37c2eab260",
      "number": 8,
      "content": "Ðấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:34:32.400Z",
      "updatedAt": "2025-04-22T08:34:32.400Z",
      "chapterId": "2dfebab3-c577-427b-b7de-87b2bba1e9d8"
    }
  ],
