{
  "chapter": {
    "id": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f",
    "number": 11,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2011&version=BD2011",
    "createdAt": "2025-04-24T03:03:15.081Z",
    "updatedAt": "2025-04-24T03:03:15.081Z",
    "bookId": "f2dba16d-ebf7-4e93-9b11-74f6ef6ae98b"
  },
  "verses": [
    {
      "id": "3c3c19c3-167e-4959-9e0a-23bb06b32c5b",
      "number": 1,
      "content": "Tôi nương náu mình nơi Chúa.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.655Z",
      "updatedAt": "2025-04-24T03:48:29.655Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "be21b600-a52a-468c-b724-7319a3bcdc6e",
      "number": 1,
      "content": "Thế mà sao các người lại bảo linh hồn tôi rằng,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.694Z",
      "updatedAt": "2025-04-24T03:48:29.694Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "8653dbb3-a845-47d1-a7a3-e8b8885d6b1c",
      "number": 1,
      "content": "“Hãy làm như con chim và bay lên núi trốn đi”?",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.706Z",
      "updatedAt": "2025-04-24T03:48:29.706Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "0f9c6e26-926d-4056-9e24-0118cf543d45",
      "number": 2,
      "content": "Vì kìa, kẻ ác đã giương cung;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.719Z",
      "updatedAt": "2025-04-24T03:48:29.719Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "ccf5c119-b814-415f-91d6-11a94523489c",
      "number": 2,
      "content": "Chúng đã đặt mũi tên vào dây cung,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.736Z",
      "updatedAt": "2025-04-24T03:48:29.736Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "4243d0f6-9bb1-48db-a412-4d58f05b3a7e",
      "number": 2,
      "content": "Ðể từ trong bóng tối nhắm bắn người có lòng ngay thẳng.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.752Z",
      "updatedAt": "2025-04-24T03:48:29.752Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "0c954358-f9eb-4eab-9fae-61d2fd1c9728",
      "number": 3,
      "content": "Nếu các nền tảng bị phá hủy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.771Z",
      "updatedAt": "2025-04-24T03:48:29.771Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "23ed0561-5798-413b-af11-b7fac05a7892",
      "number": 3,
      "content": "Người ngay lành còn có thể làm gì?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.788Z",
      "updatedAt": "2025-04-24T03:48:29.788Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "6210e253-9a3a-4682-b923-762cfabd28e8",
      "number": 4,
      "content": "Chúa ngự trong đền thánh Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.806Z",
      "updatedAt": "2025-04-24T03:48:29.806Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "913a6ea2-9d9a-4e7f-8ec9-861116a8fd7a",
      "number": 4,
      "content": "Chúa ngự trên ngôi Ngài trên thiên đàng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.824Z",
      "updatedAt": "2025-04-24T03:48:29.824Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "95874c2e-ce10-4c63-866f-bbdcc11eeae8",
      "number": 4,
      "content": "Mắt Ngài nhìn thấy rõ hết;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.836Z",
      "updatedAt": "2025-04-24T03:48:29.836Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "8654ff46-f712-4b1a-880d-2f9d189265ed",
      "number": 4,
      "content": "Mí mắt Ngài dò xét con cái loài người.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:48:29.851Z",
      "updatedAt": "2025-04-24T03:48:29.851Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "d4df0f62-10b6-4546-beca-dc2fe1526881",
      "number": 5,
      "content": "Chúa thử nghiệm người ngay lành,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T03:48:29.867Z",
      "updatedAt": "2025-04-24T03:48:29.867Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "a8a25737-2c50-48c6-997d-e7930e09877d",
      "number": 6,
      "content": "Ngài sẽ giáng mưa than lửa và diêm sinh xuống kẻ gian ác,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2025-04-24T03:48:29.885Z",
      "updatedAt": "2025-04-24T03:48:29.885Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    },
    {
      "id": "c2573672-b141-4029-8b49-d1485058d244",
      "number": 7,
      "content": "Vì Chúa là Ðấng công chính.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T03:48:29.897Z",
      "updatedAt": "2025-04-24T03:48:29.897Z",
      "chapterId": "10b07816-ea8c-4757-ae7b-9f1ed30a7d1f"
    }
  ],
