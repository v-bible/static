  ],
  "headings": [
    {
      "id": "19d2e2b3-d507-47c3-bd2d-6f6a0cbe0866",
      "content": "Than Khóc vì Tội của I-sơ-ra-ên",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T09:01:45.404Z",
      "updatedAt": "2025-04-22T09:01:45.404Z",
      "verseId": "474e4f49-8e4f-4613-9733-5be941d6f54b",
      "chapterId": "adaf414b-e7fc-453a-9707-e1a8510cae52"
    },
    {
      "id": "7d0bed1c-16a8-4f4e-a1d1-7e44f9f664d1",
      "content": "Ngày của Chúa Thật Ðen Tối",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T09:01:46.061Z",
      "updatedAt": "2025-04-22T09:01:46.061Z",
      "verseId": "97ed3255-dc37-4c3c-bdaa-400d869fa465",
      "chapterId": "adaf414b-e7fc-453a-9707-e1a8510cae52"
    },
    {
      "id": "5d3b6c8a-41c1-4fe2-a7b5-e3ac7df8ff79",
      "content": "Chúa Không Hài Lòng về Sự Thờ Phượng Chỉ Bằng Hình Thức",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T09:01:46.159Z",
      "updatedAt": "2025-04-22T09:01:46.159Z",
      "verseId": "f6714c6d-7f6f-4415-981c-4dc80ac345d0",
      "chapterId": "adaf414b-e7fc-453a-9707-e1a8510cae52"
    }
  ],
