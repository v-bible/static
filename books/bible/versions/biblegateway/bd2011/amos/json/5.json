  "headings": [
    {
      "id": "474b9033-acf3-4c17-b237-6ca5dd3d7e11",
      "content": "Chúa Không Hài Lòng về Sự Thờ Phượng Chỉ Bằng Hình Thức",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T04:31:01.332Z",
      "updatedAt": "2025-04-24T04:31:01.332Z",
      "verseId": "46a41ad6-ed82-40ab-9b8c-5efc28896c09",
      "chapterId": "10c6e239-3976-4de8-943b-497c953241b1"
    },
    {
      "id": "b9a6e729-584c-4373-9c1c-c431f3195d51",
      "content": "Ngày của Chúa Thật Ðen Tối",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T04:31:01.222Z",
      "updatedAt": "2025-04-24T04:31:01.222Z",
      "verseId": "7eab7d9e-4e09-465c-a69c-63517550f2fb",
      "chapterId": "10c6e239-3976-4de8-943b-497c953241b1"
    },
    {
      "id": "248d6749-f276-4a48-a6be-2878c2258031",
      "content": "Than Khóc vì Tội của I-sơ-ra-ên",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T04:31:00.612Z",
      "updatedAt": "2025-04-24T04:31:00.612Z",
      "verseId": "b9795e65-b1fb-4a9d-9948-9e66705824f8",
      "chapterId": "10c6e239-3976-4de8-943b-497c953241b1"
    }
  ],
