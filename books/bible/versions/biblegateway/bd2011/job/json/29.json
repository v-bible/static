{
  "chapter": {
    "id": "4496c724-9d84-49f7-a25f-d5ca8ae5b651",
    "number": 29,
    "ref": "https://www.biblegateway.com/passage/?search=Gi%C3%B3p%2029&version=BD2011",
    "createdAt": "2025-04-22T07:37:44.945Z",
    "updatedAt": "2025-04-22T07:37:44.945Z",
    "bookId": "a3dea4d4-9413-427d-8a25-cf029980dbba"
  },
  "verses": [
    {
      "id": "028d80bc-972a-470d-975c-48b401ea501d",
      "number": 1,
      "content": "Gióp tiếp tục biện hộ cho ông và nói:",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:18:18.051Z",
      "updatedAt": "2025-04-22T08:18:18.051Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "c0157167-2f50-4422-b2c6-9950f758aa8f",
      "number": 2,
      "content": "Ôi, ước gì tôi được như thuở trước;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.082Z",
      "updatedAt": "2025-04-22T08:18:18.082Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "8a4d71c6-ccd1-4d9c-8aae-e6b34e96bbb3",
      "number": 2,
      "content": "Như những tháng ngày Ðức Chúa Trời còn gìn giữ tôi;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.092Z",
      "updatedAt": "2025-04-22T08:18:18.092Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "38c0908e-e684-405d-b8fd-8e3a82b98056",
      "number": 3,
      "content": "Khi trên đầu tôi ngọn đèn của Ngài chiếu sáng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.103Z",
      "updatedAt": "2025-04-22T08:18:18.103Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "4c0ac297-318c-4538-bd8d-18a861ec05a4",
      "number": 3,
      "content": "Khi trong bóng đêm tôi bước theo ánh sáng của Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.115Z",
      "updatedAt": "2025-04-22T08:18:18.115Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "8c5bb9fd-1ab2-48a3-a1fb-3621b10d3996",
      "number": 4,
      "content": "Như trong những ngày tôi còn tráng kiện;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.128Z",
      "updatedAt": "2025-04-22T08:18:18.128Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "3f6ee20c-14c6-4373-bfe4-a4fe7e89bf55",
      "number": 4,
      "content": "Khi mối tương giao mật thiết với Ðức Chúa Trời còn ở trên lều tôi;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.141Z",
      "updatedAt": "2025-04-22T08:18:18.141Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "ae8688fe-e2ae-44de-95d6-63fb02928364",
      "number": 5,
      "content": "Khi Ðấng Toàn Năng còn ở với tôi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.154Z",
      "updatedAt": "2025-04-22T08:18:18.154Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "9016c4e7-4d7d-43c2-bcf3-909cfacdbea6",
      "number": 5,
      "content": "Khi con cái tôi còn sum họp quanh tôi;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.167Z",
      "updatedAt": "2025-04-22T08:18:18.167Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "22e6975a-5312-4f9f-8d14-23810eccf04e",
      "number": 6,
      "content": "Khi các bước chân tôi còn dầm trong sữa của đàn bò sữa;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.180Z",
      "updatedAt": "2025-04-22T08:18:18.180Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "874f05f1-1662-40af-a989-b9d52a455454",
      "number": 6,
      "content": "Khi các rẫy ô-liu của tôi trên sườn núi đá sản xuất ra dầu;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.194Z",
      "updatedAt": "2025-04-22T08:18:18.194Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "d696f5f8-cb19-47cd-afa9-b01614861928",
      "number": 7,
      "content": "Khi tôi đến nơi cổng thành,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.207Z",
      "updatedAt": "2025-04-22T08:18:18.207Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "e9bbac15-2eff-4b0c-95f6-1f8c1a46de25",
      "number": 8,
      "content": "Những người trẻ trông thấy tôi liền tránh qua nhường chỗ;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.220Z",
      "updatedAt": "2025-04-22T08:18:18.220Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "d669d727-47d7-457c-a608-1f950c4f779c",
      "number": 8,
      "content": "Còn những người cao niên đứng dậy để đón chào;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.233Z",
      "updatedAt": "2025-04-22T08:18:18.233Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "11535b04-006b-4969-97f7-32dc479c23be",
      "number": 9,
      "content": "Những người quyền quý thấy tôi bèn ngưng nói;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.246Z",
      "updatedAt": "2025-04-22T08:18:18.246Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "d5ab8b49-34e5-4e59-a8b2-1da948f82612",
      "number": 9,
      "content": "Họ lấy tay che miệng để giữ lời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.259Z",
      "updatedAt": "2025-04-22T08:18:18.259Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "28c201de-95f7-4b3d-b334-d4167fc748ae",
      "number": 10,
      "content": "Tiếng của những người quyền quý bỗng trở nên im bặt;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.273Z",
      "updatedAt": "2025-04-22T08:18:18.273Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "1b90fbfa-4f22-4e7d-bb5d-26d584d65b3e",
      "number": 10,
      "content": "Lưỡi của họ dường như dính chặt vào hốc miệng của họ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.286Z",
      "updatedAt": "2025-04-22T08:18:18.286Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "30f2b71f-a48b-44ce-8cf6-484a82435967",
      "number": 11,
      "content": "Nghe tôi nói ai nấy đều tán thưởng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.299Z",
      "updatedAt": "2025-04-22T08:18:18.299Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "16a3f02a-e9b7-4dc9-956a-fb8d57810d8c",
      "number": 11,
      "content": "Ai thấy tôi đều tỏ vẻ đồng ý với tôi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.312Z",
      "updatedAt": "2025-04-22T08:18:18.312Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "e3656512-eee0-4159-945e-c3bda1a914c9",
      "number": 12,
      "content": "Vì tôi giải thoát người nghèo đang thiết tha cầu cứu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.326Z",
      "updatedAt": "2025-04-22T08:18:18.326Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "1bf5cd3f-b47f-4cbd-9664-f0388b9aa94b",
      "number": 12,
      "content": "Và cứu trẻ mồ côi không có ai giúp nó lúc cần.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.339Z",
      "updatedAt": "2025-04-22T08:18:18.339Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "3400bfd4-0dda-484f-8b39-4bf81763153f",
      "number": 13,
      "content": "Người hấp hối chúc phước cho tôi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.355Z",
      "updatedAt": "2025-04-22T08:18:18.355Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "72478e00-df90-4a7b-8ffb-f5c80e5c57fa",
      "number": 13,
      "content": "Tôi làm cho người góa bụa được nức lòng mừng rỡ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.369Z",
      "updatedAt": "2025-04-22T08:18:18.369Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "41973043-b906-473c-959b-75500de5736a",
      "number": 14,
      "content": "Tôi mặc lấy công chính, và nó che tôi như quần áo;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 23,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.382Z",
      "updatedAt": "2025-04-22T08:18:18.382Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "24efa427-7e0c-4c67-bf45-558831ec9926",
      "number": 14,
      "content": "Còn công lý thì như áo dài và khăn đóng của tôi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 24,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.395Z",
      "updatedAt": "2025-04-22T08:18:18.395Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "48dceca4-204a-4acf-bd19-5a1a7d6a3cf7",
      "number": 15,
      "content": "Tôi được xem như cặp mắt của người mù,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 25,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.408Z",
      "updatedAt": "2025-04-22T08:18:18.408Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "32d96c9b-05f9-47da-b870-2f7ba4f9e7c1",
      "number": 15,
      "content": "Như đôi chân của người què.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 26,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.421Z",
      "updatedAt": "2025-04-22T08:18:18.421Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "5edeb860-e443-419c-81cb-6d8954947330",
      "number": 16,
      "content": "Tôi được coi như cha của người nghèo khó,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 27,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.437Z",
      "updatedAt": "2025-04-22T08:18:18.437Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "beddcf42-53fa-451d-82de-2cd17d6d69ea",
      "number": 16,
      "content": "Như người tranh đấu cho duyên cớ của kiều dân.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 28,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.450Z",
      "updatedAt": "2025-04-22T08:18:18.450Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "9622f7a4-25f5-4fc0-ae59-273a94115c09",
      "number": 17,
      "content": "Tôi bẻ gãy răng nanh của quân gian ác,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 29,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.464Z",
      "updatedAt": "2025-04-22T08:18:18.464Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "ccc79768-206e-4ba5-8bce-b9ed4c1e1d41",
      "number": 17,
      "content": "Và giựt con mồi ra khỏi miệng chúng sắp ăn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 30,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.477Z",
      "updatedAt": "2025-04-22T08:18:18.477Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "33ec18a5-7343-4dae-aeab-e4184116b948",
      "number": 18,
      "content": "Lúc đó tôi đã tự nhủ: Tôi sẽ chết bình yên trong tổ ấm;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 31,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.491Z",
      "updatedAt": "2025-04-22T08:18:18.491Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "50bfaca7-e6ef-4f82-be3a-a3e9c87f6a7c",
      "number": 18,
      "content": "Những ngày của đời tôi sẽ nhiều như cát ở bờ sông.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 32,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.504Z",
      "updatedAt": "2025-04-22T08:18:18.504Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "8e85301c-7165-4301-a06d-ee6bcc9a7b84",
      "number": 19,
      "content": "Rễ của tôi sẽ ăn dần ra dòng nước;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 33,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.518Z",
      "updatedAt": "2025-04-22T08:18:18.518Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "1de90725-d738-49c5-8b5d-0a1e47549040",
      "number": 19,
      "content": "Sương cả đêm sẽ đọng lại trên cành của tôi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 34,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.531Z",
      "updatedAt": "2025-04-22T08:18:18.531Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "2605b8d8-dca0-4351-bc75-150f0b6c2c16",
      "number": 20,
      "content": "Vinh hiển của tôi sẽ luôn luôn mới mẻ;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 35,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.545Z",
      "updatedAt": "2025-04-22T08:18:18.545Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "0d2ccb2f-58d2-495d-a0d3-91111fd08460",
      "number": 20,
      "content": "Cây cung trong tay tôi sẽ được giương bằng sức mới mẻ;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 36,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.558Z",
      "updatedAt": "2025-04-22T08:18:18.558Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "168c6220-6dff-4a22-accf-c5a6b1674a58",
      "number": 21,
      "content": "Người ta đợi để nghe tôi nói,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 37,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.572Z",
      "updatedAt": "2025-04-22T08:18:18.572Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "849a31ae-7973-4160-923c-bbfc2545817a",
      "number": 21,
      "content": "Họ lặng im nghe ý kiến của tôi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 38,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.588Z",
      "updatedAt": "2025-04-22T08:18:18.588Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "51c26b54-d2d5-4ab1-a5ad-8380865d8a6c",
      "number": 22,
      "content": "Sau khi tôi phát biểu không ai muốn bàn gì nữa;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 39,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.601Z",
      "updatedAt": "2025-04-22T08:18:18.601Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "91913003-ec25-485e-b48e-3e717556ce86",
      "number": 22,
      "content": "Vì ý kiến của tôi họ thấy đã tốt quá rồi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 40,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.614Z",
      "updatedAt": "2025-04-22T08:18:18.614Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "d7e65068-e038-4ccf-8884-c79a297f964d",
      "number": 23,
      "content": "Họ trông đợi tôi như người ta chờ mưa xuống;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 41,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.628Z",
      "updatedAt": "2025-04-22T08:18:18.628Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "1b8587ec-184e-45e6-aa21-c577cba75b42",
      "number": 23,
      "content": "Hả miệng ra đón những giọt mưa xuân.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 42,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.641Z",
      "updatedAt": "2025-04-22T08:18:18.641Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "3bdf3bea-f111-46c7-8534-05d6bc13713c",
      "number": 24,
      "content": "Khi tôi mỉm cười với họ, họ khó tin là thật;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 43,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.654Z",
      "updatedAt": "2025-04-22T08:18:18.654Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "72945e49-0d2d-42a1-b487-b2efe89cee9a",
      "number": 24,
      "content": "Nét rạng rỡ trên mặt tôi khiến họ khó u buồn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 44,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.667Z",
      "updatedAt": "2025-04-22T08:18:18.667Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "6efd4291-5560-4181-bfdc-abee1db742fb",
      "number": 25,
      "content": "Tôi chọn cho họ con đường nào tốt nhất;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 45,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.680Z",
      "updatedAt": "2025-04-22T08:18:18.680Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "381e7270-dd07-4f62-a8e6-68c04314fda9",
      "number": 25,
      "content": "Tôi ngồi giữa họ như thủ lãnh của họ;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 46,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.693Z",
      "updatedAt": "2025-04-22T08:18:18.693Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "cc487ad0-5800-42bc-8fd7-d037e55c8da6",
      "number": 25,
      "content": "Tôi sống giữa họ như vị vua sống giữa ba quân,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 47,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.707Z",
      "updatedAt": "2025-04-22T08:18:18.707Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    },
    {
      "id": "c44aa7d1-2f2a-4b5b-aef9-738c25ab4054",
      "number": 25,
      "content": "Như một người an ủi những người sầu thảm.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 48,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:18:18.720Z",
      "updatedAt": "2025-04-22T08:18:18.720Z",
      "chapterId": "4496c724-9d84-49f7-a25f-d5ca8ae5b651"
    }
  ],
