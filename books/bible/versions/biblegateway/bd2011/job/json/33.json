      "id": "ce8bb8c4-eff3-49db-a306-285107d28f09",
      "content": "Ê-li-hu Trách Gióp",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T03:46:06.717Z",
      "updatedAt": "2025-04-24T03:46:06.717Z",
