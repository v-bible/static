    },
    {
      "id": "cbbbd74a-c43a-4a1e-a7f7-04ab70f0ddbd",
      "number": 20,
      "content": "Những người ở phương đông nghe về nó đều phải kinh hồn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 38,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:17:17.511Z",
      "updatedAt": "2025-04-22T08:17:17.511Z",
      "chapterId": "47a15d8e-ef9e-4c20-849a-daad32d0e797"
    },
    {
      "id": "92435336-bf1e-467a-bc43-c6ae96ac1731",
      "number": 21,
      "content": "Quả thật đó là nơi ở của phường gian ác;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 39,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:17:17.524Z",
      "updatedAt": "2025-04-22T08:17:17.524Z",
      "chapterId": "47a15d8e-ef9e-4c20-849a-daad32d0e797"
    },
    {
      "id": "4f584073-c3d8-465f-8799-1de11c3f7ed2",
      "number": 21,
      "content": "Ðó là nơi ở của những kẻ không nhận biết Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 40,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:17:17.537Z",
      "updatedAt": "2025-04-22T08:17:17.537Z",
