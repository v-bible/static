    },
    {
      "id": "e7eb3cf4-72bc-4dd7-a290-20c49b62e30a",
      "number": 20,
      "content": "Những người ở phương đông nghe về nó đều phải kinh hồn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 38,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:44:37.692Z",
      "updatedAt": "2025-04-24T03:44:37.692Z",
      "chapterId": "f21aa208-b99a-4688-81a9-8f1f47c1b31f"
    },
    {
      "id": "923e40ad-67bc-44c0-840d-ddcf2da46d60",
      "number": 21,
      "content": "Quả thật đó là nơi ở của phường gian ác;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 39,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:44:37.708Z",
      "updatedAt": "2025-04-24T03:44:37.708Z",
      "chapterId": "f21aa208-b99a-4688-81a9-8f1f47c1b31f"
    },
    {
      "id": "088cc86d-2765-40b9-a17a-80bc95d2041b",
      "number": 21,
      "content": "Ðó là nơi ở của những kẻ không nhận biết Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 40,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:44:37.722Z",
      "updatedAt": "2025-04-24T03:44:37.722Z",
