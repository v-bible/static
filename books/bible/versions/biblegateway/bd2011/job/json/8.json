{
  "chapter": {
    "id": "327d0654-b5ed-46bd-a624-4fcbc02048c2",
    "number": 8,
    "ref": "https://www.biblegateway.com/passage/?search=Gi%C3%B3p%208&version=BD2011",
    "createdAt": "2025-04-24T03:03:13.973Z",
    "updatedAt": "2025-04-24T03:03:13.973Z",
    "bookId": "5f99eb6e-a440-4453-bedd-dc90f5d3a3ae"
  },
  "verses": [
    {
      "id": "5590010e-0e01-4e13-9a8d-05ea5f6306dc",
      "number": 1,
      "content": "Bấy giờ Binh-đát người Su-a đáp lời và nói:",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T03:43:41.085Z",
      "updatedAt": "2025-04-24T03:43:41.085Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "e0a1b76f-10b6-4d4e-abc8-0643bf5affbe",
      "number": 2,
      "content": "Anh cứ nói những điều ấy cho đến bao giờ mới thôi?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.113Z",
      "updatedAt": "2025-04-24T03:43:41.113Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "bdce57ec-4f44-4d59-867b-0aae27498b9f",
      "number": 2,
      "content": "Anh cứ nói ào ào như cuồng phong thổi tới.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.126Z",
      "updatedAt": "2025-04-24T03:43:41.126Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "b806207a-1ac0-4469-9d30-0082364bca62",
      "number": 3,
      "content": "Ðức Chúa Trời có bao giờ bẻ cong công lý chăng?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.139Z",
      "updatedAt": "2025-04-24T03:43:41.139Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "cb20b621-2440-45d0-a9b9-a07998cb9bb5",
      "number": 3,
      "content": "Ðấng Toàn Năng há uốn cong lẽ chính trực hay sao?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.153Z",
      "updatedAt": "2025-04-24T03:43:41.153Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "15faa489-7f9b-427a-a466-b43c1f55ef4d",
      "number": 4,
      "content": "Nếu các con anh đã phạm tội đối với Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.166Z",
      "updatedAt": "2025-04-24T03:43:41.166Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "38955f70-2031-461d-84a1-5c2e7444f8ac",
      "number": 4,
      "content": "Thì Ngài bắt chúng phải gánh lấy hậu quả tội lỗi của chúng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.179Z",
      "updatedAt": "2025-04-24T03:43:41.179Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "24ff211b-3fde-4629-9280-56d46bbe1886",
      "number": 5,
      "content": "Nếu anh chuyên tâm tìm kiếm Ðức Chúa Trời,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.192Z",
      "updatedAt": "2025-04-24T03:43:41.192Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "2acdf563-204b-4c3d-b357-5155241fd0fb",
      "number": 5,
      "content": "Và hết lòng khẩn nguyện với Ðấng Toàn Năng,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.206Z",
      "updatedAt": "2025-04-24T03:43:41.206Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "f8e4f451-7a9c-4c1e-ad23-5bcc37d05b14",
      "number": 6,
      "content": "Nếu anh quả thật là người trong sạch và ngay thẳng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.219Z",
      "updatedAt": "2025-04-24T03:43:41.219Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "a58e81c5-75a1-4b92-8c51-970f63488eaa",
      "number": 6,
      "content": "Thì bây giờ Ngài sẽ đứng dậy để binh vực anh,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.233Z",
      "updatedAt": "2025-04-24T03:43:41.233Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "4e46f058-c64d-48ad-8a92-175c6aa84f7a",
      "number": 6,
      "content": "Và sẽ tái lập anh vào cương vị của một người công chính;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.246Z",
      "updatedAt": "2025-04-24T03:43:41.246Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "121836a2-a0fa-42aa-91fd-02bf542b5001",
      "number": 7,
      "content": "Dù lúc bắt đầu hơi có phần khiêm tốn,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.259Z",
      "updatedAt": "2025-04-24T03:43:41.259Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "38d6f0bc-34a6-4537-bf84-2cb2586916b3",
      "number": 7,
      "content": "Nhưng đến cuối cùng sẽ vĩ đại khôn lường.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.273Z",
      "updatedAt": "2025-04-24T03:43:41.273Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "4712daa8-b00d-43d7-8b99-293943fe6f04",
      "number": 8,
      "content": "Tôi xin anh hãy tìm hiểu kinh nghiệm của các bậc tiền nhân,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.286Z",
      "updatedAt": "2025-04-24T03:43:41.286Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "40b76f39-39f9-41ac-a819-8c9b5ca3e161",
      "number": 8,
      "content": "Và suy xét những gì tổ tiên của họ đã tìm thấy.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.299Z",
      "updatedAt": "2025-04-24T03:43:41.299Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "493946e7-0b6c-4b48-8986-637777841c04",
      "number": 9,
      "content": "Vì chúng ta mới có mặt ngày hôm qua và chẳng biết gì,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.312Z",
      "updatedAt": "2025-04-24T03:43:41.312Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "55dc87b0-51b7-4ca3-a656-efe4b1e1b703",
      "number": 9,
      "content": "Vì những ngày của chúng ta trên đất qua mau như cái bóng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.326Z",
      "updatedAt": "2025-04-24T03:43:41.326Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "d7eb5c02-0079-4481-b829-a8b172df76fd",
      "number": 10,
      "content": "Há chẳng phải các vị tiền bối sẽ dạy anh và bảo anh,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.339Z",
      "updatedAt": "2025-04-24T03:43:41.339Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "d6386c45-1bf5-4a47-a2e9-671adb584630",
      "number": 10,
      "content": "Và sẽ nói cho anh biết tri thức nằm lòng của họ hay sao?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.352Z",
      "updatedAt": "2025-04-24T03:43:41.352Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "0f84f345-889c-458e-9641-f123c0a65fe0",
      "number": 11,
      "content": "Cây lau há có thể mọc lên nơi không phải bùn lầy?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.365Z",
      "updatedAt": "2025-04-24T03:43:41.365Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "8335b2e5-063e-4373-a307-7283fb41e6aa",
      "number": 11,
      "content": "Cây sậy há có thể mọc lên nơi không có nước?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.378Z",
      "updatedAt": "2025-04-24T03:43:41.378Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "2de00448-d304-44c6-822b-262234de1d33",
      "number": 12,
      "content": "Khi đang còn xanh tươi chưa kịp cắt về,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.391Z",
      "updatedAt": "2025-04-24T03:43:41.391Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "65bb76de-87fd-47c8-be14-44bd3f2ec562",
      "number": 12,
      "content": "Chúng đã bị khô héo trước các loài cỏ khác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.405Z",
      "updatedAt": "2025-04-24T03:43:41.405Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "ee0256c9-abfc-41b5-b109-e38f561446ec",
      "number": 13,
      "content": "Ðó là số phận của mọi kẻ quên ơn lìa bỏ Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 23,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.418Z",
      "updatedAt": "2025-04-24T03:43:41.418Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "adca4996-2393-4955-a31f-a324f75dc93a",
      "number": 13,
      "content": "Hy vọng của những kẻ không tin kính Chúa sẽ tiêu vong.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 24,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.431Z",
      "updatedAt": "2025-04-24T03:43:41.431Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "1cd412e6-1dde-46ec-bb51-2572a5929d46",
      "number": 14,
      "content": "Nơi nó nương dựa bị hoàn toàn sụp đổ;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 25,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.444Z",
      "updatedAt": "2025-04-24T03:43:41.444Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "15c4b039-7c15-4b00-8541-1ca0df76509f",
      "number": 14,
      "content": "Chỗ nó tin cậy chỉ là màng nhện mong manh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 26,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.458Z",
      "updatedAt": "2025-04-24T03:43:41.458Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "3d3d06c4-2c5a-4e4a-8e15-9b9ff429401b",
      "number": 15,
      "content": "Nó dựa vào căn nhà, nhưng nhà không đứng vững;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 27,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.471Z",
      "updatedAt": "2025-04-24T03:43:41.471Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "2f5f4a03-a919-4880-833b-1f7a4bbe697c",
      "number": 15,
      "content": "Nó bám vào căn nhà, nhưng nhà chẳng còn được bao lâu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 28,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.487Z",
      "updatedAt": "2025-04-24T03:43:41.487Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "8d8396da-8524-419e-8ecd-5ac1f52ce91e",
      "number": 16,
      "content": "Nó như cây lớn mạnh xanh tươi dưới ánh mặt trời,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 29,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.501Z",
      "updatedAt": "2025-04-24T03:43:41.501Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "b1e3d0bc-0d18-42db-8e41-2136825ef0d5",
      "number": 16,
      "content": "Các nhánh nó vươn ra tỏa rộng khắp vườn;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 30,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.514Z",
      "updatedAt": "2025-04-24T03:43:41.514Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "52d284db-b425-442b-a647-329695d17f95",
      "number": 17,
      "content": "Các rễ nó quấn chằng chịt chung quanh đống đá;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 31,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.527Z",
      "updatedAt": "2025-04-24T03:43:41.527Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "66cf02d5-caf5-498f-a91a-dd6a7cb07c37",
      "number": 17,
      "content": "Các rễ con nó len lỏi vào giữa những sỏi đá và bám vào.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 32,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.541Z",
      "updatedAt": "2025-04-24T03:43:41.541Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "5eef5a5d-020e-475c-8527-60a4660e294c",
      "number": 18,
      "content": "Nhưng khi nó vừa bị bật gốc khỏi nơi ấy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 33,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.554Z",
      "updatedAt": "2025-04-24T03:43:41.554Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "34f692b8-1dc9-4e64-8b2d-0868c6725b6e",
      "number": 18,
      "content": "Nơi ấy liền phủ nhận nó rằng, “Ta chưa hề thấy ngươi.”",
      "order": 1,
      "parNumber": 1,
      "parIndex": 34,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.567Z",
      "updatedAt": "2025-04-24T03:43:41.567Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "f765bf54-2689-43a0-830e-228e903f73dd",
      "number": 19,
      "content": "Này, đây là niềm vui của đường lối nó:",
      "order": 0,
      "parNumber": 1,
      "parIndex": 35,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.580Z",
      "updatedAt": "2025-04-24T03:43:41.580Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "1b43836c-f9b6-4811-89f2-2d8aa55e3c84",
      "number": 19,
      "content": "Từ dưới mặt đất sẽ có các cây khác mọc lên.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 36,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.594Z",
      "updatedAt": "2025-04-24T03:43:41.594Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "646058fc-cef0-4ba3-b12c-9615db32ec91",
      "number": 20,
      "content": "Này, Ðức Chúa Trời sẽ không loại bỏ người vẹn toàn đạo đức;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 37,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.607Z",
      "updatedAt": "2025-04-24T03:43:41.607Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "9ed43097-80cb-4d2b-9ea2-5985084f62a7",
      "number": 20,
      "content": "Ngài sẽ không phù hộ tay kẻ gian ác bao giờ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 38,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.620Z",
      "updatedAt": "2025-04-24T03:43:41.620Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "829c43d9-a6a9-48fa-b19a-f70c060f6091",
      "number": 21,
      "content": "Ngài sẽ làm cho miệng anh đầy tiếng cười vui vẻ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 39,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.633Z",
      "updatedAt": "2025-04-24T03:43:41.633Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "04f4fd40-9949-4a0d-9c1d-f2015a5966f5",
      "number": 21,
      "content": "Ngài sẽ ban cho môi anh vang những tiếng reo mừng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 40,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.647Z",
      "updatedAt": "2025-04-24T03:43:41.647Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "05c24308-10d5-48ad-b269-72dc164f7e1c",
      "number": 22,
      "content": "Những kẻ ghét anh sẽ mặc lấy xấu hổ;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 41,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.660Z",
      "updatedAt": "2025-04-24T03:43:41.660Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    },
    {
      "id": "d1ba73b6-2153-4108-9e63-f08dcde172a3",
      "number": 22,
      "content": "Và chỗ ở của bọn gian ác sẽ không còn nữa.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 42,
      "isPoetry": true,
      "createdAt": "2025-04-24T03:43:41.673Z",
      "updatedAt": "2025-04-24T03:43:41.673Z",
      "chapterId": "327d0654-b5ed-46bd-a624-4fcbc02048c2"
    }
  ],
