    },
    {
      "id": "df95f911-6c4e-4de2-bdf2-71a040a883cf",
      "number": 40,
      "content": "Hay khi nào chúng sẽ rình mồi trong bụi rậm?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 78,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:09.258Z",
      "updatedAt": "2025-04-22T08:19:09.258Z",
      "chapterId": "82ca291c-28f8-4db1-92b6-bb70fdf69a72"
    },
    {
      "id": "3163236e-2b75-4ff4-98a5-31b8d45e7ede",
      "number": 41,
      "content": "Ai cung cấp thức ăn cho quạ khi đàn quạ con đói bụng kêu xin Ðức Chúa Trời,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 79,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:09.268Z",
      "updatedAt": "2025-04-22T08:19:09.268Z",
      "chapterId": "82ca291c-28f8-4db1-92b6-bb70fdf69a72"
    },
    {
      "id": "6cbce1d9-2bde-4c76-a259-ea3fd6fed323",
      "number": 41,
      "content": "Trong khi quạ mẹ đi tìm khắp nơi mà vẫn không kiếm ra được thức ăn?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 80,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:19:09.279Z",
      "updatedAt": "2025-04-22T08:19:09.279Z",
