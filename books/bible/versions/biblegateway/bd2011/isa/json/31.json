    },
    {
      "id": "1936435d-dc0c-4006-8627-48fdac7a59a6",
      "content": "Chúa Sẽ Giải Cứu Giê-ru-sa-lem",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T08:45:06.373Z",
      "updatedAt": "2025-04-22T08:45:06.373Z",
