{
  "chapter": {
    "id": "da3d223a-f7ca-4819-84ab-7af63ab73ceb",
    "number": 29,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2029&version=BD2011",
    "createdAt": "2025-04-24T03:03:20.889Z",
    "updatedAt": "2025-04-24T03:03:20.889Z",
    "bookId": "e4e58f0e-ae57-420f-8009-a5608bc5a58a"
  },
  "verses": [
    {
      "id": "fe4b1b76-bb80-4970-93e5-c3300ae8d621",
      "number": 1,
      "content": "Khốn cho A-ri-ên, cho A-ri-ên,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.054Z",
      "updatedAt": "2025-04-24T04:13:45.054Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "ead9bf8c-9c7d-4386-bee1-9876b8da994c",
      "number": 1,
      "content": "Thành trước kia Ða-vít đã hạ trại dàn quân,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.094Z",
      "updatedAt": "2025-04-24T04:13:45.094Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "a096f89f-2099-4e0a-ae17-d9ba47b7b0a9",
      "number": 1,
      "content": "Vì từ năm này qua năm khác,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.106Z",
      "updatedAt": "2025-04-24T04:13:45.106Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "67df7819-1ce6-410c-91b4-0cda73794082",
      "number": 1,
      "content": "Người ta cứ theo chu kỳ tổ chức hết lễ nọ đến lễ kia.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.118Z",
      "updatedAt": "2025-04-24T04:13:45.118Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "a8818d73-968e-441b-a61f-0e9f2ceb1419",
      "number": 2,
      "content": "Dầu vậy Ta sẽ làm cho A-ri-ên gặp cơn khốn khó,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.131Z",
      "updatedAt": "2025-04-24T04:13:45.131Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "c030855e-e340-4a7b-8b79-4727b6680447",
      "number": 2,
      "content": "Rồi nó sẽ có than khóc và thảm sầu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.145Z",
      "updatedAt": "2025-04-24T04:13:45.145Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "ca906c3b-3f87-408f-ba3f-3a1d5f9c0ad6",
      "number": 2,
      "content": "Nhưng đối với Ta nó vẫn mãi là A-ri-ên.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.158Z",
      "updatedAt": "2025-04-24T04:13:45.158Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "cd09c98a-f97e-4c36-b424-8c40cd843ad2",
      "number": 3,
      "content": "Giống như Ða-vít, Ta sẽ hạ trại chống lại ngươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.178Z",
      "updatedAt": "2025-04-24T04:13:45.178Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "bea4dcc9-4873-44f3-aa0a-05c7b83c04ec",
      "number": 3,
      "content": "Ta sẽ cất các tháp canh bao vây ngươi;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.191Z",
      "updatedAt": "2025-04-24T04:13:45.191Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "d7faa7fa-5ac4-4c27-a0c4-ef320323974f",
      "number": 3,
      "content": "Ta sẽ lập chiến lũy cô lập ngươi.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.205Z",
      "updatedAt": "2025-04-24T04:13:45.205Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "0f8389b3-f1ca-44bc-b6fe-df534459f031",
      "number": 4,
      "content": "Ngươi sẽ bị hạ xuống;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.218Z",
      "updatedAt": "2025-04-24T04:13:45.218Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "9abe52bb-42f8-40c1-9ebb-22329273415d",
      "number": 4,
      "content": "Bấy giờ từ dưới lòng đất ngươi sẽ cất tiếng lên;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.231Z",
      "updatedAt": "2025-04-24T04:13:45.231Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "5f677813-ccae-44bf-ab8b-75c7e817941c",
      "number": 4,
      "content": "Tận dưới bụi đất lời ngươi nói sẽ vọng lên văng vẳng;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.245Z",
      "updatedAt": "2025-04-24T04:13:45.245Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "248df485-a541-4273-98d2-c211c2186acb",
      "number": 4,
      "content": "Tiếng của ngươi từ dưới đất thoát ra ai oán tựa hồn ma,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.258Z",
      "updatedAt": "2025-04-24T04:13:45.258Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "9ad64850-9c25-4898-866f-08bf210ea38c",
      "number": 4,
      "content": "Từ dưới bụi đất tiếng của ngươi thều thào yếu ớt.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.272Z",
      "updatedAt": "2025-04-24T04:13:45.272Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "dd1ce0ef-1e47-46e4-9f80-8044606a9a7e",
      "number": 5,
      "content": "Nhưng rồi đoàn quân thù đông đảo vây ngươi sẽ như bụi nhỏ li ti;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.286Z",
      "updatedAt": "2025-04-24T04:13:45.286Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "228f9248-ccaf-4516-bc94-40778ce719a4",
      "number": 5,
      "content": "Ðoàn quân đông cường bạo đó sẽ bị thổi bay đi như trấu rác;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.299Z",
      "updatedAt": "2025-04-24T04:13:45.299Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "1f803563-c22c-4415-9e4d-3b23a3140b7d",
      "number": 5,
      "content": "Việc ấy sẽ xảy ra thình lình và hoàn tất trong khoảnh khắc.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.313Z",
      "updatedAt": "2025-04-24T04:13:45.313Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "a7fde8c7-79a5-4c4f-a901-961c1b5a078a",
      "number": 6,
      "content": "Chúa các đạo quân sẽ thăm viếng ngươi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.326Z",
      "updatedAt": "2025-04-24T04:13:45.326Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "6c2696b4-397a-4879-84bb-271ec8c9d8a3",
      "number": 6,
      "content": "Cùng với sấm sét, động đất, và tiếng nổ rầm trời,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.339Z",
      "updatedAt": "2025-04-24T04:13:45.339Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "69e3c261-d751-4eac-9f19-410696a976b1",
      "number": 6,
      "content": "Với cuồng phong, bão tố, và lửa cháy ngập trời,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.352Z",
      "updatedAt": "2025-04-24T04:13:45.352Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "704dc743-c23f-460a-a3c3-a6ac4bcdc2b3",
      "number": 7,
      "content": "Bấy giờ đoàn quân đông đảo của tất cả các nước đang tấn công A-ri-ên;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.365Z",
      "updatedAt": "2025-04-24T04:13:45.365Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "a7131958-c107-4994-afcc-6f4f6c8915c6",
      "number": 7,
      "content": "Tất cả những kẻ tiến đánh nó, công hãm thành trì nó, và làm nó khổ sở sẽ biến mất như một giấc mơ, như một cảnh tượng thấy trong chiêm bao.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.378Z",
      "updatedAt": "2025-04-24T04:13:45.378Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "3ff68426-7f6e-46b3-8450-a86b926b1eaf",
      "number": 8,
      "content": "Giống như người đói nằm mơ thấy mình được ăn no, khi thức dậy đói vẫn hoàn đói thể nào,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.394Z",
      "updatedAt": "2025-04-24T04:13:45.394Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "f714fbf2-0339-44b2-847e-0a01902bcaa7",
      "number": 8,
      "content": "Hay như người khát nằm chiêm bao thấy mình được uống nước, khi thức dậy thấy người mệt lả, khát vẫn hoàn khát thể nào,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.410Z",
      "updatedAt": "2025-04-24T04:13:45.410Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "d7fa5b52-5aed-479c-a056-478def80ba93",
      "number": 8,
      "content": "Ðoàn quân đông đảo của tất cả các nước tấn công Núi Si-ôn cũng sẽ như thể ấy.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.423Z",
      "updatedAt": "2025-04-24T04:13:45.423Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "ba47c952-2187-4312-92da-6a3b167e9f93",
      "number": 9,
      "content": "Hãy sững sờ và kinh ngạc!",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.433Z",
      "updatedAt": "2025-04-24T04:13:45.433Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "754c5aac-85e0-4de9-9129-2f99ca62310a",
      "number": 9,
      "content": "Hãy che mắt để khỏi nhìn thấy và làm cho mình như đã bị mù đi!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.443Z",
      "updatedAt": "2025-04-24T04:13:45.443Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "101230fb-264f-483e-b320-8035823f3cd5",
      "number": 9,
      "content": "Hãy như người say, nhưng không phải say vì rượu,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.453Z",
      "updatedAt": "2025-04-24T04:13:45.453Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "590ffa97-3834-419d-8082-1372de04c0cb",
      "number": 9,
      "content": "Hãy loạng choạng bước đi, nhưng không phải vì bị ảnh hưởng của thức uống say!",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.463Z",
      "updatedAt": "2025-04-24T04:13:45.463Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "f9cf6df9-0a0a-4a7b-bc38-68971bda2331",
      "number": 10,
      "content": "Vì Chúa đã đổ xuống các ngươi một thần khí hôn mê;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.474Z",
      "updatedAt": "2025-04-24T04:13:45.474Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "219f2f71-1d5f-4a3e-8080-29818ef9b78c",
      "number": 10,
      "content": "Hỡi các ngươi là các tiên tri,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.486Z",
      "updatedAt": "2025-04-24T04:13:45.486Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "62edc609-07d9-417f-8456-9eeedf39db9a",
      "number": 10,
      "content": "Ngài đã làm cho mắt các ngươi nhắm lại;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.500Z",
      "updatedAt": "2025-04-24T04:13:45.500Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "a9dff0e8-47fd-44cd-9176-53b22d9c781c",
      "number": 10,
      "content": "Hỡi các ngươi là các nhà tiên kiến, đầu các ngươi Ngài đã trùm kín lại rồi.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.513Z",
      "updatedAt": "2025-04-24T04:13:45.513Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "24ebd54d-4bd8-4b2d-a062-77edaa54fca7",
      "number": 11,
      "content": "Khải tượng về tất cả những điều này, đối với các ngươi, giống như một tài liệu bị niêm phong. Nếu nó được trao cho một người biết đọc và bảo, “Hãy đọc tài liệu này,” người ấy sẽ bảo, “Tôi không thể đọc được, vì nó đã bị niêm phong.”",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:13:45.526Z",
      "updatedAt": "2025-04-24T04:13:45.526Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "a0b1f514-9c24-43c8-8cfd-daaa242c74c9",
      "number": 12,
      "content": "Còn nếu nó được trao cho một người không biết chữ và bảo, “Hãy đọc tài liệu này,” người ấy sẽ đáp, “Tôi không biết đọc.”",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:13:45.540Z",
      "updatedAt": "2025-04-24T04:13:45.540Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "4a23661e-73b9-4550-b2ea-eea9326699cc",
      "number": 13,
      "content": "Chúa phán,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-24T04:13:45.553Z",
      "updatedAt": "2025-04-24T04:13:45.553Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "cd7f8ab9-de2b-4c78-8e02-865ebddd7aee",
      "number": 13,
      "content": "“Vì dân này đến gần Ta chỉ bằng miệng lưỡi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.566Z",
      "updatedAt": "2025-04-24T04:13:45.566Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "d456a6bc-85da-4fe7-88bd-63e85aeb33da",
      "number": 13,
      "content": "Chúng tôn kính Ta chỉ bằng đầu môi chót lưỡi mà thôi,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.580Z",
      "updatedAt": "2025-04-24T04:13:45.580Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "b656ee97-9583-4b1d-8b8c-8ba9c1d50a52",
      "number": 13,
      "content": "Còn lòng của chúng cách xa Ta lắm.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.593Z",
      "updatedAt": "2025-04-24T04:13:45.593Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "c441aa9c-7c61-4c61-ba26-003b31dd75cd",
      "number": 13,
      "content": "Chúng thờ phượng Ta vì theo truyền thống của loài người;",
      "order": 4,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.606Z",
      "updatedAt": "2025-04-24T04:13:45.606Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "3ee0510e-431d-4ca2-be01-b750594df4d2",
      "number": 13,
      "content": "Những gì chúng được dạy bảo chỉ toàn là sáo ngữ.",
      "order": 5,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.622Z",
      "updatedAt": "2025-04-24T04:13:45.622Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "264f20a9-ee0f-4725-a26f-faf50862f1b2",
      "number": 14,
      "content": "Vì thế, một lần nữa, Ta sẽ làm cho dân này những điều kỳ lạ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.639Z",
      "updatedAt": "2025-04-24T04:13:45.639Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "7226ce0f-2782-4623-a2be-3a15b2fbb603",
      "number": 14,
      "content": "Những ngạc nhiên khiến chúng phải sững sờ;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.652Z",
      "updatedAt": "2025-04-24T04:13:45.652Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "7bd187d8-cb73-4fd4-a989-ab6efef2e9db",
      "number": 14,
      "content": "Khiến sự khôn ngoan của những người khôn ngoan của chúng ra vô dụng,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.665Z",
      "updatedAt": "2025-04-24T04:13:45.665Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "9cf3b942-aa3c-4a6e-85e6-7f9030ea664c",
      "number": 14,
      "content": "Làm sự thông thái của những kẻ thông thái của chúng bị che khuất.”",
      "order": 3,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.679Z",
      "updatedAt": "2025-04-24T04:13:45.679Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "4670656a-f29b-4271-858f-db6f223378f4",
      "number": 15,
      "content": "Khốn cho các ngươi, những kẻ cố tình giấu kỹ chương trình của mình khỏi Chúa;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.691Z",
      "updatedAt": "2025-04-24T04:13:45.691Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "2637b7f7-2422-4b26-b7c3-220cfbea55e6",
      "number": 15,
      "content": "Các ngươi hành động trong bóng tối, rồi nói rằng,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.703Z",
      "updatedAt": "2025-04-24T04:13:45.703Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "92c426bf-dd12-414e-89de-d534a1140e58",
      "number": 15,
      "content": "“Ðâu có ai thấy chúng ta?",
      "order": 2,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.714Z",
      "updatedAt": "2025-04-24T04:13:45.714Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "237c0ed1-f228-46fd-a7b7-6f438fb2067b",
      "number": 15,
      "content": "Nào ai biết chúng ta đã làm gì?”",
      "order": 3,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.724Z",
      "updatedAt": "2025-04-24T04:13:45.724Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "5ad9dc5c-bd38-4a75-99cf-2573768f787f",
      "number": 16,
      "content": "Thật các ngươi quả làm chuyện ngược đời!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.736Z",
      "updatedAt": "2025-04-24T04:13:45.736Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "d0982652-3765-46b0-82f1-4275c69edc5f",
      "number": 16,
      "content": "Thợ gốm có bao giờ bị xem ngang hàng như đất sét chăng?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.746Z",
      "updatedAt": "2025-04-24T04:13:45.746Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "fa922122-d038-4cc6-a1ab-8b03165e6cbe",
      "number": 16,
      "content": "Có thể nào một sản phẩm nói với người tạo ra nó rằng,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.757Z",
      "updatedAt": "2025-04-24T04:13:45.757Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "a090b309-ad9c-4c90-b208-03ce2fec3a10",
      "number": 16,
      "content": "“Người ấy đâu có làm ra tôi,”",
      "order": 3,
      "parNumber": 5,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.772Z",
      "updatedAt": "2025-04-24T04:13:45.772Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "ff6976a3-c2ab-48ee-b0ba-0b8968a64ced",
      "number": 16,
      "content": "Hay món đồ gốm có thể nói với người thợ gốm rằng,",
      "order": 4,
      "parNumber": 5,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.785Z",
      "updatedAt": "2025-04-24T04:13:45.785Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "b2de36b6-8d4f-46c2-a114-c6eb23e677da",
      "number": 16,
      "content": "“Hắn chẳng biết gì” chăng?",
      "order": 5,
      "parNumber": 5,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.798Z",
      "updatedAt": "2025-04-24T04:13:45.798Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "da03faaf-eb50-45b0-8f10-d6cebbedc119",
      "number": 17,
      "content": "Há chẳng phải trong ít lâu nữa Li-băng sẽ trở thành một cánh đồng phì nhiêu,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.811Z",
      "updatedAt": "2025-04-24T04:13:45.811Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "5585e0d0-158d-46d2-b357-7960935378b3",
      "number": 17,
      "content": "Rồi cánh đồng phì nhiêu ấy sẽ biến thành một đám rừng trù phú hay sao?",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.837Z",
      "updatedAt": "2025-04-24T04:13:45.837Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "1c7c6533-36d8-464a-8261-606e0586b07c",
      "number": 18,
      "content": "Trong ngày ấy người điếc sẽ được nghe những lời chép trong sách;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.851Z",
      "updatedAt": "2025-04-24T04:13:45.851Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "3e940492-28a9-4397-bf27-69f39b26bfca",
      "number": 18,
      "content": "Thoát ra khỏi cảnh mù mịt tối tăm, mắt của người đui sẽ trông thấy rõ ràng.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.864Z",
      "updatedAt": "2025-04-24T04:13:45.864Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "ce8474fb-73e9-44b8-ae01-6301f5bf4543",
      "number": 19,
      "content": "Người thấp kém sẽ có thêm niềm vui mới trong Chúa;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.877Z",
      "updatedAt": "2025-04-24T04:13:45.877Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "037b85ad-3657-4268-8fae-1d6dd0a1b284",
      "number": 19,
      "content": "Người khó nghèo sẽ vui mừng trong Ðấng Thánh của I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.890Z",
      "updatedAt": "2025-04-24T04:13:45.890Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "b0e36c3c-d70a-4f99-9a0d-b54c265e09b1",
      "number": 20,
      "content": "Vì những kẻ bạo tàn sẽ không còn nữa;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.904Z",
      "updatedAt": "2025-04-24T04:13:45.904Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "206aad6f-72c7-41d2-a1e1-63115dd0d314",
      "number": 20,
      "content": "Quân nhạo báng kiêu căng đều bị diệt sạch cả rồi;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.917Z",
      "updatedAt": "2025-04-24T04:13:45.917Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "0a723777-517a-4da7-9509-49aaad793165",
      "number": 20,
      "content": "Tất cả những kẻ rắp tâm làm điều ác sẽ bị tiêu diệt,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.930Z",
      "updatedAt": "2025-04-24T04:13:45.930Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "d77f88e1-a906-457d-9532-3674f205c00b",
      "number": 21,
      "content": "Tức những kẻ dùng lời nói dối khiến người ngay mắc tội;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.943Z",
      "updatedAt": "2025-04-24T04:13:45.943Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "00e67dc7-fa68-41af-aedb-3ff75817008f",
      "number": 21,
      "content": "Tại cửa công, người ngồi xử án cũng bị mắc bẫy mà kết tội sai lầm;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.957Z",
      "updatedAt": "2025-04-24T04:13:45.957Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "c154d181-1fb6-47e5-b9dc-f4ce45d3d575",
      "number": 21,
      "content": "Chúng đã khéo dùng những lời lẽ vô căn cứ để loại ra công lý của người công chính.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.971Z",
      "updatedAt": "2025-04-24T04:13:45.971Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "727775fd-c7a1-47fe-b225-a6a03100f573",
      "number": 22,
      "content": "Vì vậy, Chúa, Ðấng cứu chuộc Áp-ra-ham, phán về nhà Gia-cốp thế này:",
      "order": 0,
      "parNumber": 6,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.984Z",
      "updatedAt": "2025-04-24T04:13:45.984Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "5a9dd317-354f-4586-a42a-ff22d7ca1c62",
      "number": 22,
      "content": "Gia-cốp sẽ không còn bị hổ thẹn nữa,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:45.998Z",
      "updatedAt": "2025-04-24T04:13:45.998Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "37ec181f-760d-4303-9b57-511d5bf0eaef",
      "number": 22,
      "content": "Mặt của nó sẽ không còn biến sắc vì xấu hổ nữa.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:46.011Z",
      "updatedAt": "2025-04-24T04:13:46.011Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "aa282329-9d31-48f9-b8ce-938ad4bee0a4",
      "number": 23,
      "content": "Nhưng khi nó thấy con cháu nó, là công việc của tay Ta, ở giữa nó,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:46.024Z",
      "updatedAt": "2025-04-24T04:13:46.024Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "b9e9db9f-d756-4bbe-8b13-81e3ac0fb72d",
      "number": 23,
      "content": "Con cháu nó sẽ tôn thánh danh Ta,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:46.038Z",
      "updatedAt": "2025-04-24T04:13:46.038Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "3f832b1c-447b-40a9-928a-8f83b130d37b",
      "number": 23,
      "content": "Chúng sẽ tôn thánh Ðấng Thánh của Gia-cốp,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:46.051Z",
      "updatedAt": "2025-04-24T04:13:46.051Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "63b24f96-2aba-4acc-882c-29700c6188d0",
      "number": 23,
      "content": "Và chúng sẽ kính sợ Ðức Chúa Trời của I-sơ-ra-ên.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:46.064Z",
      "updatedAt": "2025-04-24T04:13:46.064Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "2bda6b94-a049-4000-ba1f-795b6562a032",
      "number": 24,
      "content": "Bấy giờ những kẻ có tâm linh lầm lạc sẽ bắt đầu hiểu biết;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:13:46.077Z",
      "updatedAt": "2025-04-24T04:13:46.077Z",
      "chapterId": "da3d223a-f7ca-4819-84ab-7af63ab73ceb"
    },
    {
      "id": "1a5a150f-082b-4d84-a06d-32bf142fe42b",
      "number": 24,
      "content": "Những kẻ hay oán trách lầm bầm sẽ chấp nhận lời chỉ bảo dạy khuyên.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 20,
