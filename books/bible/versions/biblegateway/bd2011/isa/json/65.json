{
  "chapter": {
    "id": "99a1a13c-ee22-48d2-9b40-5a7432a18b39",
    "number": 65,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2065&version=BD2011",
    "createdAt": "2025-04-24T03:03:21.734Z",
    "updatedAt": "2025-04-24T03:03:21.734Z",
    "bookId": "e4e58f0e-ae57-420f-8009-a5608bc5a58a"
  },
  "verses": [
    {
      "id": "c2418009-a2d8-4cea-a18c-68071330427a",
      "number": 1,
      "content": "“Ta đã cho những kẻ chẳng thèm hỏi đến Ta được cầu xin Ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.313Z",
      "updatedAt": "2025-04-24T04:17:26.313Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "f03525e3-2da5-452a-a712-55da6fddf7af",
      "number": 1,
      "content": "Ta đã cho những kẻ chẳng tìm kiếm Ta được tìm Ta.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.341Z",
      "updatedAt": "2025-04-24T04:17:26.341Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "1767c7f4-344f-422b-bfc9-4d9ade5b2b09",
      "number": 1,
      "content": "Ta nói với một dân chẳng thèm kêu cầu danh Ta rằng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.351Z",
      "updatedAt": "2025-04-24T04:17:26.351Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "bf551576-d971-43a5-9174-aca56d942043",
      "number": 1,
      "content": "‘Ta ở đây! Ta ở đây!’",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.362Z",
      "updatedAt": "2025-04-24T04:17:26.362Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "c517492f-82c1-4284-81e5-6da861469b66",
      "number": 2,
      "content": "Suốt ngày Ta dang hai tay mời gọi một dân phản nghịch,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.375Z",
      "updatedAt": "2025-04-24T04:17:26.375Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "21d52d0d-3e3f-480e-a6a2-3b84975c00be",
      "number": 2,
      "content": "Một dân cứ bước theo đường lối gian tà, theo ý riêng mình,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.387Z",
      "updatedAt": "2025-04-24T04:17:26.387Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "fe1cfc13-f4fc-4d49-bdbf-c1fa17fb9781",
      "number": 3,
      "content": "Một dân không ngừng chọc giận ngay vào mặt Ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.400Z",
      "updatedAt": "2025-04-24T04:17:26.400Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "e100a872-e33f-4a91-8aec-46130494f32e",
      "number": 3,
      "content": "Chúng dâng con vật hiến tế cho tà thần trong các vườn,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.414Z",
      "updatedAt": "2025-04-24T04:17:26.414Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "a58fda3a-9a71-4b37-8f89-fdc0dde94ba4",
      "number": 3,
      "content": "Và dâng hương cho các thần tượng trên những bàn thờ bằng gạch;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.429Z",
      "updatedAt": "2025-04-24T04:17:26.429Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "a26533d7-950e-41d4-a280-06268b7e8467",
      "number": 4,
      "content": "Chúng ngồi trầm tư mặc tưởng giữa các mồ mả,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.442Z",
      "updatedAt": "2025-04-24T04:17:26.442Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "7a67c714-5751-4327-9122-f26206ec48d8",
      "number": 4,
      "content": "Và thức thâu đêm tĩnh tọa nơi mộ địa âm u.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.455Z",
      "updatedAt": "2025-04-24T04:17:26.455Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "8a480bce-1851-4d91-baa8-4240e189462c",
      "number": 4,
      "content": "Chúng ăn thịt heo ô uế;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.468Z",
      "updatedAt": "2025-04-24T04:17:26.468Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "5a2175f3-fce4-40c2-b71e-62c19a85c2c2",
      "number": 4,
      "content": "Bát đĩa chúng chứa đầy các thức ăn gớm ghiếc.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.481Z",
      "updatedAt": "2025-04-24T04:17:26.481Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "dd27a1b4-0548-40ea-b1c4-3dc8bceaf951",
      "number": 5,
      "content": "Chúng là những kẻ đã nói,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.494Z",
      "updatedAt": "2025-04-24T04:17:26.494Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "b2b59023-5dd3-44fa-b460-e53e5acfd7ca",
      "number": 5,
      "content": "‘Hãy đứng xa ra, chớ lại gần ta, vì ta thánh khiết hơn ngươi!’",
      "order": 1,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.508Z",
      "updatedAt": "2025-04-24T04:17:26.508Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "cc7fd611-4334-4821-9f75-c84570e07401",
      "number": 5,
      "content": "Những điều ấy làm cho khói giận của Ta bốc ra nơi lỗ mũi,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.521Z",
      "updatedAt": "2025-04-24T04:17:26.521Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "264eef50-8745-4b3b-a08c-383a2c83fb59",
      "number": 5,
      "content": "Như lửa hừng cháy hừng hực suốt ngày.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.534Z",
      "updatedAt": "2025-04-24T04:17:26.534Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "26fc90f5-667f-492a-ac92-6f2f06bc7fd9",
      "number": 6,
      "content": "Này, những việc ấy đã được ghi lại trước mặt Ta;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.548Z",
      "updatedAt": "2025-04-24T04:17:26.548Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "9a0c5a77-4e54-4446-8c09-501fbeb938af",
      "number": 6,
      "content": "Ta sẽ không im lặng, nhưng sẽ báo trả,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.565Z",
      "updatedAt": "2025-04-24T04:17:26.565Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "f087fc0b-9497-430d-b28a-9343753b1a47",
      "number": 6,
      "content": "Ta sẽ báo trả ngay vào lòng chúng;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.578Z",
      "updatedAt": "2025-04-24T04:17:26.578Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "f5daba4a-8c0b-42aa-9b80-ce9a6b626aae",
      "number": 7,
      "content": "Ta sẽ báo trả tội của các ngươi và tội của tổ tiên các ngươi nữa,”",
      "order": 0,
      "parNumber": 0,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.592Z",
      "updatedAt": "2025-04-24T04:17:26.592Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "def8d98f-f44d-446d-97c4-1dba85f7ba12",
      "number": 7,
      "content": "Chúa phán.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.605Z",
      "updatedAt": "2025-04-24T04:17:26.605Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "8ba26c5a-de42-4510-a79a-7991e43fe645",
      "number": 7,
      "content": "“Chúng đã dâng hương cho các tà thần trên các núi;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.621Z",
      "updatedAt": "2025-04-24T04:17:26.621Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "c87a5425-33e1-41d3-a2f4-fc550cb8aa4a",
      "number": 7,
      "content": "Chúng đã xúc phạm đến Ta trên các ngọn đồi,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 23,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.634Z",
      "updatedAt": "2025-04-24T04:17:26.634Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "2a853e90-f1ce-455c-ae34-a18af245ffb7",
      "number": 7,
      "content": "Nên Ta sẽ lường theo những việc chúng làm khi trước,",
      "order": 4,
      "parNumber": 0,
      "parIndex": 24,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.648Z",
      "updatedAt": "2025-04-24T04:17:26.648Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "d5757404-684f-488a-952c-ddee8a9b36d7",
      "number": 7,
      "content": "Mà báo trả đích đáng vào lòng chúng.”",
      "order": 5,
      "parNumber": 0,
      "parIndex": 25,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.664Z",
      "updatedAt": "2025-04-24T04:17:26.664Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "8c49ca26-ce6a-49be-a025-3b4e9444768f",
      "number": 8,
      "content": "Chúa phán thế này:",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.677Z",
      "updatedAt": "2025-04-24T04:17:26.677Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "ae4526f3-554a-4641-90ac-6358fe2ded3b",
      "number": 8,
      "content": "“Như rượu mới còn tìm thấy trong chùm nho héo,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.690Z",
      "updatedAt": "2025-04-24T04:17:26.690Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "a0b98993-f08a-4cc7-b6c0-2a7f72f048c1",
      "number": 8,
      "content": "Người ta bảo rằng, ‘Ðừng bỏ nó đi, vì trong đó còn tìm thấy chút phước lành,’",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.703Z",
      "updatedAt": "2025-04-24T04:17:26.703Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "b259e668-aa7f-4611-8d0d-7a8ad071f22a",
      "number": 8,
      "content": "Thì vì cớ các tôi tớ Ta, Ta sẽ làm như vậy;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.717Z",
      "updatedAt": "2025-04-24T04:17:26.717Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "b5a8ccfd-f581-48f6-88d7-9471a4d21363",
      "number": 8,
      "content": "Ta sẽ không tiêu diệt hết chúng đâu.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.730Z",
      "updatedAt": "2025-04-24T04:17:26.730Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "e36b1af7-4e71-4cb0-8ba9-c965a4907331",
      "number": 9,
      "content": "Từ Gia-cốp Ta sẽ cho phát sinh một dòng dõi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.743Z",
      "updatedAt": "2025-04-24T04:17:26.743Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "035c0ee7-f30a-4273-af9a-7879ccefcb70",
      "number": 9,
      "content": "Từ Giu-đa sẽ ra một người thừa kế các núi thánh của Ta;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.756Z",
      "updatedAt": "2025-04-24T04:17:26.756Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "29872028-90bd-47e4-8b89-3383e6f44abe",
      "number": 9,
      "content": "Người Ta chọn sẽ thừa hưởng cơ nghiệp đó;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.773Z",
      "updatedAt": "2025-04-24T04:17:26.773Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "c659da1f-5185-4886-a91f-a66e957296e4",
      "number": 9,
      "content": "Các tôi tớ Ta sẽ cư ngụ ở đó.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.789Z",
      "updatedAt": "2025-04-24T04:17:26.789Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "265c61f3-3e71-46fe-ba73-902da3b654ff",
      "number": 10,
      "content": "Sa-rôn sẽ là đồng cỏ cho các bầy súc vật;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.802Z",
      "updatedAt": "2025-04-24T04:17:26.802Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "0d4fa29a-dbd3-4871-970f-d86c159e716e",
      "number": 10,
      "content": "Thung lũng A-cơ sẽ là chỗ cho các đàn bò nằm,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.816Z",
      "updatedAt": "2025-04-24T04:17:26.816Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "5bb2283e-3c81-4727-b590-c5887b485811",
      "number": 10,
      "content": "Vì dân Ta đã tìm kiếm Ta.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.829Z",
      "updatedAt": "2025-04-24T04:17:26.829Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "19371b6d-0227-49f5-8f07-b11bd5757e4e",
      "number": 11,
      "content": "Nhưng hỡi các ngươi là những kẻ bỏ Chúa,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.842Z",
      "updatedAt": "2025-04-24T04:17:26.842Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "43530f04-b9cc-4dca-902e-e174dc1eb775",
      "number": 11,
      "content": "Những kẻ quên núi thánh của Ta,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.855Z",
      "updatedAt": "2025-04-24T04:17:26.855Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "932103e6-fab0-45d1-ab5d-47299044412c",
      "number": 11,
      "content": "Những kẻ lập bàn thờ cho Thần May Mắn,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.869Z",
      "updatedAt": "2025-04-24T04:17:26.869Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "a22981e2-a5d0-42b3-b83b-557b5f64ef06",
      "number": 11,
      "content": "Dâng rượu pha cho Thần Ðịnh Mệnh,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.897Z",
      "updatedAt": "2025-04-24T04:17:26.897Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "4ab0348b-44c0-451f-ae6c-03bdfa11925a",
      "number": 12,
      "content": "Vì thế Ta sẽ định phần số các ngươi cho gươm giáo;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.918Z",
      "updatedAt": "2025-04-24T04:17:26.918Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "8c8d31e5-8bad-446d-ae5d-29687eda2466",
      "number": 12,
      "content": "Tất cả các ngươi sẽ quỳ xuống trước kẻ giết mình,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.929Z",
      "updatedAt": "2025-04-24T04:17:26.929Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "b05a393a-9a86-4a60-b5ed-8d7fb856b233",
      "number": 12,
      "content": "Vì khi Ta gọi, các ngươi chẳng thèm đáp lại;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.939Z",
      "updatedAt": "2025-04-24T04:17:26.939Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "9c742b15-c024-47e6-81fe-75adfc2184b4",
      "number": 12,
      "content": "Khi Ta phán, các ngươi chẳng muốn lắng nghe;",
      "order": 3,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.950Z",
      "updatedAt": "2025-04-24T04:17:26.950Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "fa173bea-2824-4f09-bbad-5bc565c8c7dc",
      "number": 12,
      "content": "Nhưng các ngươi cứ làm điều ác trước mắt Ta,",
      "order": 4,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.961Z",
      "updatedAt": "2025-04-24T04:17:26.961Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "c54ca383-974d-4342-aa7a-4920aa074902",
      "number": 12,
      "content": "Và cứ chọn làm những điều Ta không thích.”",
      "order": 5,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.972Z",
      "updatedAt": "2025-04-24T04:17:26.972Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "be1a50fa-f707-4870-85be-3e58c7f715c5",
      "number": 13,
      "content": "Vì thế Chúa Hằng Hữu phán thế này:",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.982Z",
      "updatedAt": "2025-04-24T04:17:26.982Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "d520dd8e-2bcb-46cd-bf91-7391ebb685e1",
      "number": 13,
      "content": "“Này, các tôi tớ Ta sẽ ăn, nhưng các ngươi sẽ đói;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:26.993Z",
      "updatedAt": "2025-04-24T04:17:26.993Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "ac298a5c-1ef3-4617-b1bc-384817c95ef2",
      "number": 13,
      "content": "Này, các tôi tớ Ta sẽ uống, nhưng các ngươi sẽ khát;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.005Z",
      "updatedAt": "2025-04-24T04:17:27.005Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "ac193d8b-d26e-4bda-b97d-7189c0e00d0c",
      "number": 13,
      "content": "Này, các tôi tớ Ta sẽ vui mừng, nhưng các ngươi sẽ tủi hổ;",
      "order": 3,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.018Z",
      "updatedAt": "2025-04-24T04:17:27.018Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "1dc69a7d-514b-4144-805d-844eac10e4b6",
      "number": 14,
      "content": "Này, các tôi tớ Ta sẽ ca hát vì tâm hồn vui vẻ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.032Z",
      "updatedAt": "2025-04-24T04:17:27.032Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "2e550aef-8c88-4aa6-a1d7-612fab0b9f68",
      "number": 14,
      "content": "Nhưng các ngươi sẽ khóc than vì sầu thảm trong lòng,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.043Z",
      "updatedAt": "2025-04-24T04:17:27.043Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "0762ac21-8302-4bdb-bc9d-3cd52fbe8024",
      "number": 14,
      "content": "Và các ngươi sẽ khóc lóc vì tâm linh đau đớn.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.055Z",
      "updatedAt": "2025-04-24T04:17:27.055Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "fa4e39a9-3756-426b-9ec0-1adb293cf455",
      "number": 15,
      "content": "Các ngươi cũng sẽ được lưu danh,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.065Z",
      "updatedAt": "2025-04-24T04:17:27.065Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "7420dcfc-16fc-4b3d-82b5-1269d0c24943",
      "number": 15,
      "content": "Nhưng chỉ để làm lời nguyền rủa cho những kẻ Ta chọn,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.075Z",
      "updatedAt": "2025-04-24T04:17:27.075Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "31b04c59-f865-4347-a12d-0f52e38b80aa",
      "number": 15,
      "content": "Vì Chúa Hằng Hữu sẽ tiêu diệt ngươi,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.085Z",
      "updatedAt": "2025-04-24T04:17:27.085Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "e66ee024-81fa-4e21-9ed0-05be12264d16",
      "number": 15,
      "content": "Nhưng các tôi tớ Ngài, Ngài sẽ gọi họ bằng một danh khác.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.096Z",
      "updatedAt": "2025-04-24T04:17:27.096Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "3d34e5b5-5436-49e8-864d-449581ef8bbd",
      "number": 16,
      "content": "Vì vậy, ở trong xứ, ai cầu phúc cho mình sẽ nhân danh Ðức Chúa Trời của sự thật mà cầu phúc;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.108Z",
      "updatedAt": "2025-04-24T04:17:27.108Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "b1b5b373-2a37-40b2-b830-0fa159411314",
      "number": 16,
      "content": "Ở trong xứ, ai tuyên thệ sẽ nhân danh Ðức Chúa Trời của sự thật mà thề,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.121Z",
      "updatedAt": "2025-04-24T04:17:27.121Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "e56f7fd9-eed7-4237-a31c-0a4d7bce5a61",
      "number": 16,
      "content": "Vì những gian truân khi trước đã bị quên rồi,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.137Z",
      "updatedAt": "2025-04-24T04:17:27.137Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "3df06e93-eded-483e-9ef4-2389cc553b49",
      "number": 16,
      "content": "Và vì chúng đã bị che khuất khỏi mắt Ta rồi.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.150Z",
      "updatedAt": "2025-04-24T04:17:27.150Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "1437c19b-7130-4f45-8d09-59f748d42205",
      "number": 17,
      "content": "Vì này, Ta sẽ dựng nên trời mới và đất mới;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.163Z",
      "updatedAt": "2025-04-24T04:17:27.163Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "62eebefd-297c-4521-9b6e-a0a8c452999e",
      "number": 17,
      "content": "Những gì trước kia sẽ không còn được nhớ đến nữa,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.192Z",
      "updatedAt": "2025-04-24T04:17:27.192Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "0693f13d-6282-49ec-9539-48e5fddd79fc",
      "number": 17,
      "content": "Và cũng không còn đến trong trí nữa.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.205Z",
      "updatedAt": "2025-04-24T04:17:27.205Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "29338d3f-597f-498f-a2d0-959c6b83dcf7",
      "number": 18,
      "content": "Nhưng hãy hân hoan và vui mừng mãi mãi về những gì Ta tạo dựng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.218Z",
      "updatedAt": "2025-04-24T04:17:27.218Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "7470926c-7d4a-4043-b42f-cb5f2a5fbc99",
      "number": 18,
      "content": "Vì này, Ta dựng nên Giê-ru-sa-lem để làm một niềm vui;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.231Z",
      "updatedAt": "2025-04-24T04:17:27.231Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "b4f5866e-1cba-4ac1-9735-9f0df1d8e995",
      "number": 18,
      "content": "Và dân thành ấy sẽ là một niềm vui thỏa.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.243Z",
      "updatedAt": "2025-04-24T04:17:27.243Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "9d1c729f-e296-4f20-b829-85f8ea86438f",
      "number": 19,
      "content": "Ta sẽ vui vẻ vì Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.254Z",
      "updatedAt": "2025-04-24T04:17:27.254Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "b6319a9f-2ddc-49a4-b78f-eb47910d7d02",
      "number": 19,
      "content": "Ta sẽ vui mừng vì dân Ta;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.264Z",
      "updatedAt": "2025-04-24T04:17:27.264Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "7828c955-aeda-434f-9e80-65bdf93df49d",
      "number": 19,
      "content": "Tiếng than khóc sẽ không còn nghe trong thành ấy;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.278Z",
      "updatedAt": "2025-04-24T04:17:27.278Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "8c8cf810-9583-49aa-8c08-02ceb538d306",
      "number": 19,
      "content": "Tiếng khóc than sẽ vĩnh viễn không còn.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.290Z",
      "updatedAt": "2025-04-24T04:17:27.290Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "61e3f11f-8b9e-49c2-a4e5-ac56b90f8213",
      "number": 20,
      "content": "Trong thành ấy trẻ thơ sẽ không còn chết yểu;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.302Z",
      "updatedAt": "2025-04-24T04:17:27.302Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "cf55827e-6027-4a8a-9c93-f41c2cedeb74",
      "number": 20,
      "content": "Không người già nào chẳng hưởng trọn tuổi thọ của mình;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.316Z",
      "updatedAt": "2025-04-24T04:17:27.316Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "4372da80-e9d0-41d1-ab1b-8e7bdbaf9b0d",
      "number": 20,
      "content": "Vì ai trăm tuổi mà qua đời sẽ bị xem là chết trẻ;",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.325Z",
      "updatedAt": "2025-04-24T04:17:27.325Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "0495d0e8-6774-4916-aecf-e791cbe5552d",
      "number": 20,
      "content": "Người chưa được bách niên mà thất lộc sẽ xem như bị rủa nguyền.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.336Z",
      "updatedAt": "2025-04-24T04:17:27.336Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "9f1ff65d-a211-46b9-83c1-3dc62bf18f5e",
      "number": 21,
      "content": "Chúng sẽ xây nhà và an cư trong các nhà đó;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.345Z",
      "updatedAt": "2025-04-24T04:17:27.345Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "91c8f6a5-342c-4260-9c61-835ca64a643c",
      "number": 21,
      "content": "Chúng sẽ trồng vườn nho và được ăn trái mình trồng.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.358Z",
      "updatedAt": "2025-04-24T04:17:27.358Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "225cbb08-e3ab-4020-890a-ec255482a0e5",
      "number": 22,
      "content": "Chúng sẽ không xây nhà để bị người khác vào chiếm ở;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.371Z",
      "updatedAt": "2025-04-24T04:17:27.371Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "f782775c-c66a-47f3-853e-070d7cf6f857",
      "number": 22,
      "content": "Chúng sẽ không trồng cây để bị người khác đến chiếm ăn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.386Z",
      "updatedAt": "2025-04-24T04:17:27.386Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "cb9de9b0-d211-4857-a350-0303b8b740e8",
      "number": 22,
      "content": "Vì cây cối sống bao nhiêu tuổi, dân Ta cũng sẽ thọ bấy nhiêu.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.399Z",
      "updatedAt": "2025-04-24T04:17:27.399Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "eda01e4c-2d8e-42af-af26-e76a71d4a085",
      "number": 22,
      "content": "Những kẻ Ta chọn sẽ hưởng lâu dài mọi kết quả do tay mình làm ra.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.409Z",
      "updatedAt": "2025-04-24T04:17:27.409Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "3b5cf0d0-aba4-4949-946f-bd53fffe6adc",
      "number": 23,
      "content": "Chúng sẽ không làm lụng cách luống công vô ích;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.419Z",
      "updatedAt": "2025-04-24T04:17:27.419Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "8a4ced6f-e9f9-4af2-96fd-3bcae1b171f6",
      "number": 23,
      "content": "Chúng sinh con ra chẳng phải để chuốc lấy âu sầu,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.428Z",
      "updatedAt": "2025-04-24T04:17:27.428Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "6fb10ddc-6a64-4b1a-b699-7b648b3309e8",
      "number": 23,
      "content": "Vì con cháu chúng sẽ là những người được Chúa ban phước,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.438Z",
      "updatedAt": "2025-04-24T04:17:27.438Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "6cf66853-3c68-4615-be66-299d58067ac9",
      "number": 23,
      "content": "Và dòng dõi chúng cùng với chúng nữa.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.448Z",
      "updatedAt": "2025-04-24T04:17:27.448Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "7374df50-6e69-4e5a-8f77-24e397e70a53",
      "number": 24,
      "content": "Rồi đây, trước khi chúng cầu xin, Ta đã trả lời;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.458Z",
      "updatedAt": "2025-04-24T04:17:27.458Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "19629c07-abf6-4dcf-8542-5245f326e177",
      "number": 24,
      "content": "Khi chúng còn đang nói, Ta đã nghe rõ hết.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.472Z",
      "updatedAt": "2025-04-24T04:17:27.472Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "2d136a87-a727-4b98-8b13-978ba7b8ef1d",
      "number": 25,
      "content": "Chó sói và chiên con sẽ ăn cỏ với nhau,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.488Z",
      "updatedAt": "2025-04-24T04:17:27.488Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "882435d8-92f5-4beb-8ec1-0ba039b3625b",
      "number": 25,
      "content": "Sư tử sẽ ăn rơm rạ như bò,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.501Z",
      "updatedAt": "2025-04-24T04:17:27.501Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "2c662053-9fd7-4946-a5dc-4496552ac069",
      "number": 25,
      "content": "Còn bụi đất sẽ là thức ăn của loài rắn;",
      "order": 2,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.512Z",
      "updatedAt": "2025-04-24T04:17:27.512Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "d4282b6c-6f22-4b74-982d-abccbd0471bc",
      "number": 25,
      "content": "Chúng sẽ không gây đau thương hay hủy diệt trong khắp núi thánh của Ta,”",
      "order": 3,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.522Z",
      "updatedAt": "2025-04-24T04:17:27.522Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    },
    {
      "id": "bbcaae6f-aff7-4748-85f5-189542e57f78",
      "number": 25,
      "content": "Chúa phán.",
      "order": 4,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:17:27.531Z",
      "updatedAt": "2025-04-24T04:17:27.531Z",
      "chapterId": "99a1a13c-ee22-48d2-9b40-5a7432a18b39"
    }
  ],
