    },
    {
      "id": "fe058204-554e-409c-8547-099c9c206600",
      "number": 12,
      "content": "Chúa ôi, lẽ nào Ngài đành lòng để mặc chúng con chịu mãi cảnh trạng như thế sao?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:48:31.583Z",
      "updatedAt": "2025-04-22T08:48:31.583Z",
