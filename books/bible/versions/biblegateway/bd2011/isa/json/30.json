    }
  ],
  "headings": [
    {
      "id": "8df82f4d-df53-4ba1-b7bb-077133a9305d",
      "content": "Quở Trách vì Cậy Vào Ai-cập",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T04:13:51.460Z",
      "updatedAt": "2025-04-24T04:13:51.460Z",
      "verseId": "2d3cc50b-dae3-4c79-8da9-ed8ac0c1e0b5",
      "chapterId": "aebeab57-a6f7-49bd-a63e-6b80ebef640f"
