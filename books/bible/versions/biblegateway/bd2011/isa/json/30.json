    },
    {
      "id": "b9040b86-3be1-4d7a-a96f-8d659cd6eca7",
      "content": "ctd: lò thiêu người của Tô-phết",
      "position": 15,
      "order": 1,
      "createdAt": "2025-04-22T08:45:00.818Z",
      "updatedAt": "2025-04-22T08:45:00.818Z",
      "verseId": "c7152a5a-dca3-493c-9191-b1925f8deffe",
