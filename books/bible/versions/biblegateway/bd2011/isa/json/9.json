{
  "chapter": {
    "id": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911",
    "number": 9,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%209&version=BD2011",
    "createdAt": "2025-04-24T03:03:20.371Z",
    "updatedAt": "2025-04-24T03:03:20.371Z",
    "bookId": "e4e58f0e-ae57-420f-8009-a5608bc5a58a"
  },
  "verses": [
    {
      "id": "704b79e7-b71b-4577-a7ed-0be8e9e55e2b",
      "number": 1,
      "content": "Tuy nhiên sẽ không còn sầu thảm cho những kẻ bị hoạn nạn nữa.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:41.989Z",
      "updatedAt": "2025-04-24T04:11:41.989Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "78f4f693-2ae9-4546-a216-1950d2fb03de",
      "number": 1,
      "content": "Trong quá khứ Ngài đã hạ xứ Xê-bu-lun và xứ Náp-ta-li xuống,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.009Z",
      "updatedAt": "2025-04-24T04:11:42.009Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "58adc4d3-3834-4b71-8460-e6483e26e162",
      "number": 1,
      "content": "Nhưng trong tương lai Ngài sẽ làm rạng rỡ con đường nằm ven biển,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.023Z",
      "updatedAt": "2025-04-24T04:11:42.023Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "14f9cb0b-4626-4525-97ad-6f1c15fc0f9b",
      "number": 1,
      "content": "Tức miền bên kia Sông Giô-đanh,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.034Z",
      "updatedAt": "2025-04-24T04:11:42.034Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "64c785e7-7bc6-43ff-a617-c5f25ee3cd02",
      "number": 1,
      "content": "Thuộc vùng Ga-li-lê của các dân ngoại.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.048Z",
      "updatedAt": "2025-04-24T04:11:42.048Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "e96efc63-211a-4515-a7a2-9fde07d3efc0",
      "number": 2,
      "content": "Dân đi trong bóng tối sẽ thấy ánh sáng lớn;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.060Z",
      "updatedAt": "2025-04-24T04:11:42.060Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "694b4f8a-4102-4793-aeb5-2017afd5e25b",
      "number": 2,
      "content": "Ðối với những kẻ sống trong xứ của bóng tử thần,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.082Z",
      "updatedAt": "2025-04-24T04:11:42.082Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "56840d43-96bc-43ea-9d99-bb273b4e0392",
      "number": 2,
      "content": "Một vầng chân quang đã bừng lên chiếu sáng trên họ.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.098Z",
      "updatedAt": "2025-04-24T04:11:42.098Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "225dfe19-9c13-4c85-aab9-40a8bc1cdacc",
      "number": 3,
      "content": "Ngài đã làm cho đất nước này được tăng trưởng bội phần;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.110Z",
      "updatedAt": "2025-04-24T04:11:42.110Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "0b5a009c-b6c6-4825-900a-3cf944035e81",
      "number": 3,
      "content": "Ngài đã cho niềm vui của họ gia tăng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.137Z",
      "updatedAt": "2025-04-24T04:11:42.137Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "e41ed154-3404-443f-b06d-4ed1331dc3ff",
      "number": 3,
      "content": "Họ vui mừng trước mặt Ngài, như vui trong mùa gặt, như vui khi chia nhau chiến lợi phẩm,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.150Z",
      "updatedAt": "2025-04-24T04:11:42.150Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "a22b91b5-c919-4cf2-bd34-c2dbe4ab0107",
      "number": 4,
      "content": "Vì cái ách trên cổ họ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.164Z",
      "updatedAt": "2025-04-24T04:11:42.164Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "5d1cd0ec-ae0c-4618-ab2f-750dab352064",
      "number": 4,
      "content": "Ðòn gánh trên vai họ,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.182Z",
      "updatedAt": "2025-04-24T04:11:42.182Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "ff59061a-9f2c-4604-bc53-e64b4108ecad",
      "number": 4,
      "content": "Và cây roi của kẻ áp bức họ",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.195Z",
      "updatedAt": "2025-04-24T04:11:42.195Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "90fa1140-ee19-49e7-b7e7-ccd6b9a335bd",
      "number": 4,
      "content": "Ðã bị Ngài bẻ gãy như trong ngày Mi-đi-an bị đánh bại.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.207Z",
      "updatedAt": "2025-04-24T04:11:42.207Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "8381a9cf-7fa3-4e31-8f72-e4082002a257",
      "number": 5,
      "content": "Vì mọi giày trận mang ra chiến trường,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.222Z",
      "updatedAt": "2025-04-24T04:11:42.222Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "b65ddb6b-b751-41b2-b64e-04dd415cac2d",
      "number": 5,
      "content": "Và mọi quân phục đẫm máu quân thù,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.233Z",
      "updatedAt": "2025-04-24T04:11:42.233Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "b37eb0c7-2403-464e-a60e-ca636d8055cd",
      "number": 5,
      "content": "Ðều sẽ bị đem ra thiêu hủy như nhiên liệu được dùng để đốt.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.244Z",
      "updatedAt": "2025-04-24T04:11:42.244Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "e00506ed-5ab8-4570-b393-1ff31b3aea06",
      "number": 6,
      "content": "Vì sẽ có một Con Trẻ sinh cho chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.256Z",
      "updatedAt": "2025-04-24T04:11:42.256Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "282beb1e-371f-4863-883e-9aaf19f5522e",
      "number": 6,
      "content": "Một Con Trai ban cho chúng ta;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.267Z",
      "updatedAt": "2025-04-24T04:11:42.267Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "70d2ca2a-f93a-476f-95c8-036c06d65851",
      "number": 6,
      "content": "Quyền trị vì sẽ đặt trên vai Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.277Z",
      "updatedAt": "2025-04-24T04:11:42.277Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "460ded1a-99e4-4dbd-91fe-dff1755f0a06",
      "number": 6,
      "content": "Ngài sẽ được xưng là",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.287Z",
      "updatedAt": "2025-04-24T04:11:42.287Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "a2905d38-b7e6-4c9b-993b-7a1fc781de37",
      "number": 6,
      "content": "Ðấng Mưu Luận Lạ Lùng,",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.297Z",
      "updatedAt": "2025-04-24T04:11:42.297Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "2043b60a-004e-43d8-beef-d5cb141c372a",
      "number": 6,
      "content": "Ðức Chúa Trời Quyền Năng,",
      "order": 5,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.307Z",
      "updatedAt": "2025-04-24T04:11:42.307Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "50ad47b2-c1fa-483b-b031-d41adf112934",
      "number": 6,
      "content": "Cha Ðời Ðời,",
      "order": 6,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.318Z",
      "updatedAt": "2025-04-24T04:11:42.318Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "8c86704b-be48-44e2-8696-df43d1a321d6",
      "number": 6,
      "content": "Chúa Bình An.",
      "order": 7,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.328Z",
      "updatedAt": "2025-04-24T04:11:42.328Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "77414436-f635-4272-a5d3-d72af20585ac",
      "number": 7,
      "content": "Quyền trị vì của Ngài sẽ thêm mãi không ngừng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.339Z",
      "updatedAt": "2025-04-24T04:11:42.339Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "86cfcb94-86ee-4b33-bbba-ce15dfdd9581",
      "number": 7,
      "content": "Và hòa bình sẽ kéo dài vô tận",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.351Z",
      "updatedAt": "2025-04-24T04:11:42.351Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "d276775d-8b2b-4173-aa9c-6f6c8c5e3c96",
      "number": 7,
      "content": "trên ngai của Ða-vít và trong vương quốc của Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.363Z",
      "updatedAt": "2025-04-24T04:11:42.363Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "dbbe780a-264f-413b-a09b-db7fab410914",
      "number": 7,
      "content": "Ngài sẽ thiết lập và duy trì quyền trị vì của Ngài bằng công lý và công chính,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.376Z",
      "updatedAt": "2025-04-24T04:11:42.376Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "bb9a53b0-f7fc-49a3-ae35-0b404a10832b",
      "number": 7,
      "content": "Từ nay cho đến đời đời.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.388Z",
      "updatedAt": "2025-04-24T04:11:42.388Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "94dd92a9-b6b0-40db-9bfb-6e5b9644a9d2",
      "number": 7,
      "content": "Nguyện lòng nhiệt thành của Chúa các đạo quân sẽ thực hiện điều ấy.",
      "order": 5,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.401Z",
      "updatedAt": "2025-04-24T04:11:42.401Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "ab499f5f-2704-485a-82fc-ab7f80dad2ca",
      "number": 8,
      "content": "Chúa đã truyền một sứ điệp nghịch lại Gia-cốp;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.414Z",
      "updatedAt": "2025-04-24T04:11:42.414Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "2c422102-4ecf-44bf-981e-ac1c86a783b4",
      "number": 8,
      "content": "Sứ điệp ấy bao gồm toàn thể I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.442Z",
      "updatedAt": "2025-04-24T04:11:42.442Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "2bb7c20a-9450-4bcf-a7d3-d314d4e01edb",
      "number": 9,
      "content": "Toàn dân sẽ biết đến,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.462Z",
      "updatedAt": "2025-04-24T04:11:42.462Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "068f06cd-b2de-42b5-aaa6-ef3d138096e5",
      "number": 9,
      "content": "Tức mọi người ở Ép-ra-im và dân chúng ở Sa-ma-ri.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.480Z",
      "updatedAt": "2025-04-24T04:11:42.480Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "5527bd92-4653-4506-9dcd-dc4920897cbe",
      "number": 9,
      "content": "Với lòng đầy kiêu căng và ngạo mạn, họ nói rằng,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.497Z",
      "updatedAt": "2025-04-24T04:11:42.497Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "45cf5b78-1ba0-4513-9644-5940a9f3a165",
      "number": 10,
      "content": "“Gạch đã đổ xuống, ta sẽ xây lại bằng đá khối;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.517Z",
      "updatedAt": "2025-04-24T04:11:42.517Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "0f9e91c1-b5f5-4cd3-98da-d94048564c63",
      "number": 10,
      "content": "Những cây sung đã bị đốn hạ, ta sẽ thay thế bằng những cây tùng.”",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.538Z",
      "updatedAt": "2025-04-24T04:11:42.538Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "605969b5-f32e-4472-a9d3-06a62b55da3c",
      "number": 11,
      "content": "Nhưng Chúa sẽ khiến những kẻ thù của Rê-xin chống lại họ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.551Z",
      "updatedAt": "2025-04-24T04:11:42.551Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "56ee1a99-cbf0-4ed2-b65a-333e1cf92b4a",
      "number": 11,
      "content": "Ngài khích động quân thù của họ nổi lên.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.570Z",
      "updatedAt": "2025-04-24T04:11:42.570Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "6b999e6e-1adc-4de1-8f87-fee0e83dced5",
      "number": 12,
      "content": "Phía đông có A-ram, phía tây có Phi-li-tin;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.581Z",
      "updatedAt": "2025-04-24T04:11:42.581Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "b32b6be1-5c30-47e7-b1ba-b83eac1f467c",
      "number": 12,
      "content": "Chúng hả mồm nuốt lấy I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.592Z",
      "updatedAt": "2025-04-24T04:11:42.592Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "bf5d8c3c-b130-44c0-a222-459b6568d3d3",
      "number": 12,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.604Z",
      "updatedAt": "2025-04-24T04:11:42.604Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "520545ba-ce23-4aa5-8e78-946f5a1d36d0",
      "number": 12,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.619Z",
      "updatedAt": "2025-04-24T04:11:42.619Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "60d0c438-56b8-4142-8598-fcc8d29c940e",
      "number": 13,
      "content": "Thế mà con dân Ngài vẫn không quay về với Ðấng đánh phạt mình;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.634Z",
      "updatedAt": "2025-04-24T04:11:42.634Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "56c5913d-8ade-4ead-9f55-67bb3cb485b5",
      "number": 13,
      "content": "Họ không tìm kiếm Chúa của các đạo quân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.649Z",
      "updatedAt": "2025-04-24T04:11:42.649Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "0bc10303-0fd8-4e35-99a6-4df6e730c213",
      "number": 14,
      "content": "Vì vậy Chúa sẽ chặt đầu chặt đuôi của I-sơ-ra-ên;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.663Z",
      "updatedAt": "2025-04-24T04:11:42.663Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "cb2ce2b0-f833-46e6-95a7-d6857ca75da7",
      "number": 14,
      "content": "Trong một ngày, cả cành cây chà là và cây sậy đều bị chặt.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.676Z",
      "updatedAt": "2025-04-24T04:11:42.676Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "ab0af1c4-4ad4-4887-a587-0bc0b97d3da9",
      "number": 15,
      "content": "Các trưởng lão và các người quyền quý của họ là đầu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.687Z",
      "updatedAt": "2025-04-24T04:11:42.687Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "255e9dee-4ea0-4d7e-8e70-b7cf431ffee6",
      "number": 15,
      "content": "Các tiên tri dạy dỗ những điều dối trá là đuôi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.698Z",
      "updatedAt": "2025-04-24T04:11:42.698Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "4c492da5-53f4-49b0-a1b2-a19517d73cb3",
      "number": 16,
      "content": "Những kẻ lãnh đạo dân này đã dẫn họ đi lạc hướng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.708Z",
      "updatedAt": "2025-04-24T04:11:42.708Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "7d8e1a08-4f61-4168-adf7-ec7ebe12b5fc",
      "number": 16,
      "content": "Những kẻ dẫn dắt dân này đã đưa họ đến chỗ diệt vong.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.719Z",
      "updatedAt": "2025-04-24T04:11:42.719Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "4ab3f232-1118-4aef-8407-f0b554d830f2",
      "number": 17,
      "content": "Vì thế Chúa chẳng thương tiếc các trai tráng của họ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.730Z",
      "updatedAt": "2025-04-24T04:11:42.730Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "218ae351-46a6-4954-b801-934634c47913",
      "number": 17,
      "content": "Ngài cũng chẳng đoái hoài các cô nhi quả phụ của họ,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.751Z",
      "updatedAt": "2025-04-24T04:11:42.751Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "1b792eee-0e14-4738-889f-8be4f8fb878e",
      "number": 17,
      "content": "Bởi ai nấy đều gian manh và tà ác;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.761Z",
      "updatedAt": "2025-04-24T04:11:42.761Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "f09b5372-ec35-4f93-ab41-5438edf6c6ba",
      "number": 17,
      "content": "Mọi miệng đều nói những lời rồ dại ngông cuồng.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.775Z",
      "updatedAt": "2025-04-24T04:11:42.775Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "e8afb0f3-62d8-4565-b090-b0b34b91fce5",
      "number": 17,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 4,
      "parNumber": 4,
      "parIndex": 23,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.784Z",
      "updatedAt": "2025-04-24T04:11:42.784Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "36c2e9b7-c673-4f1c-8762-e4dfa7cb21db",
      "number": 17,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 5,
      "parNumber": 4,
      "parIndex": 24,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.799Z",
      "updatedAt": "2025-04-24T04:11:42.799Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "d2270ad4-5527-4f0b-bb8b-ef7b208b4faf",
      "number": 18,
      "content": "Sự gian ác bùng lên như lửa cháy;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.811Z",
      "updatedAt": "2025-04-24T04:11:42.811Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "d118d988-7255-42c9-b81d-085f1f6d8281",
      "number": 18,
      "content": "Nó đốt thiêu gai góc và bụi gai;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.823Z",
      "updatedAt": "2025-04-24T04:11:42.823Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "e1147bf8-aa80-4233-a208-15687c268525",
      "number": 18,
      "content": "Nó thiêu sạch các bụi rậm trong rừng,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.840Z",
      "updatedAt": "2025-04-24T04:11:42.840Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "c5e59633-602d-414e-bb0d-e099b15c2090",
      "number": 18,
      "content": "Rồi bốc lên thành một luồng khói đen mù mịt.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.850Z",
      "updatedAt": "2025-04-24T04:11:42.850Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "77997f2c-50e6-4d1b-b763-2199423d36d2",
      "number": 19,
      "content": "Vì Chúa các đạo quân nổi cơn thịnh nộ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.860Z",
      "updatedAt": "2025-04-24T04:11:42.860Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "7078fe7e-d4d7-4283-be12-a3adbcda1294",
      "number": 19,
      "content": "Nên đất nước này bị lửa cháy ngập trời,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.873Z",
      "updatedAt": "2025-04-24T04:11:42.873Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "5ded6840-6fe2-48f5-b864-411f284a6ffd",
      "number": 19,
      "content": "Còn dân chúng trở thành mồi cho lửa hừng thiêu đốt,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.883Z",
      "updatedAt": "2025-04-24T04:11:42.883Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "809b1174-cd57-471b-badd-cb684c319d20",
      "number": 19,
      "content": "Ðến nỗi anh chị em cũng chẳng thương tiếc nhau.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.895Z",
      "updatedAt": "2025-04-24T04:11:42.895Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "2b70e3eb-4b0b-40f8-91e4-88b86d401ac2",
      "number": 20,
      "content": "Chúng chộp giật bên phải, nhưng bụng vẫn đói;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.918Z",
      "updatedAt": "2025-04-24T04:11:42.918Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "b0e597bd-0bcf-4d63-9e9d-56c38c5ec126",
      "number": 20,
      "content": "Chúng ngoạm lấy bên trái, nhưng bụng vẫn không no;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.933Z",
      "updatedAt": "2025-04-24T04:11:42.933Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "369879d1-2bd9-4b68-a77f-4134418ef290",
      "number": 20,
      "content": "Thậm chí chúng ăn thịt luôn cả người thân của mình!",
      "order": 2,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.949Z",
      "updatedAt": "2025-04-24T04:11:42.949Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "8a03ed94-a3d8-467b-aaeb-10993e63e9b9",
      "number": 21,
      "content": "Ma-na-se ăn nuốt Ép-ra-im;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.976Z",
      "updatedAt": "2025-04-24T04:11:42.976Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "94912353-e871-4118-a462-f82745d1de64",
      "number": 21,
      "content": "Ép-ra-im ăn nuốt Ma-na-se;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:42.995Z",
      "updatedAt": "2025-04-24T04:11:42.995Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "c5876998-df50-4e44-b263-b7f4ddbfe16b",
      "number": 21,
      "content": "Rồi cả hai cùng đánh Giu-đa.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:43.010Z",
      "updatedAt": "2025-04-24T04:11:43.010Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "10bad412-d3ea-4c13-b53c-b4fafa0765a9",
      "number": 21,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 3,
      "parNumber": 5,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:43.022Z",
      "updatedAt": "2025-04-24T04:11:43.022Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    },
    {
      "id": "111f01e0-2a98-40de-8268-9c920996a850",
      "number": 21,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:11:43.034Z",
      "updatedAt": "2025-04-24T04:11:43.034Z",
      "chapterId": "e3a6d7f9-99a6-4bac-8a4e-88f0ca77d911"
    }
  ],
