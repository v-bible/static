  "headings": [
    {
      "id": "3bc9a1d5-8e3d-4483-ae47-cbae16d2ee92",
      "content": "Cơn Giận của Chúa đối với Những Kẻ Lãnh Ðạo Gian Ác",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T04:11:42.426Z",
