{
  "chapter": {
    "id": "5a08495d-8caa-4556-9d82-48b12a4cc8d0",
    "number": 46,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2046&version=BD2011",
    "createdAt": "2025-04-24T03:03:21.289Z",
    "updatedAt": "2025-04-24T03:03:21.289Z",
    "bookId": "e4e58f0e-ae57-420f-8009-a5608bc5a58a"
  },
  "verses": [
    {
      "id": "c6102a1e-1cca-4039-854a-da5e5afb25a8",
      "number": 1,
      "content": "Thần Bên cúi xuống, Thần Nê-bô rạp mình;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.021Z",
      "updatedAt": "2025-04-24T04:15:27.021Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "6a6925e2-6b33-496f-98a3-82368f76769e",
      "number": 1,
      "content": "Các thần tượng của chúng được chở đi bằng các xe do thú vật và trâu bò kéo;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.052Z",
      "updatedAt": "2025-04-24T04:15:27.052Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "f9705f60-650d-448c-806b-e6b1add552d8",
      "number": 1,
      "content": "Các thần tượng các ngươi mang theo đã trở thành gánh nặng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.062Z",
      "updatedAt": "2025-04-24T04:15:27.062Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "900e0b14-e1d2-441f-8d2c-6f33ec40e505",
      "number": 1,
      "content": "Một trọng tải nặng nề khiến các thú vật kéo xe mệt nhọc.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.073Z",
      "updatedAt": "2025-04-24T04:15:27.073Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "63dacb99-34d4-47e1-95d0-248362d0088d",
      "number": 2,
      "content": "Các thần tượng ấy cúi rạp xuống;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.089Z",
      "updatedAt": "2025-04-24T04:15:27.089Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "c23dfb0a-12fd-4cae-ac34-c1dbcd3e7cfc",
      "number": 2,
      "content": "Chúng thảy đều gục xuống;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.102Z",
      "updatedAt": "2025-04-24T04:15:27.102Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "ab45e384-8131-49e6-ab40-3a0b96ee2290",
      "number": 2,
      "content": "Chúng chẳng cứu nổi các súc vật đang kéo nặng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.115Z",
      "updatedAt": "2025-04-24T04:15:27.115Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "9e3c0ea3-fc9e-4e81-9639-c64e11d80e2c",
      "number": 2,
      "content": "Vì chính bản thân chúng cũng bị bắt đem đi lưu đày.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.129Z",
      "updatedAt": "2025-04-24T04:15:27.129Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "3a718bfd-43aa-4516-ae1f-12552383a02f",
      "number": 3,
      "content": "Hỡi nhà Gia-cốp,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.142Z",
      "updatedAt": "2025-04-24T04:15:27.142Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "f35ac72a-e40f-43f6-ac36-f8cdf9072abd",
      "number": 3,
      "content": "Hỡi những kẻ còn sót lại của nhà I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.156Z",
      "updatedAt": "2025-04-24T04:15:27.156Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "755b53aa-4392-4c00-a290-750e6e34ab19",
      "number": 3,
      "content": "Hãy lắng nghe Ta:",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.169Z",
      "updatedAt": "2025-04-24T04:15:27.169Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "112b67b3-d01f-4ed4-809b-3a002c8fc72a",
      "number": 3,
      "content": "Ta đã bồng ẵm các ngươi từ lúc các ngươi mới chào đời;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.182Z",
      "updatedAt": "2025-04-24T04:15:27.182Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "e02ee1c4-75bd-441a-a054-819a4a216562",
      "number": 3,
      "content": "Ta đã cưu mang các ngươi từ khi các ngươi còn trong lòng mẹ,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.196Z",
      "updatedAt": "2025-04-24T04:15:27.196Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "1bd60a04-5ba5-4f51-a0ac-275ed3607846",
      "number": 4,
      "content": "Cho đến khi các ngươi già nua cao tuổi, Ta vẫn là Ðấng ấy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.209Z",
      "updatedAt": "2025-04-24T04:15:27.209Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "63ec42a7-f991-4eac-aa17-012eb2e8ef23",
      "number": 4,
      "content": "Dầu cho các ngươi đã ra da mồi tóc bạc, Ta sẽ còn bồng ẵm các ngươi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.222Z",
      "updatedAt": "2025-04-24T04:15:27.222Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "c3abe0e5-2226-4c1e-a849-f12efc927e1e",
      "number": 4,
      "content": "Ta đã tạo ra, nên Ta sẽ bồng ẵm;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.237Z",
      "updatedAt": "2025-04-24T04:15:27.237Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "716a815b-6ad9-47cf-a636-5ef30a655e70",
      "number": 4,
      "content": "Ta sẽ cưu mang và sẽ giải cứu các ngươi.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.250Z",
      "updatedAt": "2025-04-24T04:15:27.250Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "b80bdb86-189f-419a-8021-cc52024d4d54",
      "number": 5,
      "content": "Các ngươi sẽ ví sánh ai với Ta và cho ai được ngang hàng với Ta?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.264Z",
      "updatedAt": "2025-04-24T04:15:27.264Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "707def49-7b51-4691-9121-665a37f2c80b",
      "number": 5,
      "content": "Các ngươi so sánh ai với Ta như thể kẻ đó và Ta có điều gì tương xứng?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.277Z",
      "updatedAt": "2025-04-24T04:15:27.277Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "ee2eab2a-64d0-4a4e-b478-a1895a8c5948",
      "number": 6,
      "content": "Chúng lấy vàng trong túi ra;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.292Z",
      "updatedAt": "2025-04-24T04:15:27.292Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "ba8fe2b8-6c8f-4d89-a2b4-719bba26ddff",
      "number": 6,
      "content": "Chúng lấy bạc đem cân trên cân;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.306Z",
      "updatedAt": "2025-04-24T04:15:27.306Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "434c6260-eb5c-48d4-8a05-e9256243b70d",
      "number": 6,
      "content": "Chúng mướn một người thợ bạc;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.322Z",
      "updatedAt": "2025-04-24T04:15:27.322Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "56ba9e9d-0884-4f54-aaf0-80a9ed30dfa4",
      "number": 6,
      "content": "Chúng nhờ hắn lấy vàng bạc đó làm nên một tượng thần,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.335Z",
      "updatedAt": "2025-04-24T04:15:27.335Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "156fe4e1-cc07-4f6a-8661-a65a07de5613",
      "number": 6,
      "content": "Rồi chúng sụp xuống thờ lạy tượng thần đó.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.355Z",
      "updatedAt": "2025-04-24T04:15:27.355Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "eefd6365-3750-4c7c-96fe-8a069d8af1d5",
      "number": 7,
      "content": "Chúng nhấc nó lên vai và cùng nhau khiêng nó đi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.365Z",
      "updatedAt": "2025-04-24T04:15:27.365Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "e2d4b188-3671-484b-beff-db76e50d7f3a",
      "number": 7,
      "content": "Chúng đặt nó vào một chỗ, và nó cứ đứng yên tại đó;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.378Z",
      "updatedAt": "2025-04-24T04:15:27.378Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "2667dee3-3f83-4cf1-b5cb-5768b6819a52",
      "number": 7,
      "content": "Nó không thể di chuyển khỏi chỗ đứng của nó.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.393Z",
      "updatedAt": "2025-04-24T04:15:27.393Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "5c3590a7-db8f-4e48-9532-e575b341b928",
      "number": 7,
      "content": "Nếu ai kêu cầu nó, nó chẳng thể trả lời;",
      "order": 3,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.411Z",
      "updatedAt": "2025-04-24T04:15:27.411Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "cbd2c1d1-4a81-4090-bd10-77017c7a05a7",
      "number": 7,
      "content": "Nó không thể cứu ai khỏi cơn hoạn nạn.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.425Z",
      "updatedAt": "2025-04-24T04:15:27.425Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "e37d0bab-2214-4dca-a6f7-2d97f5738e01",
      "number": 8,
      "content": "Hãy ghi nhớ điều ấy mà sống xứng đáng làm người,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.437Z",
      "updatedAt": "2025-04-24T04:15:27.437Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "ac746e69-5758-4397-9b21-0d78e8c2000a",
      "number": 8,
      "content": "Hỡi các ngươi, những kẻ hay phạm tội, hãy ghi nhớ trong lòng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.451Z",
      "updatedAt": "2025-04-24T04:15:27.451Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "acbaf486-7a49-489d-8d3f-70dc8c56a3b8",
      "number": 9,
      "content": "Hãy nhớ lại những chuyện xưa kia,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.462Z",
      "updatedAt": "2025-04-24T04:15:27.462Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "a23e625a-59d8-4717-8791-a4d3f6f6c782",
      "number": 9,
      "content": "Vì Ta là Ðức Chúa Trời, không có thần nào khác;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.479Z",
      "updatedAt": "2025-04-24T04:15:27.479Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "e85f98ec-f455-4cb0-9169-ab686fb521a5",
      "number": 9,
      "content": "Ta là Ðức Chúa Trời, không có thần nào khác như Ta.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.494Z",
      "updatedAt": "2025-04-24T04:15:27.494Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "e046633b-eddf-4a45-83eb-4b41579ba2bf",
      "number": 10,
      "content": "Từ ban đầu Ta đã cho biết cuối cùng sẽ ra thể nào;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.507Z",
      "updatedAt": "2025-04-24T04:15:27.507Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "907a3553-306e-4653-97fb-9eaff788c730",
      "number": 10,
      "content": "Từ ngàn xưa Ta đã báo trước việc sẽ xảy đến trong tương lai;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.522Z",
      "updatedAt": "2025-04-24T04:15:27.522Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "4abfc800-2403-478c-a4b6-57063a0fc48c",
      "number": 10,
      "content": "Ta phán: Những gì Ta dự định Ta sẽ làm cho thành tựu;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.535Z",
      "updatedAt": "2025-04-24T04:15:27.535Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "0b056d4a-5b5a-4214-800c-56c5894760a5",
      "number": 10,
      "content": "Mọi điều lòng Ta vui thích Ta sẽ thực hiện cho xong.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.549Z",
      "updatedAt": "2025-04-24T04:15:27.549Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "5b4aeb70-6a86-4622-9590-40f5b9b731c7",
      "number": 11,
      "content": "Ta sẽ gọi một chim săn mồi từ phương đông bay đến,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.563Z",
      "updatedAt": "2025-04-24T04:15:27.563Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "fade7249-0fa0-4b57-9a85-5a0cfbe277c7",
      "number": 11,
      "content": "Một người thi hành ý chỉ Ta sẽ đến từ một xứ xa xăm;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.574Z",
      "updatedAt": "2025-04-24T04:15:27.574Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "c2fc73c4-196d-4bbb-bdca-9dfd897d8a53",
      "number": 11,
      "content": "Thật vậy, điều Ta nói, Ta sẽ làm;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.588Z",
      "updatedAt": "2025-04-24T04:15:27.588Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "840f58a3-fe17-4060-9041-66979a83cc86",
      "number": 11,
      "content": "Ðiều Ta định, Ta sẽ thực hiện.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.603Z",
      "updatedAt": "2025-04-24T04:15:27.603Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "bd44d97e-fcdc-45e0-a426-54345a9b77fb",
      "number": 12,
      "content": "Hãy lắng nghe Ta, hỡi những kẻ có lòng bướng bỉnh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.617Z",
      "updatedAt": "2025-04-24T04:15:27.617Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "6ce543c0-b0a2-4c1e-8e19-ddbbf9214843",
      "number": 12,
      "content": "Những kẻ đang ở xa lẽ công chính:",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.632Z",
      "updatedAt": "2025-04-24T04:15:27.632Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "81a570ce-9716-4145-82c0-156f0cf1d165",
      "number": 13,
      "content": "Ngày Ta thực hiện lẽ công chính của Ta đã gần;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.649Z",
      "updatedAt": "2025-04-24T04:15:27.649Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "a4d6a3e9-ac8c-4ee3-bbf4-752b90d0c1c0",
      "number": 13,
      "content": "Chẳng còn bao lâu nữa đâu;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.665Z",
      "updatedAt": "2025-04-24T04:15:27.665Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "5cfe5e17-3afc-4c09-9734-c6376b3aa362",
      "number": 13,
      "content": "Ơn cứu rỗi của Ta sẽ không đình trệ;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.682Z",
      "updatedAt": "2025-04-24T04:15:27.682Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "961df8be-9ce2-4678-9cc0-ffffe3fa2299",
      "number": 13,
      "content": "Ta sẽ ban ơn cứu rỗi cho Si-ôn,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.696Z",
      "updatedAt": "2025-04-24T04:15:27.696Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    },
    {
      "id": "ef2a7471-6f00-4488-b885-d9198007cd99",
      "number": 13,
      "content": "Vì cớ I-sơ-ra-ên, vinh hiển Ta.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:27.716Z",
      "updatedAt": "2025-04-24T04:15:27.716Z",
      "chapterId": "5a08495d-8caa-4556-9d82-48b12a4cc8d0"
    }
  ],
