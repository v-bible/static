{
  "chapter": {
    "id": "982b9f75-5793-4cc9-b5fd-29656ac9b60e",
    "number": 46,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2046&version=BD2011",
    "createdAt": "2025-04-22T07:37:51.821Z",
    "updatedAt": "2025-04-22T07:37:51.821Z",
    "bookId": "8bf2cad5-d336-4359-ba12-d2c225638c58"
  },
  "verses": [
    {
      "id": "64869db5-89dc-4cf3-a1dc-ebf323655167",
      "number": 1,
      "content": "Thần Bên cúi xuống, Thần Nê-bô rạp mình;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.620Z",
      "updatedAt": "2025-04-22T08:46:45.620Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "0ac1c05f-4bb4-4ea2-996a-199d69a8e873",
      "number": 1,
      "content": "Các thần tượng của chúng được chở đi bằng các xe do thú vật và trâu bò kéo;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.648Z",
      "updatedAt": "2025-04-22T08:46:45.648Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "164ee2a9-3252-42ba-81bc-a52cc05977d7",
      "number": 1,
      "content": "Các thần tượng các ngươi mang theo đã trở thành gánh nặng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.659Z",
      "updatedAt": "2025-04-22T08:46:45.659Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "4f0dcb83-9414-4cc2-9a8a-f6d0ce172fa7",
      "number": 1,
      "content": "Một trọng tải nặng nề khiến các thú vật kéo xe mệt nhọc.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.669Z",
      "updatedAt": "2025-04-22T08:46:45.669Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "d4d3b5e6-cabb-46dc-a320-a50dc0d74238",
      "number": 2,
      "content": "Các thần tượng ấy cúi rạp xuống;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.682Z",
      "updatedAt": "2025-04-22T08:46:45.682Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "fe362b87-4b08-4e8d-9e4e-0cc464b477ed",
      "number": 2,
      "content": "Chúng thảy đều gục xuống;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.697Z",
      "updatedAt": "2025-04-22T08:46:45.697Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "ca44edde-8d33-4372-9556-01efdf8bb3bc",
      "number": 2,
      "content": "Chúng chẳng cứu nổi các súc vật đang kéo nặng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.714Z",
      "updatedAt": "2025-04-22T08:46:45.714Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "4fd56971-b040-45c6-8e9d-33e61d3bf0e5",
      "number": 2,
      "content": "Vì chính bản thân chúng cũng bị bắt đem đi lưu đày.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.731Z",
      "updatedAt": "2025-04-22T08:46:45.731Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "0d0b40ea-e69e-4a16-8f39-107c79c94cf6",
      "number": 3,
      "content": "Hỡi nhà Gia-cốp,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.744Z",
      "updatedAt": "2025-04-22T08:46:45.744Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "ded1a655-8b33-4b36-8ce4-dc3c8226ce4b",
      "number": 3,
      "content": "Hỡi những kẻ còn sót lại của nhà I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.757Z",
      "updatedAt": "2025-04-22T08:46:45.757Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "51d72af5-1542-45dd-8950-7e551c169f01",
      "number": 3,
      "content": "Hãy lắng nghe Ta:",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.770Z",
      "updatedAt": "2025-04-22T08:46:45.770Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "61ac5d43-13f7-483d-972e-cc6e1a57b4e0",
      "number": 3,
      "content": "Ta đã bồng ẵm các ngươi từ lúc các ngươi mới chào đời;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.783Z",
      "updatedAt": "2025-04-22T08:46:45.783Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "cbed1700-3851-48c4-b523-4901f85169c3",
      "number": 3,
      "content": "Ta đã cưu mang các ngươi từ khi các ngươi còn trong lòng mẹ,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.796Z",
      "updatedAt": "2025-04-22T08:46:45.796Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "00782814-19f7-4168-9205-af1ff223c0c0",
      "number": 4,
      "content": "Cho đến khi các ngươi già nua cao tuổi, Ta vẫn là Ðấng ấy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.809Z",
      "updatedAt": "2025-04-22T08:46:45.809Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "aa425fe8-90c9-4bf2-a739-a58df27c1c98",
      "number": 4,
      "content": "Dầu cho các ngươi đã ra da mồi tóc bạc, Ta sẽ còn bồng ẵm các ngươi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.822Z",
      "updatedAt": "2025-04-22T08:46:45.822Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "7d961dfb-a4e3-4d20-b088-f1e4a9b824ae",
      "number": 4,
      "content": "Ta đã tạo ra, nên Ta sẽ bồng ẵm;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.835Z",
      "updatedAt": "2025-04-22T08:46:45.835Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "206dc802-a74e-4706-9df6-c46d603bf57b",
      "number": 4,
      "content": "Ta sẽ cưu mang và sẽ giải cứu các ngươi.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.848Z",
      "updatedAt": "2025-04-22T08:46:45.848Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "5c2fa0ef-aec3-4345-a1e1-79fa4adc3478",
      "number": 5,
      "content": "Các ngươi sẽ ví sánh ai với Ta và cho ai được ngang hàng với Ta?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.861Z",
      "updatedAt": "2025-04-22T08:46:45.861Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "d9d0d096-e8db-47c8-bebe-f45df23f958a",
      "number": 5,
      "content": "Các ngươi so sánh ai với Ta như thể kẻ đó và Ta có điều gì tương xứng?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.874Z",
      "updatedAt": "2025-04-22T08:46:45.874Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "05281073-0946-4395-b03e-c44070c743b0",
      "number": 6,
      "content": "Chúng lấy vàng trong túi ra;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.887Z",
      "updatedAt": "2025-04-22T08:46:45.887Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "4f9d1df6-8394-46f7-99c8-9b688a6379c4",
      "number": 6,
      "content": "Chúng lấy bạc đem cân trên cân;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.900Z",
      "updatedAt": "2025-04-22T08:46:45.900Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "654932a0-065a-4ff0-922a-5d2335213e5d",
      "number": 6,
      "content": "Chúng mướn một người thợ bạc;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.913Z",
      "updatedAt": "2025-04-22T08:46:45.913Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "c47ecc55-5720-4539-aeaa-e1775c135855",
      "number": 6,
      "content": "Chúng nhờ hắn lấy vàng bạc đó làm nên một tượng thần,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.927Z",
      "updatedAt": "2025-04-22T08:46:45.927Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "466c5653-64cf-448d-ba3c-a592f5093e8f",
      "number": 6,
      "content": "Rồi chúng sụp xuống thờ lạy tượng thần đó.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.939Z",
      "updatedAt": "2025-04-22T08:46:45.939Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "21c5bd9d-d283-485f-9dc7-30f4fdca5d7b",
      "number": 7,
      "content": "Chúng nhấc nó lên vai và cùng nhau khiêng nó đi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.949Z",
      "updatedAt": "2025-04-22T08:46:45.949Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "29a05732-41c1-4900-b945-c5b54e3c4666",
      "number": 7,
      "content": "Chúng đặt nó vào một chỗ, và nó cứ đứng yên tại đó;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.960Z",
      "updatedAt": "2025-04-22T08:46:45.960Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "164060fc-8d6f-45af-937d-fefcef359cf1",
      "number": 7,
      "content": "Nó không thể di chuyển khỏi chỗ đứng của nó.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.970Z",
      "updatedAt": "2025-04-22T08:46:45.970Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "cb03c5be-1234-42af-9991-5e5927051516",
      "number": 7,
      "content": "Nếu ai kêu cầu nó, nó chẳng thể trả lời;",
      "order": 3,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.981Z",
      "updatedAt": "2025-04-22T08:46:45.981Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "96b481a9-0b37-4738-b33c-c8a5892b17f8",
      "number": 7,
      "content": "Nó không thể cứu ai khỏi cơn hoạn nạn.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:45.991Z",
      "updatedAt": "2025-04-22T08:46:45.991Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "88726b0b-2631-4d86-bca3-6ea7aeabd231",
      "number": 8,
      "content": "Hãy ghi nhớ điều ấy mà sống xứng đáng làm người,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.001Z",
      "updatedAt": "2025-04-22T08:46:46.001Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "f8d644b9-99c4-4ab6-8f36-d996e7d6af4a",
      "number": 8,
      "content": "Hỡi các ngươi, những kẻ hay phạm tội, hãy ghi nhớ trong lòng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.013Z",
      "updatedAt": "2025-04-22T08:46:46.013Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "7f4ade42-c2b0-4284-8104-4329c451ea53",
      "number": 9,
      "content": "Hãy nhớ lại những chuyện xưa kia,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.025Z",
      "updatedAt": "2025-04-22T08:46:46.025Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "ec6a398a-a104-4c17-aade-3ae4c36414f9",
      "number": 9,
      "content": "Vì Ta là Ðức Chúa Trời, không có thần nào khác;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.039Z",
      "updatedAt": "2025-04-22T08:46:46.039Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "984eba2f-8b38-44e3-b80e-c3558fc32254",
      "number": 9,
      "content": "Ta là Ðức Chúa Trời, không có thần nào khác như Ta.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.052Z",
      "updatedAt": "2025-04-22T08:46:46.052Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "ebcc8332-dc98-44da-aadd-5dcc2836e495",
      "number": 10,
      "content": "Từ ban đầu Ta đã cho biết cuối cùng sẽ ra thể nào;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.066Z",
      "updatedAt": "2025-04-22T08:46:46.066Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "818ef0e5-66d6-4c8c-bc13-bc26572bd644",
      "number": 10,
      "content": "Từ ngàn xưa Ta đã báo trước việc sẽ xảy đến trong tương lai;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.080Z",
      "updatedAt": "2025-04-22T08:46:46.080Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "20bb9ef3-4568-4d71-9f2f-1da53ba6d351",
      "number": 10,
      "content": "Ta phán: Những gì Ta dự định Ta sẽ làm cho thành tựu;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.100Z",
      "updatedAt": "2025-04-22T08:46:46.100Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "a5907a5f-97d9-4189-a766-e888d9c0078a",
      "number": 10,
      "content": "Mọi điều lòng Ta vui thích Ta sẽ thực hiện cho xong.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.114Z",
      "updatedAt": "2025-04-22T08:46:46.114Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "c246ef1d-7876-4ec5-ae84-3c2e825b41d5",
      "number": 11,
      "content": "Ta sẽ gọi một chim săn mồi từ phương đông bay đến,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.127Z",
      "updatedAt": "2025-04-22T08:46:46.127Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "5f4a4774-1216-40d8-bb5c-56ef9ea86736",
      "number": 11,
      "content": "Một người thi hành ý chỉ Ta sẽ đến từ một xứ xa xăm;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.141Z",
      "updatedAt": "2025-04-22T08:46:46.141Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "91ae94f6-6095-42ee-ba34-28830c64c2d0",
      "number": 11,
      "content": "Thật vậy, điều Ta nói, Ta sẽ làm;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.154Z",
      "updatedAt": "2025-04-22T08:46:46.154Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "fd184a02-449b-4797-975a-eca38c638e43",
      "number": 11,
      "content": "Ðiều Ta định, Ta sẽ thực hiện.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.168Z",
      "updatedAt": "2025-04-22T08:46:46.168Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "8b429c0a-158d-4ec0-9b06-db94a0e1bd30",
      "number": 12,
      "content": "Hãy lắng nghe Ta, hỡi những kẻ có lòng bướng bỉnh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.181Z",
      "updatedAt": "2025-04-22T08:46:46.181Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "96c12101-27cf-4580-8e78-de68cdb96977",
      "number": 12,
      "content": "Những kẻ đang ở xa lẽ công chính:",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.194Z",
      "updatedAt": "2025-04-22T08:46:46.194Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "562db5bd-6114-403b-b2cb-9b15a7f46cd1",
      "number": 13,
      "content": "Ngày Ta thực hiện lẽ công chính của Ta đã gần;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.207Z",
      "updatedAt": "2025-04-22T08:46:46.207Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "f94cc363-ca27-4537-bddc-4b3cadf8e59d",
      "number": 13,
      "content": "Chẳng còn bao lâu nữa đâu;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.221Z",
      "updatedAt": "2025-04-22T08:46:46.221Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "4f0f7080-6f2f-4bdb-8225-6876d1590350",
      "number": 13,
      "content": "Ơn cứu rỗi của Ta sẽ không đình trệ;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.234Z",
      "updatedAt": "2025-04-22T08:46:46.234Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "10754beb-ca38-4338-96ff-95998cd4b745",
      "number": 13,
      "content": "Ta sẽ ban ơn cứu rỗi cho Si-ôn,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.247Z",
      "updatedAt": "2025-04-22T08:46:46.247Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    },
    {
      "id": "79aa8594-a748-4387-bbe5-f2d9c280303b",
      "number": 13,
      "content": "Vì cớ I-sơ-ra-ên, vinh hiển Ta.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:46.260Z",
      "updatedAt": "2025-04-22T08:46:46.260Z",
      "chapterId": "982b9f75-5793-4cc9-b5fd-29656ac9b60e"
    }
  ],
