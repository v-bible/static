      "chapterId": "ede500d2-24d7-4eb2-8a90-cd8ef65b66da"
    },
    {
      "id": "b78bf672-423c-4738-878b-161deb655bde",
      "content": "Sự Hư Không của Thần Tượng",
      "order": 0,
      "createdAt": "2024-09-24T02:23:44.533Z",
      "updatedAt": "2024-09-24T02:23:44.533Z",
