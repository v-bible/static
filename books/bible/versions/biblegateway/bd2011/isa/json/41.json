      "chapterId": "c7b8fdb2-8a51-4dec-83dc-9ade5f7f7890"
    },
    {
      "id": "73c25586-1a37-4eda-834a-036b18c52582",
      "content": "Sự Hư Không của Thần Tượng",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T08:46:04.614Z",
      "updatedAt": "2025-04-22T08:46:04.614Z",
