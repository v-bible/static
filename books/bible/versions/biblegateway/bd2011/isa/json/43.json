{
  "chapter": {
    "id": "bc508e29-3435-46e5-ac3e-1a0af3749069",
    "number": 43,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2043&version=BD2011",
    "createdAt": "2025-04-22T07:37:51.748Z",
    "updatedAt": "2025-04-22T07:37:51.748Z",
    "bookId": "8bf2cad5-d336-4359-ba12-d2c225638c58"
  },
  "verses": [
    {
      "id": "8274deac-7d74-4e64-a986-76a60b52d00b",
      "number": 1,
      "content": "Nhưng bây giờ hỡi Gia-cốp,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.646Z",
      "updatedAt": "2025-04-22T08:46:15.646Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "06cc3b5a-1998-4194-aca5-de074da9b09e",
      "number": 1,
      "content": "Chúa là Ðấng dựng nên ngươi;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.674Z",
      "updatedAt": "2025-04-22T08:46:15.674Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "917874b9-6adf-47c7-a856-9a1a7ab20834",
      "number": 1,
      "content": "Hỡi I-sơ-ra-ên, Ðấng tạo thành ngươi phán thếnày:",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.684Z",
      "updatedAt": "2025-04-22T08:46:15.684Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "91b2e233-6cbe-471a-95da-a25bfb408a90",
      "number": 1,
      "content": "Ðừng sợ, vì Ta đã chuộc ngươi;",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.695Z",
      "updatedAt": "2025-04-22T08:46:15.695Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "c0f4b25b-8c85-4e3b-bedb-89d938140aac",
      "number": 1,
      "content": "Ta đã gọi đích danh ngươi;",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.708Z",
      "updatedAt": "2025-04-22T08:46:15.708Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "f62bd750-aae8-4648-b521-4736d5463119",
      "number": 1,
      "content": "Ngươi thuộc về Ta.",
      "order": 5,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.722Z",
      "updatedAt": "2025-04-22T08:46:15.722Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "952fb312-b437-4110-be5a-ca04d0296754",
      "number": 2,
      "content": "Khi ngươi đi qua nước, Ta sẽ ở với ngươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.733Z",
      "updatedAt": "2025-04-22T08:46:15.733Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "dfbd1458-819e-491b-a3b8-16f3aa55ebff",
      "number": 2,
      "content": "Khi ngươi vượt qua sông, nước sẽ chẳng cuốn trôi ngươi;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.744Z",
      "updatedAt": "2025-04-22T08:46:15.744Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "a3a5b43a-a4ec-4199-af8a-318b0f89b20b",
      "number": 2,
      "content": "Khi ngươi băng qua lửa, lửa sẽ không đốt cháy ngươi;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.757Z",
      "updatedAt": "2025-04-22T08:46:15.757Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "5e668b18-f44c-4145-8275-b05a6afa8566",
      "number": 2,
      "content": "Ngọn lửa sẽ không thiêu rụi ngươi.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.766Z",
      "updatedAt": "2025-04-22T08:46:15.766Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "a95fe49d-98bc-4332-b88c-b83f381d4f84",
      "number": 3,
      "content": "Vì Ta là Chúa, Ðức Chúa Trời của ngươi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.776Z",
      "updatedAt": "2025-04-22T08:46:15.776Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "b2563c30-0f51-4e1e-bed3-f3a1d0f79959",
      "number": 3,
      "content": "Ðấng Thánh của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.786Z",
      "updatedAt": "2025-04-22T08:46:15.786Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "80949ee0-6de0-411c-b8a0-cd5b6685af1e",
      "number": 3,
      "content": "Ðấng Giải Cứu của ngươi;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.796Z",
      "updatedAt": "2025-04-22T08:46:15.796Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "53278c23-c906-43b2-a9c6-5898fa3b1b1f",
      "number": 3,
      "content": "Ta ban Ai-cập làm tiền chuộc mạng ngươi;",
      "order": 3,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.809Z",
      "updatedAt": "2025-04-22T08:46:15.809Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "cf0fd39a-9dea-4730-a599-3934282db125",
      "number": 3,
      "content": "Ta phó Ê-thi-ô-pi và Sê-ba để thế chỗ cho ngươi,",
      "order": 4,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.819Z",
      "updatedAt": "2025-04-22T08:46:15.819Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "8cf594df-0bfa-499f-a0b8-9c379a2efead",
      "number": 4,
      "content": "Vì ngươi rất quý báu trước mặt Ta;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.840Z",
      "updatedAt": "2025-04-22T08:46:15.840Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "206d7dfe-a881-45cb-aef2-e2204d07ae6e",
      "number": 4,
      "content": "Ngươi đã được Ta trân quý,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.851Z",
      "updatedAt": "2025-04-22T08:46:15.851Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "cb825369-5ecc-4e2f-a0fa-d7ee717db635",
      "number": 4,
      "content": "Và Ta đã yêu thương ngươi,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.862Z",
      "updatedAt": "2025-04-22T08:46:15.862Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "712c6785-a62a-4e56-8feb-9ddef6d9c51c",
      "number": 4,
      "content": "Nên Ta ban những người khác chịu thay cho ngươi,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.875Z",
      "updatedAt": "2025-04-22T08:46:15.875Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "e3e5a7a9-76da-4a19-aab1-0890020a27bd",
      "number": 4,
      "content": "Phó các dân khác thế mạng ngươi.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.887Z",
      "updatedAt": "2025-04-22T08:46:15.887Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "d6deb40e-e478-437f-bb5f-e3de628109b9",
      "number": 5,
      "content": "Ðừng sợ, vì Ta ở với ngươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.900Z",
      "updatedAt": "2025-04-22T08:46:15.900Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "94c64687-6e02-4b6c-85c1-ade44e721882",
      "number": 5,
      "content": "Ta sẽ đem dòng dõi ngươi từ phương đông trở về;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.913Z",
      "updatedAt": "2025-04-22T08:46:15.913Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "8ba72a66-5e42-4737-8666-a8ab003fa247",
      "number": 5,
      "content": "Ta sẽ nhóm họp con cháu ngươi từ phương tây về.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.926Z",
      "updatedAt": "2025-04-22T08:46:15.926Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "6668750d-4e9f-4af8-bde6-91decff3968e",
      "number": 6,
      "content": "Ta sẽ bảo phương bắc rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 23,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.939Z",
      "updatedAt": "2025-04-22T08:46:15.939Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "ca68f3c3-877b-4f01-b26e-41aa640f58e0",
      "number": 6,
      "content": "“Hãy thả chúng ra!”",
      "order": 1,
      "parNumber": 0,
      "parIndex": 24,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.952Z",
      "updatedAt": "2025-04-22T08:46:15.952Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "21bd5b5c-f3bf-4a1a-a6ca-7a5c499dc895",
      "number": 6,
      "content": "Và nói với phương nam rằng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 25,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.966Z",
      "updatedAt": "2025-04-22T08:46:15.966Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "f788b69b-d542-46b8-b5b9-7a9036be9260",
      "number": 6,
      "content": "“Ðừng giữ chúng lại!”",
      "order": 3,
      "parNumber": 0,
      "parIndex": 26,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.978Z",
      "updatedAt": "2025-04-22T08:46:15.978Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "2d5d643b-daff-4a8e-b7cd-131d56b0f316",
      "number": 6,
      "content": "Hãy đem các con trai Ta từ chốn xa xăm trở về;",
      "order": 4,
      "parNumber": 0,
      "parIndex": 27,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:15.991Z",
      "updatedAt": "2025-04-22T08:46:15.991Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "3cbde3ae-0669-4c07-9257-df015dd3273a",
      "number": 6,
      "content": "Hãy đưa các con gái Ta từ các nơi tận cùng trái đất trở về,",
      "order": 5,
      "parNumber": 0,
      "parIndex": 28,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.004Z",
      "updatedAt": "2025-04-22T08:46:16.004Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "10effa3d-5f14-48ba-8dba-f7e7a4dcaf48",
      "number": 7,
      "content": "Tức tất cả những người được gọi bằng danh Ta;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 29,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.017Z",
      "updatedAt": "2025-04-22T08:46:16.017Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "39b0c884-c591-4e1f-a401-057b51587700",
      "number": 7,
      "content": "Ta đã dựng nên mỗi người vì vinh hiển Ta;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 30,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.030Z",
      "updatedAt": "2025-04-22T08:46:16.030Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "ccc85b0b-b654-4351-9c7c-51e6ce3b14d4",
      "number": 7,
      "content": "Ta đã tạo thành người ấy;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 31,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.046Z",
      "updatedAt": "2025-04-22T08:46:16.046Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "ce908064-4d65-4f5a-bbdb-b43a2361a96c",
      "number": 7,
      "content": "Phải, Ta đã dựng nên người ấy.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 32,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.062Z",
      "updatedAt": "2025-04-22T08:46:16.062Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "870ad95d-efbb-4cf7-9630-a3967bd424e2",
      "number": 8,
      "content": "Hãy để dân ấy ra đi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.075Z",
      "updatedAt": "2025-04-22T08:46:16.075Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "9ca6b174-668d-4a07-a8e8-5fa0d73611f8",
      "number": 8,
      "content": "Dân có mắt nhưng mù và có tai nhưng điếc đó.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.091Z",
      "updatedAt": "2025-04-22T08:46:16.091Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "3bdf4087-a1c3-4f1f-b81a-ffea529406ff",
      "number": 9,
      "content": "Tất cả các nước khá họp lại với nhau;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.104Z",
      "updatedAt": "2025-04-22T08:46:16.104Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "88f3bf26-c1d8-4c4d-9df2-88580b5e51f9",
      "number": 9,
      "content": "Các dân khá tụ tập lại đi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.117Z",
      "updatedAt": "2025-04-22T08:46:16.117Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "e8fb8547-b07c-4631-956d-f5cd4c21991b",
      "number": 9,
      "content": "Ai trong các ngươi có thể công bố điều này?",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.130Z",
      "updatedAt": "2025-04-22T08:46:16.130Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "c5208e0d-12bc-4fbb-a7c9-d0f0e5d47d35",
      "number": 9,
      "content": "Ai có thể giãi bày cho chúng ta hiểu những việc đã xảy đến trước kia?",
      "order": 3,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.143Z",
      "updatedAt": "2025-04-22T08:46:16.143Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "01a56e81-f0d8-41af-9824-ae6f1936e095",
      "number": 9,
      "content": "Hãy để chúng đem các nhân chứng ra biện hộ cho chúng;",
      "order": 4,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.159Z",
      "updatedAt": "2025-04-22T08:46:16.159Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "0ad01468-d6d3-4bb8-80da-8c6f326eb1c7",
      "number": 9,
      "content": "Hãy để người ta nghe và nhìn nhận rằng, “Ðó là sự thật.”",
      "order": 5,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.172Z",
      "updatedAt": "2025-04-22T08:46:16.172Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "3e2bb61a-981b-4679-8a7f-eb59e33a7d02",
      "number": 10,
      "content": "Chúa phán, “Các ngươi là những nhân chứng của Ta,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.185Z",
      "updatedAt": "2025-04-22T08:46:16.185Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "cd7d98e6-564a-4cec-85cc-b749826c4b89",
      "number": 10,
      "content": "Là đầy tớ mà Ta đã chọn,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.198Z",
      "updatedAt": "2025-04-22T08:46:16.198Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "4b8bd6a1-28cc-425e-9619-e0edf5bbb253",
      "number": 10,
      "content": "Ðể các ngươi biết Ta và tin Ta,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.211Z",
      "updatedAt": "2025-04-22T08:46:16.211Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "e53bdc16-a3ee-4c16-ad2a-919cb4297de2",
      "number": 10,
      "content": "Và hiểu rằng Ta là Ðấng ấy.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.223Z",
      "updatedAt": "2025-04-22T08:46:16.223Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "86f8f02f-d5ac-4d3a-a2cb-e905e6d795f8",
      "number": 10,
      "content": "Không có thần nào hiện hữu trước Ta,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.236Z",
      "updatedAt": "2025-04-22T08:46:16.236Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "e9c2ac71-47a3-4e7a-8c9b-052088b1d8ac",
      "number": 10,
      "content": "Và sau Ta cũng không có thần nào khác.",
      "order": 5,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.249Z",
      "updatedAt": "2025-04-22T08:46:16.249Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "6ae8da57-17ae-4ead-ae29-1e72e589b54e",
      "number": 11,
      "content": "Ta, chính Ta, là Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.262Z",
      "updatedAt": "2025-04-22T08:46:16.262Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "03630b5c-70ac-4942-8b7f-b90ce1647352",
      "number": 11,
      "content": "Ngoài Ta không có Ðấng Giải Cứu nào khác.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.275Z",
      "updatedAt": "2025-04-22T08:46:16.275Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "6c75264a-6b3e-4e23-8185-813505bd85aa",
      "number": 12,
      "content": "Ta đã tuyên bố, đã giải cứu, và đã công bố;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.288Z",
      "updatedAt": "2025-04-22T08:46:16.288Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "dfaaee39-8d74-4635-9871-fe0a9c5b7b5b",
      "number": 12,
      "content": "Chớ có thần lạ nào ở giữa các ngươi,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.301Z",
      "updatedAt": "2025-04-22T08:46:16.301Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "73f5f506-93ea-41b2-bc66-f8bfa8bc192b",
      "number": 12,
      "content": "Vì các ngươi là những nhân chứng của Ta,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.314Z",
      "updatedAt": "2025-04-22T08:46:16.314Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "ffeff86b-7e95-414f-bc81-985ceeea020d",
      "number": 12,
      "content": "Chúa phán.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.327Z",
      "updatedAt": "2025-04-22T08:46:16.327Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "3d8c6d3b-637d-421d-b30f-3cd8ef67c873",
      "number": 13,
      "content": "Ta là Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.343Z",
      "updatedAt": "2025-04-22T08:46:16.343Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "534c6b10-f9ac-4bdf-866a-6dd0ce6eca1a",
      "number": 13,
      "content": "Thật vậy, trước khi thời gian hiện hữu, Ta là Ðấng ấy;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.359Z",
      "updatedAt": "2025-04-22T08:46:16.359Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "70157804-07cf-4c65-a387-0ac55ed54148",
      "number": 13,
      "content": "Không ai thoát khỏi tay Ta,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.372Z",
      "updatedAt": "2025-04-22T08:46:16.372Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "569d7940-5e97-4ff3-874d-b16bda5f276b",
      "number": 13,
      "content": "Khi Ta hành động, kẻ nào dám làm ngược lại?",
      "order": 3,
      "parNumber": 1,
      "parIndex": 23,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.384Z",
      "updatedAt": "2025-04-22T08:46:16.384Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "d81c2996-cc76-419e-8d0d-db943bf44333",
      "number": 14,
      "content": "Chúa, Ðấng Cứu Chuộc các ngươi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.397Z",
      "updatedAt": "2025-04-22T08:46:16.397Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "229d117f-cfde-49cc-97ac-81bf4a680c70",
      "number": 14,
      "content": "Ðấng Thánh của I-sơ-ra-ên phán thếnày:",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.410Z",
      "updatedAt": "2025-04-22T08:46:16.410Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "591314c8-9168-4785-8c63-a58ed733cd74",
      "number": 14,
      "content": "Vì cớ các ngươi, Ta sẽ sai người đến Ba-by-lôn,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.422Z",
      "updatedAt": "2025-04-22T08:46:16.422Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "70215980-cac8-4733-bf34-e43229a103aa",
      "number": 14,
      "content": "Ðể phá đổ tất cả các chấn song của nó,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.435Z",
      "updatedAt": "2025-04-22T08:46:16.435Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "35aa4a11-67fa-46e7-bb80-b8203a8f0142",
      "number": 14,
      "content": "Khiến tiếng reo hò của dân Canh-đê trở thành tiếng khóc than.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.448Z",
      "updatedAt": "2025-04-22T08:46:16.448Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "c90e101d-1254-444e-bed9-7e1f013202d5",
      "number": 15,
      "content": "Ta là Chúa,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.476Z",
      "updatedAt": "2025-04-22T08:46:16.476Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "bc577cd0-5a47-44a1-9827-dc9421c6a18d",
      "number": 15,
      "content": "Ðấng Thánh của các ngươi,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.493Z",
      "updatedAt": "2025-04-22T08:46:16.493Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "7f68a9a2-5dbe-4e4a-96fb-07795e157c8b",
      "number": 15,
      "content": "Ðấng Tạo Thành I-sơ-ra-ên,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.508Z",
      "updatedAt": "2025-04-22T08:46:16.508Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "cf1041af-533a-4144-be7f-f73f7536c608",
      "number": 15,
      "content": "Vua các ngươi.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.521Z",
      "updatedAt": "2025-04-22T08:46:16.521Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "2c5c932a-6090-4c6b-8759-40d809671224",
      "number": 16,
      "content": "Chúa phán thế này:",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.534Z",
      "updatedAt": "2025-04-22T08:46:16.534Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "fc125017-842e-4e41-9804-6fba54e742e5",
      "number": 16,
      "content": "Ðấng làm nên một con đường giữa biển,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.547Z",
      "updatedAt": "2025-04-22T08:46:16.547Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "4b8b0756-5a77-4241-8f68-40e9c7903611",
      "number": 16,
      "content": "Tạo một lối đi giữa những dòng nước mạnh,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.563Z",
      "updatedAt": "2025-04-22T08:46:16.563Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "0df3d97b-8ef9-4459-8fb2-d0eca449ac90",
      "number": 17,
      "content": "Ðấng khiến những chiến xa và chiến mã,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.576Z",
      "updatedAt": "2025-04-22T08:46:16.576Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "6984106b-cc78-4488-9b41-970335e272c5",
      "number": 17,
      "content": "Những quân mạnh và binh hùng",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.597Z",
      "updatedAt": "2025-04-22T08:46:16.597Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "526f3a94-49ec-4ddf-8fe4-613c48400e37",
      "number": 17,
      "content": "phải nằm xuống, rồi không trỗi dậy được nữa;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.610Z",
      "updatedAt": "2025-04-22T08:46:16.610Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "3153c164-4cda-4f90-8a1b-2edd9d46c06b",
      "number": 17,
      "content": "Chúng đã tắt lịm, như tim đèn bị tàn lụi, phán,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.623Z",
      "updatedAt": "2025-04-22T08:46:16.623Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "ea3d0ea8-8f0f-4f89-9b2d-03d10bb5ca85",
      "number": 18,
      "content": "Ðừng nhớ đến những chuyện đã qua,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.636Z",
      "updatedAt": "2025-04-22T08:46:16.636Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "f76d8fee-5aa0-414e-ae6e-749e2d63dfc8",
      "number": 18,
      "content": "Ðừng bận tâm đến những gì đã xảy ra thời xa xưa nữa.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.649Z",
      "updatedAt": "2025-04-22T08:46:16.649Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "13bc2124-3a31-4699-854c-ca62774f9cc7",
      "number": 19,
      "content": "Này, Ta đang làm một việc mới.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.662Z",
      "updatedAt": "2025-04-22T08:46:16.662Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "4d073a0f-fecd-4d17-aab4-5f305585b46e",
      "number": 19,
      "content": "Kìa, việc ấy đang bắt đầu xảy ra,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.675Z",
      "updatedAt": "2025-04-22T08:46:16.675Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "256a7198-a0ea-41f0-b19d-0979cdb13d90",
      "number": 19,
      "content": "Các ngươi không nhận thấy sao?",
      "order": 2,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.688Z",
      "updatedAt": "2025-04-22T08:46:16.688Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "2bbf6c25-5f93-432f-8361-719330f596ea",
      "number": 19,
      "content": "Ta sẽ mở một con đường giữa đồng hoang vắng vẻ,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.702Z",
      "updatedAt": "2025-04-22T08:46:16.702Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "2b3c1a57-a265-45f8-8a0a-16313388a316",
      "number": 19,
      "content": "Khơi nguồn các dòng sông giữa sa mạc khô khan.",
      "order": 4,
      "parNumber": 3,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.714Z",
      "updatedAt": "2025-04-22T08:46:16.714Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "31463ef0-41ce-4fd4-bdc1-4ade4716efa2",
      "number": 20,
      "content": "Những thú rừng, chó rừng, và đà điểu sẽ tôn ngợi Ta;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.730Z",
      "updatedAt": "2025-04-22T08:46:16.730Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "91f58126-18a7-42d7-9168-cad9d7c96288",
      "number": 20,
      "content": "Vì Ta ban nước uống giữa chốn đồng hoang,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.743Z",
      "updatedAt": "2025-04-22T08:46:16.743Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "06236625-9246-496f-a410-e6796c1a9407",
      "number": 20,
      "content": "Khiến các sông tuôn tràn trong sa mạc,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.756Z",
      "updatedAt": "2025-04-22T08:46:16.756Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "f23727be-7763-435e-a2de-3af34e7daff6",
      "number": 20,
      "content": "Ðể có nước cho tuyển dân Ta uống.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.769Z",
      "updatedAt": "2025-04-22T08:46:16.769Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "1f8a4bff-df23-4a43-8515-fab049ed4ef3",
      "number": 21,
      "content": "Ấy là dân Ta đã tạo dựng cho chính Ta,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.785Z",
      "updatedAt": "2025-04-22T08:46:16.785Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "c10d0976-a793-468e-8652-8963471f2367",
      "number": 21,
      "content": "Ðể họ có thể rao ra lời ca ngợi Ta.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.800Z",
      "updatedAt": "2025-04-22T08:46:16.800Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "5ea4bac4-ab0b-463b-a3ee-594e219ae15b",
      "number": 22,
      "content": "Thế mà, hỡi Gia-cốp, ngươi đã không kêu cầu Ta;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.814Z",
      "updatedAt": "2025-04-22T08:46:16.814Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "bc4803c7-3f2e-4744-90c9-308e3b2528ba",
      "number": 22,
      "content": "Hỡi I-sơ-ra-ên, ngươi lại chán ngán Ta.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.826Z",
      "updatedAt": "2025-04-22T08:46:16.826Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "6e8d3496-dfce-4648-a66e-5050b20dcccb",
      "number": 23,
      "content": "Ngươi chẳng đem chiên đến làm của lễ thiêu dâng lên Ta;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.839Z",
      "updatedAt": "2025-04-22T08:46:16.839Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "665ca95a-a599-407e-8c64-9c9b0b994b46",
      "number": 23,
      "content": "Ngươi chẳng tôn kính Ta bằng các con vật hiến tế;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.852Z",
      "updatedAt": "2025-04-22T08:46:16.852Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "c79e52ac-feae-47d3-bc9e-75b10ddc8506",
      "number": 23,
      "content": "Ta đâu có bắt các ngươi phải cực nhọc phụng sự Ta bằng các của lễ chay;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.865Z",
      "updatedAt": "2025-04-22T08:46:16.865Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "10a5b9fd-ce41-4f6e-8195-e848db3d5b58",
      "number": 23,
      "content": "Ta cũng không muốn làm các ngươi mệt nhọc vì phải dâng hương khói;",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.877Z",
      "updatedAt": "2025-04-22T08:46:16.877Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "19bda9df-4cfe-4e73-a4dc-485c7924691f",
      "number": 24,
      "content": "Ngươi đã không bỏ tiền ra mua cho Ta được một nén hương;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.890Z",
      "updatedAt": "2025-04-22T08:46:16.890Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "55118435-229c-4aa3-8a8b-61a993bd6faa",
      "number": 24,
      "content": "Ngươi cũng không làm cho Ta vui lòng vì mỡ của các con vật hiến tế,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.900Z",
      "updatedAt": "2025-04-22T08:46:16.900Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "cbf53d2e-f450-46c2-bef9-d845f8651f66",
      "number": 24,
      "content": "Nhưng ngươi đã làm cho Ta nặng nhọc vì những tội lỗi của ngươi;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.910Z",
      "updatedAt": "2025-04-22T08:46:16.910Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "76f8210a-d9dc-47f2-aacb-28ba9cefd9ab",
      "number": 24,
      "content": "Ngươi đã làm cho Ta chán ngán vì những gian ác của ngươi.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:16.920Z",
      "updatedAt": "2025-04-22T08:46:16.920Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "e69bf883-d82e-4653-9110-a37c5e4e73c9",
      "number": 25,
      "content": "Nhưng chính Ta, phải, chính Ta, vì cớ Ta, xóa sạch những vi phạm của ngươi;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:46:16.930Z",
      "updatedAt": "2025-04-22T08:46:16.930Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "bb0ce035-3fac-44ae-b8dc-2940d8902be1",
      "number": 26,
      "content": "Hãy nhắc cho Ta nhớ lại điều ngươi phiền trách,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:46:16.940Z",
      "updatedAt": "2025-04-22T08:46:16.940Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "9b1779fc-8b38-4fe9-a575-f29c05b04c09",
      "number": 27,
      "content": "Tổ phụ đầu tiên của ngươi đã phạm tội;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:46:16.951Z",
      "updatedAt": "2025-04-22T08:46:16.951Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    },
    {
      "id": "d80fe19b-123f-477a-aad9-84926b1d013f",
      "number": 28,
      "content": "Vì thế Ta đã để cho những kẻ lãnh đạo trong nơi thánh bị làm nhục,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": false,
      "createdAt": "2025-04-22T08:46:16.963Z",
      "updatedAt": "2025-04-22T08:46:16.963Z",
      "chapterId": "bc508e29-3435-46e5-ac3e-1a0af3749069"
    }
  ],
