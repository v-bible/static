{
  "chapter": {
    "id": "52a64044-2bff-49b9-a10b-1cda3ca4053d",
    "number": 54,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2054&version=BD2011",
    "createdAt": "2025-04-22T07:37:52.010Z",
    "updatedAt": "2025-04-22T07:37:52.010Z",
    "bookId": "8bf2cad5-d336-4359-ba12-d2c225638c58"
  },
  "verses": [
    {
      "id": "df31a36d-54d1-4ed3-9d47-942e5fa913cc",
      "number": 1,
      "content": "“Hãy hát lên,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.324Z",
      "updatedAt": "2025-04-22T08:47:33.324Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "669b24b1-69ae-4162-8df3-9e76f805bac4",
      "number": 1,
      "content": "Hỡi người phụ nữ hiếm muộn,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.351Z",
      "updatedAt": "2025-04-22T08:47:33.351Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "0fb01ee0-23cb-4610-84f6-864a1dde0748",
      "number": 1,
      "content": "Hỡi người phụ nữ không sinh con!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.361Z",
      "updatedAt": "2025-04-22T08:47:33.361Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "e29ed054-b93e-4043-b26f-d1ceb7a5f52a",
      "number": 1,
      "content": "Hãy cất lên tiếng hát và trỗi tiếng reo mừng,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.371Z",
      "updatedAt": "2025-04-22T08:47:33.371Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "4897b3fd-36bc-4694-8b20-e48435a9172d",
      "number": 1,
      "content": "Hỡi người phụ nữ chưa hề kinh nghiệm cơn đau lúc lâm bồn;",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.383Z",
      "updatedAt": "2025-04-22T08:47:33.383Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "a9f29bda-ba17-4332-bf53-141ec4e98f65",
      "number": 1,
      "content": "Vì con cái của người vợ bị hất hủi sẽ đông hơn con cái của người phụ nữ đang sống với chồng,”",
      "order": 5,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.396Z",
      "updatedAt": "2025-04-22T08:47:33.396Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "e3265deb-40c3-4146-8e0c-9effc8051bb9",
      "number": 17,
      "content": "Chúa phán.",
      "order": 5,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.190Z",
      "updatedAt": "2025-04-22T08:47:34.190Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "1d15eac1-69db-4fa2-99fe-81d909f68ded",
      "number": 1,
      "content": "Chúa phán.",
      "order": 6,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.410Z",
      "updatedAt": "2025-04-22T08:47:33.410Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "0493eea0-7c5e-461c-8456-16b052eb5f66",
      "number": 2,
      "content": "“Hãy mở rộng trại của ngươi cho thêm chỗ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.423Z",
      "updatedAt": "2025-04-22T08:47:33.423Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "7aa48260-723a-4528-84db-f9ce592689d8",
      "number": 2,
      "content": "Hãy căng các tấm bạt nơi ở của ngươi ra,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.436Z",
      "updatedAt": "2025-04-22T08:47:33.436Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "570f13bb-bb25-4e81-9f49-13892d134caf",
      "number": 2,
      "content": "Ðừng giữ lại, hãy nới dài dây căng trại ra,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.449Z",
      "updatedAt": "2025-04-22T08:47:33.449Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "592b7671-3452-40e4-840e-15fa68399c94",
      "number": 2,
      "content": "Hãy đóng các cọc trại của ngươi cho thật chắc;",
      "order": 3,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.462Z",
      "updatedAt": "2025-04-22T08:47:33.462Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "8bec1246-8a4d-4b6c-a722-6a43b9ef4c02",
      "number": 3,
      "content": "Vì ngươi sẽ tràn ra bên phải và bên trái;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.475Z",
      "updatedAt": "2025-04-22T08:47:33.475Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "5c80189f-4da5-4a06-b98f-7461f0be7c0e",
      "number": 3,
      "content": "Dòng dõi ngươi sẽ thừa hưởng các nước,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.488Z",
      "updatedAt": "2025-04-22T08:47:33.488Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "ccecba96-79d8-49fd-a3da-18bf61f3296a",
      "number": 3,
      "content": "Biến các thành bỏ hoang thành những nơi cư trú đông người.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.504Z",
      "updatedAt": "2025-04-22T08:47:33.504Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "301a48ae-5059-4fb5-83ba-44c7e7cd396e",
      "number": 4,
      "content": "Ðừng sợ vì ngươi sẽ không bị hổ thẹn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.518Z",
      "updatedAt": "2025-04-22T08:47:33.518Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "6d8d9edc-ca9b-4201-8527-7688dc8722fb",
      "number": 4,
      "content": "Chớ bối rối vì ngươi sẽ không bị sỉ nhục,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.534Z",
      "updatedAt": "2025-04-22T08:47:33.534Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "cded9acb-489f-4593-b037-34f6902078da",
      "number": 4,
      "content": "Vì ngươi sẽ quên hết nỗi xấu hổ của tuổi thanh xuân;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.547Z",
      "updatedAt": "2025-04-22T08:47:33.547Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "20c7a15c-16aa-406f-bff1-102aed732492",
      "number": 4,
      "content": "Ngươi sẽ không còn nhớ đến nỗi tủi nhục của thời góa bụa nữa,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.560Z",
      "updatedAt": "2025-04-22T08:47:33.560Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "db042974-8727-467d-b837-687fc0aff4ad",
      "number": 5,
      "content": "Vì Ðấng Dựng Nên ngươi là chồng ngươi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.577Z",
      "updatedAt": "2025-04-22T08:47:33.577Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "ab397883-9e0e-4565-b078-9e2a28aa6da9",
      "number": 5,
      "content": "Danh Ngài là Chúa các đạo quân;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.590Z",
      "updatedAt": "2025-04-22T08:47:33.590Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "7cd05c7b-88f4-4a19-b081-63837e7ff671",
      "number": 5,
      "content": "Ðấng Cứu Chuộc ngươi là Ðấng Thánh của I-sơ-ra-ên;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.606Z",
      "updatedAt": "2025-04-22T08:47:33.606Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "d80a8059-8e38-4069-8883-de0eaf808060",
      "number": 5,
      "content": "Ngài được gọi là Ðức Chúa Trời của khắp đất.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.619Z",
      "updatedAt": "2025-04-22T08:47:33.619Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "f931a80e-3f24-4e55-9d9f-7fbe63cbaa2c",
      "number": 6,
      "content": "Vì Chúa đã gọi ngươi về,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.633Z",
      "updatedAt": "2025-04-22T08:47:33.633Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "682e04c4-d3af-4224-aaf7-8419d27d909d",
      "number": 6,
      "content": "Ngươi vốn như người vợ bị bỏ và tâm linh sầu não,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.652Z",
      "updatedAt": "2025-04-22T08:47:33.652Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "ac86df67-e077-4a6d-9e1e-037fac088488",
      "number": 6,
      "content": "Như người vợ được cưới lúc thanh xuân bị hất hủi;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.665Z",
      "updatedAt": "2025-04-22T08:47:33.665Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "7fafedfb-5fbf-446e-88bf-e34ef83bfec4",
      "number": 6,
      "content": "Ðức Chúa Trời của ngươi phán.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.678Z",
      "updatedAt": "2025-04-22T08:47:33.678Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "0a5b5a80-5a78-4d1a-8fd4-daaadb9119eb",
      "number": 7,
      "content": "Vì Ta đã bỏ ngươi trong một thời gian ngắn,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.695Z",
      "updatedAt": "2025-04-22T08:47:33.695Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "92d9845e-b636-45d8-92a8-4faeceba94ba",
      "number": 7,
      "content": "Nhưng vì lòng thương xót lớn lao, Ta sẽ đón ngươi về.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.711Z",
      "updatedAt": "2025-04-22T08:47:33.711Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "a2bfbfbc-2465-450d-b6bc-62364458650b",
      "number": 8,
      "content": "Trong cơn thịnh nộ Ta đã ngoảnh mặt khỏi ngươi một thời gian ngắn,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.725Z",
      "updatedAt": "2025-04-22T08:47:33.725Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "18ea80a8-a257-4e99-8e6d-f2cd4281a991",
      "number": 8,
      "content": "Nhưng bởi tình yêu đời đời Ta sẽ thương xót ngươi;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.738Z",
      "updatedAt": "2025-04-22T08:47:33.738Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "a3f4ce61-8cea-4f7e-8971-54ec5047f7ff",
      "number": 8,
      "content": "Chúa, Ðấng Cứu Chuộc ngươi, phán.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.751Z",
      "updatedAt": "2025-04-22T08:47:33.751Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "e4415aab-6311-457b-bd38-fc4ffd2e6c14",
      "number": 9,
      "content": "Ðiều này đối với Ta cũng giống như trong thời Nô-ê,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.765Z",
      "updatedAt": "2025-04-22T08:47:33.765Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "54212467-dda6-4b51-bc64-26cb88844896",
      "number": 9,
      "content": "Như Ta đã thề rằng nước lụt trong thời Nô-ê sẽ không bao giờ phủ trên mặt đất nữa,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.781Z",
      "updatedAt": "2025-04-22T08:47:33.781Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "63367b7b-8c2e-4f15-b75d-0f0a04fe8326",
      "number": 9,
      "content": "Thì Ta cũng thề rằng Ta sẽ không giận ngươi và không trách ngươi nữa.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.795Z",
      "updatedAt": "2025-04-22T08:47:33.795Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "8b8747ad-0557-4473-abe8-d1c30e7b8630",
      "number": 10,
      "content": "Dù núi có thể dời và đồi có thể chuyển,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.807Z",
      "updatedAt": "2025-04-22T08:47:33.807Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "2df575c3-3fc4-4f82-888a-388b2ecb3d86",
      "number": 10,
      "content": "Nhưng tình yêu của Ta đối với ngươi sẽ không lìa khỏi ngươi,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.819Z",
      "updatedAt": "2025-04-22T08:47:33.819Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "3a3888b9-f56e-4e55-8c8e-f643ecfa5dc4",
      "number": 10,
      "content": "Và giao ước hòa bình thịnh vượng của Ta dành cho ngươi sẽ không bao giờ bị xóa bỏ,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.831Z",
      "updatedAt": "2025-04-22T08:47:33.831Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "9f968e29-b206-4ba3-a432-43a30e728778",
      "number": 10,
      "content": "Chúa, Ðấng thương xót ngươi, phán.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.847Z",
      "updatedAt": "2025-04-22T08:47:33.847Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "539dd2a5-534b-4a65-b4a1-31910408d426",
      "number": 11,
      "content": "Hỡi ngươi là thành khốn khổ, bị bão tố dập vùi, và không được an ủi,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.860Z",
      "updatedAt": "2025-04-22T08:47:33.860Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "30f33c10-9883-4291-bd52-8d98d9a198b6",
      "number": 11,
      "content": "Này, Ta sẽ lấy đá quý màu sắc đẹp đẽ làm đá lót đường của ngươi,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.873Z",
      "updatedAt": "2025-04-22T08:47:33.873Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "482eb6cf-1f8f-4467-bd48-17426278057e",
      "number": 11,
      "content": "Lấy bích ngọc làm nền của ngươi.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.885Z",
      "updatedAt": "2025-04-22T08:47:33.885Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "e7e7736c-efdf-4573-abcf-c73d06c6ca58",
      "number": 12,
      "content": "Ta sẽ lấy hồng ngọc xây các tháp canh,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.898Z",
      "updatedAt": "2025-04-22T08:47:33.898Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "3ca763bb-3385-4262-ae8e-ff7ed1995e42",
      "number": 12,
      "content": "Lấy bạch ngọc xây các cổng,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.912Z",
      "updatedAt": "2025-04-22T08:47:33.912Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "1ebf7038-86e6-4df8-8e45-1cea0a20f44c",
      "number": 12,
      "content": "Và lấy đá quý xây tất cả tường thành.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.928Z",
      "updatedAt": "2025-04-22T08:47:33.928Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "8c8afbf0-77f6-4b79-9d66-aaae2ddc0128",
      "number": 13,
      "content": "Tất cả con cái ngươi sẽ được Chúa dạy dỗ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.945Z",
      "updatedAt": "2025-04-22T08:47:33.945Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "41020988-5a39-4da1-9524-7b7b255307e7",
      "number": 13,
      "content": "Con cái ngươi sẽ hưởng hòa bình thịnh vượng lâu dài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.958Z",
      "updatedAt": "2025-04-22T08:47:33.958Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "8d91c0ad-fdf0-4fda-bd6c-b57cc0eca71c",
      "number": 14,
      "content": "Ngươi sẽ được lập vững trong sự công chính;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.971Z",
      "updatedAt": "2025-04-22T08:47:33.971Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "a1803cd9-6829-40e1-85e5-20b93cb6a0b0",
      "number": 14,
      "content": "Sự áp bức sẽ tránh xa ngươi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:33.988Z",
      "updatedAt": "2025-04-22T08:47:33.988Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "e803af9d-ed7f-41f3-89a2-3dc017962727",
      "number": 14,
      "content": "Ngươi sẽ chẳng còn lo sợ;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.001Z",
      "updatedAt": "2025-04-22T08:47:34.001Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "8b4d49cb-c27f-4a44-866b-f6f3b37024fb",
      "number": 14,
      "content": "Ngươi không còn phải sợ hãi nữa,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.014Z",
      "updatedAt": "2025-04-22T08:47:34.014Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "c44420e1-6554-43a4-af63-b1d603f05db5",
      "number": 14,
      "content": "Vì nó sẽ không đến gần ngươi.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.027Z",
      "updatedAt": "2025-04-22T08:47:34.027Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "ba68410c-a75e-463a-9cc5-1f5334066917",
      "number": 15,
      "content": "Này, chắc chắn có kẻ sẽ tụ tập nhau, nhưng không do Ta sai bảo;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.041Z",
      "updatedAt": "2025-04-22T08:47:34.041Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "05c67ed0-4468-4127-9e44-b88ae468e22e",
      "number": 15,
      "content": "Kẻ nào tụ tập nhau để chống lại ngươi sẽ vì cớ ngươi mà ngã gục.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.054Z",
      "updatedAt": "2025-04-22T08:47:34.054Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "4e029bca-a4ed-4516-a236-e07ae9ec797a",
      "number": 16,
      "content": "Này, chính Ta đã dựng nên người thợ rèn,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.067Z",
      "updatedAt": "2025-04-22T08:47:34.067Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "6270f63f-5084-4901-90e1-99227f1673d9",
      "number": 16,
      "content": "Nó bơm gió thổi vào than để tạo nên lò lửa,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.081Z",
      "updatedAt": "2025-04-22T08:47:34.081Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "9097a1b8-f982-408b-a0dc-9c1091fb2796",
      "number": 16,
      "content": "Rồi từ đó chế ra một dụng cụ để dùng.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.094Z",
      "updatedAt": "2025-04-22T08:47:34.094Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "72df4b90-9cf9-4832-9b5a-230f810ebb2f",
      "number": 16,
      "content": "Và cũng chính Ta đã dựng nên kẻ làm hư để phá hỏng.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.107Z",
      "updatedAt": "2025-04-22T08:47:34.107Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "9d5b2907-7702-47f7-98c5-16843660a9cd",
      "number": 17,
      "content": "Mọi vũ khí chế tạo để đối phó với ngươi sẽ bất thành;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.123Z",
      "updatedAt": "2025-04-22T08:47:34.123Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "b7582286-bb87-4a3b-a6a1-ad378be28d62",
      "number": 17,
      "content": "Mọi lưỡi nổi lên chống lại ngươi trong khi xét xử,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.136Z",
      "updatedAt": "2025-04-22T08:47:34.136Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "c95246e9-7da1-4232-a56f-732fd72fdac4",
      "number": 17,
      "content": "Ngươi sẽ kết tội chúng.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.150Z",
      "updatedAt": "2025-04-22T08:47:34.150Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "7666cced-4ff5-4625-87e9-43184e6caa70",
      "number": 17,
      "content": "Ðó là cơ nghiệp của các tôi tớ Chúa;",
      "order": 3,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.163Z",
      "updatedAt": "2025-04-22T08:47:34.163Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    },
    {
      "id": "39333932-2740-4c97-bb3d-3603df63e4e6",
      "number": 17,
      "content": "Lẽ công chính của họ do Ta quyết định,”",
      "order": 4,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:47:34.176Z",
      "updatedAt": "2025-04-22T08:47:34.176Z",
      "chapterId": "52a64044-2bff-49b9-a10b-1cda3ca4053d"
    }
  ],
