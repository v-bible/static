{
  "chapter": {
    "id": "974b4143-6579-4af0-940d-d7c2c78a24fe",
    "number": 34,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2034&version=BD2011",
    "createdAt": "2025-04-24T03:03:21.011Z",
    "updatedAt": "2025-04-24T03:03:21.011Z",
    "bookId": "e4e58f0e-ae57-420f-8009-a5608bc5a58a"
  },
  "verses": [
    {
      "id": "ac9d3bab-48c9-422c-be61-0c2a881d5d05",
      "number": 1,
      "content": "Hỡi các nước, hãy đến gần và nghe;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.336Z",
      "updatedAt": "2025-04-24T04:14:14.336Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "daefabbb-50f5-4f5b-a6a7-0e2dcd8c5185",
      "number": 1,
      "content": "Hỡi các dân, hãy chú ý!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.364Z",
      "updatedAt": "2025-04-24T04:14:14.364Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "ecd229f5-348a-4427-8855-42dd24774da8",
      "number": 1,
      "content": "Hỡi trái đất và mọi loài sống trên đất,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.375Z",
      "updatedAt": "2025-04-24T04:14:14.375Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "8c84032b-13c5-4294-8bd3-cb1060c40976",
      "number": 1,
      "content": "Hỡi thế gian và mọi vật ra từ thế gian,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.387Z",
      "updatedAt": "2025-04-24T04:14:14.387Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "b7dbf285-4255-4290-a066-f5ab8a194432",
      "number": 1,
      "content": "Tất cả hãy lắng nghe;",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.401Z",
      "updatedAt": "2025-04-24T04:14:14.401Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "1b907358-f43e-4c94-a2e5-0156a51075e6",
      "number": 2,
      "content": "Vì cơn giận của Chúa đang nghịch lại mọi quốc gia;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.420Z",
      "updatedAt": "2025-04-24T04:14:14.420Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "470042a3-b2e8-43e2-a6f6-34d435a2746c",
      "number": 2,
      "content": "Cơn thịnh nộ của Ngài nghịch lại mọi đạo quân của họ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.434Z",
      "updatedAt": "2025-04-24T04:14:14.434Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "82f68587-a800-4887-877d-a34de2479585",
      "number": 2,
      "content": "Ngài muốn diệt hẳn họ,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.456Z",
      "updatedAt": "2025-04-24T04:14:14.456Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "3e1c6c9a-d985-4f0a-a7c4-6954613d7fc4",
      "number": 2,
      "content": "Ngài phó họ cho sự tàn sát.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.467Z",
      "updatedAt": "2025-04-24T04:14:14.467Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "0e64cf4a-6588-47b3-8202-914f315e7bef",
      "number": 3,
      "content": "Xác người bị giết của họ sẽ bị ném ra ngoài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.480Z",
      "updatedAt": "2025-04-24T04:14:14.480Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "ab4e067b-117e-4047-88fd-d2174d744049",
      "number": 3,
      "content": "Mùi hôi thối của chúng sẽ bay lên nồng nặc.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.490Z",
      "updatedAt": "2025-04-24T04:14:14.490Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "741a09bb-f82d-4bd6-b5fd-f6ec3929b9db",
      "number": 3,
      "content": "Máu người sẽ chảy lênh láng trên các núi.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.501Z",
      "updatedAt": "2025-04-24T04:14:14.501Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "f5c3f643-8dbc-492d-8434-61eff9b65587",
      "number": 4,
      "content": "Toàn thể đạo binh trên không gian sẽ tan rã;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.512Z",
      "updatedAt": "2025-04-24T04:14:14.512Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "c02e9029-71f2-410d-a72a-5c2420e2736b",
      "number": 4,
      "content": "Các tầng trời sẽ bị cuốn lại như một cuộn sách;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.523Z",
      "updatedAt": "2025-04-24T04:14:14.523Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "53ce902c-f752-4d74-b164-f42aa95a08d9",
      "number": 4,
      "content": "Tất cả các đạo binh của họ sẽ ngã xuống,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.537Z",
      "updatedAt": "2025-04-24T04:14:14.537Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "2d35d016-13f8-474d-b5e8-cb6319395188",
      "number": 4,
      "content": "Như lá nho rơi rụng,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.550Z",
      "updatedAt": "2025-04-24T04:14:14.550Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "a69958f0-0e9f-4e06-987d-4014a6d3571c",
      "number": 4,
      "content": "Như lá vả lìa cành.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.563Z",
      "updatedAt": "2025-04-24T04:14:14.563Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "e3229004-ccd2-4372-8dbe-7c95f9c99f9a",
      "number": 5,
      "content": "Khi lưỡi gươm Ta đã say máu trên không gian,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.576Z",
      "updatedAt": "2025-04-24T04:14:14.576Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "e27365a0-6429-4f62-bb64-2b69bddfbd11",
      "number": 5,
      "content": "Này, nó sẽ giáng xuống trên Ê-đôm,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.603Z",
      "updatedAt": "2025-04-24T04:14:14.603Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "dbb1e70c-de50-4b78-be2c-855c35e94b11",
      "number": 5,
      "content": "Tức giáng xuống trên dân Ta đã định phải bị đoán phạt.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.616Z",
      "updatedAt": "2025-04-24T04:14:14.616Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "32a9b95b-6262-43b8-82be-ddd1226c5513",
      "number": 6,
      "content": "Gươm của Chúa dính đầy máu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.629Z",
      "updatedAt": "2025-04-24T04:14:14.629Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "f502f335-ffa9-4eac-9cba-84140439399f",
      "number": 6,
      "content": "Mỡ bao kín lưỡi gươm,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.642Z",
      "updatedAt": "2025-04-24T04:14:14.642Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "8b66a3ad-86c7-435f-96b7-1efc746b2c33",
      "number": 6,
      "content": "Nào máu chiên, máu dê,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.656Z",
      "updatedAt": "2025-04-24T04:14:14.656Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "ee5cbaa9-56e9-4f8f-bc7e-45dba24ae1a1",
      "number": 6,
      "content": "Nào mỡ bọc trái cật của các chiên đực,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.669Z",
      "updatedAt": "2025-04-24T04:14:14.669Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "9bedabcf-6734-4378-b3d9-ca16a7e5832b",
      "number": 6,
      "content": "Vì Chúa có cuộc tế lễ ở Bốt-ra,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.682Z",
      "updatedAt": "2025-04-24T04:14:14.682Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "bf35b879-da38-4c3b-9a9e-a3ebab7098c8",
      "number": 6,
      "content": "Một cuộc tàn sát lớn trong xứ Ê-đôm.",
      "order": 5,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.695Z",
      "updatedAt": "2025-04-24T04:14:14.695Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "3d212cfa-05d1-4de0-88cb-fa4a65b30f13",
      "number": 7,
      "content": "Các bò rừng sẽ ngã xuống với chúng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.709Z",
      "updatedAt": "2025-04-24T04:14:14.709Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "c29297a5-033d-46c2-a9c4-c5cad4659ece",
      "number": 7,
      "content": "Các bò tơ và các bò đực mạnh cũng vậy;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.722Z",
      "updatedAt": "2025-04-24T04:14:14.722Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "1652f5bc-194c-4378-b005-e0ba4d80fa1b",
      "number": 7,
      "content": "Ðất của chúng thấm đầy máu;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.735Z",
      "updatedAt": "2025-04-24T04:14:14.735Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "d0b9aac2-78d3-4636-86ea-4276a986c100",
      "number": 7,
      "content": "Bụi của chúng lẫn lộn với mỡ khắp nơi.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.748Z",
      "updatedAt": "2025-04-24T04:14:14.748Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "90a399d5-61a2-4b84-b802-d313cf015241",
      "number": 8,
      "content": "Vì Chúa sẽ có một ngày báo thù,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.762Z",
      "updatedAt": "2025-04-24T04:14:14.762Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "3576617e-3801-4f93-b188-b7ec0c370af3",
      "number": 8,
      "content": "Một năm báo trả để minh oan cho duyên cớ của Si-ôn.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.775Z",
      "updatedAt": "2025-04-24T04:14:14.775Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "713d3ef0-9ea1-441e-b712-e706fec8483f",
      "number": 9,
      "content": "Các sông suối của Ê-đôm sẽ biến thành hắc ín;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.788Z",
      "updatedAt": "2025-04-24T04:14:14.788Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "8272ec12-5da1-4f8b-be50-3e4ca3bbfde8",
      "number": 9,
      "content": "Bụi đất của nó sẽ hóa thành lưu huỳnh;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.802Z",
      "updatedAt": "2025-04-24T04:14:14.802Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "f504f28c-9979-4336-8068-2b8b96368d18",
      "number": 9,
      "content": "Ðất cát của nó sẽ trở nên hắc ín âm ỉ cháy,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.815Z",
      "updatedAt": "2025-04-24T04:14:14.815Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "76f465c3-f1c1-48ef-b684-8fa79edd729f",
      "number": 10,
      "content": "Cả ngày lẫn đêm nó cháy mãi không ngừng;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.828Z",
      "updatedAt": "2025-04-24T04:14:14.828Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "517e0ffe-2c3c-4a3f-988c-882000e4e79d",
      "number": 10,
      "content": "Khói đen của nó cứ bay lên không dứt;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.841Z",
      "updatedAt": "2025-04-24T04:14:14.841Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "0d7ed256-76cf-4a20-b14a-799da32afa90",
      "number": 10,
      "content": "Từ thế hệ này qua thế hệ khác nó nằm đó trong cảnh đổ nát hoang tàn;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.857Z",
      "updatedAt": "2025-04-24T04:14:14.857Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "48d84510-f61e-40b0-a0d9-5163f6d0fe56",
      "number": 10,
      "content": "Muôn đời sẽ không ai muốn đi ngang qua đó.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.870Z",
      "updatedAt": "2025-04-24T04:14:14.870Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "36b79eaa-04ce-482a-bf24-d118386e8a62",
      "number": 11,
      "content": "Diều hâu và nhím sẽ chiếm lấy xứ đó;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.884Z",
      "updatedAt": "2025-04-24T04:14:14.884Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "1bfbe380-ef87-4da5-90b9-25eb57b1c2df",
      "number": 11,
      "content": "Cú và quạ sẽ ở tại đó;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.897Z",
      "updatedAt": "2025-04-24T04:14:14.897Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "b6036cf1-5a80-4adb-ad8d-61c33056f233",
      "number": 11,
      "content": "Trên xứ đó Ngài sẽ giăng một sợi dây của sự hỗn mang,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.911Z",
      "updatedAt": "2025-04-24T04:14:14.911Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "22a2bab0-c61d-4900-9318-b129a44388f9",
      "number": 11,
      "content": "Một dây dọi của sự trống không.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.924Z",
      "updatedAt": "2025-04-24T04:14:14.924Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "be932a5f-4b01-45af-8b5a-3730722451ef",
      "number": 12,
      "content": "Các nhà quý tộc của nó sẽ không còn gì để gọi là một vương quốc;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.937Z",
      "updatedAt": "2025-04-24T04:14:14.937Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "a4baf3cb-5d87-4fe4-be12-d3763622982d",
      "number": 12,
      "content": "Tất cả người quyền quý của nó đều sẽ như không.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.951Z",
      "updatedAt": "2025-04-24T04:14:14.951Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "f61fb0fd-42ba-4be6-9828-f8adf3404e2a",
      "number": 13,
      "content": "Gai góc sẽ mọc lên trong các lâu đài cung điện;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.964Z",
      "updatedAt": "2025-04-24T04:14:14.964Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "25424197-10cd-42b1-a786-7f155dab2719",
      "number": 13,
      "content": "Gai dại và bụi gai sẽ mọc trên các thành lũy xưa kia.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.977Z",
      "updatedAt": "2025-04-24T04:14:14.977Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "928c69f1-7376-4027-a0ed-b5f51e2a37c2",
      "number": 13,
      "content": "Nó sẽ trở nên nơi ở của chó rừng,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:14.990Z",
      "updatedAt": "2025-04-24T04:14:14.990Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "e47f8a20-9f2a-47d0-995b-9e2febbb7694",
      "number": 13,
      "content": "Một sân chơi của đà điểu.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.003Z",
      "updatedAt": "2025-04-24T04:14:15.003Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "31a81df8-d3f3-47b2-b1f8-3e8c11b11145",
      "number": 14,
      "content": "Ở đó mèo rừng sẽ đụng độ với chó sói;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.016Z",
      "updatedAt": "2025-04-24T04:14:15.016Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "2cdd3207-12d7-4b81-8531-1d3a79460fd3",
      "number": 14,
      "content": "Dê rừng sẽ cất tiếng gọi nhau;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.030Z",
      "updatedAt": "2025-04-24T04:14:15.030Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "5a8b0c23-2e43-4314-8e86-4248b6850cea",
      "number": 14,
      "content": "Các thú vật sống về đêm sẽ nghỉ ngơi tại đó;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.043Z",
      "updatedAt": "2025-04-24T04:14:15.043Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "79a3642c-7d74-4263-993b-c31eeaf8eab0",
      "number": 14,
      "content": "Chúng sẽ tìm được một nơi nghỉ ngơi thích hợp cho mình.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.056Z",
      "updatedAt": "2025-04-24T04:14:15.056Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "87c6d061-024d-4017-9c20-da28131328ed",
      "number": 15,
      "content": "Chim cú sẽ làm tổ và đẻ trứng ở đó;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.069Z",
      "updatedAt": "2025-04-24T04:14:15.069Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "f2b74882-e854-4d8a-a033-a31360f232ab",
      "number": 15,
      "content": "Chúng sẽ ấp trứng của chúng và ấp ủ đàn con dưới cánh;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.082Z",
      "updatedAt": "2025-04-24T04:14:15.082Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "f6eb9039-964e-45e0-a826-f205860fa643",
      "number": 15,
      "content": "Nơi đó diều hâu sẽ tụ lại với nhau;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.096Z",
      "updatedAt": "2025-04-24T04:14:15.096Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "b34abe05-b199-4ebb-8303-d70243a5ecc2",
      "number": 15,
      "content": "Con nào cũng có đôi có cặp.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.112Z",
      "updatedAt": "2025-04-24T04:14:15.112Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "9735aa38-9b5b-4651-b03b-2ce065185ae5",
      "number": 16,
      "content": "Hãy tìm trong sách của Chúa và đọc xem:",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.125Z",
      "updatedAt": "2025-04-24T04:14:15.125Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "ad93464e-fbbe-4ad4-8e15-367dbd19c7ff",
      "number": 16,
      "content": "Không một con nào trong các con này sẽ mất;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.139Z",
      "updatedAt": "2025-04-24T04:14:15.139Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "3640b7e4-3ebf-4832-9e00-9605200a8189",
      "number": 16,
      "content": "Không con nào là không có cặp có đôi;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.152Z",
      "updatedAt": "2025-04-24T04:14:15.152Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "6a2732f7-d99b-49b9-9081-0599a6dbceaf",
      "number": 16,
      "content": "Vì miệng Ngài đã truyền ra như vậy;",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.166Z",
      "updatedAt": "2025-04-24T04:14:15.166Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "b50d9c92-796d-46c7-acaf-85cff19d07cb",
      "number": 16,
      "content": "Thần của Ngài đã tụ họp chúng về.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.192Z",
      "updatedAt": "2025-04-24T04:14:15.192Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "1511257f-81a5-46ce-878f-5a38b3b53452",
      "number": 17,
      "content": "Ngài đã bốc thăm định phần cho chúng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.205Z",
      "updatedAt": "2025-04-24T04:14:15.205Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "ca7a0ed0-9e10-48ae-9459-de6cf210c5c2",
      "number": 17,
      "content": "Tay Ngài đã dùng dây đo chia phần cho chúng;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.218Z",
      "updatedAt": "2025-04-24T04:14:15.218Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "d666d0f3-f78f-4c07-b3cc-ed7bcbce4106",
      "number": 17,
      "content": "Chúng sẽ ở tại đó muôn đời;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.231Z",
      "updatedAt": "2025-04-24T04:14:15.231Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    },
    {
      "id": "16157f4d-6670-4296-ba4d-978f0802aebc",
      "number": 17,
      "content": "Chúng sẽ ngụ tại đó từ thế hệ này qua thế hệ khác.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:15.245Z",
      "updatedAt": "2025-04-24T04:14:15.245Z",
      "chapterId": "974b4143-6579-4af0-940d-d7c2c78a24fe"
    }
  ],
