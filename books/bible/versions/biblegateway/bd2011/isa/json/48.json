{
  "chapter": {
    "id": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2",
    "number": 48,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2048&version=BD2011",
    "createdAt": "2025-04-22T07:37:51.869Z",
    "updatedAt": "2025-04-22T07:37:51.869Z",
    "bookId": "8bf2cad5-d336-4359-ba12-d2c225638c58"
  },
  "verses": [
    {
      "id": "0b137aad-2c9b-4bec-a1d2-195dd12db57b",
      "number": 1,
      "content": "Hãy nghe đây, hỡi nhà Gia-cốp;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.447Z",
      "updatedAt": "2025-04-22T08:46:57.447Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "39f003dd-c4ba-4ae2-bc21-20558fb5a9c4",
      "number": 1,
      "content": "Hỡi những kẻ được gọi bằng danh I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.467Z",
      "updatedAt": "2025-04-22T08:46:57.467Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "7702a241-b05f-41fd-8038-e45adc1be451",
      "number": 1,
      "content": "Và những người ra từ nguồn cội của Giu-đa,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.477Z",
      "updatedAt": "2025-04-22T08:46:57.477Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "632a87cf-8c55-47ef-9747-eca59e865938",
      "number": 1,
      "content": "Những người lấy danh Chúa mà thề,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.487Z",
      "updatedAt": "2025-04-22T08:46:57.487Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "35cb894c-3af3-4e08-b7dd-125fa11ad4de",
      "number": 1,
      "content": "Cầu khẩn Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.497Z",
      "updatedAt": "2025-04-22T08:46:57.497Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "045147fc-f5ce-4dc7-90bf-baf5d26b1aee",
      "number": 1,
      "content": "Nhưng lại không thật lòng và ngay thẳng,",
      "order": 5,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.507Z",
      "updatedAt": "2025-04-22T08:46:57.507Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "be56ebd2-e882-4b8f-800c-5eb9d84eed3e",
      "number": 2,
      "content": "Dù chúng cho mình là dân của thành thánh,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.519Z",
      "updatedAt": "2025-04-22T08:46:57.519Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "c537b855-233f-4196-ac02-cc42a1e0e5cd",
      "number": 2,
      "content": "Và nương cậy vào Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.530Z",
      "updatedAt": "2025-04-22T08:46:57.530Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "f303428d-03fe-4b83-a418-7c61da0c04ce",
      "number": 2,
      "content": "Tức Ðấng có danh là Chúa các đạo quân.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.540Z",
      "updatedAt": "2025-04-22T08:46:57.540Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "1dd3b63e-cabc-498a-9111-3dc205c56231",
      "number": 3,
      "content": "Những chuyện đã qua Ta đã báo trước tự ngàn xưa;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.552Z",
      "updatedAt": "2025-04-22T08:46:57.552Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "0cf93691-80a9-48f1-80b0-e956eadc8a7d",
      "number": 3,
      "content": "Những điều ấy đã ra từ miệng Ta,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.563Z",
      "updatedAt": "2025-04-22T08:46:57.563Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "b9220367-17ec-44cf-be12-99aef9fc8f57",
      "number": 3,
      "content": "Và Ta làm cho người ta biết chúng,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.574Z",
      "updatedAt": "2025-04-22T08:46:57.574Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "4df5b080-4320-4444-b1f4-985a89eed01b",
      "number": 3,
      "content": "Rồi thình lình Ta thực hiện những điều ấy,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.585Z",
      "updatedAt": "2025-04-22T08:46:57.585Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "9e755c2d-64c2-4692-9065-821d5e7ece54",
      "number": 3,
      "content": "Và chúng đã xảy ra.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.602Z",
      "updatedAt": "2025-04-22T08:46:57.602Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "46782cf1-b1bb-4aa2-a2ad-ab9ab37fafe0",
      "number": 4,
      "content": "Vì Ta biết ngươi là kẻ cứng đầu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.613Z",
      "updatedAt": "2025-04-22T08:46:57.613Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "8b9e04ca-4a60-4601-9074-d8aa03b50754",
      "number": 4,
      "content": "Gân cổ ngươi cứng như sắt,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.624Z",
      "updatedAt": "2025-04-22T08:46:57.624Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "ccedc585-f52d-4477-9e3b-32285e91ddf5",
      "number": 4,
      "content": "Trán của ngươi cứng như đồng,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.635Z",
      "updatedAt": "2025-04-22T08:46:57.635Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "4001bf04-de85-468f-b6ea-63b02c8ff084",
      "number": 5,
      "content": "Nên ngay từ ban đầu Ta đã nói cho ngươi những điều ấy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.646Z",
      "updatedAt": "2025-04-22T08:46:57.646Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "b7715e99-9423-4757-988c-1fea6a66bc42",
      "number": 5,
      "content": "Trước khi chúng xảy ra, Ta đã nói về chúng cho ngươi hay,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.658Z",
      "updatedAt": "2025-04-22T08:46:57.658Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "a4cbcd5a-1b95-4ccc-a5e0-00cb375ad962",
      "number": 5,
      "content": "Ðể ngươi không thể nói,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.671Z",
      "updatedAt": "2025-04-22T08:46:57.671Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "0749e3f3-696e-46db-8c3b-cd46793f2da1",
      "number": 7,
      "content": "Ðể ngươi không thể nói,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.807Z",
      "updatedAt": "2025-04-22T08:46:57.807Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "b60e039d-18c8-4d3b-a01a-5b6b7f738866",
      "number": 5,
      "content": "“Ấy là thần tượng của tôi đã làm những điều ấy;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.683Z",
      "updatedAt": "2025-04-22T08:46:57.683Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "96bff69d-18de-40b6-a664-7a16fc991c68",
      "number": 5,
      "content": "Ðó là tượng chạm của tôi và tượng đúc của tôi đã ra lịnh cho chúng phải xảy ra.”",
      "order": 4,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.696Z",
      "updatedAt": "2025-04-22T08:46:57.696Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "2c0f83c3-5e72-453e-96dd-809f097bea97",
      "number": 6,
      "content": "Ngươi đã nghe rồi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.708Z",
      "updatedAt": "2025-04-22T08:46:57.708Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "1506795d-473c-401c-a69c-6a37577808fd",
      "number": 6,
      "content": "Ngươi đã thấy tất cả rồi;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.719Z",
      "updatedAt": "2025-04-22T08:46:57.719Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "b4ce0973-dd53-4bbc-8424-3bfa61030366",
      "number": 6,
      "content": "Sao ngươi không tuyên bố ra đi?",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.730Z",
      "updatedAt": "2025-04-22T08:46:57.730Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "363ff687-73e3-4826-acce-0dc0eabf67b2",
      "number": 6,
      "content": "Từ nay Ta sẽ cho ngươi nghe những điều mới,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.742Z",
      "updatedAt": "2025-04-22T08:46:57.742Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "b04c1393-56be-403c-b620-e5e41fbf864d",
      "number": 6,
      "content": "Ngay cả những điều còn giữ kín mà ngươi chưa hề biết.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.754Z",
      "updatedAt": "2025-04-22T08:46:57.754Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "89d4e2da-cecf-4881-91e3-e1082977a55a",
      "number": 7,
      "content": "Ðến bây giờ những điều ấy mới được tạo nên,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.766Z",
      "updatedAt": "2025-04-22T08:46:57.766Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "3dcebdbb-1665-4c30-b180-562c996362eb",
      "number": 7,
      "content": "Chứ chẳng phải chúng đã có sẵn từ lâu rồi đâu;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.778Z",
      "updatedAt": "2025-04-22T08:46:57.778Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "d368af86-fe95-46c0-8ee6-051375bb6072",
      "number": 7,
      "content": "Trước ngày này ngươi chưa bao giờ nghe về chúng,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.792Z",
      "updatedAt": "2025-04-22T08:46:57.792Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "1f4a4cd1-edb8-4297-800b-750e77104c63",
      "number": 7,
      "content": "“Ồ tưởng gì chứ những điều đó tôi đã biết cả rồi.”",
      "order": 4,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.819Z",
      "updatedAt": "2025-04-22T08:46:57.819Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "2c5edd5e-439b-4023-a050-fd7671782489",
      "number": 8,
      "content": "Chắc chắn ngươi chưa nghe đến;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.831Z",
      "updatedAt": "2025-04-22T08:46:57.831Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "0865cfda-4346-4da9-b0fe-4e58b79a0bbb",
      "number": 8,
      "content": "Chắc chắn ngươi chưa biết đến;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.842Z",
      "updatedAt": "2025-04-22T08:46:57.842Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "d0d9049b-c571-4920-9ada-a2356e3f70f3",
      "number": 8,
      "content": "Chắc chắn từ lâu tai ngươi chưa mở ra,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.861Z",
      "updatedAt": "2025-04-22T08:46:57.861Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "d025ca2c-c384-4a5c-b312-a7878e758e93",
      "number": 8,
      "content": "Vì Ta biết ngươi là kẻ phản bội,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.875Z",
      "updatedAt": "2025-04-22T08:46:57.875Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "721c69de-9394-4dae-a7a1-b98ce649bb3b",
      "number": 8,
      "content": "Ngươi mang tên là kẻ phản phúc từ lúc ngươi còn là một thai nhi.",
      "order": 4,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.888Z",
      "updatedAt": "2025-04-22T08:46:57.888Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "bdf1acb4-3e1a-461a-904c-b74f8bc6231d",
      "number": 9,
      "content": "Bấy lâu nay, vì danh Ta, Ta cam nén giận,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.902Z",
      "updatedAt": "2025-04-22T08:46:57.902Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "6a3cad9f-42cd-49b6-b8a1-cca1d63db551",
      "number": 9,
      "content": "Vì sự ca ngợi dành cho Ta, Ta dằn lòng chịu đựng ngươi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.915Z",
      "updatedAt": "2025-04-22T08:46:57.915Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "ea5bff07-0db7-4857-825d-179ba30605f8",
      "number": 9,
      "content": "Mà không tiêu diệt ngươi.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.928Z",
      "updatedAt": "2025-04-22T08:46:57.928Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "18fb721f-e3d2-4c09-b1b6-f1ad369887d4",
      "number": 10,
      "content": "Này, Ta đã tinh luyện ngươi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.942Z",
      "updatedAt": "2025-04-22T08:46:57.942Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "12e05825-7786-45ce-9bbe-5c170cef8195",
      "number": 10,
      "content": "Nhưng không như người ta luyện bạc;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.956Z",
      "updatedAt": "2025-04-22T08:46:57.956Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "efabf430-4328-45d7-9c15-dd652c7ca22c",
      "number": 10,
      "content": "Ta đã rèn luyện ngươi trong lò lửa khổ đau.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.969Z",
      "updatedAt": "2025-04-22T08:46:57.969Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "658010ad-ff84-4a16-97dd-d3fb3d4578a8",
      "number": 11,
      "content": "Nhưng nay vì cớ Ta, vì chính Ta, Ta sẽ ra tay hành động;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.980Z",
      "updatedAt": "2025-04-22T08:46:57.980Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "1d7f5216-7c15-4904-b6e1-42c1c25bc949",
      "number": 11,
      "content": "Chứ lẽ nào Ta cứ để cho danh Ta bị sỉ nhục hay sao?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:57.991Z",
      "updatedAt": "2025-04-22T08:46:57.991Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "53bd869b-e162-4e5c-995b-9eeaf22ffb94",
      "number": 11,
      "content": "Ta sẽ không nhường vinh hiển Ta cho một kẻ nào khác.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.001Z",
      "updatedAt": "2025-04-22T08:46:58.001Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "426e90a4-926c-4785-b5fb-032cdfc823fe",
      "number": 12,
      "content": "Hỡi Gia-cốp, hãy lắng nghe Ta;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.011Z",
      "updatedAt": "2025-04-22T08:46:58.011Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "2cf68f33-b626-4931-80e5-433eb61cd957",
      "number": 12,
      "content": "Hỡi I-sơ-ra-ên, người Ta đã gọi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.021Z",
      "updatedAt": "2025-04-22T08:46:58.021Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "0013c4a7-23e5-4920-b857-8b3292119ea7",
      "number": 12,
      "content": "Ta là Ðấng ấy,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.032Z",
      "updatedAt": "2025-04-22T08:46:58.032Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "399504fd-e542-4981-b6c3-ca3b99583bba",
      "number": 12,
      "content": "Ta là Ðấng Ðầu Tiên, Ta cũng là Ðấng Sau Cùng.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.041Z",
      "updatedAt": "2025-04-22T08:46:58.041Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "3f3e61ef-6db4-4219-ad3b-10a51b8d5056",
      "number": 13,
      "content": "Chính tay Ta đã đặt nền trái đất;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.052Z",
      "updatedAt": "2025-04-22T08:46:58.052Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "5a2d1c23-f87f-4123-94b5-bd1023d6bf3d",
      "number": 13,
      "content": "Tay phải Ta đã trải rộng bầu trời;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.063Z",
      "updatedAt": "2025-04-22T08:46:58.063Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "8b07ca31-6404-411f-8202-b8923851c01d",
      "number": 13,
      "content": "Khi Ta gọi chúng, chúng cùng nhau đứng dậy.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.074Z",
      "updatedAt": "2025-04-22T08:46:58.074Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "d6acd06f-76cc-4742-8a29-5f8766bbd6b9",
      "number": 14,
      "content": "Hỡi tất cả các ngươi, hãy họp lại và lắng nghe!",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.086Z",
      "updatedAt": "2025-04-22T08:46:58.086Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "23606504-a1a2-4c89-bbc6-7bbfabd70a48",
      "number": 14,
      "content": "Ai trong vòng chúng đã công bố những điều ấy?",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.099Z",
      "updatedAt": "2025-04-22T08:46:58.099Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "8235730b-86d2-4d46-bbf7-dbdee2ecde71",
      "number": 14,
      "content": "Chúa yêu thương nó;",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.112Z",
      "updatedAt": "2025-04-22T08:46:58.112Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "27bbc45c-40e5-449d-b1d8-d216424b9b55",
      "number": 14,
      "content": "Ngài sẽ thực hiện điều đẹp ý Ngài đối với Ba-by-lôn;",
      "order": 3,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.125Z",
      "updatedAt": "2025-04-22T08:46:58.125Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "2ca1550e-00a7-477a-8418-ba12c87b3534",
      "number": 14,
      "content": "Ngài sẽ đưa tay ra nghịch lại người Canh-đê.",
      "order": 4,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.139Z",
      "updatedAt": "2025-04-22T08:46:58.139Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "6acff082-f151-4f36-aeca-ace1cf05d64a",
      "number": 15,
      "content": "Ta, chính Ta, đã phán;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.152Z",
      "updatedAt": "2025-04-22T08:46:58.152Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "3d001f51-07cf-4ac1-81cd-f49a50841e36",
      "number": 15,
      "content": "Phải, Ta đã gọi nó;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.165Z",
      "updatedAt": "2025-04-22T08:46:58.165Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "8b63f5ed-bc97-49d7-b4fb-37475543664d",
      "number": 15,
      "content": "Ta đã dẫn dắt nó,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.178Z",
      "updatedAt": "2025-04-22T08:46:58.178Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "88a5654e-affb-411d-9c65-8ec6b7518093",
      "number": 15,
      "content": "Nên đường lối nó sẽ thành công thịnh vượng.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.192Z",
      "updatedAt": "2025-04-22T08:46:58.192Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "92c12b0e-7247-4e42-a54c-bd944bfedf9f",
      "number": 16,
      "content": "Hãy đến gần Ta và nghe rõ điều này:",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.207Z",
      "updatedAt": "2025-04-22T08:46:58.207Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "81d26108-2c5a-48dd-bc71-ca44dc15daa9",
      "number": 16,
      "content": "Từ ban đầu, Ta không nói điều gì bí ẩn;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.218Z",
      "updatedAt": "2025-04-22T08:46:58.218Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "b043f88d-3c8c-4bc9-83d5-e36ba716736f",
      "number": 16,
      "content": "Lúc nó xảy ra, Ta đã có ở đó rồi.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.230Z",
      "updatedAt": "2025-04-22T08:46:58.230Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "4f90b110-e8e2-48a0-817f-ac6052c71117",
      "number": 16,
      "content": "Và bây giờ Chúa Hằng Hữu và Thần của Ngài đã sai tôi.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.240Z",
      "updatedAt": "2025-04-22T08:46:58.240Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "dd3b3b04-ef1a-41e1-a01e-ec0e12187c84",
      "number": 17,
      "content": "Chúa, Ðấng Cứu Chuộc ngươi, Ðấng Thánh của I-sơ-ra-ên, phán thế này:",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.250Z",
      "updatedAt": "2025-04-22T08:46:58.250Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "a5844673-4515-4170-91b2-e11345b1ba48",
      "number": 17,
      "content": "Ta là Chúa, Ðức Chúa Trời của ngươi,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.261Z",
      "updatedAt": "2025-04-22T08:46:58.261Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "7109f899-1ae1-4e63-88c4-77d0d898c3bc",
      "number": 17,
      "content": "Ðấng dạy bảo ngươi để ngươi được phước,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.271Z",
      "updatedAt": "2025-04-22T08:46:58.271Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "a06b8c78-07c4-4fd1-ba9d-6ff87744d806",
      "number": 17,
      "content": "Ðấng dẫn dắt ngươi vào đường lối ngươi nên đi.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.282Z",
      "updatedAt": "2025-04-22T08:46:58.282Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "22d88f59-b165-4954-9bdf-5299545cf533",
      "number": 18,
      "content": "Ôi, ước chi ngươi chú ý đến các điều răn Ta,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.293Z",
      "updatedAt": "2025-04-22T08:46:58.293Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "a482e99e-f6bf-4caa-b2c8-ebcf6dd33919",
      "number": 18,
      "content": "Thì hòa bình thịnh vượng của ngươi sẽ còn mãi như dòng sông vô tận,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.308Z",
      "updatedAt": "2025-04-22T08:46:58.308Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "6265821e-47dd-4dd0-93ee-2f3562d47a2e",
      "number": 18,
      "content": "Sự thành công của ngươi sẽ không ngừng như sóng ở đại dương,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.324Z",
      "updatedAt": "2025-04-22T08:46:58.324Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "059ed8d7-79b7-4d10-a56e-c9bf5804c33e",
      "number": 19,
      "content": "Con cháu của ngươi sẽ đông như cát nơi bờ biển,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.338Z",
      "updatedAt": "2025-04-22T08:46:58.338Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "22eb07ed-1b34-4ef0-abb1-6ce7205fe2ed",
      "number": 19,
      "content": "Dòng dõi của ngươi sẽ nhiều tựa cát của đại dương,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.351Z",
      "updatedAt": "2025-04-22T08:46:58.351Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "66660edf-b4e7-4def-ad9b-89f53c7e4d2f",
      "number": 19,
      "content": "Danh của ngươi sẽ không bao giờ bị xóa bỏ,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.364Z",
      "updatedAt": "2025-04-22T08:46:58.364Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "1fa44837-ec6f-48ff-adaf-02353620eca9",
      "number": 19,
      "content": "Và chẳng bao giờ bị diệt mất trước mặt Ta.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.377Z",
      "updatedAt": "2025-04-22T08:46:58.377Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "c59a7add-08bd-49b0-8545-fb58089fb5c1",
      "number": 20,
      "content": "Hãy rời khỏi Ba-by-lôn,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.391Z",
      "updatedAt": "2025-04-22T08:46:58.391Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "390a79a8-a992-4b37-9d52-0f00ebc8d40b",
      "number": 20,
      "content": "Hãy trốn khỏi dân Canh-đê,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.404Z",
      "updatedAt": "2025-04-22T08:46:58.404Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "cce5579f-0506-4b4b-97b6-fd0d620d53b1",
      "number": 20,
      "content": "Hãy cất tiếng hát ca,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.417Z",
      "updatedAt": "2025-04-22T08:46:58.417Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "a967f506-8fe5-4ee8-aab2-25aaa9cb0615",
      "number": 20,
      "content": "Hãy tuyên bố,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.431Z",
      "updatedAt": "2025-04-22T08:46:58.431Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "0a3eb396-c0aa-4939-853e-bd2d316b5f5c",
      "number": 20,
      "content": "Hãy công bố điều ấy ra,",
      "order": 4,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.444Z",
      "updatedAt": "2025-04-22T08:46:58.444Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "8c5d96ff-7e5b-48f4-b296-4781fcf2af86",
      "number": 20,
      "content": "Hãy truyền tin ấy đến tận cùng trái đất,",
      "order": 5,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.456Z",
      "updatedAt": "2025-04-22T08:46:58.456Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "ef214b25-a6c8-4b8d-bf09-57ca2fc87215",
      "number": 20,
      "content": "Bảo rằng, “Chúa cứu chuộc Gia-cốp đầy tớ Ngài.”",
      "order": 6,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.468Z",
      "updatedAt": "2025-04-22T08:46:58.468Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "a5d12bda-9f72-4f5e-abbe-2822a5affe73",
      "number": 21,
      "content": "Dù Ngài dẫn họ đi qua sa mạc,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.479Z",
      "updatedAt": "2025-04-22T08:46:58.479Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "8957ef12-fbb6-4a13-a220-be45a0d54bb7",
      "number": 21,
      "content": "Họ cũng không khát nước,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.490Z",
      "updatedAt": "2025-04-22T08:46:58.490Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "7095f68d-326d-4876-ac09-6d5e743709e7",
      "number": 21,
      "content": "Vì Ngài khiến nước từ vầng đá chảy ra cho họ uống.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.502Z",
      "updatedAt": "2025-04-22T08:46:58.502Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "500442c2-49c1-41be-8836-50f96e5a52e3",
      "number": 21,
      "content": "Ngài chẻ vầng đá, và nước đã trào ra.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.513Z",
      "updatedAt": "2025-04-22T08:46:58.513Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "174563ba-9163-4b5a-b30a-8637defae090",
      "number": 22,
      "content": "Tuy nhiên phường gian tà sẽ không có bình an,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.524Z",
      "updatedAt": "2025-04-22T08:46:58.524Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    },
    {
      "id": "0e8b5dde-5e43-42cd-add0-2a53d7dfd413",
      "number": 22,
      "content": "Chúa phán.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-22T08:46:58.536Z",
      "updatedAt": "2025-04-22T08:46:58.536Z",
      "chapterId": "6a64aab9-4ec0-4b72-b9f8-e637341de4b2"
    }
  ],
