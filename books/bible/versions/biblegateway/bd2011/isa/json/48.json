{
  "chapter": {
    "id": "5221b3f3-2ecf-4e1c-9278-c9331a493177",
    "number": 48,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2048&version=BD2011",
    "createdAt": "2025-04-24T03:03:21.335Z",
    "updatedAt": "2025-04-24T03:03:21.335Z",
    "bookId": "e4e58f0e-ae57-420f-8009-a5608bc5a58a"
  },
  "verses": [
    {
      "id": "dc1d705c-a3f8-441e-b64f-d3dade2880e4",
      "number": 1,
      "content": "Hãy nghe đây, hỡi nhà Gia-cốp;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.081Z",
      "updatedAt": "2025-04-24T04:15:39.081Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "795ddf13-a873-4a72-8c0c-fa3cd6dff13c",
      "number": 1,
      "content": "Hỡi những kẻ được gọi bằng danh I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.115Z",
      "updatedAt": "2025-04-24T04:15:39.115Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "0c02d61f-9aa7-4e5b-b36a-ba1325b98613",
      "number": 1,
      "content": "Và những người ra từ nguồn cội của Giu-đa,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.126Z",
      "updatedAt": "2025-04-24T04:15:39.126Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "02caf26b-6bf3-463e-88e4-51af7f0be3b9",
      "number": 1,
      "content": "Những người lấy danh Chúa mà thề,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.144Z",
      "updatedAt": "2025-04-24T04:15:39.144Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "0a418439-9050-4155-8f47-40167fc366a7",
      "number": 1,
      "content": "Cầu khẩn Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.155Z",
      "updatedAt": "2025-04-24T04:15:39.155Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "3eb52bf9-fc0d-4441-b1e4-4d6ce526565f",
      "number": 1,
      "content": "Nhưng lại không thật lòng và ngay thẳng,",
      "order": 5,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.165Z",
      "updatedAt": "2025-04-24T04:15:39.165Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "33f1c0be-e7ea-4bf9-a337-47cccfbc958c",
      "number": 2,
      "content": "Dù chúng cho mình là dân của thành thánh,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.176Z",
      "updatedAt": "2025-04-24T04:15:39.176Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "7dac8631-9dd0-4df3-ad74-7bab3a80605b",
      "number": 2,
      "content": "Và nương cậy vào Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.196Z",
      "updatedAt": "2025-04-24T04:15:39.196Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "825000a1-b13a-466e-8d4d-208949ec9fed",
      "number": 2,
      "content": "Tức Ðấng có danh là Chúa các đạo quân.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.217Z",
      "updatedAt": "2025-04-24T04:15:39.217Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "59839b46-1493-41cb-8728-60886b9e231b",
      "number": 3,
      "content": "Những chuyện đã qua Ta đã báo trước tự ngàn xưa;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.229Z",
      "updatedAt": "2025-04-24T04:15:39.229Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "6e1c33d5-2b46-4f08-86ae-faa508fd2bb6",
      "number": 3,
      "content": "Những điều ấy đã ra từ miệng Ta,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.240Z",
      "updatedAt": "2025-04-24T04:15:39.240Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "0aa423b2-b360-4114-8634-0e6b3188de1a",
      "number": 3,
      "content": "Và Ta làm cho người ta biết chúng,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.252Z",
      "updatedAt": "2025-04-24T04:15:39.252Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "9cf6439c-96bf-4487-b476-662fc5e2a8fd",
      "number": 3,
      "content": "Rồi thình lình Ta thực hiện những điều ấy,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.267Z",
      "updatedAt": "2025-04-24T04:15:39.267Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "748f7813-7ce7-4182-a978-e19b10e9e8eb",
      "number": 3,
      "content": "Và chúng đã xảy ra.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.277Z",
      "updatedAt": "2025-04-24T04:15:39.277Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "07937069-f555-4832-ac43-4ac139c76e2a",
      "number": 4,
      "content": "Vì Ta biết ngươi là kẻ cứng đầu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.287Z",
      "updatedAt": "2025-04-24T04:15:39.287Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "a0c4b054-973c-467d-84e5-694f42257349",
      "number": 4,
      "content": "Gân cổ ngươi cứng như sắt,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.300Z",
      "updatedAt": "2025-04-24T04:15:39.300Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "8f685a78-7603-4c02-9167-de0360458b60",
      "number": 4,
      "content": "Trán của ngươi cứng như đồng,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.310Z",
      "updatedAt": "2025-04-24T04:15:39.310Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "21019692-b498-4a5a-a4b4-dbe82e0c7876",
      "number": 5,
      "content": "Nên ngay từ ban đầu Ta đã nói cho ngươi những điều ấy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.321Z",
      "updatedAt": "2025-04-24T04:15:39.321Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "6679fd04-f62d-4b14-9b52-2938620b3d46",
      "number": 5,
      "content": "Trước khi chúng xảy ra, Ta đã nói về chúng cho ngươi hay,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.331Z",
      "updatedAt": "2025-04-24T04:15:39.331Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "1a1104e2-a915-4d6e-a9dc-13c7ff90d4a2",
      "number": 5,
      "content": "Ðể ngươi không thể nói,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.342Z",
      "updatedAt": "2025-04-24T04:15:39.342Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "6bcf1090-08c2-4de2-9a16-109e3c5b788c",
      "number": 5,
      "content": "“Ấy là thần tượng của tôi đã làm những điều ấy;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.352Z",
      "updatedAt": "2025-04-24T04:15:39.352Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "82cadc6b-7108-485c-823e-4c347626e04d",
      "number": 5,
      "content": "Ðó là tượng chạm của tôi và tượng đúc của tôi đã ra lịnh cho chúng phải xảy ra.”",
      "order": 4,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.363Z",
      "updatedAt": "2025-04-24T04:15:39.363Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "c460cfd3-3af9-439b-83d0-b07c14b70f07",
      "number": 6,
      "content": "Ngươi đã nghe rồi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.375Z",
      "updatedAt": "2025-04-24T04:15:39.375Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "a1b19f20-895d-427c-9948-a513608a89ed",
      "number": 6,
      "content": "Ngươi đã thấy tất cả rồi;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.388Z",
      "updatedAt": "2025-04-24T04:15:39.388Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "174cf26e-aa93-4d20-8ab9-853030267f9f",
      "number": 6,
      "content": "Sao ngươi không tuyên bố ra đi?",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.400Z",
      "updatedAt": "2025-04-24T04:15:39.400Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "9ce2c611-06cd-4449-9581-5b61dbdd6866",
      "number": 6,
      "content": "Từ nay Ta sẽ cho ngươi nghe những điều mới,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.413Z",
      "updatedAt": "2025-04-24T04:15:39.413Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "6b82a027-d158-4f27-9d33-6d6fe0fd5e1c",
      "number": 6,
      "content": "Ngay cả những điều còn giữ kín mà ngươi chưa hề biết.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.425Z",
      "updatedAt": "2025-04-24T04:15:39.425Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "892997b3-07df-4801-b7a4-6050d85097b6",
      "number": 7,
      "content": "Ðến bây giờ những điều ấy mới được tạo nên,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.438Z",
      "updatedAt": "2025-04-24T04:15:39.438Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "53c5310d-d53b-44fd-b518-8d20bbd479ba",
      "number": 7,
      "content": "Chứ chẳng phải chúng đã có sẵn từ lâu rồi đâu;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.451Z",
      "updatedAt": "2025-04-24T04:15:39.451Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "e3187271-bee6-4cf7-a30e-680b00dee80b",
      "number": 7,
      "content": "Trước ngày này ngươi chưa bao giờ nghe về chúng,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.463Z",
      "updatedAt": "2025-04-24T04:15:39.463Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "2fe282ec-7a41-4e3f-ad9a-dca92b66c901",
      "number": 7,
      "content": "Ðể ngươi không thể nói,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.476Z",
      "updatedAt": "2025-04-24T04:15:39.476Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "35c2aadf-5760-4e1d-b323-24293bbee7df",
      "number": 7,
      "content": "“Ồ tưởng gì chứ những điều đó tôi đã biết cả rồi.”",
      "order": 4,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.488Z",
      "updatedAt": "2025-04-24T04:15:39.488Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "b613aab0-447c-4fdb-aae5-ad5e82042323",
      "number": 8,
      "content": "Chắc chắn ngươi chưa nghe đến;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.501Z",
      "updatedAt": "2025-04-24T04:15:39.501Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "e630b921-151c-41f7-9322-94016163cc57",
      "number": 8,
      "content": "Chắc chắn ngươi chưa biết đến;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.514Z",
      "updatedAt": "2025-04-24T04:15:39.514Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "96468c60-54ce-42bb-af27-0ed86376824f",
      "number": 8,
      "content": "Chắc chắn từ lâu tai ngươi chưa mở ra,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.527Z",
      "updatedAt": "2025-04-24T04:15:39.527Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "e4f94849-dd22-4213-b028-647ee19afff2",
      "number": 8,
      "content": "Vì Ta biết ngươi là kẻ phản bội,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.539Z",
      "updatedAt": "2025-04-24T04:15:39.539Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "c4cb5fa7-43b0-443d-9f4d-0e6e2867d1a3",
      "number": 8,
      "content": "Ngươi mang tên là kẻ phản phúc từ lúc ngươi còn là một thai nhi.",
      "order": 4,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.552Z",
      "updatedAt": "2025-04-24T04:15:39.552Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "f787a5f6-0659-4742-a7a8-68df955d55b3",
      "number": 9,
      "content": "Bấy lâu nay, vì danh Ta, Ta cam nén giận,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.564Z",
      "updatedAt": "2025-04-24T04:15:39.564Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "fd403375-d24b-42b4-8f75-590634ea887d",
      "number": 9,
      "content": "Vì sự ca ngợi dành cho Ta, Ta dằn lòng chịu đựng ngươi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.577Z",
      "updatedAt": "2025-04-24T04:15:39.577Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "5e44545b-d67c-40c1-9ca4-c5ce204c1ba8",
      "number": 9,
      "content": "Mà không tiêu diệt ngươi.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.589Z",
      "updatedAt": "2025-04-24T04:15:39.589Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "45f39f10-976c-4571-bf86-490dce6cdfe4",
      "number": 10,
      "content": "Này, Ta đã tinh luyện ngươi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.602Z",
      "updatedAt": "2025-04-24T04:15:39.602Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "bd173b0d-e29a-4167-a5ce-d68c57c2c6d6",
      "number": 10,
      "content": "Nhưng không như người ta luyện bạc;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.614Z",
      "updatedAt": "2025-04-24T04:15:39.614Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "52daa41b-970b-48ab-84ae-8558c1efa8ec",
      "number": 10,
      "content": "Ta đã rèn luyện ngươi trong lò lửa khổ đau.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.626Z",
      "updatedAt": "2025-04-24T04:15:39.626Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "d91177c3-fb33-4f5a-b7a8-4992b9d073d1",
      "number": 11,
      "content": "Nhưng nay vì cớ Ta, vì chính Ta, Ta sẽ ra tay hành động;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.639Z",
      "updatedAt": "2025-04-24T04:15:39.639Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "ff7678ad-bd7e-4192-87bb-4b40c63c1620",
      "number": 11,
      "content": "Chứ lẽ nào Ta cứ để cho danh Ta bị sỉ nhục hay sao?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.651Z",
      "updatedAt": "2025-04-24T04:15:39.651Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "e39ae309-5714-49fc-9fe8-5cbce7143cd6",
      "number": 11,
      "content": "Ta sẽ không nhường vinh hiển Ta cho một kẻ nào khác.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.664Z",
      "updatedAt": "2025-04-24T04:15:39.664Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "dcc53dcb-da89-450e-98f7-bcad2ec23ffb",
      "number": 12,
      "content": "Hỡi Gia-cốp, hãy lắng nghe Ta;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.679Z",
      "updatedAt": "2025-04-24T04:15:39.679Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "c9962093-ef4b-4e6c-9b0f-978b3b7cbeba",
      "number": 12,
      "content": "Hỡi I-sơ-ra-ên, người Ta đã gọi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.692Z",
      "updatedAt": "2025-04-24T04:15:39.692Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "e01f4e73-e99c-46fc-b53a-48644f04bb1b",
      "number": 12,
      "content": "Ta là Ðấng ấy,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.705Z",
      "updatedAt": "2025-04-24T04:15:39.705Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "47ea4769-cc18-4da8-b015-d7c145a44bbd",
      "number": 12,
      "content": "Ta là Ðấng Ðầu Tiên, Ta cũng là Ðấng Sau Cùng.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.718Z",
      "updatedAt": "2025-04-24T04:15:39.718Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "722085d0-faa4-47f9-b8d2-ec8cd29d1b8e",
      "number": 13,
      "content": "Chính tay Ta đã đặt nền trái đất;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.730Z",
      "updatedAt": "2025-04-24T04:15:39.730Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "33336e4e-8c91-44ad-b6ef-c65942605764",
      "number": 13,
      "content": "Tay phải Ta đã trải rộng bầu trời;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.743Z",
      "updatedAt": "2025-04-24T04:15:39.743Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "bca89bca-908b-4983-a03b-58a789713c9e",
      "number": 13,
      "content": "Khi Ta gọi chúng, chúng cùng nhau đứng dậy.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.755Z",
      "updatedAt": "2025-04-24T04:15:39.755Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "8ec1cc15-b93d-4f50-870d-4be6c5120bb2",
      "number": 14,
      "content": "Hỡi tất cả các ngươi, hãy họp lại và lắng nghe!",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.774Z",
      "updatedAt": "2025-04-24T04:15:39.774Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "e078d438-125b-4506-b3f1-413f74f76bf7",
      "number": 14,
      "content": "Ai trong vòng chúng đã công bố những điều ấy?",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.790Z",
      "updatedAt": "2025-04-24T04:15:39.790Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "0a76664c-7c43-4cb0-a604-0b8e1b15321b",
      "number": 14,
      "content": "Chúa yêu thương nó;",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.806Z",
      "updatedAt": "2025-04-24T04:15:39.806Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "71d56037-b78d-4d8e-95b0-b67cd8275430",
      "number": 14,
      "content": "Ngài sẽ thực hiện điều đẹp ý Ngài đối với Ba-by-lôn;",
      "order": 3,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.819Z",
      "updatedAt": "2025-04-24T04:15:39.819Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "678ed2b6-603b-4495-9b64-b228102a600d",
      "number": 14,
      "content": "Ngài sẽ đưa tay ra nghịch lại người Canh-đê.",
      "order": 4,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.832Z",
      "updatedAt": "2025-04-24T04:15:39.832Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "3db5dbf4-d41a-44c7-8cb3-417faea9b16b",
      "number": 15,
      "content": "Ta, chính Ta, đã phán;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.844Z",
      "updatedAt": "2025-04-24T04:15:39.844Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "3122820d-baa0-4b44-802c-09b61949e93c",
      "number": 15,
      "content": "Phải, Ta đã gọi nó;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.856Z",
      "updatedAt": "2025-04-24T04:15:39.856Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "f7fa00d1-65bf-4d98-8aea-d5d730b2d3c6",
      "number": 15,
      "content": "Ta đã dẫn dắt nó,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.869Z",
      "updatedAt": "2025-04-24T04:15:39.869Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "2f9fb3df-8ba2-498c-b632-075246c5b67e",
      "number": 15,
      "content": "Nên đường lối nó sẽ thành công thịnh vượng.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.882Z",
      "updatedAt": "2025-04-24T04:15:39.882Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "5b5d9aff-ba35-47c3-a2ad-8846db3f16ae",
      "number": 16,
      "content": "Hãy đến gần Ta và nghe rõ điều này:",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.895Z",
      "updatedAt": "2025-04-24T04:15:39.895Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "23b42fb4-7de0-4285-8351-997cc6419aa2",
      "number": 16,
      "content": "Từ ban đầu, Ta không nói điều gì bí ẩn;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.910Z",
      "updatedAt": "2025-04-24T04:15:39.910Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "be5ba18c-cad5-43d5-949e-04390f55c30b",
      "number": 16,
      "content": "Lúc nó xảy ra, Ta đã có ở đó rồi.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.923Z",
      "updatedAt": "2025-04-24T04:15:39.923Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "8bf80a3c-bf16-4faf-a961-bf511d742263",
      "number": 16,
      "content": "Và bây giờ Chúa Hằng Hữu và Thần của Ngài đã sai tôi.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.936Z",
      "updatedAt": "2025-04-24T04:15:39.936Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "a8076575-253f-4a83-a4dc-d144fbd79eb1",
      "number": 17,
      "content": "Chúa, Ðấng Cứu Chuộc ngươi, Ðấng Thánh của I-sơ-ra-ên, phán thế này:",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.948Z",
      "updatedAt": "2025-04-24T04:15:39.948Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "041fdc28-0273-49e8-a18f-5fce06ec7ce1",
      "number": 17,
      "content": "Ta là Chúa, Ðức Chúa Trời của ngươi,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.961Z",
      "updatedAt": "2025-04-24T04:15:39.961Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "9e30bddd-5dcb-47ee-b2cd-3adbf22f4a34",
      "number": 17,
      "content": "Ðấng dạy bảo ngươi để ngươi được phước,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.974Z",
      "updatedAt": "2025-04-24T04:15:39.974Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "d7409106-ff45-47a2-a0e9-9f9e5c5dccaf",
      "number": 17,
      "content": "Ðấng dẫn dắt ngươi vào đường lối ngươi nên đi.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.986Z",
      "updatedAt": "2025-04-24T04:15:39.986Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "2406e877-7640-488c-9c00-9d47afaf6ac6",
      "number": 18,
      "content": "Ôi, ước chi ngươi chú ý đến các điều răn Ta,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:39.999Z",
      "updatedAt": "2025-04-24T04:15:39.999Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "0adeccc6-44aa-472b-af05-1adbac4ae43a",
      "number": 18,
      "content": "Thì hòa bình thịnh vượng của ngươi sẽ còn mãi như dòng sông vô tận,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.011Z",
      "updatedAt": "2025-04-24T04:15:40.011Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "c1c8072b-a1e6-4f14-a59e-b5c1c0031f33",
      "number": 18,
      "content": "Sự thành công của ngươi sẽ không ngừng như sóng ở đại dương,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.024Z",
      "updatedAt": "2025-04-24T04:15:40.024Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "3fa6720a-d7a1-4ec1-a887-e60a7266e6e4",
      "number": 19,
      "content": "Con cháu của ngươi sẽ đông như cát nơi bờ biển,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.036Z",
      "updatedAt": "2025-04-24T04:15:40.036Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "e4fb380d-3558-497c-900a-bb3b68d30e21",
      "number": 19,
      "content": "Dòng dõi của ngươi sẽ nhiều tựa cát của đại dương,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.049Z",
      "updatedAt": "2025-04-24T04:15:40.049Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "2b83d963-5b2d-47bd-940f-07ecdf822d2e",
      "number": 19,
      "content": "Danh của ngươi sẽ không bao giờ bị xóa bỏ,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.061Z",
      "updatedAt": "2025-04-24T04:15:40.061Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "0791a325-d93f-4933-9fb3-264c10046434",
      "number": 19,
      "content": "Và chẳng bao giờ bị diệt mất trước mặt Ta.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.074Z",
      "updatedAt": "2025-04-24T04:15:40.074Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "5f6c5eb3-692b-49f7-beea-24ed8a22ac9f",
      "number": 20,
      "content": "Hãy rời khỏi Ba-by-lôn,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.090Z",
      "updatedAt": "2025-04-24T04:15:40.090Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "8381cc46-e695-4b12-9745-93861bdbdde4",
      "number": 20,
      "content": "Hãy trốn khỏi dân Canh-đê,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.103Z",
      "updatedAt": "2025-04-24T04:15:40.103Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "afbb9d06-9bbb-4de5-8568-b3ae09248bbc",
      "number": 20,
      "content": "Hãy cất tiếng hát ca,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.115Z",
      "updatedAt": "2025-04-24T04:15:40.115Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "c4d81ecb-5732-464e-b256-1336100e01e7",
      "number": 20,
      "content": "Hãy tuyên bố,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.128Z",
      "updatedAt": "2025-04-24T04:15:40.128Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "117b71e2-743f-48f7-b636-7d31267dc2ed",
      "number": 20,
      "content": "Hãy công bố điều ấy ra,",
      "order": 4,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.140Z",
      "updatedAt": "2025-04-24T04:15:40.140Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "0ff2b56e-1709-44be-82e1-6d3f7218fb56",
      "number": 20,
      "content": "Hãy truyền tin ấy đến tận cùng trái đất,",
      "order": 5,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.153Z",
      "updatedAt": "2025-04-24T04:15:40.153Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "88f5c487-0249-456d-a77b-892671f2b244",
      "number": 20,
      "content": "Bảo rằng, “Chúa cứu chuộc Gia-cốp đầy tớ Ngài.”",
      "order": 6,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.165Z",
      "updatedAt": "2025-04-24T04:15:40.165Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "e75a0e2f-6471-480c-a800-2ee035f08579",
      "number": 21,
      "content": "Dù Ngài dẫn họ đi qua sa mạc,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.178Z",
      "updatedAt": "2025-04-24T04:15:40.178Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "367286c7-075c-4f05-88a8-704710742950",
      "number": 21,
      "content": "Họ cũng không khát nước,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.190Z",
      "updatedAt": "2025-04-24T04:15:40.190Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "30285e4f-cb85-46ff-becf-f09c0bc5347f",
      "number": 21,
      "content": "Vì Ngài khiến nước từ vầng đá chảy ra cho họ uống.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.203Z",
      "updatedAt": "2025-04-24T04:15:40.203Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "d9c3a6b2-8120-4e48-88d0-6f6d0737b1c3",
      "number": 21,
      "content": "Ngài chẻ vầng đá, và nước đã trào ra.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.215Z",
      "updatedAt": "2025-04-24T04:15:40.215Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "599b01f4-55f3-4c37-8e90-54b3af883d08",
      "number": 22,
      "content": "Tuy nhiên phường gian tà sẽ không có bình an,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.228Z",
      "updatedAt": "2025-04-24T04:15:40.228Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    },
    {
      "id": "1554e993-0881-4994-9dec-85c937fbfb52",
      "number": 22,
      "content": "Chúa phán.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:15:40.247Z",
      "updatedAt": "2025-04-24T04:15:40.247Z",
      "chapterId": "5221b3f3-2ecf-4e1c-9278-c9331a493177"
    }
  ],
