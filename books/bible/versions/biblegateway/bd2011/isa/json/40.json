{
  "chapter": {
    "id": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce",
    "number": 40,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2040&version=BD2011",
    "createdAt": "2025-04-24T03:03:21.155Z",
    "updatedAt": "2025-04-24T03:03:21.155Z",
    "bookId": "e4e58f0e-ae57-420f-8009-a5608bc5a58a"
  },
  "verses": [
    {
      "id": "e451b51c-ba58-4a3b-b5d8-cb1bad73a33a",
      "number": 1,
      "content": "Ðức Chúa Trời của các ngươi phán rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.412Z",
      "updatedAt": "2025-04-24T04:14:48.412Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "6d76a752-9348-42b0-86ad-b77be94fbd77",
      "number": 1,
      "content": "“Hãy an ủi, hãy an ủi dân Ta.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.441Z",
      "updatedAt": "2025-04-24T04:14:48.441Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "011abfd2-3f1a-4c7d-a4e2-888b067dc0a1",
      "number": 2,
      "content": "Hãy nói với Giê-ru-sa-lem cách trìu mến,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.451Z",
      "updatedAt": "2025-04-24T04:14:48.451Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "98536bfd-4903-4d44-b1a1-923059b2597d",
      "number": 2,
      "content": "Và rao báo cho nó biết rằng,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.463Z",
      "updatedAt": "2025-04-24T04:14:48.463Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "b66584c8-8aef-4897-999f-117797267bb8",
      "number": 2,
      "content": "Nó đã chịu hình phạt đủ rồi;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.476Z",
      "updatedAt": "2025-04-24T04:14:48.476Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "94c062b8-c35e-478b-85a2-05fbc1e56872",
      "number": 2,
      "content": "Nó đã đền tội xong rồi;",
      "order": 3,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.490Z",
      "updatedAt": "2025-04-24T04:14:48.490Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "f71a2717-46fd-459c-9143-5b1b26321fe5",
      "number": 2,
      "content": "Nó đã nhận gấp đôi từ tay Chúa vì những tội của nó.”",
      "order": 4,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.504Z",
      "updatedAt": "2025-04-24T04:14:48.504Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "d6a18407-a2cf-4950-b7fb-2b43a75b9534",
      "number": 3,
      "content": "Có tiếng kêu lên rằng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.517Z",
      "updatedAt": "2025-04-24T04:14:48.517Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "2ed28301-c7ac-47ce-93cc-8b09414e21c6",
      "number": 3,
      "content": "“Hãy mở cho Chúa một con đường trong đồng hoang;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.531Z",
      "updatedAt": "2025-04-24T04:14:48.531Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "45c2623e-df7e-45aa-9a05-7fe61e25e381",
      "number": 3,
      "content": "Hãy làm một đại lộ thẳng bằng cho Ðức Chúa Trời chúng ta trong sa mạc.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.545Z",
      "updatedAt": "2025-04-24T04:14:48.545Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "5f964aa1-a56f-4cb0-b338-d032969aed61",
      "number": 4,
      "content": "Mọi thung lũng phải lấp cho đầy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.558Z",
      "updatedAt": "2025-04-24T04:14:48.558Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "294b4f2b-ee63-4e3b-a7be-a208e38dd0ad",
      "number": 4,
      "content": "Mọi núi đồi phải san cho phẳng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.571Z",
      "updatedAt": "2025-04-24T04:14:48.571Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "24621ea4-309d-401a-8a99-b2476313314b",
      "number": 4,
      "content": "Mọi gò nổng phải ủi cho bằng;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.583Z",
      "updatedAt": "2025-04-24T04:14:48.583Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "3cb5b08c-ecc7-4540-b939-b9f9c8340844",
      "number": 4,
      "content": "Những nơi gồ ghề phải làm cho bằng phẳng,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.597Z",
      "updatedAt": "2025-04-24T04:14:48.597Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "47d68906-db6e-45e8-afcd-0bbc4e0b7866",
      "number": 5,
      "content": "Bấy giờ vinh quang của Chúa sẽ hiển lộ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.610Z",
      "updatedAt": "2025-04-24T04:14:48.610Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "bdfdee7e-2342-4f76-bd8a-7d68860aa951",
      "number": 5,
      "content": "Và mọi loài xác thịt sẽ cùng nhau xem thấy,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.623Z",
      "updatedAt": "2025-04-24T04:14:48.623Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "3dfb3e4b-9af0-4c5c-9d34-f7c48c8a21f8",
      "number": 5,
      "content": "Vì miệng Chúa đã phán vậy.”",
      "order": 2,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.637Z",
      "updatedAt": "2025-04-24T04:14:48.637Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "9ba473dc-ac22-41a1-855d-73b062a1fd70",
      "number": 6,
      "content": "Có tiếng phán rằng, “Ngươi hãy kêu lên!”",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.650Z",
      "updatedAt": "2025-04-24T04:14:48.650Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "bf6ec5c1-058b-4dfa-bc5a-cced55b8f373",
      "number": 6,
      "content": "Tôi thưa rằng, “Con phải nói gì?”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.664Z",
      "updatedAt": "2025-04-24T04:14:48.664Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "50582113-9825-4b53-a30c-cf1049ee0177",
      "number": 6,
      "content": "Mọi loài xác thịt như cỏ dại,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.677Z",
      "updatedAt": "2025-04-24T04:14:48.677Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "30a918e7-3cad-45f1-b08c-a9544a39491a",
      "number": 6,
      "content": "Vẻ đẹp của chúng như hoa ngoài đồng.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.690Z",
      "updatedAt": "2025-04-24T04:14:48.690Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "34247a8a-83b5-4c18-8828-995d89ee4a2a",
      "number": 7,
      "content": "Cỏ héo, hoa tàn,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.703Z",
      "updatedAt": "2025-04-24T04:14:48.703Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "35e4e811-3057-4c71-bac7-53b2a16c6194",
      "number": 7,
      "content": "Vì hơi thở của Chúa thổi qua trên chúng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.717Z",
      "updatedAt": "2025-04-24T04:14:48.717Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "2a64646f-b4da-4382-a64e-d4ed2427a14f",
      "number": 7,
      "content": "Thật vậy loài người chẳng khác gì cỏ dại.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.730Z",
      "updatedAt": "2025-04-24T04:14:48.730Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "5740746e-8c26-48ef-93cd-4e9526b94b1a",
      "number": 8,
      "content": "Cỏ héo, hoa tàn,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.743Z",
      "updatedAt": "2025-04-24T04:14:48.743Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "06c7909b-c94c-4ceb-8ea6-12ce9b4f20c8",
      "number": 8,
      "content": "Nhưng lời của Ðức Chúa Trời chúng ta đứng vững đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.756Z",
      "updatedAt": "2025-04-24T04:14:48.756Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "7d571c53-f7a5-4236-8053-cd7d8ab27828",
      "number": 9,
      "content": "Hỡi các ngươi, những người đem tin mừng đến Si-ôn,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.770Z",
      "updatedAt": "2025-04-24T04:14:48.770Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "45afa1f4-1d45-4cb9-9b2e-2a210ac7c281",
      "number": 9,
      "content": "Hãy lên núi cao.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.783Z",
      "updatedAt": "2025-04-24T04:14:48.783Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "02a98690-7d8c-4fa6-9ce6-67128fa7d1e2",
      "number": 9,
      "content": "Hỡi các ngươi, những người đem tin mừng đến Giê-ru-sa-lem,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.797Z",
      "updatedAt": "2025-04-24T04:14:48.797Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "574e8aa7-a3d0-4db9-a14b-443afcc5e171",
      "number": 9,
      "content": "Hãy hết sức nói lớn tiếng lên,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.811Z",
      "updatedAt": "2025-04-24T04:14:48.811Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "aef7bc85-6315-4efd-b46d-cc60265d7080",
      "number": 9,
      "content": "Hãy cất tiếng lên, đừng sợ hãi,",
      "order": 4,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.824Z",
      "updatedAt": "2025-04-24T04:14:48.824Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "87d7cd06-e0ab-4974-bbb0-61ed191d0784",
      "number": 9,
      "content": "Hãy nói cho các thành của Giu-đa biết rằng, “Ðây là Ðức Chúa Trời của các ngươi.”",
      "order": 5,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.841Z",
      "updatedAt": "2025-04-24T04:14:48.841Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "59effc95-bfcc-45ee-a59b-6e9d06ca37a5",
      "number": 10,
      "content": "Này, Chúa Hằng Hữu ngự đến với uy quyền;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.854Z",
      "updatedAt": "2025-04-24T04:14:48.854Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "cee2e1c5-f214-4c5d-aaee-a2d01f16c877",
      "number": 10,
      "content": "Ngài sẽ trị vì bằng cánh tay Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.867Z",
      "updatedAt": "2025-04-24T04:14:48.867Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "dff4fb98-3955-4a16-a4ba-682a9b04972a",
      "number": 10,
      "content": "Này, Ngài đem phần thưởng theo với Ngài;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.880Z",
      "updatedAt": "2025-04-24T04:14:48.880Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "5b1839b9-7ba5-4e14-afee-9fecbbacbcc0",
      "number": 10,
      "content": "Các vật tưởng thưởng của Ngài đi trước Ngài.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.894Z",
      "updatedAt": "2025-04-24T04:14:48.894Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "6afe0c1a-794b-45e1-a6a9-de3f7e50f085",
      "number": 11,
      "content": "Ngài sẽ chăn giữ bầy chiên của Ngài như một người chăn;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.908Z",
      "updatedAt": "2025-04-24T04:14:48.908Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "42ec5528-3088-436f-9d81-b2c99c21d0b5",
      "number": 11,
      "content": "Ngài sẽ bồng ẵm các chiên con trong tay Ngài;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.921Z",
      "updatedAt": "2025-04-24T04:14:48.921Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "8371b8bf-932f-433c-ae64-4aabd6bd0fd0",
      "number": 11,
      "content": "Ngài sẽ ôm chúng vào lòng Ngài,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.934Z",
      "updatedAt": "2025-04-24T04:14:48.934Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "25b924a2-4c40-43bf-ad2e-977e05db1642",
      "number": 11,
      "content": "Và Ngài sẽ chăm sóc các chiên mẹ có con còn thơ dại.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.947Z",
      "updatedAt": "2025-04-24T04:14:48.947Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "dee6fe8b-174e-4f4d-84c0-a2b5304eca4f",
      "number": 12,
      "content": "Ai đã dùng lòng bàn tay lường nước biển,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.960Z",
      "updatedAt": "2025-04-24T04:14:48.960Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "ae0e6f79-5cc9-4fbb-955b-08bf4bab2caa",
      "number": 12,
      "content": "Dùng gang tay đo các tầng trời,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.973Z",
      "updatedAt": "2025-04-24T04:14:48.973Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "62c841da-a42f-46ee-abea-8c42fe79fcd5",
      "number": 12,
      "content": "Lấy cái đấu đong bụi đất?",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.986Z",
      "updatedAt": "2025-04-24T04:14:48.986Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "4c09edec-b49c-41f7-9776-224d2169bda1",
      "number": 12,
      "content": "Ai lấy bàn cân cân núi lớn,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:48.999Z",
      "updatedAt": "2025-04-24T04:14:48.999Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "ae1294cb-f95d-490e-b292-3eb1d8c3bd43",
      "number": 12,
      "content": "Hoặc dùng dĩa cân cân các đồi cao?",
      "order": 4,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.016Z",
      "updatedAt": "2025-04-24T04:14:49.016Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "316faf26-bfc4-44c7-a46a-3f9fb312ee66",
      "number": 13,
      "content": "Ai có thể hướng dẫn được Thần của Chúa,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.029Z",
      "updatedAt": "2025-04-24T04:14:49.029Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "585ab754-4b3a-4e44-bcb6-5ac56bfd6f33",
      "number": 13,
      "content": "Hoặc làm quân sư để chỉ dạy Ngài?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.042Z",
      "updatedAt": "2025-04-24T04:14:49.042Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "d0bb45c3-030b-45cd-8965-f4a0995dd72a",
      "number": 14,
      "content": "Ngài đã tham khảo mưu kế của ai?",
      "order": 0,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.055Z",
      "updatedAt": "2025-04-24T04:14:49.055Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "91f4bde3-9737-45ed-ab94-f165a8dd4909",
      "number": 14,
      "content": "Ai đã chỉ dạy Ngài?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.069Z",
      "updatedAt": "2025-04-24T04:14:49.069Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "88049a20-b69e-4beb-9222-dd0e4b9311df",
      "number": 14,
      "content": "Ai đã dạy Ngài con đường công lý?",
      "order": 2,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.082Z",
      "updatedAt": "2025-04-24T04:14:49.082Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "0d8c4d62-4b90-4813-a9cf-818524b70800",
      "number": 14,
      "content": "Ai đã dạy Ngài tri thức vô biên?",
      "order": 3,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.104Z",
      "updatedAt": "2025-04-24T04:14:49.104Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "46029326-c294-4603-9984-56042a818434",
      "number": 14,
      "content": "Ai đã chỉ cho Ngài đường nào để được trí tuệ?",
      "order": 4,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.117Z",
      "updatedAt": "2025-04-24T04:14:49.117Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "e530f9e3-5453-4984-9e7a-b0e38a911cf0",
      "number": 15,
      "content": "Này, các quốc gia khác nào giọt nước trong thùng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.130Z",
      "updatedAt": "2025-04-24T04:14:49.130Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "1c0a99ed-1bdd-4a61-a050-487266deed2f",
      "number": 15,
      "content": "Họ bị xem như chút bụi dính nơi dĩa cân;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.142Z",
      "updatedAt": "2025-04-24T04:14:49.142Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "bb12e7b2-b27a-4b44-8170-b15d24c1c820",
      "number": 15,
      "content": "Này, Ngài nhấc bổng các đảo như một vật bé nhỏ.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.155Z",
      "updatedAt": "2025-04-24T04:14:49.155Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "3325a0a1-b3dd-4995-a8a2-d30cd4e98c76",
      "number": 16,
      "content": "Gỗ của núi Li-băng không đủ đốt dâng của lễ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.167Z",
      "updatedAt": "2025-04-24T04:14:49.167Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "94945d75-696e-4f81-8171-22486be2daad",
      "number": 16,
      "content": "Các thú trên núi ấy cũng không đủ làm của lễ thiêu.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.184Z",
      "updatedAt": "2025-04-24T04:14:49.184Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "2d28aea6-c6fe-4b57-babf-0c72bf9f61db",
      "number": 17,
      "content": "Tất cả các quốc gia chẳng có nghĩa lý gì trước mặt Ngài;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.201Z",
      "updatedAt": "2025-04-24T04:14:49.201Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "cdd51ff0-0595-46b0-b928-b213d3d8cb3a",
      "number": 17,
      "content": "Ðối với Ngài, mọi quốc gia trên thế giới chẳng là gì cả và chỉ là hư vô.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.217Z",
      "updatedAt": "2025-04-24T04:14:49.217Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "d4577be8-08e2-4cda-840b-ba3768e7ffc0",
      "number": 18,
      "content": "Thế thì các ngươi ví sánh Ðức Chúa Trời với ai?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.230Z",
      "updatedAt": "2025-04-24T04:14:49.230Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "a01fff90-479d-4a6d-aaac-f9cb9de6e735",
      "number": 18,
      "content": "Hay lấy hình ảnh nào để so sánh với Ngài được?",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.242Z",
      "updatedAt": "2025-04-24T04:14:49.242Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "9ea04088-de07-4bb5-95bd-22a73cf78e98",
      "number": 19,
      "content": "Phải chăng một pho tượng do thợ đúc tượng đúc ra?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.258Z",
      "updatedAt": "2025-04-24T04:14:49.258Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "427517df-2f24-43da-ae66-200939ab9213",
      "number": 19,
      "content": "Rồi thợ dát vàng thếp một lớp vàng mỏng lên trên,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.268Z",
      "updatedAt": "2025-04-24T04:14:49.268Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "702e49cd-71ae-4a02-a493-109ca0a0d9b3",
      "number": 19,
      "content": "Và người thợ bạc đúc thêm một dây chuyền bạc.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.279Z",
      "updatedAt": "2025-04-24T04:14:49.279Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "d65a1334-19c3-407f-b9fd-69f372210da7",
      "number": 20,
      "content": "Người nghèo không đủ khả năng làm tượng như thế để thờ thì chọn một khúc gỗ chưa mục,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.291Z",
      "updatedAt": "2025-04-24T04:14:49.291Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "c75ea225-b825-40ed-91c7-8b221ade0a59",
      "number": 20,
      "content": "Rồi tìm một người thợ mộc khéo tay,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.304Z",
      "updatedAt": "2025-04-24T04:14:49.304Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "e0f2cfe1-cc9d-4caf-8922-35960b7b31d4",
      "number": 20,
      "content": "Mướn thợ ấy đục đẽo thành một tượng gỗ không bị ngã.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.323Z",
      "updatedAt": "2025-04-24T04:14:49.323Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "db2295bb-5485-4b37-9ba1-8fff39f2cb49",
      "number": 21,
      "content": "Các ngươi chưa biết sao?",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.337Z",
      "updatedAt": "2025-04-24T04:14:49.337Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "437a3021-d8a1-4d44-93f0-3a912d9cc864",
      "number": 21,
      "content": "Các ngươi chưa nghe sao?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.350Z",
      "updatedAt": "2025-04-24T04:14:49.350Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "75ec6aa2-ffaa-49fc-9521-57fbed29dd5d",
      "number": 21,
      "content": "Từ lúc ban đầu các ngươi chưa được bảo cho biết sao?",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.363Z",
      "updatedAt": "2025-04-24T04:14:49.363Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "8b0ccaff-dfdd-4b45-a624-0290f5d940f2",
      "number": 21,
      "content": "Từ lúc lập nền trái đất các ngươi há chưa hiểu sao?",
      "order": 3,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.376Z",
      "updatedAt": "2025-04-24T04:14:49.376Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "7e8d154d-7d55-4568-9da7-45c8b8be22b1",
      "number": 22,
      "content": "Ðấng ngự trên vòm trái đất,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.390Z",
      "updatedAt": "2025-04-24T04:14:49.390Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "20094ba2-b18b-455d-a5e1-c7a8680eabc0",
      "number": 22,
      "content": "Nhìn xuống nhân gian như châu chấu cào cào;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.403Z",
      "updatedAt": "2025-04-24T04:14:49.403Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "9135cba9-e7d1-482c-ba3c-bd94df601f3f",
      "number": 22,
      "content": "Ngài giăng bầu trời ra như giăng một tấm màn;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.416Z",
      "updatedAt": "2025-04-24T04:14:49.416Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "4686a5f1-c489-4338-99de-056a9346291a",
      "number": 22,
      "content": "Ngài căng chúng ra như người ta căng lều để ở.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.433Z",
      "updatedAt": "2025-04-24T04:14:49.433Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "dbcd103e-dd80-402d-8323-82283bd6c7d6",
      "number": 23,
      "content": "Ngài biến những kẻ quyền thế ra hư không;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.446Z",
      "updatedAt": "2025-04-24T04:14:49.446Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "bbd8316c-f977-4ecd-8696-cf8e3edeafba",
      "number": 23,
      "content": "Những kẻ cầm quyền xét đoán trên đất trở thành hư ảo.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.462Z",
      "updatedAt": "2025-04-24T04:14:49.462Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "27736ff8-4f50-4369-9426-1c239194f447",
      "number": 24,
      "content": "Chúng vừa mới được trồng;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.476Z",
      "updatedAt": "2025-04-24T04:14:49.476Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "c18fb799-2601-46e7-852f-5edcc7231777",
      "number": 24,
      "content": "Chúng vừa mới được gieo;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.489Z",
      "updatedAt": "2025-04-24T04:14:49.489Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "e0fa58eb-1d67-476e-b155-d392f251b243",
      "number": 24,
      "content": "Chúng vừa đâm rễ vào đất;",
      "order": 2,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.502Z",
      "updatedAt": "2025-04-24T04:14:49.502Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "eac4fcc1-0bf1-421b-b769-5039fac770c4",
      "number": 24,
      "content": "Ngài thổi qua chúng một hơi,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.515Z",
      "updatedAt": "2025-04-24T04:14:49.515Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "2673578f-103e-48c1-9c1e-a5a5042bc918",
      "number": 24,
      "content": "Chúng liền héo khô ngay,",
      "order": 4,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.535Z",
      "updatedAt": "2025-04-24T04:14:49.535Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "5ccf96e5-447a-495e-9a58-36633cb641d7",
      "number": 24,
      "content": "Và một cơn lốc đùa chúng đi như gốc rạ.",
      "order": 5,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.548Z",
      "updatedAt": "2025-04-24T04:14:49.548Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "ba647d2f-e065-4065-bf87-89fc396da5b8",
      "number": 25,
      "content": "Ðấng Thánh phán hỏi:",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.563Z",
      "updatedAt": "2025-04-24T04:14:49.563Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "a3605fbe-a203-4edf-b554-beb2198f21e8",
      "number": 25,
      "content": "Các ngươi ví sánh Ta với ai?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.577Z",
      "updatedAt": "2025-04-24T04:14:49.577Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "6371d8fe-8e86-4cfd-ad52-a478db80f8f6",
      "number": 25,
      "content": "Các ngươi cho Ta bằng ai?",
      "order": 2,
      "parNumber": 11,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.590Z",
      "updatedAt": "2025-04-24T04:14:49.590Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "96b24b2c-b4ea-445b-9573-842fbaadb61e",
      "number": 26,
      "content": "Hãy ngước mắt nhìn lên trời,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.606Z",
      "updatedAt": "2025-04-24T04:14:49.606Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "658b977b-bcf9-40ae-8182-04094e6303e4",
      "number": 26,
      "content": "Xem ai đã tạo nên những thiên thể ấy?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.625Z",
      "updatedAt": "2025-04-24T04:14:49.625Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "81b7a2eb-76e1-4906-ab21-ea4a401d6a01",
      "number": 26,
      "content": "Ai đã làm chúng hiện hữu vô sốnhư vậy?",
      "order": 2,
      "parNumber": 11,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.639Z",
      "updatedAt": "2025-04-24T04:14:49.639Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "04252050-2992-4527-a608-6635e6237c7d",
      "number": 26,
      "content": "Thế mà Ngài gọi chúng theo từng tên riêng.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.654Z",
      "updatedAt": "2025-04-24T04:14:49.654Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "33d3c96a-c688-4990-afd9-0e68182b3e84",
      "number": 26,
      "content": "Nhờ quyền năng lớn lao của Ngài,",
      "order": 4,
      "parNumber": 11,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.667Z",
      "updatedAt": "2025-04-24T04:14:49.667Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "20440f23-865a-41ee-bd07-7c4f9326d9f8",
      "number": 26,
      "content": "Nhờ sức mạnh vô hạn của Ngài,",
      "order": 5,
      "parNumber": 11,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.680Z",
      "updatedAt": "2025-04-24T04:14:49.680Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "5dd6f194-5813-4ef8-ad8a-995861ed0eeb",
      "number": 26,
      "content": "Không một thiên thể nào bị bỏ sót.",
      "order": 6,
      "parNumber": 11,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.693Z",
      "updatedAt": "2025-04-24T04:14:49.693Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "7cc465aa-29ef-4665-a0d8-b523fb804906",
      "number": 27,
      "content": "Hỡi Gia-cốp, tại sao ngươi nói,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.707Z",
      "updatedAt": "2025-04-24T04:14:49.707Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "762ecffb-65d7-4d88-ad11-fa4f05c05fa3",
      "number": 27,
      "content": "Hỡi I-sơ-ra-ên, tại sao ngươi bảo:",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.721Z",
      "updatedAt": "2025-04-24T04:14:49.721Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "31758eb9-f49f-4d2d-88dd-40d6794407d2",
      "number": 27,
      "content": "“Ðường lối tôi đã bị che khuất khỏi mắt Chúa,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.734Z",
      "updatedAt": "2025-04-24T04:14:49.734Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "fc1550f5-4be5-4f8e-a262-21130d537f83",
      "number": 27,
      "content": "Công lý dành cho tôi Ðức Chúa Trời đã chẳng đoái hoài.”",
      "order": 3,
      "parNumber": 12,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.747Z",
      "updatedAt": "2025-04-24T04:14:49.747Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "d5640c28-f65b-4a52-bd5e-266a6ed46fa6",
      "number": 28,
      "content": "Các ngươi chưa biết sao?",
      "order": 0,
      "parNumber": 12,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.763Z",
      "updatedAt": "2025-04-24T04:14:49.763Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "1ceeb6eb-527d-4432-994f-45128e55aaec",
      "number": 28,
      "content": "Các ngươi chưa nghe sao?",
      "order": 1,
      "parNumber": 12,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.776Z",
      "updatedAt": "2025-04-24T04:14:49.776Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "72220cec-0ffe-4777-ab77-e4fc4b070789",
      "number": 28,
      "content": "Chúa là Ðức Chúa Trời đời đời,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.789Z",
      "updatedAt": "2025-04-24T04:14:49.789Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "076e8f3b-303d-402d-a812-44d901688797",
      "number": 28,
      "content": "Ðấng Tạo Hóa của toàn cả địa cầu;",
      "order": 3,
      "parNumber": 12,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.802Z",
      "updatedAt": "2025-04-24T04:14:49.802Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "6f985e14-2f2c-4d98-9866-3d3e7bea8cf9",
      "number": 28,
      "content": "Ngài không kiệt lực và không mệt mỏi;",
      "order": 4,
      "parNumber": 12,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.814Z",
      "updatedAt": "2025-04-24T04:14:49.814Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "b3eb3b74-926b-42b3-8a77-f4dd53bf9364",
      "number": 28,
      "content": "Trí tuệ Ngài thật khôn dò.",
      "order": 5,
      "parNumber": 12,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.826Z",
      "updatedAt": "2025-04-24T04:14:49.826Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "c4e4da54-e101-4ef1-991a-da2bf5e1a96f",
      "number": 29,
      "content": "Ngài ban năng lực cho người kiệt lực;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.843Z",
      "updatedAt": "2025-04-24T04:14:49.843Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "842dc775-5bcf-4d29-81dc-bdcba476f401",
      "number": 29,
      "content": "Ngài ban sức mạnh cho người đuối sức.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.856Z",
      "updatedAt": "2025-04-24T04:14:49.856Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "b6fd6d77-7498-4c82-849e-3781ef6d0482",
      "number": 30,
      "content": "Ngay cả những người trẻ tuổi cũng sẽ kiệt lực và mệt mỏi;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.869Z",
      "updatedAt": "2025-04-24T04:14:49.869Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "863ef59b-55b5-48b9-b0b0-87bfefaccdb1",
      "number": 30,
      "content": "Các thanh niên cũng sẽ đuối sức ngã dài;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.882Z",
      "updatedAt": "2025-04-24T04:14:49.882Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "e1115987-701f-496a-a8da-3fc05cf1b985",
      "number": 31,
      "content": "Nhưng ai trông cậy Chúa sẽ được sức mới;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.896Z",
      "updatedAt": "2025-04-24T04:14:49.896Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "cc6f6358-0692-43a9-8289-2b1fbc677018",
      "number": 31,
      "content": "Họ sẽ cất cánh bay cao như phượng hoàng;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.909Z",
      "updatedAt": "2025-04-24T04:14:49.909Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "64200c6d-0d44-4586-a1e1-7f0110ba8716",
      "number": 31,
      "content": "Họ sẽ chạy mà không mệt mỏi;",
      "order": 2,
      "parNumber": 12,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.923Z",
      "updatedAt": "2025-04-24T04:14:49.923Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    },
    {
      "id": "ad90f6d9-bf63-4814-9f7c-bc5dc3f696db",
      "number": 31,
      "content": "Họ sẽ đi mà không kiệt lực.",
      "order": 3,
      "parNumber": 12,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:14:49.937Z",
      "updatedAt": "2025-04-24T04:14:49.937Z",
      "chapterId": "80e90c35-4212-43fd-8f75-3f5a2b56f2ce"
    }
  ],
