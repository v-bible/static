{
  "chapter": {
    "id": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f",
    "number": 52,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2052&version=BD2011",
    "createdAt": "2025-04-24T03:03:21.423Z",
    "updatedAt": "2025-04-24T03:03:21.423Z",
    "bookId": "e4e58f0e-ae57-420f-8009-a5608bc5a58a"
  },
  "verses": [
    {
      "id": "53c02a61-d0e2-42a7-81c1-6c219410a817",
      "number": 1,
      "content": "Hỡi Si-ôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:04.896Z",
      "updatedAt": "2025-04-24T04:16:04.896Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "89d4a913-84d5-40b1-b7a8-ae7a30a546ea",
      "number": 1,
      "content": "Hãy thức dậy! Hãy thức dậy!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:04.930Z",
      "updatedAt": "2025-04-24T04:16:04.930Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "0ce39a7d-f35a-49f9-8db4-b96077895e32",
      "number": 1,
      "content": "Hãy mặc lấy sức lực của ngươi;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:04.942Z",
      "updatedAt": "2025-04-24T04:16:04.942Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "f710e7db-a8c9-4a3c-be67-8c027d5d82ef",
      "number": 1,
      "content": "Hỡi Giê-ru-sa-lem thành thánh,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:04.955Z",
      "updatedAt": "2025-04-24T04:16:04.955Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "fba5eec2-e6ff-438f-90fb-4eff2f0e0057",
      "number": 1,
      "content": "Hãy mặc lấy y phục đẹp đẽ của ngươi;",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:04.969Z",
      "updatedAt": "2025-04-24T04:16:04.969Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "036b3541-6f06-490d-a44b-44c93c474e80",
      "number": 1,
      "content": "Vì những kẻ không được cắt bì và những kẻ ô uế sẽ không vào giữa ngươi nữa.",
      "order": 5,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:04.982Z",
      "updatedAt": "2025-04-24T04:16:04.982Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "d848a6a5-06a1-484f-a5bd-165fc6ea0d46",
      "number": 2,
      "content": "Hỡi Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:04.996Z",
      "updatedAt": "2025-04-24T04:16:04.996Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "c9103e0c-9d14-41fe-90bd-f1e01cf4a379",
      "number": 2,
      "content": "Hãy đứng dậy và phủi sạch bụi trên mình,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.012Z",
      "updatedAt": "2025-04-24T04:16:05.012Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "fcc1177b-63e9-48a4-ba93-49fb2b3c1a71",
      "number": 2,
      "content": "Rồi ngồi xuống trị vì;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.026Z",
      "updatedAt": "2025-04-24T04:16:05.026Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "7068fa55-c58d-48a3-8ce8-98ba1e31cdda",
      "number": 2,
      "content": "Hỡi ái nữ của Si-ôn đang bị lưu đày,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.039Z",
      "updatedAt": "2025-04-24T04:16:05.039Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "added575-9b4b-4414-bb27-7595ae49ffce",
      "number": 2,
      "content": "Hãy vứt bỏ xiềng xích khỏi cổ ngươi.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.052Z",
      "updatedAt": "2025-04-24T04:16:05.052Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "0fb4ed4e-d32b-4fde-8026-999543833fd7",
      "number": 3,
      "content": "Vì Chúa phán thế này:",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.065Z",
      "updatedAt": "2025-04-24T04:16:05.065Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "7eef1164-4038-432b-a798-495928a0faf2",
      "number": 3,
      "content": "“Các ngươi đã bán mình mà không nhận một đồng nào,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.079Z",
      "updatedAt": "2025-04-24T04:16:05.079Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "27aef11f-0e86-4186-8723-42b165ac8047",
      "number": 3,
      "content": "Nên các ngươi sẽ được chuộc lại mà khỏi phải trả tiền.”",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.092Z",
      "updatedAt": "2025-04-24T04:16:05.092Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "df45b2c8-960b-456e-b8fd-d36e4dbc4c58",
      "number": 4,
      "content": "Vì Chúa Hằng Hữu phán thế này:",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.106Z",
      "updatedAt": "2025-04-24T04:16:05.106Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "93c6ac0f-af12-40ce-9ae9-33793f4e2a53",
      "number": 4,
      "content": "“Ban đầu dân Ta xuống Ai-cập và kiều ngụ tại đó,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.119Z",
      "updatedAt": "2025-04-24T04:16:05.119Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "4dbfd4e7-9e28-4794-b078-ea75e5446062",
      "number": 4,
      "content": "Về sau dân A-sy-ri đến đàn áp chúng.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.132Z",
      "updatedAt": "2025-04-24T04:16:05.132Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "1ea6d1b8-aaa0-49d0-9535-52a9e2025966",
      "number": 5,
      "content": "Thế thì bây giờ Ta còn gì?”",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.145Z",
      "updatedAt": "2025-04-24T04:16:05.145Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "75406a26-ccef-49f2-8978-e675fe6df52c",
      "number": 5,
      "content": "Chúa phán.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.159Z",
      "updatedAt": "2025-04-24T04:16:05.159Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "1b2c4124-4a87-4c2c-854a-33ef86f76673",
      "number": 5,
      "content": "“Phải chăng dân Ta đã bị bắt đi mà không tốn kém gì?",
      "order": 2,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.172Z",
      "updatedAt": "2025-04-24T04:16:05.172Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "b7180b2e-ac8a-4399-baf0-889c57f6f68e",
      "number": 5,
      "content": "Những kẻ cai trị chúng đã nhiếc móc chúng,”",
      "order": 3,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.185Z",
      "updatedAt": "2025-04-24T04:16:05.185Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "4a7ae4ed-40cf-4b73-bf08-808cd3a96f34",
      "number": 5,
      "content": "Chúa phán,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.198Z",
      "updatedAt": "2025-04-24T04:16:05.198Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "8c14e6f3-4e82-467e-854c-3d046d50738f",
      "number": 5,
      "content": "“Và danh Ta đã bị xúc phạm suốt ngày.",
      "order": 5,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.211Z",
      "updatedAt": "2025-04-24T04:16:05.211Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "4bba90a4-0105-4d1b-a80f-9efa448f8a20",
      "number": 6,
      "content": "Vì thế dân Ta sẽ biết danh Ta,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.224Z",
      "updatedAt": "2025-04-24T04:16:05.224Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "e58d8bee-5142-43e0-b926-35285f7adda2",
      "number": 6,
      "content": "Vì vậy trong ngày ấy chúng sẽ biết rằng,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.237Z",
      "updatedAt": "2025-04-24T04:16:05.237Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "956a78eb-c273-401c-ad2c-fd0a98859286",
      "number": 6,
      "content": "Chính Ta là Ðấng đã phán, ‘Này, có Ta đây.’”",
      "order": 2,
      "parNumber": 1,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.248Z",
      "updatedAt": "2025-04-24T04:16:05.248Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "bcf98bb9-fbfa-4057-bfdd-3d7fc2dba808",
      "number": 7,
      "content": "Bàn chân của người mang tin mừng trên các núi đẹp biết bao!",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.263Z",
      "updatedAt": "2025-04-24T04:16:05.263Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "1323e5f2-72c0-4863-8f13-07752483bb2e",
      "number": 7,
      "content": "Ấy là người rao báo tin hòa bình thịnh vượng,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.275Z",
      "updatedAt": "2025-04-24T04:16:05.275Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "06018dac-8379-440c-9673-b452947a4b3e",
      "number": 7,
      "content": "Ðem tin vui phước hạnh,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.287Z",
      "updatedAt": "2025-04-24T04:16:05.287Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "ec1bbf0d-5ca4-403e-a7b8-2d8b1e1b9024",
      "number": 7,
      "content": "Loan truyền ơn cứu rỗi,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.298Z",
      "updatedAt": "2025-04-24T04:16:05.298Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "311f3819-855f-4d2a-b90e-aab36ef5c191",
      "number": 7,
      "content": "Và nói cho Si-ôn biết,",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.309Z",
      "updatedAt": "2025-04-24T04:16:05.309Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "91604f1a-3f90-4d13-a03a-4f308a534abf",
      "number": 7,
      "content": "“Ðức Chúa Trời của ngươi trị vì!”",
      "order": 5,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.321Z",
      "updatedAt": "2025-04-24T04:16:05.321Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "37a01c33-2a0f-45e0-afcb-955b80e5e338",
      "number": 8,
      "content": "Những người canh gác của ngươi sẽ cất tiếng reo hò,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.334Z",
      "updatedAt": "2025-04-24T04:16:05.334Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "f82f4c5e-ff3d-4af9-b44f-7e74a813e06e",
      "number": 8,
      "content": "Họ sẽ cùng nhau trỗi tiếng hoan ca,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.347Z",
      "updatedAt": "2025-04-24T04:16:05.347Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "128dba24-bcdf-4133-ac9d-63922862efd1",
      "number": 8,
      "content": "Bởi chính mắt họ sẽ thấy,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.360Z",
      "updatedAt": "2025-04-24T04:16:05.360Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "217d4114-2342-4904-a0ed-ed18a1b82ad8",
      "number": 8,
      "content": "Khi Chúa trở về với Si-ôn.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.374Z",
      "updatedAt": "2025-04-24T04:16:05.374Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "7665c263-e616-45dd-b2ff-620612dd4b3d",
      "number": 9,
      "content": "Hãy bật lên tiếng vui mừng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.387Z",
      "updatedAt": "2025-04-24T04:16:05.387Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "780e85b8-86c8-49ae-b179-37090d816023",
      "number": 9,
      "content": "Hãy cùng nhau ca hát,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.400Z",
      "updatedAt": "2025-04-24T04:16:05.400Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "e2bd66c0-5044-4a29-9b33-21c825475b68",
      "number": 9,
      "content": "Hỡi các ngươi, những nơi hoang phế của Giê-ru-sa-lem!",
      "order": 2,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.413Z",
      "updatedAt": "2025-04-24T04:16:05.413Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "f43a27c6-9575-49f5-8cb9-685251a8586a",
      "number": 9,
      "content": "Vì Chúa đã an ủi dân Ngài,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.427Z",
      "updatedAt": "2025-04-24T04:16:05.427Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "9967dc7f-e24c-41bf-8924-2ddb0503891c",
      "number": 9,
      "content": "Ngài đã cứu chuộc Giê-ru-sa-lem.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.440Z",
      "updatedAt": "2025-04-24T04:16:05.440Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "93ad72cf-0c9d-429d-92ca-022cd5df4534",
      "number": 10,
      "content": "Trước mắt của mọi nước,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.453Z",
      "updatedAt": "2025-04-24T04:16:05.453Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "0b351091-cca2-4d59-978f-d056e1c73981",
      "number": 10,
      "content": "Chúa đã biểu dương cánh tay thánh quyền năng của Ngài;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.469Z",
      "updatedAt": "2025-04-24T04:16:05.469Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "f88afcfc-dfa0-4e8c-9377-8b8b78728bc7",
      "number": 10,
      "content": "Bấy giờ mọi nơi xa xôi hẻo lánh trên mặt đất sẽ thấy ơn cứu rỗi của Ðức Chúa Trời chúng ta.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.482Z",
      "updatedAt": "2025-04-24T04:16:05.482Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "ca77aa23-7abe-4a20-9f0c-526e76e3e87b",
      "number": 11,
      "content": "Hãy ra đi! Hãy ra đi!",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.495Z",
      "updatedAt": "2025-04-24T04:16:05.495Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "64680f97-5353-4659-9f0d-60bc392da37d",
      "number": 11,
      "content": "Hãy đi khỏi nơi đó!",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.508Z",
      "updatedAt": "2025-04-24T04:16:05.508Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "adc89099-020c-43f0-8fb5-45a8306d1c1f",
      "number": 11,
      "content": "Ðừng đụng đến đồ ô uế.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.522Z",
      "updatedAt": "2025-04-24T04:16:05.522Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "2d430523-9de7-4837-b19d-a5e4a26ddb83",
      "number": 11,
      "content": "Hãy ra khỏi nơi ấy.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.535Z",
      "updatedAt": "2025-04-24T04:16:05.535Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "4520fa37-8887-4d80-abf2-4181cfd89b94",
      "number": 11,
      "content": "Hãy làm cho mình ra thanh sạch,",
      "order": 4,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.548Z",
      "updatedAt": "2025-04-24T04:16:05.548Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "36ef15f3-8bdb-45ee-bc01-5e26f44bbbc8",
      "number": 11,
      "content": "Hỡi các ngươi là những người mang các vật dụng để thờ phượng Chúa.",
      "order": 5,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.561Z",
      "updatedAt": "2025-04-24T04:16:05.561Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "6dc05ef7-6a74-4f2b-889e-4137b186d130",
      "number": 12,
      "content": "Vì các ngươi sẽ ra đi, nhưng không cần vội vã,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.574Z",
      "updatedAt": "2025-04-24T04:16:05.574Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "a178fcb0-91f6-4d30-9b38-204c60b7ca04",
      "number": 12,
      "content": "Không phải ra đi như chạy trốn;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.587Z",
      "updatedAt": "2025-04-24T04:16:05.587Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "085b2b94-8546-4a03-bdd8-a1ce39aeccb2",
      "number": 12,
      "content": "Vì Chúa sẽ đi trước mở đường cho các ngươi,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.600Z",
      "updatedAt": "2025-04-24T04:16:05.600Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "f88d4d1f-6c7c-4251-b426-91b23663c01a",
      "number": 12,
      "content": "Và Ðức Chúa Trời của I-sơ-ra-ên sẽ đi sau để hộ tống các ngươi.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.614Z",
      "updatedAt": "2025-04-24T04:16:05.614Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "d24ad934-849e-4d30-b1e8-81edcc774cee",
      "number": 13,
      "content": "Này, Ðầy Tớ Ta sẽ hành động khôn ngoan;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.628Z",
      "updatedAt": "2025-04-24T04:16:05.628Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "c5e88ad6-6f7f-4201-a371-7d582996a59d",
      "number": 13,
      "content": "Người sẽ được tôn cao, tán tụng, và suy tôn đến tột cùng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.654Z",
      "updatedAt": "2025-04-24T04:16:05.654Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "2f9a8ed4-bbb5-4c36-a57d-a0d7d8e29a7c",
      "number": 14,
      "content": "Như bao kẻ đã ngạc nhiên khi trông thấy Người,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.667Z",
      "updatedAt": "2025-04-24T04:16:05.667Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "b842fe5a-bb9f-4869-b7c3-fb2f260f28dd",
      "number": 14,
      "content": "Vì mặt mày và hình dáng Người rất tiều tụy, trông chẳng ra người,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.681Z",
      "updatedAt": "2025-04-24T04:16:05.681Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "ad369c49-e986-4c29-98fd-64577af91f0d",
      "number": 15,
      "content": "Thì cũng thế, Người sẽ làm cho muôn dân phải bàng hoàng sửng sốt;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.694Z",
      "updatedAt": "2025-04-24T04:16:05.694Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "3ae95873-9637-461a-925a-c3292d194552",
      "number": 15,
      "content": "Các vua phải ngậm miệng vì Người;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.707Z",
      "updatedAt": "2025-04-24T04:16:05.707Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "7195f7a3-2ce1-484c-bd8d-8ca7363295f6",
      "number": 15,
      "content": "Bởi họ sẽ thấy những gì họ chưa hề nghe nói,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.722Z",
      "updatedAt": "2025-04-24T04:16:05.722Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    },
    {
      "id": "44a3d471-313a-41e5-a401-acaeabe313d2",
      "number": 15,
      "content": "Họ sẽ hiểu những gì họ chưa nghĩ đến bao giờ.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2025-04-24T04:16:05.737Z",
      "updatedAt": "2025-04-24T04:16:05.737Z",
      "chapterId": "41c797cc-7e65-4e9c-aa04-fc9bf4ffef4f"
    }
  ],
