  "headings": [
    {
      "id": "ca77f62d-b70b-4393-8432-47ca5ebc3a00",
      "content": "Báo động",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:12:39.414Z",
      "updatedAt": "2025-04-22T07:12:39.414Z",
      "verseId": "676832b0-68f9-4690-a6a8-ae38bcdd33e2",
      "chapterId": "0120b7b8-4858-41a0-84ba-2619a31b8342"
    },
    {
      "id": "5bd19c50-4d22-4d26-9dad-254fb4c81838",
      "content": "Xã hội hỗn loạn vì dân Chúa thờ ngẫu tượng",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:12:39.606Z",
      "updatedAt": "2025-04-22T07:12:39.606Z",
      "verseId": "7bc5972f-b027-4091-ab7f-33d81c3a6134",
      "chapterId": "0120b7b8-4858-41a0-84ba-2619a31b8342"
    },
    {
      "id": "dbc9ad4f-76d4-4de0-b405-c836a097b4f0",
      "content": "Chống nền phụng tự vụ hình thức",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:12:40.045Z",
      "updatedAt": "2025-04-22T07:12:40.045Z",
      "verseId": "ab084404-8edc-4d68-87ee-480316e14652",
      "chapterId": "0120b7b8-4858-41a0-84ba-2619a31b8342"
    },
    {
      "id": "46a79cc3-97c9-47ac-aa2a-4cd6e01b1236",
      "content": "Ít-ra-en bị tiêu diệt vì cầu cứu ngoại bang",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:12:39.849Z",
      "updatedAt": "2025-04-22T07:12:39.849Z",
      "verseId": "c79bd400-3999-4828-8dfa-7b95a9e78da6",
      "chapterId": "0120b7b8-4858-41a0-84ba-2619a31b8342"
    }
  ],
