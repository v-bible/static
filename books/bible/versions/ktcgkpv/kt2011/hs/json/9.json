{
  "chapter": {
    "id": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276",
    "number": 9,
    "ref": "",
    "createdAt": "2025-04-22T00:48:32.383Z",
    "updatedAt": "2025-04-22T00:48:32.383Z",
    "bookId": "bc1f8449-5c82-4714-8441-bed4d1ab970c"
  },
  "verses": [
    {
      "id": "4e4c3804-880e-48d8-ba95-8bc7deee3d5c",
      "number": 1,
      "content": "Hỡi Ít-ra-en, chớ vui cười,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.113Z",
      "updatedAt": "2025-04-22T07:13:04.113Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "71b537f7-9772-4364-a4e4-41c71bddf15d",
      "number": 1,
      "content": "đừng hớn hở như phường dân ngoại.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.142Z",
      "updatedAt": "2025-04-22T07:13:04.142Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "8372883c-a52a-4703-9dee-bb16cf49000d",
      "number": 1,
      "content": "Ngươi đã bỏ Thiên Chúa ngươi mà làm điếm",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.167Z",
      "updatedAt": "2025-04-22T07:13:04.167Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "c9b80063-8164-4240-8dd0-6f44f92f1022",
      "number": 1,
      "content": "và mong được trả công trên mọi sân đập lúa.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.180Z",
      "updatedAt": "2025-04-22T07:13:04.180Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "59490557-847b-4b53-8b69-764416d3b0e0",
      "number": 2,
      "content": "Lúa trong sân, nho trong bồn, chúng sẽ không được hưởng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.195Z",
      "updatedAt": "2025-04-22T07:13:04.195Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "8d273003-8163-41fb-be30-454229877ccd",
      "number": 2,
      "content": "càng mong rượu mới, chúng càng thất vọng ê chề.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.223Z",
      "updatedAt": "2025-04-22T07:13:04.223Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "35baf6aa-7ccf-4755-955c-4b8cf5017696",
      "number": 3,
      "content": "Không được ở trên đất của ĐỨC CHÚA,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.237Z",
      "updatedAt": "2025-04-22T07:13:04.237Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "fa2fae73-aba9-40be-80b9-bd82f006d05c",
      "number": 3,
      "content": "Ép-ra-im sẽ phải trở về Ai-cập,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.266Z",
      "updatedAt": "2025-04-22T07:13:04.266Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "c0f06721-561b-453d-95d3-f01164d9ed5a",
      "number": 3,
      "content": "và phải dùng thức ăn ô uế tại Át-sua.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.295Z",
      "updatedAt": "2025-04-22T07:13:04.295Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "4eda0c29-67b7-4d95-967c-b1f01c763b97",
      "number": 4,
      "content": "Chúng sẽ không được rót rượu tế dâng ĐỨC CHÚA,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.310Z",
      "updatedAt": "2025-04-22T07:13:04.310Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "0d3eaf89-8431-4483-97e5-2f91a729b102",
      "number": 4,
      "content": "các hy lễ của chúng cũng chẳng đẹp lòng Người.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.336Z",
      "updatedAt": "2025-04-22T07:13:04.336Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "08314420-c990-4821-8f9a-60ce32f6aeb8",
      "number": 4,
      "content": "Bánh chúng ăn khác nào bánh tại nhà có tang,",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.347Z",
      "updatedAt": "2025-04-22T07:13:04.347Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "35b25442-a407-4b2e-aee0-5d75fdb6cbf1",
      "number": 4,
      "content": "ai ăn vào cũng trở thành ô uế,",
      "order": 3,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.370Z",
      "updatedAt": "2025-04-22T07:13:04.370Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "537c8bea-fe8f-4ea7-a63f-6b114770559e",
      "number": 4,
      "content": "vì bánh của chúng chỉ để chúng nuôi thân,",
      "order": 4,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.382Z",
      "updatedAt": "2025-04-22T07:13:04.382Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "85dfe6ee-7534-41ee-bbb7-74d6a00d3e65",
      "number": 4,
      "content": "chứ không được đưa vào nhà ĐỨC CHÚA.",
      "order": 5,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.396Z",
      "updatedAt": "2025-04-22T07:13:04.396Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "93a0c48a-57f7-4f36-8909-234b39945512",
      "number": 5,
      "content": "Các ngươi sẽ làm gì trong ngày hội, ngày lễ của ĐỨC CHÚA ?",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.424Z",
      "updatedAt": "2025-04-22T07:13:04.424Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "0032d0e7-00bd-453f-bea5-12e37154c669",
      "number": 6,
      "content": "Vì này chúng sẽ phải ra đi sau khi đất nước bị tàn phá.",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.453Z",
      "updatedAt": "2025-04-22T07:13:04.453Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "8d40ebcb-aad5-4e8b-ba37-52d225d7b879",
      "number": 6,
      "content": "Chúng sẽ bị Ai-cập tập trung, và sẽ bị Mốp chôn vùi.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.467Z",
      "updatedAt": "2025-04-22T07:13:04.467Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "8ba717ed-44f5-4dbe-a82f-d55995454b68",
      "number": 6,
      "content": "Cỏ dại mọc um tùm trên đồ bạc chúng coi là quý giá,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.496Z",
      "updatedAt": "2025-04-22T07:13:04.496Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "a700349b-c526-4cc9-af5e-f0cecb2cc675",
      "number": 6,
      "content": "gai góc nằm ngổn ngang ngay trong lều chúng ở.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.510Z",
      "updatedAt": "2025-04-22T07:13:04.510Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "42268ceb-1593-4468-9c0d-7946affe9fbc",
      "number": 7,
      "content": "Đã đến rồi những ngày trừng phạt,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.524Z",
      "updatedAt": "2025-04-22T07:13:04.524Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "092ad445-60da-4b85-9de1-a42aa71ffd63",
      "number": 7,
      "content": "đã đến rồi những ngày báo oán : Ít-ra-en sẽ thấy điều này !",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.565Z",
      "updatedAt": "2025-04-22T07:13:04.565Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "b6a6620d-0183-41eb-85f3-9358f34ad5b5",
      "number": 7,
      "content": "– “Ngôn sứ điên rồi ! Người được linh hứng nói sảng !”",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.579Z",
      "updatedAt": "2025-04-22T07:13:04.579Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "a4b29e07-f87d-46e4-85c2-738522ee80f5",
      "number": 7,
      "content": "Ngươi nói vậy, vì tội ác của ngươi tày trời,",
      "order": 3,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.607Z",
      "updatedAt": "2025-04-22T07:13:04.607Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "4fe1be4a-f790-46a2-a391-0ee7a89d6f7a",
      "number": 7,
      "content": "ngươi lại còn bày đủ trò khủng bố.",
      "order": 4,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.621Z",
      "updatedAt": "2025-04-22T07:13:04.621Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "a5e11508-230f-4a2b-a9f1-5f33a9a89464",
      "number": 8,
      "content": "Ngôn sứ là người canh gác Ép-ra-im, vẫn ở cùng Thiên Chúa,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.635Z",
      "updatedAt": "2025-04-22T07:13:04.635Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "93d5abd6-2457-45f5-94a9-d3b89fd33ede",
      "number": 8,
      "content": "thế mà thiên hạ lại giăng bẫy trên khắp mọi nẻo đường ông đi,",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.650Z",
      "updatedAt": "2025-04-22T07:13:04.650Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "fd61984f-6a3a-4442-a937-d2dc97f9af80",
      "number": 8,
      "content": "khủng bố ông ngay trong nhà Thiên Chúa.",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.678Z",
      "updatedAt": "2025-04-22T07:13:04.678Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "d57e5b95-5a65-4b10-b6d3-d4141d921421",
      "number": 9,
      "content": "Chúng chìm sâu trong vũng lầy hư hỏng như hồi ở Ghíp-a.",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.706Z",
      "updatedAt": "2025-04-22T07:13:04.706Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "fa1774f7-21f4-4ff6-a781-6e3287c3135d",
      "number": 9,
      "content": "Người không quên điều gian ác chúng làm,",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.734Z",
      "updatedAt": "2025-04-22T07:13:04.734Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "233bfd86-609f-41e5-b50d-2e6091b2a282",
      "number": 9,
      "content": "nhưng sẽ trị tội chúng.",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.761Z",
      "updatedAt": "2025-04-22T07:13:04.761Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "01171af1-7060-438e-bd0f-641d51e12050",
      "number": 10,
      "content": "Ta đã gặp được Ít-ra-en, khác nào chùm nho trong sa mạc,",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.775Z",
      "updatedAt": "2025-04-22T07:13:04.775Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "c83d7b8b-f76a-4d6c-b583-e29257c39d7d",
      "number": 10,
      "content": "Ta đã xem cha ông các ngươi",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.818Z",
      "updatedAt": "2025-04-22T07:13:04.818Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "3e477f35-e236-4339-8202-4ff61971e387",
      "number": 10,
      "content": "như trái đầu mùa trên cây vả mới bói.",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.832Z",
      "updatedAt": "2025-04-22T07:13:04.832Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "c3541816-f5f5-4c00-b20c-17c25fe8463d",
      "number": 10,
      "content": "Nhưng phần chúng, khi đến Ba-an Pơ-o,",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.860Z",
      "updatedAt": "2025-04-22T07:13:04.860Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "ee0985cf-85cc-457e-9c15-48773c48b9b0",
      "number": 10,
      "content": "chúng lại gắn bó với các thần ô nhục,",
      "order": 4,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.903Z",
      "updatedAt": "2025-04-22T07:13:04.903Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "6f2ee17a-e61b-4c9d-a38f-3f07ef99ae5f",
      "number": 10,
      "content": "và trở thành đồ ghê tởm như những thần chúng yêu mến.",
      "order": 5,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.917Z",
      "updatedAt": "2025-04-22T07:13:04.917Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "3a1b7e74-e253-4c82-be94-b570d739d278",
      "number": 11,
      "content": "Vinh quang của Ép-ra-im như cánh chim vụt bay :",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.945Z",
      "updatedAt": "2025-04-22T07:13:04.945Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "5c8a0e97-05e3-4912-b390-8f3f7d2adbff",
      "number": 11,
      "content": "không còn thai nghén, chẳng còn sinh nở.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.959Z",
      "updatedAt": "2025-04-22T07:13:04.959Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "90120085-1177-4353-ae9b-4a72abc85684",
      "number": 12,
      "content": "Giả như chúng có nuôi con cho khôn lớn,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:04.988Z",
      "updatedAt": "2025-04-22T07:13:04.988Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "14b06eac-bc7c-46db-a358-34b0252cec40",
      "number": 12,
      "content": "Ta cũng không để cho thành người.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.016Z",
      "updatedAt": "2025-04-22T07:13:05.016Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "a3e6136d-2257-4832-8449-e2f5c85547f5",
      "number": 12,
      "content": "Thật khốn nạn cho chúng khi bị Ta từ bỏ.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.029Z",
      "updatedAt": "2025-04-22T07:13:05.029Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "0d517811-bace-4432-8185-8183620b718e",
      "number": 13,
      "content": "Xưa Ta thấy Ép-ra-im như thành Tia toạ lạc trong đồng cỏ,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.043Z",
      "updatedAt": "2025-04-22T07:13:05.043Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "bc6f430b-19c9-4a1e-99df-59b90de42a83",
      "number": 13,
      "content": "nhưng nay Ép-ra-im",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.057Z",
      "updatedAt": "2025-04-22T07:13:05.057Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "978b0617-b031-45c4-8a35-d33a3c49e4bd",
      "number": 13,
      "content": "sắp phải nộp con mình cho phường khát máu.",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.071Z",
      "updatedAt": "2025-04-22T07:13:05.071Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "3f3ce3f3-b4ca-4f69-b6ca-00b805888d3d",
      "number": 14,
      "content": "Lạy ĐỨC CHÚA, xin Ngài làm cho chúng...",
      "order": 0,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.085Z",
      "updatedAt": "2025-04-22T07:13:05.085Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "29159031-8d15-4d68-b964-cc89494367ff",
      "number": 14,
      "content": "Ngài sẽ làm gì cho chúng đây ?",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.113Z",
      "updatedAt": "2025-04-22T07:13:05.113Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "afb99347-a417-4b2f-8d0a-3bac79bec06d",
      "number": 14,
      "content": "Xin cho chúng không còn sinh nở, hết cho bú mớm.",
      "order": 2,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.128Z",
      "updatedAt": "2025-04-22T07:13:05.128Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "e0391e2d-f9d8-42f3-8b0c-adaae2b56d8f",
      "number": 15,
      "content": "Mọi tội ác của chúng lộ hết ở Ghin-gan.",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.161Z",
      "updatedAt": "2025-04-22T07:13:05.161Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "84c8ffaa-7546-4252-8fdb-451bee5e506b",
      "number": 15,
      "content": "Tại nơi này Ta chê ghét chúng.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.198Z",
      "updatedAt": "2025-04-22T07:13:05.198Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "004f7a56-759a-433a-af4e-7c9598007e5e",
      "number": 15,
      "content": "Những hành vi gian ác của chúng",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.212Z",
      "updatedAt": "2025-04-22T07:13:05.212Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "5f60819c-88e6-4fd4-8164-98ae314fb671",
      "number": 15,
      "content": "khiến Ta xua đuổi chúng khỏi nhà Ta.",
      "order": 3,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.226Z",
      "updatedAt": "2025-04-22T07:13:05.226Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "1f2344cc-f558-49ab-bf49-a56d9d415de0",
      "number": 15,
      "content": "Ta không còn yêu thương chúng nữa.",
      "order": 4,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.254Z",
      "updatedAt": "2025-04-22T07:13:05.254Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "333483e2-f2d2-412e-8fc1-dde911458264",
      "number": 15,
      "content": "Hết thảy các thủ lãnh của chúng đều là quân phản bội.",
      "order": 5,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.282Z",
      "updatedAt": "2025-04-22T07:13:05.282Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "1210d64a-f70e-4912-98e5-bc44cc193f3f",
      "number": 16,
      "content": "Ép-ra-im đã bị đánh nhừ đòn, gốc rễ của chúng bị héo khô,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.296Z",
      "updatedAt": "2025-04-22T07:13:05.296Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "eecf76b5-c684-4748-bb8d-ab52420f8005",
      "number": 16,
      "content": "chúng không mang lại hoa trái gì được.",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.328Z",
      "updatedAt": "2025-04-22T07:13:05.328Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "aa96a3fd-09c3-47a5-a0b0-11386f1ec55d",
      "number": 16,
      "content": "Và giả như chúng có sinh sản, Ta cũng sẽ giết chết",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.342Z",
      "updatedAt": "2025-04-22T07:13:05.342Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "4ef5bd95-8969-4982-ad4e-283f4c1a5124",
      "number": 16,
      "content": "những đứa con yêu quý chúng đã cưu mang.",
      "order": 3,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.371Z",
      "updatedAt": "2025-04-22T07:13:05.371Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "fb4ee5e8-72b3-43ec-b4cc-dc5d120643a8",
      "number": 17,
      "content": "Thiên Chúa của tôi sẽ xua đuổi chúng,",
      "order": 0,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.385Z",
      "updatedAt": "2025-04-22T07:13:05.385Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "d80420b8-f348-4f05-aa26-53e1240e2969",
      "number": 17,
      "content": "vì chúng không chịu nghe lời Người ;",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.414Z",
      "updatedAt": "2025-04-22T07:13:05.414Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "1b337638-908b-489f-8359-a968130e4c71",
      "number": 17,
      "content": "giữa chư dân, chúng sẽ phải lang thang phiêu bạt.",
      "order": 2,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:13:05.428Z",
      "updatedAt": "2025-04-22T07:13:05.428Z",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    }
  ],
