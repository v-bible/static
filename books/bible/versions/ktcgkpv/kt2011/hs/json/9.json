  "headings": [
    {
      "id": "9374b604-9756-4f00-bc30-82366392aae2",
      "content": "Trừng phạt tội ác tại Ba-an Pơ-o",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:13:04.790Z",
      "updatedAt": "2025-04-22T07:13:04.790Z",
      "verseId": "01171af1-7060-438e-bd0f-641d51e12050",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "db3b54f6-2935-447e-a6bd-c8363ced020f",
      "content": "Ngôn sứ bị khủng bố vì loan báo cuộc trừng phạt",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:13:04.538Z",
      "updatedAt": "2025-04-22T07:13:04.538Z",
      "verseId": "42268ceb-1593-4468-9c0d-7946affe9fbc",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "2f1db0ef-c576-4828-8cf9-f4a4b418e5f9",
      "content": "Sầu khổ ở chốn lưu đày",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:13:04.131Z",
      "updatedAt": "2025-04-22T07:13:04.131Z",
      "verseId": "4e4c3804-880e-48d8-ba95-8bc7deee3d5c",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    },
    {
      "id": "09b9c03c-b746-472c-ad66-77c4c9abd51e",
      "content": "Trừng phạt tội ác tại Ghin-gan",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:13:05.173Z",
      "updatedAt": "2025-04-22T07:13:05.173Z",
      "verseId": "e0391e2d-f9d8-42f3-8b0c-adaae2b56d8f",
      "chapterId": "033d8ce8-2b0f-47d3-a8c8-a28bfe6aa276"
    }
  ],
