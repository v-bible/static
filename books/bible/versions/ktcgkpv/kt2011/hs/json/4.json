    },
    {
      "id": "55ab526c-c127-4bfd-a206-9f1702ead01c",
      "content": "Tình trạng đồi bại chung",
      "level": 2,
      "order": 1,
      "createdAt": "2025-04-22T07:12:07.403Z",
      "updatedAt": "2025-04-22T07:12:07.403Z",
