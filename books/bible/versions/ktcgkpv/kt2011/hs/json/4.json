      "chapterId": "51b88d1d-2503-4e83-87a8-f873667e29a6"
    },
    {
      "id": "3326d101-f58d-4981-aa63-12251c2b612b",
      "content": "Lên án hàng tư tế",
      "level": 2,
      "order": 0,
      "createdAt": "2025-04-22T07:12:07.638Z",
      "updatedAt": "2025-04-22T07:12:07.638Z",
