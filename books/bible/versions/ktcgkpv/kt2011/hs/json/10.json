  "headings": [
    {
      "id": "af9db00c-08ea-46f4-bf7f-50b5aa39db2a",
      "content": "Ít-ra-en đã không đáp ứng điều ĐỨC CHÚA chờ đợi",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:13:17.639Z",
      "updatedAt": "2025-04-22T07:13:17.639Z",
      "verseId": "309f416e-f1aa-4d2d-a5ef-18883f7e8d4a",
      "chapterId": "f7c7d97d-0132-4170-972f-f41442bbe513"
    },
    {
      "id": "90166249-725a-4684-8c14-b661b363d2f7",
      "content": "Ngẫu tượng Ít-ra-en thờ sẽ bị phá huỷ",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:13:16.932Z",
      "updatedAt": "2025-04-22T07:13:16.932Z",
