      "chapterId": "21e1cd5d-46b1-4adb-9837-621ad8c72cc5"
    },
    {
      "id": "c23a8f93-9355-45c9-84fc-c6a02ce20de9",
      "content": "Liên minh với ngoại bang chỉ là hư ảo",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:12:21.249Z",
      "updatedAt": "2025-04-22T07:12:21.249Z",
