  "headings": [
    {
      "id": "1c1301cb-b26e-460e-8cdf-aff24ce30975",
      "content": "Chiến tranh huynh đệ tương tàn",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T07:12:20.951Z",
      "updatedAt": "2025-04-22T07:12:20.951Z",
