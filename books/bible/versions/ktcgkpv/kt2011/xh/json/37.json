      "chapterId": "a840b470-2395-49e3-8ffa-ebe1a044f44e"
    },
    {
      "id": "5579e04b-e5e2-4771-b98e-192398d40f9e",
      "content": "Trụ đèn",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T02:46:15.914Z",
      "updatedAt": "2025-04-22T02:46:15.914Z",
      "verseId": "4992e981-587d-425a-b230-f85a71b5a1cc",
      "chapterId": "a840b470-2395-49e3-8ffa-ebe1a044f44e"
    },
    {
      "id": "6fc32f0a-7436-4f60-86fd-5f5eae23941c",
      "content": "Bàn để bánh tiến",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T02:46:15.821Z",
      "updatedAt": "2025-04-22T02:46:15.821Z",
      "verseId": "cd1d7c30-ada0-4f6f-975c-9e105ec3da7e",
      "chapterId": "a840b470-2395-49e3-8ffa-ebe1a044f44e"
    },
    {
      "id": "d1139801-c695-4143-936a-40a52966c944",
      "content": "Hương án. Dầu tấn phong và hương thơm.",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T02:46:16.039Z",
      "updatedAt": "2025-04-22T02:46:16.039Z",
      "verseId": "dc672f1d-9998-4150-bb98-9cb55c09cd1a",
      "chapterId": "a840b470-2395-49e3-8ffa-ebe1a044f44e"
    }
  ],
