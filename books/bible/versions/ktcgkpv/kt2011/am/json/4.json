    },
    {
      "id": "6c1de4e0-3019-4c27-9498-ac0b900c0dd4",
      "number": 10,
      "content": "thế mà các ngươi vẫn không chịu trở về với Ta",
      "order": 6,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:44.027Z",
      "updatedAt": "2025-04-22T07:15:44.027Z",
