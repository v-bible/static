    },
    {
      "id": "0f337fe6-2ac3-4b46-af05-b44b96f8461c",
      "number": 9,
      "content": "– sấm ngôn của ĐỨC CHÚA.",
      "order": 5,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.869Z",
      "updatedAt": "2025-04-22T07:15:43.869Z",
