  },
  "verses": [
    {
      "id": "44112ed9-ec7d-4895-aeac-82bda279cbc1",
      "number": 1,
      "content": "Hãy nghe lời này,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:42.927Z",
      "updatedAt": "2025-04-22T07:15:42.927Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "02d1483b-31e2-47d6-b91e-23a414cf247a",
      "number": 1,
      "content": "hỡi các mụ bò cái xứ Ba-san trên vùng núi Sa-ma-ri !",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:42.981Z",
      "updatedAt": "2025-04-22T07:15:42.981Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "13802cc1-33bc-45ff-9beb-4ea2407997f5",
      "number": 1,
      "content": "Các ngươi ngược đãi kẻ yếu hèn, chà đạp người nghèo đói,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.022Z",
      "updatedAt": "2025-04-22T07:15:43.022Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "58b6d127-fac7-439a-8531-c4e586cfe4a8",
      "number": 1,
      "content": "và bảo các ông chồng của mình : “Đem rượu lại đây cho ta uống !”",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.036Z",
      "updatedAt": "2025-04-22T07:15:43.036Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "a1d7e47b-0607-4ee8-949c-201af5728dd2",
      "number": 2,
      "content": "Với danh nghĩa là Đấng Thánh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.064Z",
      "updatedAt": "2025-04-22T07:15:43.064Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "58d28986-cb5a-417b-8fe3-77df87c62567",
      "number": 2,
      "content": "ĐỨC CHÚA là Chúa Thượng đã thề rằng :",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.079Z",
      "updatedAt": "2025-04-22T07:15:43.079Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "521f724a-2bed-4b18-a79f-eed24f3cdaa1",
      "number": 2,
      "content": "Đây sắp đến những ngày người ta kéo các ngươi đi bằng lao móc",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.112Z",
      "updatedAt": "2025-04-22T07:15:43.112Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "798bb6e3-4241-4062-8368-0a4a60d78bed",
      "number": 2,
      "content": "và lấy lưỡi câu lôi những kẻ theo sau các ngươi.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.123Z",
      "updatedAt": "2025-04-22T07:15:43.123Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "a6d1cb99-e72d-422d-9734-3ca79e5c4ec4",
      "number": 3,
      "content": "Qua những lỗ hổng của tường thành bị phá,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.135Z",
      "updatedAt": "2025-04-22T07:15:43.135Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "6378e03e-ced9-4624-ac40-8c47bbf6ac8c",
      "number": 3,
      "content": "các ngươi sẽ phải chui ra, mạnh ai nấy chạy,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.149Z",
      "updatedAt": "2025-04-22T07:15:43.149Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "040c76b2-3f2d-41e9-a6ec-39251ddb1280",
      "number": 3,
      "content": "và sẽ bị dồn về phía Hác-môn – sấm ngôn của ĐỨC CHÚA.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.164Z",
      "updatedAt": "2025-04-22T07:15:43.164Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "b4aa7aa7-e62d-4cb4-97a6-a3de68ffb309",
      "number": 4,
      "content": "Hỡi con cái Ít-ra-en,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.221Z",
      "updatedAt": "2025-04-22T07:15:43.221Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "a53b0f36-7146-49fd-92b4-9becefb8648e",
      "number": 4,
      "content": "cứ đến Bết Ên mà phản bội,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.248Z",
      "updatedAt": "2025-04-22T07:15:43.248Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "10652994-2d4b-4c30-a223-b997e7489fc3",
      "number": 4,
      "content": "đến Ghin-gan mà phản bội nữa đi !",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.279Z",
      "updatedAt": "2025-04-22T07:15:43.279Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "68aef0ec-a8c7-4f2f-8766-e86d5546a6ec",
      "number": 4,
      "content": "Sáng sáng hãy tiến dâng hy lễ",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.323Z",
      "updatedAt": "2025-04-22T07:15:43.323Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "3c682738-df20-4dd3-8ef1-ca4eee49f12e",
      "number": 4,
      "content": "và cứ ba ngày lại dâng cúng lễ thập phân.",
      "order": 4,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.351Z",
      "updatedAt": "2025-04-22T07:15:43.351Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "09bf613a-0496-436e-860a-20694041f0cf",
      "number": 5,
      "content": "Cứ đốt bánh không men làm lễ tạ ơn,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.366Z",
      "updatedAt": "2025-04-22T07:15:43.366Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "54d1590c-9965-4ea6-95d4-3eb096ff757a",
      "number": 5,
      "content": "và những lễ phẩm tự nguyện các ngươi dâng,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.395Z",
      "updatedAt": "2025-04-22T07:15:43.395Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "355cf532-f597-4b01-9353-d1d8ad475a81",
      "number": 5,
      "content": "hãy rêu rao cho người ta biết, bởi các ngươi thích làm như vậy",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.409Z",
      "updatedAt": "2025-04-22T07:15:43.409Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "c5552dea-87ed-47f4-bcef-ea4b990b0a07",
      "number": 5,
      "content": "– sấm ngôn của ĐỨC CHÚA là Chúa Thượng.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.451Z",
      "updatedAt": "2025-04-22T07:15:43.451Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "33db10e1-ee84-4cd2-9f80-4fc015c50b36",
      "number": 6,
      "content": "Phần Ta, Ta đã làm cho các ngươi",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.481Z",
      "updatedAt": "2025-04-22T07:15:43.481Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "63c5a8e7-36ca-4067-896f-02d7e86f9ab7",
      "number": 6,
      "content": "phải đói nhăn răng ở hết mọi thành phố,",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.524Z",
      "updatedAt": "2025-04-22T07:15:43.524Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "21587b6c-88d0-4ca2-89cb-212165fc6def",
      "number": 6,
      "content": "thiếu bánh ăn ở mọi nơi các ngươi cư ngụ ;",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.538Z",
      "updatedAt": "2025-04-22T07:15:43.538Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "0e7f4dc4-22a4-4999-b57b-a92b865bc372",
      "number": 8,
      "content": "thế mà các ngươi vẫn không chịu trở về với Ta",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.741Z",
      "updatedAt": "2025-04-22T07:15:43.741Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "3e280562-0236-48b0-9701-6e6074dc5016",
      "number": 6,
      "content": "thế mà các ngươi vẫn không chịu trở về với Ta",
      "order": 3,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.552Z",
      "updatedAt": "2025-04-22T07:15:43.552Z",
      "chapterId": "689bf902-9a52-4014-912b-2a88277e50bd"
    },
    {
      "id": "601ba41f-1712-4270-bc6e-f0aaa1500d8a",
      "number": 9,
      "content": "thế mà các ngươi vẫn không chịu trở về với Ta",
      "order": 4,
