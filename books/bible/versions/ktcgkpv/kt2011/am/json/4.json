    },
    {
      "id": "75c9e56e-201f-41ca-9e3d-8fce591190d4",
      "number": 8,
      "content": "– sấm ngôn của ĐỨC CHÚA.",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:15:43.755Z",
      "updatedAt": "2025-04-22T07:15:43.755Z",
