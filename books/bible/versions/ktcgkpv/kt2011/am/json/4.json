    },
    {
      "id": "33402dc4-d414-414b-aecc-72a27c961e81",
      "number": 6,
      "content": "– sấm ngôn của ĐỨC CHÚA.",
      "order": 4,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
