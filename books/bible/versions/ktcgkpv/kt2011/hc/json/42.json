      "position": 43,
      "order": 15,
      "createdAt": "2025-04-22T06:15:34.949Z",
      "updatedAt": "2025-04-22T06:15:34.949Z",
      "verseId": "a04e676e-2de2-4c9c-b79e-bb44eae1770d",
      "chapterId": "5eb8aad2-5b7b-471f-90ca-b8db9b72eac6"
    },
