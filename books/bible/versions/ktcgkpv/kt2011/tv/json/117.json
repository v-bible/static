  "verses": [
    {
      "id": "61ee4e9c-b177-43f9-b78b-8298e50ceef7",
      "number": 1,
      "content": "Muôn nước hỡi, nào ca ngợi CHÚA,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:26.641Z",
      "updatedAt": "2025-04-22T05:39:26.641Z",
      "chapterId": "c3da8531-7703-4c4c-a0d5-0ed5e5221805"
    },
    {
      "id": "85a07fe8-2e14-4301-9ba7-3fba491296ee",
      "number": 1,
      "content": "ngàn dân ơi, hãy chúc tụng Người !",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:26.693Z",
      "updatedAt": "2025-04-22T05:39:26.693Z",
      "chapterId": "c3da8531-7703-4c4c-a0d5-0ed5e5221805"
    },
    {
      "id": "4f692d33-54b8-4dc4-9eac-376ea2062fbd",
      "number": 2,
      "content": "Vì tình Chúa thương ta thật là mãnh liệt,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:26.706Z",
      "updatedAt": "2025-04-22T05:39:26.706Z",
      "chapterId": "c3da8531-7703-4c4c-a0d5-0ed5e5221805"
    },
    {
      "id": "a04f8ca0-36fe-40ab-94f8-a7443ec9baf0",
      "number": 2,
      "content": "lòng thành tín của Người bền vững muôn năm. Ha-lê-lui-a.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:26.720Z",
      "updatedAt": "2025-04-22T05:39:26.720Z",
      "chapterId": "c3da8531-7703-4c4c-a0d5-0ed5e5221805"
    }
  ],
