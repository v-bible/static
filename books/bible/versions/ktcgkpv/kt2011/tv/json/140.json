  "headings": [
    {
      "id": "547e7cbe-d383-4561-8ee0-85883a5752f4",
      "content": "Tv 140 (139) ĐỨC CHÚA là Đấng bảo vệ người khốn khổ khó nghèo",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T05:44:13.269Z",
