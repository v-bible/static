  "verses": [
    {
      "id": "ce713cf7-12f0-438a-a383-45d1ee236a92",
      "number": 1,
      "content": "Ha-lê-lui-a.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.024Z",
      "updatedAt": "2025-04-22T05:39:33.024Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "393c18c4-9ea0-463a-8e66-1d51e2ba3208",
      "number": 1,
      "content": "A-lép",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.070Z",
      "updatedAt": "2025-04-22T05:39:33.070Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "58b56688-790a-43a4-a71a-316b562b32c9",
      "number": 1,
      "content": "Hạnh phúc thay, người kính sợ CHÚA,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.081Z",
      "updatedAt": "2025-04-22T05:39:33.081Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "1233737a-5e1b-4b26-9323-829d17a19d05",
      "number": 1,
      "content": "Bết",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.093Z",
      "updatedAt": "2025-04-22T05:39:33.093Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "82b1d6a0-f89e-409e-908a-9cbddc7233c2",
      "number": 1,
      "content": "những ưa cùng thích mệnh lệnh Chúa truyền ban.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.110Z",
      "updatedAt": "2025-04-22T05:39:33.110Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "c7c34f9d-280e-4100-8bb8-f7c6e7938692",
      "number": 2,
      "content": "Ghi-men",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.123Z",
      "updatedAt": "2025-04-22T05:39:33.123Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "9311a840-8087-4b4e-a4de-63218323c4df",
      "number": 2,
      "content": "Trên mặt đất, con cháu của họ sẽ hùng cường,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.138Z",
      "updatedAt": "2025-04-22T05:39:33.138Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "a9d2cbad-0a1f-4445-ad7b-7ce9d235b011",
      "number": 2,
      "content": "Đa-lét",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.152Z",
      "updatedAt": "2025-04-22T05:39:33.152Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "df07574e-f62a-4302-b448-6d284fa9cd78",
      "number": 2,
      "content": "dòng dõi kẻ ngay lành được Chúa thương giáng phúc.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.166Z",
      "updatedAt": "2025-04-22T05:39:33.166Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "8916be83-a2cb-473f-bc5b-490d20546755",
      "number": 3,
      "content": "Hê",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.181Z",
      "updatedAt": "2025-04-22T05:39:33.181Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "06efd19d-c8a4-4ccf-8f4b-a2e88e382d72",
      "number": 3,
      "content": "Gia đình họ phú quý giàu sang,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.198Z",
      "updatedAt": "2025-04-22T05:39:33.198Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "01fa44d5-2832-4ede-9ea2-c47cc8abac36",
      "number": 3,
      "content": "Vau",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.215Z",
      "updatedAt": "2025-04-22T05:39:33.215Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "73b26dcb-63db-42d7-8882-9da6640adb33",
      "number": 3,
      "content": "đức công chính của họ tồn tại muôn đời.",
      "order": 3,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.229Z",
      "updatedAt": "2025-04-22T05:39:33.229Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "d348b4f5-9fa6-4614-8b08-31e040fc6cfe",
      "number": 4,
      "content": "Da-in",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.258Z",
      "updatedAt": "2025-04-22T05:39:33.258Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "fddc3eee-6b8f-4789-9780-2080549683a9",
      "number": 4,
      "content": "Giữa tối tăm bừng lên một ánh sáng",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.271Z",
      "updatedAt": "2025-04-22T05:39:33.271Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "cb45f26e-c084-4054-a2b7-73c798574030",
      "number": 4,
      "content": "chiếu rọi kẻ ngay lành :",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.285Z",
      "updatedAt": "2025-04-22T05:39:33.285Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "0dd650bd-1a02-44db-85c1-e3f15cbf680e",
      "number": 4,
      "content": "Khết",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.299Z",
      "updatedAt": "2025-04-22T05:39:33.299Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "6349709c-52c7-4299-b91a-7224ab014080",
      "number": 4,
      "content": "đó là người từ bi nhân hậu và công chính.",
      "order": 4,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.313Z",
      "updatedAt": "2025-04-22T05:39:33.313Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "30def57b-6d9d-4d74-bdbc-f9f08b3b2918",
      "number": 5,
      "content": "Tết",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.341Z",
      "updatedAt": "2025-04-22T05:39:33.341Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "ab27856a-c627-48fc-a372-2069bdbf8215",
      "number": 5,
      "content": "Phúc thay người biết cảm thương và cho vay mượn,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.355Z",
      "updatedAt": "2025-04-22T05:39:33.355Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "c4346d15-18ea-45a2-a54a-949ab52288d3",
      "number": 5,
      "content": "Giốt",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.382Z",
      "updatedAt": "2025-04-22T05:39:33.382Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "12a6c867-18a2-4531-a96e-2808c8337b0b",
      "number": 5,
      "content": "biết theo lẽ phải mà thu xếp công việc mình.",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.396Z",
      "updatedAt": "2025-04-22T05:39:33.396Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "d1e6d2ed-9e57-404c-b982-b2d7f2791505",
      "number": 6,
      "content": "Cáp",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.410Z",
      "updatedAt": "2025-04-22T05:39:33.410Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "52243156-782f-4436-9171-3e5e6b72cc19",
      "number": 6,
      "content": "Họ sẽ không bao giờ lay chuyển,",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.424Z",
      "updatedAt": "2025-04-22T05:39:33.424Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "e863001d-d373-4108-856e-1df519ce6a0c",
      "number": 6,
      "content": "La-mét",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.438Z",
      "updatedAt": "2025-04-22T05:39:33.438Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "58a2c82d-b36a-402a-ba46-1e1eb59d673c",
      "number": 6,
      "content": "thiên hạ muôn đời còn tưởng nhớ chính nhân.",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.452Z",
      "updatedAt": "2025-04-22T05:39:33.452Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "420c9213-09dd-466f-bcbe-da715bdf4e4c",
      "number": 7,
      "content": "Mêm",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.466Z",
      "updatedAt": "2025-04-22T05:39:33.466Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "7eb0917e-ed9e-4d01-9185-895d1ca6c45b",
      "number": 7,
      "content": "Họ không lo phải nghe tin dữ,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.480Z",
      "updatedAt": "2025-04-22T05:39:33.480Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "76b09b0c-8bc8-47bf-8f2e-beeeab9d6132",
      "number": 7,
      "content": "Nun",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.494Z",
      "updatedAt": "2025-04-22T05:39:33.494Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "d2b51249-09a1-46bb-bbba-1b639620cdc0",
      "number": 7,
      "content": "hằng an tâm và tin cậy CHÚA,",
      "order": 3,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.523Z",
      "updatedAt": "2025-04-22T05:39:33.523Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "2cb6b36f-8818-4fd5-bec0-777ccea19f00",
      "number": 8,
      "content": "Xa-méc",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.537Z",
      "updatedAt": "2025-04-22T05:39:33.537Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "958a56a7-a3d5-4bdb-b2ad-51a99522a785",
      "number": 8,
      "content": "luôn vững lòng không sợ hãi chi",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.551Z",
      "updatedAt": "2025-04-22T05:39:33.551Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "70cbc3bd-011f-4f64-993c-fa70db368ee1",
      "number": 8,
      "content": "A-in",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.565Z",
      "updatedAt": "2025-04-22T05:39:33.565Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "5c12ceca-79a2-4e12-8e0b-948e705e9793",
      "number": 8,
      "content": "và rốt cuộc coi khinh lũ địch thù.",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.578Z",
      "updatedAt": "2025-04-22T05:39:33.578Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "af10c352-3e68-4049-a2c4-05e2e02afdd6",
      "number": 9,
      "content": "Pê",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.591Z",
      "updatedAt": "2025-04-22T05:39:33.591Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "80871403-4425-4609-895a-c140a3506c8e",
      "number": 9,
      "content": "Kẻ túng nghèo, họ rộng tay làm phúc,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.606Z",
      "updatedAt": "2025-04-22T05:39:33.606Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "4a9c1e6e-423a-4d54-96c2-bcb0564f5306",
      "number": 9,
      "content": "Xa-đê",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.620Z",
      "updatedAt": "2025-04-22T05:39:33.620Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "1a62cad1-a7f9-4655-8ef6-e8a234b8a7d9",
      "number": 9,
      "content": "đức công chính của họ tồn tại muôn đời,",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.633Z",
      "updatedAt": "2025-04-22T05:39:33.633Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "c48e3a4b-d168-4cff-9d0c-a1d801fb6eee",
      "number": 9,
      "content": "Cốp",
      "order": 4,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.647Z",
      "updatedAt": "2025-04-22T05:39:33.647Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "2fdd26d6-4cc5-4316-81cf-1ef5af67ba29",
      "number": 9,
      "content": "uy thế họ vươn cao rực rỡ.",
      "order": 5,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.661Z",
      "updatedAt": "2025-04-22T05:39:33.661Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "44c77c1d-fa2c-47a4-b90e-f88411e5fcff",
      "number": 10,
      "content": "Rết",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.689Z",
      "updatedAt": "2025-04-22T05:39:33.689Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "f4822221-786c-479d-97f0-dfe4a9e2a5d4",
      "number": 10,
      "content": "Thấy cảnh này ác nhân tức giận,",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:33.704Z",
      "updatedAt": "2025-04-22T05:39:33.704Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "3e87311c-a54a-48b7-8e79-0eacb7603eb3",
      "number": 10,
      "content": "Sin",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:33.719Z",
      "updatedAt": "2025-04-22T05:39:33.719Z",
      "chapterId": "be9ad100-4718-41ec-9de1-26880530f71a"
    },
    {
      "id": "2b64ef67-edfb-4da5-8323-9467ac51618d",
      "number": 10,
      "content": "nghiến răng nghiến lợi và mòn mỏi héo hon,",
      "order": 3,
      "parNumber": 43,
