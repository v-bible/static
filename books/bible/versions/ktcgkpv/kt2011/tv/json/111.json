    },
    {
      "id": "2fc09570-b092-4406-a82f-cf412216f527",
      "number": 1,
      "content": "A-lép",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.597Z",
      "updatedAt": "2025-04-22T05:39:23.597Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "adf32617-b558-4440-887d-8e6db8dd5190",
      "number": 1,
      "content": "Tôi xin hết lòng cảm tạ CHÚA,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.608Z",
      "updatedAt": "2025-04-22T05:39:23.608Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "314941c5-73cd-4132-8203-0b4d418452bb",
      "number": 1,
      "content": "Bết",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.619Z",
      "updatedAt": "2025-04-22T05:39:23.619Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "27720925-bb33-4d18-8a40-35e0ee09a1cf",
      "number": 1,
      "content": "trong cộng đoàn chính nhân, giữa lòng đại hội.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.630Z",
      "updatedAt": "2025-04-22T05:39:23.630Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "359314bd-f6f1-46ff-8d17-2fabb2172cb5",
      "number": 2,
      "content": "Ghi-men",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.643Z",
      "updatedAt": "2025-04-22T05:39:23.643Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "bdcbb5ff-df83-4b03-8a1f-3aeb54aeb9a9",
      "number": 2,
      "content": "Việc CHÚA làm quả thật lớn lao,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.657Z",
      "updatedAt": "2025-04-22T05:39:23.657Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "e31d71f0-8129-4e43-84f7-2a39bf9dc27a",
      "number": 2,
      "content": "Đa-lét",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.672Z",
      "updatedAt": "2025-04-22T05:39:23.672Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "8ed2fd6f-4caf-4668-b5ac-f036db4c6261",
      "number": 2,
      "content": "người mộ mến ra công tìm hiểu.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.686Z",
      "updatedAt": "2025-04-22T05:39:23.686Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "70729d6b-cb9e-451c-93d4-a3ae8ef287b3",
      "number": 3,
      "content": "Hê",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.701Z",
      "updatedAt": "2025-04-22T05:39:23.701Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "726cf67f-77cd-4b69-9a74-b4fdd2bb0a14",
      "number": 3,
      "content": "Sự nghiệp Chúa oai phong hiển hách,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.715Z",
      "updatedAt": "2025-04-22T05:39:23.715Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "7c56df4e-5216-4d4b-8186-e70f3557afe2",
      "number": 3,
      "content": "Vau",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.729Z",
      "updatedAt": "2025-04-22T05:39:23.729Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "7bde73aa-910a-4857-bddb-44364803458d",
      "number": 3,
      "content": "đức công chính của Người tồn tại thiên thu.",
      "order": 3,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.743Z",
      "updatedAt": "2025-04-22T05:39:23.743Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "9f3faa7c-9093-42b0-9e3e-57ada5087afb",
      "number": 4,
      "content": "Da-in",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.771Z",
      "updatedAt": "2025-04-22T05:39:23.771Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "e18578df-092a-4523-b9fd-b6e7f7df0a78",
      "number": 4,
      "content": "Chúa đã truyền tưởng niệm những kỳ công của Người.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.786Z",
      "updatedAt": "2025-04-22T05:39:23.786Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "541e3988-15b5-4bec-bc25-18aba35bf282",
      "number": 4,
      "content": "Khết",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.801Z",
      "updatedAt": "2025-04-22T05:39:23.801Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "a61ab0e2-8603-48c6-82de-93054579ee48",
      "number": 4,
      "content": "CHÚA là Đấng từ bi nhân hậu.",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.816Z",
      "updatedAt": "2025-04-22T05:39:23.816Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "1960e176-a805-4d4c-83e6-01e0c6a89eed",
      "number": 5,
      "content": "Tết",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.830Z",
      "updatedAt": "2025-04-22T05:39:23.830Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "7024ce1c-bab0-465f-954d-bf4fc5e7332d",
      "number": 5,
      "content": "Ai kính sợ Người, Người ban phát của ăn ;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.844Z",
      "updatedAt": "2025-04-22T05:39:23.844Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "2b36d9e8-8535-4650-ba40-00f1dbc7d946",
      "number": 5,
      "content": "Giốt",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.872Z",
      "updatedAt": "2025-04-22T05:39:23.872Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "b6ee403c-897b-4cb1-b785-ce95bd1a2a34",
      "number": 5,
      "content": "giao ước đã lập ra, muôn đời Người nhớ mãi.",
      "order": 3,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.886Z",
      "updatedAt": "2025-04-22T05:39:23.886Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "4cd09fb9-46bd-4bba-be9b-a953ee07fa65",
      "number": 6,
      "content": "Cáp",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.901Z",
      "updatedAt": "2025-04-22T05:39:23.901Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "92350d11-e38f-4232-83b6-fd58336e655f",
      "number": 6,
      "content": "Sức mạnh việc Người làm, Người cho dân thấy rõ,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.916Z",
      "updatedAt": "2025-04-22T05:39:23.916Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "5ee3f2b3-d4bf-4be2-b01a-d2bdf5ccac29",
      "number": 6,
      "content": "La-mét",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.930Z",
      "updatedAt": "2025-04-22T05:39:23.930Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "d9512c40-08c0-4cac-a21b-6a78c87dc028",
      "number": 6,
      "content": "khi tặng ban cho họ gia sản của chư dân.",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.944Z",
      "updatedAt": "2025-04-22T05:39:23.944Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "3748185c-1129-4b1a-845e-38a9f89db7cd",
      "number": 7,
      "content": "Mêm",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:23.958Z",
      "updatedAt": "2025-04-22T05:39:23.958Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "13389bb0-825a-4095-96b4-540681e30fd5",
      "number": 7,
      "content": "Những công trình tay Chúa thực hiện",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.972Z",
      "updatedAt": "2025-04-22T05:39:23.972Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "d5f7937c-4a9e-4138-b823-b10f926cc1a2",
      "number": 7,
      "content": "quả là chân thật và công minh.",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:23.993Z",
      "updatedAt": "2025-04-22T05:39:23.993Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "10afe285-14cc-4561-a14e-aef245fdbf0d",
      "number": 7,
      "content": "Nun",
      "order": 3,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:24.007Z",
      "updatedAt": "2025-04-22T05:39:24.007Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "ea980045-8e8c-42a3-ab25-5cd007387696",
      "number": 7,
      "content": "Huấn lệnh Người ban đều chắc chắn,",
      "order": 4,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:24.030Z",
      "updatedAt": "2025-04-22T05:39:24.030Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "d244da17-2bda-4fb4-87d4-20793b7acc55",
      "number": 8,
      "content": "Xa-méc",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:24.043Z",
      "updatedAt": "2025-04-22T05:39:24.043Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "6071772a-8edb-4b1f-9e7f-b6afefabb68b",
      "number": 8,
      "content": "bền vững đến muôn đời muôn thuở,",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:24.055Z",
      "updatedAt": "2025-04-22T05:39:24.055Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "1eff6249-880c-46bf-9f5e-97d54248fe34",
      "number": 8,
      "content": "A-in",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:24.068Z",
      "updatedAt": "2025-04-22T05:39:24.068Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "0ae5bddb-99ca-4570-802c-973e77273fb2",
      "number": 8,
      "content": "căn cứ vào sự thật lẽ ngay.",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:24.083Z",
      "updatedAt": "2025-04-22T05:39:24.083Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "4403b1f1-f136-4502-8252-ce29831ffee5",
      "number": 9,
      "content": "Pê",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:24.098Z",
      "updatedAt": "2025-04-22T05:39:24.098Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "11a42fab-b147-4265-ab3b-94f3da1375c2",
      "number": 9,
      "content": "Người đem lại cho dân ơn giải thoát,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:24.112Z",
      "updatedAt": "2025-04-22T05:39:24.112Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "bacbef00-3ebf-4c00-a8aa-4b3bc64b86e7",
      "number": 9,
      "content": "Xa-đê",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:24.127Z",
      "updatedAt": "2025-04-22T05:39:24.127Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "6b38daf9-0bfa-4d3c-ba4f-291970af102f",
      "number": 9,
      "content": "thiết lập giao ước đến muôn đời.",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:24.142Z",
      "updatedAt": "2025-04-22T05:39:24.142Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "2c385838-e6c2-4299-b492-276b0e3ee9d0",
      "number": 9,
      "content": "Cốp",
      "order": 4,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:24.156Z",
      "updatedAt": "2025-04-22T05:39:24.156Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "60115d8a-c5ab-4712-99b0-c7adee5ad98a",
      "number": 9,
      "content": "Tôn danh Người thánh thiêng khả uý.",
      "order": 5,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:24.170Z",
      "updatedAt": "2025-04-22T05:39:24.170Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "1e9e4823-e044-4b69-983e-977e2e76fcf9",
      "number": 10,
      "content": "Rết",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:24.185Z",
      "updatedAt": "2025-04-22T05:39:24.185Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "266ee876-b2a0-4b25-80fd-88edc8474ef9",
      "number": 10,
      "content": "Kính sợ CHÚA là đầu mối khôn ngoan.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:24.199Z",
      "updatedAt": "2025-04-22T05:39:24.199Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "17099d60-db22-4b03-865a-7a694a172437",
      "number": 10,
      "content": "Sin",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:39:24.246Z",
      "updatedAt": "2025-04-22T05:39:24.246Z",
      "chapterId": "43ff40cd-dcea-4660-b589-382cd0f37f8a"
    },
    {
      "id": "8570dc4e-8333-4449-8ba5-b1a128e57dbe",
      "number": 10,
      "content": "Sáng suốt thay kẻ thực hành như vậy.",
      "order": 3,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:39:24.260Z",
