{
  "chapter": {
    "id": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f",
    "number": 37,
    "ref": "",
    "createdAt": "2025-04-22T00:48:31.714Z",
    "updatedAt": "2025-04-22T00:48:31.714Z",
    "bookId": "9beff2a2-3de1-4b2d-b189-6eadb8dd6815"
  },
  "verses": [
    {
      "id": "12fe2a6d-448c-40d0-82b9-54437d91ec0d",
      "number": 1,
      "content": "*Của vua Đa-vít.*",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.177Z",
      "updatedAt": "2025-04-22T05:28:12.177Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "3f22f853-7d76-4dfc-a75a-a706d4805d50",
      "number": 1,
      "content": "A-lép",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.245Z",
      "updatedAt": "2025-04-22T05:28:12.245Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "c47c05f8-29b9-41ad-b009-f631b108b0ef",
      "number": 1,
      "content": "Bạn đừng nổi giận vì quân gian ác,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.259Z",
      "updatedAt": "2025-04-22T05:28:12.259Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "298361a3-e1b5-4c8d-9daa-a15787c3244a",
      "number": 1,
      "content": "chớ phân bì với kẻ bất lương,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.273Z",
      "updatedAt": "2025-04-22T05:28:12.273Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "b0488d4a-6965-4d5b-93e0-c98d67f7a409",
      "number": 2,
      "content": "vì chúng tựa cỏ hoang mau úa",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.286Z",
      "updatedAt": "2025-04-22T05:28:12.286Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "a99b4c34-82ca-4eaf-9d89-4d7a9c3c7e9d",
      "number": 2,
      "content": "và giống như thanh thảo chóng tàn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.314Z",
      "updatedAt": "2025-04-22T05:28:12.314Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "1d139cc2-f2e0-4ae3-829e-59fe2f4694f8",
      "number": 3,
      "content": "Bết",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.328Z",
      "updatedAt": "2025-04-22T05:28:12.328Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "46343d6b-e683-4013-af34-34d9ff071d88",
      "number": 3,
      "content": "Cứ tin tưởng vào CHÚA và làm điều thiện,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.342Z",
      "updatedAt": "2025-04-22T05:28:12.342Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "862dab46-7754-4bd1-8352-fd950e069b4f",
      "number": 3,
      "content": "thì sẽ được ở trong đất nước và sống yên hàn.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.356Z",
      "updatedAt": "2025-04-22T05:28:12.356Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "e1d7766d-00d1-4e00-aaa8-f119918bd3aa",
      "number": 4,
      "content": "Hãy lấy CHÚA làm niềm vui của bạn,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.384Z",
      "updatedAt": "2025-04-22T05:28:12.384Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "4d39259a-7e33-4537-9bb5-d73b30a92452",
      "number": 4,
      "content": "Người sẽ cho được phỉ chí toại lòng.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.398Z",
      "updatedAt": "2025-04-22T05:28:12.398Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "24eb2d93-3409-4f0d-a8ee-73f0f937bd5c",
      "number": 5,
      "content": "Ghi-men",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.412Z",
      "updatedAt": "2025-04-22T05:28:12.412Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "cec2985b-f3b2-4862-bf8b-9cd8d036375e",
      "number": 5,
      "content": "Hãy ký thác đường đời cho CHÚA,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.427Z",
      "updatedAt": "2025-04-22T05:28:12.427Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "c768e81e-e265-49fe-8dfe-da99cee64ced",
      "number": 5,
      "content": "tin tưởng vào Người, Người sẽ ra tay.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.441Z",
      "updatedAt": "2025-04-22T05:28:12.441Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "e43eed7d-6fac-4778-ba8f-58691bd6ab5b",
      "number": 6,
      "content": "Chính nghĩa bạn, Chúa sẽ làm rực rỡ tựa bình minh,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.455Z",
      "updatedAt": "2025-04-22T05:28:12.455Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "6e5e7a80-3fe9-4e6d-b7cd-eb1141b17120",
      "number": 6,
      "content": "công lý bạn, Người sẽ cho huy hoàng như chính ngọ.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.469Z",
      "updatedAt": "2025-04-22T05:28:12.469Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "804feaf4-49ff-4ceb-8564-81aa53fd44f0",
      "number": 7,
      "content": "Đa-lét",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.486Z",
      "updatedAt": "2025-04-22T05:28:12.486Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "215b07f8-e037-460f-ae84-beb6331205e1",
      "number": 7,
      "content": "Hãy lặng thinh trước mặt CHÚA và đợi trông Người.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.500Z",
      "updatedAt": "2025-04-22T05:28:12.500Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "7b2e00cc-cbdc-4706-87ae-c9278d68b0c5",
      "number": 7,
      "content": "Bạn chẳng nên nổi giận",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.526Z",
      "updatedAt": "2025-04-22T05:28:12.526Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "62e93990-51db-4522-aef0-2c64bbdb1dcb",
      "number": 7,
      "content": "với kẻ được thành công hay với người xảo trá.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.541Z",
      "updatedAt": "2025-04-22T05:28:12.541Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "cafeb1ca-df1d-441d-bbbd-2ed60396a699",
      "number": 8,
      "content": "Hê",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.555Z",
      "updatedAt": "2025-04-22T05:28:12.555Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "bf3aeb62-c276-4d06-a32e-19fa64f556bd",
      "number": 8,
      "content": "Dừng cơn phẫn nộ và chớ mãi nổi xung,",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.570Z",
      "updatedAt": "2025-04-22T05:28:12.570Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "c2421375-2270-44e4-ad8e-381995fa7ad2",
      "number": 8,
      "content": "đừng nổi giận kẻo sinh ra tội lỗi,",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.585Z",
      "updatedAt": "2025-04-22T05:28:12.585Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "165d6851-1ff2-47d7-82b1-3d0e21ffdf81",
      "number": 9,
      "content": "vì bọn gian ác sẽ bị diệt trừ,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.599Z",
      "updatedAt": "2025-04-22T05:28:12.599Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "3950815b-f153-4e3f-babd-f2dcf6500d84",
      "number": 9,
      "content": "còn người trông đợi CHÚA, sẽ được đất hứa làm gia nghiệp.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.613Z",
      "updatedAt": "2025-04-22T05:28:12.613Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "1bc6109b-3826-486f-bfe0-da0d39b5c24d",
      "number": 10,
      "content": "Vau",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.627Z",
      "updatedAt": "2025-04-22T05:28:12.627Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "594f6249-ca8d-45ce-9e56-33266a70c799",
      "number": 10,
      "content": "Ít lâu nữa ác nhân sẽ chẳng còn,",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.642Z",
      "updatedAt": "2025-04-22T05:28:12.642Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "45127b19-cf01-4a73-b468-0e900187b5aa",
      "number": 10,
      "content": "đến chỗ xưa cũng không tìm thấy hắn.",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.655Z",
      "updatedAt": "2025-04-22T05:28:12.655Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "445222e9-3dc7-4674-874e-6e7400d92fb6",
      "number": 11,
      "content": "Còn kẻ nghèo hèn được đất hứa làm gia nghiệp",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.670Z",
      "updatedAt": "2025-04-22T05:28:12.670Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "52118a99-53b7-4688-bb0b-d8df4b4712eb",
      "number": 11,
      "content": "và vui hưởng cảnh an lạc chan hoà.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.699Z",
      "updatedAt": "2025-04-22T05:28:12.699Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "6bcf7327-10f1-4c35-a9de-5540f817e728",
      "number": 12,
      "content": "Da-in",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.713Z",
      "updatedAt": "2025-04-22T05:28:12.713Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "a53fdb13-76eb-4521-8dfb-a82772d09fb3",
      "number": 12,
      "content": "Kẻ gian ác mưu hại người công chính, và hậm hực nghiến răng.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.727Z",
      "updatedAt": "2025-04-22T05:28:12.727Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "4b476ddd-c0fc-4854-b361-83a6d6ba39f7",
      "number": 13,
      "content": "Nhưng Chúa cười nhạo nó, vì thấy nó đã đến ngày tàn.",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.741Z",
      "updatedAt": "2025-04-22T05:28:12.741Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "c0f43ff2-2122-46ee-a54d-3b6a5f8b81d4",
      "number": 14,
      "content": "Khết",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.755Z",
      "updatedAt": "2025-04-22T05:28:12.755Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "24b751e1-3dbb-4152-a151-a546737dc7eb",
      "number": 14,
      "content": "Phường ác nhân tuốt gươm giương nỏ",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.769Z",
      "updatedAt": "2025-04-22T05:28:12.769Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "cf26e3fd-92ec-4a15-ae18-5418e87ec4f7",
      "number": 14,
      "content": "nhằm hạ kẻ nghèo hèn, và giết người chính trực ;",
      "order": 2,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.784Z",
      "updatedAt": "2025-04-22T05:28:12.784Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "9d5da416-b04e-41e0-966c-2e233c578afc",
      "number": 15,
      "content": "nhưng gươm chúng lại đâm vào tim chúng, và nỏ gẫy tan tành.",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.798Z",
      "updatedAt": "2025-04-22T05:28:12.798Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "921d7a32-c6b8-4d68-8e54-60b825640b6a",
      "number": 16,
      "content": "Tết",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.826Z",
      "updatedAt": "2025-04-22T05:28:12.826Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "a6939413-f861-492a-bddb-bdd1f76b4996",
      "number": 16,
      "content": "Ít tiền ít của mà là người công chính",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.840Z",
      "updatedAt": "2025-04-22T05:28:12.840Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "8c2fd33b-f727-4e04-87a8-01b65406253e",
      "number": 16,
      "content": "hơn nhiều vàng nhiều bạc mà là kẻ ác nhân.",
      "order": 2,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.863Z",
      "updatedAt": "2025-04-22T05:28:12.863Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "bc162cfe-81fd-4391-89a4-f0a175a826f6",
      "number": 17,
      "content": "Vì cánh tay bọn ác nhân sẽ bị bẻ gẫy,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.875Z",
      "updatedAt": "2025-04-22T05:28:12.875Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "419355e6-9f01-4f21-a9ea-a4224d823a7c",
      "number": 17,
      "content": "còn người công chính được CHÚA độ trì.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.887Z",
      "updatedAt": "2025-04-22T05:28:12.887Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "89f1a477-947a-4fad-b60a-35b7ba23d65a",
      "number": 18,
      "content": "Giốt",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.899Z",
      "updatedAt": "2025-04-22T05:28:12.899Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "db7d5543-0fb4-4af5-9134-c8aa21a71214",
      "number": 18,
      "content": "CHÚA chăm sóc cuộc đời người thiện hảo,",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.912Z",
      "updatedAt": "2025-04-22T05:28:12.912Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "fd329b29-d769-4308-a913-44b9ea65d92e",
      "number": 18,
      "content": "gia nghiệp họ tồn tại đến muôn năm.",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.926Z",
      "updatedAt": "2025-04-22T05:28:12.926Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "8a7d92a1-5f6f-4cb3-9688-7f1ef93a2576",
      "number": 19,
      "content": "Buổi gian nan họ không hề hổ thẹn,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.940Z",
      "updatedAt": "2025-04-22T05:28:12.940Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "f3c11539-b567-4429-a152-2c15014e7178",
      "number": 19,
      "content": "ngày đói kém lại được no đầy.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.955Z",
      "updatedAt": "2025-04-22T05:28:12.955Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "fe74ad72-87ef-4c90-8dec-9765a1b0bd41",
      "number": 20,
      "content": "Cáp",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:12.969Z",
      "updatedAt": "2025-04-22T05:28:12.969Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "694711bb-1f27-4235-9f98-7fa250a8b3ce",
      "number": 20,
      "content": "Còn quân gian ác sẽ phải tiêu vong ;",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.983Z",
      "updatedAt": "2025-04-22T05:28:12.983Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "8c0458fc-4878-458e-8a11-e3bb14100556",
      "number": 20,
      "content": "những kẻ thù của CHÚA",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:12.997Z",
      "updatedAt": "2025-04-22T05:28:12.997Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "6c639339-6bab-4ca0-94be-6b27f51ebfc6",
      "number": 20,
      "content": "sẽ tan đi như cỏ nội hoa đồng, tan đi thành mây khói.",
      "order": 3,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.011Z",
      "updatedAt": "2025-04-22T05:28:13.011Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "9b9828bb-3853-4aaa-b62a-323cac7433d7",
      "number": 21,
      "content": "La-mét",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:13.026Z",
      "updatedAt": "2025-04-22T05:28:13.026Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "f09e1626-8bb8-4d6c-b957-e6ec05130c2d",
      "number": 21,
      "content": "Kẻ gian ác vay mà không trả,",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.040Z",
      "updatedAt": "2025-04-22T05:28:13.040Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "e12d7315-c90a-4a58-a50f-6a261a24abba",
      "number": 21,
      "content": "người công chính thông cảm và cho không.",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.054Z",
      "updatedAt": "2025-04-22T05:28:13.054Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "596dbf86-451f-4f3a-95e1-2caaa8442107",
      "number": 22,
      "content": "Người Chúa chúc lành được đất hứa làm gia nghiệp,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.069Z",
      "updatedAt": "2025-04-22T05:28:13.069Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "008fa428-718c-49ee-8ded-2b67d17de716",
      "number": 22,
      "content": "kẻ Chúa nguyền rủa sẽ bị diệt trừ.",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.083Z",
      "updatedAt": "2025-04-22T05:28:13.083Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "92c3d9f7-05fc-4e51-9be4-36db230dcab6",
      "number": 23,
      "content": "Mêm",
      "order": 0,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:13.098Z",
      "updatedAt": "2025-04-22T05:28:13.098Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "778946fe-ff96-48d3-a618-bb0004ac5b35",
      "number": 23,
      "content": "CHÚA giúp con người bước đi vững chãi,",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.112Z",
      "updatedAt": "2025-04-22T05:28:13.112Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "bc7a7698-f83d-482e-a459-e3cf2f76c37a",
      "number": 23,
      "content": "ưa chuộng đường lối họ dõi theo.",
      "order": 2,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.140Z",
      "updatedAt": "2025-04-22T05:28:13.140Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "629ab956-ac6b-41e9-9fa1-606842353264",
      "number": 24,
      "content": "Dầu họ có vấp cũng không ngã gục,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.154Z",
      "updatedAt": "2025-04-22T05:28:13.154Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "552037c8-63fc-4491-86b8-d8e996a079fa",
      "number": 24,
      "content": "bởi vì đã có CHÚA cầm tay.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.168Z",
      "updatedAt": "2025-04-22T05:28:13.168Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "5c666071-d907-4c23-979a-f51c5dfc931e",
      "number": 25,
      "content": "Nun",
      "order": 0,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:13.181Z",
      "updatedAt": "2025-04-22T05:28:13.181Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "328d45fb-7bfe-4b13-9f4f-7392e1ed09b7",
      "number": 25,
      "content": "Từ nhỏ dại tới nay tôi già cả,",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.207Z",
      "updatedAt": "2025-04-22T05:28:13.207Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "21dd21aa-e7f6-4345-a2ec-f214583ca0ef",
      "number": 25,
      "content": "chưa thấy người công chính bị bỏ rơi,",
      "order": 2,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.218Z",
      "updatedAt": "2025-04-22T05:28:13.218Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "8a764a81-75e1-4c9f-a842-ba4191154b08",
      "number": 25,
      "content": "hoặc dòng giống phải ăn mày thiên hạ.",
      "order": 3,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.230Z",
      "updatedAt": "2025-04-22T05:28:13.230Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "9fa85f8f-09d9-48a9-bd5d-956d867ebcb8",
      "number": 26,
      "content": "Ngày ngày họ thông cảm và cho mượn cho vay,",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.242Z",
      "updatedAt": "2025-04-22T05:28:13.242Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "22cdd5c2-72c9-43db-9504-b28e4e9307df",
      "number": 26,
      "content": "dòng giống mai sau hưởng phúc lành.",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.256Z",
      "updatedAt": "2025-04-22T05:28:13.256Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "8a855bdf-456c-4fad-9f08-c19e989b54ca",
      "number": 27,
      "content": "Hãy làm lành lánh dữ, bạn sẽ được một nơi ở muôn đời.",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.271Z",
      "updatedAt": "2025-04-22T05:28:13.271Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "f1c1007f-837d-474a-8205-9cf86839ca30",
      "number": 28,
      "content": "Bởi vì CHÚA yêu thích điều chính trực,",
      "order": 0,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.285Z",
      "updatedAt": "2025-04-22T05:28:13.285Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "ff7d28ff-92c2-4a8d-9dd9-f9c6ca70af4b",
      "number": 28,
      "content": "chẳng bỏ rơi những bậc hiếu trung.",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.298Z",
      "updatedAt": "2025-04-22T05:28:13.298Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "edd6396f-be52-46ca-9761-b31a29d628e0",
      "number": 28,
      "content": "A-in",
      "order": 2,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:13.312Z",
      "updatedAt": "2025-04-22T05:28:13.312Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "b60b8eed-7cb3-4098-b857-486bdda46a28",
      "number": 28,
      "content": "Quân bất chính sẽ hoàn toàn bị tiêu diệt,",
      "order": 3,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.325Z",
      "updatedAt": "2025-04-22T05:28:13.325Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "f90f7eac-2a65-4655-8c97-978365d00e84",
      "number": 28,
      "content": "dòng giống ác nhân rồi cũng phải tru di.",
      "order": 4,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.339Z",
      "updatedAt": "2025-04-22T05:28:13.339Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "515a72e4-3cef-4645-b197-c366fbe12269",
      "number": 29,
      "content": "Còn người công chính được đất hứa làm gia nghiệp,",
      "order": 0,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.354Z",
      "updatedAt": "2025-04-22T05:28:13.354Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "d8bd259d-2f18-4c39-a72c-e81f23f6e59f",
      "number": 29,
      "content": "và định cư tại đó mãi muôn đời.",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.369Z",
      "updatedAt": "2025-04-22T05:28:13.369Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "b5ef52bf-89f9-4bac-a2f6-b0080f13db21",
      "number": 30,
      "content": "Pê",
      "order": 0,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:13.389Z",
      "updatedAt": "2025-04-22T05:28:13.389Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "11db6cd2-3a44-4ff9-84b4-17a58d7d3940",
      "number": 30,
      "content": "Miệng người công chính niệm lẽ khôn ngoan,",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.404Z",
      "updatedAt": "2025-04-22T05:28:13.404Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "acf7993f-c213-4a1e-a60a-2198da7e2b20",
      "number": 30,
      "content": "và lưỡi họ nói lên điều chính trực.",
      "order": 2,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.419Z",
      "updatedAt": "2025-04-22T05:28:13.419Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "e0665790-ab13-48ae-9b6f-2af0d33fb809",
      "number": 31,
      "content": "Luật Thiên Chúa, họ ghi tạc vào lòng,",
      "order": 0,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.436Z",
      "updatedAt": "2025-04-22T05:28:13.436Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "6f89f179-4cd2-44d5-aa1f-e3e88cdd884c",
      "number": 31,
      "content": "bước chân đi không hề lảo đảo.",
      "order": 1,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.467Z",
      "updatedAt": "2025-04-22T05:28:13.467Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "6bc56af3-d2b8-4ac4-bc9f-8a088bd1698f",
      "number": 32,
      "content": "Xa-đê",
      "order": 0,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:13.481Z",
      "updatedAt": "2025-04-22T05:28:13.481Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "7da61f80-d3e1-4fae-aae4-c90e2780fed6",
      "number": 32,
      "content": "Kẻ gian ác rình mò, mưu giết người công chính.",
      "order": 1,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.495Z",
      "updatedAt": "2025-04-22T05:28:13.495Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "55629340-a077-4b2f-85bb-9667615e0783",
      "number": 33,
      "content": "Nhưng CHÚA không để họ lọt vào tay ác nhân,",
      "order": 0,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.510Z",
      "updatedAt": "2025-04-22T05:28:13.510Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "e00cd6f6-aaef-4ee9-ab5d-a5f6ad71f84f",
      "number": 33,
      "content": "không để bị kết án khi phải ra trước toà.",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.524Z",
      "updatedAt": "2025-04-22T05:28:13.524Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "18f41d01-197e-4bdd-8c60-ba80ebdeaecf",
      "number": 34,
      "content": "Cốp",
      "order": 0,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:13.538Z",
      "updatedAt": "2025-04-22T05:28:13.538Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "28981171-cafc-40a7-8f64-36c79541abf2",
      "number": 34,
      "content": "Hãy trông chờ CHÚA, giữ vững đường lối Người,",
      "order": 1,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.553Z",
      "updatedAt": "2025-04-22T05:28:13.553Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "a231d1c5-b411-44b4-a8ea-e93190aeb62b",
      "number": 34,
      "content": "rồi Người sẽ cất nhắc bạn lên",
      "order": 2,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.567Z",
      "updatedAt": "2025-04-22T05:28:13.567Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "a242577c-0429-42b3-9105-ac0b9be572a2",
      "number": 34,
      "content": "và ban đất hứa làm gia nghiệp",
      "order": 3,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.579Z",
      "updatedAt": "2025-04-22T05:28:13.579Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "5dfe5b40-57fc-4547-a8c3-96760df6b6f6",
      "number": 34,
      "content": "bạn sẽ nhìn thấy ác nhân bị diệt trừ.",
      "order": 4,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.594Z",
      "updatedAt": "2025-04-22T05:28:13.594Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "620ccc6a-ce50-4dab-8626-c5c124db28d9",
      "number": 35,
      "content": "Rết",
      "order": 0,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:13.608Z",
      "updatedAt": "2025-04-22T05:28:13.608Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "4ae39a58-4c7b-48f2-be11-58115fb668ff",
      "number": 35,
      "content": "Tôi đã thấy ác nhân ngạo ngược,",
      "order": 1,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.622Z",
      "updatedAt": "2025-04-22T05:28:13.622Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "37aa1ef0-051c-4aa1-8d8b-6e99f0b38780",
      "number": 35,
      "content": "nó vươn mình tựa hương bá xanh tươi.",
      "order": 2,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.650Z",
      "updatedAt": "2025-04-22T05:28:13.650Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "ca890887-ac72-4bf6-a62a-908117df27cc",
      "number": 36,
      "content": "Nay trở lại, nó không còn nữa,",
      "order": 0,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.665Z",
      "updatedAt": "2025-04-22T05:28:13.665Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "45ae5c15-cebc-4a20-b8bd-c19fbec0893b",
      "number": 36,
      "content": "tìm kiếm hoài mà chẳng thấy tăm hơi.",
      "order": 1,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.679Z",
      "updatedAt": "2025-04-22T05:28:13.679Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "c3969cc9-43c8-49af-9f05-6f6847c59bab",
      "number": 37,
      "content": "Sin",
      "order": 0,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:13.694Z",
      "updatedAt": "2025-04-22T05:28:13.694Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "6110c794-5f79-425d-86d1-26d37692770c",
      "number": 37,
      "content": "Hãy xem kẻ thiện toàn, cứ nhìn người chính trực,",
      "order": 1,
      "parNumber": 95,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.707Z",
      "updatedAt": "2025-04-22T05:28:13.707Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "57955d7e-977a-43a9-b215-ae1d529dc02c",
      "number": 37,
      "content": "ai hiếu hoà sẽ được con dòng cháu giống.",
      "order": 2,
      "parNumber": 96,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.721Z",
      "updatedAt": "2025-04-22T05:28:13.721Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "d56cbde3-6a84-4091-b3af-93678e3155cd",
      "number": 38,
      "content": "Quân tội lỗi đều bị diệt trừ,",
      "order": 0,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.736Z",
      "updatedAt": "2025-04-22T05:28:13.736Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "75049c4d-0e01-4ff1-a61b-761170191ecb",
      "number": 38,
      "content": "nòi ác nhân rồi cũng phải tru di.",
      "order": 1,
      "parNumber": 98,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.749Z",
      "updatedAt": "2025-04-22T05:28:13.749Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "77cd0ebc-6cf5-41f8-9950-5a88b674c432",
      "number": 39,
      "content": "Tau",
      "order": 0,
      "parNumber": 99,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:28:13.763Z",
      "updatedAt": "2025-04-22T05:28:13.763Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "31c667b8-705a-4c25-9da7-2b4244076d6c",
      "number": 39,
      "content": "Còn chính nhân được CHÚA thương cứu độ",
      "order": 1,
      "parNumber": 100,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.777Z",
      "updatedAt": "2025-04-22T05:28:13.777Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "7325dff4-c363-4038-8cde-459ef6460ab6",
      "number": 39,
      "content": "và bảo vệ chở che trong buổi ngặt nghèo.",
      "order": 2,
      "parNumber": 101,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.792Z",
      "updatedAt": "2025-04-22T05:28:13.792Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "bc8c37e5-f16f-4062-9113-e512f36fb349",
      "number": 40,
      "content": "CHÚA phù trợ và Người giải thoát,",
      "order": 0,
      "parNumber": 102,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.807Z",
      "updatedAt": "2025-04-22T05:28:13.807Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "fa987551-6b41-480e-a876-b22454a43290",
      "number": 40,
      "content": "giải thoát khỏi ác nhân và thương cứu độ,",
      "order": 1,
      "parNumber": 103,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.822Z",
      "updatedAt": "2025-04-22T05:28:13.822Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "9c32ccb7-53ee-4730-a848-d662b773d36c",
      "number": 40,
      "content": "bởi vì họ ẩn náu bên Người.",
      "order": 2,
      "parNumber": 104,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:28:13.835Z",
      "updatedAt": "2025-04-22T05:28:13.835Z",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    }
  ],
  "footnotes": [
    {
      "id": "c7c0e250-3f5f-4917-8b71-4d7e2590fc49",
      "content": "Thánh vịnh này là *một Thánh vịnh giáo huấn theo mẫu tự*. Tác giả phân biệt ở đây hai nhóm : kẻ dữ và người lành. Mặc dầu có sự phân biệt rõ ràng giữa hai nhóm, khó mà tìm ra thứ tự các ý tưởng được triển khai trong Tv này. Tuy nhiên, độc giả có thể bắt được ý tưởng chính sau đây : đứng trước cảnh ác nhân hoành hành, tác giả khuyên người lành cứ bình tĩnh và an tâm, bởi vì họ sẽ được *đất hứa làm gia nghiệp* (cc. 9.11a.22a.29a.34c), vì ở đó họ sẽ sống yên hàn (c.3), được vui hưởng cảnh an lạc chan hoà (c.11b), được no đầy trong ngày đói kém (c.19b), vì có Thiên Chúa can thiệp. Quả thật, Thiên Chúa phá tan mưu đồ của ác nhân (c.15), không hề bỏ rơi ai bao giờ (c.28) ; Người sẽ che chở phù trì (cc. 5b.17b.23-24.33.39-40), giải thoát và cứu độ (cc. 39-40) ; Người sẽ bảo vệ sự sống và dòng dõi của người lành (c.37), sẽ cho họ được phỉ chí toại lòng (c.4).",
      "position": 35,
      "order": 0,
      "createdAt": "2025-04-22T05:28:12.217Z",
      "updatedAt": "2025-04-22T05:28:12.217Z",
      "headingId": "1415c79c-fa07-4057-9758-18a81c516f06",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "5a83006c-4240-478f-94e3-aca9d574e1e4",
      "content": "*Đất nước* ở đây là xứ Pa-lét-tin (x. cc. 9.11a.22a.29a.34c). *Sống yên hàn* ở đó và được hưởng dùng của cải và tài nguyên phong phú : đó là một trong những ý tưởng chính của Tv này.",
      "position": 28,
      "order": 1,
      "createdAt": "2025-04-22T05:28:12.370Z",
      "updatedAt": "2025-04-22T05:28:12.370Z",
      "verseId": "862dab46-7754-4bd1-8352-fd950e069b4f",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
    {
      "id": "a19d45d4-fc8f-484d-89b3-36158489a749",
      "content": "Ác nhân gây hại cho bản thân bằng chính phương tiện họ muốn dùng để hại người khác (*gậy ông lại đập lưng ông*). X. Tv 7,16-17 ; 9,16 ; 57,7 ; cũng x. Cn 5,22 ; 14,32 ; 26,27 ; G 4,8 ; Gv 10,8 ; Hc 27,25 ; Kn 11,16 ; 12,23 ; 18,4 ; Hs 8,7 ; 10,13 ...",
      "position": 38,
      "order": 2,
      "createdAt": "2025-04-22T05:28:12.812Z",
      "updatedAt": "2025-04-22T05:28:12.812Z",
      "verseId": "9d5da416-b04e-41e0-966c-2e233c578afc",
      "chapterId": "a5b6cd2c-8268-4fb3-8d6a-82ce1c77e58f"
    },
