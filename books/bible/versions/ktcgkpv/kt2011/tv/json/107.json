  "verses": [
    {
      "id": "749ec609-9965-401a-acea-7c45310a38bd",
      "number": 1,
      "content": "Ha-lê-lui-a.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.327Z",
      "updatedAt": "2025-04-22T05:37:48.327Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "111b56ac-cca4-40f9-af24-d02dce4450b0",
      "number": 1,
      "content": "Hãy tạ ơn CHÚA vì Chúa nhân từ,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.384Z",
      "updatedAt": "2025-04-22T05:37:48.384Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "32cafac8-65c3-40be-a218-ee6a3e333a0e",
      "number": 1,
      "content": "muôn ngàn đời Chúa vẫn trọn tình thương.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.412Z",
      "updatedAt": "2025-04-22T05:37:48.412Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "970694cb-8776-454e-b9ba-49ddfdaa4307",
      "number": 2,
      "content": "Họ sẽ cùng nói lên như vậy,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.425Z",
      "updatedAt": "2025-04-22T05:37:48.425Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "28687ff9-83a3-423a-aa28-ec1f27193a8d",
      "number": 2,
      "content": "những người được CHÚA thương giải thoát ;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.439Z",
      "updatedAt": "2025-04-22T05:37:48.439Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "b340277e-9ba6-4c62-8670-dca6cedb86e1",
      "number": 2,
      "content": "giải thoát cho khỏi tay địch thù,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.469Z",
      "updatedAt": "2025-04-22T05:37:48.469Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "48874e93-7ef6-45c0-94fe-1f332d661eff",
      "number": 3,
      "content": "triệu tập về từ bao viễn xứ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.483Z",
      "updatedAt": "2025-04-22T05:37:48.483Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "0fe63b05-9592-41bd-9009-a474596bfa5c",
      "number": 3,
      "content": "khắp miền nam bắc, khắp ngả đông tây.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.497Z",
      "updatedAt": "2025-04-22T05:37:48.497Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "3f4e1960-4e47-406f-9491-9b840a44f72e",
      "number": 4,
      "content": "Họ lạc bước trong vùng sa mạc, nơi cằn cỗi,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.512Z",
      "updatedAt": "2025-04-22T05:37:48.512Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "96f5722b-815b-4da1-b304-5beab7027b34",
      "number": 4,
      "content": "không thấy đường về chốn thành thị để định cư,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.526Z",
      "updatedAt": "2025-04-22T05:37:48.526Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "373db5f7-b7d9-4091-bb3b-c88b1c734abb",
      "number": 5,
      "content": "vừa đói vừa khát, mạng sống đã hầu tàn.",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.540Z",
      "updatedAt": "2025-04-22T05:37:48.540Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "21f83e10-ec36-4db9-94cf-26f9481c9ff2",
      "number": 5,
      "content": "Khi gặp bước ngặt nghèo,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.554Z",
      "updatedAt": "2025-04-22T05:37:48.554Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "ef2ce4e0-ca43-4fce-830a-5669449764d3",
      "number": 6,
      "content": "họ kêu lên cùng CHÚA,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.568Z",
      "updatedAt": "2025-04-22T05:37:48.568Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "d05e7ece-0706-4fb0-b782-d9a4a78c76de",
      "number": 6,
      "content": "Người đưa tay kéo họ ra khỏi cảnh gian truân,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.582Z",
      "updatedAt": "2025-04-22T05:37:48.582Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "252c556a-acaa-4a11-ad3b-4e559d7c7182",
      "number": 7,
      "content": "dắt họ đi thẳng đường ngay lối",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.597Z",
      "updatedAt": "2025-04-22T05:37:48.597Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "f539d270-4849-4962-a1e3-fa4a2e8e38b3",
      "number": 7,
      "content": "về chốn thành thị để định cư.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.624Z",
      "updatedAt": "2025-04-22T05:37:48.624Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "a68311fd-7d48-4a4b-be3b-fd7bcc8270db",
      "number": 8,
      "content": "Ước chi họ dâng lời cảm tạ vì tình thương của Chúa,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.637Z",
      "updatedAt": "2025-04-22T05:37:48.637Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "73cd0f94-6027-41eb-a5ea-868596163051",
      "number": 15,
      "content": "Ước chi họ dâng lời cảm tạ vì tình thương của Chúa,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.879Z",
      "updatedAt": "2025-04-22T05:37:48.879Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "7a4726b2-02d3-4d53-9740-c6638dddfd8f",
      "number": 21,
      "content": "Ước chi họ dâng lời cảm tạ vì tình thương của Chúa,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.049Z",
      "updatedAt": "2025-04-22T05:37:49.049Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "9dda645d-2c23-4eb6-b334-b4ba91df3b9b",
      "number": 31,
      "content": "Ước chi họ dâng lời cảm tạ vì tình thương của Chúa,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.344Z",
      "updatedAt": "2025-04-22T05:37:49.344Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "7de7cf86-a786-45e5-b463-dbd8569c184d",
      "number": 8,
      "content": "và vì những kỳ công Chúa đã thực hiện cho người trần.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.652Z",
      "updatedAt": "2025-04-22T05:37:48.652Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "5ba46857-81ed-470f-acd4-4d89fdcb2d2a",
      "number": 15,
      "content": "và vì những kỳ công Chúa đã thực hiện cho người trần.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.892Z",
      "updatedAt": "2025-04-22T05:37:48.892Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "4eb029da-d480-4ac6-a67c-bc501e79ecd6",
      "number": 21,
      "content": "và vì những kỳ công Chúa đã thực hiện cho người trần.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.064Z",
      "updatedAt": "2025-04-22T05:37:49.064Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "3983a746-1c98-4bdf-9b82-2424fd1347e3",
      "number": 31,
      "content": "và vì những kỳ công Chúa đã thực hiện cho người trần.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.357Z",
      "updatedAt": "2025-04-22T05:37:49.357Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "4f983848-4c3f-4b04-9b60-502e7eb1b76a",
      "number": 9,
      "content": "Họng ráo khô, Chúa cho uống phỉ tình,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.666Z",
      "updatedAt": "2025-04-22T05:37:48.666Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "0b0bbd23-c0d8-4594-a27b-e33817639407",
      "number": 9,
      "content": "bụng đói lả, Người cho ăn thoả thích.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.679Z",
      "updatedAt": "2025-04-22T05:37:48.679Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "777c226d-9892-40ae-a7bf-f07ace5ce3ee",
      "number": 10,
      "content": "Họ phải sống trong cảnh tối tăm mù mịt,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.693Z",
      "updatedAt": "2025-04-22T05:37:48.693Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "e99c845b-bf27-4ecd-88ea-7ee60167551f",
      "number": 10,
      "content": "kiếp lầm than xiềng xích gông cùm ;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.722Z",
      "updatedAt": "2025-04-22T05:37:48.722Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "45412364-d63e-41cb-9f4c-2064f3530903",
      "number": 11,
      "content": "vì họ chống cưỡng lời Thiên Chúa,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.736Z",
      "updatedAt": "2025-04-22T05:37:48.736Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "f230fc29-7e21-4b78-b006-53e592a48555",
      "number": 11,
      "content": "dám khinh nhờn lệnh Đấng Tối Cao,",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.750Z",
      "updatedAt": "2025-04-22T05:37:48.750Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "20834500-dc3f-4ce3-abb8-efe240f361b1",
      "number": 12,
      "content": "Người bắt họ nếm mùi gian khổ,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.779Z",
      "updatedAt": "2025-04-22T05:37:48.779Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "29e7943c-2de1-4ed3-9df4-c35b5442073c",
      "number": 12,
      "content": "phải lao đao mà không kẻ đỡ nâng.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.793Z",
      "updatedAt": "2025-04-22T05:37:48.793Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "3b7eedf6-8323-4c7d-96d6-94dc58a98c6c",
      "number": 13,
      "content": "Khi gặp bước ngặt nghèo, họ kêu lên cùng CHÚA,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.807Z",
      "updatedAt": "2025-04-22T05:37:48.807Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "94f09f5b-69a3-4e2c-9aae-0c8cb3d127b5",
      "number": 19,
      "content": "Khi gặp bước ngặt nghèo, họ kêu lên cùng CHÚA,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.993Z",
      "updatedAt": "2025-04-22T05:37:48.993Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "322e7497-3d08-4a04-b6f7-1105bf0d837f",
      "number": 28,
      "content": "Khi gặp bước ngặt nghèo, họ kêu lên cùng CHÚA,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.260Z",
      "updatedAt": "2025-04-22T05:37:49.260Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "25329afd-5887-468f-9ff4-6208d995aa8d",
      "number": 13,
      "content": "Người ra tay cứu họ thoát ngàn nỗi gian truân :",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.822Z",
      "updatedAt": "2025-04-22T05:37:48.822Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "df9a220c-099b-431a-891e-60b975fe2425",
      "number": 14,
      "content": "đưa ra khỏi cảnh tối tăm mù mịt",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.851Z",
      "updatedAt": "2025-04-22T05:37:48.851Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "e22bfab3-ace1-4770-b192-a2bfb9395604",
      "number": 14,
      "content": "và đập tan xiềng xích gông cùm.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.864Z",
      "updatedAt": "2025-04-22T05:37:48.864Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "8292f6eb-a700-44a0-b0df-e8ca879c1040",
      "number": 16,
      "content": "Cửa đồng kia, Chúa đã phá tung,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.907Z",
      "updatedAt": "2025-04-22T05:37:48.907Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "0c193d04-8cc8-4f5a-aaf5-98a30e4ca0a8",
      "number": 16,
      "content": "then sắt nọ, tay Người bẻ gãy.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.921Z",
      "updatedAt": "2025-04-22T05:37:48.921Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "4a8d836d-ca66-4949-9925-0c6bb985b31d",
      "number": 17,
      "content": "Họ hoá điên trong cuộc đời lầm lỗi,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.935Z",
      "updatedAt": "2025-04-22T05:37:48.935Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "7f506eac-7af7-4ba6-98f5-9ce65a96dac5",
      "number": 17,
      "content": "gánh tội tình đè nặng tấm thân,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.950Z",
      "updatedAt": "2025-04-22T05:37:48.950Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "628fc309-6cf8-40c4-8765-3a2cd964cd15",
      "number": 18,
      "content": "mọi thức ăn, họ đều chê chán,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.964Z",
      "updatedAt": "2025-04-22T05:37:48.964Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "5f31c612-85a4-4f7e-8576-60eecd73cce8",
      "number": 18,
      "content": "và tiến gần tới cửa âm ty.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:48.978Z",
      "updatedAt": "2025-04-22T05:37:48.978Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "8bb027ae-97dd-4ff3-9944-ca647da8afbd",
      "number": 19,
      "content": "Người ra tay cứu họ thoát ngàn nỗi gian truân.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.007Z",
      "updatedAt": "2025-04-22T05:37:49.007Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "3b08cac0-804e-485b-a834-ab485330c117",
      "number": 20,
      "content": "Sai lời Người đến, chữa cho lành mạnh,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.021Z",
      "updatedAt": "2025-04-22T05:37:49.021Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "3bf5140e-7a46-4ddb-83f0-a33002e61f88",
      "number": 20,
      "content": "cứu họ khỏi sa vào hố sâu.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.035Z",
      "updatedAt": "2025-04-22T05:37:49.035Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "85e02fae-9efa-4d68-a1a3-ab9de1a87b4a",
      "number": 22,
      "content": "Ước chi họ dâng lễ tế tạ ơn Chúa,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.077Z",
      "updatedAt": "2025-04-22T05:37:49.077Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "7362d3cd-2735-4b12-ada1-6c666cbf9894",
      "number": 22,
      "content": "cất tiếng hò reo, tường thuật việc Chúa làm.",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.105Z",
      "updatedAt": "2025-04-22T05:37:49.105Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "f9e591f9-a47b-4558-bfa3-aca3474c0744",
      "number": 23,
      "content": "Họ vượt biển ngược xuôi nghề thương mãi,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.120Z",
      "updatedAt": "2025-04-22T05:37:49.120Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "9af846f6-5085-4e63-a3db-c6c56f99509f",
      "number": 23,
      "content": "giữa trùng dương lèo lái con tàu,",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.135Z",
      "updatedAt": "2025-04-22T05:37:49.135Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "57394f99-25fa-4289-9766-8d0a4221998b",
      "number": 24,
      "content": "mắt đã tường việc CHÚA làm nên",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.148Z",
      "updatedAt": "2025-04-22T05:37:49.148Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "b338886f-13c6-41c0-a0d6-4bd108aea092",
      "number": 24,
      "content": "và kỳ công Người thực hiện giữa dòng nước lũ.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.162Z",
      "updatedAt": "2025-04-22T05:37:49.162Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "a5dd4956-a160-44c1-87cf-8a2c91471c90",
      "number": 25,
      "content": "Chúa truyền lệnh khiến bùng lên bão táp,",
      "order": 0,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.176Z",
      "updatedAt": "2025-04-22T05:37:49.176Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "59bdbc78-a390-4d68-b161-3c7b54c555ab",
      "number": 25,
      "content": "lớp sóng xô cuồn cuộn dập dồn.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.189Z",
      "updatedAt": "2025-04-22T05:37:49.189Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "afc27cf9-83ba-4c0c-9fd6-961ad0102673",
      "number": 26,
      "content": "Họ nhô lên tận trời, nhào xuống vực sâu,",
      "order": 0,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.203Z",
      "updatedAt": "2025-04-22T05:37:49.203Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "22781337-c63b-4222-a108-7cd44355a31d",
      "number": 26,
      "content": "lúc nguy hiểm, hồn xiêu phách lạc,",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.217Z",
      "updatedAt": "2025-04-22T05:37:49.217Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "d35a79c2-4b47-4703-8dc2-503bf5065765",
      "number": 27,
      "content": "bị quay cuồng, lảo đảo như say,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.231Z",
      "updatedAt": "2025-04-22T05:37:49.231Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "de70fde4-1218-4378-a158-3e46b04006fd",
      "number": 27,
      "content": "khéo cùng khôn đã chìm đâu mất.",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.245Z",
      "updatedAt": "2025-04-22T05:37:49.245Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "a95bfc27-cc48-4db2-833d-e4075057892a",
      "number": 28,
      "content": "Người đưa tay kéo họ ra khỏi cảnh gian truân.",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.274Z",
      "updatedAt": "2025-04-22T05:37:49.274Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "d756d379-5479-4ebb-a321-7776a98e8ec7",
      "number": 29,
      "content": "Đổi phong ba thành gió thoảng nhẹ nhàng,",
      "order": 0,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.288Z",
      "updatedAt": "2025-04-22T05:37:49.288Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "aff9bb8e-5843-490a-9914-38441ae24543",
      "number": 29,
      "content": "sóng đang gầm, bỗng đâu im tiếng,",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.302Z",
      "updatedAt": "2025-04-22T05:37:49.302Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "936b987a-6f63-4d23-9afe-6a1234f8a433",
      "number": 30,
      "content": "họ vui sướng, vì trời yên bể lặng",
      "order": 0,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.316Z",
      "updatedAt": "2025-04-22T05:37:49.316Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "9383ff4b-4650-4a07-bbe1-b0c82dc1e4d8",
      "number": 30,
      "content": "và Chúa dẫn đưa về bờ bến mong chờ.",
      "order": 1,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.330Z",
      "updatedAt": "2025-04-22T05:37:49.330Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "f13f5751-223a-4027-b9f1-c528486db10d",
      "number": 32,
      "content": "Ước chi họ tán dương Người nơi nhóm họp toàn dân",
      "order": 0,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.371Z",
      "updatedAt": "2025-04-22T05:37:49.371Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "5d031a54-0cd7-4352-9a25-11a70a0410dc",
      "number": 32,
      "content": "và ca tụng giữa hội đồng bô lão.",
      "order": 1,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.385Z",
      "updatedAt": "2025-04-22T05:37:49.385Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "75930602-1d46-418e-9e2e-0cd23ad46801",
      "number": 33,
      "content": "Chúa khiến sông ngòi cạn đi thành sa mạc,",
      "order": 0,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.400Z",
      "updatedAt": "2025-04-22T05:37:49.400Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "83bf97ac-9c51-444e-ac30-155c5f3a4fe4",
      "number": 33,
      "content": "đổi suối nguồn ra hoang địa khô khan,",
      "order": 1,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.428Z",
      "updatedAt": "2025-04-22T05:37:49.428Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "742a7804-9534-457b-bc5c-70d7a18a21a0",
      "number": 34,
      "content": "đất mầu mỡ hoá đồng chua nước mặn,",
      "order": 0,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.445Z",
      "updatedAt": "2025-04-22T05:37:49.445Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "b7d812c6-b471-449b-996e-459ce796c2de",
      "number": 34,
      "content": "vì dân cư độc ác gian tà.",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.459Z",
      "updatedAt": "2025-04-22T05:37:49.459Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "78b67eb7-531c-418a-912d-7789ca18bc96",
      "number": 35,
      "content": "Người lại biến sa mạc thành hồ ao",
      "order": 0,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.474Z",
      "updatedAt": "2025-04-22T05:37:49.474Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "0811bcb8-a972-4c45-8aea-1ae2c7480502",
      "number": 35,
      "content": "và hoang địa khô khan nên nguồn suối.",
      "order": 1,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.502Z",
      "updatedAt": "2025-04-22T05:37:49.502Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "a2ff8cce-f731-4d7f-9006-26aee3269162",
      "number": 36,
      "content": "Kẻ nghèo đói, Người cho đến ở,",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.516Z",
      "updatedAt": "2025-04-22T05:37:49.516Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "a65f0a58-9925-406a-b868-93981b04835a",
      "number": 36,
      "content": "họ lập nên thành thị để định cư.",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.531Z",
      "updatedAt": "2025-04-22T05:37:49.531Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "8f9d9099-833d-41fd-a8d9-5b3439eae171",
      "number": 37,
      "content": "Gieo lúa ngoài ruộng, trồng nho trong vườn,",
      "order": 0,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.545Z",
      "updatedAt": "2025-04-22T05:37:49.545Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "9dc3d1bc-449f-4457-b89f-563c8814ba90",
      "number": 37,
      "content": "họ thu hoạch hoa màu lợi tức.",
      "order": 1,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.560Z",
      "updatedAt": "2025-04-22T05:37:49.560Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "17d43c54-5ee3-4407-9ba7-0063bc0b91d8",
      "number": 38,
      "content": "Chúa chúc lành, họ sinh sôi nảy nở,",
      "order": 0,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.574Z",
      "updatedAt": "2025-04-22T05:37:49.574Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "4acefcf3-0bf9-4d3c-85b2-208c7d57daa4",
      "number": 38,
      "content": "bầy gia súc, Người không để giảm đi.",
      "order": 1,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.587Z",
      "updatedAt": "2025-04-22T05:37:49.587Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "eb2c8940-f029-4268-9187-1219eda4e2d9",
      "number": 39,
      "content": "Nhưng rồi họ phải tiêu hao lụn bại,",
      "order": 0,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.600Z",
      "updatedAt": "2025-04-22T05:37:49.600Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "1593f212-6fcd-443d-9b0c-c9d807874157",
      "number": 39,
      "content": "điêu đứng vì tai hoạ với khổ đau.",
      "order": 1,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.614Z",
      "updatedAt": "2025-04-22T05:37:49.614Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "accf644a-f6ca-430b-afbb-b82a34ef58e0",
      "number": 40,
      "content": "Chúa đổ nhuốc nhơ xuống hàng quyền thế,",
      "order": 0,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.628Z",
      "updatedAt": "2025-04-22T05:37:49.628Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "efacbefc-4b07-4598-b4fa-483bd4bb835e",
      "number": 40,
      "content": "bắt phiêu bạt giữa chốn hoang vu,",
      "order": 1,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.643Z",
      "updatedAt": "2025-04-22T05:37:49.643Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "cc86620f-8df8-468d-b923-a24bf6e0631b",
      "number": 40,
      "content": "không đường ra lối vào.",
      "order": 2,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.657Z",
      "updatedAt": "2025-04-22T05:37:49.657Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "a0dcff8c-9633-48d6-bb6e-6d694f806c24",
      "number": 41,
      "content": "Nhưng Chúa lại cất nhắc",
      "order": 0,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.675Z",
      "updatedAt": "2025-04-22T05:37:49.675Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "398738f4-beb6-400f-a23c-b6745d2c671a",
      "number": 41,
      "content": "kẻ nghèo hèn thoát cảnh lầm than,",
      "order": 1,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.689Z",
      "updatedAt": "2025-04-22T05:37:49.689Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "6ef00a94-d973-4111-92eb-98bbf3f17119",
      "number": 41,
      "content": "ban cho cả giống dòng",
      "order": 2,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.703Z",
      "updatedAt": "2025-04-22T05:37:49.703Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "6059db85-aafc-484c-9c66-31729a8d8f66",
      "number": 41,
      "content": "như chiên cừu sinh năm đẻ bảy.",
      "order": 3,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.717Z",
      "updatedAt": "2025-04-22T05:37:49.717Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "bbcda07a-7843-41ba-809f-4fc6cdf74296",
      "number": 42,
      "content": "Người chính trực thấy thế mà mừng rỡ,",
      "order": 0,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.731Z",
      "updatedAt": "2025-04-22T05:37:49.731Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "a739f8a7-980d-4ade-977e-a52daf14984a",
      "number": 42,
      "content": "bọn gian tà chẳng dám hé môi.",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.759Z",
      "updatedAt": "2025-04-22T05:37:49.759Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "1ed0a6f9-6a88-4e97-895c-69fb47ab08ff",
      "number": 43,
      "content": "Ai kẻ hiền nhân, hãy để tâm suy nghĩ,",
      "order": 0,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.773Z",
      "updatedAt": "2025-04-22T05:37:49.773Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "4368cdf3-4952-4e87-844b-a09b72d09f89",
      "number": 43,
      "content": "thì sẽ am tường lòng CHÚA thương yêu.",
      "order": 1,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:37:49.787Z",
      "updatedAt": "2025-04-22T05:37:49.787Z",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    }
  ],
  "footnotes": [
    {
      "id": "2f19b9c8-91cd-4d5d-afa5-26b34d0921e8",
      "content": "Tv 107 là *một Tv hỗn hợp* gồm hai phần : lời tạ ơn của tập thể (cc. 4-32) và thánh thi (cc. 33-43). Phần I (cc. 4-32) có một cấu trúc riêng biệt : Có bốn đoạn trong phần I (cc. 4-9 ; 10-16 ; 17-22 ; 23-32) ; mỗi đoạn đều gồm bốn điểm sau đây : **1**) Hoàn cảnh bi đát (cc. 4-5 ; 10-12 ; 17-18 ; 23-27) ; **2**) Điệp ca (cc. 6 ; 13 ; 19 ; 28) ; **3**) Thiên Chúa giải thoát (cc. 7 ; 14 ; 20 ; 29-30) ; **4**) Lời cảm tạ (cc. 8-9 ; 15-16 ; 21-22 ; 31-32). Phần II (cc. 33-43) là một thánh thi ca ngợi Đức Chúa, Đấng làm chủ thiên nhiên và điều khiển mọi biến cố.",
      "position": 48,
      "order": 0,
      "createdAt": "2025-04-22T05:37:48.363Z",
      "updatedAt": "2025-04-22T05:37:48.363Z",
      "headingId": "0c66110e-a797-41a6-af78-89922b1a1519",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "6e30992d-782e-4ef9-a604-454b1014ce09",
      "content": "*Những người được Chúa thương giải thoát* là những kẻ được Chúa cứu khỏi một hiểm hoạ lớn : **1**) Đoàn người cùng đi trong một nơi hoang vắng, hay những người Ít-ra-en thời Xuất Hành đi trong sa mạc, hoặc những người từ Ba-by-lon đi qua sa mạc Xy-ri, Ả-rập trở về Pa-lét-tin (cc. 4-5) ; **2**) Những người bị giam tù (c.10) ; **3**) Các bệnh nhân (cc. 17-18.20) ; **4**) Các thương gia vượt biển (c.23).",
      "position": 39,
      "order": 1,
      "createdAt": "2025-04-22T05:37:48.454Z",
      "updatedAt": "2025-04-22T05:37:48.454Z",
      "verseId": "28687ff9-83a3-423a-aa28-ec1f27193a8d",
      "chapterId": "4cde2da9-42d7-43c6-b82d-9e55a74ac387"
    },
    {
      "id": "59059829-c892-4b04-83a7-205c44487745",
      "content": "English: Most High - French: Très-Haut - Latin: Altissimus - Origin: &#1506;&#1462;&#1500;&#1456;&#1497;&#64331;&#1503; &#933;&#968;&#953;&#963;&#964;&#959;&#962; - Vietnamese: Đấng Tối Cao được dùng như tên riêng của Thiên Chúa thật, Is 14,14 ; Tv 91,1.9 ; Lc 1,32",
      "position": 32,
      "order": 2,
