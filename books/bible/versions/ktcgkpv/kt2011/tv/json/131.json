  },
  "verses": [
    {
      "id": "ebd8814e-f1f8-4be3-9bed-ceb1559ceb8b",
      "number": 1,
      "content": "*Ca khúc lên Đền. Của vua Đa-vít.*",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:42:53.936Z",
      "updatedAt": "2025-04-22T05:42:53.936Z",
      "chapterId": "34df4b53-91bb-456e-9452-def605648185"
    },
    {
      "id": "aa5dbb1d-e817-4749-ac7a-d2b49d08c3ea",
      "number": 1,
      "content": "Lòng con chẳng dám tự cao,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:42:53.999Z",
      "updatedAt": "2025-04-22T05:42:53.999Z",
      "chapterId": "34df4b53-91bb-456e-9452-def605648185"
    },
    {
      "id": "ee632f97-e074-4e45-bd3c-81db78c0d2e5",
      "number": 1,
      "content": "mắt con chẳng dám tự hào, CHÚA ơi !",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:42:54.013Z",
      "updatedAt": "2025-04-22T05:42:54.013Z",
      "chapterId": "34df4b53-91bb-456e-9452-def605648185"
    },
    {
      "id": "667ff556-2170-488c-b011-35d6a6c5bb0b",
      "number": 1,
      "content": "Đường cao vọng, chẳng đời nào bước,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:42:54.028Z",
      "updatedAt": "2025-04-22T05:42:54.028Z",
      "chapterId": "34df4b53-91bb-456e-9452-def605648185"
    },
    {
      "id": "ea385edc-1ec4-4648-89e8-b5c9090b2a00",
      "number": 1,
      "content": "việc diệu kỳ vượt sức, chẳng cầu ;",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:42:54.042Z",
      "updatedAt": "2025-04-22T05:42:54.042Z",
      "chapterId": "34df4b53-91bb-456e-9452-def605648185"
    },
    {
      "id": "c89e4749-7d19-48e0-a433-ca5718b8bbc5",
      "number": 2,
      "content": "hồn con, con vẫn trước sau",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:42:54.057Z",
      "updatedAt": "2025-04-22T05:42:54.057Z",
      "chapterId": "34df4b53-91bb-456e-9452-def605648185"
    },
    {
      "id": "400237c2-6d21-4dae-af78-32f512767a94",
      "number": 2,
      "content": "giữ cho thinh lặng, giữ sao thanh bình.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:42:54.074Z",
      "updatedAt": "2025-04-22T05:42:54.074Z",
      "chapterId": "34df4b53-91bb-456e-9452-def605648185"
    },
    {
      "id": "3681281c-c682-40e5-964d-8ac75d6a4d0c",
      "number": 2,
      "content": "Như trẻ thơ nép mình lòng mẹ,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:42:54.089Z",
      "updatedAt": "2025-04-22T05:42:54.089Z",
      "chapterId": "34df4b53-91bb-456e-9452-def605648185"
    },
    {
      "id": "e1dfc909-adb6-47f2-a6c8-16f93e73c253",
      "number": 2,
      "content": "trong con, hồn lặng lẽ an vui.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:42:54.103Z",
      "updatedAt": "2025-04-22T05:42:54.103Z",
      "chapterId": "34df4b53-91bb-456e-9452-def605648185"
    },
    {
      "id": "5e6a29b3-4d2a-4d80-8721-417321370ebe",
      "number": 3,
      "content": "Cậy vào CHÚA, Ít-ra-en ơi,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:42:54.118Z",
      "updatedAt": "2025-04-22T05:42:54.118Z",
      "chapterId": "34df4b53-91bb-456e-9452-def605648185"
    },
    {
      "id": "49b16292-1a37-442d-be74-b51235fe7033",
      "number": 3,
      "content": "từ nay đến mãi muôn đời muôn năm.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:42:54.145Z",
      "updatedAt": "2025-04-22T05:42:54.145Z",
      "chapterId": "34df4b53-91bb-456e-9452-def605648185"
    }
  ],
