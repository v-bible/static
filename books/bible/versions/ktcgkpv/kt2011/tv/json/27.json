{
  "chapter": {
    "id": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b",
    "number": 27,
    "ref": "",
    "createdAt": "2025-04-22T00:48:31.713Z",
    "updatedAt": "2025-04-22T00:48:31.713Z",
    "bookId": "9beff2a2-3de1-4b2d-b189-6eadb8dd6815"
  },
  "verses": [
    {
      "id": "4e148872-785c-4f17-b042-306a1d32dac3",
      "number": 1,
      "content": "*Của vua Đa-vít.*",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T05:26:00.992Z",
      "updatedAt": "2025-04-22T05:26:00.992Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "f0bf62d9-a0b3-46b9-881f-ae533da15a6c",
      "number": 1,
      "content": "CHÚA là nguồn ánh sáng và ơn cứu độ của tôi,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.082Z",
      "updatedAt": "2025-04-22T05:26:01.082Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "4fa7dac8-f6bb-4717-9afe-bfd3bff7e369",
      "number": 1,
      "content": "tôi còn sợ người nào ?",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.096Z",
      "updatedAt": "2025-04-22T05:26:01.096Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "07038ce4-6934-4971-a0bd-1204b9412e4f",
      "number": 1,
      "content": "CHÚA là thành luỹ bảo vệ đời tôi,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.111Z",
      "updatedAt": "2025-04-22T05:26:01.111Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "e10f4e08-ff18-474f-a10a-31ce566e07ed",
      "number": 1,
      "content": "tôi khiếp gì ai nữa ?",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.125Z",
      "updatedAt": "2025-04-22T05:26:01.125Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "840b4070-4199-4241-904f-3acd99349664",
      "number": 2,
      "content": "Khi ác nhân xông vào, định nuốt sống thân tôi,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.139Z",
      "updatedAt": "2025-04-22T05:26:01.139Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "5309ed6e-2d6b-4f47-bd46-814a0d1c93a5",
      "number": 2,
      "content": "ai ngờ chính đối phương, chính những thù địch ấy,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.153Z",
      "updatedAt": "2025-04-22T05:26:01.153Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "c4ec0085-ea20-46de-98ad-c9e12742279d",
      "number": 2,
      "content": "lại lảo đảo té nhào.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.167Z",
      "updatedAt": "2025-04-22T05:26:01.167Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "04180b4e-3ecf-4eb1-8153-6e9a0c7fece1",
      "number": 3,
      "content": "Dù cả một đạo quân vây đánh, lòng tôi chẳng sợ gì.",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.181Z",
      "updatedAt": "2025-04-22T05:26:01.181Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "ef78ae2e-3f9d-47fe-bb8e-7a6bfc85f78c",
      "number": 3,
      "content": "Dù có phải lâm vào chiến trận, tôi vẫn cứ cậy tin.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.196Z",
      "updatedAt": "2025-04-22T05:26:01.196Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "9091ef08-b3c7-42d0-9e69-eee11f8f2792",
      "number": 4,
      "content": "Một điều tôi kiếm tôi xin,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.209Z",
      "updatedAt": "2025-04-22T05:26:01.209Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "3c712ef2-820f-411a-b935-4d833b20cf27",
      "number": 4,
      "content": "là luôn được ở trong đền CHÚA tôi",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.223Z",
      "updatedAt": "2025-04-22T05:26:01.223Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "9575b9ec-8153-400a-88bb-49b51914b963",
      "number": 4,
      "content": "mọi ngày trong suốt cuộc đời,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.238Z",
      "updatedAt": "2025-04-22T05:26:01.238Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "8b85fbcd-3557-489c-830b-fd69dd1706d0",
      "number": 4,
      "content": "để chiêm ngưỡng CHÚA tuyệt vời cao sang,",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.252Z",
      "updatedAt": "2025-04-22T05:26:01.252Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "5b5c1961-a8b9-4ffd-92b4-7237d538f416",
      "number": 4,
      "content": "ngắm xem thánh điện huy hoàng.",
      "order": 4,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.267Z",
      "updatedAt": "2025-04-22T05:26:01.267Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "5d17d92a-31be-413b-82b8-43ade117a504",
      "number": 5,
      "content": "Ngày tôi gặp tai ương hoạn nạn,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.281Z",
      "updatedAt": "2025-04-22T05:26:01.281Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "8cf34bc1-edfc-4e6e-aad0-5446eeec0441",
      "number": 5,
      "content": "Người che chở tôi trong lều thánh,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.295Z",
      "updatedAt": "2025-04-22T05:26:01.295Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "7641fd49-4fa8-467c-957a-3394beb7f16a",
      "number": 5,
      "content": "đem giấu tôi thật kín trong nhà,",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.312Z",
      "updatedAt": "2025-04-22T05:26:01.312Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "1c0e30e9-f8d9-4768-87bd-151ad338ee10",
      "number": 5,
      "content": "đặt an toàn trên tảng đá cao.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.327Z",
      "updatedAt": "2025-04-22T05:26:01.327Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "c190268c-2b87-4bf9-bb73-c7bc90db1719",
      "number": 6,
      "content": "Nên giờ đây tôi ngẩng đầu đắc ý,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.354Z",
      "updatedAt": "2025-04-22T05:26:01.354Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "0954f274-0675-4f65-b896-7f3a58285582",
      "number": 6,
      "content": "nhìn quân thù vây bủa chung quanh.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.369Z",
      "updatedAt": "2025-04-22T05:26:01.369Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "4955639d-09c2-4296-8631-bfcf4497f08c",
      "number": 6,
      "content": "Tôi sẽ dâng lễ tế trong thánh điện,",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.383Z",
      "updatedAt": "2025-04-22T05:26:01.383Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "3eebd81c-3ff2-4a7a-b0a6-30656341756f",
      "number": 6,
      "content": "lễ tạ ơn, nhã nhạc vang lừng,",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.397Z",
      "updatedAt": "2025-04-22T05:26:01.397Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "c348dc1b-3e70-43e5-b214-175ec453c523",
      "number": 6,
      "content": "tôi sẽ đàn ca mừng kính CHÚA.",
      "order": 4,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.425Z",
      "updatedAt": "2025-04-22T05:26:01.425Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "54fb6b9d-37ab-4e2e-8b52-39d258417752",
      "number": 7,
      "content": "Lạy CHÚA, cúi xin Ngài nghe tiếng con kêu,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.438Z",
      "updatedAt": "2025-04-22T05:26:01.438Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "afa4f8b5-00b9-41d5-8ca0-dcd87b9f6817",
      "number": 7,
      "content": "xin thương tình đáp lại.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.452Z",
      "updatedAt": "2025-04-22T05:26:01.452Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "363f9073-d75f-4375-b27e-3b8ab0e5ba08",
      "number": 8,
      "content": "Nghĩ về Ngài, lòng con tự nhủ : hãy tìm kiếm Thánh Nhan.",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.466Z",
      "updatedAt": "2025-04-22T05:26:01.466Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "dc45ed98-9712-492c-b040-90eb01592fdf",
      "number": 8,
      "content": "Lạy CHÚA, con tìm thánh nhan Ngài,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.480Z",
      "updatedAt": "2025-04-22T05:26:01.480Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "6f7c7d56-bfcb-4604-84f1-4c46fe01e662",
      "number": 9,
      "content": "xin Ngài đừng ẩn mặt.",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.495Z",
      "updatedAt": "2025-04-22T05:26:01.495Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "72ad1501-eec7-410d-8a37-e84ad6ea8f37",
      "number": 9,
      "content": "Tôi tớ Ngài đây, xin đừng giận mà ruồng rẫy,",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.509Z",
      "updatedAt": "2025-04-22T05:26:01.509Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "f421e7bf-e6cb-4116-acb0-91761918f329",
      "number": 9,
      "content": "chính Ngài là Đấng phù trợ con.",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.523Z",
      "updatedAt": "2025-04-22T05:26:01.523Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "5f494d75-60df-41a5-8a8b-6eb0239b9639",
      "number": 9,
      "content": "Xin chớ bỏ rơi, xin đừng xua đuổi,",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.538Z",
      "updatedAt": "2025-04-22T05:26:01.538Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "cc94d81e-4f35-41c1-9d9d-2329034cebe3",
      "number": 9,
      "content": "lạy Thiên Chúa, Đấng cứu độ con.",
      "order": 4,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.552Z",
      "updatedAt": "2025-04-22T05:26:01.552Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "4060802f-3e88-4ba2-86c8-ce880e9dbada",
      "number": 10,
      "content": "Dầu cha mẹ có bỏ con đi nữa,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.565Z",
      "updatedAt": "2025-04-22T05:26:01.565Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "74ec88f9-8ab2-45f9-ac27-9385e8f27228",
      "number": 10,
      "content": "thì hãy còn có CHÚA đón nhận con.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.578Z",
      "updatedAt": "2025-04-22T05:26:01.578Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "a089707f-30b3-4b2e-a677-4fb6a396605c",
      "number": 11,
      "content": "Xin dạy con đường nẻo Ngài, lạy CHÚA,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.592Z",
      "updatedAt": "2025-04-22T05:26:01.592Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "1a2779bd-ad7b-4772-8d80-dffd3b17506f",
      "number": 11,
      "content": "dẫn con đi trên lối phẳng phiu,",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.606Z",
      "updatedAt": "2025-04-22T05:26:01.606Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "9ecff111-1464-460e-b66c-809dca423e8c",
      "number": 11,
      "content": "vì có những người đang rình rập.",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.621Z",
      "updatedAt": "2025-04-22T05:26:01.621Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "6cd661fd-d6dc-4318-82d3-e94c0f2efceb",
      "number": 12,
      "content": "Xin đừng phó mặc con cho kẻ thù hung hãn,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.635Z",
      "updatedAt": "2025-04-22T05:26:01.635Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "54a4e497-286a-474f-87d0-2154bab62a1b",
      "number": 12,
      "content": "vì lũ chứng gian đứng dậy tố con,",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.649Z",
      "updatedAt": "2025-04-22T05:26:01.649Z",
      "chapterId": "5fc7dd1e-afdc-4713-8299-6b5fe0598a8b"
    },
    {
      "id": "01a6079a-9fd2-4f99-8023-38ed47324c65",
      "number": 12,
      "content": "giương bộ mặt hằm hằm sát khí.",
      "order": 2,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T05:26:01.662Z",
      "updatedAt": "2025-04-22T05:26:01.662Z",
