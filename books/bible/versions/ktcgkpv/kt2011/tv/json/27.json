    },
    {
      "id": "aab5ca54-0ed3-45f6-8b4a-87dffe92e496",
      "number": 13,
      "content": "sẽ được thấy ân lộc CHÚA ban trong cõi đất dành cho kẻ sống.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
