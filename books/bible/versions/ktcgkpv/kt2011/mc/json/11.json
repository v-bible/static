  "headings": [
    {
      "id": "8340a34b-b9f2-40d5-a5e2-a0862b9dab75",
      "content": "IV. SỨ VỤ CỦA ĐỨC GIÊ-SU TẠI GIÊ-RU-SA-LEM",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T01:04:04.959Z",
      "updatedAt": "2025-04-22T01:04:04.959Z",
      "verseId": "0b48399a-b4bf-4e9d-ab3e-10c811236410",
      "chapterId": "55d683e8-4d6d-4b81-8dc2-54f433241278"
    },
    {
      "id": "c01651d8-21ad-4760-92c6-f2f57905d4d3",
      "content": "Tranh luận về thẩm quyền của Đức Giê-su",
      "level": 2,
      "order": 0,
      "createdAt": "2025-04-22T01:04:05.483Z",
      "updatedAt": "2025-04-22T01:04:05.483Z",
      "verseId": "0e1553b5-cac6-4cc0-b22c-40a5c2f6f3bb",
      "chapterId": "55d683e8-4d6d-4b81-8dc2-54f433241278"
    },
    {
      "id": "a700270f-4995-45c0-a22d-00b111ad2597",
      "content": "Cây vả bị rủa",
      "level": 2,
      "order": 0,
      "createdAt": "2025-04-22T01:04:05.194Z",
      "updatedAt": "2025-04-22T01:04:05.194Z",
      "verseId": "4d3f6109-d2d4-4cb1-b6c2-2895dbf93536",
      "chapterId": "55d683e8-4d6d-4b81-8dc2-54f433241278"
    },
    {
      "id": "fce28b4a-26a3-4fcb-9bcc-47e92c5f9d63",
      "content": "Đức Giê-su đuổi những người buôn bán ra khỏi Đền Thờ",
      "level": 2,
      "order": 0,
      "createdAt": "2025-04-22T01:04:05.244Z",
      "updatedAt": "2025-04-22T01:04:05.244Z",
      "verseId": "790dd993-fd80-4a44-82e0-6e303b3a0587",
      "chapterId": "55d683e8-4d6d-4b81-8dc2-54f433241278"
    },
    {
      "id": "ad0e25cf-5741-4b47-b8e5-e67550b6685a",
      "content": "Cây vả bị khô héo. Đức tin và cầu nguyện.",
      "level": 2,
      "order": 0,
      "createdAt": "2025-04-22T01:04:05.333Z",
      "updatedAt": "2025-04-22T01:04:05.333Z",
      "verseId": "8bd622d0-6f3f-4bd1-a123-e8cfe22be726",
      "chapterId": "55d683e8-4d6d-4b81-8dc2-54f433241278"
    },
    {
      "id": "5c2f5062-08a0-4b57-bd46-7b05388b156c",
      "content": "Đức Giê-su vào Giê-ru-sa-lem với tư cách Mê-si-a",
      "level": 2,
      "order": 1,
      "createdAt": "2025-04-22T01:04:04.969Z",
      "updatedAt": "2025-04-22T01:04:04.969Z",
      "verseId": "0b48399a-b4bf-4e9d-ab3e-10c811236410",
      "chapterId": "55d683e8-4d6d-4b81-8dc2-54f433241278"
    }
  ],
