  "headings": [
    {
      "id": "b8531b71-89cb-4e2f-8e1f-80bf868e67ed",
      "content": "Xây bàn thờ trên bờ sông Gio-đan",
      "level": 2,
      "order": 0,
      "createdAt": "2025-04-22T03:30:25.932Z",
      "updatedAt": "2025-04-22T03:30:25.932Z",
      "verseId": "02739bdf-51e7-4e1d-a356-56c9f9ac92ca",
      "chapterId": "3dd4c177-a469-42ea-b334-7311125e2d11"
    },
    {
      "id": "c42ccd77-44c6-4059-acff-857810b568c9",
      "content": "Các chi tộc bên kia sông Gio-đan tự biện hộ",
      "level": 2,
      "order": 0,
      "createdAt": "2025-04-22T03:30:26.397Z",
      "updatedAt": "2025-04-22T03:30:26.397Z",
      "verseId": "5fab6ffa-f6c2-49eb-b6c2-29cf96e59ad3",
      "chapterId": "3dd4c177-a469-42ea-b334-7311125e2d11"
    },
    {
      "id": "9bb20f15-1184-491d-a435-7b252ad10f2b",
      "content": "Con cái Ít-ra-en trách móc các chi tộc ở phía đông",
      "level": 2,
      "order": 0,
      "createdAt": "2025-04-22T03:30:26.065Z",
      "updatedAt": "2025-04-22T03:30:26.065Z",
      "verseId": "6ebdd18e-2dd6-440b-93a4-80ede9af8a8d",
      "chapterId": "3dd4c177-a469-42ea-b334-7311125e2d11"
    },
    {
      "id": "ec36757d-2b84-443d-bdcb-24475fcf9a62",
      "content": "III. CUỐI ĐỜI ÔNG GIÔ-SUÊ",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T03:30:25.493Z",
      "updatedAt": "2025-04-22T03:30:25.493Z",
      "verseId": "72a71f16-3f8d-4474-a8da-0765c7a395ce",
      "chapterId": "3dd4c177-a469-42ea-b334-7311125e2d11"
    },
    {
      "id": "9aabdbe0-466e-406c-9d4a-adc6a430c41e",
      "content": "Hai bên thoả hiệp với nhau",
      "level": 2,
      "order": 0,
      "createdAt": "2025-04-22T03:30:26.701Z",
      "updatedAt": "2025-04-22T03:30:26.701Z",
      "verseId": "d5e4f647-cdd5-4d02-a244-a531b79e18c5",
      "chapterId": "3dd4c177-a469-42ea-b334-7311125e2d11"
    },
    {
      "id": "b416b928-fbd0-4cc9-9f3f-1a5d359b9574",
      "content": "1. CÁC CHI TỘC BÊN KIA SÔNG GIO-ĐAN TRỞ VỀ. VẤN ĐỀ BÀN THỜ CỦA HỌ.",
      "level": 1,
      "order": 1,
      "createdAt": "2025-04-22T03:30:25.523Z",
      "updatedAt": "2025-04-22T03:30:25.523Z",
      "verseId": "72a71f16-3f8d-4474-a8da-0765c7a395ce",
      "chapterId": "3dd4c177-a469-42ea-b334-7311125e2d11"
    },
    {
      "id": "b4022421-2381-435d-9a81-e4d8c6021edd",
      "content": "Đưa quân của các chi tộc bên kia sông Gio-đan trở về",
      "level": 2,
      "order": 2,
      "createdAt": "2025-04-22T03:30:25.551Z",
      "updatedAt": "2025-04-22T03:30:25.551Z",
      "verseId": "72a71f16-3f8d-4474-a8da-0765c7a395ce",
      "chapterId": "3dd4c177-a469-42ea-b334-7311125e2d11"
    }
  ],
