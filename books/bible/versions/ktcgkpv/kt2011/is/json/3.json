{
  "chapter": {
    "id": "f2cd1868-5df8-4193-bca6-a05f2202372a",
    "number": 3,
    "ref": "",
    "createdAt": "2025-04-22T00:48:32.209Z",
    "updatedAt": "2025-04-22T00:48:32.209Z",
    "bookId": "60b7a09b-afb6-41c3-8441-2d0936fa1d5e"
  },
  "verses": [
    {
      "id": "f6d52cfe-2ea9-4d0e-b004-e5489c5e32f6",
      "number": 1,
      "content": "chỗ tựa chỗ nương là bánh và nước,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:00.827Z",
      "updatedAt": "2025-04-22T06:19:00.827Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "4f35eda7-3265-40e5-80b5-e8e584db8a42",
      "number": 2,
      "content": "người hùng và chiến sĩ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:00.845Z",
      "updatedAt": "2025-04-22T06:19:00.845Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "34254b41-c565-40c3-9d34-2e3857bfd429",
      "number": 2,
      "content": "quan toà và ngôn sứ,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:00.857Z",
      "updatedAt": "2025-04-22T06:19:00.857Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "2d1031cc-71b7-4f6d-963b-bd09ab1cd850",
      "number": 2,
      "content": "thầy bói và kỳ mục,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:00.869Z",
      "updatedAt": "2025-04-22T06:19:00.869Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "b498900a-3e7f-4e07-9ed7-6eca1e6bc7f9",
      "number": 3,
      "content": "sĩ quan và thân hào,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:00.882Z",
      "updatedAt": "2025-04-22T06:19:00.882Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "0ac43808-f257-4654-9941-6bfd52058b0e",
      "number": 3,
      "content": "cố vấn với phù thuỷ cao tay",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:00.896Z",
      "updatedAt": "2025-04-22T06:19:00.896Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "4cd2f1a6-4f0b-4619-8826-23b2f4b4ab21",
      "number": 3,
      "content": "và những kẻ thạo nghề bùa ngải.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:00.910Z",
      "updatedAt": "2025-04-22T06:19:00.910Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "ef1b24ef-ef42-4130-ae2f-7c0f0062427f",
      "number": 4,
      "content": "Ta sẽ đặt bọn nhãi con",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:00.924Z",
      "updatedAt": "2025-04-22T06:19:00.924Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "de6d6601-eb03-4b7c-b91f-2c7214e7c572",
      "number": 4,
      "content": "làm người chỉ huy chúng,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:00.953Z",
      "updatedAt": "2025-04-22T06:19:00.953Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "fc53ae8b-5269-4512-890b-249ca66a2192",
      "number": 4,
      "content": "và lũ trẻ ranh",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:00.968Z",
      "updatedAt": "2025-04-22T06:19:00.968Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "116189f6-ba68-4dbb-a675-444f470c478c",
      "number": 4,
      "content": "làm người cai trị chúng.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:00.985Z",
      "updatedAt": "2025-04-22T06:19:00.985Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "b49084fa-784e-4bfb-a69b-f7373dfc9a96",
      "number": 5,
      "content": "Dân chúng sẽ ức hiếp lẫn nhau,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01Z",
      "updatedAt": "2025-04-22T06:19:01Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "ceebc668-3671-4df7-9d85-3591a5e7d399",
      "number": 5,
      "content": "người thân cận ức hiếp người thân cận ;",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.014Z",
      "updatedAt": "2025-04-22T06:19:01.014Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "c280444c-5186-473b-a1fe-e3cfb373776a",
      "number": 5,
      "content": "nhãi con hỗn láo với ông già,",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.028Z",
      "updatedAt": "2025-04-22T06:19:01.028Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "99af032b-a0f4-457a-a87c-5a7bdf9b088f",
      "number": 5,
      "content": "đứa vô loại hỗn láo với người quyền quý.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.042Z",
      "updatedAt": "2025-04-22T06:19:01.042Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "5982fd77-7916-4ae7-82cd-83580fec4e74",
      "number": 6,
      "content": "Người ta sẽ níu lấy người anh em trong nhà cha mình mà nói :",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.057Z",
      "updatedAt": "2025-04-22T06:19:01.057Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "20121c82-f789-4bcb-a544-b16ca9440b46",
      "number": 6,
      "content": "“Anh còn áo choàng,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.071Z",
      "updatedAt": "2025-04-22T06:19:01.071Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "c81d13c2-6a43-4cf2-80f0-ef4ecaf19b6e",
      "number": 6,
      "content": "hãy làm đầu chúng tôi,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.085Z",
      "updatedAt": "2025-04-22T06:19:01.085Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "3b292be8-d748-4795-9ce3-9a12f265c5b9",
      "number": 6,
      "content": "và đống hoang tàn này",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.099Z",
      "updatedAt": "2025-04-22T06:19:01.099Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "70820d18-aaef-47d6-b90c-959d3fef3cb1",
      "number": 6,
      "content": "sẽ ở dưới quyền anh.”",
      "order": 4,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.114Z",
      "updatedAt": "2025-04-22T06:19:01.114Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "999b5208-51d5-4fae-a4a1-5704324fa522",
      "number": 7,
      "content": "Ngày đó,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.128Z",
      "updatedAt": "2025-04-22T06:19:01.128Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "6456572a-d1c7-45a4-9ee5-ff34ac9bba6e",
      "number": 7,
      "content": "người kia sẽ lên tiếng nói :",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.142Z",
      "updatedAt": "2025-04-22T06:19:01.142Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "b59af3c8-350c-4b67-bd6f-5d4cc0ae70d0",
      "number": 7,
      "content": "“Tôi không muốn làm thầy thuốc đâu,",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.160Z",
      "updatedAt": "2025-04-22T06:19:01.160Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "795960f6-e0b3-4966-83b3-e57bd336d59f",
      "number": 7,
      "content": "và trong nhà tôi chẳng có bánh, cũng chẳng có áo choàng,",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.174Z",
      "updatedAt": "2025-04-22T06:19:01.174Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "d70222ed-0975-462d-9d3c-0ef8e3d765ef",
      "number": 7,
      "content": "xin đừng đặt tôi làm đầu dân.”",
      "order": 4,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.188Z",
      "updatedAt": "2025-04-22T06:19:01.188Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "0eaaccd8-84d8-49c8-bf8e-d06fa75ed815",
      "number": 8,
      "content": "Giê-ru-sa-lem sẩy chân,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.202Z",
      "updatedAt": "2025-04-22T06:19:01.202Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "2a39bb42-baa5-4249-8346-739dad55b1aa",
      "number": 8,
      "content": "Giu-đa té nhào,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.231Z",
      "updatedAt": "2025-04-22T06:19:01.231Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "edf3165e-7c74-4179-85ca-b8327366d006",
      "number": 8,
      "content": "vì chúng đã nói và hành động chống lại ĐỨC CHÚA,",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.245Z",
      "updatedAt": "2025-04-22T06:19:01.245Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "db189ac3-62c8-49dd-af1d-62791dabe214",
      "number": 8,
      "content": "phản loạn trước tôn nhan vinh hiển của Người.",
      "order": 3,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.274Z",
      "updatedAt": "2025-04-22T06:19:01.274Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "58d57ccf-a50b-418b-aba9-e0795fd144df",
      "number": 9,
      "content": "Vẻ mặt của chúng là bằng chứng cáo tội chúng ;",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.288Z",
      "updatedAt": "2025-04-22T06:19:01.288Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "8317350b-d0b8-4249-9a90-abcb6644f737",
      "number": 9,
      "content": "chúng khoe tội mình như Xơ-đôm, chứ không giấu giếm.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.303Z",
      "updatedAt": "2025-04-22T06:19:01.303Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "ff99ebd6-1e36-48f9-95ea-19a68138d786",
      "number": 9,
      "content": "Thật khốn cho chúng, vì chúng tự chuốc hoạ vào thân.",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.346Z",
      "updatedAt": "2025-04-22T06:19:01.346Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "beea4cf1-6748-4a6c-a15b-6f420868034b",
      "number": 10,
      "content": "người công chính thật có phúc,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.374Z",
      "updatedAt": "2025-04-22T06:19:01.374Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "d64229f1-c121-49e9-98d7-9696ae94b5c0",
      "number": 10,
      "content": "vì kết quả việc họ làm,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.389Z",
      "updatedAt": "2025-04-22T06:19:01.389Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "7b1532fe-af1b-45eb-8b29-9fad47b4d295",
      "number": 10,
      "content": "họ sẽ được dùng được hưởng.",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.404Z",
      "updatedAt": "2025-04-22T06:19:01.404Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "7655a062-5c81-4c72-bd3d-13a0d532bf5d",
      "number": 11,
      "content": "Khốn thay kẻ gian ác,",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.417Z",
      "updatedAt": "2025-04-22T06:19:01.417Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "22d7eeeb-7414-408c-b3de-9c3846af6e33",
      "number": 11,
      "content": "thật khốn thay,",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.431Z",
      "updatedAt": "2025-04-22T06:19:01.431Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "f3bfc81b-6e0d-4520-938d-e8bbc7e5924b",
      "number": 11,
      "content": "vì hậu quả việc tay nó làm,",
      "order": 2,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.445Z",
      "updatedAt": "2025-04-22T06:19:01.445Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "96dcd75a-7df6-48d3-8660-d2ff3378e304",
      "number": 11,
      "content": "nó sẽ phải chuốc lấy.",
      "order": 3,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.460Z",
      "updatedAt": "2025-04-22T06:19:01.460Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "e4618c91-a340-477d-a0ea-733959d7a43d",
      "number": 12,
      "content": "những kẻ cai trị nó lại là đàn bà.",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.475Z",
      "updatedAt": "2025-04-22T06:19:01.475Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "46091025-a2cc-45b7-a82f-4c766794c550",
      "number": 12,
      "content": "Ôi dân Ta, những kẻ dẫn dắt ngươi làm ngươi lạc hướng,",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.489Z",
      "updatedAt": "2025-04-22T06:19:01.489Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "7336bd51-54b5-4eb6-945c-6315dc3ae891",
      "number": 12,
      "content": "đường lối ngươi đi,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.503Z",
      "updatedAt": "2025-04-22T06:19:01.503Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "2f624627-605d-48c4-b31b-dbe32939c666",
      "number": 12,
      "content": "chúng làm cho rối loạn !",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.518Z",
      "updatedAt": "2025-04-22T06:19:01.518Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "800e8b90-9dd2-49a0-a843-770e657b7b43",
      "number": 14,
      "content": "ĐỨC CHÚA đưa ra toà xét xử",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.532Z",
      "updatedAt": "2025-04-22T06:19:01.532Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "fb42bfd4-81eb-4df3-90f1-7804507d1bf3",
      "number": 14,
      "content": "hàng kỳ mục và những kẻ lãnh đạo dân Người :",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.560Z",
      "updatedAt": "2025-04-22T06:19:01.560Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "98e744b2-80c0-4c70-a1bd-eb718f432730",
      "number": 14,
      "content": "“Chính các ngươi đã ăn hại vườn nho.",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.573Z",
      "updatedAt": "2025-04-22T06:19:01.573Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "eb4c7ab5-26b2-4c61-822e-4426b1a609a1",
      "number": 14,
      "content": "Của cải người nghèo các ngươi đã bóc lột",
      "order": 3,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.603Z",
      "updatedAt": "2025-04-22T06:19:01.603Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "2c347f78-9f1e-4f84-a9ff-e24d72d279b1",
      "number": 14,
      "content": "còn đang ở trong nhà các ngươi.",
      "order": 4,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.617Z",
      "updatedAt": "2025-04-22T06:19:01.617Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "f36524a0-3c80-4ce7-b9d7-34214e23a269",
      "number": 15,
      "content": "Cớ sao các ngươi lại chà đạp dân Ta,",
      "order": 0,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.632Z",
      "updatedAt": "2025-04-22T06:19:01.632Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "a77eb54b-2114-4d39-bc8d-4609eb3150df",
      "number": 15,
      "content": "làm tan nát mặt mày những kẻ nghèo khó ?”",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.666Z",
      "updatedAt": "2025-04-22T06:19:01.666Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "647cddd0-517c-4de1-9a61-9fd45c9e4da1",
      "number": 15,
      "content": "Sấm ngôn của Chúa Thượng, ĐỨC CHÚA các đạo binh !",
      "order": 2,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.681Z",
      "updatedAt": "2025-04-22T06:19:01.681Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "8127e703-4f34-4af7-bd02-54d9041ee627",
      "number": 16,
      "content": "ĐỨC CHÚA phán : Vì các con gái Xi-on kiêu kỳ,",
      "order": 0,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.709Z",
      "updatedAt": "2025-04-22T06:19:01.709Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "f82577c2-a026-4289-85f4-cf21501c80d8",
      "number": 16,
      "content": "chúng vừa đi vừa vênh mặt, liếc mắt,",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.807Z",
      "updatedAt": "2025-04-22T06:19:01.807Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "9f6687a8-225b-4249-91d7-b9c825e1928b",
      "number": 16,
      "content": "chúng bước đi nhún nhẩy, khua kiềng đeo chân,",
      "order": 2,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.821Z",
      "updatedAt": "2025-04-22T06:19:01.821Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "fed5e4f8-a8fa-4daa-a080-ef84d8147cdf",
      "number": 17,
      "content": "nên Chúa Thượng sẽ làm cho con gái Xi-on ghẻ chốc đầy đầu,",
      "order": 0,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.835Z",
      "updatedAt": "2025-04-22T06:19:01.835Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "ac999383-1dcd-48ad-b2d3-4172ef4e62ea",
      "number": 17,
      "content": "và ĐỨC CHÚA sẽ lột trần chúng.",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.871Z",
      "updatedAt": "2025-04-22T06:19:01.871Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "eec616a3-742a-437a-8cf3-1b4c791e0fcf",
      "number": 18,
      "content": "Ngày đó, Chúa Thượng sẽ lấy đi đồ trang sức : kiềng, hình mặt trời, hình trăng khuyết,",
      "order": 0,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T06:19:01.894Z",
      "updatedAt": "2025-04-22T06:19:01.894Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "fdb445a1-0b51-49e8-b6ce-a9752a3aa8d5",
      "number": 19,
      "content": "vòng cổ, xuyến, khăn trùm,",
      "order": 0,
      "parNumber": 60,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2025-04-22T06:19:01.908Z",
      "updatedAt": "2025-04-22T06:19:01.908Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "40f61c2c-9795-4618-af8f-a3e9fee31245",
      "number": 20,
      "content": "khăn chít, xà tích, thắt lưng, hộp phấn son, bùa phép,",
      "order": 0,
      "parNumber": 60,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2025-04-22T06:19:01.922Z",
      "updatedAt": "2025-04-22T06:19:01.922Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "d594b22a-ba75-4b83-b548-6142956c9ed8",
      "number": 21,
      "content": "nhẫn, khuyên mũi,",
      "order": 0,
      "parNumber": 60,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2025-04-22T06:19:01.936Z",
      "updatedAt": "2025-04-22T06:19:01.936Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "9c5f8b1e-ad8e-442f-8ccc-2cdec6ae6e07",
      "number": 22,
      "content": "áo ngày lễ, áo khoác, áo choàng, túi cầm tay,",
      "order": 0,
      "parNumber": 60,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2025-04-22T06:19:01.949Z",
      "updatedAt": "2025-04-22T06:19:01.949Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "33e4f3bd-bdbd-45c0-98f3-207fc08786a3",
      "number": 23,
      "content": "gương soi, áo vải mịn, dải cột tóc, khăn trùm dài.",
      "order": 0,
      "parNumber": 60,
      "parIndex": 5,
      "isPoetry": false,
      "createdAt": "2025-04-22T06:19:01.963Z",
      "updatedAt": "2025-04-22T06:19:01.963Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "5fd47fed-8aad-457c-9ed6-0ad3fdc6218e",
      "number": 24,
      "content": "Thay vì hương thơm",
      "order": 0,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.977Z",
      "updatedAt": "2025-04-22T06:19:01.977Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "6eb70671-fe4b-4581-b3e0-5d26f1c17c59",
      "number": 24,
      "content": "sẽ là mùi thối,",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:01.991Z",
      "updatedAt": "2025-04-22T06:19:01.991Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "88969e77-c4e4-45b6-bc28-e6ccbc045c43",
      "number": 24,
      "content": "thay vì thắt lưng",
      "order": 2,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:02.005Z",
      "updatedAt": "2025-04-22T06:19:02.005Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "a10383f7-31ec-4cff-9a6d-c931bddc95d0",
      "number": 24,
      "content": "là sợi dây thừng,",
      "order": 3,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:02.020Z",
      "updatedAt": "2025-04-22T06:19:02.020Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "9f92690a-da2e-4220-8ff6-1fa818555c8a",
      "number": 24,
      "content": "thay vì bím tóc là đầu cạo trọc,",
      "order": 4,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:02.034Z",
      "updatedAt": "2025-04-22T06:19:02.034Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "d9ea35d9-0176-44b5-8319-a8d59f22be74",
      "number": 24,
      "content": "thay vì áo đẹp là khố vải thô,",
      "order": 5,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:02.063Z",
      "updatedAt": "2025-04-22T06:19:02.063Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "6cf856c9-c90f-4e1f-9491-290fa67d882e",
      "number": 24,
      "content": "thay vì nhan sắc là vết sắt nung.",
      "order": 6,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:02.076Z",
      "updatedAt": "2025-04-22T06:19:02.076Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "18ff5d08-b330-43d6-8877-ac9eb225fc1f",
      "number": 25,
      "content": "Nam nhi của ngươi sẽ ngã gục vì gươm,",
      "order": 0,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:02.091Z",
      "updatedAt": "2025-04-22T06:19:02.091Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "65a01c10-bf6e-4d3d-a41b-91413e730d1f",
      "number": 25,
      "content": "anh hùng của ngươi sẽ tử trận.",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:02.146Z",
      "updatedAt": "2025-04-22T06:19:02.146Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "32d4c0ae-dfdb-4bb4-9473-ab9cdb401c1d",
      "number": 26,
      "content": "Các cửa thành than vãn, để tang,",
      "order": 0,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:02.160Z",
      "updatedAt": "2025-04-22T06:19:02.160Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    },
    {
      "id": "3701408a-523b-41c7-a51d-936b553451d7",
      "number": 26,
      "content": "nó sẽ ngồi dưới đất trơ trọi một mình.",
      "order": 1,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T06:19:02.174Z",
      "updatedAt": "2025-04-22T06:19:02.174Z",
      "chapterId": "f2cd1868-5df8-4193-bca6-a05f2202372a"
    }
  ],
