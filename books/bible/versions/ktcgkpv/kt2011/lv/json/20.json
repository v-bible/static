    },
    {
      "id": "a853f378-248b-4837-942d-31859c6ef083",
      "content": "X. 18,21+.",
      "position": 181,
      "order": 1,
      "createdAt": "2025-04-22T03:09:54.607Z",
      "updatedAt": "2025-04-22T03:09:54.607Z",
      "verseId": "5f445976-88f0-4d11-b933-c6bdf36cb796",
      "chapterId": "e6e251d9-dec4-48bd-9e37-464ff94cea4e"
    },
