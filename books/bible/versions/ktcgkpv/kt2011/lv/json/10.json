      "verseId": "99cedbe1-7346-4838-a3c2-f9a6b3a38d5c",
      "chapterId": "d0526856-aaff-4804-8714-02776ce681dc"
    },
    {
      "id": "d03dd5e6-81d6-4996-bb1e-a4f14b3d5db6",
      "content": "E. Quy định đặc biệt về lễ tạ tội",
      "order": 0,
      "createdAt": "2024-09-23T03:41:09.417Z",
      "updatedAt": "2024-09-23T03:41:09.417Z",
      "verseId": "51684276-ef3b-44bf-ab49-5e73b2feda68",
