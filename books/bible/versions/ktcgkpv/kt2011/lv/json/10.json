    },
    {
      "id": "d439b772-86ab-489d-bd27-9455cadeef00",
      "content": "B. Khiêng xác đi",
      "order": 0,
      "createdAt": "2024-09-23T03:41:09.332Z",
      "updatedAt": "2024-09-23T03:41:09.332Z",
      "verseId": "79497394-c16d-4c02-b2ae-64759b2987a6",
      "chapterId": "d0526856-aaff-4804-8714-02776ce681dc"
    },
    {
      "id": "ad697914-f43b-462f-ad50-34ad9dbf1e82",
      "content": "Quy định bổ sung",
      "order": 0,
      "createdAt": "2024-09-23T03:41:09.290Z",
      "updatedAt": "2024-09-23T03:41:09.290Z",
      "verseId": "41bd69aa-8e75-49f3-8470-0bdf173e2c51",
      "chapterId": "d0526856-aaff-4804-8714-02776ce681dc"
    },
    {
      "id": "7a39ab83-32bf-4e01-92e5-d02b1a8f1863",
      "content": "D. Cấm uống rượu",
      "order": 0,
      "createdAt": "2024-09-23T03:41:09.360Z",
      "updatedAt": "2024-09-23T03:41:09.360Z",
      "verseId": "18fb983a-4459-42b7-8888-6a648f90ecdf",
      "chapterId": "d0526856-aaff-4804-8714-02776ce681dc"
    },
    {
      "id": "a1e90f0c-bd02-4554-8486-57001593d6b6",
      "content": "Đ. Phần lễ tiến dành cho tư tế",
      "order": 0,
      "createdAt": "2024-09-23T03:41:09.385Z",
      "updatedAt": "2024-09-23T03:41:09.385Z",
      "verseId": "2a533cc2-346a-4810-8604-423346845455",
      "chapterId": "d0526856-aaff-4804-8714-02776ce681dc"
    },
    {
      "id": "65d1dd03-49b9-4c2d-9673-196977227aff",
      "content": "A. Trường hợp ông [Na-đáp]() và ông [A-vi-hu]()",
