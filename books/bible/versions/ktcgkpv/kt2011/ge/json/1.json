{
  "chapter": {
    "id": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1",
    "number": 1,
    "ref": "",
    "createdAt": "2025-04-22T00:48:32.398Z",
    "updatedAt": "2025-04-22T00:48:32.398Z",
    "bookId": "1d2d08ce-5c09-41e7-be83-08c5aa054ef6"
  },
  "verses": [
    {
      "id": "53218e86-a1a7-486e-aaeb-154c1d97d27a",
      "number": 1,
      "content": "Lời ĐỨC CHÚA ngỏ với ông Giô-en, con ông Pơ-thu-ên.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T07:14:10.997Z",
      "updatedAt": "2025-04-22T07:14:10.997Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "390dc0d2-182c-4fee-98c2-f49a8285b379",
      "number": 2,
      "content": "Hỡi các kỳ mục, hãy nghe những điều này,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.078Z",
      "updatedAt": "2025-04-22T07:14:11.078Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "24a0ca81-63bd-4f26-9e61-369b74484681",
      "number": 2,
      "content": "tất cả cư dân trong xứ, hãy lắng nghe !",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.147Z",
      "updatedAt": "2025-04-22T07:14:11.147Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "0d208cae-6beb-4d4b-bfc6-58719bc70358",
      "number": 2,
      "content": "Đã bao giờ xảy ra như thế trong thời của các ngươi",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.160Z",
      "updatedAt": "2025-04-22T07:14:11.160Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "3205965d-cb46-4acf-8b98-f87df15b45c5",
      "number": 2,
      "content": "hay trong thời tổ tiên các ngươi chưa ?",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.174Z",
      "updatedAt": "2025-04-22T07:14:11.174Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "85fedfd0-4126-4dfe-be20-4a21212bc145",
      "number": 3,
      "content": "Hãy kể lại cho con cái các ngươi những điều này,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.188Z",
      "updatedAt": "2025-04-22T07:14:11.188Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "814784d4-6a80-4d0a-a5eb-b78aa7f0d1de",
      "number": 3,
      "content": "rồi con cái các ngươi kể cho con cái chúng,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.202Z",
      "updatedAt": "2025-04-22T07:14:11.202Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "f09c012a-5acb-4253-973c-bd8cec2dfd3e",
      "number": 3,
      "content": "con cái chúng kể cho thế hệ sau.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.215Z",
      "updatedAt": "2025-04-22T07:14:11.215Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "5fe1a8f3-fc0e-478c-84ae-fa8f51fe3d88",
      "number": 4,
      "content": "Thứ gì châu chấu bỏ lại thì hoàng trùng ăn.",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.229Z",
      "updatedAt": "2025-04-22T07:14:11.229Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "08837e6e-400e-4696-8360-70b2a35139cc",
      "number": 4,
      "content": "Thứ gì hoàng trùng bỏ lại thì cào cào ăn.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.273Z",
      "updatedAt": "2025-04-22T07:14:11.273Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "863ecf31-f17a-43d9-aeb3-c0dfff0e198c",
      "number": 4,
      "content": "Thứ gì cào cào bỏ lại thì sạt sành ăn.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.287Z",
      "updatedAt": "2025-04-22T07:14:11.287Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "207a11ad-b33c-4961-aa4f-3d896151c9c0",
      "number": 5,
      "content": "Hỡi những kẻ say sưa, hãy tỉnh dậy và than khóc.",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.301Z",
      "updatedAt": "2025-04-22T07:14:11.301Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "dc9663a1-c371-4e4c-acc8-c157e2b619d8",
      "number": 5,
      "content": "Hết mọi tay bợm rượu, rú lên đi vì chẳng còn nước nho nữa :",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.343Z",
      "updatedAt": "2025-04-22T07:14:11.343Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "a8e77aa7-10b6-4fd2-928b-fa7df30400b3",
      "number": 5,
      "content": "nước nho kề miệng đã bị giựt mất rồi !",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.357Z",
      "updatedAt": "2025-04-22T07:14:11.357Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "0e289c7a-d7a7-4ff1-9826-79fa80c6bb3e",
      "number": 6,
      "content": "Quả thật, một dân nước uy hùng đông đảo",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.385Z",
      "updatedAt": "2025-04-22T07:14:11.385Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "0bda5561-449c-4e75-b7b3-d141b110f450",
      "number": 6,
      "content": "đã xông lên đánh phá đất của Ta.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.413Z",
      "updatedAt": "2025-04-22T07:14:11.413Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "056a81a3-13b3-4084-81b4-099b0096f360",
      "number": 6,
      "content": "Răng chúng là răng chúa sơn lâm,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.427Z",
      "updatedAt": "2025-04-22T07:14:11.427Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "28e8dd92-8b73-4c8e-abbc-64615fa4a9d7",
      "number": 6,
      "content": "nanh chúng là nanh sư tử cái.",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.456Z",
      "updatedAt": "2025-04-22T07:14:11.456Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "658c680f-d3f6-44fc-98d5-d62a4dba1cff",
      "number": 7,
      "content": "Vườn nho của Ta, chúng biến nên chốn hoang tàn,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.470Z",
      "updatedAt": "2025-04-22T07:14:11.470Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "003602dc-153b-4a83-8530-c31698168ef2",
      "number": 7,
      "content": "cây vả của Ta, chúng làm thành đống củi vụn.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.498Z",
      "updatedAt": "2025-04-22T07:14:11.498Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "b449acc0-bc35-4b15-88e5-a4089445306f",
      "number": 7,
      "content": "Chúng tước sạch vỏ, hạ đổ thân cây, bóc cành trắng hếu.",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.513Z",
      "updatedAt": "2025-04-22T07:14:11.513Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "4ab6f3ce-2d87-48b6-9ecb-bc32ca6a40f6",
      "number": 8,
      "content": "Rên siết đi nào, tựa trinh nữ mặc áo vải thô",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.527Z",
      "updatedAt": "2025-04-22T07:14:11.527Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "31cd9c08-233f-405b-a28f-ab997bc6305d",
      "number": 8,
      "content": "khóc thương chồng sắp cưới thuở thanh xuân !",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.566Z",
      "updatedAt": "2025-04-22T07:14:11.566Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "2258e327-d448-4f12-b9a8-facc179b6abd",
      "number": 9,
      "content": "Nơi Nhà ĐỨC CHÚA, lễ hiến dâng và lễ tưới rượu",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.588Z",
      "updatedAt": "2025-04-22T07:14:11.588Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "ce5add1f-848a-4392-9668-a3482450d9cc",
      "number": 9,
      "content": "chẳng được cử hành nữa.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.623Z",
      "updatedAt": "2025-04-22T07:14:11.623Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "a39cf022-8669-46e4-92cb-125522a85e6b",
      "number": 9,
      "content": "Hàng tư tế, bề tôi của ĐỨC CHÚA, đang u sầu như thể chịu tang.",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.637Z",
      "updatedAt": "2025-04-22T07:14:11.637Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "de742489-c5c7-4d2c-81f8-dd1c51ff1761",
      "number": 10,
      "content": "Đồng ruộng bị tàn phá,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.680Z",
      "updatedAt": "2025-04-22T07:14:11.680Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "fb6f678d-958c-42d8-ba9b-5a8d86e1da0f",
      "number": 10,
      "content": "đất đai cũng u sầu như thể chịu tang,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.694Z",
      "updatedAt": "2025-04-22T07:14:11.694Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "13fb6930-a068-460d-889b-72f3f2fdf995",
      "number": 10,
      "content": "vì lúa mì bị tàn phá,",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.709Z",
      "updatedAt": "2025-04-22T07:14:11.709Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "35bcbe7b-ff03-4b1a-a9d5-214f5609b626",
      "number": 10,
      "content": "rượu mới đã cạn khô, dầu tươi chẳng còn nữa.",
      "order": 3,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.723Z",
      "updatedAt": "2025-04-22T07:14:11.723Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "6564f0fc-b418-4b8b-aa13-43a417ec4d52",
      "number": 11,
      "content": "Hỡi nhà nông, hãy thẹn thùng xấu hổ,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.740Z",
      "updatedAt": "2025-04-22T07:14:11.740Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "76dc2031-1f36-464c-a69d-436f940f009c",
      "number": 11,
      "content": "kẻ trồng nho, hãy rú lên nào, tiếc cho lúa miến, lúa mạch,",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.755Z",
      "updatedAt": "2025-04-22T07:14:11.755Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "dcc68621-c1e3-46f3-ba4f-bb04d4d87c9a",
      "number": 11,
      "content": "vì đồng ruộng chẳng còn gì để gặt hái.",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.769Z",
      "updatedAt": "2025-04-22T07:14:11.769Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "4a958732-5469-4bd5-aef3-67f4b87acb40",
      "number": 12,
      "content": "Nho cằn cỗi, vả héo tàn, cả lựu, cả chà là lẫn táo,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.783Z",
      "updatedAt": "2025-04-22T07:14:11.783Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "b00c586a-af96-4c48-a078-48d9e5cde8c6",
      "number": 12,
      "content": "mọi cây cối ngoài đồng đã chết khô.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.797Z",
      "updatedAt": "2025-04-22T07:14:11.797Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "f504fca5-644f-4d8a-a988-51ecaff1f15e",
      "number": 12,
      "content": "Thế là đã cạn hết niềm vui của con cái loài người.",
      "order": 2,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.829Z",
      "updatedAt": "2025-04-22T07:14:11.829Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "9dfa47cd-fbe8-468e-b9e6-e5189b856e54",
      "number": 13,
      "content": "Hỡi các tư tế, hãy mặc áo vải thô mà than van kêu khóc !",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.858Z",
      "updatedAt": "2025-04-22T07:14:11.858Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "94516c16-2fdf-46c4-b669-414bdd54e3ea",
      "number": 13,
      "content": "Rú lên đi, hỡi những người phục vụ bàn thờ !",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.900Z",
      "updatedAt": "2025-04-22T07:14:11.900Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "43d7aeb4-2c20-4f49-95db-8ea7003e2ecb",
      "number": 13,
      "content": "Hãy đến, mặc áo vải thô mà thức suốt đêm,",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.914Z",
      "updatedAt": "2025-04-22T07:14:11.914Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "3f1a286e-0890-43d5-8854-d43fb0be9a90",
      "number": 13,
      "content": "hỡi những người phục vụ Thiên Chúa,",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.928Z",
      "updatedAt": "2025-04-22T07:14:11.928Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "59ffdb25-249e-4233-b38d-54f8eacdc4cc",
      "number": 13,
      "content": "vì lễ hiến dâng và lễ tưới rượu",
      "order": 4,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.942Z",
      "updatedAt": "2025-04-22T07:14:11.942Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "be401583-454e-4864-a47f-ef5d52a20a2a",
      "number": 13,
      "content": "đã bị cấm cử hành nơi Nhà Thiên Chúa.",
      "order": 5,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.970Z",
      "updatedAt": "2025-04-22T07:14:11.970Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "99137fef-dd02-4ea7-abad-f47df9730875",
      "number": 14,
      "content": "Hãy ra lệnh giữ chay thánh, công bố mở cuộc họp long trọng,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:11.983Z",
      "updatedAt": "2025-04-22T07:14:11.983Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "e039892b-bb2e-41f7-9e6c-cbf766c21ae4",
      "number": 14,
      "content": "triệu tập các cụ già và toàn thể cư dân trong xứ",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.013Z",
      "updatedAt": "2025-04-22T07:14:12.013Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "14f5e474-9015-4f06-ac52-c52dfab2edf9",
      "number": 14,
      "content": "tại Nhà ĐỨC CHÚA, Thiên Chúa các ngươi.",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.026Z",
      "updatedAt": "2025-04-22T07:14:12.026Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "c80d2b95-cd97-486d-b8e5-4fd826c93fb8",
      "number": 14,
      "content": "Hãy kêu lên ĐỨC CHÚA.",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.049Z",
      "updatedAt": "2025-04-22T07:14:12.049Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "0f23c044-98f7-490c-a95f-49c703289fdb",
      "number": 15,
      "content": "Ôi, ngày đáng sợ thay ! Ngày của ĐỨC CHÚA quả đã gần kề !",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.075Z",
      "updatedAt": "2025-04-22T07:14:12.075Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "09cc7e0b-99d0-4610-95e6-de43e2fcb74c",
      "number": 15,
      "content": "Ngày sẽ đến tựa cơn tàn phá do lệnh Đấng Toàn Năng.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.146Z",
      "updatedAt": "2025-04-22T07:14:12.146Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "35dfe70d-43c6-4bca-850b-519e7488cbf8",
      "number": 16,
      "content": "Lương thực đã chẳng bị cất xa tầm mắt chúng ta,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.174Z",
      "updatedAt": "2025-04-22T07:14:12.174Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "e92fe032-19be-4ce2-aab2-85d8bd916c81",
      "number": 16,
      "content": "niềm vui mừng hoan lạc",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.189Z",
      "updatedAt": "2025-04-22T07:14:12.189Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "6bfecc7e-733c-4e57-a6d3-094f3c10edd5",
      "number": 16,
      "content": "đã chẳng bị cất khỏi nhà Thiên Chúa rồi đó sao ?",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.204Z",
      "updatedAt": "2025-04-22T07:14:12.204Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "aede96df-b5f2-4987-8c23-3d2f16315764",
      "number": 17,
      "content": "Hạt gieo vãi đã chết khô dưới những tảng đất ;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.218Z",
      "updatedAt": "2025-04-22T07:14:12.218Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "d301fa83-24f9-493b-9cb3-bddb174bc6bc",
      "number": 17,
      "content": "kho lẫm tan hoang, vựa lúa sụp đổ, bởi lúa mì đã cạn.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.233Z",
      "updatedAt": "2025-04-22T07:14:12.233Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "57399ac7-8b16-4b53-a245-ac992c448469",
      "number": 18,
      "content": "Đàn vật rên siết thảm thương, bò bê tán loạn,",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.247Z",
      "updatedAt": "2025-04-22T07:14:12.247Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "085b5108-68b3-4692-9420-cadedecec17d",
      "number": 18,
      "content": "vì không còn cỏ ăn.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.277Z",
      "updatedAt": "2025-04-22T07:14:12.277Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "29e099c4-4d9b-4ceb-bbf2-ad9af1d57d5e",
      "number": 18,
      "content": "Cả đàn cừu cũng bị chung tai hoạ.",
      "order": 2,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.291Z",
      "updatedAt": "2025-04-22T07:14:12.291Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "8ad1f08b-bc28-43a6-a432-fe3707013041",
      "number": 19,
      "content": "Lạy ĐỨC CHÚA, con kêu lên Ngài,",
      "order": 0,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.312Z",
      "updatedAt": "2025-04-22T07:14:12.312Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "96484081-c132-4e9e-aba6-b7074fa58b67",
      "number": 19,
      "content": "vì lửa hồng đã thiêu hết bãi cỏ trong hoang địa,",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.341Z",
      "updatedAt": "2025-04-22T07:14:12.341Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "6a574f9e-3f7e-4d39-9f5e-a217cc836c37",
      "number": 19,
      "content": "và ngọn lửa đã đốt sạch cây cối ngoài đồng.",
      "order": 2,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.375Z",
      "updatedAt": "2025-04-22T07:14:12.375Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "569b1872-362b-4154-9822-1649e08392d8",
      "number": 20,
      "content": "Ngay cả loài dã thú cũng khát mong hướng về Ngài,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.390Z",
      "updatedAt": "2025-04-22T07:14:12.390Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "6fcfc78d-25e3-42c4-b3bc-7956ef2f5125",
      "number": 20,
      "content": "vì suối nước đã cạn khô",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.404Z",
      "updatedAt": "2025-04-22T07:14:12.404Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "f54295ac-2b6c-4eef-9d2a-f2e58113d915",
      "number": 20,
      "content": "và lửa hồng đã thiêu hết bãi cỏ trong hoang địa.",
      "order": 2,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T07:14:12.418Z",
      "updatedAt": "2025-04-22T07:14:12.418Z",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    }
  ],
  "footnotes": [
    {
      "id": "26977ce4-cc90-4cd8-8ad4-9a41b6a5eead",
      "content": "Trong phần thứ nhất (1 – 2), sách Giô-en nói đến hai tai ương : nạn châu chấu (2 lần : 1) 1,4-7 ; 2) 2,2-11) và hạn hán (1,10-12.16-20). Cũng trong phần này, ngôn sứ đưa ra hai lời kêu gọi sám hối (1) 1,13-15 ; 2) 2,12-17). Sau cùng, câu trả lời thuận lợi của Đức Chúa (2,18-27) trở thành kết luận của phần thứ nhất.",
      "position": 16,
      "order": 0,
      "createdAt": "2025-04-22T07:14:11.106Z",
      "updatedAt": "2025-04-22T07:14:11.106Z",
      "headingId": "16f64e1a-664e-4b4a-a2a1-f131c6e121cc",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "7dd8f661-7ddf-492f-a469-560179e346ca",
      "content": "Thường thường, từ *châu chấu* được dùng để dịch từ Híp-ri arBè. Như trong tiếng Híp-ri, ba từ kia (*hoàng trùng, cào cào, sạt sành*) đều chỉ ba loại khác nhau thuộc đại loại *châu chấu*. Đáng lẽ trong c.4, phải để từ *châu chấu* ở vị trí thứ hai. Nhưng vì từ này là một từ thông dụng, các dịch giả nghĩ rằng nên đặt ngay ở đầu câu. C.4 nhằm cho độc giả thấy : cây cối hoàn toàn bị tàn phá.",
      "position": 16,
      "order": 1,
      "createdAt": "2025-04-22T07:14:11.244Z",
      "updatedAt": "2025-04-22T07:14:11.244Z",
      "verseId": "5fe1a8f3-fc0e-478c-84ae-fa8f51fe3d88",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "8164f4e5-e432-4464-a62c-1507762ddb2d",
      "content": "Nạn nhân của hai loại tai ương (nạn châu chấu và hạn hán) : những kẻ say sưa (c.5), các tư tế (cc. 8-9), nhà nông (cc. 11-12).",
      "position": 20,
      "order": 2,
      "createdAt": "2025-04-22T07:14:11.315Z",
      "updatedAt": "2025-04-22T07:14:11.315Z",
      "verseId": "207a11ad-b33c-4961-aa4f-3d896151c9c0",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "234e26cd-ed1f-496c-bed9-4013fe0c97a0",
      "content": "*Áo vải thô* chỉ một miếng vải thô, thắt ngang lưng trở xuống : đó là áo tang hoặc áo mặc để tỏ lòng sám hối (x. St 37,34 ; 2 Sm 3,31 ; Is 22,12 ; Gr 48,37 ; Ed 27,31 ; Am 8,10 ; Ge 1,13 ; Gn 3,5.6.8 ; ...).",
      "position": 44,
      "order": 3,
      "createdAt": "2025-04-22T07:14:11.541Z",
      "updatedAt": "2025-04-22T07:14:11.541Z",
      "verseId": "4ab6f3ce-2d87-48b6-9ecb-bc32ca6a40f6",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
    {
      "id": "e0951eac-a33d-4d9a-8d10-da65895c0e90",
      "content": "Nạn châu chấu loan báo *ngày của Đức Chúa*, ngày đáng sợ (x. 2,1-2.11 ; Am 5,18).",
      "position": 41,
      "order": 4,
      "createdAt": "2025-04-22T07:14:12.104Z",
      "updatedAt": "2025-04-22T07:14:12.104Z",
      "verseId": "0f23c044-98f7-490c-a95f-49c703289fdb",
      "chapterId": "84ffe415-9e5b-4f59-bb9d-c1621dc013c1"
    },
