    {
      "id": "35f9931f-8b6d-4a7b-a004-ad490691ec98",
      "number": 41,
      "content": "Ai cung cấp thức ăn cho quạ",
      "order": 0,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:34:49.043Z",
      "updatedAt": "2025-04-22T10:34:49.043Z",
