    },
    {
      "id": "b858a466-1472-4770-b8d6-2a962d7c4e2b",
      "number": 41,
      "content": "Khi đàn quạ con đói bụng kêu xin Đức Chúa Trời,",
      "order": 1,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:34:49.053Z",
      "updatedAt": "2025-04-22T10:34:49.053Z",
      "chapterId": "8e83b786-0ef3-457a-8525-dddd49e0e1d9"
    },
    {
      "id": "787bcf81-e5d2-409a-a8cd-25196f664ac7",
      "number": 41,
      "content": "Trong khi quạ mẹ đi tìm khắp nơi mà vẫn không kiếm ra được thức ăn?",
      "order": 2,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:34:49.063Z",
      "updatedAt": "2025-04-22T10:34:49.063Z",
