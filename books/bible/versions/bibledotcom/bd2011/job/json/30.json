    {
      "id": "006914e6-9d0f-45b5-947f-c8a90ac589b0",
      "content": "Gióp Lại Tiếp Tục Binh Vực Ông",
      "order": 0,
      "createdAt": "2024-09-23T05:42:03.733Z",
      "updatedAt": "2024-09-23T05:42:03.733Z",
