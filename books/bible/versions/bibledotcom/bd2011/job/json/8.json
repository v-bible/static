{
  "chapter": {
    "id": "5504c6e8-d81a-40d0-b301-616c3620b840",
    "number": 8,
    "ref": "https://www.bible.com/bible/19/JOB.8.BD2011",
    "createdAt": "2025-04-22T10:10:25.151Z",
    "updatedAt": "2025-04-22T10:10:25.151Z",
    "bookId": "af28f979-437b-4185-a0f1-8d7ffd91c046"
  },
  "verses": [
    {
      "id": "10feea73-b2a4-47c8-9f91-213ee57112f6",
      "number": 1,
      "content": "Bấy giờ Binh-đát người Su-a đáp lời và nói:",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T10:33:10.004Z",
      "updatedAt": "2025-04-22T10:33:10.004Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "52680f47-7ee0-49de-8cbd-6ffd31a3eb5a",
      "number": 2,
      "content": "Anh cứ nói những điều ấy cho đến bao giờ mới thôi?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.030Z",
      "updatedAt": "2025-04-22T10:33:10.030Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "fef54892-d18b-4881-aee7-05a9ffdcf653",
      "number": 2,
      "content": "Anh cứ nói ào ào như cuồng phong thổi tới.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.040Z",
      "updatedAt": "2025-04-22T10:33:10.040Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "7bfc28cd-34d8-41ab-b91b-0674856a93ca",
      "number": 3,
      "content": "Đức Chúa Trời có bao giờ bẻ cong công lý chăng?",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.051Z",
      "updatedAt": "2025-04-22T10:33:10.051Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "5afeee44-7261-4970-b46f-c8edb7e1ccbe",
      "number": 3,
      "content": "Đấng Toàn Năng há uốn cong lẽ chính trực hay sao?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.061Z",
      "updatedAt": "2025-04-22T10:33:10.061Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "97537ffc-b933-4d86-bb64-5db42d8be6e9",
      "number": 4,
      "content": "Nếu các con anh đã phạm tội đối với Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.072Z",
      "updatedAt": "2025-04-22T10:33:10.072Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "d2adf482-fcbc-4633-b792-ce22f0615657",
      "number": 4,
      "content": "Thì Ngài bắt chúng phải gánh lấy hậu quả tội lỗi của chúng;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.083Z",
      "updatedAt": "2025-04-22T10:33:10.083Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "bc563777-59b1-4591-b85f-aa31b56d775b",
      "number": 5,
      "content": "Nếu anh chuyên tâm tìm kiếm Đức Chúa Trời,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.094Z",
      "updatedAt": "2025-04-22T10:33:10.094Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "80fc7c30-ebd2-4f60-8b7e-5380a0ab9499",
      "number": 5,
      "content": "Và hết lòng khẩn nguyện với Đấng Toàn Năng,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.105Z",
      "updatedAt": "2025-04-22T10:33:10.105Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "3d289c8f-166d-426a-802a-16e6df8ffa67",
      "number": 6,
      "content": "Nếu anh quả thật là người trong sạch và ngay thẳng,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.118Z",
      "updatedAt": "2025-04-22T10:33:10.118Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "5892c8f3-e85a-4ccb-9522-936d54b81af6",
      "number": 6,
      "content": "Thì bây giờ Ngài sẽ đứng dậy để binh vực anh,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.131Z",
      "updatedAt": "2025-04-22T10:33:10.131Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "e88b61a9-4d72-44c3-ae0e-0dba21cc4e66",
      "number": 6,
      "content": "Và sẽ tái lập anh vào cương vị của một người công chính;",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.144Z",
      "updatedAt": "2025-04-22T10:33:10.144Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "efff3da3-2b59-427f-94fb-dde7aaba40b9",
      "number": 7,
      "content": "Dù lúc bắt đầu hơi có phần khiêm tốn,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.156Z",
      "updatedAt": "2025-04-22T10:33:10.156Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "040938f6-9bc5-4cda-955e-a414ffee8157",
      "number": 7,
      "content": "Nhưng đến cuối cùng sẽ vĩ đại khôn lường.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.169Z",
      "updatedAt": "2025-04-22T10:33:10.169Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "d402921b-dc8b-477e-a0cd-2fee7c7f2b8e",
      "number": 8,
      "content": "Tôi xin anh hãy tìm hiểu kinh nghiệm của các bậc tiền nhân,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.182Z",
      "updatedAt": "2025-04-22T10:33:10.182Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "1bcc1826-0cd2-46d1-b2bb-175030ade0d3",
      "number": 8,
      "content": "Và suy xét những gì tổ tiên của họ đã tìm thấy.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.196Z",
      "updatedAt": "2025-04-22T10:33:10.196Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "9babada0-18ef-48cc-b2e3-456c484c54f1",
      "number": 9,
      "content": "Vì chúng ta mới có mặt ngày hôm qua và chẳng biết gì,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.208Z",
      "updatedAt": "2025-04-22T10:33:10.208Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "e2cc8930-d573-4b1e-ba29-6cd76b26a875",
      "number": 9,
      "content": "Vì những ngày của chúng ta trên đất qua mau như cái bóng.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.221Z",
      "updatedAt": "2025-04-22T10:33:10.221Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "a12a142f-bcb4-4ba1-a891-f61a40d00e83",
      "number": 10,
      "content": "Há chẳng phải các vị tiền bối sẽ dạy anh và bảo anh,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.234Z",
      "updatedAt": "2025-04-22T10:33:10.234Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "793620b3-8112-4e29-9b44-d8c286419342",
      "number": 10,
      "content": "Và sẽ nói cho anh biết tri thức nằm lòng của họ hay sao?",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.247Z",
      "updatedAt": "2025-04-22T10:33:10.247Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "65b212ef-5f62-45ee-a9a7-362681083ae5",
      "number": 11,
      "content": "Cây lau há có thể mọc lên nơi không phải bùn lầy?",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.260Z",
      "updatedAt": "2025-04-22T10:33:10.260Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "11825d17-19d1-4342-b100-1ff3d7dfc921",
      "number": 11,
      "content": "Cây sậy há có thể mọc lên nơi không có nước?",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.276Z",
      "updatedAt": "2025-04-22T10:33:10.276Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "0f7ecdcc-1025-492f-a3bd-e18627661953",
      "number": 12,
      "content": "Khi đang còn xanh tươi chưa kịp cắt về,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.290Z",
      "updatedAt": "2025-04-22T10:33:10.290Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "da338966-cc92-4425-83c3-73494705c740",
      "number": 12,
      "content": "Chúng đã bị khô héo trước các loài cỏ khác.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.303Z",
      "updatedAt": "2025-04-22T10:33:10.303Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "a0d5d58f-865d-4163-90c1-7aa1ba021f88",
      "number": 13,
      "content": "Đó là số phận của mọi kẻ quên ơn lìa bỏ Đức Chúa Trời;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.317Z",
      "updatedAt": "2025-04-22T10:33:10.317Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "7ba5f933-f558-41f5-98d4-0dd6f558eb92",
      "number": 13,
      "content": "Hy vọng của những kẻ không tin kính Chúa sẽ tiêu vong.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.330Z",
      "updatedAt": "2025-04-22T10:33:10.330Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "18ebc006-f06f-40be-ac83-b4e9955412c2",
      "number": 14,
      "content": "Nơi nó nương dựa bị hoàn toàn sụp đổ;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.346Z",
      "updatedAt": "2025-04-22T10:33:10.346Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "86509050-e5c0-4c11-a751-53b1b6d96dd4",
      "number": 14,
      "content": "Chỗ nó tin cậy chỉ là màng nhện mong manh.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.362Z",
      "updatedAt": "2025-04-22T10:33:10.362Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "4636ee66-79c0-402c-ab1e-38f5d5760138",
      "number": 15,
      "content": "Nó dựa vào căn nhà, nhưng nhà không đứng vững;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.377Z",
      "updatedAt": "2025-04-22T10:33:10.377Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "5610a46a-e84a-4510-bdc2-9e2c57d80911",
      "number": 15,
      "content": "Nó bám vào căn nhà, nhưng nhà chẳng còn được bao lâu.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.390Z",
      "updatedAt": "2025-04-22T10:33:10.390Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "3dba4e08-5b12-4eb8-b996-8ba1c9637471",
      "number": 16,
      "content": "Nó như cây lớn mạnh xanh tươi dưới ánh mặt trời,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.402Z",
      "updatedAt": "2025-04-22T10:33:10.402Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "08a3d770-495a-4bba-b357-bc18097e35ca",
      "number": 16,
      "content": "Các nhánh nó vươn ra tỏa rộng khắp vườn;",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.419Z",
      "updatedAt": "2025-04-22T10:33:10.419Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "f8af3f2c-47c1-4aa6-837a-a92a8aaf03c4",
      "number": 17,
      "content": "Các rễ nó quấn chằng chịt chung quanh đống đá;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.433Z",
      "updatedAt": "2025-04-22T10:33:10.433Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "cc3efcd9-80e4-467b-9b50-9500a195b5ad",
      "number": 17,
      "content": "Các rễ con nó len lỏi vào giữa những sỏi đá và bám vào.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.446Z",
      "updatedAt": "2025-04-22T10:33:10.446Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "9b0c85e9-dfa5-43d6-9848-74382286bb62",
      "number": 18,
      "content": "Nhưng khi nó vừa bị bật gốc khỏi nơi ấy,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.459Z",
      "updatedAt": "2025-04-22T10:33:10.459Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "23dc8338-595f-4003-9473-f050ebf9f66a",
      "number": 18,
      "content": "Nơi ấy liền phủ nhận nó rằng, “Ta chưa hề thấy ngươi.”",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.473Z",
      "updatedAt": "2025-04-22T10:33:10.473Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "cca4609f-3f12-4147-afde-90c018c24169",
      "number": 19,
      "content": "Này, đây là niềm vui của đường lối nó:",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.486Z",
      "updatedAt": "2025-04-22T10:33:10.486Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "4649e664-f38d-44b2-965e-4cc0d9aac429",
      "number": 19,
      "content": "Từ dưới mặt đất sẽ có các cây khác mọc lên.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.498Z",
      "updatedAt": "2025-04-22T10:33:10.498Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "d9900e8c-e253-45bd-b724-0276aba33e26",
      "number": 20,
      "content": "Này, Đức Chúa Trời sẽ không loại bỏ người vẹn toàn đạo đức;",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.510Z",
      "updatedAt": "2025-04-22T10:33:10.510Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "5512a30d-ffbc-4c82-8a59-3a592fc90333",
      "number": 20,
      "content": "Ngài sẽ không phù hộ tay kẻ gian ác bao giờ.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.522Z",
      "updatedAt": "2025-04-22T10:33:10.522Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "4a1fc48d-3c11-43e1-8c76-3f7659d0edf4",
      "number": 21,
      "content": "Ngài sẽ làm cho miệng anh đầy tiếng cười vui vẻ,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.534Z",
      "updatedAt": "2025-04-22T10:33:10.534Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "c2429b6c-e262-43d3-8cc0-6d3f6279e3e5",
      "number": 21,
      "content": "Ngài sẽ ban cho môi anh vang những tiếng reo mừng.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.546Z",
      "updatedAt": "2025-04-22T10:33:10.546Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "4d50398b-40db-4cd3-9039-7da53f8d7f76",
      "number": 22,
      "content": "Những kẻ ghét anh sẽ mặc lấy xấu hổ;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.559Z",
      "updatedAt": "2025-04-22T10:33:10.559Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    },
    {
      "id": "02ded202-a010-497e-8dc6-165574022d35",
      "number": 22,
      "content": "Và chỗ ở của bọn gian ác sẽ không còn nữa.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:33:10.571Z",
      "updatedAt": "2025-04-22T10:33:10.571Z",
      "chapterId": "5504c6e8-d81a-40d0-b301-616c3620b840"
    }
  ],
