      "chapterId": "b30f99e7-2bca-4717-8420-dfaa196c11ca"
    },
    {
      "id": "76c38857-a423-496c-9d22-1f729fd136df",
      "content": "Sự Khôn Ngoan Dự Phần trong Cuộc Sáng Tạo",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T10:46:13.284Z",
      "updatedAt": "2025-04-22T10:46:13.284Z",
