    },
    {
      "id": "aabe6576-f111-4da8-b117-4a34d3d21877",
      "content": "Hãy Nghe Lời Khuyên Dạy của Cha Mẹ",
      "order": 0,
      "createdAt": "2024-09-23T05:55:35.264Z",
      "updatedAt": "2024-09-23T05:55:35.264Z",
      "verseId": "d1888e6b-2658-43ac-9a9b-ddf0f6fe6a51",
      "chapterId": "7fffb370-a6d8-4ddd-b039-f9b95315bb42"
    },
    {
      "id": "b00cdf83-a174-4f27-8871-9639508aa9b9",
      "content": "Hãy Tránh Xa Kẻ Xấu",
      "order": 0,
      "createdAt": "2024-09-23T05:55:35.278Z",
      "updatedAt": "2024-09-23T05:55:35.278Z",
      "verseId": "6a3e6bab-7fc6-485c-881f-1a2fb9431680",
      "chapterId": "7fffb370-a6d8-4ddd-b039-f9b95315bb42"
    },
    {
      "id": "3368f271-f1fb-4b31-bd04-4be5c3b0c48b",
      "content": "Sự Khôn Ngoan Mời Gọi",
      "order": 0,
