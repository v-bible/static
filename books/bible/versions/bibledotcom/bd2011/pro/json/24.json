    {
      "id": "94864527-1b73-4646-880e-a535e45239a6",
      "content": "Những Lời Khuyên Bảo Khác của Người Khôn Ngoan",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T10:47:04.213Z",
