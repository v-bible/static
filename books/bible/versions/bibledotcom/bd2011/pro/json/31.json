    },
    {
      "id": "c7d4304d-4854-4b19-966f-1ec79bf6b4e2",
      "content": "Những Lời của Mẹ Vua Lê-mu-ên Khuyên Con Bà",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T02:02:31.546Z",
      "updatedAt": "2025-04-24T02:02:31.546Z",
