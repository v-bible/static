    },
    {
      "id": "43cfc5fa-5149-41b8-b38f-f102c47bd78d",
      "content": "Khen Ngợi Người Vợ Tài Đức",
      "order": 0,
      "createdAt": "2024-09-23T05:57:08.976Z",
      "updatedAt": "2024-09-23T05:57:08.976Z",
