    },
    {
      "id": "93c4d133-d4e7-409d-a803-dc7a2dc4c183",
      "content": "Lời khuyên Nhủ của Từ Phụ",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T10:45:58.608Z",
      "updatedAt": "2025-04-22T10:45:58.608Z",
