{
  "chapter": {
    "id": "2b2e1325-eb14-4a73-9435-0295a5d9abe4",
    "number": 3,
    "ref": "https://www.bible.com/bible/19/PRO.3.BD2011",
    "createdAt": "2025-04-22T10:10:27.378Z",
    "updatedAt": "2025-04-22T10:10:27.378Z",
    "bookId": "3cb100ae-f29d-45d9-958d-3944bc91671c"
  },
  "verses": [
    {
      "id": "a4eb71d6-0660-458e-932d-e5a83d28ad94",
      "number": 1,
      "content": "Con ơi, chớ quên luật pháp ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.076Z",
      "updatedAt": "2025-04-22T10:45:55.076Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "8fcc9a68-58ba-4697-92be-a3081d53aab9",
      "number": 1,
      "content": "Nhưng hãy để lòng con gìn giữ mệnh lệnh ta;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.103Z",
      "updatedAt": "2025-04-22T10:45:55.103Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "64535e48-293a-403a-86a5-66caee09da4a",
      "number": 2,
      "content": "Vì chúng sẽ làm gia tăng số ngày và số năm của đời con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.113Z",
      "updatedAt": "2025-04-22T10:45:55.113Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "ce49440e-8924-4c68-80a3-219b165d7edd",
      "number": 2,
      "content": "Chúng sẽ đem lại cho con bình an thịnh vượng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.123Z",
      "updatedAt": "2025-04-22T10:45:55.123Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "fc4179d1-e880-470c-8b69-4d0458b7ee66",
      "number": 3,
      "content": "Đừng để lòng thương xót và tính chân thật lìa khỏi con;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.134Z",
      "updatedAt": "2025-04-22T10:45:55.134Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "5ef8f7fe-be91-409e-a14e-d833436976fb",
      "number": 3,
      "content": "Hãy buộc chúng quanh cổ con;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.144Z",
      "updatedAt": "2025-04-22T10:45:55.144Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "ff536715-b887-406b-a33c-635b2d10a386",
      "number": 3,
      "content": "Hãy ghi tạc chúng vào bia lòng con.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.156Z",
      "updatedAt": "2025-04-22T10:45:55.156Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "483b9037-a415-4b73-8379-23a9fe2803a9",
      "number": 4,
      "content": "Vì như thế con sẽ được ơn và được tiếng tốt,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.168Z",
      "updatedAt": "2025-04-22T10:45:55.168Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "0fba81fa-b72b-4a97-84ea-ed88039c778a",
      "number": 4,
      "content": "Trước mặt Đức Chúa Trời và luôn cả người ta.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.184Z",
      "updatedAt": "2025-04-22T10:45:55.184Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "6ff61ed6-0c21-4104-91b3-c63b7c0c6f55",
      "number": 5,
      "content": "Hãy hết lòng tin cậy **CHÚA**;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.198Z",
      "updatedAt": "2025-04-22T10:45:55.198Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "c44a7f54-2c21-445c-85a4-56d06fac1c36",
      "number": 5,
      "content": "Chớ cậy vào sự thông sáng của con;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.211Z",
      "updatedAt": "2025-04-22T10:45:55.211Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "5b430671-239f-47a7-82f9-f9dd08be010e",
      "number": 6,
      "content": "Phàm trong các việc làm của con, khá nhận biết Ngài,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.224Z",
      "updatedAt": "2025-04-22T10:45:55.224Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "099b3cfd-d0d2-4098-8bc2-8a72bf85a410",
      "number": 6,
      "content": "Thì Ngài sẽ chỉ dẫn các lối của con.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.237Z",
      "updatedAt": "2025-04-22T10:45:55.237Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "b6c9b760-64f8-4be4-9d16-46b6db75bd8e",
      "number": 7,
      "content": "Đừng cho mình là khôn ngoan theo mắt mình;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.251Z",
      "updatedAt": "2025-04-22T10:45:55.251Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "125963d0-d488-4de7-9c4d-b443056873bc",
      "number": 7,
      "content": "Hãy kính sợ **CHÚA** và lánh xa điều ác.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.264Z",
      "updatedAt": "2025-04-22T10:45:55.264Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "0f34d683-d551-43be-80b6-55a347e761fe",
      "number": 8,
      "content": "Như vậy thân xác con sẽ được mạnh khỏe,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.282Z",
      "updatedAt": "2025-04-22T10:45:55.282Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "6260fa07-95a4-4b6e-ba15-484f4e89f7fa",
      "number": 8,
      "content": "Và xương cốt con sẽ được cứng cáp vững vàng.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.296Z",
      "updatedAt": "2025-04-22T10:45:55.296Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "3db3b368-5e43-42b9-8fd4-b56ff2078cc3",
      "number": 9,
      "content": "Hãy dùng của cải con để tôn vinh **CHÚA**,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.309Z",
      "updatedAt": "2025-04-22T10:45:55.309Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "b9980315-38a3-435f-ad33-b5ef6461a38d",
      "number": 9,
      "content": "Hãy dâng các hoa lợi đầu mùa của con để cảm tạ Ngài.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.322Z",
      "updatedAt": "2025-04-22T10:45:55.322Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "5f90a2fa-fbc5-4436-8d46-55b15af3ef8d",
      "number": 10,
      "content": "Như vậy các vựa lẫm của con sẽ tràn đầy lúa thóc,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.335Z",
      "updatedAt": "2025-04-22T10:45:55.335Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "972862ed-cc69-4230-8796-5000a9f44dc6",
      "number": 10,
      "content": "Các thùng rượu của con rượu mới sẽ tràn đầy.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.349Z",
      "updatedAt": "2025-04-22T10:45:55.349Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "0a9e3c9b-c3ac-4955-b890-050eda0279e0",
      "number": 11,
      "content": "Con ơi, chớ coi thường sự sửa phạt của **CHÚA**,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.363Z",
      "updatedAt": "2025-04-22T10:45:55.363Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "df6083ef-d2b2-40b2-a02e-71d704d66ebe",
      "number": 11,
      "content": "Đừng bất mãn trước sự quở trách của Ngài;",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.376Z",
      "updatedAt": "2025-04-22T10:45:55.376Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "7aa939e5-3e53-4ff9-800d-e9fc6039db7a",
      "number": 12,
      "content": "Vì ai được **CHÚA** thương thì Ngài quở trách,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.392Z",
      "updatedAt": "2025-04-22T10:45:55.392Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "4d75cd60-fcbf-4b59-86be-a2f3e32293e3",
      "number": 12,
      "content": "Như cha thương con mình nên trách phạt khuyên răn.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.405Z",
      "updatedAt": "2025-04-22T10:45:55.405Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "2949339a-c95a-49b2-afb9-3f66004d4de7",
      "number": 13,
      "content": "Phước thay cho người tìm được sự khôn ngoan,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.418Z",
      "updatedAt": "2025-04-22T10:45:55.418Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "02eff608-1a70-46f5-ab35-c23caf8f1e0f",
      "number": 13,
      "content": "Và cho người có được sự thông sáng.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.449Z",
      "updatedAt": "2025-04-22T10:45:55.449Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "f4316fb8-e6d9-419c-884b-85e83aa9efb1",
      "number": 14,
      "content": "Vì lợi ích của khôn ngoan trỗi hơn tiền bạc;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.462Z",
      "updatedAt": "2025-04-22T10:45:55.462Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "7b21abf9-01f3-4add-933a-e648ece664d4",
      "number": 14,
      "content": "Ích lợi của nàng lấn át cả vàng ròng.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.476Z",
      "updatedAt": "2025-04-22T10:45:55.476Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "1e7238d4-0437-4cda-b8df-03f38307f63d",
      "number": 15,
      "content": "Nàng quý báu hơn châu ngọc;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.489Z",
      "updatedAt": "2025-04-22T10:45:55.489Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "a78c2e06-f043-4f90-85ac-0f404105d6db",
      "number": 15,
      "content": "Chẳng có vật gì con ưa thích có thể sánh được với nàng.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.502Z",
      "updatedAt": "2025-04-22T10:45:55.502Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "0955b3bf-0229-41a4-9737-cd86e5c3dc6c",
      "number": 16,
      "content": "Tay phải nàng phát ban trường thọ;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.516Z",
      "updatedAt": "2025-04-22T10:45:55.516Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "ad8914c9-b13d-40d5-b45a-f25244f13d6f",
      "number": 16,
      "content": "Tay trái nàng cho giàu có lẫn hiển vinh.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.531Z",
      "updatedAt": "2025-04-22T10:45:55.531Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "e977b864-da39-4c01-ac13-400b25dea98c",
      "number": 17,
      "content": "Con đường nàng là con đường vui thỏa;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.545Z",
      "updatedAt": "2025-04-22T10:45:55.545Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "2dd5aecc-7adb-4979-9eda-f8945beab11e",
      "number": 17,
      "content": "Mọi nẻo nàng là mọi ngả bình an.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.560Z",
      "updatedAt": "2025-04-22T10:45:55.560Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "55ef823d-3d2a-4950-892c-07b801fe502e",
      "number": 18,
      "content": "Khôn ngoan là cây sự sống cho người nào nắm giữ được;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.573Z",
      "updatedAt": "2025-04-22T10:45:55.573Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "39338042-46ef-4d0c-ad4d-a131148be28f",
      "number": 18,
      "content": "Ai giữ chặt nàng sẽ được phước lâu dài.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.586Z",
      "updatedAt": "2025-04-22T10:45:55.586Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "2185ef4f-7942-47fb-96d4-8484610afb60",
      "number": 19,
      "content": "**CHÚA** dùng sự khôn ngoan để dựng nên trái đất;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.600Z",
      "updatedAt": "2025-04-22T10:45:55.600Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "0c88c3ed-908e-403d-be5a-ec1ea67e77c1",
      "number": 19,
      "content": "Ngài dùng sự thông sáng để tạo lập các tầng trời;",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.624Z",
      "updatedAt": "2025-04-22T10:45:55.624Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "42c0e5a0-5223-4b99-bca8-690ebe73be20",
      "number": 20,
      "content": "Nhờ tri thức của Ngài các vực sâu mở rộng;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.636Z",
      "updatedAt": "2025-04-22T10:45:55.636Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "0102fb6a-9ecc-400c-b0ec-fac0934ff9f1",
      "number": 20,
      "content": "Những áng mây mù nhỏ những giọt sương đêm.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.648Z",
      "updatedAt": "2025-04-22T10:45:55.648Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "cf036575-6e1e-4685-a14e-09d65162e9d2",
      "number": 21,
      "content": "Con ơi, đừng để những điều này vuột khỏi tầm mắt con:",
      "order": 0,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.660Z",
      "updatedAt": "2025-04-22T10:45:55.660Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "96a248cf-4918-4e12-bac0-6bc8465c38a2",
      "number": 21,
      "content": "Hãy giữ lấy sự khôn ngoan và sự cẩn trọng.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.684Z",
      "updatedAt": "2025-04-22T10:45:55.684Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "52144f0f-29f4-42d5-a03b-f230b193b6c7",
      "number": 22,
      "content": "Những điều ấy sẽ là sức sống của linh hồn con,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.695Z",
      "updatedAt": "2025-04-22T10:45:55.695Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "b0ad5ef7-aef8-40f6-908f-0a5da1f970cd",
      "number": 22,
      "content": "Và là những đồ trang sức nơi cổ con.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.708Z",
      "updatedAt": "2025-04-22T10:45:55.708Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "915551df-35c4-4737-8f85-151e86f7b2e0",
      "number": 23,
      "content": "Nhờ những điều ấy đường con đi sẽ an toàn bảo đảm,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.720Z",
      "updatedAt": "2025-04-22T10:45:55.720Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "c7ad0f24-d6a6-4e61-af63-2d3adedb484d",
      "number": 23,
      "content": "Bước chân con sẽ chẳng vấp phạm bao giờ.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.735Z",
      "updatedAt": "2025-04-22T10:45:55.735Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "88dbdfbe-1488-4b67-89ae-b2f585649e13",
      "number": 24,
      "content": "Lúc nằm nghỉ con sẽ không lo sợ;",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.751Z",
      "updatedAt": "2025-04-22T10:45:55.751Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "9d4b3be8-d79e-496d-98be-1d8abc62c7cc",
      "number": 24,
      "content": "Khi đặt lưng con sẽ an giấc ngủ ngon.",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.763Z",
      "updatedAt": "2025-04-22T10:45:55.763Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "39a251dd-f61a-49df-a9fd-9fe26d3b981c",
      "number": 25,
      "content": "Đừng sợ hãi nỗi kinh hoàng xảy ra bất chợt;",
      "order": 0,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.775Z",
      "updatedAt": "2025-04-22T10:45:55.775Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "c2c6ebb0-5a6d-480e-a5ec-f5d06bd4a8e8",
      "number": 25,
      "content": "Đừng âu lo khi đợt phá hoại của kẻ ác đến gần;",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.787Z",
      "updatedAt": "2025-04-22T10:45:55.787Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "adb8f3cd-4986-4d5d-80f2-b49190128eb3",
      "number": 26,
      "content": "Vì **CHÚA** sẽ là nguồn trông cậy của con;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.800Z",
      "updatedAt": "2025-04-22T10:45:55.800Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "c2b28558-52d1-4952-8126-65dbe0201369",
      "number": 26,
      "content": "Ngài sẽ giữ chân con khỏi sa vào bẫy.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.816Z",
      "updatedAt": "2025-04-22T10:45:55.816Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "ae0d567f-8235-4411-b216-01306ca8264d",
      "number": 27,
      "content": "Đừng từ chối làm việc thiện cho những người đáng giúp,",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.829Z",
      "updatedAt": "2025-04-22T10:45:55.829Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "02d9d171-4c57-4371-ae05-1abeadc35f75",
      "number": 27,
      "content": "Nếu con có khả năng, hãy giúp đỡ người ta.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.842Z",
      "updatedAt": "2025-04-22T10:45:55.842Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "0ffbc75a-bb7d-4f12-bc60-aa6c13a1fff0",
      "number": 28,
      "content": "Con đừng nói với người lân cận, “Về đi, mai trở lại, tôi sẽ cho,”",
      "order": 0,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.855Z",
      "updatedAt": "2025-04-22T10:45:55.855Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "d834cdbc-13c0-47e2-b72e-d88e455264aa",
      "number": 28,
      "content": "Trong khi con có sẵn điều người ấy đang cần.",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.868Z",
      "updatedAt": "2025-04-22T10:45:55.868Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "d6303576-bec3-44c7-8a2c-d2734bf8460e",
      "number": 29,
      "content": "Con chớ lập mưu hại người lân cận,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.882Z",
      "updatedAt": "2025-04-22T10:45:55.882Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "d851b54b-7483-4075-9b50-d0bf1fd2651c",
      "number": 29,
      "content": "Người ở gần và tin tưởng nơi con.",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.895Z",
      "updatedAt": "2025-04-22T10:45:55.895Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "199b3c91-bf8f-4d6f-9825-2bac4a181a2b",
      "number": 30,
      "content": "Con đừng gây sự với ai vô cớ,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.908Z",
      "updatedAt": "2025-04-22T10:45:55.908Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "4650dd10-75a8-44f4-aaff-d0afada27bbe",
      "number": 30,
      "content": "Khi không ai gây cho con thiệt hại điều gì.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.921Z",
      "updatedAt": "2025-04-22T10:45:55.921Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "93ecf1b2-2718-4bc7-9413-6845e7358abd",
      "number": 31,
      "content": "Đừng ganh tị với quân tàn bạo;",
      "order": 0,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.940Z",
      "updatedAt": "2025-04-22T10:45:55.940Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "eb3cac43-5992-4cc2-9c19-294722b4f31b",
      "number": 31,
      "content": "Chớ chọn đường nào chúng chọn lấy để đi;",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.953Z",
      "updatedAt": "2025-04-22T10:45:55.953Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "76d02a2a-2c00-477d-b1d7-e6d1aa57a6f1",
      "number": 32,
      "content": "Vì **CHÚA** ghê tởm kẻ gian tà tội lỗi,",
      "order": 0,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.966Z",
      "updatedAt": "2025-04-22T10:45:55.966Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "263d89e2-22fa-4544-b0ef-06c824ad60a9",
      "number": 32,
      "content": "Nhưng Ngài kết bạn tâm giao với người chính trực ngay lành.",
      "order": 1,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.979Z",
      "updatedAt": "2025-04-22T10:45:55.979Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "2763d7ed-efd0-4c76-8f35-e0b357de81c3",
      "number": 33,
      "content": "Sự nguyền rủa của **CHÚA** giáng trên nhà kẻ gian ác,",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:55.993Z",
      "updatedAt": "2025-04-22T10:45:55.993Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "989ecd95-323d-4ca9-9980-ebb93c23b2a4",
      "number": 33,
      "content": "Nhưng phước hạnh Ngài ban cho chỗ ở người ngay lành.",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:56.005Z",
      "updatedAt": "2025-04-22T10:45:56.005Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "e1acb586-c9d1-43a8-b037-457155b343ca",
      "number": 34,
      "content": "Ngài chế giễu phường chê bai nhạo báng,",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:56.019Z",
      "updatedAt": "2025-04-22T10:45:56.019Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "62f4b045-8bc6-4253-8187-2ad5f34fa9ad",
      "number": 34,
      "content": "Nhưng Ngài ban ơn cho người thật sự khiêm nhường.",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:56.032Z",
      "updatedAt": "2025-04-22T10:45:56.032Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "b73ce7c8-0717-4ac5-97e1-901e48eb4264",
      "number": 35,
      "content": "Người khôn ngoan sẽ thừa hưởng những hiển vinh tôn trọng,",
      "order": 0,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:56.049Z",
      "updatedAt": "2025-04-22T10:45:56.049Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    },
    {
      "id": "d989bf87-3498-41d9-b80d-60830d8c7e69",
      "number": 35,
      "content": "Còn kẻ điên rồ bị tống cho nhục nhã ê chề.",
      "order": 1,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:56.062Z",
      "updatedAt": "2025-04-22T10:45:56.062Z",
      "chapterId": "2b2e1325-eb14-4a73-9435-0295a5d9abe4"
    }
  ],
