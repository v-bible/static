      "number": 33,
      "content": "Và đánh vào mũi sẽ làm cho chảy máu,",
      "order": 1,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:47:23.591Z",
