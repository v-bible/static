    },
    {
      "id": "6523156f-a668-484e-b592-7512ec168bbd",
      "number": 33,
      "content": "Vì ép sữa sẽ làm ra bơ,",
      "order": 0,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:29.770Z",
      "updatedAt": "2025-04-24T02:02:29.770Z",
