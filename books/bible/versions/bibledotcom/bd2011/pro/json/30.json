    },
    {
      "id": "7048e3c5-bca5-4082-ab52-4861abc92296",
      "number": 33,
      "content": "Và đánh vào mũi sẽ làm cho chảy máu,",
      "order": 1,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:29.780Z",
      "updatedAt": "2025-04-24T02:02:29.780Z",
      "chapterId": "08fe14cc-86b6-4570-b48b-a62bb997b1fc"
    },
    {
      "id": "60a78f60-daf3-4d6b-9f05-7cafa5d30275",
      "number": 33,
      "content": "Cũng vậy chọc cho giận sẽ sinh ra tranh cạnh.",
      "order": 2,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
