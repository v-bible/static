    {
      "id": "5047d1e7-c3a8-47e4-a59a-2a42f6f423a5",
      "number": 32,
      "content": "Hãy lấy tay bụm miệng mình lại.",
      "order": 2,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:29.760Z",
