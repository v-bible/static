{
  "chapter": {
    "id": "f48d962c-9539-4bdb-84ed-1e6d2392f86b",
    "number": 4,
    "ref": "https://www.bible.com/bible/19/SNG.4.BD2011",
    "createdAt": "2025-04-24T01:29:08.360Z",
    "updatedAt": "2025-04-24T01:29:08.360Z",
    "bookId": "1d4ae9a6-83b3-4337-b059-52a71c117497"
  },
  "verses": [
    {
      "id": "f917d208-f8c1-4ff4-b07e-df19e4e17aa6",
      "number": 1,
      "content": "Em đẹp quá, em yêu!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.379Z",
      "updatedAt": "2025-04-24T02:03:04.379Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "971995d0-4a85-465b-9e81-3263ae799647",
      "number": 1,
      "content": "Em đẹp quá!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.417Z",
      "updatedAt": "2025-04-24T02:03:04.417Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "beccbf1c-4984-43ca-96b0-845a09ac8373",
      "number": 1,
      "content": "Sau màn the che mặt,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.427Z",
      "updatedAt": "2025-04-24T02:03:04.427Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "cefb824e-c73b-49c7-90c4-f557f188fabc",
      "number": 1,
      "content": "Đôi mắt em như một cặp bồ câu;",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.440Z",
      "updatedAt": "2025-04-24T02:03:04.440Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "8c08f8b4-dd49-4db2-a9e1-f306e72a4f69",
      "number": 1,
      "content": "Mái tóc em bồng bềnh như một đàn sơn dương",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.453Z",
      "updatedAt": "2025-04-24T02:03:04.453Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "d3b56763-a616-464a-bcc2-656c1646b3e7",
      "number": 1,
      "content": "Đang từ triền núi Ghi-lê-át đi xuống.",
      "order": 5,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.466Z",
      "updatedAt": "2025-04-24T02:03:04.466Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "3ec6a023-ceef-4e70-9975-a70b174c353e",
      "number": 2,
      "content": "Răng em trắng đẹp như một đàn chiên",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.476Z",
      "updatedAt": "2025-04-24T02:03:04.476Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "abb363dd-940b-4748-b84c-1ba96baa604d",
      "number": 2,
      "content": "Vừa mới được hớt lông, tắm dưới suối đi lên,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.486Z",
      "updatedAt": "2025-04-24T02:03:04.486Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "9457bf59-15c8-437f-bd08-f2c6bcdfaa55",
      "number": 2,
      "content": "Tất cả đều là cặp đôi,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.495Z",
      "updatedAt": "2025-04-24T02:03:04.495Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "a7fb4d59-7433-4441-8528-b26350e79441",
      "number": 2,
      "content": "Không chiếc nào đơn lẻ.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.505Z",
      "updatedAt": "2025-04-24T02:03:04.505Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "b4789cb7-81bb-4607-b23f-b68a5d6d6adb",
      "number": 3,
      "content": "Môi em mọng đỏ như tao chỉ hồng,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.518Z",
      "updatedAt": "2025-04-24T02:03:04.518Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "c1c9d009-773e-4607-bd7a-7ed8b650f0d4",
      "number": 3,
      "content": "Miệng em duyên dáng thật đáng yêu.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.527Z",
      "updatedAt": "2025-04-24T02:03:04.527Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "de21c4e1-d958-43d7-b774-8e65c51e235f",
      "number": 3,
      "content": "Má em đỏ hây hây như nửa quả thạch lựu,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.537Z",
      "updatedAt": "2025-04-24T02:03:04.537Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "673cca7b-6a21-4c08-bc78-dc4ab167f947",
      "number": 3,
      "content": "Thấp thoáng sau màn the che mặt.",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.547Z",
      "updatedAt": "2025-04-24T02:03:04.547Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "806bf1b1-6355-42e4-b28f-51aa48eeec5f",
      "number": 4,
      "content": "Cổ em xinh đẹp như Tháp Đa-vít,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.557Z",
      "updatedAt": "2025-04-24T02:03:04.557Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "f6753224-47ac-4cd5-baf5-e8b3eea4ad07",
      "number": 4,
      "content": "Được xây dựng để chứa binh khí,",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.570Z",
      "updatedAt": "2025-04-24T02:03:04.570Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "b6b8766a-5aa3-45b7-9b78-bad416e0a996",
      "number": 4,
      "content": "Trên đó treo một ngàn cái khiên,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.580Z",
      "updatedAt": "2025-04-24T02:03:04.580Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "40bdd4a3-cede-4515-a204-0ecd0834dd71",
      "number": 4,
      "content": "Đó là khiên của những dũng sĩ.",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.596Z",
      "updatedAt": "2025-04-24T02:03:04.596Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "d8542f04-ea3b-4225-bfdf-09251e189353",
      "number": 5,
      "content": "Đôi gò bồng đảo em như một cặp linh dương con,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.610Z",
      "updatedAt": "2025-04-24T02:03:04.610Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "cff875c7-18d8-4ae7-9fb0-094b261031f9",
      "number": 5,
      "content": "Cặp con sinh đôi của một linh dương mẹ,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.622Z",
      "updatedAt": "2025-04-24T02:03:04.622Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "e8be6321-4a2a-4874-909a-871e85a65859",
      "number": 5,
      "content": "Đang gặm cỏ non giữa thung lũng hoa huệ.",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.632Z",
      "updatedAt": "2025-04-24T02:03:04.632Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "d47a7356-413e-4fe0-99da-9363592b9357",
      "number": 6,
      "content": "Trước khi bình minh đến",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.645Z",
      "updatedAt": "2025-04-24T02:03:04.645Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "0e5407f3-f2ac-4f1b-9f76-2dc7aaa3d89c",
      "number": 6,
      "content": "Và bóng tối biến đi,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.655Z",
      "updatedAt": "2025-04-24T02:03:04.655Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "384cd7c2-54ad-4516-af72-2666ce3f1e19",
      "number": 6,
      "content": "Anh sẽ đến núi mộc dược",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.665Z",
      "updatedAt": "2025-04-24T02:03:04.665Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "7cabcf20-3836-4f8f-be50-42dd99769654",
      "number": 6,
      "content": "Và đến đồi nhũ hương.",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.675Z",
      "updatedAt": "2025-04-24T02:03:04.675Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "37e1a7c8-2586-4f40-9429-58d7b91565f1",
      "number": 7,
      "content": "Em yêu ơi, toàn thân em đẹp tuyệt trần,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.685Z",
      "updatedAt": "2025-04-24T02:03:04.685Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "d640f889-bf55-4d22-9421-c36dd3d4cd16",
      "number": 7,
      "content": "Không chỗ nào có thể chê được.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.694Z",
      "updatedAt": "2025-04-24T02:03:04.694Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "eee99e96-aeeb-40ff-953d-eb3a9b926c97",
      "number": 8,
      "content": "Hỡi hôn thê của anh, hãy cùng anh rời khỏi Li-băng,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.704Z",
      "updatedAt": "2025-04-24T02:03:04.704Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "2fa72c90-ed85-48da-ba71-982f2f4008eb",
      "number": 8,
      "content": "Hãy cùng anh rời khỏi Li-băng,",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.714Z",
      "updatedAt": "2025-04-24T02:03:04.714Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "a40caf47-7892-432d-bf74-f227b07470e2",
      "number": 8,
      "content": "Hãy cùng nhau ngắm cảnh bình minh từ đỉnh A-ma-na,",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.723Z",
      "updatedAt": "2025-04-24T02:03:04.723Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "fcb363f2-9a1c-41a5-97c8-ebefb9204b66",
      "number": 8,
      "content": "Từ đỉnh Sê-nia và từ đỉnh Hẹt-môn,",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.733Z",
      "updatedAt": "2025-04-24T02:03:04.733Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "a5bf53f7-6cd9-4047-aa12-b321b93a742c",
      "number": 8,
      "content": "Từ các hang sư tử",
      "order": 4,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.743Z",
      "updatedAt": "2025-04-24T02:03:04.743Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "f24143b7-2909-489b-9890-335275b1c806",
      "number": 8,
      "content": "Và từ những núi hùm beo.",
      "order": 5,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.753Z",
      "updatedAt": "2025-04-24T02:03:04.753Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "cbc4733a-ea61-4c04-a6b5-4619dad6526f",
      "number": 9,
      "content": "Em đã chiếm đoạt tim anh rồi, hỡi em gái của anh, hôn thê của anh;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.762Z",
      "updatedAt": "2025-04-24T02:03:04.762Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "5471066a-3c54-4cc3-bad5-dcbf92a32baa",
      "number": 9,
      "content": "Em đã cướp mất hồn anh bằng một cái liếc mắt của em,",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.775Z",
      "updatedAt": "2025-04-24T02:03:04.775Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "77c5d796-4519-467f-9fa1-e463d413bf85",
      "number": 9,
      "content": "Bằng một dây chuyền đeo cổ của em.",
      "order": 2,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.785Z",
      "updatedAt": "2025-04-24T02:03:04.785Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "5a44f028-98ea-4538-be89-9fe3ee461caa",
      "number": 10,
      "content": "Tình yêu của em thật tuyệt vời, hỡi em gái của anh, hôn thê của anh,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.795Z",
      "updatedAt": "2025-04-24T02:03:04.795Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "0118b7e8-de6f-4e93-9643-62994496476e",
      "number": 10,
      "content": "Tình yêu của em thật nồng nàn hơn rượu;",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.805Z",
      "updatedAt": "2025-04-24T02:03:04.805Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "3ddac84a-03da-4cf8-aa42-8a9b2f975952",
      "number": 10,
      "content": "Mùi nước hoa từ người em làm sảng khoái hơn mọi thứ hương thơm.",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.814Z",
      "updatedAt": "2025-04-24T02:03:04.814Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "e2a13ffa-482d-4a14-b09b-05b667e5f33f",
      "number": 11,
      "content": "Môi em ngọt lịm như mật nhểu, hỡi hôn thê của anh;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.824Z",
      "updatedAt": "2025-04-24T02:03:04.824Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "0c5cc810-d2a5-4375-b115-29f0c86d4143",
      "number": 11,
      "content": "Dưới lưỡi em là mật ngọt và sữa ngon.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.834Z",
      "updatedAt": "2025-04-24T02:03:04.834Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "38310c00-9693-4e6a-809d-a6c7c2220278",
      "number": 11,
      "content": "Mùi thơm từ y phục em tỏa ra như mùi thơm trên Núi Li-băng.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.844Z",
      "updatedAt": "2025-04-24T02:03:04.844Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "2951e1d0-370c-4e3b-a8cd-6f008d71464b",
      "number": 12,
      "content": "Em là mảnh vườn bị khóa kín, hỡi em gái của anh, hôn thê của anh,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.854Z",
      "updatedAt": "2025-04-24T02:03:04.854Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "d45f8e0a-4be5-4797-912b-ae2eac79850b",
      "number": 12,
      "content": "Em là suối nước bị rào ngăn, là mạch nước bị niêm phong, chưa hề có ai uống.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.864Z",
      "updatedAt": "2025-04-24T02:03:04.864Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "0cc9bc4e-d1ed-485c-87bf-7fa3b3d014d3",
      "number": 13,
      "content": "Vườn cây của em là một vườn đầy thạch lựu,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.874Z",
      "updatedAt": "2025-04-24T02:03:04.874Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "d33ae034-d3b7-43dc-adf4-befddf037991",
      "number": 13,
      "content": "Đầy những hoa thơm trái ngọt,",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.889Z",
      "updatedAt": "2025-04-24T02:03:04.889Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "ea6e2d28-2738-4203-baa2-4ee29bbc45f8",
      "number": 13,
      "content": "Nào phụng tiên và cam tùng,",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.899Z",
      "updatedAt": "2025-04-24T02:03:04.899Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "0eebdb50-1611-43b9-920c-283a618d1b0e",
      "number": 14,
      "content": "Cam tùng và hoàng khương, đinh hương và nhục quế,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.909Z",
      "updatedAt": "2025-04-24T02:03:04.909Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "dc06d7ab-1b48-4e59-8b13-d105c31bd935",
      "number": 14,
      "content": "Cùng mọi cây nhũ hương,",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.919Z",
      "updatedAt": "2025-04-24T02:03:04.919Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "13b828b7-a01c-46f9-ba30-fd8b4b8e8339",
      "number": 14,
      "content": "Mộc dược và trầm hương,",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.928Z",
      "updatedAt": "2025-04-24T02:03:04.928Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "a4b4250b-74f2-49bd-8327-74872d0dc85e",
      "number": 14,
      "content": "Với tất cả những cây sản xuất các hương liệu thượng hạng.",
      "order": 3,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.938Z",
      "updatedAt": "2025-04-24T02:03:04.938Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "52fa9a77-5a71-474a-a90d-183263477a37",
      "number": 15,
      "content": "Em là suối nước trong vườn,",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.948Z",
      "updatedAt": "2025-04-24T02:03:04.948Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "fb0dff72-8b51-401d-8543-120625ec9ccc",
      "number": 15,
      "content": "Là giếng nước trào tuôn, tuôn dòng từ Li-băng chảy xuống.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.958Z",
      "updatedAt": "2025-04-24T02:03:04.958Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "8946d370-aed0-4032-a6e2-55b1589ea470",
      "number": 16,
      "content": "Hãy nổi dậy, hỡi gió từ phương bắc.",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.968Z",
      "updatedAt": "2025-04-24T02:03:04.968Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "9b7edd74-5915-45d0-8b0c-4c5006b6adb6",
      "number": 16,
      "content": "Hãy đến đây, hỡi gió từ phương nam.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.987Z",
      "updatedAt": "2025-04-24T02:03:04.987Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "2245f1ea-04ac-448c-aca1-910a21c864de",
      "number": 16,
      "content": "Hãy thổi trên mảnh vườn của ta,",
      "order": 2,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:04.997Z",
      "updatedAt": "2025-04-24T02:03:04.997Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "c664c71e-30cc-4917-833a-dcb71262b88a",
      "number": 16,
      "content": "Để hương thơm của nó tỏa lan ra ngoài;",
      "order": 3,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:05.007Z",
      "updatedAt": "2025-04-24T02:03:05.007Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "b87729c6-192d-461f-98f0-fe5c9e1efb56",
      "number": 16,
      "content": "Để người yêu của ta đến trong vườn của chàng,",
      "order": 4,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:05.017Z",
      "updatedAt": "2025-04-24T02:03:05.017Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    },
    {
      "id": "619e1576-1145-4c30-a923-1134b72e7806",
      "number": 16,
      "content": "Hầu thưởng thức những trái ngọt ngon của nó.",
      "order": 5,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:05.027Z",
      "updatedAt": "2025-04-24T02:03:05.027Z",
      "chapterId": "f48d962c-9539-4bdb-84ed-1e6d2392f86b"
    }
  ],
