{
  "chapter": {
    "id": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1",
    "number": 7,
    "ref": "https://www.bible.com/bible/19/SNG.7.BD2011",
    "createdAt": "2025-04-22T10:10:28Z",
    "updatedAt": "2025-04-22T10:10:28Z",
    "bookId": "0866c1f9-bea1-4051-9bb8-75be65257148"
  },
  "verses": [
    {
      "id": "e333b07b-5e85-4277-804f-9930e683f9fd",
      "number": 1,
      "content": "Công nương hỡi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.739Z",
      "updatedAt": "2025-04-22T10:48:20.739Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "44b2e434-6156-4f60-9eb5-fe3f013dfc16",
      "number": 1,
      "content": "Hai bàn chân em thật xinh đẹp trong đôi hài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.756Z",
      "updatedAt": "2025-04-22T10:48:20.756Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "04f4c338-2c39-4430-bbbd-12339cf15608",
      "number": 1,
      "content": "Đường cong của cặp đùi em giống như đôi trụ ngọc thạch",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.766Z",
      "updatedAt": "2025-04-22T10:48:20.766Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "25f70ecf-d245-456d-aee0-3da49a36fed4",
      "number": 1,
      "content": "Được bàn tay nghệ thuật tài khéo tạc nên.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.777Z",
      "updatedAt": "2025-04-22T10:48:20.777Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "15d48eb9-6748-4f3e-915e-db068c1b42da",
      "number": 2,
      "content": "Rốn em tròn như chung rượu,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.792Z",
      "updatedAt": "2025-04-22T10:48:20.792Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "604cc4a7-648f-426b-8080-65aa64334e5a",
      "number": 2,
      "content": "Không bao giờ cạn hết rượu pha.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.813Z",
      "updatedAt": "2025-04-22T10:48:20.813Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "a32d7ca8-3735-4090-b706-d4f6b29f249d",
      "number": 2,
      "content": "Bụng em như một gò lúa mạch,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.824Z",
      "updatedAt": "2025-04-22T10:48:20.824Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "0c990c4d-de46-4e50-a3ac-316ba50172b3",
      "number": 2,
      "content": "Bao quanh bằng các hoa huệ.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.834Z",
      "updatedAt": "2025-04-22T10:48:20.834Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "283bcfd0-d27d-452a-85c6-d11bf6723712",
      "number": 3,
      "content": "Đôi gò bồng đảo em như một cặp linh dương con,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.847Z",
      "updatedAt": "2025-04-22T10:48:20.847Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "8d14ff6d-17b2-499f-90e1-9a5eba9c3620",
      "number": 3,
      "content": "Cặp con sinh đôi của một linh dương mẹ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.857Z",
      "updatedAt": "2025-04-22T10:48:20.857Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "71a0c0dd-e812-403e-8618-e2a97746d892",
      "number": 4,
      "content": "Cổ em như một tháp ngà,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.867Z",
      "updatedAt": "2025-04-22T10:48:20.867Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "e2fa036b-4ed0-46a8-b0da-467740b1a619",
      "number": 4,
      "content": "Mắt em như các hồ nước ở Hếch-bôn,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.877Z",
      "updatedAt": "2025-04-22T10:48:20.877Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "a82c5d04-120d-4a5a-bd41-6c43d4455088",
      "number": 4,
      "content": "Bên Cổng Bát Ra-bim.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.887Z",
      "updatedAt": "2025-04-22T10:48:20.887Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "399a4cd2-52ae-4294-86bc-f62466a1880c",
      "number": 4,
      "content": "Mũi em như Tháp Li-băng,",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.900Z",
      "updatedAt": "2025-04-22T10:48:20.900Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "3b3d54d2-4378-4d93-9b57-f65d516929a1",
      "number": 4,
      "content": "Hướng nhìn về Đa-mách.",
      "order": 4,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.910Z",
      "updatedAt": "2025-04-22T10:48:20.910Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "d54b3111-9e89-4229-9b70-b693282cddaa",
      "number": 5,
      "content": "Đầu em trên thân hình em như Núi Cạt-mên.",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.920Z",
      "updatedAt": "2025-04-22T10:48:20.920Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "4ee197ec-ae9a-42b8-966d-38b9b58ec8f2",
      "number": 5,
      "content": "Tóc em như những giải lụa hồng",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.930Z",
      "updatedAt": "2025-04-22T10:48:20.930Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "a4ec88c5-099f-4a47-a315-7ad447ead765",
      "number": 5,
      "content": "Mà nhà vua đã bị trói buộc vào đó.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.940Z",
      "updatedAt": "2025-04-22T10:48:20.940Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "6b2f3896-4578-40c5-81f5-ab73dcdf38f1",
      "number": 6,
      "content": "Em xinh đẹp quá và duyên dáng biết bao;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.950Z",
      "updatedAt": "2025-04-22T10:48:20.950Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "8dbd3a8c-2102-4853-aaee-434c4d9aeb72",
      "number": 6,
      "content": "Người yêu hỡi, em quả là tuyệt đẹp.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.963Z",
      "updatedAt": "2025-04-22T10:48:20.963Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "dac0055b-eb99-4b4e-85b0-8921be91bc86",
      "number": 7,
      "content": "Dáng em như một cây chà là,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.976Z",
      "updatedAt": "2025-04-22T10:48:20.976Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "f562f742-8bf6-4bef-8a06-37880bc20432",
      "number": 7,
      "content": "Đôi gò bồng đảo em như một chùm trái ngọt ngon.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.986Z",
      "updatedAt": "2025-04-22T10:48:20.986Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "8cdade02-c47d-46f4-b8fe-b4d21539153f",
      "number": 8,
      "content": "Anh nói, anh sẽ trèo lên cây chà là đó,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:20.996Z",
      "updatedAt": "2025-04-22T10:48:20.996Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "910a0c90-b225-451c-8a30-7822d6183277",
      "number": 8,
      "content": "Hái trái nó, và ăn.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.006Z",
      "updatedAt": "2025-04-22T10:48:21.006Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "20cf969c-342f-4763-8919-f109b6bb063f",
      "number": 8,
      "content": "Ôi, ước gì đôi gò bồng đảo em như chùm nho chín mọng,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.017Z",
      "updatedAt": "2025-04-22T10:48:21.017Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "069e132d-c826-448f-a508-44f3b79132b2",
      "number": 8,
      "content": "Hơi thở em như những trái táo thơm ngon,",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.028Z",
      "updatedAt": "2025-04-22T10:48:21.028Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "3fb96002-1966-4208-96f8-ab70d08846a4",
      "number": 9,
      "content": "Và miệng em như rượu nồng hảo hạng.",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.041Z",
      "updatedAt": "2025-04-22T10:48:21.041Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "da1248a9-e6f7-468b-a442-ed70be97879f",
      "number": 9,
      "content": "Nguyện rượu ấy rót vào miệng người yêu của tôi,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.054Z",
      "updatedAt": "2025-04-22T10:48:21.054Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "11b3ce36-5e03-46e3-9e16-eac5ee4150f1",
      "number": 9,
      "content": "Chảy nhẹ nhàng qua bờ môi thiếp ngủ và lẻn vào làm tê tái chân răng.",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.083Z",
      "updatedAt": "2025-04-22T10:48:21.083Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "77e75a4d-beba-4b37-ae5a-36fdc5b1475a",
      "number": 10,
      "content": "Tôi thuộc về người yêu của tôi,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.096Z",
      "updatedAt": "2025-04-22T10:48:21.096Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "2fada674-75dd-4520-9a3d-0b4108d1ba9b",
      "number": 10,
      "content": "Sự thèm khát của chàng sẽ do tôi làm thỏa mãn.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.109Z",
      "updatedAt": "2025-04-22T10:48:21.109Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "2ce276a6-eacf-48ba-9213-ea899688be22",
      "number": 11,
      "content": "Hãy đến, người yêu của em;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.125Z",
      "updatedAt": "2025-04-22T10:48:21.125Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "eedd652e-e25a-4935-8857-613e42ddfcce",
      "number": 11,
      "content": "Chúng ta hãy đi về miền quê,",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.139Z",
      "updatedAt": "2025-04-22T10:48:21.139Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "5e00579f-2d5e-44e0-ae47-4e552a521adc",
      "number": 11,
      "content": "Đêm nay chúng mình sẽ ngủ lại trong làng.",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.152Z",
      "updatedAt": "2025-04-22T10:48:21.152Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "7d3bb691-1040-4692-ac83-5e940ebdf688",
      "number": 12,
      "content": "Sáng mai chúng ta sẽ dậy sớm đi vào vườn nho,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.165Z",
      "updatedAt": "2025-04-22T10:48:21.165Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "d6ba1cf3-44f5-401d-833c-06c671820211",
      "number": 12,
      "content": "Để xem các nhành nho đã đâm chồi nẩy lộc,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.178Z",
      "updatedAt": "2025-04-22T10:48:21.178Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "d7651721-d9ed-44cc-b7e6-3885708cdfe5",
      "number": 12,
      "content": "Hoa nho đã nở,",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.192Z",
      "updatedAt": "2025-04-22T10:48:21.192Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "7cc54d28-7194-40a4-82ef-54445928fd4d",
      "number": 12,
      "content": "Và thạch lựu đã trổ hoa hay chưa,",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.205Z",
      "updatedAt": "2025-04-22T10:48:21.205Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "546b6602-40f5-4e6f-a2b4-de7a5d59057c",
      "number": 12,
      "content": "Rồi ở đó em sẽ dâng tặng anh tình yêu của em.",
      "order": 4,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.218Z",
      "updatedAt": "2025-04-22T10:48:21.218Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "7946d769-ef72-462e-87ac-443e3cf71e05",
      "number": 13,
      "content": "Trái phong già đã phảng phất mùi thơm,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:21.231Z",
      "updatedAt": "2025-04-22T10:48:21.231Z",
      "chapterId": "5d571ca7-5e23-4fe9-a563-cbd0092e39a1"
    },
    {
      "id": "b2ded970-30fb-45ef-a1a0-5ad52947eb3b",
      "number": 13,
      "content": "Tại các cửa đã có mọi trái ngọt ngon dành sẵn,",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
