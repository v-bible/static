{
  "chapter": {
    "id": "b409a5b6-5769-4314-a882-b086fa9f367f",
    "number": 7,
    "ref": "https://www.bible.com/bible/19/SNG.7.BD2011",
    "createdAt": "2025-04-24T01:29:08.402Z",
    "updatedAt": "2025-04-24T01:29:08.402Z",
    "bookId": "1d4ae9a6-83b3-4337-b059-52a71c117497"
  },
  "verses": [
    {
      "id": "830837ac-d590-4491-8a53-d0c7d94ab267",
      "number": 1,
      "content": "Công nương hỡi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.717Z",
      "updatedAt": "2025-04-24T02:03:11.717Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "e7068e48-5cdb-410e-8828-64b425c84dce",
      "number": 1,
      "content": "Hai bàn chân em thật xinh đẹp trong đôi hài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.733Z",
      "updatedAt": "2025-04-24T02:03:11.733Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "9e81718f-d8e5-4766-a119-0fec8379b992",
      "number": 1,
      "content": "Đường cong của cặp đùi em giống như đôi trụ ngọc thạch",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.742Z",
      "updatedAt": "2025-04-24T02:03:11.742Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "8caab17c-5325-4a06-a9c3-3ac347f4dab0",
      "number": 1,
      "content": "Được bàn tay nghệ thuật tài khéo tạc nên.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.755Z",
      "updatedAt": "2025-04-24T02:03:11.755Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "601b1ab6-a72f-4ff0-b647-849713ad3939",
      "number": 2,
      "content": "Rốn em tròn như chung rượu,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.768Z",
      "updatedAt": "2025-04-24T02:03:11.768Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "a6e6e13f-283c-46fa-b382-dd6f85bcd8c9",
      "number": 2,
      "content": "Không bao giờ cạn hết rượu pha.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.778Z",
      "updatedAt": "2025-04-24T02:03:11.778Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "2c163b85-711d-4ca8-9003-bbb74ec404ad",
      "number": 2,
      "content": "Bụng em như một gò lúa mạch,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.788Z",
      "updatedAt": "2025-04-24T02:03:11.788Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "090d92bc-bebd-4c82-81d8-373e5df4c627",
      "number": 2,
      "content": "Bao quanh bằng các hoa huệ.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.798Z",
      "updatedAt": "2025-04-24T02:03:11.798Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "01c23445-d6b9-457c-aa24-d1ead191c081",
      "number": 3,
      "content": "Đôi gò bồng đảo em như một cặp linh dương con,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.807Z",
      "updatedAt": "2025-04-24T02:03:11.807Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "56a072bf-f484-4fd1-a298-1d45b2e363d9",
      "number": 3,
      "content": "Cặp con sinh đôi của một linh dương mẹ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.817Z",
      "updatedAt": "2025-04-24T02:03:11.817Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "2be09d3e-68d9-4101-9c23-9c221747d625",
      "number": 4,
      "content": "Cổ em như một tháp ngà,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.827Z",
      "updatedAt": "2025-04-24T02:03:11.827Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "da11d505-fe03-49b3-8478-26375eaefde1",
      "number": 4,
      "content": "Mắt em như các hồ nước ở Hếch-bôn,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.837Z",
      "updatedAt": "2025-04-24T02:03:11.837Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "bac41341-33c4-43b0-9b4d-52a575c4b18c",
      "number": 4,
      "content": "Bên Cổng Bát Ra-bim.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.846Z",
      "updatedAt": "2025-04-24T02:03:11.846Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "18200159-becd-4476-87da-6bb5a5e70584",
      "number": 4,
      "content": "Mũi em như Tháp Li-băng,",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.856Z",
      "updatedAt": "2025-04-24T02:03:11.856Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "69211007-e584-4b27-9862-0d7795540c77",
      "number": 4,
      "content": "Hướng nhìn về Đa-mách.",
      "order": 4,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.865Z",
      "updatedAt": "2025-04-24T02:03:11.865Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "d68caab1-5f8c-46f7-a7ec-68b1f29828e3",
      "number": 5,
      "content": "Đầu em trên thân hình em như Núi Cạt-mên.",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.875Z",
      "updatedAt": "2025-04-24T02:03:11.875Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "cb800d84-c554-44a8-9ee3-99f4a5aa3aed",
      "number": 5,
      "content": "Tóc em như những giải lụa hồng",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.885Z",
      "updatedAt": "2025-04-24T02:03:11.885Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "31ab954d-3a26-4cfc-885a-317b56483ccc",
      "number": 5,
      "content": "Mà nhà vua đã bị trói buộc vào đó.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.898Z",
      "updatedAt": "2025-04-24T02:03:11.898Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "bac262b1-98e2-4cb9-b929-946c3ca79709",
      "number": 6,
      "content": "Em xinh đẹp quá và duyên dáng biết bao;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.908Z",
      "updatedAt": "2025-04-24T02:03:11.908Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "50eb858d-5c32-485f-91eb-6a942a2e695b",
      "number": 6,
      "content": "Người yêu hỡi, em quả là tuyệt đẹp.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.917Z",
      "updatedAt": "2025-04-24T02:03:11.917Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "8918824a-3574-4511-90ef-a30036641c67",
      "number": 7,
      "content": "Dáng em như một cây chà là,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.930Z",
      "updatedAt": "2025-04-24T02:03:11.930Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "2a9a6907-b5c4-489d-bc6d-d9485efc3987",
      "number": 7,
      "content": "Đôi gò bồng đảo em như một chùm trái ngọt ngon.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.940Z",
      "updatedAt": "2025-04-24T02:03:11.940Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "34b3d073-8aed-40ae-8a28-458aa6d3d323",
      "number": 8,
      "content": "Anh nói, anh sẽ trèo lên cây chà là đó,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.950Z",
      "updatedAt": "2025-04-24T02:03:11.950Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "d177186a-2046-41b4-8137-c21c1dc3196f",
      "number": 8,
      "content": "Hái trái nó, và ăn.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.960Z",
      "updatedAt": "2025-04-24T02:03:11.960Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "380614db-a58e-401c-94c7-a6a242bcaf9c",
      "number": 8,
      "content": "Ôi, ước gì đôi gò bồng đảo em như chùm nho chín mọng,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.969Z",
      "updatedAt": "2025-04-24T02:03:11.969Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "b57b18ef-b165-4e45-97e6-7393574a647e",
      "number": 8,
      "content": "Hơi thở em như những trái táo thơm ngon,",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.979Z",
      "updatedAt": "2025-04-24T02:03:11.979Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "17b022bb-31d8-4d60-bb2a-99441265b6f4",
      "number": 9,
      "content": "Và miệng em như rượu nồng hảo hạng.",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.989Z",
      "updatedAt": "2025-04-24T02:03:11.989Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "a00510c6-dea8-40e1-bd54-41e8eba20400",
      "number": 9,
      "content": "Nguyện rượu ấy rót vào miệng người yêu của tôi,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:11.999Z",
      "updatedAt": "2025-04-24T02:03:11.999Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "e9c8a8a2-894d-4706-99a6-6d338428c569",
      "number": 9,
      "content": "Chảy nhẹ nhàng qua bờ môi thiếp ngủ và lẻn vào làm tê tái chân răng.",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.022Z",
      "updatedAt": "2025-04-24T02:03:12.022Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "92705d90-408a-450b-bec7-26d95a451472",
      "number": 10,
      "content": "Tôi thuộc về người yêu của tôi,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.032Z",
      "updatedAt": "2025-04-24T02:03:12.032Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "e0a6a1ed-7314-4172-8630-279e06f3c312",
      "number": 10,
      "content": "Sự thèm khát của chàng sẽ do tôi làm thỏa mãn.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.042Z",
      "updatedAt": "2025-04-24T02:03:12.042Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "416f030a-e26e-44e6-a1d8-0a238196779c",
      "number": 11,
      "content": "Hãy đến, người yêu của em;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.051Z",
      "updatedAt": "2025-04-24T02:03:12.051Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "b0a1dee7-b7e7-4ed1-9da8-dfa4e6f2481d",
      "number": 11,
      "content": "Chúng ta hãy đi về miền quê,",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.061Z",
      "updatedAt": "2025-04-24T02:03:12.061Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "1848876f-2081-4fd0-8910-1c05f2cc3107",
      "number": 11,
      "content": "Đêm nay chúng mình sẽ ngủ lại trong làng.",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.071Z",
      "updatedAt": "2025-04-24T02:03:12.071Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "0f198711-584b-481f-bff8-d2a98a122a43",
      "number": 12,
      "content": "Sáng mai chúng ta sẽ dậy sớm đi vào vườn nho,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.081Z",
      "updatedAt": "2025-04-24T02:03:12.081Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "5e3ea9c4-b259-4724-a7b6-7bffa8e751ce",
      "number": 12,
      "content": "Để xem các nhành nho đã đâm chồi nẩy lộc,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.090Z",
      "updatedAt": "2025-04-24T02:03:12.090Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "6270384a-c346-4bde-b888-9323a2b6244b",
      "number": 12,
      "content": "Hoa nho đã nở,",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.100Z",
      "updatedAt": "2025-04-24T02:03:12.100Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "6e8dfc35-630f-4663-b377-1985f1db7d37",
      "number": 12,
      "content": "Và thạch lựu đã trổ hoa hay chưa,",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.110Z",
      "updatedAt": "2025-04-24T02:03:12.110Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "5dec3dc6-ef74-4cc7-a82a-fdda2e807dc3",
      "number": 12,
      "content": "Rồi ở đó em sẽ dâng tặng anh tình yêu của em.",
      "order": 4,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.120Z",
      "updatedAt": "2025-04-24T02:03:12.120Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "691361e3-9faa-41d2-9877-58ebc73cb6e3",
      "number": 13,
      "content": "Trái phong già đã phảng phất mùi thơm,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.130Z",
      "updatedAt": "2025-04-24T02:03:12.130Z",
      "chapterId": "b409a5b6-5769-4314-a882-b086fa9f367f"
    },
    {
      "id": "aa2c097f-5477-4bcf-a377-0e19aa7fc20a",
      "number": 13,
      "content": "Tại các cửa đã có mọi trái ngọt ngon dành sẵn,",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:12.139Z",
      "updatedAt": "2025-04-24T02:03:12.139Z",
