{
  "chapter": {
    "id": "fedae4d9-e26e-4a65-a786-9913787406d0",
    "number": 2,
    "ref": "https://www.bible.com/bible/19/SNG.2.BD2011",
    "createdAt": "2025-04-24T01:29:08.335Z",
    "updatedAt": "2025-04-24T01:29:08.335Z",
    "bookId": "1d4ae9a6-83b3-4337-b059-52a71c117497"
  },
  "verses": [
    {
      "id": "4533f1ba-e8d6-4a15-9e5e-2b5e8d81ec45",
      "number": 1,
      "content": "Tôi là đóa hoa hồng của Sa-rôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.830Z",
      "updatedAt": "2025-04-24T02:02:59.830Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "e24f7046-6751-4637-a3d6-e140a13ba66b",
      "number": 1,
      "content": "Là hoa huệ trong các thung lũng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.851Z",
      "updatedAt": "2025-04-24T02:02:59.851Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "fa7c1813-298a-4e9c-82e3-354e1261972a",
      "number": 2,
      "content": "Như đóa hoa huệ ở giữa những gai góc thể nào,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.860Z",
      "updatedAt": "2025-04-24T02:02:59.860Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "fe70b239-0397-4aa8-8b6b-43f84bed74ec",
      "number": 2,
      "content": "Người yêu của tôi ở giữa các thiếu nữ cũng như vậy.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.880Z",
      "updatedAt": "2025-04-24T02:02:59.880Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "8dab0303-6503-48a2-8218-7ff7220ac3e3",
      "number": 3,
      "content": "Như cây táo ở giữa những cây rừng thể nào,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.890Z",
      "updatedAt": "2025-04-24T02:02:59.890Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "05090b6b-b52c-49d0-a9a0-1bc5d6baa3cb",
      "number": 3,
      "content": "Người yêu của tôi ở giữa các chàng trai cũng như vậy.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.909Z",
      "updatedAt": "2025-04-24T02:02:59.909Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "2b359a0d-422d-4cca-ad15-f4cc7e30ce2d",
      "number": 3,
      "content": "Tôi yêu thích được ngồi dưới bóng mát của chàng,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.919Z",
      "updatedAt": "2025-04-24T02:02:59.919Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "fdf1129d-8f7f-4ecd-a2bf-f9bb0417dae9",
      "number": 3,
      "content": "Trái của chàng làm ngọt lịm miệng tôi.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.929Z",
      "updatedAt": "2025-04-24T02:02:59.929Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "2f4c501b-958d-4675-b77a-0f234bfa669e",
      "number": 4,
      "content": "Chàng đưa tôi vào phòng tiệc;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.939Z",
      "updatedAt": "2025-04-24T02:02:59.939Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "38aa32a9-7a87-4266-be5b-5941ccb5bb72",
      "number": 4,
      "content": "Băng biểu ngữ của chàng trên tôi là tình yêu.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.949Z",
      "updatedAt": "2025-04-24T02:02:59.949Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "58a13b88-0224-4ba2-890e-1f9cd635f130",
      "number": 5,
      "content": "Hãy cho tôi những trái nho khô để bổ sức tôi lại,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.959Z",
      "updatedAt": "2025-04-24T02:02:59.959Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "d1ba24f2-8613-479a-bb25-32fc9294f36d",
      "number": 5,
      "content": "Hãy cho tôi những trái táo tươi để tôi được bồi dưỡng lại,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.969Z",
      "updatedAt": "2025-04-24T02:02:59.969Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "a6f59f21-e945-42e6-aede-cee4b851c9a2",
      "number": 5,
      "content": "Bởi tôi đang kiệt lực vì yêu.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.979Z",
      "updatedAt": "2025-04-24T02:02:59.979Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "c826aa5e-0d29-4ee7-80a4-a48cd35f9da3",
      "number": 6,
      "content": "Tay trái chàng để dưới đầu tôi,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.988Z",
      "updatedAt": "2025-04-24T02:02:59.988Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "a0f7563e-5a48-4746-a022-4ee1de1d9f93",
      "number": 6,
      "content": "Còn tay phải chàng ôm tôi.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:02:59.998Z",
      "updatedAt": "2025-04-24T02:02:59.998Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "38adccea-0930-45df-9a80-1e46d998129a",
      "number": 7,
      "content": "Hỡi các thiếu nữ ở Giê-ru-sa-lem, tôi nài xin các chị:",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.011Z",
      "updatedAt": "2025-04-24T02:03:00.011Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "d5095316-a5c5-42b2-a981-e46ab32cc05e",
      "number": 7,
      "content": "Hãy quan sát những linh dương và những nai tơ trong đồng,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.021Z",
      "updatedAt": "2025-04-24T02:03:00.021Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "da640625-9045-494f-9480-1fc23f31c232",
      "number": 7,
      "content": "Mà đừng khơi dậy hay đánh thức ái tình",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.031Z",
      "updatedAt": "2025-04-24T02:03:00.031Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "38c9b6fc-7128-48ae-86d5-d231dba781b8",
      "number": 7,
      "content": "Cho đến khi nó muốn.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.041Z",
      "updatedAt": "2025-04-24T02:03:00.041Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "cfef2046-effa-49e7-9334-29a72fa2eff4",
      "number": 8,
      "content": "Hãy lắng nghe! Tiếng của người yêu tôi đó.",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.051Z",
      "updatedAt": "2025-04-24T02:03:00.051Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "c1385e3d-0ab3-468e-9c2e-344acf375fe9",
      "number": 8,
      "content": "Xem kìa! Chàng đang đến,",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.070Z",
      "updatedAt": "2025-04-24T02:03:00.070Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "64dc91b0-2aac-47d9-a9ae-a715a318dc18",
      "number": 8,
      "content": "Vượt qua các rặng núi,",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.080Z",
      "updatedAt": "2025-04-24T02:03:00.080Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "a9820071-7024-41fc-bf8c-097cdf571954",
      "number": 8,
      "content": "Băng qua các ngọn đồi.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.090Z",
      "updatedAt": "2025-04-24T02:03:00.090Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "5402b1d1-baed-4cd6-bbc0-09257a3a3730",
      "number": 9,
      "content": "Người yêu của tôi như một con linh dương,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.099Z",
      "updatedAt": "2025-04-24T02:03:00.099Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "8a076f45-6e80-469d-818b-6a6371609bd4",
      "number": 9,
      "content": "Như một chú nai tơ.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.109Z",
      "updatedAt": "2025-04-24T02:03:00.109Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "6b7152f8-1441-4352-b152-ac82c98eff52",
      "number": 9,
      "content": "Kìa, chàng đang đứng",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.119Z",
      "updatedAt": "2025-04-24T02:03:00.119Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "3892d736-e670-434c-ba03-e3347b52bfa7",
      "number": 9,
      "content": "Sau tường của chúng tôi,",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.129Z",
      "updatedAt": "2025-04-24T02:03:00.129Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "a06dc9c3-5359-4c9b-a009-d65ad5d97674",
      "number": 9,
      "content": "Chàng đang nhìn qua cửa sổ,",
      "order": 4,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.139Z",
      "updatedAt": "2025-04-24T02:03:00.139Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "73aa2ada-cf92-4e4b-9fcf-d2203c3bd92a",
      "number": 9,
      "content": "Từ sau các chấn song, chàng đang đưa mắt nhìn tôi.",
      "order": 5,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.149Z",
      "updatedAt": "2025-04-24T02:03:00.149Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "cfe43cc1-8c71-4d60-962b-34ed590a74f8",
      "number": 10,
      "content": "Người yêu của tôi cất tiếng nói, chàng bảo tôi,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.158Z",
      "updatedAt": "2025-04-24T02:03:00.158Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "901a1746-d128-4939-bd86-c4d55ed6f0ae",
      "number": 10,
      "content": "“Dậy đi em, người yêu của anh, người đẹp của anh,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.168Z",
      "updatedAt": "2025-04-24T02:03:00.168Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "72a0d1e0-228f-44a9-92e9-f252aadddcb0",
      "number": 10,
      "content": "Hãy ra đây với anh,",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.178Z",
      "updatedAt": "2025-04-24T02:03:00.178Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "552f3f08-4940-4366-9f46-050b93f47230",
      "number": 11,
      "content": "Vì kìa, mùa đông đã qua rồi,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.188Z",
      "updatedAt": "2025-04-24T02:03:00.188Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "12c0edc2-031f-42a7-a718-b5faa4bc9762",
      "number": 11,
      "content": "Mưa đã tạnh và đã dứt hẳn rồi,",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.197Z",
      "updatedAt": "2025-04-24T02:03:00.197Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "78c845e2-dc84-41df-af12-126c309b0b90",
      "number": 12,
      "content": "Hoa tươi đang nở rộ khắp nơi trên đất,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.207Z",
      "updatedAt": "2025-04-24T02:03:00.207Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "f075863b-62ae-4fb3-9321-d03f28d9a4c4",
      "number": 12,
      "content": "Mùa ca hát đã đến,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.217Z",
      "updatedAt": "2025-04-24T02:03:00.217Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "fe3e4b4d-e4be-4051-8360-43dab10d41dc",
      "number": 12,
      "content": "Tiếng chim gáy đã trỗi lên",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.227Z",
      "updatedAt": "2025-04-24T02:03:00.227Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "02d48d80-0443-47d4-b3b2-723e3ea1bb55",
      "number": 12,
      "content": "Trong khắp xứ chúng ta,",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.237Z",
      "updatedAt": "2025-04-24T02:03:00.237Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "1e9f3f1c-2ca9-46e8-b60e-c82826bdc792",
      "number": 13,
      "content": "Cây vả đang ra trái non,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.246Z",
      "updatedAt": "2025-04-24T02:03:00.246Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "a10ef9b0-d4e9-4d98-9fc3-6ea72fefed92",
      "number": 13,
      "content": "Hoa nho đang tỏa mùi thơm ngát.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.256Z",
      "updatedAt": "2025-04-24T02:03:00.256Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "3e3bca0f-68a9-4f28-8a65-729a1be412fe",
      "number": 13,
      "content": "Dậy đi em, người yêu của anh, người đẹp của anh,",
      "order": 2,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.265Z",
      "updatedAt": "2025-04-24T02:03:00.265Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "22ea07f3-1a11-42d0-b277-75781dda03a6",
      "number": 13,
      "content": "Hãy ra đây với anh.”",
      "order": 3,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.275Z",
      "updatedAt": "2025-04-24T02:03:00.275Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "bb8bde54-d64a-480d-9fec-2768fd64454b",
      "number": 14,
      "content": "Hỡi bồ câu của anh trong hốc đá,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.285Z",
      "updatedAt": "2025-04-24T02:03:00.285Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "af5cbc23-72e5-4b77-b596-6ca707415e68",
      "number": 14,
      "content": "Trong hốc nhỏ giữa lưng chừng vách núi,",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.305Z",
      "updatedAt": "2025-04-24T02:03:00.305Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "98a709ed-6c19-4a66-ae53-4cf799826404",
      "number": 14,
      "content": "Hãy cho anh trông thấy mặt em,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.314Z",
      "updatedAt": "2025-04-24T02:03:00.314Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "f4dd6363-cc34-4c6b-ac2c-3fdd7c37e3be",
      "number": 14,
      "content": "Xin cho anh nghe tiếng nói của em.",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.324Z",
      "updatedAt": "2025-04-24T02:03:00.324Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "5444534c-1488-491e-bbef-9fce2f34b4e2",
      "number": 14,
      "content": "Vì tiếng em ngọt ngào,",
      "order": 4,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.334Z",
      "updatedAt": "2025-04-24T02:03:00.334Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "91fb0c93-4f77-4e98-a7f5-1d351142bcaa",
      "number": 14,
      "content": "Và mặt em xinh đẹp đáng yêu.",
      "order": 5,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.343Z",
      "updatedAt": "2025-04-24T02:03:00.343Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "3c0c3ad3-f273-4386-a0a6-420ace4f542d",
      "number": 15,
      "content": "Hãy bắt giùm chúng tôi mấy con chồn,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.353Z",
      "updatedAt": "2025-04-24T02:03:00.353Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "0445044b-8638-4d58-8894-13db36f4d5c7",
      "number": 15,
      "content": "Mấy con chồn nhỏ phá hoại vườn nho.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.363Z",
      "updatedAt": "2025-04-24T02:03:00.363Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "ec4cfc29-8d53-40ef-9475-a32675b4f88b",
      "number": 15,
      "content": "Vì vườn nho của chúng tôi đang trổ hoa.",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.372Z",
      "updatedAt": "2025-04-24T02:03:00.372Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "62e329e9-3a6b-420e-a407-dc0c29d8a683",
      "number": 16,
      "content": "Người yêu của tôi là của tôi, và tôi là của chàng;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.382Z",
      "updatedAt": "2025-04-24T02:03:00.382Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "cc05ff9f-f457-498e-bbaa-600e23fa6b29",
      "number": 16,
      "content": "Chàng cho bầy chiên của chàng ăn cỏ giữa đám hoa huệ.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.400Z",
      "updatedAt": "2025-04-24T02:03:00.400Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "b8679863-bdd2-490d-81cb-7fdfbf33175c",
      "number": 17,
      "content": "Trước khi bình minh đến",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.410Z",
      "updatedAt": "2025-04-24T02:03:00.410Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "633c5e1d-2db7-4268-8392-eadeb7ee4971",
      "number": 17,
      "content": "Và bóng tối biến đi,",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.420Z",
      "updatedAt": "2025-04-24T02:03:00.420Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "2ac22444-115f-4ae0-bbf2-ca68d9ae376d",
      "number": 17,
      "content": "Hãy quay lại với em, anh yêu.",
      "order": 2,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.430Z",
      "updatedAt": "2025-04-24T02:03:00.430Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    },
    {
      "id": "e052d94c-b114-46a9-a200-f1fa97c00d87",
      "number": 17,
      "content": "Hãy như con linh dương hay như con nai tơ trên gành núi lởm chởm.",
      "order": 3,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:00.440Z",
      "updatedAt": "2025-04-24T02:03:00.440Z",
      "chapterId": "fedae4d9-e26e-4a65-a786-9913787406d0"
    }
  ],
