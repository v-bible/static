{
  "chapter": {
    "id": "88572d66-d64b-45c2-a21f-f78fd6573743",
    "number": 2,
    "ref": "https://www.bible.com/bible/19/SNG.2.BD2011",
    "createdAt": "2025-04-22T10:10:27.938Z",
    "updatedAt": "2025-04-22T10:10:27.938Z",
    "bookId": "0866c1f9-bea1-4051-9bb8-75be65257148"
  },
  "verses": [
    {
      "id": "18024bcc-6907-4038-9c03-c6855c52ec02",
      "number": 1,
      "content": "Tôi là đóa hoa hồng của Sa-rôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.793Z",
      "updatedAt": "2025-04-22T10:48:03.793Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "67933163-0ad8-41e9-b328-73a418f58c81",
      "number": 1,
      "content": "Là hoa huệ trong các thung lũng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.817Z",
      "updatedAt": "2025-04-22T10:48:03.817Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "f06308fd-fbb0-457d-9b8a-b45389f91cde",
      "number": 2,
      "content": "Như đóa hoa huệ ở giữa những gai góc thể nào,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.827Z",
      "updatedAt": "2025-04-22T10:48:03.827Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "16eac1d0-6c4f-4a24-9f4f-fea517c1142c",
      "number": 2,
      "content": "Người yêu của tôi ở giữa các thiếu nữ cũng như vậy.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.847Z",
      "updatedAt": "2025-04-22T10:48:03.847Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "5f0e3552-4cdf-49d0-b7d0-4863e3ca248a",
      "number": 3,
      "content": "Như cây táo ở giữa những cây rừng thể nào,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.857Z",
      "updatedAt": "2025-04-22T10:48:03.857Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "69c0756b-6de4-451a-a9cd-6f62a42498a4",
      "number": 3,
      "content": "Người yêu của tôi ở giữa các chàng trai cũng như vậy.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.880Z",
      "updatedAt": "2025-04-22T10:48:03.880Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "18dae2dd-6602-477a-adc3-73b8f1bc1820",
      "number": 3,
      "content": "Tôi yêu thích được ngồi dưới bóng mát của chàng,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.890Z",
      "updatedAt": "2025-04-22T10:48:03.890Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "1294523f-2869-4f23-824c-c60dd9e72c67",
      "number": 3,
      "content": "Trái của chàng làm ngọt lịm miệng tôi.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.900Z",
      "updatedAt": "2025-04-22T10:48:03.900Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "5fdf33f3-84e6-497a-bed0-a486b802a6cc",
      "number": 4,
      "content": "Chàng đưa tôi vào phòng tiệc;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.910Z",
      "updatedAt": "2025-04-22T10:48:03.910Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "6707bbc4-4c6a-4506-82d2-2490e0cfa2e9",
      "number": 4,
      "content": "Băng biểu ngữ của chàng trên tôi là tình yêu.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.920Z",
      "updatedAt": "2025-04-22T10:48:03.920Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "86e2203b-04cc-40c2-b3d1-8f53c2c33df0",
      "number": 5,
      "content": "Hãy cho tôi những trái nho khô để bổ sức tôi lại,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.932Z",
      "updatedAt": "2025-04-22T10:48:03.932Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "b79eb6a1-337d-4c83-aaec-c1af29608d50",
      "number": 5,
      "content": "Hãy cho tôi những trái táo tươi để tôi được bồi dưỡng lại,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.942Z",
      "updatedAt": "2025-04-22T10:48:03.942Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "6b60fec6-9240-4377-a3cb-6b8d08007b38",
      "number": 5,
      "content": "Bởi tôi đang kiệt lực vì yêu.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.952Z",
      "updatedAt": "2025-04-22T10:48:03.952Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "ea5783e7-017f-4371-b30c-8d4a6d6fa4c1",
      "number": 6,
      "content": "Tay trái chàng để dưới đầu tôi,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.964Z",
      "updatedAt": "2025-04-22T10:48:03.964Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "0760d6f5-3ec6-44cd-84c3-c07e2211cc96",
      "number": 6,
      "content": "Còn tay phải chàng ôm tôi.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.975Z",
      "updatedAt": "2025-04-22T10:48:03.975Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "c8a720a3-ec9c-4b54-9c08-d0819d798370",
      "number": 7,
      "content": "Hỡi các thiếu nữ ở Giê-ru-sa-lem, tôi nài xin các chị:",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:03.987Z",
      "updatedAt": "2025-04-22T10:48:03.987Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "814492f2-0623-482b-a99b-0a3cde3e03a8",
      "number": 7,
      "content": "Hãy quan sát những linh dương và những nai tơ trong đồng,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04Z",
      "updatedAt": "2025-04-22T10:48:04Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "6a3e9b83-5926-48c6-b125-4fe4e20448e5",
      "number": 7,
      "content": "Mà đừng khơi dậy hay đánh thức ái tình",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.013Z",
      "updatedAt": "2025-04-22T10:48:04.013Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "66a075e7-19c4-4c32-91ba-db22542a5a49",
      "number": 7,
      "content": "Cho đến khi nó muốn.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.023Z",
      "updatedAt": "2025-04-22T10:48:04.023Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "66d4aee6-c736-4f42-9d91-5c67b49d2d5e",
      "number": 8,
      "content": "Hãy lắng nghe! Tiếng của người yêu tôi đó.",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.032Z",
      "updatedAt": "2025-04-22T10:48:04.032Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "ed879773-816a-4862-b724-a6d3ac45c72b",
      "number": 8,
      "content": "Xem kìa! Chàng đang đến,",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.052Z",
      "updatedAt": "2025-04-22T10:48:04.052Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "1caa3eb0-2117-4e83-b401-6cf8d4f90341",
      "number": 8,
      "content": "Vượt qua các rặng núi,",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.062Z",
      "updatedAt": "2025-04-22T10:48:04.062Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "047329e0-2d30-48a5-9dd6-27b21a1afb7f",
      "number": 8,
      "content": "Băng qua các ngọn đồi.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.072Z",
      "updatedAt": "2025-04-22T10:48:04.072Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "d2ca581d-2994-4cbe-91a0-5b6c8993669f",
      "number": 9,
      "content": "Người yêu của tôi như một con linh dương,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.082Z",
      "updatedAt": "2025-04-22T10:48:04.082Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "9f0a1866-ea65-49dd-a1fc-39e85a1ac224",
      "number": 9,
      "content": "Như một chú nai tơ.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.092Z",
      "updatedAt": "2025-04-22T10:48:04.092Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "7b0afaaa-e4ff-4ac0-9372-4827e7a83db3",
      "number": 9,
      "content": "Kìa, chàng đang đứng",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.102Z",
      "updatedAt": "2025-04-22T10:48:04.102Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "6d603bc9-0400-4eed-942c-fbec1ec1d50c",
      "number": 9,
      "content": "Sau tường của chúng tôi,",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.114Z",
      "updatedAt": "2025-04-22T10:48:04.114Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "20f4d0e2-8f9b-4200-9d01-d99a4c6e102f",
      "number": 9,
      "content": "Chàng đang nhìn qua cửa sổ,",
      "order": 4,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.127Z",
      "updatedAt": "2025-04-22T10:48:04.127Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "f73150fb-b572-40dd-9e35-7bc2ce34a682",
      "number": 9,
      "content": "Từ sau các chấn song, chàng đang đưa mắt nhìn tôi.",
      "order": 5,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.137Z",
      "updatedAt": "2025-04-22T10:48:04.137Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "ed8ab1c7-f0bf-4a9e-93f4-531597f64a66",
      "number": 10,
      "content": "Người yêu của tôi cất tiếng nói, chàng bảo tôi,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.147Z",
      "updatedAt": "2025-04-22T10:48:04.147Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "fb9d8863-dc37-4ae7-9bf2-e45a758f4c65",
      "number": 10,
      "content": "“Dậy đi em, người yêu của anh, người đẹp của anh,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.157Z",
      "updatedAt": "2025-04-22T10:48:04.157Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "b123e781-5821-4cf0-9160-fe7c65b7d9ab",
      "number": 10,
      "content": "Hãy ra đây với anh,",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.166Z",
      "updatedAt": "2025-04-22T10:48:04.166Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "dab98ee5-9309-4845-b80a-f84e68db528a",
      "number": 11,
      "content": "Vì kìa, mùa đông đã qua rồi,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.176Z",
      "updatedAt": "2025-04-22T10:48:04.176Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "660f29dc-3837-4ce2-a43e-c62d80ad7b43",
      "number": 11,
      "content": "Mưa đã tạnh và đã dứt hẳn rồi,",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.186Z",
      "updatedAt": "2025-04-22T10:48:04.186Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "767dba63-6992-4743-9680-612751b0f1c6",
      "number": 12,
      "content": "Hoa tươi đang nở rộ khắp nơi trên đất,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.196Z",
      "updatedAt": "2025-04-22T10:48:04.196Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "6f48d682-cda8-4e5b-841d-a952ca6e7528",
      "number": 12,
      "content": "Mùa ca hát đã đến,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.206Z",
      "updatedAt": "2025-04-22T10:48:04.206Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "95377a2c-2666-4ef1-80f6-1e6598a4f03f",
      "number": 12,
      "content": "Tiếng chim gáy đã trỗi lên",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.216Z",
      "updatedAt": "2025-04-22T10:48:04.216Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "8bec1ba8-0d1c-4044-8f64-29434418c5e7",
      "number": 12,
      "content": "Trong khắp xứ chúng ta,",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.228Z",
      "updatedAt": "2025-04-22T10:48:04.228Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "2d29ad7c-da4a-49a8-bc26-c5e7276592b5",
      "number": 13,
      "content": "Cây vả đang ra trái non,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.239Z",
      "updatedAt": "2025-04-22T10:48:04.239Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "da3c2d49-af35-4898-ade3-4572429b0eb6",
      "number": 13,
      "content": "Hoa nho đang tỏa mùi thơm ngát.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.249Z",
      "updatedAt": "2025-04-22T10:48:04.249Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "98400922-0cf4-4a81-bb33-bfcba5f384c8",
      "number": 13,
      "content": "Dậy đi em, người yêu của anh, người đẹp của anh,",
      "order": 2,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.259Z",
      "updatedAt": "2025-04-22T10:48:04.259Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "89365bb1-9f1f-480f-95bb-27115742403b",
      "number": 13,
      "content": "Hãy ra đây với anh.”",
      "order": 3,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.269Z",
      "updatedAt": "2025-04-22T10:48:04.269Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "e7d2639c-75c4-4ab3-b2b8-2b41d98e2540",
      "number": 14,
      "content": "Hỡi bồ câu của anh trong hốc đá,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.279Z",
      "updatedAt": "2025-04-22T10:48:04.279Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "57b1391c-befe-4c14-8569-7d69aa06fea4",
      "number": 14,
      "content": "Trong hốc nhỏ giữa lưng chừng vách núi,",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.298Z",
      "updatedAt": "2025-04-22T10:48:04.298Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "c0958120-b451-44c0-950f-6af79fa61061",
      "number": 14,
      "content": "Hãy cho anh trông thấy mặt em,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.308Z",
      "updatedAt": "2025-04-22T10:48:04.308Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "3013341a-f5bd-4993-bdb8-9e8c28a29ac2",
      "number": 14,
      "content": "Xin cho anh nghe tiếng nói của em.",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.318Z",
      "updatedAt": "2025-04-22T10:48:04.318Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "05c18e00-784d-4c82-a26f-9a648fe86add",
      "number": 14,
      "content": "Vì tiếng em ngọt ngào,",
      "order": 4,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.327Z",
      "updatedAt": "2025-04-22T10:48:04.327Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "72e1cb29-b44a-488f-b4a4-b1fdebd9d746",
      "number": 14,
      "content": "Và mặt em xinh đẹp đáng yêu.",
      "order": 5,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.337Z",
      "updatedAt": "2025-04-22T10:48:04.337Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "581f7bad-182c-4c46-9e9a-cb7c7f00991d",
      "number": 15,
      "content": "Hãy bắt giùm chúng tôi mấy con chồn,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.347Z",
      "updatedAt": "2025-04-22T10:48:04.347Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "e371d2bd-76d4-4050-a459-8ede7b5c31e3",
      "number": 15,
      "content": "Mấy con chồn nhỏ phá hoại vườn nho.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.357Z",
      "updatedAt": "2025-04-22T10:48:04.357Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "42bd5522-1842-4e99-86bd-d03e03600ea2",
      "number": 15,
      "content": "Vì vườn nho của chúng tôi đang trổ hoa.",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.367Z",
      "updatedAt": "2025-04-22T10:48:04.367Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "48218f55-df5a-43fb-8cbd-f9feb047fa62",
      "number": 16,
      "content": "Người yêu của tôi là của tôi, và tôi là của chàng;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.377Z",
      "updatedAt": "2025-04-22T10:48:04.377Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "cb7287aa-82e7-4ef8-a6ac-5c1a3e4c731e",
      "number": 16,
      "content": "Chàng cho bầy chiên của chàng ăn cỏ giữa đám hoa huệ.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.396Z",
      "updatedAt": "2025-04-22T10:48:04.396Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "319eccf2-808b-442f-8e58-7c13a8a4ac20",
      "number": 17,
      "content": "Trước khi bình minh đến",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.406Z",
      "updatedAt": "2025-04-22T10:48:04.406Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "eff51671-0a3f-41ed-8f3b-b63952d72be2",
      "number": 17,
      "content": "Và bóng tối biến đi,",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.416Z",
      "updatedAt": "2025-04-22T10:48:04.416Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "0c7d082c-f8af-4769-86f7-a482e6bd0296",
      "number": 17,
      "content": "Hãy quay lại với em, anh yêu.",
      "order": 2,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.425Z",
      "updatedAt": "2025-04-22T10:48:04.425Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    },
    {
      "id": "ab055393-4743-4726-93f8-dba6bcbaaf68",
      "number": 17,
      "content": "Hãy như con linh dương hay như con nai tơ trên gành núi lởm chởm.",
      "order": 3,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:48:04.435Z",
      "updatedAt": "2025-04-22T10:48:04.435Z",
      "chapterId": "88572d66-d64b-45c2-a21f-f78fd6573743"
    }
  ],
