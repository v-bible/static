{
  "chapter": {
    "id": "bbc11326-1631-4467-a592-531b36b3c198",
    "number": 8,
    "ref": "https://www.bible.com/bible/19/SNG.8.BD2011",
    "createdAt": "2025-04-24T01:29:08.415Z",
    "updatedAt": "2025-04-24T01:29:08.415Z",
    "bookId": "1d4ae9a6-83b3-4337-b059-52a71c117497"
  },
  "verses": [
    {
      "id": "47c73ea5-ace2-4036-8c18-97b209156de6",
      "number": 1,
      "content": "Ước gì anh là anh ruột của em,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.411Z",
      "updatedAt": "2025-04-24T02:03:14.411Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "310ef253-31ff-4e00-8bfb-d5b05c632271",
      "number": 1,
      "content": "Người cùng được nuôi nơi bầu sữa của mẹ em,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.428Z",
      "updatedAt": "2025-04-24T02:03:14.428Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "70017319-e6c1-48a4-8a30-aa72fc6c3a36",
      "number": 1,
      "content": "Để khi gặp anh ngoài đường, em có thể hôn anh,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.438Z",
      "updatedAt": "2025-04-24T02:03:14.438Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "b39d8e47-6807-4f03-98e8-0a52cd97166a",
      "number": 1,
      "content": "Mà không ai có quyền chê cười em.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.450Z",
      "updatedAt": "2025-04-24T02:03:14.450Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "cdc18ce0-44e9-4bf0-b6e1-a9b7ded3d33d",
      "number": 2,
      "content": "Em sẽ dẫn anh về và đưa anh",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.460Z",
      "updatedAt": "2025-04-24T02:03:14.460Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "2e8a1fe5-0364-43a8-a724-6332c3bdb876",
      "number": 2,
      "content": "Vào nhà mẹ em,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.470Z",
      "updatedAt": "2025-04-24T02:03:14.470Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "b6d19747-75f4-4de7-b890-b716cc2877b0",
      "number": 2,
      "content": "Là người đã dạy dỗ em.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.479Z",
      "updatedAt": "2025-04-24T02:03:14.479Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "830c7bf3-d4ee-4409-a46b-5c2788ebe3e7",
      "number": 2,
      "content": "Em sẽ mời anh uống rượu thơm hảo hạng,",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.489Z",
      "updatedAt": "2025-04-24T02:03:14.489Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "3be75007-01aa-406a-8137-81f3e7a1886c",
      "number": 2,
      "content": "Và uống nước thạch lựu của em.",
      "order": 4,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.499Z",
      "updatedAt": "2025-04-24T02:03:14.499Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "83aadb1d-aec0-43fc-b38d-0dd2361e6093",
      "number": 3,
      "content": "Tay trái chàng để dưới đầu tôi,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.509Z",
      "updatedAt": "2025-04-24T02:03:14.509Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "b16b522d-36bc-45b3-84ff-37ea60b4516e",
      "number": 3,
      "content": "Còn tay phải chàng ôm tôi.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.519Z",
      "updatedAt": "2025-04-24T02:03:14.519Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "6aaf7da5-2c35-4842-a2de-de40f6339040",
      "number": 4,
      "content": "Hỡi các thiếu nữ ở Giê-ru-sa-lem, tôi nài xin các chị:",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.529Z",
      "updatedAt": "2025-04-24T02:03:14.529Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "86256fd8-9ab3-4976-8458-9f9f009a97da",
      "number": 4,
      "content": "Xin đừng khơi dậy hay đánh thức ái tình",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.539Z",
      "updatedAt": "2025-04-24T02:03:14.539Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "21b6908e-1b8e-4222-aa88-33694288f284",
      "number": 4,
      "content": "Cho đến khi nó muốn.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.548Z",
      "updatedAt": "2025-04-24T02:03:14.548Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "61a47763-1a3e-42c9-bc94-6a28eeafddad",
      "number": 5,
      "content": "Ai từ sa mạc đi lên,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.558Z",
      "updatedAt": "2025-04-24T02:03:14.558Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "546f64b3-bf6e-4f71-9275-1917204dcd8c",
      "number": 5,
      "content": "Nép mình vào ngực người yêu?",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.587Z",
      "updatedAt": "2025-04-24T02:03:14.587Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "825914ca-2b24-4edb-817e-7ee86bf1c980",
      "number": 5,
      "content": "Dưới tàng cây táo anh đã đánh thức em dậy;",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.597Z",
      "updatedAt": "2025-04-24T02:03:14.597Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "bc39d97a-31bd-4e5d-be43-1917828d2025",
      "number": 5,
      "content": "Đây là nơi mẹ em đã mang thai em,",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.620Z",
      "updatedAt": "2025-04-24T02:03:14.620Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "fbcbc92b-4f01-4dfd-8985-dc75c8a076ec",
      "number": 5,
      "content": "Chính nơi đây mẹ em đã chuyển bụng và sinh em.",
      "order": 4,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.631Z",
      "updatedAt": "2025-04-24T02:03:14.631Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "e9d3a79a-b04c-41c8-99fc-eb2b0efd1841",
      "number": 6,
      "content": "Xin đặt em như một dấu ấn trên trái tim anh,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.641Z",
      "updatedAt": "2025-04-24T02:03:14.641Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "5c604a88-9a2d-4359-b662-dd651bded275",
      "number": 6,
      "content": "Như một dấu ấn trên cánh tay anh;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.659Z",
      "updatedAt": "2025-04-24T02:03:14.659Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "31801adc-3871-46b5-9ada-86fc25fdb172",
      "number": 6,
      "content": "Vì tình yêu mạnh như tử thần,",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.669Z",
      "updatedAt": "2025-04-24T02:03:14.669Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "52bd9b78-8600-44a1-9c69-42d40ef4dfe7",
      "number": 6,
      "content": "Cơn ghen tương hung dữ tựa âm phủ;",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.678Z",
      "updatedAt": "2025-04-24T02:03:14.678Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "f8d6c3ee-2b65-43f6-82c8-ae3e84062ecd",
      "number": 6,
      "content": "Ngọn lửa của nó tựa như ngọn lửa hừng,",
      "order": 4,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.688Z",
      "updatedAt": "2025-04-24T02:03:14.688Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "2576ea79-0d24-4f7e-924e-bc592a3e5424",
      "number": 6,
      "content": "Tựa ngọn lửa cháy phừng phừng cuồn cuộn.",
      "order": 5,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.698Z",
      "updatedAt": "2025-04-24T02:03:14.698Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "e40caf28-e37d-43c5-9141-de252e7c5453",
      "number": 7,
      "content": "Dù nước nhiều bao nhiêu cũng không dập tắt được tình yêu,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.708Z",
      "updatedAt": "2025-04-24T02:03:14.708Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "24ffdaee-de41-4b24-a8f1-da0e72e0d7a7",
      "number": 7,
      "content": "Dù các dòng sông tràn vào cũng không thể cuốn nó trôi đi được.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.718Z",
      "updatedAt": "2025-04-24T02:03:14.718Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "ec8a15ee-3c8c-4781-9d45-adc9980427ef",
      "number": 7,
      "content": "Nếu ai lấy tất cả tiền của ở nhà mình",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.728Z",
      "updatedAt": "2025-04-24T02:03:14.728Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "479fd9ee-d368-429b-8f17-a29ace288216",
      "number": 7,
      "content": "Mong mua được tình yêu,",
      "order": 3,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.737Z",
      "updatedAt": "2025-04-24T02:03:14.737Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "225ed345-b50c-40aa-9c91-b412b9701e51",
      "number": 7,
      "content": "Người ấy sẽ chuốc lấy cảnh bị người đời khinh chê miệt thị.",
      "order": 4,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.747Z",
      "updatedAt": "2025-04-24T02:03:14.747Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "20fb8e84-ae78-4344-a276-7b60cbeb233c",
      "number": 8,
      "content": "Chúng tôi có một người em gái nhỏ,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.757Z",
      "updatedAt": "2025-04-24T02:03:14.757Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "e99c6f2d-e4ad-4f47-a3af-b96c2f87ecdc",
      "number": 8,
      "content": "Đôi gò bồng đảo nàng chưa nẩy nở dậy thì;",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.776Z",
      "updatedAt": "2025-04-24T02:03:14.776Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "6faaf511-53d2-44ec-8ca5-8b8c995d144f",
      "number": 8,
      "content": "Chúng tôi phải làm gì cho em gái của chúng tôi,",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.786Z",
      "updatedAt": "2025-04-24T02:03:14.786Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "26250e8a-4d49-488f-bc23-ab12ae7fed9b",
      "number": 8,
      "content": "Trong ngày nàng được người ta dạm hỏi?",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.796Z",
      "updatedAt": "2025-04-24T02:03:14.796Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "ab23a9b4-9fab-42b0-9e5e-a37be28d7e75",
      "number": 9,
      "content": "Nếu nàng là bức tường thành,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.805Z",
      "updatedAt": "2025-04-24T02:03:14.805Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "69a4527c-9f9a-41df-8849-4520facc8ae8",
      "number": 9,
      "content": "Chúng tôi sẽ xây trên tường một tháp canh bằng bạc.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.815Z",
      "updatedAt": "2025-04-24T02:03:14.815Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "605ec3aa-662c-4e64-899d-d5225f50108c",
      "number": 9,
      "content": "Nếu nàng là cửa ra vào,",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.825Z",
      "updatedAt": "2025-04-24T02:03:14.825Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "66277f3b-f960-4acb-8d1e-da2af37ec481",
      "number": 9,
      "content": "Chúng tôi sẽ bọc cửa ấy bằng những tấm ván gỗ bá hương.",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.835Z",
      "updatedAt": "2025-04-24T02:03:14.835Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "560f6540-c4bb-4c12-ba45-f6b3fff1c948",
      "number": 10,
      "content": "Tôi là bức tường thành,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.845Z",
      "updatedAt": "2025-04-24T02:03:14.845Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "59a4ee64-0822-46ce-b9e7-e8475d2412b3",
      "number": 10,
      "content": "Đôi gò bồng đảo tôi là những tháp canh;",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.864Z",
      "updatedAt": "2025-04-24T02:03:14.864Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "c53e4f88-1dc5-4de2-b590-dad3e3b3ab2d",
      "number": 10,
      "content": "Trước mắt chàng tôi được xem như thế,",
      "order": 2,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.874Z",
      "updatedAt": "2025-04-24T02:03:14.874Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "ad2abc3d-d3ba-4e28-b36b-7ea353fa7332",
      "number": 10,
      "content": "Tôi là người làm cho chàng được an vui thỏa thích.",
      "order": 3,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.884Z",
      "updatedAt": "2025-04-24T02:03:14.884Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "7457a9c6-a31e-4ee3-81c1-6c5e0eaddda1",
      "number": 11,
      "content": "Sa-lô-môn có một vườn nho ở Ba-anh Ha-môn;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.894Z",
      "updatedAt": "2025-04-24T02:03:14.894Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "3182d1bd-54bf-4648-bf87-3e35095b7798",
      "number": 11,
      "content": "Người để vườn nho đó cho các tá điền chăm sóc.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.903Z",
      "updatedAt": "2025-04-24T02:03:14.903Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "72ebe318-7a3e-4b92-a6ff-c16dcbefbf08",
      "number": 11,
      "content": "Mỗi tá điền phải nộp một ngàn miếng bạc tiền hoa lợi.",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.913Z",
      "updatedAt": "2025-04-24T02:03:14.913Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "ce13dc33-3c3b-4f21-ae5e-46e4a65a4f33",
      "number": 12,
      "content": "Còn vườn nho của tôi là của tôi, do tôi quyết định;",
      "order": 0,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.923Z",
      "updatedAt": "2025-04-24T02:03:14.923Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "337e1f23-47e4-4290-b000-90e37fc847f0",
      "number": 12,
      "content": "Sa-lô-môn hỡi, em xin trả cho anh một ngàn miếng bạc,",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.932Z",
      "updatedAt": "2025-04-24T02:03:14.932Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "b981a2df-1411-4bcf-a501-853736f1db19",
      "number": 12,
      "content": "Còn hai trăm miếng kia, em trả cho những người chăm sóc các hoa quả của ngôi vườn.",
      "order": 2,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.942Z",
      "updatedAt": "2025-04-24T02:03:14.942Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "efff1e2b-9a52-4c55-b7a4-165aa24d4cc0",
      "number": 13,
      "content": "Hỡi người đang ở trong vườn,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.952Z",
      "updatedAt": "2025-04-24T02:03:14.952Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "0e5f71d2-36d5-45a2-a541-791f02948108",
      "number": 13,
      "content": "Các bạn anh đang lắng nghe tiếng nói của em đó.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.971Z",
      "updatedAt": "2025-04-24T02:03:14.971Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "7fce28e4-9d0d-43f0-88d9-3703d5f20cc9",
      "number": 13,
      "content": "Hãy lên tiếng cho anh nghe đi!",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.981Z",
      "updatedAt": "2025-04-24T02:03:14.981Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "2c26bc93-c849-4661-a126-e4c048dd9faf",
      "number": 14,
      "content": "Hãy nhanh lên, người yêu của em.",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:14.991Z",
      "updatedAt": "2025-04-24T02:03:14.991Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "a858adc5-8527-435b-9592-d56afb3a0406",
      "number": 14,
      "content": "Hãy như con linh dương,",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:15.010Z",
      "updatedAt": "2025-04-24T02:03:15.010Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "20dd806f-7d20-40ee-9118-1c010f93c82e",
      "number": 14,
      "content": "Hay như con nai tơ,",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:15.023Z",
      "updatedAt": "2025-04-24T02:03:15.023Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    },
    {
      "id": "44c70ec9-818e-4d27-9d3a-b30f174d2da3",
      "number": 14,
      "content": "Trên những đồi núi tràn đầy hương thơm.",
      "order": 3,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:03:15.033Z",
      "updatedAt": "2025-04-24T02:03:15.033Z",
      "chapterId": "bbc11326-1631-4467-a592-531b36b3c198"
    }
  ],
