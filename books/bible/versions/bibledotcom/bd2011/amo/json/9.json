{
  "chapter": {
    "id": "7d75cb1b-8535-4afb-b833-d02ced741fcd",
    "number": 9,
    "ref": "https://www.bible.com/bible/19/AMO.9.BD2011",
    "createdAt": "2025-04-22T10:10:30.583Z",
    "updatedAt": "2025-04-22T10:10:30.583Z",
    "bookId": "91e07856-e631-4f96-a955-91c2d3416e2f"
  },
  "verses": [
    {
      "id": "9ee6026d-2cbe-4937-86fc-a42d8897d418",
      "number": 1,
      "content": "Tôi thấy Chúa đứng bên cạnh bàn thờ, và Ngài phán,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T11:00:09.629Z",
      "updatedAt": "2025-04-22T11:00:09.629Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "cc15b305-53cd-4bb3-ade9-ed83dc5cfdf1",
      "number": 1,
      "content": "“Hãy đánh vào các đầu trụ cột để các ngạch cửa lung lay;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.660Z",
      "updatedAt": "2025-04-22T11:00:09.660Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "e8626047-561f-4962-9584-027b0ce85937",
      "number": 1,
      "content": "Hãy đập phá tan tành để chúng rơi xuống đầu mọi người bên dưới;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.672Z",
      "updatedAt": "2025-04-22T11:00:09.672Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "75c8ee98-2435-4164-85db-c098f3a99aa5",
      "number": 1,
      "content": "Những kẻ còn sót lại sẽ bị giết bằng gươm;",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.685Z",
      "updatedAt": "2025-04-22T11:00:09.685Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "9296974a-81c8-494e-81aa-555b9d928297",
      "number": 1,
      "content": "Không một người nào sẽ chạy thoát;",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.697Z",
      "updatedAt": "2025-04-22T11:00:09.697Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "ad7889c4-ae35-46a1-9409-6f75e09aa639",
      "number": 1,
      "content": "Không một ai sẽ trốn khỏi.",
      "order": 5,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.709Z",
      "updatedAt": "2025-04-22T11:00:09.709Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "235a3363-0c8e-44d5-99be-d72fe4b8505c",
      "number": 2,
      "content": "Dù cho chúng đào hầm sâu đến âm phủ để trốn,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.722Z",
      "updatedAt": "2025-04-22T11:00:09.722Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "5fd9bd9c-6bad-4267-833d-253c1eb7ca59",
      "number": 2,
      "content": "Ta sẽ đưa tay xuống đó để lôi cổ chúng lên.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.734Z",
      "updatedAt": "2025-04-22T11:00:09.734Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "944d2ebf-e8fc-470a-b0ff-d42fd25e90e5",
      "number": 2,
      "content": "Dù cho chúng trèo lên tận trời cao để núp,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.753Z",
      "updatedAt": "2025-04-22T11:00:09.753Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "651e8fbd-3ebe-4a35-8b34-b85ab8053a8c",
      "number": 2,
      "content": "Ta sẽ lôi cổ chúng từ trên đó đem xuống.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.765Z",
      "updatedAt": "2025-04-22T11:00:09.765Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "163f95ac-84ad-47ff-81bf-36c8619f1b7d",
      "number": 3,
      "content": "Dù cho chúng lên đỉnh Núi Cạt-mên để tránh,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.777Z",
      "updatedAt": "2025-04-22T11:00:09.777Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "7ef0a3ca-fe98-4f99-ae3c-8091ec27ada8",
      "number": 3,
      "content": "Tại đó Ta sẽ tìm và mang chúng trở về.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.789Z",
      "updatedAt": "2025-04-22T11:00:09.789Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "cf0ed1aa-6506-4ba5-8e94-d50285775731",
      "number": 3,
      "content": "Dù cho chúng chui vào tận đáy biển để lánh mặt Ta,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.805Z",
      "updatedAt": "2025-04-22T11:00:09.805Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "8985066c-62be-4843-92bf-d82ceeab32f7",
      "number": 3,
      "content": "Tại đó Ta sẽ truyền cho rắn biển cắn chúng chết.",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.817Z",
      "updatedAt": "2025-04-22T11:00:09.817Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "92952b14-680a-4e4f-9518-9c3c40820fcc",
      "number": 4,
      "content": "Dù cho chúng bị quân thù bắt đem lưu đày biệt xứ,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.830Z",
      "updatedAt": "2025-04-22T11:00:09.830Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "4692238f-a240-431f-9402-0499726492b0",
      "number": 4,
      "content": "Tại những nơi ấy, Ta sẽ truyền cho gươm đao theo giết chúng.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.842Z",
      "updatedAt": "2025-04-22T11:00:09.842Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "b1b9b867-50f2-4abd-a8dd-888ae7fa2ff1",
      "number": 4,
      "content": "Ta sẽ để mắt Ta theo dõi chúng",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.854Z",
      "updatedAt": "2025-04-22T11:00:09.854Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "60021548-eed3-4a49-8535-98b8be7598be",
      "number": 4,
      "content": "Hầu giáng họa chứ không ban phước.”",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.866Z",
      "updatedAt": "2025-04-22T11:00:09.866Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "a9234568-eabe-4c61-a5f6-6910cd1dcde0",
      "number": 5,
      "content": "Vì Chúa, **CHÚA** các đạo quân,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.879Z",
      "updatedAt": "2025-04-22T11:00:09.879Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "0ff3e0da-b13b-4f33-9107-9b98155b0b07",
      "number": 5,
      "content": "Đụng vào đất, đất liền tan chảy,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.891Z",
      "updatedAt": "2025-04-22T11:00:09.891Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "7594dbbe-511e-4fbe-8117-9a80131e515d",
      "number": 5,
      "content": "Và mọi người sống trên đất đều than khóc;",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.903Z",
      "updatedAt": "2025-04-22T11:00:09.903Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "a0f52b27-3698-49e2-bf5f-07979aeae6ac",
      "number": 5,
      "content": "Cả xứ sẽ trồi lên như nước Sông Nin dâng lên,",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.915Z",
      "updatedAt": "2025-04-22T11:00:09.915Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "a613946c-a58f-40ca-b377-9bfad79b26e2",
      "number": 5,
      "content": "Rồi trụt xuống như nước Sông Nin ở Ai-cập rút xuống.",
      "order": 4,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.929Z",
      "updatedAt": "2025-04-22T11:00:09.929Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "68d637b0-fd07-4e3e-b9f2-3a369dfffb2f",
      "number": 6,
      "content": "Đấng xây các cung điện của Ngài trên trời cao vời vợi,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.940Z",
      "updatedAt": "2025-04-22T11:00:09.940Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "b3d99d4d-f11c-4ded-9ec5-14486cc3007c",
      "number": 6,
      "content": "Và đặt nền cho vòm của nó bên trên trái đất,",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.953Z",
      "updatedAt": "2025-04-22T11:00:09.953Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "7237655d-76e6-472b-a1dd-9a373d81fd26",
      "number": 6,
      "content": "Ngài gọi nước biển",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.966Z",
      "updatedAt": "2025-04-22T11:00:09.966Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "f8a950fd-d00b-47bf-b0c9-bb7ba1a4d28a",
      "number": 6,
      "content": "Và đổ chúng ra trên mặt đất.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.981Z",
      "updatedAt": "2025-04-22T11:00:09.981Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "fec48f8b-c9d7-4f7a-9bfd-15f75ab71b05",
      "number": 6,
      "content": "Danh Ngài là **CHÚA**.",
      "order": 4,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:09.992Z",
      "updatedAt": "2025-04-22T11:00:09.992Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "8450717e-3fa4-472b-89ab-ecabb32ac8d2",
      "number": 7,
      "content": "“Hỡi dân I-sơ-ra-ên, đối với Ta,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.005Z",
      "updatedAt": "2025-04-22T11:00:10.005Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "83659db3-f20c-47d4-bc5c-a8ad609bee42",
      "number": 7,
      "content": "Các ngươi chẳng phải như dân Ê-thi-ô-pi sao?” **CHÚA** phán.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.018Z",
      "updatedAt": "2025-04-22T11:00:10.018Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "87f6aed6-99ab-40e5-b65b-e2333527f8c5",
      "number": 7,
      "content": "“Chẳng phải Ta đã đem dân I-sơ-ra-ên ra khỏi Ai-cập,",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.027Z",
      "updatedAt": "2025-04-22T11:00:10.027Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "e3431ccd-50b9-4962-bc65-01e811ad0607",
      "number": 7,
      "content": "Dân Phi-li-tin ra khỏi Đảo Cáp-tô, và dân A-ram ra khỏi Ki-rơ sao?",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.037Z",
      "updatedAt": "2025-04-22T11:00:10.037Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "1b35a90e-d79e-4163-a36f-90e7f5b17582",
      "number": 8,
      "content": "Này, mắt của **CHÚA** Hằng Hữu đang nhìn vào vương quốc tội lỗi.",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.067Z",
      "updatedAt": "2025-04-22T11:00:10.067Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "bd4bf937-7ff7-4826-be43-4861aec511a1",
      "number": 8,
      "content": "Ta sẽ tiêu diệt nó khỏi mặt đất,",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.078Z",
      "updatedAt": "2025-04-22T11:00:10.078Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "18ca0c3a-e2b6-4d40-ba91-3ccc615c0c85",
      "number": 8,
      "content": "Nhưng Ta sẽ không tiêu diệt hoàn toàn nhà Gia-cốp,”",
      "order": 2,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.089Z",
      "updatedAt": "2025-04-22T11:00:10.089Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "0ae22d86-b745-4d2b-9d93-adcb7609e1bb",
      "number": 8,
      "content": "**CHÚA** phán.",
      "order": 3,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.100Z",
      "updatedAt": "2025-04-22T11:00:10.100Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "09e2311c-8e36-44f8-b252-43ef07e691ac",
      "number": 9,
      "content": "“Vì này, Ta sẽ truyền lịnh,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.113Z",
      "updatedAt": "2025-04-22T11:00:10.113Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "de48a158-a319-4116-a828-24d142e91519",
      "number": 9,
      "content": "Ta sẽ sàng sảy nhà I-sơ-ra-ên giữa các dân",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.126Z",
      "updatedAt": "2025-04-22T11:00:10.126Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "04ea27ef-0ac7-4a99-bc64-e00574c7bbff",
      "number": 9,
      "content": "Như người ta sàng gạo trong cái sàng,",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.138Z",
      "updatedAt": "2025-04-22T11:00:10.138Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "fa611710-37e7-430b-a1b3-7d7cd4d6923b",
      "number": 9,
      "content": "Và không hạt nào bị rơi xuống đất.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.151Z",
      "updatedAt": "2025-04-22T11:00:10.151Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "84571382-8456-4749-8173-a6fc2d3e47fa",
      "number": 10,
      "content": "Tất cả những kẻ tội lỗi trong dân Ta sẽ bị giết bằng gươm,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.164Z",
      "updatedAt": "2025-04-22T11:00:10.164Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "86aeec4a-7ffe-4a02-b560-9ff723ed3df8",
      "number": 10,
      "content": "Những kẻ đã nói rằng, ‘Tai họa sẽ không bao giờ đuổi kịp chúng ta và chúng ta sẽ không phải đối diện với tai họa.’”",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.177Z",
      "updatedAt": "2025-04-22T11:00:10.177Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "adf33200-a911-4f4f-9595-08d5cfc3eac0",
      "number": 11,
      "content": "“Trong ngày ấy Ta sẽ dựng lại trại của Đa-vít,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.190Z",
      "updatedAt": "2025-04-22T11:00:10.190Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "d6d467b6-11d9-49bb-9325-07711f6271bf",
      "number": 11,
      "content": "Trại đã bị sụp đổ.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.215Z",
      "updatedAt": "2025-04-22T11:00:10.215Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "bb03583d-4278-46d9-99d6-08747baeabeb",
      "number": 11,
      "content": "Ta sẽ xây lại những chỗ đổ vỡ ở tường thành,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.228Z",
      "updatedAt": "2025-04-22T11:00:10.228Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "e6bcbe3b-8cb4-44b6-9394-06491e77fe92",
      "number": 11,
      "content": "Ta sẽ dựng lại những gì đã đổ nát,",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.241Z",
      "updatedAt": "2025-04-22T11:00:10.241Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "e246d1e8-800f-43ce-a15d-514841ea5dbf",
      "number": 11,
      "content": "Ta sẽ xây nó lại giống như ngày trước.",
      "order": 4,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.257Z",
      "updatedAt": "2025-04-22T11:00:10.257Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "12c9ab95-ff6c-4a9a-8899-7d7e28e04838",
      "number": 12,
      "content": "Để nó chiếm hữu những gì còn lại của Ê-đôm",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.270Z",
      "updatedAt": "2025-04-22T11:00:10.270Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "51e26714-ebb4-4293-a9fb-2cd3866e5fe6",
      "number": 12,
      "content": "Và tất cả các dân được gọi bằng danh Ta,”",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.286Z",
      "updatedAt": "2025-04-22T11:00:10.286Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "3ba9a38b-1f59-4198-ab7c-75e92545701d",
      "number": 12,
      "content": "**CHÚA**, Đấng sẽ thực hiện những điều đó, phán.",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.302Z",
      "updatedAt": "2025-04-22T11:00:10.302Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "e9059699-3196-444b-b241-2d66ccf758c9",
      "number": 13,
      "content": "“Này, những ngày tới,” **CHÚA** phán,",
      "order": 0,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.315Z",
      "updatedAt": "2025-04-22T11:00:10.315Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "5fb25afb-5b04-49c8-9593-a35446329b2d",
      "number": 13,
      "content": "“Người cày ruộng sẽ nối gót thợ gặt lúa;",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.328Z",
      "updatedAt": "2025-04-22T11:00:10.328Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "030f5800-a843-4897-9793-38a377917500",
      "number": 13,
      "content": "Người đạp nho sẽ tiếp nối người gieo giống;",
      "order": 2,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.341Z",
      "updatedAt": "2025-04-22T11:00:10.341Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "9619d4c4-f973-43ed-88b3-cf1d37725e29",
      "number": 13,
      "content": "Các núi rừng sẽ nhỏ ra rượu ngọt;",
      "order": 3,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.354Z",
      "updatedAt": "2025-04-22T11:00:10.354Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "dcb15eee-ec13-476c-91d4-82e9443e9f6b",
      "number": 13,
      "content": "Mọi ngọn đồi sẽ chảy ra rượu ngon.",
      "order": 4,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.367Z",
      "updatedAt": "2025-04-22T11:00:10.367Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "59c5fbae-0796-4117-8c98-1fedda73c5de",
      "number": 14,
      "content": "Ta sẽ đem những kẻ bị lưu đày của dân I-sơ-ra-ên Ta trở về;",
      "order": 0,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.380Z",
      "updatedAt": "2025-04-22T11:00:10.380Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "2eac353e-0608-4217-8216-a38c5ba004ae",
      "number": 14,
      "content": "Chúng sẽ xây dựng lại những thành bị hoang phế, rồi cư ngụ trong các thành ấy;",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.393Z",
      "updatedAt": "2025-04-22T11:00:10.393Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "1ffbdc6b-f82a-4034-a8ad-0107725126a7",
      "number": 14,
      "content": "Chúng sẽ trồng các vườn nho và uống rượu do nho các vườn ấy sản xuất;",
      "order": 2,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.407Z",
      "updatedAt": "2025-04-22T11:00:10.407Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "26d2552c-961f-4bab-ba88-e44c3cb0ce43",
      "number": 14,
      "content": "Chúng sẽ canh tác ruộng vườn và ăn hoa quả do ruộng vườn của chúng sinh ra.",
      "order": 3,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.422Z",
      "updatedAt": "2025-04-22T11:00:10.422Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "f98df8c9-04ba-43ae-aebd-34d686fe53ab",
      "number": 15,
      "content": "Ta sẽ trồng chúng xuống trong đất của chúng,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.435Z",
      "updatedAt": "2025-04-22T11:00:10.435Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "ca8e9122-5864-4262-bd41-4cb1af64ac4d",
      "number": 15,
      "content": "Chúng sẽ không bao giờ bị nhổ lên",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.448Z",
      "updatedAt": "2025-04-22T11:00:10.448Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "510a18a4-eba6-41f8-b778-f2959d26b517",
      "number": 15,
      "content": "Khỏi đất Ta đã ban cho chúng nữa,”",
      "order": 2,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.461Z",
      "updatedAt": "2025-04-22T11:00:10.461Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    },
    {
      "id": "6154a4cc-901e-44c5-8691-aa291278444a",
      "number": 15,
      "content": "**CHÚA**, Đức Chúa Trời của ngươi phán vậy.",
      "order": 3,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T11:00:10.472Z",
      "updatedAt": "2025-04-22T11:00:10.472Z",
      "chapterId": "7d75cb1b-8535-4afb-b833-d02ced741fcd"
    }
  ],
