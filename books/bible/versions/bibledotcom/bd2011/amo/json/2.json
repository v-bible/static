{
  "chapter": {
    "id": "8493b1a1-4694-473b-9f0e-c6f3b522b682",
    "number": 2,
    "ref": "https://www.bible.com/bible/19/AMO.2.BD2011",
    "createdAt": "2025-04-22T10:10:30.489Z",
