  },
  "verses": [
    {
      "id": "1a469638-883c-4695-b525-8a6d4f0b8f4c",
      "number": 1,
      "content": "**CHÚA** phán thế này,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T10:59:42.899Z",
      "updatedAt": "2025-04-22T10:59:42.899Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "20f84876-2f05-4c5f-8db0-4491dbeff66c",
      "number": 4,
      "content": "**CHÚA** phán thế này,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T10:59:43.032Z",
      "updatedAt": "2025-04-22T10:59:43.032Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "f361e61e-0192-4831-a8a6-2070126131a5",
      "number": 6,
      "content": "**CHÚA** phán thế này,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2025-04-22T10:59:43.138Z",
      "updatedAt": "2025-04-22T10:59:43.138Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "e6562e67-8aa9-44b7-9800-237e3699d2ef",
      "number": 1,
      "content": "“Vì tội của Mô-áp nặng gấp ba gấp bốn lần,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:42.919Z",
      "updatedAt": "2025-04-22T10:59:42.919Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "7a23fec9-43e9-47b9-bb4a-30851449f6ec",
      "number": 1,
      "content": "Nên Ta sẽ chẳng quay mặt đi mà không phạt nó.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:42.929Z",
      "updatedAt": "2025-04-22T10:59:42.929Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "921aebc6-5a3a-433b-be95-b19e4a1b03ff",
      "number": 4,
      "content": "Nên Ta sẽ chẳng quay mặt đi mà không phạt nó.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.064Z",
      "updatedAt": "2025-04-22T10:59:43.064Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "0be912ec-1f8b-4e82-ac2e-1ac854dda27e",
      "number": 6,
      "content": "Nên Ta sẽ chẳng quay mặt đi mà không phạt nó.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.174Z",
      "updatedAt": "2025-04-22T10:59:43.174Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "6805ae59-2f54-4464-86a1-0276d3f2b50c",
      "number": 1,
      "content": "Vì nó đã đốt hài cốt vua Ê-đôm",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:42.939Z",
      "updatedAt": "2025-04-22T10:59:42.939Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "449af61b-b322-48a9-ac31-e19175611278",
      "number": 1,
      "content": "Ra tro bụi,",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:42.951Z",
      "updatedAt": "2025-04-22T10:59:42.951Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "d7f806d3-2d00-4ffd-b7ff-3fa2323a2b49",
      "number": 2,
      "content": "Nên Ta sẽ sai một ngọn lửa đến trên Mô-áp;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:42.961Z",
      "updatedAt": "2025-04-22T10:59:42.961Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "598aea73-be42-45c7-b459-77127f1db9ac",
      "number": 2,
      "content": "Ngọn lửa ấy sẽ thiêu rụi các lâu đài của Kê-ri-ốt.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:42.974Z",
      "updatedAt": "2025-04-22T10:59:42.974Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "a9e70199-a1e9-4248-9cf8-1f0f91e62dcf",
      "number": 2,
      "content": "Mô-áp sẽ bị diệt vong trong cảnh hỗn loạn,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:42.983Z",
      "updatedAt": "2025-04-22T10:59:42.983Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "47b7007e-2f4f-476c-9466-5d6a8740473a",
      "number": 2,
      "content": "Giữa những tiếng gào thét giao tranh và những tiếng kèn xung trận.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:42.993Z",
      "updatedAt": "2025-04-22T10:59:42.993Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "1854455c-ba9c-45c6-a259-6d2425567cf4",
      "number": 3,
      "content": "Ta sẽ tiêu diệt kẻ cầm quyền phán xét khỏi nó,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.003Z",
      "updatedAt": "2025-04-22T10:59:43.003Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "413a5c99-e6e6-42f6-8cbc-aca7603a1f2f",
      "number": 3,
      "content": "Và diệt trừ tất cả các kẻ có quyền hành chung với kẻ ấy,”",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.013Z",
      "updatedAt": "2025-04-22T10:59:43.013Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "39ad5a2a-4ebf-4ede-a254-9eddf5206c2d",
      "number": 3,
      "content": "**CHÚA** phán.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.023Z",
      "updatedAt": "2025-04-22T10:59:43.023Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "24e481d1-ada8-4a24-8d21-994dabef738a",
      "number": 16,
      "content": "**CHÚA** phán.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.604Z",
      "updatedAt": "2025-04-22T10:59:43.604Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "17809683-ca00-4dab-acb5-38b072e55f3c",
      "number": 11,
      "content": "**CHÚA** phán.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.421Z",
      "updatedAt": "2025-04-22T10:59:43.421Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "81a33fad-8a45-4d7a-b5f2-23a36d826ae4",
      "number": 4,
      "content": "“Vì tội của Giu-đa nặng gấp ba gấp bốn lần,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.052Z",
      "updatedAt": "2025-04-22T10:59:43.052Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "6c6f94af-9aad-42ed-93e7-6a3c5c49cf4c",
      "number": 4,
      "content": "Vì chúng đã loại bỏ Luật Pháp của **CHÚA**",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.075Z",
      "updatedAt": "2025-04-22T10:59:43.075Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "c509aa98-c277-42cd-a1f8-6a33886d0a87",
      "number": 4,
      "content": "Và không vâng giữ các luật lệ Ngài,",
      "order": 4,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.085Z",
      "updatedAt": "2025-04-22T10:59:43.085Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "5af6eadf-1f6f-4ae9-9715-4ea0a777024c",
      "number": 4,
      "content": "Chúng đã để các thần tượng giả dối lừa gạt chúng đi sai lạc,",
      "order": 5,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.094Z",
      "updatedAt": "2025-04-22T10:59:43.094Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "ccbe6887-39cd-490c-8c20-45bc64ce6e16",
      "number": 4,
      "content": "Đó là các thần tượng giả dối mà tổ tiên chúng đã tin thờ,",
      "order": 6,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.104Z",
      "updatedAt": "2025-04-22T10:59:43.104Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "ab215317-37cc-4dbb-af80-118d2512178b",
      "number": 5,
      "content": "Nên Ta sẽ sai một ngọn lửa đến trên Giu-đa;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.117Z",
      "updatedAt": "2025-04-22T10:59:43.117Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "244a0ca8-2509-456e-8de0-47a1eca3af91",
      "number": 5,
      "content": "Ngọn lửa ấy sẽ thiêu rụi các lâu đài của Giê-ru-sa-lem.”",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.127Z",
      "updatedAt": "2025-04-22T10:59:43.127Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "6c16cfa2-40f8-4942-b7bd-16f17651063e",
      "number": 6,
      "content": "“Vì tội của I-sơ-ra-ên nặng gấp ba gấp bốn lần,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.164Z",
      "updatedAt": "2025-04-22T10:59:43.164Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "38212d74-8212-4bb9-baf7-2f508408f2d6",
      "number": 6,
      "content": "Vì chúng đã bán đứng người công chính để lấy bạc,",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.190Z",
      "updatedAt": "2025-04-22T10:59:43.190Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "b126e40f-2126-4876-b64b-0626124f9009",
      "number": 6,
      "content": "Chúng đã bán người nghèo để lấy một đôi giày,",
      "order": 4,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.200Z",
      "updatedAt": "2025-04-22T10:59:43.200Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "e0e8262c-6f30-46e3-bfd7-2b6c986715de",
      "number": 7,
      "content": "Chúng đã đạp đầu người nghèo vào bụi đất,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.210Z",
      "updatedAt": "2025-04-22T10:59:43.210Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "03f9b7a6-9f61-48d9-a528-433cb204dcb0",
      "number": 7,
      "content": "Chúng đã cướp công lý của người bị áp bức,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.220Z",
      "updatedAt": "2025-04-22T10:59:43.220Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "0a31ff6a-c5dd-4d6e-9e8f-53fb4155a6f0",
      "number": 7,
      "content": "Tệ hơn nữa, hai cha con ăn nằm với một người phụ nữ,",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.230Z",
      "updatedAt": "2025-04-22T10:59:43.230Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "acc210c0-4743-47b9-9dba-9aa21f9b10df",
      "number": 7,
      "content": "Khiến cho danh thánh của Ta bị ô uế giữa các dân.",
      "order": 3,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.241Z",
      "updatedAt": "2025-04-22T10:59:43.241Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "83cf4b84-856a-474e-a295-e3d3096576c6",
      "number": 8,
      "content": "Chúng mặc những y phục của người ta cầm thế",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.254Z",
      "updatedAt": "2025-04-22T10:59:43.254Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "bdaf0f03-f57c-4586-97b8-6dd3efe6411f",
      "number": 8,
      "content": "Mà phủ phục trước bàn thờ các thần tượng của chúng;",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.271Z",
      "updatedAt": "2025-04-22T10:59:43.271Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "734d4144-aad9-4d79-a77f-5ca34e9b6ede",
      "number": 8,
      "content": "Trong đền thờ các thần tượng của chúng,",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.286Z",
      "updatedAt": "2025-04-22T10:59:43.286Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "51f1da0c-9b07-48ed-b49e-f09224e02026",
      "number": 8,
      "content": "Chúng dâng rượu tế mua bằng tiền lừa gạt người ta.",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.297Z",
      "updatedAt": "2025-04-22T10:59:43.297Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "cd296d0e-c953-4e1a-aead-6cc1c2b64be1",
      "number": 9,
      "content": "Dù vậy Ta đã phù hộ chúng bằng cách tiêu diệt dân A-mô-ri trước mặt chúng,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.308Z",
      "updatedAt": "2025-04-22T10:59:43.308Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "90998d81-bfdb-4fe6-92b4-63bb67c59148",
      "number": 9,
      "content": "Đó là dân cao lớn như cây bá hương",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.319Z",
      "updatedAt": "2025-04-22T10:59:43.319Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "9accd256-d814-4bcf-af97-db9b7297cd68",
      "number": 9,
      "content": "Và mạnh mẽ như cây sồi;",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.330Z",
      "updatedAt": "2025-04-22T10:59:43.330Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "e12d5c8e-d596-4a88-9086-6ef7e387f4c3",
      "number": 9,
      "content": "Trên mặt đất, Ta đã diệt trừ mọi trái;",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.339Z",
      "updatedAt": "2025-04-22T10:59:43.339Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "c20d21a6-cbd1-48e7-9002-864c329c74f5",
      "number": 9,
      "content": "Dưới mặt đất, Ta đã không cho một rễ nào được sống còn.",
      "order": 4,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.349Z",
      "updatedAt": "2025-04-22T10:59:43.349Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "bba3b359-4cea-4f5a-978f-1f7104a6fd86",
      "number": 10,
      "content": "Ta đã đem các ngươi lên khỏi đất Ai-cập,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.359Z",
      "updatedAt": "2025-04-22T10:59:43.359Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "07df3962-8fa2-4bfb-a792-520400289ca7",
      "number": 10,
      "content": "Ta đã dẫn dắt các ngươi bốn mươi năm trong đồng hoang",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.369Z",
      "updatedAt": "2025-04-22T10:59:43.369Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "1a467b13-4ff8-4b38-9a42-eb37dba49626",
      "number": 10,
      "content": "Và ban cho các ngươi xứ của dân A-mô-ri.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.379Z",
      "updatedAt": "2025-04-22T10:59:43.379Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "1cdcd2e7-e952-4631-a60e-dc0b634d0120",
      "number": 11,
      "content": "Ta đã nuôi dưỡng một số con trai các ngươi thành các tiên tri,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.389Z",
      "updatedAt": "2025-04-22T10:59:43.389Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "69c5e87f-a442-44c4-82e0-96819ecd6dc1",
      "number": 11,
      "content": "Một số người trẻ của các ngươi thành những người Na-xi-rê;",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.400Z",
      "updatedAt": "2025-04-22T10:59:43.400Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "384451da-fc2f-4723-a264-174ee492990b",
      "number": 11,
      "content": "Hỡi dân I-sơ-ra-ên, có đúng như thế không?”",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.410Z",
      "updatedAt": "2025-04-22T10:59:43.410Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "f8b4096d-670c-40e2-91b0-4199ac771072",
      "number": 12,
      "content": "“Thế nhưng các ngươi đã bắt những người Na-xi-rê uống rượu,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.432Z",
      "updatedAt": "2025-04-22T10:59:43.432Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "c2a8ba09-bde6-44a9-aa06-4240de8cbbea",
      "number": 12,
      "content": "Và đã ra lệnh cho các tiên tri rằng",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.445Z",
      "updatedAt": "2025-04-22T10:59:43.445Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "2a8e0438-46b4-45f4-8439-1cc0a2365889",
      "number": 12,
      "content": "‘Các ông không được nói tiên tri.’",
      "order": 2,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.459Z",
      "updatedAt": "2025-04-22T10:59:43.459Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "6ced6021-690e-46a5-9e3b-d66e0c08db59",
      "number": 13,
      "content": "Này, Ta sẽ ép các ngươi",
      "order": 0,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.472Z",
      "updatedAt": "2025-04-22T10:59:43.472Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "2721a004-a700-46d3-990b-056657e71b42",
      "number": 13,
      "content": "Như chiếc xe chở đầy những bó lúa cán xuống mặt đường.",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.489Z",
      "updatedAt": "2025-04-22T10:59:43.489Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "3424b884-e279-4a6b-9bf8-366d323f6a89",
      "number": 14,
      "content": "Do đó người nhanh lẹ sẽ không thoát chết,",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.503Z",
      "updatedAt": "2025-04-22T10:59:43.503Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "bf4fc04f-a62d-4869-953c-e91ebe47da68",
      "number": 14,
      "content": "Người mạnh mẽ sẽ không duy trì được sức lực,",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.516Z",
      "updatedAt": "2025-04-22T10:59:43.516Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "8caa6b5f-73dc-4f77-8198-ff821adc891f",
      "number": 14,
      "content": "Kẻ dũng mãnh sẽ không cứu được mạng sống mình,",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.530Z",
      "updatedAt": "2025-04-22T10:59:43.530Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "eae856b4-3e44-4549-be42-8e30cc84dfad",
      "number": 15,
      "content": "Quân xạ tiễn sẽ không trụ được tại chỗ,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.543Z",
      "updatedAt": "2025-04-22T10:59:43.543Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "7231611c-227f-4074-8f5e-7aff698dab49",
      "number": 15,
      "content": "Các chiến sĩ chạy nhanh nhất cũng không chạy thoát,",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.555Z",
      "updatedAt": "2025-04-22T10:59:43.555Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "571334fe-8409-4b38-8683-63f5dfeb8ffd",
      "number": 15,
      "content": "Ngay cả những kỵ binh cỡi các chiến mã cũng không chạy thoát kịp để giữ mạng mình,",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.567Z",
      "updatedAt": "2025-04-22T10:59:43.567Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "da5e82fb-24f0-4f72-9eac-3ba5ff7533a6",
      "number": 16,
      "content": "Thậm chí kẻ can đảm nhất giữa vòng các dũng sĩ",
      "order": 0,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.579Z",
      "updatedAt": "2025-04-22T10:59:43.579Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    },
    {
      "id": "b499401c-620b-4b69-9baa-8a476f138039",
      "number": 16,
      "content": "Sẽ chạy trốn trần truồng trong ngày ấy,”",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:59:43.592Z",
      "updatedAt": "2025-04-22T10:59:43.592Z",
      "chapterId": "8493b1a1-4694-473b-9f0e-c6f3b522b682"
    }
  ],
