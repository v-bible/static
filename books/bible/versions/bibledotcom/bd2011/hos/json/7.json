{
  "chapter": {
    "id": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2",
    "number": 7,
    "ref": "https://www.bible.com/bible/19/HOS.7.BD2011",
    "createdAt": "2025-04-24T01:29:11.037Z",
    "updatedAt": "2025-04-24T01:29:11.037Z",
    "bookId": "b4dbbfa2-9517-4bd6-9267-ba5fb0d27ef6"
  },
  "verses": [
    {
      "id": "bfd2d19e-ec1f-44a8-a6b5-0c8a4fb3c777",
      "number": 1,
      "content": "Khi Ta muốn chữa lành cho I-sơ-ra-ên,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:53.906Z",
      "updatedAt": "2025-04-24T02:12:53.906Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "2a3cd0b6-0cbd-4490-ac41-7f47227b2278",
      "number": 1,
      "content": "Tội lỗi của Ép-ra-im",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:53.928Z",
      "updatedAt": "2025-04-24T02:12:53.928Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "9149c088-dd1f-4c3d-9b49-a5a8c67947c3",
      "number": 1,
      "content": "Và gian ác của Sa-ma-ri lại lộ ra;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:53.938Z",
      "updatedAt": "2025-04-24T02:12:53.938Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "f1b692ce-5d80-49bf-872d-2520be3ac54d",
      "number": 1,
      "content": "Vì chúng đã làm những việc gian tà;",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:53.949Z",
      "updatedAt": "2025-04-24T02:12:53.949Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "8e165f07-833a-4434-ba28-9dac1c2cb883",
      "number": 1,
      "content": "Bên trong phường ăn trộm lấy cắp;",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:53.960Z",
      "updatedAt": "2025-04-24T02:12:53.960Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "7b1d5208-d0e6-4652-bcf6-d4a172621dea",
      "number": 1,
      "content": "Bên ngoài quân ăn cướp tấn công.",
      "order": 5,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:53.972Z",
      "updatedAt": "2025-04-24T02:12:53.972Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "35e2ef04-ab02-4ede-b971-fb451ae3e8af",
      "number": 2,
      "content": "Nhưng chúng không nghĩ rằng",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:53.982Z",
      "updatedAt": "2025-04-24T02:12:53.982Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "4376a3ee-3f32-4199-963b-6c2a57f4a484",
      "number": 2,
      "content": "Ta nhớ hết mọi việc ác của chúng.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:53.992Z",
      "updatedAt": "2025-04-24T02:12:53.992Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "7a197d38-daaf-432e-8a9c-243376abb086",
      "number": 2,
      "content": "Những hành vi tội lỗi của chúng dẫy đầy chung quanh chúng,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.002Z",
      "updatedAt": "2025-04-24T02:12:54.002Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "97ce0848-5f90-4c6c-a3a1-ffe4d049943e",
      "number": 2,
      "content": "Những hành vi ấy hằng ở trước mặt Ta.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.012Z",
      "updatedAt": "2025-04-24T02:12:54.012Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "2a239406-9f55-4c30-a03c-2205cebc9b34",
      "number": 3,
      "content": "Để giúp vui cho vua, chúng đã bày những trò gian ác,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.022Z",
      "updatedAt": "2025-04-24T02:12:54.022Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "63016221-6057-49c1-97b8-f0e6669c81fe",
      "number": 3,
      "content": "Để làm hài lòng những kẻ có quyền, chúng đã tạo ra các việc dối gian.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.031Z",
      "updatedAt": "2025-04-24T02:12:54.031Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "f2a56698-ff53-4c77-8cf2-86e989104937",
      "number": 4,
      "content": "Tất cả chúng đều là những kẻ ngoại tình;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.042Z",
      "updatedAt": "2025-04-24T02:12:54.042Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "0b913f47-a6a0-46b0-aa1e-91d26634b763",
      "number": 4,
      "content": "Chúng giống như một lò lửa cứ hực cháy,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.051Z",
      "updatedAt": "2025-04-24T02:12:54.051Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "7280fed7-cd37-4ca1-873d-9d1a2f2231e2",
      "number": 4,
      "content": "Mặc dù người nướng bánh đã ngưng nuôi ngọn lửa",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.061Z",
      "updatedAt": "2025-04-24T02:12:54.061Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "8f6c9bd4-f9d4-4fa0-9ff5-ba1cb1225c59",
      "number": 4,
      "content": "Từ lúc nhồi bột",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.087Z",
      "updatedAt": "2025-04-24T02:12:54.087Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "b5cdf4db-16c3-4bf2-b833-dd252e9938ed",
      "number": 4,
      "content": "Cho đến khi bột dậy men.",
      "order": 4,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.100Z",
      "updatedAt": "2025-04-24T02:12:54.100Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "3bc43480-3efd-409e-9ff1-51487c412e70",
      "number": 5,
      "content": "Trong ngày lễ tôn trọng vua,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.110Z",
      "updatedAt": "2025-04-24T02:12:54.110Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "2ebc2a66-330d-4faf-950a-28de5cbf2c2f",
      "number": 5,
      "content": "Các quan tướng đều say mèm đến ngã bịnh;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.120Z",
      "updatedAt": "2025-04-24T02:12:54.120Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "bfacea14-7922-491b-ade2-a810fdd14718",
      "number": 5,
      "content": "Còn vua chẳng còn biết giữ thể thống gì, nên đã cười đùa với những kẻ chế nhạo mình.",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.130Z",
      "updatedAt": "2025-04-24T02:12:54.130Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "4875fe8f-8850-4881-838d-83c8b578c32e",
      "number": 6,
      "content": "Vì chúng đã chuẩn bị lòng chúng như một lò lửa;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.158Z",
      "updatedAt": "2025-04-24T02:12:54.158Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "6236994a-9c2c-481a-badd-aac1bff13683",
      "number": 6,
      "content": "Suốt đêm chúng toan tính những ý đồ tội lỗi xấu xa;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.168Z",
      "updatedAt": "2025-04-24T02:12:54.168Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "5ec3b16e-773f-4d85-b8bc-82cc860122ca",
      "number": 6,
      "content": "Sáng ngày chúng bộc lộ những điều ấy ra như một ngọn lửa bùng lên.",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.178Z",
      "updatedAt": "2025-04-24T02:12:54.178Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "372a9842-af88-44e7-89c8-56741d6d0aa6",
      "number": 7,
      "content": "Tất cả chúng đều nóng như một lò lửa hực;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.188Z",
      "updatedAt": "2025-04-24T02:12:54.188Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "09670857-8de1-4de9-be6d-b6ed8a608a3b",
      "number": 7,
      "content": "Chúng thiêu đốt cả những kẻ cầm quyền của chúng;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.198Z",
      "updatedAt": "2025-04-24T02:12:54.198Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "da04e053-30ee-4369-a3f6-7eb51e72c75d",
      "number": 7,
      "content": "Tất cả các vua chúng đã sa ngã;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.207Z",
      "updatedAt": "2025-04-24T02:12:54.207Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "bbc979cd-19ba-4f6f-af8f-5b067daaf2f1",
      "number": 7,
      "content": "Không ai trong chúng kêu cầu Ta.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.220Z",
      "updatedAt": "2025-04-24T02:12:54.220Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "b868ca7d-a111-4a4c-8e33-ac63e70b9df6",
      "number": 8,
      "content": "Ép-ra-im tin thờ chung chạ với các dân ngoại;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.230Z",
      "updatedAt": "2025-04-24T02:12:54.230Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "d916cb96-1a2b-4c05-92aa-964e6ad70911",
      "number": 8,
      "content": "Ép-ra-im trở thành cái bánh nướng không được lật qua mặt kia, nên bị cháy đen một mặt.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.240Z",
      "updatedAt": "2025-04-24T02:12:54.240Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "33f2180f-5743-456c-a4ec-73d30679e1f8",
      "number": 9,
      "content": "Sự thờ lạy những thần tượng của các dân ngoại đã làm tiêu hao sức lực của nó,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.250Z",
      "updatedAt": "2025-04-24T02:12:54.250Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "20396235-7be4-4b73-b5af-1b551deb41de",
      "number": 9,
      "content": "Nhưng nó không biết.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.260Z",
      "updatedAt": "2025-04-24T02:12:54.260Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "34db8bb7-8bf2-4c51-bd5b-7e57c8bd426a",
      "number": 9,
      "content": "Phải, nó giống như một người tóc đã hoa râm, sức khỏe đang giảm dần,",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.269Z",
      "updatedAt": "2025-04-24T02:12:54.269Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "2b9026fc-f570-49d0-9dd1-a71cf00edb10",
      "number": 9,
      "content": "Mà không biết.",
      "order": 3,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.279Z",
      "updatedAt": "2025-04-24T02:12:54.279Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "b50dcd3c-56f0-4cf0-b2f7-313d5dc5a337",
      "number": 10,
      "content": "Sự kiêu ngạo của I-sơ-ra-ên đã làm chứng nghịch lại nó;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.289Z",
      "updatedAt": "2025-04-24T02:12:54.289Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "f3727f08-608f-400f-8948-1e26131c1ed1",
      "number": 10,
      "content": "Thế mà trong mọi sự đó, nó vẫn không quay về với **CHÚA**, Đức Chúa Trời của nó,",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.299Z",
      "updatedAt": "2025-04-24T02:12:54.299Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "80a786be-32fb-40a6-93cc-b5f120fe69ff",
      "number": 10,
      "content": "Hoặc để hết lòng tìm kiếm Ngài!",
      "order": 2,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.309Z",
      "updatedAt": "2025-04-24T02:12:54.309Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "d50a4a18-10de-48cc-a4a2-9dfb32ae1103",
      "number": 11,
      "content": "Ép-ra-im giống như một con bồ câu",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.319Z",
      "updatedAt": "2025-04-24T02:12:54.319Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "b23fec66-8ddd-4916-bfdb-807876b2252b",
      "number": 11,
      "content": "Khờ dại, tâm trí chẳng biết nghĩ suy;",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.340Z",
      "updatedAt": "2025-04-24T02:12:54.340Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "4fae25ad-cb7c-486a-93ef-70dabf5cba58",
      "number": 11,
      "content": "Khi thì cầu cạnh Ai-cập, lúc thì nhờ vả A-sy-ri.",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.349Z",
      "updatedAt": "2025-04-24T02:12:54.349Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "3db03849-de25-4796-9d96-aa4c54f13cb3",
      "number": 12,
      "content": "Khi chúng ra đi, Ta sẽ bủa lưới ra giăng trên chúng;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.359Z",
      "updatedAt": "2025-04-24T02:12:54.359Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "454ea00e-6f67-455d-808f-28161dfce937",
      "number": 12,
      "content": "Ta sẽ bắt trọn chúng như bắt gọn các chim trời;",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.369Z",
      "updatedAt": "2025-04-24T02:12:54.369Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "c8e1407e-0451-4d31-aa8f-3c67712e35bf",
      "number": 12,
      "content": "Khi chúng hội họp nhau để toan tính sự nhờ cậy loài người, Ta sẽ sửa trị chúng.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.379Z",
      "updatedAt": "2025-04-24T02:12:54.379Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "b0fb38c7-dddb-4e9e-a36f-dc9807bb1ee8",
      "number": 13,
      "content": "Khốn cho chúng, vì chúng đã lìa bỏ Ta!",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.395Z",
      "updatedAt": "2025-04-24T02:12:54.395Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "a3f67652-a9a5-4607-8d06-eddafb0d56a9",
      "number": 13,
      "content": "Thảm khốc thay cho chúng, vì chúng đã nổi loạn chống lại Ta!",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.405Z",
      "updatedAt": "2025-04-24T02:12:54.405Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "89afd927-773d-4953-8ad4-507cada0d809",
      "number": 13,
      "content": "Ta muốn cứu chuộc chúng biết bao,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.415Z",
      "updatedAt": "2025-04-24T02:12:54.415Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "1763ad9a-163f-4412-8755-3bbc7d612c6c",
      "number": 13,
      "content": "Nhưng chúng cứ nói những lời dối trá xúc phạm Ta.",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.425Z",
      "updatedAt": "2025-04-24T02:12:54.425Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "7ba31da7-c7ab-4e69-a60f-e71320142b47",
      "number": 14,
      "content": "Chúng chẳng để lòng kêu cầu Ta,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.435Z",
      "updatedAt": "2025-04-24T02:12:54.435Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "321e3bfd-b588-4fd2-b39b-245d869d1e0d",
      "number": 14,
      "content": "Nhưng cứ nằm trên giường mà trăn trở thở than.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.444Z",
      "updatedAt": "2025-04-24T02:12:54.444Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "33eb8a2a-eecb-4948-8130-9170c4ed9d97",
      "number": 14,
      "content": "Chúng quay qua các thần tượng",
      "order": 2,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.454Z",
      "updatedAt": "2025-04-24T02:12:54.454Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "efb223c5-a2f8-484c-8a8a-ed1652417329",
      "number": 14,
      "content": "Và cầu xin cho ruộng lúa trúng mùa và vườn nho sản xuất rượu.",
      "order": 3,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.464Z",
      "updatedAt": "2025-04-24T02:12:54.464Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "84d9cd9f-ddb1-4ad0-89c6-f5ce6742b387",
      "number": 15,
      "content": "Dù Ta đã huấn luyện chúng và ban sức lực cho tay chúng,",
      "order": 0,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.474Z",
      "updatedAt": "2025-04-24T02:12:54.474Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "520dd90d-4123-42d5-a62f-d3428fde0fc6",
      "number": 15,
      "content": "Chúng vẫn lập mưu tính kế chống lại Ta.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.484Z",
      "updatedAt": "2025-04-24T02:12:54.484Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "a5f89d75-83db-425d-a658-1372f425b3c5",
      "number": 16,
      "content": "Chúng chẳng quay về với Đấng Tối Cao,",
      "order": 0,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.494Z",
      "updatedAt": "2025-04-24T02:12:54.494Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "f865ead4-b707-42a7-a5de-324a44c65515",
      "number": 16,
      "content": "Chúng như cây cung hỏng, không thể nào bắn trúng mục tiêu;",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.507Z",
      "updatedAt": "2025-04-24T02:12:54.507Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "072bca6f-88f8-4eb7-81b6-1a6e7787320f",
      "number": 16,
      "content": "Những kẻ lãnh đạo của chúng sẽ ngã chết bằng gươm,",
      "order": 2,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.521Z",
      "updatedAt": "2025-04-24T02:12:54.521Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "92b15fb8-87c8-49c7-bda0-cfb351b14921",
      "number": 16,
      "content": "Bởi miệng chúng đã buông những lời hỗn láo xúc phạm đến Ta;",
      "order": 3,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.534Z",
      "updatedAt": "2025-04-24T02:12:54.534Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    },
    {
      "id": "8f43ca2a-3a00-4aef-9389-c6e582357048",
      "number": 16,
      "content": "Vì vậy chúng sẽ làm đầu đề cho người ta giễu cợt ở Ai-cập.",
      "order": 4,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:12:54.547Z",
      "updatedAt": "2025-04-24T02:12:54.547Z",
      "chapterId": "c2df5723-77bb-4cbf-9dbe-6684e99fa9e2"
    }
  ],
