      "chapterId": "fc9f660d-c3fa-4fca-825d-8a33044a5e9d"
    },
    {
      "id": "1dae3323-d8e4-4b38-ad79-a2aabd98a373",
      "content": "Bị Lưu Đày Vì Cứ Phạm Tội",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T02:12:59.314Z",
      "updatedAt": "2025-04-24T02:12:59.314Z",
