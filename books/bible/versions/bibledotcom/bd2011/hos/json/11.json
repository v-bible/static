    },
    {
      "id": "36feba49-8bf1-401c-81d9-1e443d3e6814",
      "number": 12,
      "content": "“Ép-ra-im đã bao quanh Ta bằng những lời giả dối,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:13:04.940Z",
      "updatedAt": "2025-04-24T02:13:04.940Z",
      "chapterId": "e4bb3ead-7e64-4e28-a5c3-b05d3ef7578d"
    },
    {
      "id": "f0fa9d01-8742-4c1c-bdeb-0d97f2a77aa7",
      "number": 12,
      "content": "Còn nhà I-sơ-ra-ên thì dùng sự dối gian;",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:13:04.959Z",
      "updatedAt": "2025-04-24T02:13:04.959Z",
      "chapterId": "e4bb3ead-7e64-4e28-a5c3-b05d3ef7578d"
    },
    {
      "id": "43cdaf31-aa53-4e36-828b-0905421defa3",
      "number": 12,
      "content": "Nhưng Giu-đa vẫn còn kính sợ Đức Chúa Trời",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:13:04.968Z",
      "updatedAt": "2025-04-24T02:13:04.968Z",
      "chapterId": "e4bb3ead-7e64-4e28-a5c3-b05d3ef7578d"
    },
    {
      "id": "cda7ff7d-813d-410b-a041-53313d9ca693",
      "number": 12,
      "content": "Và vẫn còn trung thành với Đấng Thánh.",
      "order": 3,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:13:04.978Z",
      "updatedAt": "2025-04-24T02:13:04.978Z",
