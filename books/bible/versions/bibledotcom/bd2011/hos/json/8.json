    },
    {
      "id": "b388e7f3-ce6a-4adc-ab4d-00d6725d7d74",
      "content": "Chính Quyền Không Được Chấp Nhận và Dân Thờ Hình Tượng",
      "order": 0,
      "createdAt": "2024-09-23T06:09:28.861Z",
      "updatedAt": "2024-09-23T06:09:28.861Z",
      "verseId": "b7be860b-8804-4cfb-be0a-ed033c312330",
      "chapterId": "f5f52bb7-75d7-4b3c-aa89-40d59d12e71f"
    },
    {
      "id": "0f4ce0bd-2c2f-48ca-a19e-fafd77abe0a2",
      "content": "Tôn Giáo Có Hình Thức Nhưng Không Có Thực Chất",
      "order": 0,
      "createdAt": "2024-09-23T06:09:28.984Z",
      "updatedAt": "2024-09-23T06:09:28.984Z",
