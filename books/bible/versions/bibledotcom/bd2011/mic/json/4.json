    },
    {
      "id": "3f2bd4bd-bbb6-423f-ae69-38f95c3426f2",
      "content": "CHÚA",
      "order": 2,
      "createdAt": "2024-09-23T06:11:05.680Z",
      "updatedAt": "2024-09-23T06:11:05.680Z",
      "verseId": "6cfadf9b-a48d-4ac2-89b5-f9af30e606c5",
      "chapterId": "bb49dca6-0650-4649-a9ef-4783e2cc7588"
    },
    {
      "id": "528c12c0-a476-411d-9ef2-31572d590cb9",
      "content": "Trị Vì tại Si-ôn",
      "order": 3,
      "createdAt": "2024-09-23T06:11:05.684Z",
      "updatedAt": "2024-09-23T06:11:05.684Z",
