    {
      "id": "5bc5a0c0-9b0a-4c31-acea-4291d0622d0b",
      "content": "Lời Hứa về Sự Phục Hưng Sau Thời Bị Lưu Đày",
      "order": 0,
      "createdAt": "2024-09-23T06:11:05.821Z",
      "updatedAt": "2024-09-23T06:11:05.821Z",
