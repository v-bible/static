      "chapterId": "2599ac9e-eab3-47ca-b50f-c8eae2707e97"
    },
    {
      "id": "406baea1-8355-4a04-bd00-5ceb7bfebe8e",
      "content": "Xa-a-nan có nghĩa là “ra đi”",
      "position": 14,
      "order": 2,
      "createdAt": "2025-04-22T11:00:33.534Z",
      "updatedAt": "2025-04-22T11:00:33.534Z",
      "verseId": "c1326cb5-b8c6-421a-a4df-ebf7a98e733a",
      "chapterId": "2599ac9e-eab3-47ca-b50f-c8eae2707e97"
    },
    {
      "id": "09ed9d46-458f-4297-8f47-b00479806f08",
      "content": "Bết Ê-xên có nghĩa là “nhà chật”",
      "position": 15,
      "order": 3,
      "createdAt": "2025-04-22T11:00:33.573Z",
      "updatedAt": "2025-04-22T11:00:33.573Z",
      "verseId": "5db477bf-8bce-407a-82f5-e521b98c73b3",
      "chapterId": "2599ac9e-eab3-47ca-b50f-c8eae2707e97"
    },
    {
      "id": "27cf8247-fad6-413f-9650-4d253a2fb01b",
      "content": "nt: của ngươi",
      "position": 21,
      "order": 4,
      "createdAt": "2025-04-22T11:00:33.598Z",
      "updatedAt": "2025-04-22T11:00:33.598Z",
      "verseId": "166d353c-b202-4584-b382-73825b47b459",
      "chapterId": "2599ac9e-eab3-47ca-b50f-c8eae2707e97"
    },
    {
      "id": "df6fddda-ada1-42ab-ab6b-cac8a5750519",
      "content": "Ách-xíp có nghĩa là “dối gạt”",
      "position": 53,
      "order": 5,
      "createdAt": "2025-04-22T11:00:33.768Z",
      "updatedAt": "2025-04-22T11:00:33.768Z",
      "verseId": "5aeba47f-4659-4bca-912a-27620d71edc4",
      "chapterId": "2599ac9e-eab3-47ca-b50f-c8eae2707e97"
    },
    {
      "id": "fc27384f-fefb-4350-82f5-51c38a3b09d7",
      "content": "Ma-rê-sa có nghĩa là “kẻ chinh phục”",
      "position": 20,
      "order": 6,
      "createdAt": "2025-04-22T11:00:33.806Z",
      "updatedAt": "2025-04-22T11:00:33.806Z",
      "verseId": "db21c650-13e6-4879-bea6-f5fc47f5b0b1",
      "chapterId": "2599ac9e-eab3-47ca-b50f-c8eae2707e97"
    }
  ],
