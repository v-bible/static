      "chapterId": "48d365e9-4401-46b5-be72-74498e33cd2c"
    },
    {
      "id": "8ce362dd-b089-4453-a399-e09d343ac1b4",
      "content": "Biển Địa Trung Hải",
      "position": 48,
      "order": 2,
      "createdAt": "2025-04-22T10:18:06.866Z",
      "updatedAt": "2025-04-22T10:18:06.866Z",
      "verseId": "cb39faf9-51ff-4bf3-9fad-beed722fdfbe",
      "chapterId": "48d365e9-4401-46b5-be72-74498e33cd2c"
    },
    {
