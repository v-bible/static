      "chapterId": "aae9e6e8-836b-466d-bb1a-090622edbb82"
    },
    {
      "id": "72868c6f-7329-4d5e-a411-6adeffcda7eb",
      "content": "Biển Địa Trung Hải",
      "position": 48,
      "order": 2,
      "createdAt": "2025-04-24T01:36:19.146Z",
