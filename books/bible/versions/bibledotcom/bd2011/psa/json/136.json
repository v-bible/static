    },
    {
      "id": "e3c23376-58d5-4cbe-95d9-dfb3ba64a434",
      "number": 21,
      "content": "Rồi ban xứ của họ để làm sản nghiệp,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.829Z",
      "updatedAt": "2025-04-24T01:59:36.829Z",
      "chapterId": "2107380b-fa6a-4c55-94c9-42e27420847f"
    },
    {
      "id": "733f68d8-743f-4e1e-8f87-9c689358610b",
      "number": 21,
      "content": "Vì tình thương của Ngài còn đến đời đời,",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.844Z",
      "updatedAt": "2025-04-24T01:59:36.844Z",
      "chapterId": "2107380b-fa6a-4c55-94c9-42e27420847f"
    },
    {
      "id": "73b40fa5-27e8-4d33-ad42-0a7bdff9bd19",
      "number": 22,
      "content": "Tức làm sản nghiệp cho I-sơ-ra-ên tôi tớ Ngài,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.857Z",
      "updatedAt": "2025-04-24T01:59:36.857Z",
      "chapterId": "2107380b-fa6a-4c55-94c9-42e27420847f"
    },
    {
      "id": "7bcb3bef-e621-4b05-9316-4eb3988b4044",
      "number": 22,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.870Z",
      "updatedAt": "2025-04-24T01:59:36.870Z",
      "chapterId": "2107380b-fa6a-4c55-94c9-42e27420847f"
    },
    {
      "id": "8b1f1906-2cc6-40ef-828a-2956a0b19e0f",
      "number": 23,
      "content": "Đấng nhớ đến thân phận thấp hèn của chúng ta,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.881Z",
      "updatedAt": "2025-04-24T01:59:36.881Z",
      "chapterId": "2107380b-fa6a-4c55-94c9-42e27420847f"
    },
    {
      "id": "fe3cc7bd-7bf9-4c65-b1cc-b5a7309f7774",
      "number": 23,
      "content": "Vì tình thương của Ngài còn đến đời đời,",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.895Z",
      "updatedAt": "2025-04-24T01:59:36.895Z",
      "chapterId": "2107380b-fa6a-4c55-94c9-42e27420847f"
    },
    {
      "id": "c3562b34-6a9a-4b80-b49c-c19821b2bd3f",
      "number": 24,
      "content": "Và giải cứu chúng ta khỏi những kẻ thù của chúng ta,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.906Z",
      "updatedAt": "2025-04-24T01:59:36.906Z",
      "chapterId": "2107380b-fa6a-4c55-94c9-42e27420847f"
    },
    {
      "id": "d2bbb81e-36c3-47bb-be36-6a25b39a47de",
      "number": 24,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.916Z",
      "updatedAt": "2025-04-24T01:59:36.916Z",
      "chapterId": "2107380b-fa6a-4c55-94c9-42e27420847f"
    },
    {
      "id": "c7a7c55d-dceb-4a5e-8a39-b0d898951dda",
      "number": 25,
      "content": "Đấng ban thực phẩm cho mọi loài xác thịt,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.927Z",
      "updatedAt": "2025-04-24T01:59:36.927Z",
      "chapterId": "2107380b-fa6a-4c55-94c9-42e27420847f"
    },
    {
      "id": "24b5ea4b-fc0e-4793-83cc-a65157729355",
      "number": 25,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.942Z",
      "updatedAt": "2025-04-24T01:59:36.942Z",
      "chapterId": "2107380b-fa6a-4c55-94c9-42e27420847f"
    },
    {
      "id": "5e269ef9-954d-4f3c-b9b9-d59be6a94c6b",
      "number": 26,
      "content": "Hãy cảm tạ Đức Chúa Trời của các tầng trời,",
      "order": 0,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.952Z",
      "updatedAt": "2025-04-24T01:59:36.952Z",
      "chapterId": "2107380b-fa6a-4c55-94c9-42e27420847f"
    },
    {
      "id": "68ab6558-01a9-4de3-a3ea-f58bdf838bcb",
      "number": 26,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:36.964Z",
