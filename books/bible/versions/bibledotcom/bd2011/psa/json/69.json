{
  "chapter": {
    "id": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972",
    "number": 69,
    "ref": "https://www.bible.com/bible/19/PSA.69.BD2011",
    "createdAt": "2025-04-24T01:29:06.625Z",
    "updatedAt": "2025-04-24T01:29:06.625Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "27fd437e-6c77-412e-958d-656a5f3f09bb",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin cứu con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.714Z",
      "updatedAt": "2025-04-24T01:55:15.714Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "9e2ab4b0-9c7d-4bf1-83a9-5994cf4e9e2a",
      "number": 1,
      "content": "Vì các dòng nước đã ngập đến cổ con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.742Z",
      "updatedAt": "2025-04-24T01:55:15.742Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "4d6d61d7-5d8d-45b8-a095-bbede3bc8ecd",
      "number": 2,
      "content": "Con đã bị lún sâu trong một vũng lầy;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.752Z",
      "updatedAt": "2025-04-24T01:55:15.752Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "5676b7f9-eb51-414c-8c11-992aa28d80af",
      "number": 2,
      "content": "Thật là một vũng lầy không đáy.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.764Z",
      "updatedAt": "2025-04-24T01:55:15.764Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "9b1413f4-fea5-412e-a95f-1247c48e2133",
      "number": 2,
      "content": "Con bị rơi vào giữa dòng nước sâu;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.775Z",
      "updatedAt": "2025-04-24T01:55:15.775Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "e81aba85-2426-4334-9cdd-2513ca31f1b6",
      "number": 2,
      "content": "Dòng cuồng lưu cuốn hút lấy con.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.788Z",
      "updatedAt": "2025-04-24T01:55:15.788Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "c62bd8ac-e64e-4b05-a06e-88744256266a",
      "number": 3,
      "content": "Con kêu van đến kiệt lực;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.802Z",
      "updatedAt": "2025-04-24T01:55:15.802Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "41be1b7c-fe68-4a08-89ee-790c99964686",
      "number": 3,
      "content": "Cổ họng con đã gần tắt tiếng;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.815Z",
      "updatedAt": "2025-04-24T01:55:15.815Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "d31650c9-e555-4cad-884d-f565b5114abd",
      "number": 3,
      "content": "Mắt con đã lờ đờ,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.829Z",
      "updatedAt": "2025-04-24T01:55:15.829Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "c7ae13c4-d0f2-4cde-815c-d66df76e4e34",
      "number": 3,
      "content": "Trong khi trông đợi Đức Chúa Trời của con.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.842Z",
      "updatedAt": "2025-04-24T01:55:15.842Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "8da6927d-5a83-4912-88c5-768939577693",
      "number": 4,
      "content": "Những kẻ ghét con vô cớ",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.856Z",
      "updatedAt": "2025-04-24T01:55:15.856Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "22b7a46c-a174-4205-a6c7-cc3ab2b96e60",
      "number": 4,
      "content": "Thật nhiều hơn tóc trên đầu con;",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.869Z",
      "updatedAt": "2025-04-24T01:55:15.869Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "ad6d0f08-9f76-4389-ab26-a4d8ab431b5a",
      "number": 4,
      "content": "Những kẻ muốn tiêu diệt con thật mạnh mẽ thay!",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.883Z",
      "updatedAt": "2025-04-24T01:55:15.883Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "a130cfac-fd99-4a77-80db-83984bdcf6e7",
      "number": 4,
      "content": "Đó là những kẻ thù ghét con vô cớ.",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.896Z",
      "updatedAt": "2025-04-24T01:55:15.896Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "1af8fd0c-a5ac-4174-9ec2-2a19089f40d3",
      "number": 4,
      "content": "Con đã bị bắt bồi thường",
      "order": 4,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.910Z",
      "updatedAt": "2025-04-24T01:55:15.910Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "0a66b762-0827-4a62-bcef-abf9a0f2dfb1",
      "number": 4,
      "content": "Cho những gì con không lấy.",
      "order": 5,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.923Z",
      "updatedAt": "2025-04-24T01:55:15.923Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "5ba84630-aa99-4d15-96f9-f00993924f03",
      "number": 5,
      "content": "Đức Chúa Trời ôi, Ngài đã biết sự dại dột của con;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.937Z",
      "updatedAt": "2025-04-24T01:55:15.937Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "6054e087-a314-4d7b-969f-70d7e2b82455",
      "number": 5,
      "content": "Những tội lỗi của con chẳng giấu được Ngài.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.950Z",
      "updatedAt": "2025-04-24T01:55:15.950Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "ae0b0da6-c892-4a0b-b4a1-bff24f38a630",
      "number": 6,
      "content": "Lạy Chúa, **CHÚA** các đạo quân!",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.964Z",
      "updatedAt": "2025-04-24T01:55:15.964Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "e855fbdc-33c3-42f1-938f-9cda4e62d4fb",
      "number": 6,
      "content": "Nguyện ai trông cậy Ngài sẽ không vì con mà hổ thẹn;",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.977Z",
      "updatedAt": "2025-04-24T01:55:15.977Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "8c9ade7d-f3ea-48e7-8a1a-a021572e06e1",
      "number": 6,
      "content": "Đức Chúa Trời của I-sơ-ra-ên ôi,",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:15.990Z",
      "updatedAt": "2025-04-24T01:55:15.990Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "8e3c4778-5ada-41f9-a3ce-c41d3aaaf619",
      "number": 6,
      "content": "Nguyện những ai tìm kiếm Ngài sẽ không bị sỉ nhục vì con.",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.003Z",
      "updatedAt": "2025-04-24T01:55:16.003Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "c69949b5-d072-4b58-8103-ce932b8cf0e5",
      "number": 7,
      "content": "Vì cớ Ngài con bị người ta mắng nhiếc;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.017Z",
      "updatedAt": "2025-04-24T01:55:16.017Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "4f29afd0-32b2-4a78-98f3-d0072cfd16e7",
      "number": 7,
      "content": "Mặt con chẳng còn thể diện gì nữa.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.030Z",
      "updatedAt": "2025-04-24T01:55:16.030Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "4878aff5-57e3-4577-964f-e89561a4f1d3",
      "number": 8,
      "content": "Con đã trở thành kẻ xa lạ đối với anh chị em con,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.054Z",
      "updatedAt": "2025-04-24T01:55:16.054Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "4b0449a3-c290-4f90-816d-e02cc38c6623",
      "number": 8,
      "content": "Thành người ngoại tộc đối với các con cái của mẹ con;",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.080Z",
      "updatedAt": "2025-04-24T01:55:16.080Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "5002d3bf-8ee8-44d8-a961-11f1b668ff85",
      "number": 9,
      "content": "Vì lòng nhiệt thành về nhà Ngài đã thiêu đốt con,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.106Z",
      "updatedAt": "2025-04-24T01:55:16.106Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "5a7c88c8-dc60-493a-a5fa-2f8f87ec1c92",
      "number": 9,
      "content": "Những lời sỉ nhục của những kẻ sỉ nhục Ngài đã đổ trên con.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.120Z",
      "updatedAt": "2025-04-24T01:55:16.120Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "3655f681-1907-4b1f-b35d-e9fffb3c2ada",
      "number": 10,
      "content": "Khi con than khóc và kiêng ép linh hồn,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.133Z",
      "updatedAt": "2025-04-24T01:55:16.133Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "eae7cce7-2dfb-4e87-af3c-112a5466472d",
      "number": 10,
      "content": "Con trở thành đối tượng cho người ta mắng nhiếc.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.145Z",
      "updatedAt": "2025-04-24T01:55:16.145Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "835379ae-b871-4be2-8be3-8a0a43c534c5",
      "number": 11,
      "content": "Khi con mặc bao gai,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.157Z",
      "updatedAt": "2025-04-24T01:55:16.157Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "07e929e6-226f-4efe-9bdf-982173e1df62",
      "number": 11,
      "content": "Con trở thành đề tài giễu cợt cho người ta.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.169Z",
      "updatedAt": "2025-04-24T01:55:16.169Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "51a17779-e4c0-48c3-bb5f-2aed03d4324c",
      "number": 12,
      "content": "Những viên chức ngồi ở cổng thành chế giễu con;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.180Z",
      "updatedAt": "2025-04-24T01:55:16.180Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "58a167f4-2d5d-4406-9f92-440f3434f689",
      "number": 12,
      "content": "Con trở thành câu hát cho những kẻ say sưa châm biếm.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.191Z",
      "updatedAt": "2025-04-24T01:55:16.191Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "c81f313c-a154-49be-a033-0e935475aec2",
      "number": 13,
      "content": "Nhưng phần con, **CHÚA** ôi, con vẫn cầu nguyện với Ngài;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.201Z",
      "updatedAt": "2025-04-24T01:55:16.201Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "22aba81f-6494-4ac3-89e3-a9cb2ca808b4",
      "number": 13,
      "content": "Đức Chúa Trời ôi, để trong lúc Ngài gia ân ban phước,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.211Z",
      "updatedAt": "2025-04-24T01:55:16.211Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "0f670428-7fb7-4208-a9b9-687622fd7324",
      "number": 13,
      "content": "Theo tình thương lớn lao của Ngài, Ngài đáp lời con bằng chân lý cứu rỗi của Ngài.",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.221Z",
      "updatedAt": "2025-04-24T01:55:16.221Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "774514a5-0590-4b6f-9ff6-dc209b1c9aa5",
      "number": 14,
      "content": "Xin cứu con khỏi chốn bùn lầy;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.237Z",
      "updatedAt": "2025-04-24T01:55:16.237Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "7c60788b-003b-4fc7-b59b-21e1ce6394f6",
      "number": 14,
      "content": "Xin đừng để con bị lún chìm xuống nữa;",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.247Z",
      "updatedAt": "2025-04-24T01:55:16.247Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "f95bbcb3-f771-4eae-ad35-e564c1ace109",
      "number": 14,
      "content": "Xin giải cứu con khỏi những kẻ ghét con,",
      "order": 2,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.257Z",
      "updatedAt": "2025-04-24T01:55:16.257Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "733eed21-c567-4fbc-b0a5-ab384cd6190d",
      "number": 14,
      "content": "Và khỏi những chỗ nước sâu.",
      "order": 3,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.270Z",
      "updatedAt": "2025-04-24T01:55:16.270Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "c39cc366-6a55-404c-9bd2-ce0a805a9f35",
      "number": 15,
      "content": "Xin đừng để dòng cuồng lưu cuốn hút lấy con;",
      "order": 0,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.280Z",
      "updatedAt": "2025-04-24T01:55:16.280Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "3c8e8dfe-8a5b-484b-969b-68a44e291ef5",
      "number": 15,
      "content": "Xin đừng để vực sâu nuốt chửng con;",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.292Z",
      "updatedAt": "2025-04-24T01:55:16.292Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "da1b880c-e829-4763-94a0-0bb2de3dc015",
      "number": 15,
      "content": "Và xin đừng để vực thẳm khép miệng lại trên con.",
      "order": 2,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.303Z",
      "updatedAt": "2025-04-24T01:55:16.303Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "cc7be91b-53ab-4790-89f5-1474ce2f2917",
      "number": 16,
      "content": "Xin nhậm lời con, **CHÚA** ôi, vì tình yêu của Ngài thật tuyệt vời;",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.312Z",
      "updatedAt": "2025-04-24T01:55:16.312Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "3364d23e-f1e6-4c2a-a782-1559ec05e998",
      "number": 16,
      "content": "Xin quay lại với con theo ơn thương xót lớn lao của Ngài.",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.322Z",
      "updatedAt": "2025-04-24T01:55:16.322Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "d3910587-386c-4aa6-b238-1eca13273a15",
      "number": 17,
      "content": "Xin đừng ẩn mặt Ngài đối với tôi tớ Ngài;",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.332Z",
      "updatedAt": "2025-04-24T01:55:16.332Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "8fb9b23c-828f-4cdb-a37d-d7b75d69efd6",
      "number": 17,
      "content": "Xin mau đáp lời cầu nguyện của con vì con đang gặp hoạn nạn.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.342Z",
      "updatedAt": "2025-04-24T01:55:16.342Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "1e2374ae-408b-4d7d-a952-e623ff41391f",
      "number": 18,
      "content": "Xin đến gần linh hồn con và cứu con;",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.353Z",
      "updatedAt": "2025-04-24T01:55:16.353Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "2bfb2ef5-5739-4b2f-b3df-71629241542b",
      "number": 18,
      "content": "Xin giải cứu con vì những kẻ thù của con.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.366Z",
      "updatedAt": "2025-04-24T01:55:16.366Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "8ca5c8ed-78d1-4dba-8bdc-890023c21ecb",
      "number": 19,
      "content": "Ngài biết con bị mắng nhiếc, sỉ nhục,",
      "order": 0,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.379Z",
      "updatedAt": "2025-04-24T01:55:16.379Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "84e2ca21-3515-43c2-ad5b-78ee3002fb6c",
      "number": 19,
      "content": "Và xấu hổ đến thế nào rồi.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.392Z",
      "updatedAt": "2025-04-24T01:55:16.392Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "7216bd28-e832-4a3a-93a1-f0b4aa8e28e1",
      "number": 19,
      "content": "Tất cả kẻ thù của con đều ở trước mặt Ngài.",
      "order": 2,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.406Z",
      "updatedAt": "2025-04-24T01:55:16.406Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "78c619ed-36f5-492d-b608-406dc10aa92a",
      "number": 20,
      "content": "Những lời mắng nhiếc của chúng làm tan nát lòng con.",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.420Z",
      "updatedAt": "2025-04-24T01:55:16.420Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "3756c174-d3e9-4ae2-a745-59c4f986308e",
      "number": 20,
      "content": "Con đau đớn lắm.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.434Z",
      "updatedAt": "2025-04-24T01:55:16.434Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "afbe6890-6180-4ac5-a69e-8a91718f3735",
      "number": 20,
      "content": "Con mong có được người cảm thương, nhưng chẳng có ai.",
      "order": 2,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.447Z",
      "updatedAt": "2025-04-24T01:55:16.447Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "d6bd4f86-5ef9-4d1e-8f24-f66a4cd93b4a",
      "number": 20,
      "content": "Con ước ao được có người an ủi, nhưng nào đâu thấy.",
      "order": 3,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.461Z",
      "updatedAt": "2025-04-24T01:55:16.461Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "48a164ad-cc57-4cf2-a63e-15b96fe706ba",
      "number": 21,
      "content": "Chúng đã đưa cho con mật đắng làm thức ăn.",
      "order": 0,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.475Z",
      "updatedAt": "2025-04-24T01:55:16.475Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "6ec38826-724e-405d-9185-1f1ef1a92b39",
      "number": 21,
      "content": "Khi con khát chúng đã trao giấm chua cho con làm thức uống.",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.489Z",
      "updatedAt": "2025-04-24T01:55:16.489Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "937e4099-cf78-4dda-b2d8-e7c8b37e8c25",
      "number": 22,
      "content": "Nguyện bàn tiệc của chúng trở thành một cái bẫy.",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.502Z",
      "updatedAt": "2025-04-24T01:55:16.502Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "6e221a20-de55-4efe-8b24-55cd0297f296",
      "number": 22,
      "content": "Nguyện nó thành một hình phạt đích đáng và một cạm bẫy.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.517Z",
      "updatedAt": "2025-04-24T01:55:16.517Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "f4dba52d-a3b1-4d14-a09a-ee316fc74fa9",
      "number": 23,
      "content": "Nguyện mắt chúng bị đui mù, chẳng còn trông thấy nữa.",
      "order": 0,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.530Z",
      "updatedAt": "2025-04-24T01:55:16.530Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "cc301dbc-84c7-4fd0-a05c-b05f23e69580",
      "number": 23,
      "content": "Nguyện lưng chúng cụp xuống và run rẩy luôn luôn.",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.544Z",
      "updatedAt": "2025-04-24T01:55:16.544Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "16c175ea-acbf-4d78-b621-b79da6d8afc3",
      "number": 24,
      "content": "Xin Ngài trút cơn thịnh nộ của Ngài trên chúng;",
      "order": 0,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.558Z",
      "updatedAt": "2025-04-24T01:55:16.558Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "8c7f5d02-52d2-4800-b383-be26e69f4e30",
      "number": 24,
      "content": "Xin Ngài đổ cơn giận của Ngài xuống đầu chúng.",
      "order": 1,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.571Z",
      "updatedAt": "2025-04-24T01:55:16.571Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "7590e02a-69dd-40b1-9f37-123e81180942",
      "number": 25,
      "content": "Nguyện chỗ ở của chúng bị bỏ hoang;",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.584Z",
      "updatedAt": "2025-04-24T01:55:16.584Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "53c09648-77f1-44d0-8461-44711c255c4b",
      "number": 25,
      "content": "Nguyện trại của chúng chẳng còn ai ở,",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.598Z",
      "updatedAt": "2025-04-24T01:55:16.598Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "31057bcf-059d-4df6-b8f7-c66dcc10a6fe",
      "number": 26,
      "content": "Bởi vì chúng bách hại người bị Ngài sửa phạt,",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.612Z",
      "updatedAt": "2025-04-24T01:55:16.612Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "224cf8bc-4e39-4b99-a560-a2318c3fea19",
      "number": 26,
      "content": "Chúng làm cho đau đớn thêm người bị Ngài làm cho đau.",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.625Z",
      "updatedAt": "2025-04-24T01:55:16.625Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "a0509c4a-6107-465c-9f91-adf70aeb4d7f",
      "number": 27,
      "content": "Xin Ngài cộng thêm tội ấy vào các tội khác của chúng;",
      "order": 0,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.639Z",
      "updatedAt": "2025-04-24T01:55:16.639Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "7727a3e8-22b7-4085-8f92-1ffd7831dbce",
      "number": 27,
      "content": "Xin đừng cho chúng hưởng sự công chính của Ngài.",
      "order": 1,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.652Z",
      "updatedAt": "2025-04-24T01:55:16.652Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "97115ff0-cb81-4e77-96bc-8d6315df17cf",
      "number": 28,
      "content": "Nguyện chúng bị xóa tên khỏi sách sự sống,",
      "order": 0,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.666Z",
      "updatedAt": "2025-04-24T01:55:16.666Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "2b1303d4-52f7-475c-a2af-c566add73af8",
      "number": 28,
      "content": "Và không được ghi tên trong danh sách những người ngay lành.",
      "order": 1,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.692Z",
      "updatedAt": "2025-04-24T01:55:16.692Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "24354f0a-8601-4d2d-a876-767f5d06da63",
      "number": 29,
      "content": "Còn con, con bị khốn cùng và đau khổ;",
      "order": 0,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.706Z",
      "updatedAt": "2025-04-24T01:55:16.706Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "740be203-30b6-47ee-a08d-b1acec38ed06",
      "number": 29,
      "content": "Đức Chúa Trời ôi, nguyện ơn giải cứu của Ngài đặt con lên nơi an toàn trên cao.",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.719Z",
      "updatedAt": "2025-04-24T01:55:16.719Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "a832f562-6063-40f8-b86d-99b16f4fa1db",
      "number": 30,
      "content": "Tôi sẽ dùng bài hát để ca ngợi danh Đức Chúa Trời;",
      "order": 0,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.733Z",
      "updatedAt": "2025-04-24T01:55:16.733Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "9cbb2cc0-4592-4513-a646-4b7ad121a6b7",
      "number": 30,
      "content": "Tôi sẽ tán dương Ngài bằng sự tạ ơn.",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.750Z",
      "updatedAt": "2025-04-24T01:55:16.750Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "8786991c-b666-4b7a-b4bb-143f38db21d0",
      "number": 31,
      "content": "Điều ấy sẽ đẹp lòng **CHÚA** hơn dâng một con bò đực;",
      "order": 0,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.763Z",
      "updatedAt": "2025-04-24T01:55:16.763Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "979e0e41-7baf-4e42-a5f3-0d85ce593e61",
      "number": 31,
      "content": "Thật hơn dâng một con bò đực có đủ móng và đủ sừng.",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.777Z",
      "updatedAt": "2025-04-24T01:55:16.777Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "527b7ffa-a940-4fde-a00b-dcc271d04882",
      "number": 32,
      "content": "Hỡi những người khiêm hạ, hãy thấy điều đó mà vui mừng;",
      "order": 0,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.791Z",
      "updatedAt": "2025-04-24T01:55:16.791Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "500349c7-75d7-4063-b5c3-e306e7720219",
      "number": 32,
      "content": "Hỡi các bạn là những người tìm kiếm Đức Chúa Trời, hãy nức lòng phấn khởi.",
      "order": 1,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.805Z",
      "updatedAt": "2025-04-24T01:55:16.805Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "653e4955-9f19-4bc1-b5ed-bb088055fd7d",
      "number": 33,
      "content": "Vì **CHÚA** lắng nghe những người khốn khổ;",
      "order": 0,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.819Z",
      "updatedAt": "2025-04-24T01:55:16.819Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "2ef9e18d-5d28-4ba4-8225-2b24281337ff",
      "number": 33,
      "content": "Ngài không khinh khi con dân Ngài khi họ bị tù đày.",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.832Z",
      "updatedAt": "2025-04-24T01:55:16.832Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "d9a56f43-75d0-46cc-9450-a12ece51fddc",
      "number": 34,
      "content": "Nguyện trời và đất ca ngợi Ngài;",
      "order": 0,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.846Z",
      "updatedAt": "2025-04-24T01:55:16.846Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "d1be82f1-a0be-4940-8149-5fcb10fcc1b6",
      "number": 34,
      "content": "Nguyện biển và mọi vật trong biển ca tụng Ngài.",
      "order": 1,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.860Z",
      "updatedAt": "2025-04-24T01:55:16.860Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "80b6b695-406b-4981-beb0-36deeb1b3932",
      "number": 35,
      "content": "Vì Đức Chúa Trời sẽ cứu Si-ôn;",
      "order": 0,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.874Z",
      "updatedAt": "2025-04-24T01:55:16.874Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "6a05e3e8-cae4-4196-b679-728db6d395f2",
      "number": 35,
      "content": "Ngài sẽ xây dựng lại những thành trì của Giu-đa;",
      "order": 1,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.887Z",
      "updatedAt": "2025-04-24T01:55:16.887Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "d58113a3-bcc2-48bf-a267-6abda7c9f3ca",
      "number": 35,
      "content": "Rồi người ta sẽ định cư ở đó và lập nghiệp trong xứ ấy.",
      "order": 2,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.901Z",
      "updatedAt": "2025-04-24T01:55:16.901Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "a169fd8e-f8a2-446e-a65f-3349ba9e90f2",
      "number": 36,
      "content": "Con cháu của các tôi tớ Ngài sẽ thừa hưởng xứ đó,",
      "order": 0,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.914Z",
      "updatedAt": "2025-04-24T01:55:16.914Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    },
    {
      "id": "36ba504b-aaaa-43ed-921a-56a06a7a9fc7",
      "number": 36,
      "content": "Và những ai yêu mến danh Ngài sẽ cư ngụ trong đó.",
      "order": 1,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:16.927Z",
      "updatedAt": "2025-04-24T01:55:16.927Z",
      "chapterId": "98bbe9c9-b11a-4ea4-8a27-8ecb5d569972"
    }
  ],
