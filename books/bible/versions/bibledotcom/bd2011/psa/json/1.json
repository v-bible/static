{
  "chapter": {
    "id": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e",
    "number": 1,
    "ref": "https://www.bible.com/bible/19/PSA.1.BD2011",
    "createdAt": "2025-04-24T01:29:05.760Z",
    "updatedAt": "2025-04-24T01:29:05.760Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "d712ecbc-35ce-40b5-b596-c8098c3920c7",
      "number": 1,
      "content": "Phước cho người",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.769Z",
      "updatedAt": "2025-04-24T01:50:48.769Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "cdd50fe1-33d8-46b4-b551-54019a8c2a67",
      "number": 1,
      "content": "Không đi theo kế kẻ gian ác,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.796Z",
      "updatedAt": "2025-04-24T01:50:48.796Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "fc952927-91f2-406e-af5f-7c5e438c524a",
      "number": 1,
      "content": "Không đứng chung đường với phường tội lỗi,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.806Z",
      "updatedAt": "2025-04-24T01:50:48.806Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "90a0d008-40fa-4f2c-a8ad-05ddbb2ec1b2",
      "number": 1,
      "content": "Không ngồi cùng chỗ với đồ nhạo báng,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.816Z",
      "updatedAt": "2025-04-24T01:50:48.816Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "832d923b-ee44-402d-aca9-84a61407788e",
      "number": 2,
      "content": "Nhưng vui thích về luật pháp của **CHÚA**,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.827Z",
      "updatedAt": "2025-04-24T01:50:48.827Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "c120e1a0-fadf-41f0-9875-64758f90973d",
      "number": 2,
      "content": "Và suy gẫm luật pháp ấy ngày và đêm.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.837Z",
      "updatedAt": "2025-04-24T01:50:48.837Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "8b4d31d7-dfb7-4101-ab18-eec2f8354c00",
      "number": 3,
      "content": "Người ấy sẽ như cây",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.847Z",
      "updatedAt": "2025-04-24T01:50:48.847Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "517b98ee-e389-4a7f-9f65-ac332ed6047a",
      "number": 3,
      "content": "Trồng gần dòng nước,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.857Z",
      "updatedAt": "2025-04-24T01:50:48.857Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "444d57f2-99a1-44ce-aa78-d9b5c43337cb",
      "number": 3,
      "content": "Cứ đến mùa được hoa quả đầy cành,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.869Z",
      "updatedAt": "2025-04-24T01:50:48.869Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "33d00d63-4c66-471c-a577-cf58ab8c9a57",
      "number": 3,
      "content": "Lá vẫn xanh chứ chẳng hề héo úa;",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.888Z",
      "updatedAt": "2025-04-24T01:50:48.888Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "09a73e49-3eff-4767-801d-9f6c3f31d070",
      "number": 3,
      "content": "Mọi sự người ấy làm đều sẽ thành công.",
      "order": 4,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.899Z",
      "updatedAt": "2025-04-24T01:50:48.899Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "a0b5e328-bf20-46ae-9e5f-c5d69bc76bca",
      "number": 4,
      "content": "Những kẻ gian ác sẽ không được như vậy,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.909Z",
      "updatedAt": "2025-04-24T01:50:48.909Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "e4c85125-aada-4a08-9a60-92872aedf218",
      "number": 4,
      "content": "Nhưng chúng chẳng khác gì rác rến bị gió đùa đi.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.919Z",
      "updatedAt": "2025-04-24T01:50:48.919Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "db5f6ec2-f216-47dd-8fa0-20c58811cec0",
      "number": 5,
      "content": "Vì thế những kẻ gian ác sẽ chẳng đứng nổi trong ngày phán xét;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.928Z",
      "updatedAt": "2025-04-24T01:50:48.928Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "c9a86e5e-c6d4-4db4-a64a-3e44c19e6128",
      "number": 5,
      "content": "Những kẻ tội lỗi sẽ không được vào hội người ngay lành;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.939Z",
      "updatedAt": "2025-04-24T01:50:48.939Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "23ec0375-4a6b-4d21-917c-d886bb2c4b21",
      "number": 6,
      "content": "Vì **CHÚA** biết rõ đường lối người ngay lành,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.949Z",
      "updatedAt": "2025-04-24T01:50:48.949Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    },
    {
      "id": "94e72d41-8270-41a0-b898-603815f8d66b",
      "number": 6,
      "content": "Còn đường lối kẻ gian ác sẽ dẫn đến diệt vong.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:50:48.959Z",
      "updatedAt": "2025-04-24T01:50:48.959Z",
      "chapterId": "9216d8b3-a571-4d2f-9d6c-f6fb3ef6130e"
    }
  ],
