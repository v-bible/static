{
  "chapter": {
    "id": "62139b96-d55b-4ea9-b70d-b395d79624ef",
    "number": 1,
    "ref": "https://www.bible.com/bible/19/PSA.1.BD2011",
    "createdAt": "2025-04-22T10:10:25.592Z",
    "updatedAt": "2025-04-22T10:10:25.592Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "6ab41b5f-1e2e-4ab3-9acb-d1adacaf3862",
      "number": 1,
      "content": "Phước cho người",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:02.961Z",
      "updatedAt": "2025-04-22T10:35:02.961Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "26879e44-e7d3-4e10-9ad0-2e1162c36313",
      "number": 1,
      "content": "Không đi theo kế kẻ gian ác,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:02.988Z",
      "updatedAt": "2025-04-22T10:35:02.988Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "db73321b-afac-4c66-8e00-2a87442ab8ea",
      "number": 1,
      "content": "Không đứng chung đường với phường tội lỗi,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03Z",
      "updatedAt": "2025-04-22T10:35:03Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "9d902e9e-2e76-4272-9497-fe9c32f91c70",
      "number": 1,
      "content": "Không ngồi cùng chỗ với đồ nhạo báng,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.019Z",
      "updatedAt": "2025-04-22T10:35:03.019Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "0bc03c6f-4e0b-47a8-a445-c857b8fe1da1",
      "number": 2,
      "content": "Nhưng vui thích về luật pháp của **CHÚA**,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.032Z",
      "updatedAt": "2025-04-22T10:35:03.032Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "b4f905db-b043-455a-98a3-b6265f7289b5",
      "number": 2,
      "content": "Và suy gẫm luật pháp ấy ngày và đêm.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.044Z",
      "updatedAt": "2025-04-22T10:35:03.044Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "f459f02e-0557-4960-b689-0fcefea96e77",
      "number": 3,
      "content": "Người ấy sẽ như cây",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.057Z",
      "updatedAt": "2025-04-22T10:35:03.057Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "372d02e6-3e06-4925-b1e0-88cfb67a513e",
      "number": 3,
      "content": "Trồng gần dòng nước,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.071Z",
      "updatedAt": "2025-04-22T10:35:03.071Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "527615ad-ba3e-40a5-8b4c-fdc51294bfde",
      "number": 3,
      "content": "Cứ đến mùa được hoa quả đầy cành,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.084Z",
      "updatedAt": "2025-04-22T10:35:03.084Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "318098c9-af77-4c07-a0fa-3f650c5eafd8",
      "number": 3,
      "content": "Lá vẫn xanh chứ chẳng hề héo úa;",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.097Z",
      "updatedAt": "2025-04-22T10:35:03.097Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "fa1f14d3-4c00-4e9a-b86b-594d124e1c09",
      "number": 3,
      "content": "Mọi sự người ấy làm đều sẽ thành công.",
      "order": 4,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.110Z",
      "updatedAt": "2025-04-22T10:35:03.110Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "1a3913d3-298c-443b-b936-5df7a040e7e4",
      "number": 4,
      "content": "Những kẻ gian ác sẽ không được như vậy,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.122Z",
      "updatedAt": "2025-04-22T10:35:03.122Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "bdcd23d0-f46c-439a-bde8-883cd42ff9bb",
      "number": 4,
      "content": "Nhưng chúng chẳng khác gì rác rến bị gió đùa đi.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.139Z",
      "updatedAt": "2025-04-22T10:35:03.139Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "2f34ef02-4008-49a8-8661-56982cc374ff",
      "number": 5,
      "content": "Vì thế những kẻ gian ác sẽ chẳng đứng nổi trong ngày phán xét;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.150Z",
      "updatedAt": "2025-04-22T10:35:03.150Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "f5adf220-57f1-41f8-aaa1-06d2380a376e",
      "number": 5,
      "content": "Những kẻ tội lỗi sẽ không được vào hội người ngay lành;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.161Z",
      "updatedAt": "2025-04-22T10:35:03.161Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "a4232ed7-4656-4dd1-827f-00e59210dd80",
      "number": 6,
      "content": "Vì **CHÚA** biết rõ đường lối người ngay lành,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.176Z",
      "updatedAt": "2025-04-22T10:35:03.176Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    },
    {
      "id": "071c8a9f-95a4-4255-9411-eb331d9a0ef9",
      "number": 6,
      "content": "Còn đường lối kẻ gian ác sẽ dẫn đến diệt vong.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:03.189Z",
      "updatedAt": "2025-04-22T10:35:03.189Z",
      "chapterId": "62139b96-d55b-4ea9-b70d-b395d79624ef"
    }
  ],
