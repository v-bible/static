    },
    {
      "id": "3ce76b87-2b82-4535-80c9-0a56ef4f8b75",
      "number": 19,
      "content": "Nguyện các dân bị xét xử trước mặt Ngài.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
