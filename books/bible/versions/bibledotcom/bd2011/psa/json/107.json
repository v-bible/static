{
  "chapter": {
    "id": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa",
    "number": 107,
    "ref": "https://www.bible.com/bible/19/PSA.107.BD2011",
    "createdAt": "2025-04-22T10:10:26.827Z",
    "updatedAt": "2025-04-22T10:10:26.827Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "5f20e9ce-9e99-4154-a5f1-94b32bb9546b",
      "number": 1,
      "content": "Hãy cảm tạ **CHÚA**, vì Ngài thật tốt,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.580Z",
      "updatedAt": "2025-04-22T10:42:38.580Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "5873a93a-f389-4587-b671-7f94181d6970",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.634Z",
      "updatedAt": "2025-04-22T10:42:38.634Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "5d806cf9-2cb2-409c-a820-a51bbb5cfa9b",
      "number": 2,
      "content": "Nguyện những người được cứu chuộc của **CHÚA** nói như vậy,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.644Z",
      "updatedAt": "2025-04-22T10:42:38.644Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "6c0a171a-1a5f-4b9c-8c5c-a15079107eaf",
      "number": 2,
      "content": "Những người được Ngài cứu khỏi tay kẻ thù,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.653Z",
      "updatedAt": "2025-04-22T10:42:38.653Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "72bd39d4-2154-4a23-8b49-6739cacb52b2",
      "number": 3,
      "content": "Rồi gom nhóm họ từ các nước trở về;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.664Z",
      "updatedAt": "2025-04-22T10:42:38.664Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "e43aaaff-bd1b-4da7-967b-8b6af8f460ca",
      "number": 3,
      "content": "Từ phương đông và phương tây,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.674Z",
      "updatedAt": "2025-04-22T10:42:38.674Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "64c7fcc9-cdee-4266-b020-8c3da1c66acc",
      "number": 3,
      "content": "Phương bắc và phương nam.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.684Z",
      "updatedAt": "2025-04-22T10:42:38.684Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "15a59d46-1e46-4856-a070-af5095ac03c9",
      "number": 4,
      "content": "Họ lang thang trong đồng hoang, trong nơi hoang vắng;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.697Z",
      "updatedAt": "2025-04-22T10:42:38.697Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "043b29b9-32c9-43ee-ab83-46c674575b05",
      "number": 4,
      "content": "Họ chẳng tìm được đường về một thành để ở.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.709Z",
      "updatedAt": "2025-04-22T10:42:38.709Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "e99fdc06-1a35-4557-9717-1fa58453c075",
      "number": 5,
      "content": "Họ đói khát;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.722Z",
      "updatedAt": "2025-04-22T10:42:38.722Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "66ebd7d1-22fa-4941-a7a3-198780707ab8",
      "number": 5,
      "content": "Linh hồn họ mòn mỏi nao sờn.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.735Z",
      "updatedAt": "2025-04-22T10:42:38.735Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "726ba541-1a76-4023-a0b8-f86247e2a430",
      "number": 6,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với **CHÚA**,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.748Z",
      "updatedAt": "2025-04-22T10:42:38.748Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "355d7ef2-f923-4790-8233-535fc8ba1721",
      "number": 13,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với **CHÚA**,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.939Z",
      "updatedAt": "2025-04-22T10:42:38.939Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "075fa8b5-5521-4f24-b782-bf7de96f3ac8",
      "number": 19,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với **CHÚA**,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.094Z",
      "updatedAt": "2025-04-22T10:42:39.094Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "d25b1c6d-e234-4db2-9dfa-751745148145",
      "number": 28,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với **CHÚA**,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.333Z",
      "updatedAt": "2025-04-22T10:42:39.333Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "6a54a045-3f52-4214-afba-f9b531416578",
      "number": 6,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.761Z",
      "updatedAt": "2025-04-22T10:42:38.761Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "a14fb11a-e647-405e-a207-40ad39d01fbc",
      "number": 13,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.955Z",
      "updatedAt": "2025-04-22T10:42:38.955Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "05e26999-cb8d-45b3-8de6-1e3dc7766833",
      "number": 19,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.109Z",
      "updatedAt": "2025-04-22T10:42:39.109Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "82029638-449e-4dcd-9150-4afb5eb73219",
      "number": 28,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.346Z",
      "updatedAt": "2025-04-22T10:42:39.346Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "29640fef-e03e-4f27-9216-c98b01e530ba",
      "number": 7,
      "content": "Ngài dẫn đưa họ vào đúng con đường phải đi,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.774Z",
      "updatedAt": "2025-04-22T10:42:38.774Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "89235c66-6e8b-4651-ad93-083054e20df3",
      "number": 7,
      "content": "Để họ đến một thành, một nơi để ở.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.787Z",
      "updatedAt": "2025-04-22T10:42:38.787Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "4b02457e-8e3a-4b19-8993-aa4d0b8acf7f",
      "number": 8,
      "content": "Hãy để họ cảm tạ **CHÚA** vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.800Z",
      "updatedAt": "2025-04-22T10:42:38.800Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "82865cff-454f-4080-8d0b-6e42e2af6366",
      "number": 15,
      "content": "Hãy để họ cảm tạ **CHÚA** vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.996Z",
      "updatedAt": "2025-04-22T10:42:38.996Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "63cc8323-3977-4915-8857-0482856a5e14",
      "number": 21,
      "content": "Hãy để họ cảm tạ **CHÚA** vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.143Z",
      "updatedAt": "2025-04-22T10:42:39.143Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "5fbf4bbd-0290-4a8b-9f4f-123f87c273e0",
      "number": 31,
      "content": "Hãy để họ cảm tạ **CHÚA** vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.418Z",
      "updatedAt": "2025-04-22T10:42:39.418Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "30d8f21c-b37e-4d90-9153-e4caa0517bff",
      "number": 8,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.813Z",
      "updatedAt": "2025-04-22T10:42:38.813Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "5ea5bc78-28d4-41c4-ae98-f37e7364121f",
      "number": 9,
      "content": "Vì Ngài làm thỏa mãn linh hồn khao khát,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.827Z",
      "updatedAt": "2025-04-22T10:42:38.827Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "5bd848ad-f11a-4460-86c8-84bd24d98630",
      "number": 9,
      "content": "Và làm no nê linh hồn đói lả bằng các thức ăn ngon.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.840Z",
      "updatedAt": "2025-04-22T10:42:38.840Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "463a674b-a4b9-430c-8036-939b67cb7a6e",
      "number": 10,
      "content": "Có những kẻ sống trong tối tăm và trong bóng tử thần,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.856Z",
      "updatedAt": "2025-04-22T10:42:38.856Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "56851022-8f9b-4358-b7e6-ce39644237c7",
      "number": 10,
      "content": "Bị giam cầm trong khổ đau và xiềng xích,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.870Z",
      "updatedAt": "2025-04-22T10:42:38.870Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "425f93d5-d78a-444e-a06d-bd8fa526f66a",
      "number": 11,
      "content": "Bởi vì họ chống lại những lời của Đức Chúa Trời,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.887Z",
      "updatedAt": "2025-04-22T10:42:38.887Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "55c146b5-b319-43dd-9017-17e68b012fc5",
      "number": 11,
      "content": "Và khinh bỉ lời chỉ bảo của Đấng Chí Cao.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.900Z",
      "updatedAt": "2025-04-22T10:42:38.900Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "51696daa-b42e-4e57-9d00-8621ad97250b",
      "number": 12,
      "content": "Vì thế Ngài lấy gian lao hạ lòng họ xuống;",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.913Z",
      "updatedAt": "2025-04-22T10:42:38.913Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "3f284961-618d-41f5-88eb-b31ee1cabd37",
      "number": 12,
      "content": "Họ bị ngã nhào và chẳng ai giúp họ đứng lên.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.926Z",
      "updatedAt": "2025-04-22T10:42:38.926Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "4845e7fd-92c9-4260-851b-425975b2cd34",
      "number": 14,
      "content": "Ngài đem họ ra khỏi cảnh tối tăm và bóng tử thần,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.969Z",
      "updatedAt": "2025-04-22T10:42:38.969Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "73ec2b18-fa0e-4820-96ea-58db1e784eb5",
      "number": 14,
      "content": "Và đập tan xiềng xích gông cùm trói buộc họ.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:38.982Z",
      "updatedAt": "2025-04-22T10:42:38.982Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "4ca3e430-57bb-46c6-96d8-52e0dbd5801c",
      "number": 15,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người,",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.009Z",
      "updatedAt": "2025-04-22T10:42:39.009Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "ccbc0a80-e30c-4351-a07a-0d1cae368cf7",
      "number": 16,
      "content": "Vì Ngài phá tan các cổng bằng đồng,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.022Z",
      "updatedAt": "2025-04-22T10:42:39.022Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "a6c23d8d-80d7-414c-bffc-53e1393a68de",
      "number": 16,
      "content": "Và Ngài cắt rời những song bằng sắt.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.036Z",
      "updatedAt": "2025-04-22T10:42:39.036Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "de3d3a34-d64d-4b96-848a-27bb801f4f22",
      "number": 17,
      "content": "Có những kẻ cứ bội nghịch nên hóa ra điên dại,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.049Z",
      "updatedAt": "2025-04-22T10:42:39.049Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "dec48e87-f5b0-4aaa-84b8-c56f51a0b0eb",
      "number": 17,
      "content": "Và vì tội lỗi mình nên phải chuốc lấy khổ đau.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.061Z",
      "updatedAt": "2025-04-22T10:42:39.061Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "e50c3ff1-1ca3-4479-b279-51725aad0db3",
      "number": 18,
      "content": "Linh hồn họ gớm ghê mọi thực phẩm,",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.072Z",
      "updatedAt": "2025-04-22T10:42:39.072Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "6e0e4b33-14db-47a6-b8d5-725d6a0c1676",
      "number": 18,
      "content": "Và họ bị kéo gần đến cổng của tử thần.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.083Z",
      "updatedAt": "2025-04-22T10:42:39.083Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "edbd3d48-416a-4874-9779-dcbd38a94bc6",
      "number": 20,
      "content": "Ngài ban lời Ngài và chữa lành họ;",
      "order": 0,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.120Z",
      "updatedAt": "2025-04-22T10:42:39.120Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "e264294c-8b55-431c-9a96-4f955bd61673",
      "number": 20,
      "content": "Và giải cứu họ khỏi miệng của mộ phần.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.131Z",
      "updatedAt": "2025-04-22T10:42:39.131Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "cdad325c-f685-4f99-8fa9-5e65c30c1c03",
      "number": 21,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.155Z",
      "updatedAt": "2025-04-22T10:42:39.155Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "dace1d2f-5bc6-417e-a77f-a85c66f8f947",
      "number": 31,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.431Z",
      "updatedAt": "2025-04-22T10:42:39.431Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "915ba870-dbf9-4aec-8ed5-4edccbbf6ba7",
      "number": 22,
      "content": "Cũng hãy để họ dâng những của lễ cảm tạ lên Ngài,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.169Z",
      "updatedAt": "2025-04-22T10:42:39.169Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "ce0b2b24-194b-4293-bcdd-bf22a7766827",
      "number": 22,
      "content": "Và thuật lại các công việc Ngài bằng những lời ca ngợi vui mừng.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.186Z",
      "updatedAt": "2025-04-22T10:42:39.186Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "aa6faf80-139f-4fc0-a63f-b705d1c85266",
      "number": 23,
      "content": "Có những kẻ xuống tàu vượt đại dương thăm thẳm,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.199Z",
      "updatedAt": "2025-04-22T10:42:39.199Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "772df91f-ab68-4a6b-ba95-539e80d9094c",
      "number": 23,
      "content": "Những người làm ăn trên biển cả mênh mông;",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.213Z",
      "updatedAt": "2025-04-22T10:42:39.213Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "ca4225b9-1392-4b45-80cd-39fa8f010f7c",
      "number": 24,
      "content": "Họ trông thấy những việc làm của **CHÚA**,",
      "order": 0,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.226Z",
      "updatedAt": "2025-04-22T10:42:39.226Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "ba80a13a-3ab1-45fa-a211-8fbbb4508ed1",
      "number": 24,
      "content": "Và những việc lạ lùng của Ngài ở giữa biển sâu.",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.239Z",
      "updatedAt": "2025-04-22T10:42:39.239Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "04e11f04-1f4a-475c-af44-8baacc821be9",
      "number": 25,
      "content": "Vì Ngài phán một tiếng, bão tố liền nổi lên cuồn cuộn,",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.253Z",
      "updatedAt": "2025-04-22T10:42:39.253Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "03a532f0-2294-4dd3-8d54-a0401d2b4d89",
      "number": 25,
      "content": "Cuồng phong nâng cao những ngọn sóng kinh hồn.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.266Z",
      "updatedAt": "2025-04-22T10:42:39.266Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "521847e7-b994-4df7-932c-bcd6aa8f8189",
      "number": 26,
      "content": "Sóng cả dâng cao đến tận trời, rồi trụt sâu xuống vực thẳm;",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.279Z",
      "updatedAt": "2025-04-22T10:42:39.279Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "eb478518-fdb3-466f-bc2f-91837474c09e",
      "number": 26,
      "content": "Giữa cảnh khốn cùng họ hồn phi phách tán.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.292Z",
      "updatedAt": "2025-04-22T10:42:39.292Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "22144b31-dc47-4f3a-86dd-0bed97dfff46",
      "number": 27,
      "content": "Họ bị quay cuồng điên đảo tựa người say;",
      "order": 0,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.306Z",
      "updatedAt": "2025-04-22T10:42:39.306Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "dca2cf36-d95c-49da-b3c8-13cc434af219",
      "number": 27,
      "content": "Mọi tài trí khôn ngoan đều biến mất.",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.319Z",
      "updatedAt": "2025-04-22T10:42:39.319Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "212e6e6a-4290-4f83-8adf-e710a235e024",
      "number": 29,
      "content": "Ngài khiến bão tố yên lặng như tờ;",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.360Z",
      "updatedAt": "2025-04-22T10:42:39.360Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "7189dff5-6df8-47b1-aaa8-37b9db222636",
      "number": 29,
      "content": "Những ngọn sóng biển lặng im phăng phắc.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.377Z",
      "updatedAt": "2025-04-22T10:42:39.377Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "2d6a3c17-4498-428f-9ab1-40e4deed20d8",
      "number": 30,
      "content": "Họ rất đỗi vui mừng vì biển êm gió lặng;",
      "order": 0,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.392Z",
      "updatedAt": "2025-04-22T10:42:39.392Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "901154d7-f225-49c1-9c36-50f2c802787f",
      "number": 30,
      "content": "Ngài lại dẫn đưa họ đến hải cảng họ ước mong.",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.405Z",
      "updatedAt": "2025-04-22T10:42:39.405Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "fc624502-e67a-4b9f-8ffd-d5a4a11d04f1",
      "number": 32,
      "content": "Hãy để họ tôn cao Ngài giữa đại hội đông người,",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.445Z",
      "updatedAt": "2025-04-22T10:42:39.445Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "632add49-61af-4591-b7c5-f6da5759cb56",
      "number": 32,
      "content": "Và ca tụng Ngài giữa hội đồng trưởng lão.",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.458Z",
      "updatedAt": "2025-04-22T10:42:39.458Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "9d069e40-4ed7-420a-a3a3-ab5d8464475e",
      "number": 33,
      "content": "Ngài biến các dòng sông thành sa mạc hoang vu,",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.478Z",
      "updatedAt": "2025-04-22T10:42:39.478Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "4dabe95d-7644-418f-8bbf-cafa00b25fa1",
      "number": 33,
      "content": "Các dòng suối thành đất khô cằn cỗi,",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.491Z",
      "updatedAt": "2025-04-22T10:42:39.491Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "abd93b20-7989-4e48-a5eb-215568fba44d",
      "number": 34,
      "content": "Đất đai màu mỡ thành đồng chua nước mặn,",
      "order": 0,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.505Z",
      "updatedAt": "2025-04-22T10:42:39.505Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "4c31a090-3a99-4f61-ad28-d4ba60367a4b",
      "number": 34,
      "content": "Vì sự gian ác của dân cư ở đó.",
      "order": 1,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.518Z",
      "updatedAt": "2025-04-22T10:42:39.518Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "c426b022-eeb7-4576-93ab-abcbdb1d27f1",
      "number": 35,
      "content": "Ngài biến đồng hoang thành đất có ao hồ,",
      "order": 0,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.532Z",
      "updatedAt": "2025-04-22T10:42:39.532Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "4ca50a32-e070-4fe1-a692-2631f90256ab",
      "number": 35,
      "content": "Và xứ khô cằn thành nơi có các dòng nước ngọt;",
      "order": 1,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.545Z",
      "updatedAt": "2025-04-22T10:42:39.545Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "cb312766-40f0-4f7e-b130-154557ff3024",
      "number": 36,
      "content": "Ngài đã cho những kẻ đói khát đến cư ngụ tại đó,",
      "order": 0,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.558Z",
      "updatedAt": "2025-04-22T10:42:39.558Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "a43e0136-8dc2-43a4-9d50-ab51800dc1fe",
      "number": 36,
      "content": "Để họ xây dựng một thành mà an cư lạc nghiệp;",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.571Z",
      "updatedAt": "2025-04-22T10:42:39.571Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "96a91a29-dc87-4fd1-9cf3-d2ce1a1aa6fe",
      "number": 37,
      "content": "Họ gieo hạt ngoài đồng và trồng tỉa vườn nho,",
      "order": 0,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.585Z",
      "updatedAt": "2025-04-22T10:42:39.585Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "793042de-9a02-44c3-8721-aa5186aae6ad",
      "number": 37,
      "content": "Rồi thu hoạch hoa màu sung túc.",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.599Z",
      "updatedAt": "2025-04-22T10:42:39.599Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "804dd67b-9102-4530-9ab9-e1cedf0a198c",
      "number": 38,
      "content": "Ngài ban phước cho họ, và dân số họ gia tăng;",
      "order": 0,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.613Z",
      "updatedAt": "2025-04-22T10:42:39.613Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "13cb1873-219b-4322-9059-8e5bd3566e33",
      "number": 38,
      "content": "Ngài chẳng để cho bầy súc vật của họ bị giảm thiểu.",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.626Z",
      "updatedAt": "2025-04-22T10:42:39.626Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "489679a4-128f-4a81-9d5d-3302bbcfd75f",
      "number": 39,
      "content": "Có lúc họ bị tiêu hao lụn bại, phải cúi đầu khuất phục ngoại bang,",
      "order": 0,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.639Z",
      "updatedAt": "2025-04-22T10:42:39.639Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "5aa80f85-51fe-4fbc-b425-bccdbecd4bab",
      "number": 39,
      "content": "Bị áp bức, khổ đau, và sầu não.",
      "order": 1,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.653Z",
      "updatedAt": "2025-04-22T10:42:39.653Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "6d11a669-550a-4028-8f23-520e60b89e3c",
      "number": 40,
      "content": "Ngài trút đổ ô nhục trên những người quyền quý của họ,",
      "order": 0,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.666Z",
      "updatedAt": "2025-04-22T10:42:39.666Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "64492e72-fad8-4a2e-951e-114cc6eb32ec",
      "number": 40,
      "content": "Khiến họ lang thang trong nơi hoang vắng không đường.",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.679Z",
      "updatedAt": "2025-04-22T10:42:39.679Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "78a82824-6d07-4b4b-bb33-a454aa37d8e2",
      "number": 41,
      "content": "Nhưng Ngài lại cất những người nghèo ra khỏi cảnh lầm than,",
      "order": 0,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.692Z",
      "updatedAt": "2025-04-22T10:42:39.692Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "e0d6252c-3499-4cc5-8cae-f0e37037a368",
      "number": 41,
      "content": "Và cho con cháu họ được gia tăng đông đúc.",
      "order": 1,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.705Z",
      "updatedAt": "2025-04-22T10:42:39.705Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "0889a2ba-f97c-4005-baa4-ec16859f66f5",
      "number": 42,
      "content": "Những người ngay lành thấy thế bèn vui mừng;",
      "order": 0,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.718Z",
      "updatedAt": "2025-04-22T10:42:39.718Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "d790fb83-81a0-4df7-96f4-87d1bb7ba207",
      "number": 42,
      "content": "Còn những kẻ gian ác thảy đều câm miệng lại.",
      "order": 1,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.731Z",
      "updatedAt": "2025-04-22T10:42:39.731Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "eac3bfc7-236b-4984-99fc-3e12a273547d",
      "number": 43,
      "content": "Hỡi ai là người khôn ngoan, hãy suy gẫm những điều ấy,",
      "order": 0,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.744Z",
      "updatedAt": "2025-04-22T10:42:39.744Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    },
    {
      "id": "2d513c8c-6d41-4a19-bd72-f3c766a8e93a",
      "number": 43,
      "content": "Rồi sẽ hiểu được thế nào là tình thương của **CHÚA**.",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:42:39.758Z",
      "updatedAt": "2025-04-22T10:42:39.758Z",
      "chapterId": "75e4dcc8-9bf6-48b9-83de-97f9de31f5aa"
    }
  ],
