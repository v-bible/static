{
  "chapter": {
    "id": "fe30e995-d2ca-4dac-970d-f2f85025711c",
    "number": 70,
    "ref": "https://www.bible.com/bible/19/PSA.70.BD2011",
    "createdAt": "2025-04-24T01:29:06.635Z",
    "updatedAt": "2025-04-24T01:29:06.635Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "bb368190-f82b-4661-98bc-a15d6bf2b28e",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin mau mau giải cứu con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.476Z",
      "updatedAt": "2025-04-24T01:55:19.476Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "1ae03366-1a56-4907-bc39-453fc73ffa4e",
      "number": 1,
      "content": "**CHÚA** ôi, xin nhanh chóng đến giúp đỡ con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.503Z",
      "updatedAt": "2025-04-24T01:55:19.503Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "408032b4-95b2-47fc-95f9-97f1bcbcbc27",
      "number": 2,
      "content": "Nguyện những kẻ tìm cách tiêu diệt con",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.513Z",
      "updatedAt": "2025-04-24T01:55:19.513Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "1ca8382f-c568-48ad-b3d0-8e10335d3606",
      "number": 2,
      "content": "Bị hổ thẹn và bối rối;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.525Z",
      "updatedAt": "2025-04-24T01:55:19.525Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "e2154f69-3022-4bb3-9776-0fddd83282f8",
      "number": 2,
      "content": "Nguyện những kẻ muốn làm tổn thương con",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.536Z",
      "updatedAt": "2025-04-24T01:55:19.536Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "6d94359a-fa01-466f-8948-34c07374de9d",
      "number": 2,
      "content": "Phải bỏ đi trong sỉ nhục.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.549Z",
      "updatedAt": "2025-04-24T01:55:19.549Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "c259f64d-1956-40c4-90ba-98b2ccb2f08c",
      "number": 3,
      "content": "Nguyện những kẻ nhạo cười con rằng, “Ha ha! Ha ha!”",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.563Z",
      "updatedAt": "2025-04-24T01:55:19.563Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "3ada739f-0b02-43e0-921f-015bbf8c59e1",
      "number": 3,
      "content": "Phải rút lui vì nhục nhã.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.576Z",
      "updatedAt": "2025-04-24T01:55:19.576Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "cdd527c1-e4fd-499b-b6ec-296cb238fa24",
      "number": 4,
      "content": "Nguyện những ai tìm kiếm Ngài",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.589Z",
      "updatedAt": "2025-04-24T01:55:19.589Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "4723769c-121a-438a-a26f-a4796e76039c",
      "number": 4,
      "content": "Vui vẻ và mừng rỡ trong Ngài;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.602Z",
      "updatedAt": "2025-04-24T01:55:19.602Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "4f2bd090-81ec-4336-aa59-df615bac9596",
      "number": 4,
      "content": "Nguyện những ai yêu mến ơn cứu rỗi của Ngài",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.615Z",
      "updatedAt": "2025-04-24T01:55:19.615Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "fd004719-a7d8-4360-a1ff-8c7bfb02e190",
      "number": 4,
      "content": "Cứ nói mãi, “Đức Chúa Trời vĩ đại thay!”",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.628Z",
      "updatedAt": "2025-04-24T01:55:19.628Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "408cefc9-5454-4881-9711-d30f3520abee",
      "number": 5,
      "content": "Còn con, con đang gặp cảnh khốn khổ và cùng đường;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.642Z",
      "updatedAt": "2025-04-24T01:55:19.642Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "d125e193-a241-46f3-a72e-150230185ab7",
      "number": 5,
      "content": "Đức Chúa Trời ôi, xin mau mau đến với con.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.655Z",
      "updatedAt": "2025-04-24T01:55:19.655Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "1540bc50-8d0e-476a-89e7-2efb540bfbd1",
      "number": 5,
      "content": "Ngài là Đấng Giúp Đỡ và Đấng Giải Cứu của con;",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.668Z",
      "updatedAt": "2025-04-24T01:55:19.668Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    },
    {
      "id": "29aa38e9-b431-495d-b91b-6b7ae4b48aa7",
      "number": 5,
      "content": "**CHÚA** ôi, xin đừng trì hoãn.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:19.682Z",
      "updatedAt": "2025-04-24T01:55:19.682Z",
      "chapterId": "fe30e995-d2ca-4dac-970d-f2f85025711c"
    }
  ],
