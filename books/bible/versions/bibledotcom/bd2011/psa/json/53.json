{
  "chapter": {
    "id": "70c9521d-c728-4caf-9d18-9857afcae94a",
    "number": 53,
    "ref": "https://www.bible.com/bible/19/PSA.53.BD2011",
    "createdAt": "2025-04-22T10:10:26.181Z",
    "updatedAt": "2025-04-22T10:10:26.181Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "f04dbf1a-1da7-4093-9414-eaf3629b93fd",
      "number": 1,
      "content": "Kẻ dại nói trong lòng rằng, “Không có Đức Chúa Trời!”",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.820Z",
      "updatedAt": "2025-04-22T10:38:41.820Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "5713a01a-72d8-4910-9266-898de04a5780",
      "number": 1,
      "content": "Chúng nó đều bại hoại; chúng nó phạm những tội lỗi đáng ghê tởm;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.840Z",
      "updatedAt": "2025-04-22T10:38:41.840Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "7105c968-95d6-49a6-8f38-7807bdcd2a92",
      "number": 1,
      "content": "Chẳng ai làm điều thiện.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.851Z",
      "updatedAt": "2025-04-22T10:38:41.851Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "eb0ec4db-4ceb-47f1-aa5a-ef74296cd604",
      "number": 2,
      "content": "Đức Chúa Trời từ trời",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.862Z",
      "updatedAt": "2025-04-22T10:38:41.862Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "3b898c31-686d-4716-9f7e-8e9877e04c3a",
      "number": 2,
      "content": "Nhìn xuống con cái loài người,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.875Z",
      "updatedAt": "2025-04-22T10:38:41.875Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "2153f90e-8024-4ca6-aad0-0508ed150899",
      "number": 2,
      "content": "Xem có ai là người trí thức,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.888Z",
      "updatedAt": "2025-04-22T10:38:41.888Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "60bfc676-0b16-41a0-af2e-b3b950d59de3",
      "number": 2,
      "content": "Biết tìm kiếm Đức Chúa Trời chăng.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.901Z",
      "updatedAt": "2025-04-22T10:38:41.901Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "c8c2636c-a1a6-4a44-b2e7-8ae6a8448ece",
      "number": 3,
      "content": "Họ thảy đều bại hoại;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.914Z",
      "updatedAt": "2025-04-22T10:38:41.914Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "28b5c445-a080-4d75-b566-14884e6c3903",
      "number": 3,
      "content": "Họ cùng nhau trở nên ô uế;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.928Z",
      "updatedAt": "2025-04-22T10:38:41.928Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "05433213-db96-408a-b00d-72cb7d68b82d",
      "number": 3,
      "content": "Chẳng có người nào làm điều thiện,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.939Z",
      "updatedAt": "2025-04-22T10:38:41.939Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "006b34a3-e65b-413e-993c-0dc87144179b",
      "number": 3,
      "content": "Dẫu một người cũng không.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.949Z",
      "updatedAt": "2025-04-22T10:38:41.949Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "4c936c69-6e5e-49f8-911d-96e4a69b6469",
      "number": 4,
      "content": "Tại sao những kẻ làm ác chẳng hiểu biết?",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.960Z",
      "updatedAt": "2025-04-22T10:38:41.960Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "faf9507c-9a6f-43ae-bd8c-6acd46153921",
      "number": 4,
      "content": "Tại sao chúng cứ ăn nuốt dân tôi như thể ăn bánh?",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.969Z",
      "updatedAt": "2025-04-22T10:38:41.969Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "30877d7a-c447-4a9e-b0f5-6a5d78759ca5",
      "number": 4,
      "content": "Tại sao chúng chẳng kêu cầu Đức Chúa Trời?",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.980Z",
      "updatedAt": "2025-04-22T10:38:41.980Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "711e9240-a895-4e27-a20f-9ad1f1143d4a",
      "number": 5,
      "content": "Chúng sẽ rất sợ hãi dầu chẳng có gì đáng sợ,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:41.989Z",
      "updatedAt": "2025-04-22T10:38:41.989Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "6d030c3f-ff3e-4442-b3ff-e8a9ba0cf17c",
      "number": 5,
      "content": "Vì Đức Chúa Trời sẽ rải xương những kẻ vây hãm bạn;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:42Z",
      "updatedAt": "2025-04-22T10:38:42Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "7264e841-5e62-45a5-8aa2-046df7216c76",
      "number": 5,
      "content": "Bạn làm cho chúng phải hổ thẹn,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:42.010Z",
      "updatedAt": "2025-04-22T10:38:42.010Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "aef04841-f182-409d-adfe-1f421fae7478",
      "number": 5,
      "content": "Vì Đức Chúa Trời đã loại bỏ chúng.",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:42.020Z",
      "updatedAt": "2025-04-22T10:38:42.020Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "9b95d472-1b04-4df6-bfd6-4ad9971716ea",
      "number": 6,
      "content": "Kìa, ơn cứu rỗi của I-sơ-ra-ên đến từ Si-ôn!",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:42.031Z",
      "updatedAt": "2025-04-22T10:38:42.031Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "45418589-d0f0-41f6-b3cf-81f16993fb0a",
      "number": 6,
      "content": "Khi Đức Chúa Trời tái lập vận mạng của con dân Ngài,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:42.046Z",
      "updatedAt": "2025-04-22T10:38:42.046Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    },
    {
      "id": "bcaf3776-8874-44db-8dc3-dd7301674c01",
      "number": 6,
      "content": "Gia-cốp sẽ mừng rỡ, I-sơ-ra-ên sẽ vui mừng.",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:38:42.074Z",
      "updatedAt": "2025-04-22T10:38:42.074Z",
      "chapterId": "70c9521d-c728-4caf-9d18-9857afcae94a"
    }
  ],
