  "verses": [
    {
      "id": "08a10e11-a826-45f6-8e60-a124e30778a7",
      "number": 1,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.075Z",
      "updatedAt": "2025-04-22T10:45:43.075Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "80f11da7-b0a7-432e-9eed-1d6128d0166e",
      "number": 6,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.262Z",
      "updatedAt": "2025-04-22T10:45:43.262Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "fa346f22-da65-4eab-91ab-bbfa3d88194e",
      "number": 1,
      "content": "Hãy ca ngợi Đức Chúa Trời trong nơi thánh Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.123Z",
      "updatedAt": "2025-04-22T10:45:43.123Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "e066cb79-05d4-4e65-96a6-74e14c910c11",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong bầu trời lớn lao vô tận của Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.134Z",
      "updatedAt": "2025-04-22T10:45:43.134Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "ec492e3e-7d2b-4480-8b32-8abc89d38bd7",
      "number": 2,
      "content": "Hãy ca ngợi Ngài về những việc quyền năng Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.146Z",
      "updatedAt": "2025-04-22T10:45:43.146Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "c047bf38-2026-43d6-9c69-64aa2c70b4fd",
      "number": 2,
      "content": "Hãy ca ngợi Ngài theo sự lớn lao vĩ đại của Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.159Z",
      "updatedAt": "2025-04-22T10:45:43.159Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "8156891e-e6be-4e3c-865f-2a0a6ff01c38",
      "number": 3,
      "content": "Hãy dùng tiếng kèn ca ngợi Ngài;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.172Z",
      "updatedAt": "2025-04-22T10:45:43.172Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "1826b0a1-ad90-424a-8e0e-877d7acc3d1b",
      "number": 3,
      "content": "Hãy dùng đàn lia và hạc cầm ca ngợi Ngài;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.185Z",
      "updatedAt": "2025-04-22T10:45:43.185Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "b68f5ca5-a4b6-4448-916c-69a78527e0a0",
      "number": 4,
      "content": "Hãy dùng trống lục lạc và điệu vũ ca ngợi Ngài;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.197Z",
      "updatedAt": "2025-04-22T10:45:43.197Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "acca399f-1bd2-4762-a0dd-e68e81f2dd7f",
      "number": 4,
      "content": "Hãy dùng các nhạc khí bằng dây và ống sáo ca ngợi Ngài;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.210Z",
      "updatedAt": "2025-04-22T10:45:43.210Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "25bfb389-3687-47c8-b02c-5f3a0349ee1e",
      "number": 5,
      "content": "Hãy dùng các phèng la vang tiếng ca ngợi Ngài;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.223Z",
      "updatedAt": "2025-04-22T10:45:43.223Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "99b11b43-c21e-4e7b-bdcf-b150efa1328c",
      "number": 5,
      "content": "Hãy dùng các khánh chiêng vang rền ca ngợi Ngài;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.236Z",
      "updatedAt": "2025-04-22T10:45:43.236Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    },
    {
      "id": "9464f5a9-d0e9-4cb2-995d-928e59fb22e4",
      "number": 6,
      "content": "Phàm vật chi thở, hãy ca ngợi **CHÚA**.",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:43.249Z",
      "updatedAt": "2025-04-22T10:45:43.249Z",
      "chapterId": "aae85fe6-2b18-4544-bb7a-ca08ee27fba1"
    }
  ],
