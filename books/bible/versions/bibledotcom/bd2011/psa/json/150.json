  },
  "verses": [
    {
      "id": "4ad9d405-12d7-47a9-8378-cf0146c15f0d",
      "number": 1,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.130Z",
      "updatedAt": "2025-04-24T02:00:48.130Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "9f67efe9-3253-4c36-b464-5a1ac2f10b55",
      "number": 1,
      "content": "Hãy ca ngợi Đức Chúa Trời trong nơi thánh Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.178Z",
      "updatedAt": "2025-04-24T02:00:48.178Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "0433988e-e4b5-4749-a89c-1332e39df637",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong bầu trời lớn lao vô tận của Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.205Z",
      "updatedAt": "2025-04-24T02:00:48.205Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "7aa730f0-12f7-432c-9616-ff84de06af61",
      "number": 2,
      "content": "Hãy ca ngợi Ngài về những việc quyền năng Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.230Z",
      "updatedAt": "2025-04-24T02:00:48.230Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "c4590635-d9cb-4eac-8d71-8bb28c5547b6",
      "number": 2,
      "content": "Hãy ca ngợi Ngài theo sự lớn lao vĩ đại của Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.255Z",
      "updatedAt": "2025-04-24T02:00:48.255Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "da863db2-68b6-47fe-ad95-6084e8270b9c",
      "number": 3,
      "content": "Hãy dùng tiếng kèn ca ngợi Ngài;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.275Z",
      "updatedAt": "2025-04-24T02:00:48.275Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "22ab9422-88ba-45c9-99cb-603b22d84b8b",
      "number": 3,
      "content": "Hãy dùng đàn lia và hạc cầm ca ngợi Ngài;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.294Z",
      "updatedAt": "2025-04-24T02:00:48.294Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "92af6bb4-29af-4ef7-b228-f94d184fe898",
      "number": 4,
      "content": "Hãy dùng trống lục lạc và điệu vũ ca ngợi Ngài;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.313Z",
      "updatedAt": "2025-04-24T02:00:48.313Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "8fd9e31a-4945-45db-87c2-1f93afbbff74",
      "number": 4,
      "content": "Hãy dùng các nhạc khí bằng dây và ống sáo ca ngợi Ngài;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.332Z",
      "updatedAt": "2025-04-24T02:00:48.332Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "949b2ab7-9955-44a8-a730-ba8dfdbdde53",
      "number": 5,
      "content": "Hãy dùng các phèng la vang tiếng ca ngợi Ngài;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.351Z",
      "updatedAt": "2025-04-24T02:00:48.351Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "d890de8e-77be-42ac-9a5f-466071d18740",
      "number": 5,
      "content": "Hãy dùng các khánh chiêng vang rền ca ngợi Ngài;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.370Z",
      "updatedAt": "2025-04-24T02:00:48.370Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "482ee8dc-9269-4b0c-a756-cddd26294ce8",
      "number": 6,
      "content": "Phàm vật chi thở, hãy ca ngợi **CHÚA**.",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:48.388Z",
      "updatedAt": "2025-04-24T02:00:48.388Z",
      "chapterId": "16a167c3-07b8-4386-aabe-679c902b2c9f"
    },
    {
      "id": "612a377d-91ad-46fe-a30d-c866463952b3",
      "number": 6,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
