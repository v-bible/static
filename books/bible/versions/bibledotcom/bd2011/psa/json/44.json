{
  "chapter": {
    "id": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d",
    "number": 44,
    "ref": "https://www.bible.com/bible/19/PSA.44.BD2011",
    "createdAt": "2025-04-24T01:29:06.330Z",
    "updatedAt": "2025-04-24T01:29:06.330Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "87dc8dce-ad91-4f76-9688-af5bc63d6164",
      "number": 1,
      "content": "Đức Chúa Trời ôi, tai chúng con có nghe,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.781Z",
      "updatedAt": "2025-04-24T01:53:48.781Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "7100eb51-12ee-4516-ab47-cad71799c8dd",
      "number": 1,
      "content": "Tổ tiên chúng con có thuật lại cho chúng con",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.809Z",
      "updatedAt": "2025-04-24T01:53:48.809Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "18f69b0b-ea28-4c34-aad8-5b4d22642f02",
      "number": 1,
      "content": "Những việc Ngài đã làm trong thời của họ,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.820Z",
      "updatedAt": "2025-04-24T01:53:48.820Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "0d48d569-131d-458b-8ab7-1d1d83a4b2e0",
      "number": 1,
      "content": "Tức trong thời xa xưa.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.831Z",
      "updatedAt": "2025-04-24T01:53:48.831Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "0dfd0fa7-7a63-4f8f-9cd3-da7715ff9e7d",
      "number": 2,
      "content": "Thể nào Ngài đã dùng chính tay Ngài đuổi các dân đi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.842Z",
      "updatedAt": "2025-04-24T01:53:48.842Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "1b090c0e-605f-4fa6-b3c6-f5fffe97aec9",
      "number": 2,
      "content": "Nhưng Ngài lại lập cho tổ tiên chúng con được vững vàng;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.854Z",
      "updatedAt": "2025-04-24T01:53:48.854Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "59ed1414-ab87-45af-8a70-4d81e8905b19",
      "number": 2,
      "content": "Ngài đã làm cho các dân kia bị khốn đốn,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.868Z",
      "updatedAt": "2025-04-24T01:53:48.868Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "bab8b9c3-a486-43dd-a05b-bc55725ea6c8",
      "number": 2,
      "content": "Nhưng Ngài cho tổ tiên chúng con hưởng tự do;",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.882Z",
      "updatedAt": "2025-04-24T01:53:48.882Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "c37ed383-87d6-4002-8ea9-b99b7950bc2d",
      "number": 3,
      "content": "Vì gươm của tổ tiên chúng con chẳng giúp họ chiếm được xứ;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.896Z",
      "updatedAt": "2025-04-24T01:53:48.896Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "cd542457-e54c-4815-bbf2-380e342e5be4",
      "number": 3,
      "content": "Cánh tay họ cũng chẳng cứu được họ,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.910Z",
      "updatedAt": "2025-04-24T01:53:48.910Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "3374bd1a-2bc4-4667-a4c0-fed8aa7e8d5d",
      "number": 3,
      "content": "Nhưng ấy là nhờ tay phải Ngài, chính là cánh tay Ngài,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.924Z",
      "updatedAt": "2025-04-24T01:53:48.924Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "a775524c-9228-49e5-9748-94528185e916",
      "number": 3,
      "content": "Và nhờ ánh sáng tỏa ra từ thánh nhan Ngài,",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.938Z",
      "updatedAt": "2025-04-24T01:53:48.938Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "223d4d06-7668-4feb-9f06-1177c8c0a249",
      "number": 3,
      "content": "Vì Ngài yêu thích họ.",
      "order": 4,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.951Z",
      "updatedAt": "2025-04-24T01:53:48.951Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "ea5805db-4b32-4b6b-ab1b-708efa1a65ff",
      "number": 4,
      "content": "Lạy Đức Chúa Trời, Ngài là Vua của con;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.965Z",
      "updatedAt": "2025-04-24T01:53:48.965Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "bd9ae95e-2a02-487b-a034-ffaff6413518",
      "number": 4,
      "content": "Xin ban lịnh giải cứu Gia-cốp.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.978Z",
      "updatedAt": "2025-04-24T01:53:48.978Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "78199084-781f-4027-849f-d612711cd81b",
      "number": 5,
      "content": "Nhờ Ngài chúng con đánh lui được quân thù chúng con;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:48.992Z",
      "updatedAt": "2025-04-24T01:53:48.992Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "ab942303-f584-46cb-95f9-7260904698dd",
      "number": 5,
      "content": "Nhờ danh Ngài chúng con chà đạp những kẻ nổi lên chống lại chúng con.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.006Z",
      "updatedAt": "2025-04-24T01:53:49.006Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "108cecb0-326a-469d-9166-dd54aa653fed",
      "number": 6,
      "content": "Bởi vì con không cậy vào cây cung mình;",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.019Z",
      "updatedAt": "2025-04-24T01:53:49.019Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "3a4d53c0-22e9-4bad-83eb-dbd12703e30d",
      "number": 6,
      "content": "Thanh gươm của con cũng không cứu con.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.033Z",
      "updatedAt": "2025-04-24T01:53:49.033Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "c75976db-a953-4c7c-8b0a-c4732448dd09",
      "number": 7,
      "content": "Nhưng Ngài đã cứu chúng con khỏi tay quân thù chúng con;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.045Z",
      "updatedAt": "2025-04-24T01:53:49.045Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "11c129f3-300f-49e9-b801-7d8954796b8c",
      "number": 7,
      "content": "Ngài đã làm cho những kẻ ghét chúng con phải bị hổ thẹn.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.059Z",
      "updatedAt": "2025-04-24T01:53:49.059Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "8a0fda04-0e6b-4b2a-b8a7-88374510af02",
      "number": 8,
      "content": "Được thuộc về Đức Chúa Trời, chúng con hãnh diện suốt ngày;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.073Z",
      "updatedAt": "2025-04-24T01:53:49.073Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "d552b865-1ccc-46d0-a819-bb7d02c8b791",
      "number": 8,
      "content": "Chúng con sẽ ca ngợi danh Ngài mãi mãi. (Sê-la)",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.086Z",
      "updatedAt": "2025-04-24T01:53:49.086Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "27167213-056a-4161-a495-06e5fda31d63",
      "number": 9,
      "content": "Dầu vậy Ngài đã loại bỏ chúng con, và để cho chúng con bị sỉ nhục;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.100Z",
      "updatedAt": "2025-04-24T01:53:49.100Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "95d94656-fce1-4e57-aec4-214a3fb8e022",
      "number": 9,
      "content": "Ngài đã không ra trận với quân đội chúng con.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.114Z",
      "updatedAt": "2025-04-24T01:53:49.114Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "13f26c7a-e682-49e2-b017-9496e858f777",
      "number": 10,
      "content": "Ngài đã để chúng con quay lưng trước mặt quân thù mình;",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.128Z",
      "updatedAt": "2025-04-24T01:53:49.128Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "c0a4c384-62bf-4dfa-b747-a4611f4a6860",
      "number": 10,
      "content": "Những kẻ ghét chúng con đã cướp lấy các chiến lợi phẩm.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.142Z",
      "updatedAt": "2025-04-24T01:53:49.142Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "4ae5b495-73cb-42ac-b365-8dec84fc12f8",
      "number": 11,
      "content": "Ngài đã phó chúng con như chiên bị đem đi làm thịt,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.156Z",
      "updatedAt": "2025-04-24T01:53:49.156Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "f42b3e33-37a7-4d1c-8f7f-34af9b0081d6",
      "number": 11,
      "content": "Và đã rải chúng con ra các nước.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.170Z",
      "updatedAt": "2025-04-24T01:53:49.170Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "8736f6a7-a334-4098-ac51-f4316f242afd",
      "number": 12,
      "content": "Ngài đã bán đổ bán tháo con dân Ngài với giá rẻ mạt;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.183Z",
      "updatedAt": "2025-04-24T01:53:49.183Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "c30f22cd-7263-4e9e-a9f3-ab1a08b9a401",
      "number": 12,
      "content": "Ngài chẳng màng lời lãi gì với giá bán ấy cả.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.197Z",
      "updatedAt": "2025-04-24T01:53:49.197Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "19491fa1-2912-452f-9dad-31b384e19b45",
      "number": 13,
      "content": "Ngài đã làm cho chúng con thành câu nói mỉa mai trong các nước láng giềng của chúng con;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.210Z",
      "updatedAt": "2025-04-24T01:53:49.210Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "9efe4676-d71f-4df5-921b-ac33dc43c869",
      "number": 13,
      "content": "Chúng con đã thành đầu đề cho những kẻ chung quanh chúng con cười chê và miệt thị.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.224Z",
      "updatedAt": "2025-04-24T01:53:49.224Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "d927915c-ab2b-47a3-a415-64369ff07e59",
      "number": 14,
      "content": "Ngài đã làm cho chúng con thành một câu tục ngữ giữa các nước;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.238Z",
      "updatedAt": "2025-04-24T01:53:49.238Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "dc0b6a1f-88f4-4d2e-823d-27ed7ec4353a",
      "number": 14,
      "content": "Trong các dân, ai nghe nói đến chúng con đều cũng lắc đầu.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.253Z",
      "updatedAt": "2025-04-24T01:53:49.253Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "9a284739-dd7b-4e64-adaa-28503443d88c",
      "number": 15,
      "content": "Suốt ngày nỗi nhục nhã của con hằng ở trước mặt con;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.266Z",
      "updatedAt": "2025-04-24T01:53:49.266Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "c564748d-368e-4a28-869e-40cf751338a9",
      "number": 15,
      "content": "Nỗi nhuốc nhơ của con bao phủ mặt con,",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.279Z",
      "updatedAt": "2025-04-24T01:53:49.279Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "b87e4d29-067c-4c6f-a281-84af5d3f3ecd",
      "number": 16,
      "content": "Vì tiếng của những kẻ sỉ vả và mắng nhiếc chúng con,",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.293Z",
      "updatedAt": "2025-04-24T01:53:49.293Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "bae89a9a-a638-4a34-bd42-fbc97b515e33",
      "number": 16,
      "content": "Và vì sự hiện diện của kẻ thù và kẻ báo thù chúng con.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.307Z",
      "updatedAt": "2025-04-24T01:53:49.307Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "67749884-05f8-40b7-a4ff-5409b517a953",
      "number": 17,
      "content": "Tất cả những điều ấy đã xảy đến với chúng con,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.320Z",
      "updatedAt": "2025-04-24T01:53:49.320Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "fc648132-c92f-4815-898b-7c282e97f783",
      "number": 17,
      "content": "Nhưng chúng con không quên Ngài;",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.333Z",
      "updatedAt": "2025-04-24T01:53:49.333Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "0f3a3908-5120-4299-ad1f-9619185ca5f3",
      "number": 17,
      "content": "Chúng con không hành động giả dối đối với giao ước Ngài.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.347Z",
      "updatedAt": "2025-04-24T01:53:49.347Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "65b4d3bf-5a67-4ccb-96c5-5e3cac780cd0",
      "number": 18,
      "content": "Lòng chúng con không thối lui;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.361Z",
      "updatedAt": "2025-04-24T01:53:49.361Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "fd80877b-a0d5-4139-a8d2-b2b7414b9f83",
      "number": 18,
      "content": "Bước chân chúng con không sai lệch đường lối Ngài.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.375Z",
      "updatedAt": "2025-04-24T01:53:49.375Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "0de6f5eb-4f31-49ed-9a1e-8fe3d42f1062",
      "number": 19,
      "content": "Dù vậy Ngài đã đập nát chúng con thành chỗ ở của chó rừng,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.389Z",
      "updatedAt": "2025-04-24T01:53:49.389Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "031726f4-5173-49a9-8d13-b1b25eba0790",
      "number": 19,
      "content": "Và bao phủ chúng con bằng bóng của tử thần.",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.416Z",
      "updatedAt": "2025-04-24T01:53:49.416Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "5f5341c7-2629-42b0-bfc0-f4812ccd4b2f",
      "number": 20,
      "content": "Nếu chúng con đã quên danh Đức Chúa Trời mình,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.430Z",
      "updatedAt": "2025-04-24T01:53:49.430Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "5949d197-8039-48d1-b038-410df51f483a",
      "number": 20,
      "content": "Hoặc đã đưa tay lên hướng về các thần khác mà thờ lạy,",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.444Z",
      "updatedAt": "2025-04-24T01:53:49.444Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "0cc20ccc-01f6-4d2b-b8d2-99f7184717f6",
      "number": 21,
      "content": "Chẳng lẽ Đức Chúa Trời không biết việc ấy sao?",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.458Z",
      "updatedAt": "2025-04-24T01:53:49.458Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "2a3d306e-20e3-4806-afe1-f8295cc583ab",
      "number": 21,
      "content": "Vì Ngài biết tất cả bí mật của lòng người.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.473Z",
      "updatedAt": "2025-04-24T01:53:49.473Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "e40494ed-0490-4d3f-8dbb-65da2b903ba4",
      "number": 22,
      "content": "Thật vậy, vì cớ Ngài chúng con bị giết suốt ngày;",
      "order": 0,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.487Z",
      "updatedAt": "2025-04-24T01:53:49.487Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "ab65071e-787d-426b-9edf-ad57f5342de5",
      "number": 22,
      "content": "Chúng con bị coi như chiên bị đem đi làm thịt.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.500Z",
      "updatedAt": "2025-04-24T01:53:49.500Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "1dc34b3e-501a-46d3-9c25-322183aa3360",
      "number": 23,
      "content": "Xin thức dậy, Chúa ôi, sao Ngài vẫn còn ngủ?",
      "order": 0,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.514Z",
      "updatedAt": "2025-04-24T01:53:49.514Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "4ce8500f-9a5b-48b7-bcaf-08c250d71bd5",
      "number": 23,
      "content": "Xin trỗi dậy, xin đừng bỏ chúng con mãi thế này.",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.527Z",
      "updatedAt": "2025-04-24T01:53:49.527Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "390acb60-7af8-4d19-bd77-43aeec34fef1",
      "number": 24,
      "content": "Sao Ngài vẫn còn tránh mặt chúng con,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.541Z",
      "updatedAt": "2025-04-24T01:53:49.541Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "fa7627e3-bcde-47c5-b172-b6dd953d76a4",
      "number": 24,
      "content": "Và quên nỗi khốn khổ và áp bức của chúng con?",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.555Z",
      "updatedAt": "2025-04-24T01:53:49.555Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "a4e5622d-126d-4da2-840d-7ea84e0a5731",
      "number": 25,
      "content": "Kìa, linh hồn chúng con đã lún sâu vào trong tro bụi;",
      "order": 0,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.568Z",
      "updatedAt": "2025-04-24T01:53:49.568Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "29c3c34c-c7ef-4232-8d24-379c52daaf15",
      "number": 25,
      "content": "Thân xác chúng con đã bị vùi dập trong bụi đất.",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.582Z",
      "updatedAt": "2025-04-24T01:53:49.582Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "bebe1ba3-b72f-4db2-9b6f-301c40ac2bf6",
      "number": 26,
      "content": "Xin trỗi dậy và cứu giúp chúng con;",
      "order": 0,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.596Z",
      "updatedAt": "2025-04-24T01:53:49.596Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    },
    {
      "id": "6e09391d-2a90-46a2-97ee-ce7f4a48c27d",
      "number": 26,
      "content": "Xin giải cứu chúng con vì lòng thương xót của Ngài.",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:49.609Z",
      "updatedAt": "2025-04-24T01:53:49.609Z",
      "chapterId": "87b29220-6a9b-4d8b-b9fd-4f623c17e44d"
    }
  ],
