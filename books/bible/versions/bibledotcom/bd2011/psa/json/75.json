{
  "chapter": {
    "id": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80",
    "number": 75,
    "ref": "https://www.bible.com/bible/19/PSA.75.BD2011",
    "createdAt": "2025-04-22T10:10:26.441Z",
    "updatedAt": "2025-04-22T10:10:26.441Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "6698da59-d0f8-4fb4-baf7-fa450b05a2e6",
      "number": 1,
      "content": "Đức Chúa Trời ôi, chúng con cảm tạ Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.689Z",
      "updatedAt": "2025-04-22T10:40:21.689Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "29d1e400-0ee9-4a60-897b-efa451ff7bc3",
      "number": 1,
      "content": "Chúng con cảm tạ Ngài vì danh Ngài ở gần,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.717Z",
      "updatedAt": "2025-04-22T10:40:21.717Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "b7bbb9c8-d89d-4910-b4d7-a90a478ebfde",
      "number": 1,
      "content": "Vì các việc diệu kỳ của Ngài luôn được truyền tụng cao rao.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.727Z",
      "updatedAt": "2025-04-22T10:40:21.727Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "b93bfda9-0b25-424d-b522-1de15fa1c805",
      "number": 2,
      "content": "Khi đến thời điểm Ta đã định,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.737Z",
      "updatedAt": "2025-04-22T10:40:21.737Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "9e93635b-b50a-49eb-85e6-b9079d9d2087",
      "number": 2,
      "content": "Chính Ta sẽ xét xử công minh.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.748Z",
      "updatedAt": "2025-04-22T10:40:21.748Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "54b76e55-4b4b-46c1-95ea-f3c4ab756447",
      "number": 3,
      "content": "Khi đất và mọi kẻ sống trong nó bị lung lay,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.759Z",
      "updatedAt": "2025-04-22T10:40:21.759Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "b01a004e-0fec-4737-9ece-f27b9085778e",
      "number": 3,
      "content": "Chính Ta nắm giữ các trụ nó để nó được vững vàng. (Sê-la)",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.785Z",
      "updatedAt": "2025-04-22T10:40:21.785Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "831c14b4-1d2b-4c76-802e-8e294b3c0b28",
      "number": 4,
      "content": "Ta phán với những kẻ kiêu căng, “Chớ kiêu căng nữa!”",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.798Z",
      "updatedAt": "2025-04-22T10:40:21.798Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "284573c6-03b3-4f92-b9f4-65a1b8e22c45",
      "number": 4,
      "content": "Với những kẻ gian ác, “Chớ ngước sừng các ngươi lên;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.811Z",
      "updatedAt": "2025-04-22T10:40:21.811Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "85ec9481-4b9c-4d12-a2c1-feb48e6e389d",
      "number": 5,
      "content": "Chớ ngước sừng ngưỡng cổ dương oai;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.824Z",
      "updatedAt": "2025-04-22T10:40:21.824Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "299a48ac-9ab3-475a-a0c6-922740caf728",
      "number": 5,
      "content": "Chớ nói ra những lời kiêu căng xấc xược.”",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.838Z",
      "updatedAt": "2025-04-22T10:40:21.838Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "c4822a99-1985-43a6-b609-b45fc2d27678",
      "number": 6,
      "content": "Vì chẳng phải từ phương đông hoặc từ phương tây,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.851Z",
      "updatedAt": "2025-04-22T10:40:21.851Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "68093dcb-5c51-43ec-9323-5b378c054088",
      "number": 6,
      "content": "Hay từ miền sa mạc mà người ta được tôn trọng,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.864Z",
      "updatedAt": "2025-04-22T10:40:21.864Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "eb09ccbe-830d-47ca-9b96-156677ab46de",
      "number": 7,
      "content": "Nhưng ấy là từ Đức Chúa Trời, Đấng Đoán Xét;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.877Z",
      "updatedAt": "2025-04-22T10:40:21.877Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "61f62623-1d49-4460-a363-8fd79362793e",
      "number": 7,
      "content": "Ngài hạ kẻ này xuống và nhấc người kia lên.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.891Z",
      "updatedAt": "2025-04-22T10:40:21.891Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "78a9ecc8-daa8-4484-9600-c9861e401f95",
      "number": 8,
      "content": "Trong tay **CHÚA** có một chén rượu,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.904Z",
      "updatedAt": "2025-04-22T10:40:21.904Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "b7f9a931-e65a-414c-bd72-f174ecfc46e6",
      "number": 8,
      "content": "Một chén rượu pha, đỏ hồng, và sủi bọt;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.917Z",
      "updatedAt": "2025-04-22T10:40:21.917Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "89c71253-25ee-420a-a088-769e28478da4",
      "number": 8,
      "content": "Ngài rót nó ra",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.930Z",
      "updatedAt": "2025-04-22T10:40:21.930Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "040690cc-c052-44d6-9821-230493bf6461",
      "number": 8,
      "content": "Và bắt mọi kẻ ác trên đất phải uống;",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.943Z",
      "updatedAt": "2025-04-22T10:40:21.943Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "186caaff-8f07-450f-8f77-766763ca3348",
      "number": 8,
      "content": "Chúng phải uống cạn cho đến giọt rượu cuối cùng.",
      "order": 4,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.956Z",
      "updatedAt": "2025-04-22T10:40:21.956Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "fd42a323-582c-44b3-98fc-055f85080b53",
      "number": 9,
      "content": "Riêng phần tôi, tôi sẽ ca tụng Ngài mãi mãi;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.970Z",
      "updatedAt": "2025-04-22T10:40:21.970Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "b965ceab-47f2-4891-b7c3-94f300502d61",
      "number": 9,
      "content": "Tôi sẽ ca ngợi Đức Chúa Trời của Gia-cốp.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.983Z",
      "updatedAt": "2025-04-22T10:40:21.983Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "f5a1507d-95eb-41e9-87d3-477344a5c766",
      "number": 10,
      "content": "Vì Ngài sẽ chặt trụi các sừng của quân gian ác,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:21.997Z",
      "updatedAt": "2025-04-22T10:40:21.997Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    },
    {
      "id": "bd9643e4-3cef-4154-8287-20bb23ddf08f",
      "number": 10,
      "content": "Còn sừng của người ngay lành sẽ được nhấc cao lên.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:22.023Z",
      "updatedAt": "2025-04-22T10:40:22.023Z",
      "chapterId": "90a7fcaa-83ea-44f6-9fbb-df7c078d5a80"
    }
  ],
