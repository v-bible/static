{
  "chapter": {
    "id": "391a8208-ee97-464f-85cf-b684ec8a2e3d",
    "number": 123,
    "ref": "https://www.bible.com/bible/19/PSA.123.BD2011",
    "createdAt": "2025-04-24T01:29:07.361Z",
    "updatedAt": "2025-04-24T01:29:07.361Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "169f5dab-0734-40c5-be8b-76f1a511da29",
      "number": 1,
      "content": "Lạy Đấng ngự trên các tầng trời,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.267Z",
      "updatedAt": "2025-04-24T01:58:49.267Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "100cc4cb-421c-48bb-a7b6-9f9805194efb",
      "number": 1,
      "content": "Con ngước mắt lên hướng về Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.295Z",
      "updatedAt": "2025-04-24T01:58:49.295Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "32b94c30-c8d3-4d33-967d-434ef2e030e9",
      "number": 2,
      "content": "Kìa, như mắt các đầy tớ",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.306Z",
      "updatedAt": "2025-04-24T01:58:49.306Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "2052182d-c21a-4eb7-ba1c-c518a37f3093",
      "number": 2,
      "content": "Trông nơi tay ông chủ họ thể nào,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.317Z",
      "updatedAt": "2025-04-24T01:58:49.317Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "80fcfc87-b97f-4e4f-a3e3-18751af76b56",
      "number": 2,
      "content": "Như mắt người tớ gái",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.327Z",
      "updatedAt": "2025-04-24T01:58:49.327Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "fd48a848-c91d-4c7c-9e52-47123a824d8e",
      "number": 2,
      "content": "Trông nơi tay bà chủ nàng thể nào,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.340Z",
      "updatedAt": "2025-04-24T01:58:49.340Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "ceb54e01-1259-483f-acea-59de575963f3",
      "number": 2,
      "content": "Mắt chúng con cũng vọng trông nơi **CHÚA**, Đức Chúa Trời chúng con, thể ấy,",
      "order": 4,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.353Z",
      "updatedAt": "2025-04-24T01:58:49.353Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "907942aa-b6a7-414c-a28f-3d1dc94abc93",
      "number": 2,
      "content": "Cho đến khi Ngài bày tỏ ơn thương xót đối với chúng con.",
      "order": 5,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.366Z",
      "updatedAt": "2025-04-24T01:58:49.366Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "57202dcc-a26d-47a9-afb4-3fb480772c84",
      "number": 3,
      "content": "Xin thương xót chúng con, **CHÚA** ôi, xin thương xót chúng con,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.380Z",
      "updatedAt": "2025-04-24T01:58:49.380Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "7ee1bb15-f038-4432-a7be-10295b39c85e",
      "number": 3,
      "content": "Vì chúng con bị khinh khi quá đỗi.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.393Z",
      "updatedAt": "2025-04-24T01:58:49.393Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "88869105-a9d5-4a1c-9333-a08a498da843",
      "number": 4,
      "content": "Linh hồn chúng con quá ngao ngán",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.407Z",
      "updatedAt": "2025-04-24T01:58:49.407Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "44ba0d61-6533-47cb-a652-0b2a8951653e",
      "number": 4,
      "content": "Những lời nhạo báng của đám người kiêu ngạo,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.423Z",
      "updatedAt": "2025-04-24T01:58:49.423Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    },
    {
      "id": "d0ea1675-bb0a-4caf-8852-a77544717d8c",
      "number": 4,
      "content": "Cùng với giọng khinh người của những kẻ kiêu căng.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:49.437Z",
      "updatedAt": "2025-04-24T01:58:49.437Z",
      "chapterId": "391a8208-ee97-464f-85cf-b684ec8a2e3d"
    }
  ],
