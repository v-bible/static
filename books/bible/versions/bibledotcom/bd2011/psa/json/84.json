{
  "chapter": {
    "id": "ce9025f6-25d6-4b96-9a01-62cf37bef716",
    "number": 84,
    "ref": "https://www.bible.com/bible/19/PSA.84.BD2011",
    "createdAt": "2025-04-22T10:10:26.530Z",
    "updatedAt": "2025-04-22T10:10:26.530Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "bd6e71e9-407a-49dc-a9ee-66c2f4e9b8a5",
      "number": 1,
      "content": "Lạy **CHÚA** của các đạo quân,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.235Z",
      "updatedAt": "2025-04-22T10:40:54.235Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "3adda41f-3d15-4207-93d0-e6ab6b6022cf",
      "number": 12,
      "content": "Lạy **CHÚA** của các đạo quân,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.694Z",
      "updatedAt": "2025-04-22T10:40:54.694Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "af346aa0-52ed-497d-9673-ac4d1a2fac90",
      "number": 1,
      "content": "Chỗ ở của Ngài thật đáng yêu mến thay!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.263Z",
      "updatedAt": "2025-04-22T10:40:54.263Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "628558b2-ba3e-46c5-8ae5-454f04623f68",
      "number": 2,
      "content": "Linh hồn con khao khát và thèm muốn",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.273Z",
      "updatedAt": "2025-04-22T10:40:54.273Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "a1e91516-277f-477c-b7bd-817efc6ea154",
      "number": 2,
      "content": "Được vào sân của nhà **CHÚA** biết bao.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.284Z",
      "updatedAt": "2025-04-22T10:40:54.284Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "85385d13-9d2a-40c5-9f47-41cd09dc12af",
      "number": 2,
      "content": "Lòng con và thân xác con ước ao được về đó",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.294Z",
      "updatedAt": "2025-04-22T10:40:54.294Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "2855a9f6-ace2-40b0-967b-a3c8b3b7a0d0",
      "number": 2,
      "content": "Để reo mừng chúc tụng Đức Chúa Trời hằng sống biết dường nào.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.305Z",
      "updatedAt": "2025-04-22T10:40:54.305Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "d4f37107-5593-4a1c-bf7b-0f318936c836",
      "number": 3,
      "content": "Ngay cả con chim sẻ còn kiếm được một chỗ ở,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.318Z",
      "updatedAt": "2025-04-22T10:40:54.318Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "94db4ec1-6da8-477f-bcbb-408dc8c52473",
      "number": 3,
      "content": "Con chim én còn có nơi để làm tổ",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.331Z",
      "updatedAt": "2025-04-22T10:40:54.331Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "7eaaf909-c3d3-4040-83ee-11ca85ace8ca",
      "number": 3,
      "content": "Cho con nó gần bàn thờ Ngài,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.344Z",
      "updatedAt": "2025-04-22T10:40:54.344Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "5a41ee41-fe33-43c9-8dde-6ce45edf691a",
      "number": 3,
      "content": "Ôi **CHÚA** của các đạo quân,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.358Z",
      "updatedAt": "2025-04-22T10:40:54.358Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "13579b3e-b03d-4e47-ac0b-8bfe3210aabb",
      "number": 3,
      "content": "Vua của con và Đức Chúa Trời của con.",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.371Z",
      "updatedAt": "2025-04-22T10:40:54.371Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "25c3d68b-fbf6-413f-88a5-5b21930bac16",
      "number": 4,
      "content": "Phước cho người nào được ở trong nhà Ngài!",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.385Z",
      "updatedAt": "2025-04-22T10:40:54.385Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "54555041-1906-40ab-b08c-6e88e819948e",
      "number": 4,
      "content": "Họ sẽ được ca ngợi Ngài mãi mãi. (Sê-la)",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.398Z",
      "updatedAt": "2025-04-22T10:40:54.398Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "8ca7dd23-a7ec-4f98-baa6-4edbd3e11760",
      "number": 5,
      "content": "Phước thay cho người cậy Ngài làm sức mạnh mình,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.411Z",
      "updatedAt": "2025-04-22T10:40:54.411Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "0cabfa23-fbf5-4f8b-94f0-9e49b9f8b7e0",
      "number": 5,
      "content": "Và để lòng mình vào cuộc hành trình về nhà Ngài.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.424Z",
      "updatedAt": "2025-04-22T10:40:54.424Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "e46b43bf-7fa6-44dd-add5-cb33119c6bee",
      "number": 6,
      "content": "Khi họ đi qua thung lũng buồn đau than khóc,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.435Z",
      "updatedAt": "2025-04-22T10:40:54.435Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "9f438349-22b3-448d-a3b7-04be587fbf82",
      "number": 6,
      "content": "Họ sẽ biến nó thành nơi có những suối nước trào tuôn,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.446Z",
      "updatedAt": "2025-04-22T10:40:54.446Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "64de55e8-4a18-484e-ae8c-5f8e1bef872a",
      "number": 6,
      "content": "Như mưa đầu mùa biến nơi ấy thành vùng có nhiều vũng nước.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.456Z",
      "updatedAt": "2025-04-22T10:40:54.456Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "c03ab74a-b4fe-40b1-9505-0ef8c6a0dd7e",
      "number": 7,
      "content": "Họ càng đi tới, sức lực họ càng tăng lên,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.470Z",
      "updatedAt": "2025-04-22T10:40:54.470Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "b2474a5d-0ad2-4c27-81ff-2c606631eac9",
      "number": 7,
      "content": "Cho đến khi mọi người ra mắt Đức Chúa Trời tại Si-ôn.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.480Z",
      "updatedAt": "2025-04-22T10:40:54.480Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "56d2de54-f008-4e13-92d3-c4245157821b",
      "number": 8,
      "content": "Lạy **CHÚA**, Đức Chúa Trời của các đạo quân, xin lắng nghe lời cầu nguyện của con;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.490Z",
      "updatedAt": "2025-04-22T10:40:54.490Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "53d2a638-aedd-4f02-8c89-6fc312c6ee83",
      "number": 8,
      "content": "Đức Chúa Trời của Gia-cốp ôi, xin nghiêng tai nghe tiếng cầu xin của con. (Sê-la)",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.501Z",
      "updatedAt": "2025-04-22T10:40:54.501Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "818fcfaa-8b2f-4c37-84d8-1fad9363b068",
      "number": 9,
      "content": "Đức Chúa Trời ôi, xin đoái xem người lãnh đạo của chúng con;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.518Z",
      "updatedAt": "2025-04-22T10:40:54.518Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "a5ed68f0-f7bb-4755-9f10-884be99aab93",
      "number": 9,
      "content": "Xin cúi nhìn vào mặt người được xức dầu của Ngài.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.547Z",
      "updatedAt": "2025-04-22T10:40:54.547Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "24b11e08-0d6b-42af-b53c-5c854fd9c0d3",
      "number": 10,
      "content": "Vì một ngày được ở trong các sân của nhà Ngài",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.561Z",
      "updatedAt": "2025-04-22T10:40:54.561Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "613c30aa-1f31-4502-aeea-b08d281fd973",
      "number": 10,
      "content": "Thật đáng hơn cả ngàn ngày ở những nơi khác.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.574Z",
      "updatedAt": "2025-04-22T10:40:54.574Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "664006ee-75cb-4f19-b5f6-286653f63e96",
      "number": 10,
      "content": "Tôi thà đứng giữ cửa trong nhà Đức Chúa Trời của tôi,",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.587Z",
      "updatedAt": "2025-04-22T10:40:54.587Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "2e7020d2-80c3-4ead-b61f-421833eb3771",
      "number": 10,
      "content": "Còn hơn ở trong biệt thự của những kẻ gian ác.",
      "order": 3,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.601Z",
      "updatedAt": "2025-04-22T10:40:54.601Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "af3d4e6a-0f7b-4673-b69c-7a733aad60f8",
      "number": 11,
      "content": "Vì **CHÚA** Đức Chúa Trời là mặt trời và thuẫn khiên;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.628Z",
      "updatedAt": "2025-04-22T10:40:54.628Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "69315415-165a-4a43-a6f5-b39bc5d42bae",
      "number": 11,
      "content": "**CHÚA** sẽ ban ân sủng và vinh hiển;",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.655Z",
      "updatedAt": "2025-04-22T10:40:54.655Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "f705bcc2-634e-4d3d-a666-a2028215bb96",
      "number": 11,
      "content": "Ngài chẳng giữ lại phước hạnh nào",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.668Z",
      "updatedAt": "2025-04-22T10:40:54.668Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "028e39c9-c90c-451b-9031-35c0f8f7bf42",
      "number": 11,
      "content": "Đối với người ăn ở ngay lành.",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.681Z",
      "updatedAt": "2025-04-22T10:40:54.681Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    },
    {
      "id": "d66bfd26-33ee-4a0c-896f-ff4e9c05c1c1",
      "number": 12,
      "content": "Phước thay cho người tin cậy Ngài!",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:40:54.707Z",
      "updatedAt": "2025-04-22T10:40:54.707Z",
      "chapterId": "ce9025f6-25d6-4b96-9a01-62cf37bef716"
    }
  ],
