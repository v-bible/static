{
  "chapter": {
    "id": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3",
    "number": 84,
    "ref": "https://www.bible.com/bible/19/PSA.84.BD2011",
    "createdAt": "2025-04-24T01:29:06.816Z",
    "updatedAt": "2025-04-24T01:29:06.816Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "d469fca7-a1d8-4980-a2d2-c6a42a05d75e",
      "number": 1,
      "content": "Lạy **CHÚA** của các đạo quân,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.236Z",
      "updatedAt": "2025-04-24T01:56:08.236Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "18b24cf9-27dd-42f1-b61e-510aef6a6f4d",
      "number": 1,
      "content": "Chỗ ở của Ngài thật đáng yêu mến thay!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.270Z",
      "updatedAt": "2025-04-24T01:56:08.270Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "920e2d3c-f516-47e8-9c43-b216b9229974",
      "number": 2,
      "content": "Linh hồn con khao khát và thèm muốn",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.282Z",
      "updatedAt": "2025-04-24T01:56:08.282Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "2900f9b8-494e-418b-b362-fbf9e8925aff",
      "number": 2,
      "content": "Được vào sân của nhà **CHÚA** biết bao.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.293Z",
      "updatedAt": "2025-04-24T01:56:08.293Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "246a8a49-11c1-4490-9167-b6f819cdc434",
      "number": 2,
      "content": "Lòng con và thân xác con ước ao được về đó",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.306Z",
      "updatedAt": "2025-04-24T01:56:08.306Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "de91ce81-8d95-4b6a-9e7a-5b81d8bc76b8",
      "number": 2,
      "content": "Để reo mừng chúc tụng Đức Chúa Trời hằng sống biết dường nào.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.320Z",
      "updatedAt": "2025-04-24T01:56:08.320Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "7fdf865c-01e8-4da1-98c8-7393d16d129c",
      "number": 3,
      "content": "Ngay cả con chim sẻ còn kiếm được một chỗ ở,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.333Z",
      "updatedAt": "2025-04-24T01:56:08.333Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "17739de9-3946-4b82-9ca8-d6f14387408b",
      "number": 3,
      "content": "Con chim én còn có nơi để làm tổ",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.346Z",
      "updatedAt": "2025-04-24T01:56:08.346Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "893617d2-627c-4425-b26a-6038cd88a061",
      "number": 3,
      "content": "Cho con nó gần bàn thờ Ngài,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.360Z",
      "updatedAt": "2025-04-24T01:56:08.360Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "a03b6f68-5cda-47ef-b6f2-9e018e2ceb28",
      "number": 3,
      "content": "Ôi **CHÚA** của các đạo quân,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.375Z",
      "updatedAt": "2025-04-24T01:56:08.375Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "cbffde81-6735-40d1-b68e-b6286ec3d90c",
      "number": 3,
      "content": "Vua của con và Đức Chúa Trời của con.",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.388Z",
      "updatedAt": "2025-04-24T01:56:08.388Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "74571874-f3a4-48f5-abb7-f6f007bf370b",
      "number": 4,
      "content": "Phước cho người nào được ở trong nhà Ngài!",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.402Z",
      "updatedAt": "2025-04-24T01:56:08.402Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "ed1e7b0c-e815-43f8-b2f5-1d1dd2cad87b",
      "number": 4,
      "content": "Họ sẽ được ca ngợi Ngài mãi mãi. (Sê-la)",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.415Z",
      "updatedAt": "2025-04-24T01:56:08.415Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "c395c053-8e52-426b-904c-5bf668a372e0",
      "number": 5,
      "content": "Phước thay cho người cậy Ngài làm sức mạnh mình,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.429Z",
      "updatedAt": "2025-04-24T01:56:08.429Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "4ef016d0-cf21-4a86-bbeb-ff94edbd08d5",
      "number": 5,
      "content": "Và để lòng mình vào cuộc hành trình về nhà Ngài.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.442Z",
      "updatedAt": "2025-04-24T01:56:08.442Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "c3058344-e45c-4b57-8298-d997e6c2a4b9",
      "number": 6,
      "content": "Khi họ đi qua thung lũng buồn đau than khóc,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.456Z",
      "updatedAt": "2025-04-24T01:56:08.456Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "9df1f046-d0cc-49e0-84d0-9171fdb41500",
      "number": 6,
      "content": "Họ sẽ biến nó thành nơi có những suối nước trào tuôn,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.469Z",
      "updatedAt": "2025-04-24T01:56:08.469Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "6d0f3c66-42b0-4ac3-9420-ce8a35251138",
      "number": 6,
      "content": "Như mưa đầu mùa biến nơi ấy thành vùng có nhiều vũng nước.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.483Z",
      "updatedAt": "2025-04-24T01:56:08.483Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "7954a5f9-cc15-4120-bc21-235baf1e946d",
      "number": 7,
      "content": "Họ càng đi tới, sức lực họ càng tăng lên,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.497Z",
      "updatedAt": "2025-04-24T01:56:08.497Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "70f9ced9-eaf6-4acb-8d46-c95a9665ce8a",
      "number": 7,
      "content": "Cho đến khi mọi người ra mắt Đức Chúa Trời tại Si-ôn.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.510Z",
      "updatedAt": "2025-04-24T01:56:08.510Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "f5fb3901-b6c0-4e6c-8ad1-077c80f3050e",
      "number": 8,
      "content": "Lạy **CHÚA**, Đức Chúa Trời của các đạo quân, xin lắng nghe lời cầu nguyện của con;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.524Z",
      "updatedAt": "2025-04-24T01:56:08.524Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "028ae626-e544-4c65-81d8-3ab1196af69f",
      "number": 8,
      "content": "Đức Chúa Trời của Gia-cốp ôi, xin nghiêng tai nghe tiếng cầu xin của con. (Sê-la)",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.538Z",
      "updatedAt": "2025-04-24T01:56:08.538Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "e284c4ca-4234-40b4-91eb-11a206988979",
      "number": 9,
      "content": "Đức Chúa Trời ôi, xin đoái xem người lãnh đạo của chúng con;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.551Z",
      "updatedAt": "2025-04-24T01:56:08.551Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "b09b6efa-18f5-4f19-a15e-2cee46416b3c",
      "number": 9,
      "content": "Xin cúi nhìn vào mặt người được xức dầu của Ngài.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.579Z",
      "updatedAt": "2025-04-24T01:56:08.579Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "69e8734b-99d9-47b0-9d89-b4e67c9e3de7",
      "number": 10,
      "content": "Vì một ngày được ở trong các sân của nhà Ngài",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.597Z",
      "updatedAt": "2025-04-24T01:56:08.597Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "cce092ed-fc39-4d3f-9831-89b5d0f4fcd3",
      "number": 10,
      "content": "Thật đáng hơn cả ngàn ngày ở những nơi khác.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.610Z",
      "updatedAt": "2025-04-24T01:56:08.610Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "6e81684f-82cd-4333-8ac2-d0b3f0e2fed9",
      "number": 10,
      "content": "Tôi thà đứng giữ cửa trong nhà Đức Chúa Trời của tôi,",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.624Z",
      "updatedAt": "2025-04-24T01:56:08.624Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "2bae5e3c-0212-42d1-88e2-a81a9fa30ba9",
      "number": 10,
      "content": "Còn hơn ở trong biệt thự của những kẻ gian ác.",
      "order": 3,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.638Z",
      "updatedAt": "2025-04-24T01:56:08.638Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "5673c65a-4b56-4f05-9ab0-b20e3d558f8a",
      "number": 11,
      "content": "Vì **CHÚA** Đức Chúa Trời là mặt trời và thuẫn khiên;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.663Z",
      "updatedAt": "2025-04-24T01:56:08.663Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "9586ab81-71fc-47a6-b1ed-1af823d46c5e",
      "number": 11,
      "content": "**CHÚA** sẽ ban ân sủng và vinh hiển;",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.690Z",
      "updatedAt": "2025-04-24T01:56:08.690Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "7be7ad7b-d5dd-4851-87e5-2e5b0fe8613e",
      "number": 11,
      "content": "Ngài chẳng giữ lại phước hạnh nào",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.704Z",
      "updatedAt": "2025-04-24T01:56:08.704Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "e8cc8589-9bb4-4f83-b70e-993ef56c07dc",
      "number": 11,
      "content": "Đối với người ăn ở ngay lành.",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.718Z",
      "updatedAt": "2025-04-24T01:56:08.718Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "588b6ee4-79f9-47c3-b1ef-f0f6f8b5c2b1",
      "number": 12,
      "content": "Lạy **CHÚA** của các đạo quân,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.731Z",
      "updatedAt": "2025-04-24T01:56:08.731Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    },
    {
      "id": "78d8cf83-ff29-445c-a888-ac9c4f1ac68f",
      "number": 12,
      "content": "Phước thay cho người tin cậy Ngài!",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:08.743Z",
      "updatedAt": "2025-04-24T01:56:08.743Z",
      "chapterId": "32ff247f-f99b-4ddb-b8e5-e62f7edb3df3"
    }
  ],
