{
  "chapter": {
    "id": "203f92ca-aa6a-4f84-a662-63f6548d55fe",
    "number": 63,
    "ref": "https://www.bible.com/bible/19/PSA.63.BD2011",
    "createdAt": "2025-04-22T10:10:26.301Z",
    "updatedAt": "2025-04-22T10:10:26.301Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "cf00ae62-79b2-4c6c-a640-2119786fe071",
      "number": 1,
      "content": "Đức Chúa Trời ôi, Ngài là Đức Chúa Trời của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.552Z",
      "updatedAt": "2025-04-22T10:39:25.552Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "59dbd75e-6f86-4fd7-a043-6d44fd5744e9",
      "number": 1,
      "content": "Khi hừng đông chưa ló dạng, con đã tìm cầu Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.600Z",
      "updatedAt": "2025-04-22T10:39:25.600Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "8f45f901-724c-446f-b9bd-d8761ec73d40",
      "number": 1,
      "content": "Linh hồn con khao khát Ngài, thân thể con mong mỏi Ngài,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.610Z",
      "updatedAt": "2025-04-22T10:39:25.610Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "93f3ce6b-9d32-4d91-b0b2-ead42306f5c4",
      "number": 1,
      "content": "Trong một nơi khô khan cằn cỗi và chẳng có nước.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.622Z",
      "updatedAt": "2025-04-22T10:39:25.622Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "18cb4f01-e964-4dc9-83fc-e09af7ffd6b1",
      "number": 2,
      "content": "Con mong ước được thấy quyền năng và vinh hiển Ngài,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.635Z",
      "updatedAt": "2025-04-22T10:39:25.635Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "e8b0ec77-51f3-427c-8cea-ed5c344f9f51",
      "number": 2,
      "content": "Như con đã từng thấy trong nơi thánh.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.648Z",
      "updatedAt": "2025-04-22T10:39:25.648Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "317b4276-effe-40d3-a691-b3547953d8ca",
      "number": 3,
      "content": "Vì tình thương của Ngài tốt hơn mạng sống,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.661Z",
      "updatedAt": "2025-04-22T10:39:25.661Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "508857da-abb6-4d38-91ff-c28ad0fd4a91",
      "number": 3,
      "content": "Nên môi con sẽ ca ngợi Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.675Z",
      "updatedAt": "2025-04-22T10:39:25.675Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "aa9c2d5a-6078-4ddb-87be-617719da38df",
      "number": 4,
      "content": "Hễ con còn sống bao lâu, con sẽ ca ngợi Ngài bấy lâu;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.688Z",
      "updatedAt": "2025-04-22T10:39:25.688Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "0fb382f3-bb89-4890-a58c-af4e25faddde",
      "number": 4,
      "content": "Con sẽ đưa tay lên và chúc tụng danh Ngài.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.702Z",
      "updatedAt": "2025-04-22T10:39:25.702Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "182417f9-b67d-4144-b0dc-13ff3710f4db",
      "number": 5,
      "content": "Linh hồn con sẽ vui thỏa như được hưởng các thức ăn béo bổ,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.714Z",
      "updatedAt": "2025-04-22T10:39:25.714Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "aba8c61b-9de9-4e86-9b22-6aea9e6ef61d",
      "number": 5,
      "content": "Miệng con sẽ ca ngợi Ngài với môi vui vẻ.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.728Z",
      "updatedAt": "2025-04-22T10:39:25.728Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "33b25338-d6ee-4967-a056-325314119dd6",
      "number": 6,
      "content": "Khi nằm trên giường con nhớ đến Ngài,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.741Z",
      "updatedAt": "2025-04-22T10:39:25.741Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "f519d2a7-162f-4485-bff7-fa56d0bffe8c",
      "number": 6,
      "content": "Con nghĩ về Ngài trọn các canh đêm,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.754Z",
      "updatedAt": "2025-04-22T10:39:25.754Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "fa866b19-418d-4713-aff4-52c3b16bf484",
      "number": 7,
      "content": "Vì Ngài là nguồn cứu giúp của con,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.767Z",
      "updatedAt": "2025-04-22T10:39:25.767Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "b28ad1c6-ad5b-4f18-b538-11de30605d77",
      "number": 7,
      "content": "Nên dưới cánh Ngài con hát mừng vui vẻ.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.780Z",
      "updatedAt": "2025-04-22T10:39:25.780Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "61a1e27d-8237-461d-bb40-ddce9b6be49e",
      "number": 8,
      "content": "Linh hồn con bám chặt vào Ngài;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.793Z",
      "updatedAt": "2025-04-22T10:39:25.793Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "7fc209ca-eafd-42d0-8ad2-0fab3fa04fa9",
      "number": 8,
      "content": "Tay phải Ngài giữ vững con.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.806Z",
      "updatedAt": "2025-04-22T10:39:25.806Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "5643a4bd-8abb-43ed-9b5b-137d2fc56ab1",
      "number": 9,
      "content": "Nhưng những kẻ tìm cách tiêu diệt mạng sống con",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.819Z",
      "updatedAt": "2025-04-22T10:39:25.819Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "56819c15-4ad8-4d78-9501-575ed14fcaf5",
      "number": 9,
      "content": "Sẽ bị rơi vào đáy sâu của lòng đất.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.832Z",
      "updatedAt": "2025-04-22T10:39:25.832Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "593e2d9d-80f2-43d1-bf54-03de2ad2afc0",
      "number": 10,
      "content": "Chúng sẽ bị phó cho lưỡi gươm;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.849Z",
      "updatedAt": "2025-04-22T10:39:25.849Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "14a1439f-653c-441c-8f89-f844de675cc2",
      "number": 10,
      "content": "Chúng sẽ thành mồi cho muông sói.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.863Z",
      "updatedAt": "2025-04-22T10:39:25.863Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "ee2ae687-62b3-485a-bfe3-d3d9e13927a9",
      "number": 11,
      "content": "Nhưng vua sẽ vui mừng trong Đức Chúa Trời;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.882Z",
      "updatedAt": "2025-04-22T10:39:25.882Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "ccc5030c-9ecb-41d5-ac54-e7abdd14e5f3",
      "number": 11,
      "content": "Những ai nhân danh Ngài mà thề nguyện sẽ được vinh hiển;",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.898Z",
      "updatedAt": "2025-04-22T10:39:25.898Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    },
    {
      "id": "c86cab45-fadd-4463-9274-b6b8bf681bfd",
      "number": 11,
      "content": "Nhưng miệng của những kẻ nói dối phải ngậm lại.",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:25.912Z",
      "updatedAt": "2025-04-22T10:39:25.912Z",
      "chapterId": "203f92ca-aa6a-4f84-a662-63f6548d55fe"
    }
  ],
