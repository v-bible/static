{
  "chapter": {
    "id": "ff75dc99-79c6-463e-91a7-d3c62b3c385f",
    "number": 133,
    "ref": "https://www.bible.com/bible/19/PSA.133.BD2011",
    "createdAt": "2025-04-24T01:29:07.478Z",
