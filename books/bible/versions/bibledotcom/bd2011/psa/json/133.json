  },
  "verses": [
    {
      "id": "f53427de-e582-472f-b92d-54f3730cc47e",
      "number": 1,
      "content": "Kìa, tốt đẹp thay và phước hạnh thay",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:29.501Z",
      "updatedAt": "2025-04-22T10:44:29.501Z",
      "chapterId": "208e250d-0bde-4edc-83dc-76649c2864fc"
    },
    {
      "id": "11975b6e-2ee6-4194-8454-38a4c6349ec5",
      "number": 1,
      "content": "Khi anh chị em sống hòa thuận với nhau!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:29.530Z",
      "updatedAt": "2025-04-22T10:44:29.530Z",
      "chapterId": "208e250d-0bde-4edc-83dc-76649c2864fc"
    },
    {
      "id": "49d937ee-4a6b-4159-aca7-c3b8c163810c",
      "number": 2,
      "content": "Điều ấy chẳng khác gì dầu quý báu từ trên đầu chảy xuống,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:29.551Z",
      "updatedAt": "2025-04-22T10:44:29.551Z",
      "chapterId": "208e250d-0bde-4edc-83dc-76649c2864fc"
    },
    {
      "id": "12b840a0-e9f3-4e81-9914-72d1b8506cc4",
      "number": 2,
      "content": "Chảy xuống râu,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:29.561Z",
      "updatedAt": "2025-04-22T10:44:29.561Z",
      "chapterId": "208e250d-0bde-4edc-83dc-76649c2864fc"
    },
    {
      "id": "353bb7a1-49bd-47a0-8248-5897ae22e817",
      "number": 2,
      "content": "Chảy xuống râu của A-rôn,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:29.571Z",
      "updatedAt": "2025-04-22T10:44:29.571Z",
      "chapterId": "208e250d-0bde-4edc-83dc-76649c2864fc"
    },
    {
      "id": "b9e008b8-b052-4802-8374-e5c4a5b4371a",
      "number": 2,
      "content": "Rồi tiếp tục chảy xuống vạt áo ông.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:29.582Z",
      "updatedAt": "2025-04-22T10:44:29.582Z",
      "chapterId": "208e250d-0bde-4edc-83dc-76649c2864fc"
    },
    {
      "id": "0550f76d-702d-41c5-95f6-4bde4f12661a",
      "number": 3,
      "content": "Điều ấy giống như sương móc từ Núi Hẹt-môn",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:29.592Z",
      "updatedAt": "2025-04-22T10:44:29.592Z",
      "chapterId": "208e250d-0bde-4edc-83dc-76649c2864fc"
    },
    {
      "id": "abdfad81-73fa-4f0d-a07c-36fdee4239d7",
      "number": 3,
      "content": "Sa xuống các rặng núi Si-ôn,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:29.602Z",
      "updatedAt": "2025-04-22T10:44:29.602Z",
      "chapterId": "208e250d-0bde-4edc-83dc-76649c2864fc"
    },
    {
      "id": "63e0600a-6002-4af8-8c31-f8a3b98cc2fb",
      "number": 3,
      "content": "Vì tại đó **CHÚA** ban phước,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:29.612Z",
      "updatedAt": "2025-04-22T10:44:29.612Z",
      "chapterId": "208e250d-0bde-4edc-83dc-76649c2864fc"
    },
    {
      "id": "4b19e155-c2d1-4a37-ba96-ef9c0f176d6b",
      "number": 3,
      "content": "Phước của sự sống đời đời.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:29.623Z",
      "updatedAt": "2025-04-22T10:44:29.623Z",
      "chapterId": "208e250d-0bde-4edc-83dc-76649c2864fc"
    }
  ],
