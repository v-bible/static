{
  "chapter": {
    "id": "208e250d-0bde-4edc-83dc-76649c2864fc",
    "number": 133,
    "ref": "https://www.bible.com/bible/19/PSA.133.BD2011",
    "createdAt": "2025-04-22T10:10:27.141Z",
