  },
  "verses": [
    {
      "id": "6a638a65-66ef-4f7f-8697-360e142b52d0",
      "number": 1,
      "content": "Kìa, tốt đẹp thay và phước hạnh thay",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:23.016Z",
      "updatedAt": "2025-04-24T01:59:23.016Z",
      "chapterId": "ff75dc99-79c6-463e-91a7-d3c62b3c385f"
    },
    {
      "id": "f0964f93-b335-4904-acea-469299a25869",
      "number": 1,
      "content": "Khi anh chị em sống hòa thuận với nhau!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:23.043Z",
      "updatedAt": "2025-04-24T01:59:23.043Z",
      "chapterId": "ff75dc99-79c6-463e-91a7-d3c62b3c385f"
    },
    {
      "id": "79203d91-5be6-42f2-9a49-147cd069dd89",
      "number": 2,
      "content": "Điều ấy chẳng khác gì dầu quý báu từ trên đầu chảy xuống,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:23.067Z",
      "updatedAt": "2025-04-24T01:59:23.067Z",
      "chapterId": "ff75dc99-79c6-463e-91a7-d3c62b3c385f"
    },
    {
      "id": "52574915-78c3-4179-8339-2733b8396a4c",
      "number": 2,
      "content": "Chảy xuống râu,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:23.080Z",
      "updatedAt": "2025-04-24T01:59:23.080Z",
      "chapterId": "ff75dc99-79c6-463e-91a7-d3c62b3c385f"
    },
    {
      "id": "d181367d-e024-4128-86ab-0e10686d06e1",
      "number": 2,
      "content": "Chảy xuống râu của A-rôn,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:23.093Z",
      "updatedAt": "2025-04-24T01:59:23.093Z",
      "chapterId": "ff75dc99-79c6-463e-91a7-d3c62b3c385f"
    },
    {
      "id": "95d26076-2697-434b-a2da-3704803fab8c",
      "number": 2,
      "content": "Rồi tiếp tục chảy xuống vạt áo ông.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:23.105Z",
      "updatedAt": "2025-04-24T01:59:23.105Z",
      "chapterId": "ff75dc99-79c6-463e-91a7-d3c62b3c385f"
    },
    {
      "id": "b1f14f5b-f87f-4ff2-847f-37bcd2b6df00",
      "number": 3,
      "content": "Điều ấy giống như sương móc từ Núi Hẹt-môn",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:23.117Z",
      "updatedAt": "2025-04-24T01:59:23.117Z",
      "chapterId": "ff75dc99-79c6-463e-91a7-d3c62b3c385f"
    },
    {
      "id": "a4c7b34f-7e17-4e7f-b6fd-495a4d238b11",
      "number": 3,
      "content": "Sa xuống các rặng núi Si-ôn,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:23.127Z",
      "updatedAt": "2025-04-24T01:59:23.127Z",
      "chapterId": "ff75dc99-79c6-463e-91a7-d3c62b3c385f"
    },
    {
      "id": "ea1b6161-2922-4153-96fd-92606d07c95a",
      "number": 3,
      "content": "Vì tại đó **CHÚA** ban phước,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:23.137Z",
      "updatedAt": "2025-04-24T01:59:23.137Z",
      "chapterId": "ff75dc99-79c6-463e-91a7-d3c62b3c385f"
    },
    {
      "id": "164d6422-372c-4a11-a970-025c7e7a054a",
      "number": 3,
      "content": "Phước của sự sống đời đời.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:23.147Z",
      "updatedAt": "2025-04-24T01:59:23.147Z",
      "chapterId": "ff75dc99-79c6-463e-91a7-d3c62b3c385f"
    }
  ],
