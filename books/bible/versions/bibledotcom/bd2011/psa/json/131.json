  "verses": [
    {
      "id": "5ca7e071-7449-40bd-a727-91d6a53980e5",
      "number": 1,
      "content": "**CHÚA** ôi, lòng con không dám tự cao;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:22.840Z",
      "updatedAt": "2025-04-22T10:44:22.840Z",
      "chapterId": "d299df07-1a98-44a7-8427-8c16510209ea"
    },
    {
      "id": "3d111bd6-486d-4870-8343-d8388da4730e",
      "number": 1,
      "content": "Mắt con chẳng dám kiêu kỳ;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:22.902Z",
      "updatedAt": "2025-04-22T10:44:22.902Z",
      "chapterId": "d299df07-1a98-44a7-8427-8c16510209ea"
    },
    {
      "id": "4e6190c7-2e45-4572-9e84-1b7ef39a0989",
      "number": 1,
      "content": "Con không dám tham dự vào những việc lớn lao quá sức,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:22.916Z",
      "updatedAt": "2025-04-22T10:44:22.916Z",
      "chapterId": "d299df07-1a98-44a7-8427-8c16510209ea"
    },
    {
      "id": "38fbc654-8c1c-46aa-ad57-684cbc4bbc66",
      "number": 1,
      "content": "Hay những việc vượt quá khả năng mình.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:22.927Z",
      "updatedAt": "2025-04-22T10:44:22.927Z",
      "chapterId": "d299df07-1a98-44a7-8427-8c16510209ea"
    },
    {
      "id": "e570eec3-07e3-4b2c-95bc-34ffb52311e7",
      "number": 2,
      "content": "Nhưng con đã tự trấn tĩnh và làm cho linh hồn mình lắng dịu,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:22.938Z",
      "updatedAt": "2025-04-22T10:44:22.938Z",
      "chapterId": "d299df07-1a98-44a7-8427-8c16510209ea"
    },
    {
      "id": "4edb5717-d755-4450-a51c-a95c223e1a5c",
      "number": 2,
      "content": "Như trẻ thơ no sữa nép mình bên lòng mẹ;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:22.950Z",
      "updatedAt": "2025-04-22T10:44:22.950Z",
      "chapterId": "d299df07-1a98-44a7-8427-8c16510209ea"
    },
    {
      "id": "6f09e353-bc69-4303-9266-16a344f902bd",
      "number": 2,
      "content": "Linh hồn con ở trong con như một em bé vừa no sữa mẹ.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:22.964Z",
      "updatedAt": "2025-04-22T10:44:22.964Z",
      "chapterId": "d299df07-1a98-44a7-8427-8c16510209ea"
    },
    {
      "id": "1e1513c0-64f3-45af-8c7f-11d84997d1ef",
      "number": 3,
      "content": "Hỡi I-sơ-ra-ên, hãy hy vọng nơi **CHÚA**,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:22.977Z",
      "updatedAt": "2025-04-22T10:44:22.977Z",
      "chapterId": "d299df07-1a98-44a7-8427-8c16510209ea"
    },
    {
      "id": "8523b05f-b9d4-45f9-9e5c-04184277f42b",
      "number": 3,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:22.991Z",
      "updatedAt": "2025-04-22T10:44:22.991Z",
      "chapterId": "d299df07-1a98-44a7-8427-8c16510209ea"
    }
  ],
