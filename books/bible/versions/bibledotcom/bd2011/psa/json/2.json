    {
      "id": "8e9689b9-8b0d-447d-941c-2d63fbf40adc",
      "content": "Sự Trị Vì của Đấng Được Xức Dầu",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T10:35:07.093Z",
      "updatedAt": "2025-04-22T10:35:07.093Z",
