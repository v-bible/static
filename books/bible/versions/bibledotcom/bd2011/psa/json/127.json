{
  "chapter": {
    "id": "a266e964-2f7f-4232-9534-b01bbec8beef",
    "number": 127,
    "ref": "https://www.bible.com/bible/19/PSA.127.BD2011",
    "createdAt": "2025-04-22T10:10:27.077Z",
    "updatedAt": "2025-04-22T10:10:27.077Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "0d718980-0c23-427a-bcf7-6a0356beb389",
      "number": 1,
      "content": "Nếu **CHÚA** không xây dựng nhà,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.742Z",
      "updatedAt": "2025-04-22T10:44:09.742Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "850c9d61-b37d-4235-88d8-e77270fa07fa",
      "number": 1,
      "content": "Những thợ xây làm việc uổng công;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.783Z",
      "updatedAt": "2025-04-22T10:44:09.783Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "03f3459e-0a28-4f4a-9eda-58fe93ce82db",
      "number": 1,
      "content": "Nếu **CHÚA** không canh giữ thành,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.794Z",
      "updatedAt": "2025-04-22T10:44:09.794Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "fdc2455f-1f29-4f89-8100-7a86ef2aafdd",
      "number": 1,
      "content": "Những lính canh thức canh uổng công.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.805Z",
      "updatedAt": "2025-04-22T10:44:09.805Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "133564e2-7dfd-480b-ad13-75da2b2a2152",
      "number": 2,
      "content": "Uổng công thay cho các bạn là những người dậy sớm,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.817Z",
      "updatedAt": "2025-04-22T10:44:09.817Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "9ca4dd87-2f14-48f2-839f-916da5cfc713",
      "number": 2,
      "content": "Thức khuya, và ăn uống kham khổ,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.832Z",
      "updatedAt": "2025-04-22T10:44:09.832Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "4c2b3d85-30cf-4ed9-a01f-d4a805e4ab18",
      "number": 2,
      "content": "Vì Chúa sẽ cho người Ngài thương no đủ,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.845Z",
      "updatedAt": "2025-04-22T10:44:09.845Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "5b782b1a-ddb6-483a-a45c-2e5657e139e5",
      "number": 2,
      "content": "Ngay cả khi họ đang an giấc ngủ say.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.860Z",
      "updatedAt": "2025-04-22T10:44:09.860Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "5607e2c1-8231-4f65-9f2f-7c9b6605b687",
      "number": 3,
      "content": "Kìa, con cái là cơ nghiệp của **CHÚA** ban cho;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.872Z",
      "updatedAt": "2025-04-22T10:44:09.872Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "2d7c5ae5-2994-4a25-b2c2-fabf5f9eb716",
      "number": 3,
      "content": "Bông trái của tử cung là phần thưởng của Ngài trao tặng.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.885Z",
      "updatedAt": "2025-04-22T10:44:09.885Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "1f8cc615-fa32-47f2-b514-03759d476f07",
      "number": 4,
      "content": "Con cái sinh ra trong tuổi thanh xuân",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.898Z",
      "updatedAt": "2025-04-22T10:44:09.898Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "ecdc525a-5ad1-4a70-8e97-1d6d5ec2ed6a",
      "number": 4,
      "content": "Khác nào các mũi tên trong tay dũng sĩ.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.910Z",
      "updatedAt": "2025-04-22T10:44:09.910Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "0e7786d7-096f-46c8-bb00-fa84a98738de",
      "number": 5,
      "content": "Phước cho người",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.924Z",
      "updatedAt": "2025-04-22T10:44:09.924Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "6ba7dba5-262c-444d-bd36-837d86d5a1d3",
      "number": 5,
      "content": "Có đầy chúng trong ống tên mình;",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.934Z",
      "updatedAt": "2025-04-22T10:44:09.934Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "ed15d541-0cda-4f09-a9c8-99185f091dd6",
      "number": 5,
      "content": "Họ sẽ không bẽ mặt",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.944Z",
      "updatedAt": "2025-04-22T10:44:09.944Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    },
    {
      "id": "9686e33a-2eba-4d9a-af36-80c8917660c1",
      "number": 5,
      "content": "Khi đối đáp với kẻ thù tại cổng thành.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:09.954Z",
      "updatedAt": "2025-04-22T10:44:09.954Z",
      "chapterId": "a266e964-2f7f-4232-9534-b01bbec8beef"
    }
  ],
