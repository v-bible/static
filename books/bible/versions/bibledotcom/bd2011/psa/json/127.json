{
  "chapter": {
    "id": "52b4d024-2648-47b9-96e3-b3797d3b8fca",
    "number": 127,
    "ref": "https://www.bible.com/bible/19/PSA.127.BD2011",
    "createdAt": "2024-09-23T05:08:40.642Z",
    "updatedAt": "2024-09-23T05:08:40.642Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "ba5bf572-ef7d-424d-9ab8-de0639fbacc4",
      "number": 1,
      "content": "Nếu **CHÚA** không xây dựng nhà,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.784Z",
      "updatedAt": "2024-09-23T05:55:06.784Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "22cf8c4e-a150-4720-a72c-3ff881c75a58",
      "number": 1,
      "content": "Những thợ xây làm việc uổng công;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.800Z",
      "updatedAt": "2024-09-23T05:55:06.800Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "3b0d3341-8a27-4262-85a6-ee4b91401886",
      "number": 1,
      "content": "Nếu **CHÚA** không canh giữ thành,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.803Z",
      "updatedAt": "2024-09-23T05:55:06.803Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "4b745b96-897e-44f3-a8fd-624a49a28594",
      "number": 1,
      "content": "Những lính canh thức canh uổng công.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.808Z",
      "updatedAt": "2024-09-23T05:55:06.808Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "4295c35d-091d-4b47-a39e-264750e7f472",
      "number": 2,
      "content": "Uổng công thay cho các bạn là những người dậy sớm,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.813Z",
      "updatedAt": "2024-09-23T05:55:06.813Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "bf258c20-3d65-4819-8a45-742a2d5b714f",
      "number": 2,
      "content": "Thức khuya, và ăn uống kham khổ,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.818Z",
      "updatedAt": "2024-09-23T05:55:06.818Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "43ef3658-7782-4c2f-9daf-1df0740a3f75",
      "number": 2,
      "content": "Vì Chúa sẽ cho người Ngài thương no đủ,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.823Z",
      "updatedAt": "2024-09-23T05:55:06.823Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "e23a5542-ddc5-4ce7-9382-c4b084c24ed9",
      "number": 2,
      "content": "Ngay cả khi họ đang an giấc ngủ say.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.828Z",
      "updatedAt": "2024-09-23T05:55:06.828Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "a898cb39-1c9d-4b50-a659-2029ed3bb784",
      "number": 3,
      "content": "Kìa, con cái là cơ nghiệp của **CHÚA** ban cho;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.833Z",
      "updatedAt": "2024-09-23T05:55:06.833Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "1b927721-0805-4b24-a42d-28f79bfdde4e",
      "number": 3,
      "content": "Bông trái của tử cung là phần thưởng của Ngài trao tặng.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.838Z",
      "updatedAt": "2024-09-23T05:55:06.838Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "42e6b79b-22f8-44a2-944c-1c6d7eb9f1dd",
      "number": 4,
      "content": "Con cái sinh ra trong tuổi thanh xuân",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.842Z",
      "updatedAt": "2024-09-23T05:55:06.842Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "9de5df82-a403-450b-a760-a47124975328",
      "number": 4,
      "content": "Khác nào các mũi tên trong tay dũng sĩ.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.847Z",
      "updatedAt": "2024-09-23T05:55:06.847Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "45a4b946-e0f9-4432-a1c1-577b059b26e8",
      "number": 5,
      "content": "Phước cho người",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.852Z",
      "updatedAt": "2024-09-23T05:55:06.852Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "40bff62a-9652-4d5e-bf12-7e1011ddbded",
      "number": 5,
      "content": "Có đầy chúng trong ống tên mình;",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.857Z",
      "updatedAt": "2024-09-23T05:55:06.857Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "ec7543e9-8fe9-4e01-ab19-fe520e7af4cb",
      "number": 5,
      "content": "Họ sẽ không bẽ mặt",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.862Z",
      "updatedAt": "2024-09-23T05:55:06.862Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    },
    {
      "id": "cfc0fa00-d3f3-4e73-a2d8-bd17a7b48f6a",
      "number": 5,
      "content": "Khi đối đáp với kẻ thù tại cổng thành.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:06.867Z",
      "updatedAt": "2024-09-23T05:55:06.867Z",
      "chapterId": "52b4d024-2648-47b9-96e3-b3797d3b8fca"
    }
  ],
