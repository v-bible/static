{
  "chapter": {
    "id": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f",
    "number": 127,
    "ref": "https://www.bible.com/bible/19/PSA.127.BD2011",
    "createdAt": "2025-04-24T01:29:07.412Z",
    "updatedAt": "2025-04-24T01:29:07.412Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "7e952282-1aef-43b3-9866-ae12a31445a6",
      "number": 1,
      "content": "Nếu **CHÚA** không xây dựng nhà,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.263Z",
      "updatedAt": "2025-04-24T01:59:02.263Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "3a0e3e26-f976-4b21-a2f6-3e12dd7912ae",
      "number": 1,
      "content": "Những thợ xây làm việc uổng công;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.291Z",
      "updatedAt": "2025-04-24T01:59:02.291Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "5786a851-0b6a-4103-b734-4dec85f3e15b",
      "number": 1,
      "content": "Nếu **CHÚA** không canh giữ thành,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.301Z",
      "updatedAt": "2025-04-24T01:59:02.301Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "ba5dd9fb-d2eb-4ba5-b63f-0b8bd56d2633",
      "number": 1,
      "content": "Những lính canh thức canh uổng công.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.311Z",
      "updatedAt": "2025-04-24T01:59:02.311Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "d6d362eb-ecb5-4145-ba21-268ebccea19c",
      "number": 2,
      "content": "Uổng công thay cho các bạn là những người dậy sớm,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.322Z",
      "updatedAt": "2025-04-24T01:59:02.322Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "6ac701a2-912d-413d-abdf-47bb524e6185",
      "number": 2,
      "content": "Thức khuya, và ăn uống kham khổ,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.334Z",
      "updatedAt": "2025-04-24T01:59:02.334Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "ee39944e-2dc0-42f8-a92b-e07ca8cadd1c",
      "number": 2,
      "content": "Vì Chúa sẽ cho người Ngài thương no đủ,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.347Z",
      "updatedAt": "2025-04-24T01:59:02.347Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "9823c888-467f-4de3-af28-ccafaa92cb03",
      "number": 2,
      "content": "Ngay cả khi họ đang an giấc ngủ say.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.360Z",
      "updatedAt": "2025-04-24T01:59:02.360Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "c721358d-4761-4f47-afd1-1158d05ab01b",
      "number": 3,
      "content": "Kìa, con cái là cơ nghiệp của **CHÚA** ban cho;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.373Z",
      "updatedAt": "2025-04-24T01:59:02.373Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "ccb72899-a7f7-4c32-992d-efcddbabde2d",
      "number": 3,
      "content": "Bông trái của tử cung là phần thưởng của Ngài trao tặng.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.387Z",
      "updatedAt": "2025-04-24T01:59:02.387Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "4ba495ae-74de-425d-a2d7-4183bc9c28c2",
      "number": 4,
      "content": "Con cái sinh ra trong tuổi thanh xuân",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.400Z",
      "updatedAt": "2025-04-24T01:59:02.400Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "dbccb6a1-e2a9-45c7-9808-964b4690da59",
      "number": 4,
      "content": "Khác nào các mũi tên trong tay dũng sĩ.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.415Z",
      "updatedAt": "2025-04-24T01:59:02.415Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "9cc5358b-b148-49af-b0cf-965bb3ef3ad9",
      "number": 5,
      "content": "Phước cho người",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.429Z",
      "updatedAt": "2025-04-24T01:59:02.429Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "31443acf-26c1-4bc8-a9db-9e7047377126",
      "number": 5,
      "content": "Có đầy chúng trong ống tên mình;",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.443Z",
      "updatedAt": "2025-04-24T01:59:02.443Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "ef1bf043-719a-4701-93b0-03a94296076d",
      "number": 5,
      "content": "Họ sẽ không bẽ mặt",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.457Z",
      "updatedAt": "2025-04-24T01:59:02.457Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    },
    {
      "id": "0946e440-8e1f-4e91-b9af-f7315163cde9",
      "number": 5,
      "content": "Khi đối đáp với kẻ thù tại cổng thành.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:02.470Z",
      "updatedAt": "2025-04-24T01:59:02.470Z",
      "chapterId": "21c5f01b-d10c-4a9f-b0e1-600e6e98b01f"
    }
  ],
