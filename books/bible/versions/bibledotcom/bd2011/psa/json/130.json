{
  "chapter": {
    "id": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91",
    "number": 130,
    "ref": "https://www.bible.com/bible/19/PSA.130.BD2011",
    "createdAt": "2025-04-24T01:29:07.447Z",
    "updatedAt": "2025-04-24T01:29:07.447Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "e726d490-edab-4c72-8e2d-f75830b5c988",
      "number": 1,
      "content": "Lạy **CHÚA**, từ chốn vực sâu con kêu cầu Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:14.922Z",
      "updatedAt": "2025-04-24T01:59:14.922Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "6dd1a1cf-382d-449c-acc5-84cae3dd11cd",
      "number": 2,
      "content": "Chúa ôi, xin nghe tiếng cầu xin của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:14.950Z",
      "updatedAt": "2025-04-24T01:59:14.950Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "f0754739-f0d9-4c5b-b9f7-b996bba0bf03",
      "number": 2,
      "content": "Xin lắng tai nghe",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:14.960Z",
      "updatedAt": "2025-04-24T01:59:14.960Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "49d394d4-ab40-4854-aaf9-815188c629cd",
      "number": 2,
      "content": "Lời cầu nguyện của con.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:14.970Z",
      "updatedAt": "2025-04-24T01:59:14.970Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "acb1e1b0-dec1-491d-8634-1785b4e648de",
      "number": 3,
      "content": "Lạy **CHÚA**, nếu Ngài ghi nhớ mọi tội lỗi chúng con,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:14.980Z",
      "updatedAt": "2025-04-24T01:59:14.980Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "8b794b3f-6ddc-47c3-8a80-b2fe9970aa7f",
      "number": 3,
      "content": "Chúa ôi, còn ai đứng nổi trước mặt Ngài?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:14.991Z",
      "updatedAt": "2025-04-24T01:59:14.991Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "e7f09304-8b7a-4c42-983a-2b2d4f8226b5",
      "number": 4,
      "content": "Nhưng vì Ngài ban ơn tha thứ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.002Z",
      "updatedAt": "2025-04-24T01:59:15.002Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "ce344664-676b-4ade-83fc-6be7d3ceb605",
      "number": 4,
      "content": "Nên chúng con càng kính sợ Ngài hơn.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.014Z",
      "updatedAt": "2025-04-24T01:59:15.014Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "617a97d2-86a3-49f4-87f8-0089f732bad0",
      "number": 5,
      "content": "Tôi trông đợi **CHÚA**, linh hồn tôi trông đợi Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.028Z",
      "updatedAt": "2025-04-24T01:59:15.028Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "97d07506-c5e8-4796-914b-faee48980673",
      "number": 5,
      "content": "Tôi đặt trọn hy vọng nơi lời Ngài.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.041Z",
      "updatedAt": "2025-04-24T01:59:15.041Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "a408a886-a8e4-4912-b15a-a68ceab5823c",
      "number": 6,
      "content": "Linh hồn tôi trông đợi Chúa",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.056Z",
      "updatedAt": "2025-04-24T01:59:15.056Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "22b8452f-78ef-4394-bdc9-00a1be17990d",
      "number": 6,
      "content": "Hơn người lính canh trông đợi sáng,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.069Z",
      "updatedAt": "2025-04-24T01:59:15.069Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "5a7bd4fa-0e14-49f3-81b4-1c88d4545dc6",
      "number": 6,
      "content": "Thật hơn người lính canh trông đợi sáng.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.083Z",
      "updatedAt": "2025-04-24T01:59:15.083Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "c28e8aa0-298e-43c3-b2e9-abb4875b1ceb",
      "number": 7,
      "content": "Hỡi I-sơ-ra-ên, hãy đặt trọn hy vọng nơi **CHÚA**,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.096Z",
      "updatedAt": "2025-04-24T01:59:15.096Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "580b6282-e6d3-42d2-8793-c95582b06511",
      "number": 7,
      "content": "Vì nơi **CHÚA** có tình thương;",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.110Z",
      "updatedAt": "2025-04-24T01:59:15.110Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "183ad4f1-2d0d-4a43-b1ae-638ad447614d",
      "number": 7,
      "content": "Ngài ban ơn cứu chuộc cách dồi dào.",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.123Z",
      "updatedAt": "2025-04-24T01:59:15.123Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "d7c0baf6-fea0-4857-a5d0-ba74e45828bb",
      "number": 8,
      "content": "Ngài sẽ cứu chuộc I-sơ-ra-ên",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.136Z",
      "updatedAt": "2025-04-24T01:59:15.136Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    },
    {
      "id": "09500e40-0756-4cc5-8d90-ed554b94c101",
      "number": 8,
      "content": "Khỏi mọi tội lỗi của họ.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:15.150Z",
      "updatedAt": "2025-04-24T01:59:15.150Z",
      "chapterId": "67f81d19-0e1d-4b25-b915-bf8d6cc8fd91"
    }
  ],
