{
  "chapter": {
    "id": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9",
    "number": 91,
    "ref": "https://www.bible.com/bible/19/PSA.91.BD2011",
    "createdAt": "2025-04-24T01:29:06.912Z",
    "updatedAt": "2025-04-24T01:29:06.912Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "3549405f-9863-47e9-b81f-d2bf0aedeb14",
      "number": 1,
      "content": "Người nào ở nơi kín đáo của Đấng Tối Cao",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.118Z",
      "updatedAt": "2025-04-24T01:56:31.118Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "bd725b6e-7fb7-4b7d-b1cf-7ac6f851b93b",
      "number": 1,
      "content": "Sẽ được hằng ở dưới bóng của Đấng Toàn Năng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.145Z",
      "updatedAt": "2025-04-24T01:56:31.145Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "518b1713-5401-411a-8bf8-16e1a2ad8ace",
      "number": 2,
      "content": "Tôi thưa với **CHÚA** rằng, “Ngài là nơi ẩn náu của con và là thành trì của con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.155Z",
      "updatedAt": "2025-04-24T01:56:31.155Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "93e07442-30ce-4d80-8a83-981e35c3b2f9",
      "number": 2,
      "content": "Ngài là Đức Chúa Trời của con; con tin cậy nơi Ngài.”",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.166Z",
      "updatedAt": "2025-04-24T01:56:31.166Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "994aa149-ed74-4df4-9983-824ebf90bda0",
      "number": 3,
      "content": "Thật vậy Ngài sẽ giải cứu bạn khỏi cạm bẫy nguy hiểm",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.176Z",
      "updatedAt": "2025-04-24T01:56:31.176Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "897f2205-e1c5-4c05-a85f-fdc1730479d6",
      "number": 3,
      "content": "Và khỏi ôn dịch chết người.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.197Z",
      "updatedAt": "2025-04-24T01:56:31.197Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "f4fd20a7-6f7f-4c94-a439-77b72f782933",
      "number": 4,
      "content": "Ngài sẽ lấy lông Ngài bao phủ bạn;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.207Z",
      "updatedAt": "2025-04-24T01:56:31.207Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "244ddce5-8a9d-4c55-b680-41d1eed4b0ae",
      "number": 4,
      "content": "Dưới cánh Ngài bạn sẽ tìm được nơi nương náu an toàn.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.219Z",
      "updatedAt": "2025-04-24T01:56:31.219Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "fa630e77-9826-4e91-b604-e379b534f0b7",
      "number": 4,
      "content": "Đức thành tín Ngài là thuẫn khiên và mộc che của bạn.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.232Z",
      "updatedAt": "2025-04-24T01:56:31.232Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "7c8fbea7-0466-4387-bd38-64a5fd7fb46f",
      "number": 5,
      "content": "Bạn sẽ chẳng sợ dù sự khủng bố xảy đến ban đêm,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.246Z",
      "updatedAt": "2025-04-24T01:56:31.246Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "d5f84c16-b358-4eb0-a2a5-881410ecbf1f",
      "number": 5,
      "content": "Hoặc tên bay ban ngày,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.271Z",
      "updatedAt": "2025-04-24T01:56:31.271Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "ef2a1ffe-c34e-4f32-9153-b6a609bc6ad8",
      "number": 6,
      "content": "Hoặc ôn dịch lan truyền trong đêm khuya,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.299Z",
      "updatedAt": "2025-04-24T01:56:31.299Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "a8870a15-8b4c-4341-8674-1d48c8382da8",
      "number": 6,
      "content": "Hoặc sự hủy diệt tàn phá lúc giữa trưa.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.312Z",
      "updatedAt": "2025-04-24T01:56:31.312Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "6ed70c9c-96e0-48e4-8ba4-57a7720c8472",
      "number": 7,
      "content": "Hằng ngàn người có thể ngã xuống bên cạnh bạn,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.338Z",
      "updatedAt": "2025-04-24T01:56:31.338Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "f59e23f3-3981-4853-ad74-7b06266df0ce",
      "number": 7,
      "content": "Hằng vạn người gục ngã bên phải bạn,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.352Z",
      "updatedAt": "2025-04-24T01:56:31.352Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "b240ecad-c889-4bf0-8648-cb1a79e6cdb9",
      "number": 7,
      "content": "Nhưng nguy hiểm sẽ chẳng đến gần bạn.",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.365Z",
      "updatedAt": "2025-04-24T01:56:31.365Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "61ed676f-2473-4529-ac66-75a2f4cc0cd8",
      "number": 8,
      "content": "Bạn sẽ nhìn thấy tận mắt mình;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.378Z",
      "updatedAt": "2025-04-24T01:56:31.378Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "a51d0ba9-8538-49e2-9d85-475a6174d39d",
      "number": 8,
      "content": "Bạn sẽ thấy kẻ ác bị báo trả.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.392Z",
      "updatedAt": "2025-04-24T01:56:31.392Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "9a15029d-8402-4586-a219-ad711aea10c0",
      "number": 9,
      "content": "Vì bạn đã lấy **CHÚA** làm nơi nương náu mình,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.405Z",
      "updatedAt": "2025-04-24T01:56:31.405Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "3c8199c3-1b4f-4c86-9e68-66756af5d12b",
      "number": 9,
      "content": "Lấy Đấng Tối Cao làm nơi trú ngụ mình,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.419Z",
      "updatedAt": "2025-04-24T01:56:31.419Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "79f73738-973c-4c8b-b0e0-c77af7b56fb4",
      "number": 10,
      "content": "Nên sẽ chẳng có tai họa gì xảy đến với bạn,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.433Z",
      "updatedAt": "2025-04-24T01:56:31.433Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "f05535a4-b9ec-48ba-991f-e8a8e2ebe93e",
      "number": 10,
      "content": "Cũng chẳng có ôn dịch nào tới gần nhà bạn;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.447Z",
      "updatedAt": "2025-04-24T01:56:31.447Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "4b0007d1-ebf0-4d2b-ab14-efc0150d5493",
      "number": 11,
      "content": "Vì Ngài sẽ ra lịnh cho các thiên sứ Ngài gìn giữ bạn,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.474Z",
      "updatedAt": "2025-04-24T01:56:31.474Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "7adae10b-3960-46f3-b50b-a035b7015760",
      "number": 11,
      "content": "Để bảo vệ bạn trong mọi đường lối của bạn.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.488Z",
      "updatedAt": "2025-04-24T01:56:31.488Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "c3f484e0-b9f1-4bae-b2a3-d5982d666993",
      "number": 12,
      "content": "Các thiên sứ sẽ nâng bạn trên đôi tay họ,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.501Z",
      "updatedAt": "2025-04-24T01:56:31.501Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "a87a3d3e-b587-4e68-97d1-cc59e888a18c",
      "number": 12,
      "content": "Kẻo chân bạn vấp phải đá chăng.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.514Z",
      "updatedAt": "2025-04-24T01:56:31.514Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "a937c28e-8c69-451a-9438-f1f24374eb18",
      "number": 13,
      "content": "Bạn sẽ giẫm lên sư tử và rắn hổ mang;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.528Z",
      "updatedAt": "2025-04-24T01:56:31.528Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "7a9080f5-448f-42fb-99d3-92da7aa18b5b",
      "number": 13,
      "content": "Bạn sẽ chà đạp dưới chân sư tử tơ và rắn độc.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.542Z",
      "updatedAt": "2025-04-24T01:56:31.542Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "5c0967c0-6f1a-4ce6-a457-f9b1d5b1d6a5",
      "number": 14,
      "content": "Vì người ấy yêu kính Ta, nên Ta sẽ giải cứu người ấy;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.569Z",
      "updatedAt": "2025-04-24T01:56:31.569Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "ecc7d856-4005-4f1c-9f8f-dbd37fd94ceb",
      "number": 14,
      "content": "Ta sẽ đặt người ấy lên nơi cao, vì người ấy biết danh Ta.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.582Z",
      "updatedAt": "2025-04-24T01:56:31.582Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "2284e0e5-6950-4c26-96c7-472332047099",
      "number": 15,
      "content": "Khi người ấy kêu cầu Ta, Ta sẽ đáp lời người ấy;",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.608Z",
      "updatedAt": "2025-04-24T01:56:31.608Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "95281d56-aef0-4a61-9a7f-7662ee6330b7",
      "number": 15,
      "content": "Trong lúc nguy nan, Ta sẽ ở cùng người ấy;",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.622Z",
      "updatedAt": "2025-04-24T01:56:31.622Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "cfa5fb19-61d6-4a6d-a091-3cd6bf4ab96f",
      "number": 15,
      "content": "Ta sẽ giải cứu người ấy và làm cho người ấy được tôn trọng.",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.636Z",
      "updatedAt": "2025-04-24T01:56:31.636Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "59c49692-9d9f-47ba-b5b0-88e4d72a7e62",
      "number": 16,
      "content": "Ta sẽ cho người ấy được thỏa lòng sống lâu,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.649Z",
      "updatedAt": "2025-04-24T01:56:31.649Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    },
    {
      "id": "7db4dc84-3663-48a0-9fcb-549517d92d2b",
      "number": 16,
      "content": "Và chỉ cho người ấy ơn cứu rỗi của Ta.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:31.663Z",
      "updatedAt": "2025-04-24T01:56:31.663Z",
      "chapterId": "f1fe6439-18ac-46ec-b38c-c18a7d4d3ce9"
    }
  ],
