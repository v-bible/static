    },
    {
      "id": "978c87cd-e695-4d7a-b3d4-e140c6ce343e",
      "content": "ctd: bom nổ tàn phá lúc giữa trưa",
      "position": 39,
      "order": 3,
      "createdAt": "2025-04-22T10:41:21.880Z",
      "updatedAt": "2025-04-22T10:41:21.880Z",
      "verseId": "9e074289-fb1c-41e8-8690-6ef888fe6cee",
      "chapterId": "00e87da7-9010-4ac0-805b-fa6d1d94c373"
    },
