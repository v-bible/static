{
  "chapter": {
    "id": "04bb8057-598e-4c69-9d12-a3b0554884d0",
    "number": 91,
    "ref": "https://www.bible.com/bible/19/PSA.91.BD2011",
    "createdAt": "2024-09-23T05:08:40.525Z",
    "updatedAt": "2024-09-23T05:08:40.525Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "6e15ee15-c8d9-411c-81a1-dfa26fe7cdf5",
      "number": 1,
      "content": "Người nào ở nơi kín đáo của Đấng Tối Cao",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:56.962Z",
      "updatedAt": "2024-09-23T05:51:56.962Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "a6089335-e685-47c1-b99c-7beec469db45",
      "number": 1,
      "content": "Sẽ được hằng ở dưới bóng của Đấng Toàn Năng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:56.991Z",
      "updatedAt": "2024-09-23T05:51:56.991Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "9e26398d-8e84-4f4d-97ee-785026627c4d",
      "number": 2,
      "content": "Tôi thưa với **CHÚA** rằng, “Ngài là nơi ẩn náu của con và là thành trì của con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:56.996Z",
      "updatedAt": "2024-09-23T05:51:56.996Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "2c6736c9-e6f2-4189-a547-19b07bd1895b",
      "number": 2,
      "content": "Ngài là Đức Chúa Trời của con; con tin cậy nơi Ngài.”",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.001Z",
      "updatedAt": "2024-09-23T05:51:57.001Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "dc5c9a1e-92d0-401c-9547-e3224f9f5c3b",
      "number": 3,
      "content": "Thật vậy Ngài sẽ giải cứu bạn khỏi cạm bẫy nguy hiểm",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.006Z",
      "updatedAt": "2024-09-23T05:51:57.006Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "b58363ed-5314-4816-8eee-06530aef0a20",
      "number": 3,
      "content": "Và khỏi ôn dịch chết người.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.016Z",
      "updatedAt": "2024-09-23T05:51:57.016Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "82e07ab2-5327-49d5-a717-4e9f07ce3069",
      "number": 4,
      "content": "Ngài sẽ lấy lông Ngài bao phủ bạn;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.021Z",
      "updatedAt": "2024-09-23T05:51:57.021Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "ade04f03-b631-42b9-8bb9-0bb6aeae4b87",
      "number": 4,
      "content": "Dưới cánh Ngài bạn sẽ tìm được nơi nương náu an toàn.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.026Z",
      "updatedAt": "2024-09-23T05:51:57.026Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "22ad3225-d649-450e-8bda-887e02c6d4a6",
      "number": 4,
      "content": "Đức thành tín Ngài là thuẫn khiên và mộc che của bạn.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.030Z",
      "updatedAt": "2024-09-23T05:51:57.030Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "713ed229-b21f-4383-930e-71e2e0dfa455",
      "number": 5,
      "content": "Bạn sẽ chẳng sợ dù sự khủng bố xảy đến ban đêm,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.035Z",
      "updatedAt": "2024-09-23T05:51:57.035Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "3543f0bb-fc92-46a6-92d3-d3b02a5a735b",
      "number": 5,
      "content": "Hoặc tên bay ban ngày,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.044Z",
      "updatedAt": "2024-09-23T05:51:57.044Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "3cc20680-328e-4297-ad7f-7828e81f9a58",
      "number": 6,
      "content": "Hoặc ôn dịch lan truyền trong đêm khuya,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.054Z",
      "updatedAt": "2024-09-23T05:51:57.054Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "394a3f07-5f63-4ba8-a413-14c73193ce48",
      "number": 6,
      "content": "Hoặc sự hủy diệt tàn phá lúc giữa trưa.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.059Z",
      "updatedAt": "2024-09-23T05:51:57.059Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "2cd5a23c-6692-46b2-9637-d30cdc1b1dfb",
      "number": 7,
      "content": "Hằng ngàn người có thể ngã xuống bên cạnh bạn,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.069Z",
      "updatedAt": "2024-09-23T05:51:57.069Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "223c156b-8b2a-4d17-8e1d-abaded3cfa49",
      "number": 7,
      "content": "Hằng vạn người gục ngã bên phải bạn,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.074Z",
      "updatedAt": "2024-09-23T05:51:57.074Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "f7c21162-7965-4fda-b3c0-79ec4b0c5f96",
      "number": 7,
      "content": "Nhưng nguy hiểm sẽ chẳng đến gần bạn.",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.080Z",
      "updatedAt": "2024-09-23T05:51:57.080Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "c1cd50cb-5598-4508-8c09-3feff14ded5c",
      "number": 8,
      "content": "Bạn sẽ nhìn thấy tận mắt mình;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.085Z",
      "updatedAt": "2024-09-23T05:51:57.085Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "4b79acf5-2fd0-47a4-a69e-27ed2d5afc9a",
      "number": 8,
      "content": "Bạn sẽ thấy kẻ ác bị báo trả.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.089Z",
      "updatedAt": "2024-09-23T05:51:57.089Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "f2456e93-15e2-43b7-8209-c220df21a6e8",
      "number": 9,
      "content": "Vì bạn đã lấy **CHÚA** làm nơi nương náu mình,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.094Z",
      "updatedAt": "2024-09-23T05:51:57.094Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "37f5cf50-c8dc-4aad-97f0-33ea5d83654e",
      "number": 9,
      "content": "Lấy Đấng Tối Cao làm nơi trú ngụ mình,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.098Z",
      "updatedAt": "2024-09-23T05:51:57.098Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "dd862a30-d43f-45d6-ab54-281582bff55f",
      "number": 10,
      "content": "Nên sẽ chẳng có tai họa gì xảy đến với bạn,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.103Z",
      "updatedAt": "2024-09-23T05:51:57.103Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "4d76beee-c5c4-4df5-a145-7d824a95657d",
      "number": 10,
      "content": "Cũng chẳng có ôn dịch nào tới gần nhà bạn;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.108Z",
      "updatedAt": "2024-09-23T05:51:57.108Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "6d7bd643-ec0b-49b3-972f-39b162d289da",
      "number": 11,
      "content": "Vì Ngài sẽ ra lịnh cho các thiên sứ Ngài gìn giữ bạn,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.118Z",
      "updatedAt": "2024-09-23T05:51:57.118Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "0b6f44c5-ad43-40c9-b7cb-91a4e3208141",
      "number": 11,
      "content": "Để bảo vệ bạn trong mọi đường lối của bạn.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.123Z",
      "updatedAt": "2024-09-23T05:51:57.123Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "726727fa-b266-4f7f-b9b8-6f848156e39b",
      "number": 12,
      "content": "Các thiên sứ sẽ nâng bạn trên đôi tay họ,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.128Z",
      "updatedAt": "2024-09-23T05:51:57.128Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "e2d3be4d-585b-4b4c-bcb6-0711822c2146",
      "number": 12,
      "content": "Kẻo chân bạn vấp phải đá chăng.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.132Z",
      "updatedAt": "2024-09-23T05:51:57.132Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "898fae7b-6262-41fc-96cb-9b8706ce219a",
      "number": 13,
      "content": "Bạn sẽ giẫm lên sư tử và rắn hổ mang;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.137Z",
      "updatedAt": "2024-09-23T05:51:57.137Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "e2d11de2-6213-4887-96d9-4e0fb794ee2e",
      "number": 13,
      "content": "Bạn sẽ chà đạp dưới chân sư tử tơ và rắn độc.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.142Z",
      "updatedAt": "2024-09-23T05:51:57.142Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "88a8b75a-bd1d-452c-bf6e-b5c453c29714",
      "number": 14,
      "content": "Vì người ấy yêu kính Ta, nên Ta sẽ giải cứu người ấy;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.152Z",
      "updatedAt": "2024-09-23T05:51:57.152Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "775c31b2-a954-465e-b15a-01833307eb10",
      "number": 14,
      "content": "Ta sẽ đặt người ấy lên nơi cao, vì người ấy biết danh Ta.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.157Z",
      "updatedAt": "2024-09-23T05:51:57.157Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "7580ee46-7de1-49d6-abea-b061f79c551d",
      "number": 15,
      "content": "Khi người ấy kêu cầu Ta, Ta sẽ đáp lời người ấy;",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.167Z",
      "updatedAt": "2024-09-23T05:51:57.167Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "9058ebff-ee52-4087-812d-b2db01075154",
      "number": 15,
      "content": "Trong lúc nguy nan, Ta sẽ ở cùng người ấy;",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.172Z",
      "updatedAt": "2024-09-23T05:51:57.172Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "49e81eb0-653c-4bbb-909e-4d223d8e71d2",
      "number": 15,
      "content": "Ta sẽ giải cứu người ấy và làm cho người ấy được tôn trọng.",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.176Z",
      "updatedAt": "2024-09-23T05:51:57.176Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "62aad638-82c7-4ce7-99a5-c772eb04c99d",
      "number": 16,
      "content": "Ta sẽ cho người ấy được thỏa lòng sống lâu,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.181Z",
      "updatedAt": "2024-09-23T05:51:57.181Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
    {
      "id": "41e721fb-04d5-4616-a592-2e2fd4f8ea4b",
      "number": 16,
      "content": "Và chỉ cho người ấy ơn cứu rỗi của Ta.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:57.186Z",
      "updatedAt": "2024-09-23T05:51:57.186Z",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    }
  ],
