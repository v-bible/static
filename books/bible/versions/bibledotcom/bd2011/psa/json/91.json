    },
    {
      "id": "570167d6-d1bc-4036-b701-eca41c8f5d9f",
      "content": "ctd: bom nổ tàn phá lúc giữa trưa",
      "position": 39,
      "order": 3,
      "createdAt": "2024-09-23T05:51:57.064Z",
      "updatedAt": "2024-09-23T05:51:57.064Z",
      "verseId": "394a3f07-5f63-4ba8-a413-14c73193ce48",
      "chapterId": "04bb8057-598e-4c69-9d12-a3b0554884d0"
    },
