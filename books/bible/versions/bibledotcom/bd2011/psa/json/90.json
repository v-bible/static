{
  "chapter": {
    "id": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16",
    "number": 90,
    "ref": "https://www.bible.com/bible/19/PSA.90.BD2011",
    "createdAt": "2025-04-24T01:29:06.898Z",
    "updatedAt": "2025-04-24T01:29:06.898Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "633cb197-58e1-4d3e-ab27-de4880eb8bf5",
      "number": 1,
      "content": "Lạy Chúa, Ngài là nơi cư trú của chúng con",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:27.868Z",
      "updatedAt": "2025-04-24T01:56:27.868Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "86915b53-7194-43e9-b904-80688e6dba54",
      "number": 1,
      "content": "Trải qua mọi thế hệ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:27.900Z",
      "updatedAt": "2025-04-24T01:56:27.900Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "e4671ca4-d3f0-4ccd-b435-582cce5d18f5",
      "number": 2,
      "content": "Trước khi núi non thành hình,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:27.913Z",
      "updatedAt": "2025-04-24T01:56:27.913Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "d9ca0927-986c-47c7-9d48-6d4f0a0a1011",
      "number": 2,
      "content": "Trước khi Ngài tạo thành trái đất và thế gian,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:27.927Z",
      "updatedAt": "2025-04-24T01:56:27.927Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "26061c3e-b934-45d6-b51e-efc1404976c3",
      "number": 2,
      "content": "Từ vô cực quá khứ đến vô cực tương lai Ngài là Đức Chúa Trời.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:27.941Z",
      "updatedAt": "2025-04-24T01:56:27.941Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "23f3eeca-cbd9-4359-9370-fa22fa246c2e",
      "number": 3,
      "content": "Ngài làm cho loài người trở về cát bụi và phán rằng,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:27.954Z",
      "updatedAt": "2025-04-24T01:56:27.954Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "0bcbd707-37bd-4815-ac59-b72b9699c672",
      "number": 3,
      "content": "“Hỡi con cái loài người, hãy trở về.”",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:27.968Z",
      "updatedAt": "2025-04-24T01:56:27.968Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "f0e2d4ec-97c8-47bb-9fa5-53aa9a0c78a1",
      "number": 4,
      "content": "Vì một ngàn năm trước mắt Ngài",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:27.985Z",
      "updatedAt": "2025-04-24T01:56:27.985Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "3be3623b-572b-4bf5-a408-cecfbc9c7dd6",
      "number": 4,
      "content": "Tựa như ngày hôm qua, đã qua rồi,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28Z",
      "updatedAt": "2025-04-24T01:56:28Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "c2ad870c-6af2-49d6-9049-a3302f3c0e94",
      "number": 4,
      "content": "Hoặc như một canh đêm.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.013Z",
      "updatedAt": "2025-04-24T01:56:28.013Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "55604321-0c5f-4612-bc73-2abcd35ef5c7",
      "number": 5,
      "content": "Ngài làm cho đời người trôi nhanh như dòng nước lũ, thoáng qua như một giấc ngủ.",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.027Z",
      "updatedAt": "2025-04-24T01:56:28.027Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "84980f51-378e-4df5-9cff-c8366eed5d9e",
      "number": 5,
      "content": "Đời người như cỏ mọc ban mai,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.041Z",
      "updatedAt": "2025-04-24T01:56:28.041Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "379117b3-3be0-45b2-bc49-3240853dd45c",
      "number": 6,
      "content": "Sáng sớm chúng mọc lên và xanh tươi,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.053Z",
      "updatedAt": "2025-04-24T01:56:28.053Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "56f01036-e6ab-4198-8c46-4c754bf0928d",
      "number": 6,
      "content": "Chiều lại chúng héo khô và tàn tạ.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.067Z",
      "updatedAt": "2025-04-24T01:56:28.067Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "dc8e9b81-5866-4227-a843-e0185f927c87",
      "number": 7,
      "content": "Thật vậy đời sống chúng con chóng tàn vì cơn giận của Ngài.",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.080Z",
      "updatedAt": "2025-04-24T01:56:28.080Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "ea506bb9-6a8f-403c-812a-28a63dffe0cd",
      "number": 7,
      "content": "Chúng con kinh hãi vì cơn thịnh nộ của Ngài.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.094Z",
      "updatedAt": "2025-04-24T01:56:28.094Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "7ab839dd-715b-45c1-bf4c-d0c64b2df1d9",
      "number": 8,
      "content": "Ngài đã để những tội lỗi chúng con trước mặt Ngài,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.108Z",
      "updatedAt": "2025-04-24T01:56:28.108Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "93d8eb1e-8e00-4333-a067-edd77b0d4f66",
      "number": 8,
      "content": "Các tội bí mật chúng con trong ánh sáng của thánh nhan Ngài.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.122Z",
      "updatedAt": "2025-04-24T01:56:28.122Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "6d28dc12-3149-4517-9fd3-93f8a29e6599",
      "number": 9,
      "content": "Những ngày chúng con qua đi dưới cơn giận của Ngài;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.136Z",
      "updatedAt": "2025-04-24T01:56:28.136Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "f2b790b3-7bc5-4081-a705-f9e086ec461d",
      "number": 9,
      "content": "Những năm chúng con kết thúc như một tiếng thở dài.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.149Z",
      "updatedAt": "2025-04-24T01:56:28.149Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "be2368fe-e33d-4242-a2ee-14ea383128ad",
      "number": 10,
      "content": "Tuổi tác chúng con đến được bảy mươi,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.162Z",
      "updatedAt": "2025-04-24T01:56:28.162Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "21af6d3d-840c-4def-92e3-7c764fb4b0e5",
      "number": 10,
      "content": "Còn nếu mạnh khỏe thì đến tám mươi,",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.176Z",
      "updatedAt": "2025-04-24T01:56:28.176Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "57a0ba29-5217-4ce6-b093-b71b986d1826",
      "number": 10,
      "content": "Nhưng niềm kiêu hãnh của chúng chẳng qua là lao khổ và buồn thảm;",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.190Z",
      "updatedAt": "2025-04-24T01:56:28.190Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "2e0dfffa-1316-48ed-b357-6a6b4f93412a",
      "number": 10,
      "content": "Vì đời sống chóng qua, rồi chúng con bay mất đi.",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.202Z",
      "updatedAt": "2025-04-24T01:56:28.202Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "eadd1c62-b385-4b69-8e58-1035ef252df5",
      "number": 11,
      "content": "Ai biết được sức mạnh của cơn giận Ngài?",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.217Z",
      "updatedAt": "2025-04-24T01:56:28.217Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "8586b092-f9e0-4cc4-92d6-378ad0648d34",
      "number": 11,
      "content": "Cơn thịnh nộ Ngài lớn bao nhiêu, sự khiếp sợ Ngài càng lớn bấy nhiêu.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.230Z",
      "updatedAt": "2025-04-24T01:56:28.230Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "4e1574c6-a97a-47c7-866a-c614ec2a0239",
      "number": 12,
      "content": "Xin dạy chúng con biết đếm số ngày chúng con,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.243Z",
      "updatedAt": "2025-04-24T01:56:28.243Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "3850e8aa-d064-4909-b403-53bf59cb6f1d",
      "number": 12,
      "content": "Để chúng con được lòng khôn ngoan.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.260Z",
      "updatedAt": "2025-04-24T01:56:28.260Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "6557a17e-f892-40d3-9aa1-9595c343e0c2",
      "number": 13,
      "content": "Xin trở lại, **CHÚA** ôi; Ngài còn giận chúng con đến bao lâu nữa?",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.273Z",
      "updatedAt": "2025-04-24T01:56:28.273Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "863df0ec-4a2e-4105-8d08-9bc609a56cd7",
      "number": 13,
      "content": "Xin dủ lòng thương xót các tôi tớ Ngài.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.286Z",
      "updatedAt": "2025-04-24T01:56:28.286Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "42fa75c8-0761-4639-b0ac-9ce922725d67",
      "number": 14,
      "content": "Mỗi buổi sáng xin cho chúng con được no nê bằng ơn thương xót của Ngài,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.300Z",
      "updatedAt": "2025-04-24T01:56:28.300Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "74d488a3-a30d-47ad-858a-742ed1e586f9",
      "number": 14,
      "content": "Để chúng con được hân hoan và vui thỏa trọn những ngày của mình.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.313Z",
      "updatedAt": "2025-04-24T01:56:28.313Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "157339f0-8e42-415f-8349-6ecdece2836e",
      "number": 15,
      "content": "Xin ban cho chúng con niềm vui để bù lại những ngày Ngài phó chúng con cho hoạn nạn,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.328Z",
      "updatedAt": "2025-04-24T01:56:28.328Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "ff46e879-c720-43a6-a13e-1b23968a11c2",
      "number": 15,
      "content": "Và những năm chúng con thấy nỗi bất hạnh.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.341Z",
      "updatedAt": "2025-04-24T01:56:28.341Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "44284e2d-742b-4af8-828c-d403beeacdb2",
      "number": 16,
      "content": "Nguyện công việc của Ngài được tỏ ra cho các tôi tớ Ngài.",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.354Z",
      "updatedAt": "2025-04-24T01:56:28.354Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "29c9472c-4804-442c-8083-44f1cf9f8aeb",
      "number": 16,
      "content": "Nguyện vinh quang của Ngài tỏa rạng trên con cháu họ.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.367Z",
      "updatedAt": "2025-04-24T01:56:28.367Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "4f127240-0621-4e72-82dd-dbfb0dffff63",
      "number": 17,
      "content": "Nguyện phước hạnh của **Chúa**, Đức Chúa Trời của chúng con, tuôn đổ trên chúng con.",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.384Z",
      "updatedAt": "2025-04-24T01:56:28.384Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "012d2bc7-b750-4d3f-b0e9-dd5ef95de7f4",
      "number": 17,
      "content": "Cầu xin Ngài lập vững công việc của tay chúng con;",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:28.403Z",
      "updatedAt": "2025-04-24T01:56:28.403Z",
      "chapterId": "36cd4db0-61f7-49ab-a641-82a9a9b8ea16"
    },
    {
      "id": "5a17d3e9-9764-4344-9ee2-3386c924fa3a",
      "number": 17,
      "content": "Thật vậy xin lập vững công việc của tay chúng con.",
      "order": 2,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
