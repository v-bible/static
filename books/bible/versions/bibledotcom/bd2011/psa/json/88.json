{
  "chapter": {
    "id": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a",
    "number": 88,
    "ref": "https://www.bible.com/bible/19/PSA.88.BD2011",
    "createdAt": "2024-09-23T05:08:40.516Z",
    "updatedAt": "2024-09-23T05:08:40.516Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "806c3de5-fa44-4013-bab1-c5aaf60b89f2",
      "number": 1,
      "content": "Lạy **CHÚA**, Đức Chúa Trời, Đấng Giải Cứu của con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.260Z",
      "updatedAt": "2024-09-23T05:51:40.260Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "a3802d46-5d92-4ac3-8ce7-92194f1cfa37",
      "number": 1,
      "content": "Ngày và đêm con kêu cầu trước thánh nhan Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.277Z",
      "updatedAt": "2024-09-23T05:51:40.277Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "5467bbc2-8414-415c-9b1e-af2458e6438b",
      "number": 2,
      "content": "Nguyện lời cầu nguyện của con thấu đến trước mặt Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.279Z",
      "updatedAt": "2024-09-23T05:51:40.279Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "3677579e-e283-4066-a283-50d8975a4548",
      "number": 2,
      "content": "Xin nghiêng tai nghe tiếng kêu cầu của con,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.281Z",
      "updatedAt": "2024-09-23T05:51:40.281Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "da744446-3b9d-45f5-94eb-ad7868bddce4",
      "number": 3,
      "content": "Vì linh hồn con đang dẫy đầy hoạn nạn,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.283Z",
      "updatedAt": "2024-09-23T05:51:40.283Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "e993b150-a8b4-46b3-98bf-ef2c70ce55a6",
      "number": 3,
      "content": "Và mạng sống con đang tiến gần cửa âm phủ.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.285Z",
      "updatedAt": "2024-09-23T05:51:40.285Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "38c95781-2b63-4f01-9c49-7a67bf89bb8e",
      "number": 4,
      "content": "Con bị xem như kẻ sắp đi vào huyệt mả;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.287Z",
      "updatedAt": "2024-09-23T05:51:40.287Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "3c923df9-33d7-4537-a56a-59bb097e07be",
      "number": 4,
      "content": "Con như một người sức tận lực tàn.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.289Z",
      "updatedAt": "2024-09-23T05:51:40.289Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "b9f6e117-0be9-4227-b8a2-5fbe946c021a",
      "number": 5,
      "content": "Con bị lãng quên như những người đã chết,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.291Z",
      "updatedAt": "2024-09-23T05:51:40.291Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "35d8379d-f46e-44a1-ab3c-af75233dbabd",
      "number": 5,
      "content": "Như những kẻ đã nằm trong phần mộ từ lâu,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.293Z",
      "updatedAt": "2024-09-23T05:51:40.293Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "2542220d-3a56-4f07-8a34-6825536f3d19",
      "number": 5,
      "content": "Những kẻ Ngài không còn nhớ đến nữa,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.295Z",
      "updatedAt": "2024-09-23T05:51:40.295Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "76c9abbc-68d6-42bf-8dcc-0863647cf9fd",
      "number": 5,
      "content": "Và như những người đã bị dứt bỏ khỏi tay Ngài.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.297Z",
      "updatedAt": "2024-09-23T05:51:40.297Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "d76a836b-c456-45c3-916b-d2166d609ead",
      "number": 6,
      "content": "Ngài đã bỏ con vào tận đáy của vực sâu,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.298Z",
      "updatedAt": "2024-09-23T05:51:40.298Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "ee8ca5fc-6de3-4814-80f7-1310d924ba4b",
      "number": 6,
      "content": "Vào chỗ cực kỳ tối tăm, trong nơi tận cùng sâu thẳm.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.300Z",
      "updatedAt": "2024-09-23T05:51:40.300Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "b8e3713a-0a2a-4868-84ea-f462e490878e",
      "number": 7,
      "content": "Cơn thịnh nộ của Ngài đã giáng trên con;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.304Z",
      "updatedAt": "2024-09-23T05:51:40.304Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "1f6e95ec-554f-4921-8b9a-2778aa4a5470",
      "number": 7,
      "content": "Ngài đã phủ trên con tất cả các đợt sóng của Ngài. (Sê-la)",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.308Z",
      "updatedAt": "2024-09-23T05:51:40.308Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "89dcaea0-a304-4ac5-a72d-2bc5fe895d46",
      "number": 8,
      "content": "Ngài đã làm cho những người thân quen của con xa lánh con;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.313Z",
      "updatedAt": "2024-09-23T05:51:40.313Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "b4297c27-a1ce-458f-95d7-8d0a52ea5416",
      "number": 8,
      "content": "Ngài đã khiến con thành một đối tượng để họ ghê tởm;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.317Z",
      "updatedAt": "2024-09-23T05:51:40.317Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "649df41a-89d3-4131-a86f-256d8693da02",
      "number": 8,
      "content": "Con đã bị cô lập và không còn lối thoát.",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.323Z",
      "updatedAt": "2024-09-23T05:51:40.323Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "2fef2cf2-ad8d-4168-9e05-5cbac9a949cc",
      "number": 9,
      "content": "Mắt con thẫn thờ vì sầu khổ;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.328Z",
      "updatedAt": "2024-09-23T05:51:40.328Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "615c0a7a-b20c-4f54-b80b-acec17f94ae3",
      "number": 9,
      "content": "**CHÚA** ôi, hằng ngày con kêu cầu Ngài;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.333Z",
      "updatedAt": "2024-09-23T05:51:40.333Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "d1be008b-5671-486d-b0af-3446845dfbc8",
      "number": 9,
      "content": "Con đưa đôi tay lên và hướng về Ngài.",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.337Z",
      "updatedAt": "2024-09-23T05:51:40.337Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "aa5db690-eada-4437-ab9f-9054e144268f",
      "number": 10,
      "content": "Chẳng lẽ Ngài sẽ thực hiện phép lạ cho những người đã chết sao?",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.342Z",
      "updatedAt": "2024-09-23T05:51:40.342Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "9d796874-cb91-46b1-8c18-bfa54f4cd799",
      "number": 10,
      "content": "Những linh hồn đã xuất ra rồi có thể nào trỗi dậy để ca ngợi Ngài chăng? (Sê-la)",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.347Z",
      "updatedAt": "2024-09-23T05:51:40.347Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "db6dc865-bf4b-43e6-8957-7217ffc1bd85",
      "number": 11,
      "content": "Tình thương của Ngài có thể nào được công bố ở trong phần mộ?",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.357Z",
      "updatedAt": "2024-09-23T05:51:40.357Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "24306dd4-c3e6-4ff5-9443-1beb72396b8d",
      "number": 11,
      "content": "Hoặc đức thành tín của Ngài trong nơi bị hủy diệt sao?",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.362Z",
      "updatedAt": "2024-09-23T05:51:40.362Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "6bbba77f-6f32-4584-9cd8-6af5efdff4b1",
      "number": 12,
      "content": "Những việc diệu kỳ của Ngài sẽ được biết trong cõi u minh sao?",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.367Z",
      "updatedAt": "2024-09-23T05:51:40.367Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "b9b60cc2-9d9a-4620-becb-176d7d00a7e5",
      "number": 12,
      "content": "Hoặc đức công chính của Ngài trong vùng bị quên lãng chăng?",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.373Z",
      "updatedAt": "2024-09-23T05:51:40.373Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "ac9a95e2-d7a7-49f4-b7ab-060a8cc895a3",
      "number": 13,
      "content": "Còn con, **CHÚA** ôi, con kêu cầu Ngài giúp đỡ;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.378Z",
      "updatedAt": "2024-09-23T05:51:40.378Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "061a81df-ea4c-4643-9736-bac89ad4a24e",
      "number": 13,
      "content": "Vừa sớm tinh sương lời cầu nguyện con lên trước thánh nhan Ngài.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.383Z",
      "updatedAt": "2024-09-23T05:51:40.383Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "5f08425f-a790-4eb9-84fd-02561d584dfb",
      "number": 14,
      "content": "**CHÚA** ôi, sao Ngài nỡ từ bỏ con?",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.388Z",
      "updatedAt": "2024-09-23T05:51:40.388Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "c8c9b6db-bac9-422b-8eaa-34c4fac7f5d3",
      "number": 14,
      "content": "Sao Ngài ẩn mặt Ngài khỏi con?",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.392Z",
      "updatedAt": "2024-09-23T05:51:40.392Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "08485883-3d1c-49fb-942d-a785e3af3516",
      "number": 15,
      "content": "Con đã bị đau đớn và gần chết từ thời niên thiếu;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.397Z",
      "updatedAt": "2024-09-23T05:51:40.397Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "f3c77420-05ae-4c3c-9744-8e2f22a43e39",
      "number": 15,
      "content": "Con bị những trận đòn kinh hoàng của Ngài nên đâm ra tuyệt vọng.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.401Z",
      "updatedAt": "2024-09-23T05:51:40.401Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "b309dd74-6c19-4393-8204-22a4ccb2a1b9",
      "number": 16,
      "content": "Cơn giận phừng phừng của Ngài đã giáng trên con;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.407Z",
      "updatedAt": "2024-09-23T05:51:40.407Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "f4bc23fd-a8b0-4757-975f-db2054f5fe37",
      "number": 16,
      "content": "Những trận đòn kinh hoàng của Ngài đã tàn phá con.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.412Z",
      "updatedAt": "2024-09-23T05:51:40.412Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "8aa214a2-75cc-4751-8568-707558cae5b2",
      "number": 17,
      "content": "Những điều ấy bao phủ con như lũ lụt tràn ngập suốt ngày;",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.416Z",
      "updatedAt": "2024-09-23T05:51:40.416Z",
      "chapterId": "351a1f3c-4b3f-4f8f-b0b5-fbc2acfa4e1a"
    },
    {
      "id": "9deabd73-b1fd-4526-bd81-0205437254f5",
      "number": 17,
      "content": "Chúng cùng nhau cố nhận chìm con.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:40.422Z",
