{
  "chapter": {
    "id": "37955a9a-f5db-4671-8d12-f0a471a49ccd",
    "number": 88,
    "ref": "https://www.bible.com/bible/19/PSA.88.BD2011",
    "createdAt": "2025-04-24T01:29:06.871Z",
    "updatedAt": "2025-04-24T01:29:06.871Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "525bddf4-5e7f-4a19-8810-be957cc24ec5",
      "number": 1,
      "content": "Lạy **CHÚA**, Đức Chúa Trời, Đấng Giải Cứu của con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:19.848Z",
      "updatedAt": "2025-04-24T01:56:19.848Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "0bbec4fb-fefd-4193-8a93-f1096281cc64",
      "number": 1,
      "content": "Ngày và đêm con kêu cầu trước thánh nhan Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:19.876Z",
      "updatedAt": "2025-04-24T01:56:19.876Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "9eb06eb8-2148-43b6-a29d-457f9a554db4",
      "number": 2,
      "content": "Nguyện lời cầu nguyện của con thấu đến trước mặt Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:19.886Z",
      "updatedAt": "2025-04-24T01:56:19.886Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "ea57558a-aed6-4dd1-8a9a-c2e9ddf4a73f",
      "number": 2,
      "content": "Xin nghiêng tai nghe tiếng kêu cầu của con,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:19.900Z",
      "updatedAt": "2025-04-24T01:56:19.900Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "8599b737-34ec-49c5-b222-157839eb55b7",
      "number": 3,
      "content": "Vì linh hồn con đang dẫy đầy hoạn nạn,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:19.911Z",
      "updatedAt": "2025-04-24T01:56:19.911Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "461a2d35-550d-4616-bd1f-5f00b8a851a2",
      "number": 3,
      "content": "Và mạng sống con đang tiến gần cửa âm phủ.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:19.923Z",
      "updatedAt": "2025-04-24T01:56:19.923Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "f136b02f-104c-41a6-9236-56274b4cf92c",
      "number": 4,
      "content": "Con bị xem như kẻ sắp đi vào huyệt mả;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:19.937Z",
      "updatedAt": "2025-04-24T01:56:19.937Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "7951e63a-e423-40dc-b76b-8252b818222a",
      "number": 4,
      "content": "Con như một người sức tận lực tàn.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:19.950Z",
      "updatedAt": "2025-04-24T01:56:19.950Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "cca82991-2c66-4f1c-bbf6-bf9ccd796ae4",
      "number": 5,
      "content": "Con bị lãng quên như những người đã chết,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:19.963Z",
      "updatedAt": "2025-04-24T01:56:19.963Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "5d424648-94cd-4dcc-a400-2f66591bc6c2",
      "number": 5,
      "content": "Như những kẻ đã nằm trong phần mộ từ lâu,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:19.977Z",
      "updatedAt": "2025-04-24T01:56:19.977Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "5ccdb8c5-9834-4cdd-9bfe-911e83c56a22",
      "number": 5,
      "content": "Những kẻ Ngài không còn nhớ đến nữa,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:19.990Z",
      "updatedAt": "2025-04-24T01:56:19.990Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "481dbc23-636d-488f-b1dd-72d45ed7bf3c",
      "number": 5,
      "content": "Và như những người đã bị dứt bỏ khỏi tay Ngài.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.003Z",
      "updatedAt": "2025-04-24T01:56:20.003Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "598a8636-c18b-4b8d-bcbb-d432165d8d6c",
      "number": 6,
      "content": "Ngài đã bỏ con vào tận đáy của vực sâu,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.017Z",
      "updatedAt": "2025-04-24T01:56:20.017Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "720a40d6-c783-42a9-8592-759e564826a2",
      "number": 6,
      "content": "Vào chỗ cực kỳ tối tăm, trong nơi tận cùng sâu thẳm.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.031Z",
      "updatedAt": "2025-04-24T01:56:20.031Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "5bd4b6b3-3559-477c-8ac1-ebcf508bd3a1",
      "number": 7,
      "content": "Cơn thịnh nộ của Ngài đã giáng trên con;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.044Z",
      "updatedAt": "2025-04-24T01:56:20.044Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "d459f40a-3ffc-43a1-a7a0-cc3e90cc706e",
      "number": 7,
      "content": "Ngài đã phủ trên con tất cả các đợt sóng của Ngài. (Sê-la)",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.058Z",
      "updatedAt": "2025-04-24T01:56:20.058Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "ad0938df-8481-4136-b6b0-f53fc766c01c",
      "number": 8,
      "content": "Ngài đã làm cho những người thân quen của con xa lánh con;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.072Z",
      "updatedAt": "2025-04-24T01:56:20.072Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "03c049ef-b1ac-4cbc-b8ea-9f92dcea1e63",
      "number": 8,
      "content": "Ngài đã khiến con thành một đối tượng để họ ghê tởm;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.086Z",
      "updatedAt": "2025-04-24T01:56:20.086Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "a5a5792b-201d-47ea-a8ff-0b5b7fbabe0f",
      "number": 8,
      "content": "Con đã bị cô lập và không còn lối thoát.",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.099Z",
      "updatedAt": "2025-04-24T01:56:20.099Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "fb574b0b-c84e-4215-b810-330fb2f95141",
      "number": 9,
      "content": "Mắt con thẫn thờ vì sầu khổ;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.113Z",
      "updatedAt": "2025-04-24T01:56:20.113Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "3bace33b-68d6-41d2-ab4c-9baf1729919e",
      "number": 9,
      "content": "**CHÚA** ôi, hằng ngày con kêu cầu Ngài;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.127Z",
      "updatedAt": "2025-04-24T01:56:20.127Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "df8c3f65-f71c-4ae5-8456-50e6487a0abb",
      "number": 9,
      "content": "Con đưa đôi tay lên và hướng về Ngài.",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.140Z",
      "updatedAt": "2025-04-24T01:56:20.140Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "05fa24ad-3206-4b41-ae4e-5cca166e8df7",
      "number": 10,
      "content": "Chẳng lẽ Ngài sẽ thực hiện phép lạ cho những người đã chết sao?",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.154Z",
      "updatedAt": "2025-04-24T01:56:20.154Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "202853e3-27f6-4ea4-88b2-8c5bec0fe026",
      "number": 10,
      "content": "Những linh hồn đã xuất ra rồi có thể nào trỗi dậy để ca ngợi Ngài chăng? (Sê-la)",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.168Z",
      "updatedAt": "2025-04-24T01:56:20.168Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "65923c0d-2f09-44be-bf3f-1b81a1f1606c",
      "number": 11,
      "content": "Tình thương của Ngài có thể nào được công bố ở trong phần mộ?",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.195Z",
      "updatedAt": "2025-04-24T01:56:20.195Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "68d090af-cd4c-4336-b226-505abe8fe7d3",
      "number": 11,
      "content": "Hoặc đức thành tín của Ngài trong nơi bị hủy diệt sao?",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.209Z",
      "updatedAt": "2025-04-24T01:56:20.209Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "47703a95-4019-4ad8-8126-03d8ee771e68",
      "number": 12,
      "content": "Những việc diệu kỳ của Ngài sẽ được biết trong cõi u minh sao?",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.222Z",
      "updatedAt": "2025-04-24T01:56:20.222Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "60355a0b-fd3b-42ef-88f2-c6dc34121de5",
      "number": 12,
      "content": "Hoặc đức công chính của Ngài trong vùng bị quên lãng chăng?",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.236Z",
      "updatedAt": "2025-04-24T01:56:20.236Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "2442dd5e-fc11-43c1-9313-d67f44fcc217",
      "number": 13,
      "content": "Còn con, **CHÚA** ôi, con kêu cầu Ngài giúp đỡ;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.249Z",
      "updatedAt": "2025-04-24T01:56:20.249Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "c84a456f-2f70-4297-b37d-cd8c5cfc486c",
      "number": 13,
      "content": "Vừa sớm tinh sương lời cầu nguyện con lên trước thánh nhan Ngài.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.263Z",
      "updatedAt": "2025-04-24T01:56:20.263Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "f42a2577-518b-43e1-a815-247db8c7a62d",
      "number": 14,
      "content": "**CHÚA** ôi, sao Ngài nỡ từ bỏ con?",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.276Z",
      "updatedAt": "2025-04-24T01:56:20.276Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "32ac9089-b4a9-45d9-a9f9-0ca5737919e6",
      "number": 14,
      "content": "Sao Ngài ẩn mặt Ngài khỏi con?",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.290Z",
      "updatedAt": "2025-04-24T01:56:20.290Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "32dd0483-586b-4902-acd6-a986c07488e1",
      "number": 15,
      "content": "Con đã bị đau đớn và gần chết từ thời niên thiếu;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.303Z",
      "updatedAt": "2025-04-24T01:56:20.303Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "038b7b94-9b9b-472a-ba94-b9c7bffd5e89",
      "number": 15,
      "content": "Con bị những trận đòn kinh hoàng của Ngài nên đâm ra tuyệt vọng.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.317Z",
      "updatedAt": "2025-04-24T01:56:20.317Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "65880431-eb93-467e-acdb-caba5c3eecc9",
      "number": 16,
      "content": "Cơn giận phừng phừng của Ngài đã giáng trên con;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.330Z",
      "updatedAt": "2025-04-24T01:56:20.330Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "44e61291-68cf-4289-b5cb-20c3d125e450",
      "number": 16,
      "content": "Những trận đòn kinh hoàng của Ngài đã tàn phá con.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.344Z",
      "updatedAt": "2025-04-24T01:56:20.344Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "8f805fb5-d767-471b-addf-186dfe47c51e",
      "number": 17,
      "content": "Những điều ấy bao phủ con như lũ lụt tràn ngập suốt ngày;",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.357Z",
      "updatedAt": "2025-04-24T01:56:20.357Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "5638f2a1-6afe-4d19-ab2c-f34dac9fa305",
      "number": 17,
      "content": "Chúng cùng nhau cố nhận chìm con.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.371Z",
      "updatedAt": "2025-04-24T01:56:20.371Z",
      "chapterId": "37955a9a-f5db-4671-8d12-f0a471a49ccd"
    },
    {
      "id": "e8c1fcd1-b35b-4058-aa09-b1b7732eb9df",
      "number": 18,
      "content": "Ngài đã làm cho người yêu của con và bạn bè con xa lánh con;",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:20.384Z",
      "updatedAt": "2025-04-24T01:56:20.384Z",
