{
  "chapter": {
    "id": "45f0c55d-abb7-475a-b442-f7e0a784f5d8",
    "number": 124,
    "ref": "https://www.bible.com/bible/19/PSA.124.BD2011",
    "createdAt": "2025-04-24T01:29:07.374Z",
    "updatedAt": "2025-04-24T01:29:07.374Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "8ee88298-fa56-4a34-9816-0b813848cb5e",
      "number": 1,
      "content": "Nếu chẳng phải **CHÚA** bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.093Z",
      "updatedAt": "2025-04-24T01:58:52.093Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "e3d8a29a-ded6-429c-acd1-b96dc5f4750f",
      "number": 1,
      "content": "Hỡi I-sơ-ra-ên, hãy nói đi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.132Z",
      "updatedAt": "2025-04-24T01:58:52.132Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "38fd595e-3629-4eda-a220-0cfbd71cd0d8",
      "number": 2,
      "content": "Nếu chẳng phải **CHÚA** bênh vực chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.146Z",
      "updatedAt": "2025-04-24T01:58:52.146Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "1221bc90-f1c1-4ecf-b74b-1f2c77d4fc4d",
      "number": 2,
      "content": "Thì khi người ta nổi lên chống lại chúng ta,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.160Z",
      "updatedAt": "2025-04-24T01:58:52.160Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "23b5e654-aa28-4877-a76f-d686be004a57",
      "number": 3,
      "content": "Khi cơn cuồng nộ của chúng bừng lên nghịch lại chúng ta,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.174Z",
      "updatedAt": "2025-04-24T01:58:52.174Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "ca5b45e6-e4d7-4dbf-a799-cf705e08ee77",
      "number": 3,
      "content": "Chắc hẳn chúng đã nuốt chửng chúng ta rồi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.189Z",
      "updatedAt": "2025-04-24T01:58:52.189Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "20fdfff3-91ca-44ca-9e5f-98cec4e83e95",
      "number": 4,
      "content": "Các dòng nước lũ chắc đã nhận chìm chúng ta;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.202Z",
      "updatedAt": "2025-04-24T01:58:52.202Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "02940ca9-5ea4-42db-9105-a2d73b63f4f9",
      "number": 4,
      "content": "Luồng nước mạnh chắc đã cuốn trôi linh hồn chúng ta;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.215Z",
      "updatedAt": "2025-04-24T01:58:52.215Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "457e34da-0375-4546-9c3f-909922ef8d3b",
      "number": 5,
      "content": "Các đợt sóng hung hãn",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.229Z",
      "updatedAt": "2025-04-24T01:58:52.229Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "fc506150-5fa8-48a1-b4a1-61dc2afedb63",
      "number": 5,
      "content": "Chắc đã cuốn mất linh hồn chúng ta rồi.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.243Z",
      "updatedAt": "2025-04-24T01:58:52.243Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "b2fd6cd7-2c08-448c-b2a0-94da5f58f55b",
      "number": 6,
      "content": "Chúc tụng **CHÚA**,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.256Z",
      "updatedAt": "2025-04-24T01:58:52.256Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "381ed768-77d2-4a22-8bfc-8ef1939495bc",
      "number": 6,
      "content": "Đấng không cho phép chúng ta",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.269Z",
      "updatedAt": "2025-04-24T01:58:52.269Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "b048e372-b2f8-417d-b1d7-c04c7de03df6",
      "number": 6,
      "content": "Làm mồi cho răng chúng.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.282Z",
      "updatedAt": "2025-04-24T01:58:52.282Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "7b23cfea-f3c9-42eb-99e2-bd6b674cf2b0",
      "number": 7,
      "content": "Linh hồn chúng ta được giải thoát,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.294Z",
      "updatedAt": "2025-04-24T01:58:52.294Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "04776dbb-ccd4-4ddb-928b-ec49941568d0",
      "number": 7,
      "content": "Như con chim thoát khỏi bẫy của kẻ bẫy chim;",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.306Z",
      "updatedAt": "2025-04-24T01:58:52.306Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "ae50b083-d4bf-4077-9c76-59c2dfe1a214",
      "number": 7,
      "content": "Cái bẫy bị bẻ gãy,",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.316Z",
      "updatedAt": "2025-04-24T01:58:52.316Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "f65c3545-a0bb-4350-acf9-a17072a19ef2",
      "number": 7,
      "content": "Và chúng ta thoát ra ngoài.",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.326Z",
      "updatedAt": "2025-04-24T01:58:52.326Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "7cb08717-c74c-4223-a6a5-ed7c18e7bebd",
      "number": 8,
      "content": "Chúng ta được giúp đỡ nhờ danh **CHÚA**,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.337Z",
      "updatedAt": "2025-04-24T01:58:52.337Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    },
    {
      "id": "b6b4e55e-f1da-44cf-ad41-dcf58ea015a8",
      "number": 8,
      "content": "Đấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:52.348Z",
      "updatedAt": "2025-04-24T01:58:52.348Z",
      "chapterId": "45f0c55d-abb7-475a-b442-f7e0a784f5d8"
    }
  ],
