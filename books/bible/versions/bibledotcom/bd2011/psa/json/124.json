{
  "chapter": {
    "id": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372",
    "number": 124,
    "ref": "https://www.bible.com/bible/19/PSA.124.BD2011",
    "createdAt": "2025-04-22T10:10:27.044Z",
    "updatedAt": "2025-04-22T10:10:27.044Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "638a614f-f78a-4b3b-9a91-6a4ca4406ca8",
      "number": 1,
      "content": "Nếu chẳng phải **CHÚA** bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.461Z",
      "updatedAt": "2025-04-22T10:43:56.461Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "7cd8b579-be81-4ca0-a0c6-055f546025ff",
      "number": 2,
      "content": "Nếu chẳng phải **CHÚA** bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.550Z",
      "updatedAt": "2025-04-22T10:43:56.550Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "1ef66da5-3156-41d3-aec5-43f433c0bd0d",
      "number": 1,
      "content": "Hỡi I-sơ-ra-ên, hãy nói đi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.537Z",
      "updatedAt": "2025-04-22T10:43:56.537Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "53582f68-1c37-4928-bb48-901225569f12",
      "number": 2,
      "content": "Thì khi người ta nổi lên chống lại chúng ta,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.563Z",
      "updatedAt": "2025-04-22T10:43:56.563Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "e9da47ed-d1e3-4212-87d2-e51910de52d2",
      "number": 3,
      "content": "Khi cơn cuồng nộ của chúng bừng lên nghịch lại chúng ta,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.576Z",
      "updatedAt": "2025-04-22T10:43:56.576Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "7012dae5-ee1c-4cfc-80b4-c4997463c337",
      "number": 3,
      "content": "Chắc hẳn chúng đã nuốt chửng chúng ta rồi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.589Z",
      "updatedAt": "2025-04-22T10:43:56.589Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "b9adc16a-062f-44ad-8262-26f3b40214f1",
      "number": 4,
      "content": "Các dòng nước lũ chắc đã nhận chìm chúng ta;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.603Z",
      "updatedAt": "2025-04-22T10:43:56.603Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "c23b8b2a-5b08-41f5-89d1-ccb2b387298d",
      "number": 4,
      "content": "Luồng nước mạnh chắc đã cuốn trôi linh hồn chúng ta;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.615Z",
      "updatedAt": "2025-04-22T10:43:56.615Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "dd66f5f1-2fed-483f-b52a-eaf28972431c",
      "number": 5,
      "content": "Các đợt sóng hung hãn",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.632Z",
      "updatedAt": "2025-04-22T10:43:56.632Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "54600831-0daf-4f83-8073-1edf3d187acc",
      "number": 5,
      "content": "Chắc đã cuốn mất linh hồn chúng ta rồi.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.648Z",
      "updatedAt": "2025-04-22T10:43:56.648Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "08a8a5cf-313a-452f-8b45-bb45ee28a769",
      "number": 6,
      "content": "Chúc tụng **CHÚA**,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.661Z",
      "updatedAt": "2025-04-22T10:43:56.661Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "145868af-1a0c-493c-8dd3-73b78f952b6a",
      "number": 6,
      "content": "Đấng không cho phép chúng ta",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.677Z",
      "updatedAt": "2025-04-22T10:43:56.677Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "b7e6a8de-7306-4d0c-9ba4-dbc7c2690e0f",
      "number": 6,
      "content": "Làm mồi cho răng chúng.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.690Z",
      "updatedAt": "2025-04-22T10:43:56.690Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "45651ec2-087f-4450-806b-907142efafc4",
      "number": 7,
      "content": "Linh hồn chúng ta được giải thoát,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.703Z",
      "updatedAt": "2025-04-22T10:43:56.703Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "da8942f4-de27-40ca-a832-37a06386d6dd",
      "number": 7,
      "content": "Như con chim thoát khỏi bẫy của kẻ bẫy chim;",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.716Z",
      "updatedAt": "2025-04-22T10:43:56.716Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "3ff851dd-0874-45f5-9c25-869bbe3b8cc4",
      "number": 7,
      "content": "Cái bẫy bị bẻ gãy,",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.730Z",
      "updatedAt": "2025-04-22T10:43:56.730Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "7a901ccc-e23d-4c19-b075-8c1ce634e108",
      "number": 7,
      "content": "Và chúng ta thoát ra ngoài.",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.743Z",
      "updatedAt": "2025-04-22T10:43:56.743Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "3b868840-1d0e-434b-b126-f70b25d69f51",
      "number": 8,
      "content": "Chúng ta được giúp đỡ nhờ danh **CHÚA**,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.756Z",
      "updatedAt": "2025-04-22T10:43:56.756Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    },
    {
      "id": "276fb7c9-bbd0-4b59-8065-ca261799a953",
      "number": 8,
      "content": "Đấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:56.769Z",
      "updatedAt": "2025-04-22T10:43:56.769Z",
      "chapterId": "14da17b9-6ee7-4057-9b8e-9ac53a5d0372"
    }
  ],
