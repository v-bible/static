{
  "chapter": {
    "id": "cbb9244d-13ea-4c74-9a34-1d04b93558d1",
    "number": 124,
    "ref": "https://www.bible.com/bible/19/PSA.124.BD2011",
    "createdAt": "2024-09-23T05:08:40.633Z",
    "updatedAt": "2024-09-23T05:08:40.633Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "d5c80c29-a47d-4f06-8613-f5e741e30382",
      "number": 1,
      "content": "Nếu chẳng phải **CHÚA** bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.172Z",
      "updatedAt": "2024-09-23T05:54:54.172Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "eadb8d9a-5578-495a-aaa7-83cd6892cb08",
      "number": 2,
      "content": "Nếu chẳng phải **CHÚA** bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.193Z",
      "updatedAt": "2024-09-23T05:54:54.193Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "6823ec9c-b48c-4946-aaf3-525812d0cfd2",
      "number": 1,
      "content": "Hỡi I-sơ-ra-ên, hãy nói đi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.191Z",
      "updatedAt": "2024-09-23T05:54:54.191Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "01bfc21d-9b58-4b36-b97b-145329de1f06",
      "number": 2,
      "content": "Thì khi người ta nổi lên chống lại chúng ta,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.195Z",
      "updatedAt": "2024-09-23T05:54:54.195Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "da8904cd-78fa-4221-b857-60de97fe8627",
      "number": 3,
      "content": "Khi cơn cuồng nộ của chúng bừng lên nghịch lại chúng ta,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.197Z",
      "updatedAt": "2024-09-23T05:54:54.197Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "83bf98cd-83db-4152-811e-846c0f24edb7",
      "number": 3,
      "content": "Chắc hẳn chúng đã nuốt chửng chúng ta rồi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.199Z",
      "updatedAt": "2024-09-23T05:54:54.199Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "82c8f66a-679f-4317-aa24-af85b8139af0",
      "number": 4,
      "content": "Các dòng nước lũ chắc đã nhận chìm chúng ta;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.201Z",
      "updatedAt": "2024-09-23T05:54:54.201Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "b735897a-d983-40c5-a91f-c9d5bf5dd725",
      "number": 4,
      "content": "Luồng nước mạnh chắc đã cuốn trôi linh hồn chúng ta;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.203Z",
      "updatedAt": "2024-09-23T05:54:54.203Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "dcf0e378-b5c5-43e7-8fe2-e03ae9432168",
      "number": 5,
      "content": "Các đợt sóng hung hãn",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.205Z",
      "updatedAt": "2024-09-23T05:54:54.205Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "bbe04144-0e78-46c6-a576-043f451504aa",
      "number": 5,
      "content": "Chắc đã cuốn mất linh hồn chúng ta rồi.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.207Z",
      "updatedAt": "2024-09-23T05:54:54.207Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "d9c6d30a-5649-4bec-b179-6b5e3d930975",
      "number": 6,
      "content": "Chúc tụng **CHÚA**,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.209Z",
      "updatedAt": "2024-09-23T05:54:54.209Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "746520e9-668d-4c8e-94e6-af6ff919b15d",
      "number": 6,
      "content": "Đấng không cho phép chúng ta",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.211Z",
      "updatedAt": "2024-09-23T05:54:54.211Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "ef6428a9-7871-4fb4-b3a8-438645d6b857",
      "number": 6,
      "content": "Làm mồi cho răng chúng.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.213Z",
      "updatedAt": "2024-09-23T05:54:54.213Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "9eb0e24e-c1b9-4530-8c18-8f4c42533f0d",
      "number": 7,
      "content": "Linh hồn chúng ta được giải thoát,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.215Z",
      "updatedAt": "2024-09-23T05:54:54.215Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "9bd3e0ce-48fa-4b9b-9c9d-dd520a50f0e7",
      "number": 7,
      "content": "Như con chim thoát khỏi bẫy của kẻ bẫy chim;",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.217Z",
      "updatedAt": "2024-09-23T05:54:54.217Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "4a3c5ad0-8abc-4ad1-a914-ab327f0c8077",
      "number": 7,
      "content": "Cái bẫy bị bẻ gãy,",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.219Z",
      "updatedAt": "2024-09-23T05:54:54.219Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "3192e6e5-d4e0-4dcb-a8f4-ed5b5ad2c6b0",
      "number": 7,
      "content": "Và chúng ta thoát ra ngoài.",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.221Z",
      "updatedAt": "2024-09-23T05:54:54.221Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "26fc9b37-986f-4791-b09b-10fbf492bb52",
      "number": 8,
      "content": "Chúng ta được giúp đỡ nhờ danh **CHÚA**,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.222Z",
      "updatedAt": "2024-09-23T05:54:54.222Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    },
    {
      "id": "44a84f8a-9584-474e-bc19-15afe78998dd",
      "number": 8,
      "content": "Đấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:54.224Z",
      "updatedAt": "2024-09-23T05:54:54.224Z",
      "chapterId": "cbb9244d-13ea-4c74-9a34-1d04b93558d1"
    }
  ],
