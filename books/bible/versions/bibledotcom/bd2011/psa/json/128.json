{
  "chapter": {
    "id": "c6d3c45c-05d2-4300-aea2-c00f8da578b5",
    "number": 128,
    "ref": "https://www.bible.com/bible/19/PSA.128.BD2011",
    "createdAt": "2025-04-22T10:10:27.087Z",
    "updatedAt": "2025-04-22T10:10:27.087Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "19495eaa-8128-4782-a0bc-afd5d91f493f",
      "number": 1,
      "content": "Phước cho người kính sợ **CHÚA**,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.247Z",
      "updatedAt": "2025-04-22T10:44:13.247Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "5009c795-61eb-4621-a97d-7e1d576b15aa",
      "number": 1,
      "content": "Người đi trong đường lối Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.294Z",
      "updatedAt": "2025-04-22T10:44:13.294Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "69a9c7e6-03c8-4960-ba75-3bb23844164c",
      "number": 2,
      "content": "Bạn sẽ hưởng hoa lợi của tay mình;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.308Z",
      "updatedAt": "2025-04-22T10:44:13.308Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "58469a77-1531-43f3-b325-b748caa5d33a",
      "number": 2,
      "content": "Bạn sẽ được phước và thịnh vượng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.319Z",
      "updatedAt": "2025-04-22T10:44:13.319Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "309a5415-2be8-4e58-8508-feae9d91ad5d",
      "number": 3,
      "content": "Vợ bạn sẽ như cây nho sai trái trong nhà bạn;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.331Z",
      "updatedAt": "2025-04-22T10:44:13.331Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "86135026-3804-4da4-9bea-49ba6f2dcd20",
      "number": 3,
      "content": "Con cái bạn sẽ như các chồi ô-liu quanh bàn bạn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.344Z",
      "updatedAt": "2025-04-22T10:44:13.344Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "6650842e-7cf4-4542-9db3-828f694bcd84",
      "number": 4,
      "content": "Kìa, người nào kính sợ **CHÚA**",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.357Z",
      "updatedAt": "2025-04-22T10:44:13.357Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "a0242c42-220d-4078-b5ab-2f1cf90d9afa",
      "number": 4,
      "content": "Sẽ được phước như vậy.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.371Z",
      "updatedAt": "2025-04-22T10:44:13.371Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "e03810be-cea7-4752-b3ab-49c54bf7c141",
      "number": 5,
      "content": "Nguyện **CHÚA** ban phước cho bạn từ Si-ôn.",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.384Z",
      "updatedAt": "2025-04-22T10:44:13.384Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "4197264e-2608-4c6a-be86-40d379cbdda2",
      "number": 5,
      "content": "Nguyện suốt đời bạn",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.397Z",
      "updatedAt": "2025-04-22T10:44:13.397Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "70121aea-d8c3-42e8-a79c-15238f831055",
      "number": 5,
      "content": "Chứng kiến những phước hạnh của Giê-ru-sa-lem.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.411Z",
      "updatedAt": "2025-04-22T10:44:13.411Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "c6c41b06-5f25-4dc1-b35b-2da791cab003",
      "number": 6,
      "content": "Nguyện bạn được nhìn thấy con cháu của con cái bạn.",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.424Z",
      "updatedAt": "2025-04-22T10:44:13.424Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    },
    {
      "id": "9d00054f-e17f-4689-a1e9-6035212c26ee",
      "number": 6,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:13.438Z",
      "updatedAt": "2025-04-22T10:44:13.438Z",
      "chapterId": "c6d3c45c-05d2-4300-aea2-c00f8da578b5"
    }
  ],
