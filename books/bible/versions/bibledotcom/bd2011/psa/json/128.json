{
  "chapter": {
    "id": "ddefadbc-2612-4983-98c3-ac3f0fb1608c",
    "number": 128,
    "ref": "https://www.bible.com/bible/19/PSA.128.BD2011",
    "createdAt": "2025-04-24T01:29:07.423Z",
    "updatedAt": "2025-04-24T01:29:07.423Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "6cd723e7-dc2f-4db4-9998-4b0b6dd874f3",
      "number": 1,
      "content": "Phước cho người kính sợ **CHÚA**,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.429Z",
      "updatedAt": "2025-04-24T01:59:06.429Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "b90812c1-bf91-4824-9854-d9620b1f1f3f",
      "number": 1,
      "content": "Người đi trong đường lối Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.456Z",
      "updatedAt": "2025-04-24T01:59:06.456Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "632794d8-01e8-4b86-a063-be7fe7a3cfef",
      "number": 2,
      "content": "Bạn sẽ hưởng hoa lợi của tay mình;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.467Z",
      "updatedAt": "2025-04-24T01:59:06.467Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "ba0c61e7-2dad-412b-9057-29f26aed2a40",
      "number": 2,
      "content": "Bạn sẽ được phước và thịnh vượng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.477Z",
      "updatedAt": "2025-04-24T01:59:06.477Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "7eebe194-b9c8-4dd3-969d-e14e3b099f3c",
      "number": 3,
      "content": "Vợ bạn sẽ như cây nho sai trái trong nhà bạn;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.489Z",
      "updatedAt": "2025-04-24T01:59:06.489Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "c9938607-1e6e-4f84-9822-c6b8bc5fc563",
      "number": 3,
      "content": "Con cái bạn sẽ như các chồi ô-liu quanh bàn bạn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.501Z",
      "updatedAt": "2025-04-24T01:59:06.501Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "310112dd-5cd4-40f0-8758-b86537b56970",
      "number": 4,
      "content": "Kìa, người nào kính sợ **CHÚA**",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.515Z",
      "updatedAt": "2025-04-24T01:59:06.515Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "e50a0bfb-072d-45d5-8f99-4b43789a6e72",
      "number": 4,
      "content": "Sẽ được phước như vậy.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.528Z",
      "updatedAt": "2025-04-24T01:59:06.528Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "6382c5d2-d86f-4b47-9ed7-05d8324d3e33",
      "number": 5,
      "content": "Nguyện **CHÚA** ban phước cho bạn từ Si-ôn.",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.542Z",
      "updatedAt": "2025-04-24T01:59:06.542Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "e07425fe-a404-4a8c-9c1d-da16f9c86140",
      "number": 5,
      "content": "Nguyện suốt đời bạn",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.556Z",
      "updatedAt": "2025-04-24T01:59:06.556Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "c5def03c-ea45-4fad-a1ae-a3a910338b40",
      "number": 5,
      "content": "Chứng kiến những phước hạnh của Giê-ru-sa-lem.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.572Z",
      "updatedAt": "2025-04-24T01:59:06.572Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "0f056075-918e-40b1-b4b9-36959b45530d",
      "number": 6,
      "content": "Nguyện bạn được nhìn thấy con cháu của con cái bạn.",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.588Z",
      "updatedAt": "2025-04-24T01:59:06.588Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    },
    {
      "id": "3ab10dbb-3774-4b8a-a85d-399fdeac29a1",
      "number": 6,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:06.600Z",
      "updatedAt": "2025-04-24T01:59:06.600Z",
      "chapterId": "ddefadbc-2612-4983-98c3-ac3f0fb1608c"
    }
  ],
