{
  "chapter": {
    "id": "71493ff7-db25-4018-aa1a-4599f1cdf6d2",
    "number": 128,
    "ref": "https://www.bible.com/bible/19/PSA.128.BD2011",
    "createdAt": "2024-09-23T05:08:40.645Z",
    "updatedAt": "2024-09-23T05:08:40.645Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "7fe8483b-fa81-4b58-8843-f24dcd7f6059",
      "number": 1,
      "content": "Phước cho người kính sợ **CHÚA**,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.809Z",
      "updatedAt": "2024-09-23T05:55:10.809Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "e99331f2-7ea4-46b7-888d-93fe15d8e630",
      "number": 1,
      "content": "Người đi trong đường lối Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.829Z",
      "updatedAt": "2024-09-23T05:55:10.829Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "1aaef38d-3bb6-4d35-91a2-6dbcb1cb2854",
      "number": 2,
      "content": "Bạn sẽ hưởng hoa lợi của tay mình;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.834Z",
      "updatedAt": "2024-09-23T05:55:10.834Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "b149a99b-c2c3-47c6-8628-f84d229008e1",
      "number": 2,
      "content": "Bạn sẽ được phước và thịnh vượng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.837Z",
      "updatedAt": "2024-09-23T05:55:10.837Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "b2f91cd7-dd0d-4f4b-ac86-a0952903b6ea",
      "number": 3,
      "content": "Vợ bạn sẽ như cây nho sai trái trong nhà bạn;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.840Z",
      "updatedAt": "2024-09-23T05:55:10.840Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "44a21983-ca64-445f-9d6f-6dbc910a1396",
      "number": 3,
      "content": "Con cái bạn sẽ như các chồi ô-liu quanh bàn bạn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.843Z",
      "updatedAt": "2024-09-23T05:55:10.843Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "2ddf521c-9402-47c3-80e5-cc2e2495572e",
      "number": 4,
      "content": "Kìa, người nào kính sợ **CHÚA**",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.850Z",
      "updatedAt": "2024-09-23T05:55:10.850Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "3f022285-5db1-4fcc-b0ce-e7ed7b91dce1",
      "number": 4,
      "content": "Sẽ được phước như vậy.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.856Z",
      "updatedAt": "2024-09-23T05:55:10.856Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "e4a5f26e-6384-4ec0-a234-b36be02a5475",
      "number": 5,
      "content": "Nguyện **CHÚA** ban phước cho bạn từ Si-ôn.",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.861Z",
      "updatedAt": "2024-09-23T05:55:10.861Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "4b4e7749-bbc6-4806-a438-bc559647a838",
      "number": 5,
      "content": "Nguyện suốt đời bạn",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.865Z",
      "updatedAt": "2024-09-23T05:55:10.865Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "313a4e4d-859b-4087-ba58-cdcc5f6312cf",
      "number": 5,
      "content": "Chứng kiến những phước hạnh của Giê-ru-sa-lem.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.869Z",
      "updatedAt": "2024-09-23T05:55:10.869Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "09510238-d502-490c-a0dc-bc6b9e6bbeef",
      "number": 6,
      "content": "Nguyện bạn được nhìn thấy con cháu của con cái bạn.",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.874Z",
      "updatedAt": "2024-09-23T05:55:10.874Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    },
    {
      "id": "7d3d355d-7136-455c-be11-3386db92b619",
      "number": 6,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:10.878Z",
      "updatedAt": "2024-09-23T05:55:10.878Z",
      "chapterId": "71493ff7-db25-4018-aa1a-4599f1cdf6d2"
    }
  ],
