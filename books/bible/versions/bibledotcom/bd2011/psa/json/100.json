{
  "chapter": {
    "id": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4",
    "number": 100,
    "ref": "https://www.bible.com/bible/19/PSA.100.BD2011",
    "createdAt": "2025-04-24T01:29:07.045Z",
