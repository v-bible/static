    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "0522ad66-c275-4715-a14d-fd1ad832621a",
      "number": 1,
      "content": "Hỡi cả địa cầu, hãy lớn tiếng reo mừng tung hô **CHÚA**.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.466Z",
      "updatedAt": "2025-04-24T01:56:59.466Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    },
    {
      "id": "b4062c93-bcc2-4fbc-97f3-827c4d57ec98",
      "number": 2,
      "content": "Hãy thờ phượng **CHÚA** cách vui mừng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.494Z",
      "updatedAt": "2025-04-24T01:56:59.494Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    },
    {
      "id": "601fdb9b-23cd-4b91-aa95-d1aa42857129",
      "number": 2,
      "content": "Hãy đến trước thánh nhan Ngài với tiếng hoan ca.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.515Z",
      "updatedAt": "2025-04-24T01:56:59.515Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    },
    {
      "id": "a5a26207-d443-410c-b1f9-83d11bc6b598",
      "number": 3,
      "content": "Khá biết rằng **CHÚA** là Đức Chúa Trời.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.526Z",
      "updatedAt": "2025-04-24T01:56:59.526Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    },
    {
      "id": "6249b2d3-6452-4499-96ba-7183899d7650",
      "number": 3,
      "content": "Chính Ngài đã dựng nên chúng ta; chúng ta thuộc về Ngài;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.538Z",
      "updatedAt": "2025-04-24T01:56:59.538Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    },
    {
      "id": "7fab43cc-4b90-4244-b857-be4554a240ec",
      "number": 3,
      "content": "Chúng ta là con dân Ngài và bầy chiên của đồng cỏ Ngài.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.550Z",
      "updatedAt": "2025-04-24T01:56:59.550Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    },
    {
      "id": "29be7361-0ff3-41ea-b086-be88801e1514",
      "number": 4,
      "content": "Hãy cảm tạ mà vào các cổng Ngài;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.563Z",
      "updatedAt": "2025-04-24T01:56:59.563Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    },
    {
      "id": "d3dd0408-7673-4419-b157-166d3ace84df",
      "number": 4,
      "content": "Khá ngợi ca mà vào các sân Ngài;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.576Z",
      "updatedAt": "2025-04-24T01:56:59.576Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    },
    {
      "id": "25321dd8-d92b-475e-90c4-7cf357f3856a",
      "number": 4,
      "content": "Hãy tạ ơn Ngài, khá chúc tụng danh Ngài.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.589Z",
      "updatedAt": "2025-04-24T01:56:59.589Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    },
    {
      "id": "5b5e179b-d869-46d9-a7ca-0f61ae7d9e96",
      "number": 5,
      "content": "Vì **CHÚA** thật tốt;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.602Z",
      "updatedAt": "2025-04-24T01:56:59.602Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    },
    {
      "id": "8ab9258e-0551-4686-a840-79ecb11c6000",
      "number": 5,
      "content": "Tình thương của Ngài hằng còn mãi mãi;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.616Z",
      "updatedAt": "2025-04-24T01:56:59.616Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    },
    {
      "id": "ad93197c-ddf1-476d-a8bd-c850f4e1189f",
      "number": 5,
      "content": "Đức thành tín của Ngài còn đến muôn đời.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:56:59.630Z",
      "updatedAt": "2025-04-24T01:56:59.630Z",
      "chapterId": "5a7b5f11-b3c6-4509-a2ee-a434ef9cefc4"
    }
  ],
