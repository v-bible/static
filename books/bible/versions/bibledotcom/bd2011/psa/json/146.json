    "number": 146,
    "ref": "https://www.bible.com/bible/19/PSA.146.BD2011",
    "createdAt": "2025-04-22T10:10:27.277Z",
    "updatedAt": "2025-04-22T10:10:27.277Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "53a5f2e1-880e-41e9-8bb6-5a50965bff73",
      "number": 1,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.410Z",
      "updatedAt": "2025-04-22T10:45:25.410Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "3f3b1a96-483c-4f63-868b-567e83ef72a1",
      "number": 10,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.760Z",
      "updatedAt": "2025-04-22T10:45:25.760Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "903a6f2a-66ff-4b27-a58b-d2faa1242a86",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi **CHÚA**!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.472Z",
      "updatedAt": "2025-04-22T10:45:25.472Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "ea249047-2eee-4594-bf75-c6c53d723237",
      "number": 2,
      "content": "Tôi sẽ ca ngợi **CHÚA** trọn đời mình;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.486Z",
      "updatedAt": "2025-04-22T10:45:25.486Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "abc30bcb-46dd-4fef-9e92-8fb25eb5e2a6",
      "number": 2,
      "content": "Hễ tôi còn sống bao lâu, tôi sẽ ca hát tôn ngợi Đức Chúa Trời của tôi bấy lâu.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.500Z",
      "updatedAt": "2025-04-22T10:45:25.500Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "8fe3ff12-ad79-45da-824c-91fade4739b1",
      "number": 3,
      "content": "Đừng để lòng tin cậy vào những người quyền thế;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.513Z",
      "updatedAt": "2025-04-22T10:45:25.513Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "9d8db282-c448-4ae3-b6e5-34cfe6b4baf4",
      "number": 3,
      "content": "Đừng tin cậy loài người là những kẻ không đủ khả năng cứu giúp.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.527Z",
      "updatedAt": "2025-04-22T10:45:25.527Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "0ef719e2-e013-4fff-afdd-7e4fc753dbca",
      "number": 4,
      "content": "Hơi thở tắt đi, thân xác nó liền trở về cát bụi;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.540Z",
      "updatedAt": "2025-04-22T10:45:25.540Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "1cf19cd3-cab4-4771-85cd-7a639a515d42",
      "number": 4,
      "content": "Trong chính ngày đó các toan tính của nó đều tiêu tan.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.554Z",
      "updatedAt": "2025-04-22T10:45:25.554Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "d3f16138-1665-4d22-a94c-0de3cd8367a6",
      "number": 5,
      "content": "Phước cho người được Đức Chúa Trời của Gia-cốp giúp đỡ mình,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.567Z",
      "updatedAt": "2025-04-22T10:45:25.567Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "3bb5a411-0622-439c-aa37-e54b3acd7ad6",
      "number": 5,
      "content": "Người đặt hy vọng nơi **CHÚA**, Đức Chúa Trời mình.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.581Z",
      "updatedAt": "2025-04-22T10:45:25.581Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "0c277179-7511-4911-8eb0-83eb376d2bf6",
      "number": 6,
      "content": "Ngài là Đấng dựng nên trời và đất,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.596Z",
      "updatedAt": "2025-04-22T10:45:25.596Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "7c273b85-9bd8-4461-9d25-a624de678e7d",
      "number": 6,
      "content": "Biển và mọi vật trong đó.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.608Z",
      "updatedAt": "2025-04-22T10:45:25.608Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "e3742063-eb19-42ec-9250-ba6f4178f642",
      "number": 6,
      "content": "Ngài giữ sự thành tín đến đời đời.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.619Z",
      "updatedAt": "2025-04-22T10:45:25.619Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "bef38d6c-7993-4e95-aed5-be609485d3d4",
      "number": 7,
      "content": "Ngài thi hành công lý cho người bị áp bức;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.630Z",
      "updatedAt": "2025-04-22T10:45:25.630Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "453229c7-37a3-4c2c-bc28-60d47651c46a",
      "number": 7,
      "content": "Ngài ban bánh cho người đói.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.640Z",
      "updatedAt": "2025-04-22T10:45:25.640Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "7de31d3e-deed-4824-ab4d-02f37f2a231b",
      "number": 7,
      "content": "**CHÚA** giải thoát người bị giam cầm.",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.650Z",
      "updatedAt": "2025-04-22T10:45:25.650Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "da3a06e7-2994-4d05-83e3-45d2f50c4a95",
      "number": 8,
      "content": "**CHÚA** mở mắt người bị mù;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.660Z",
      "updatedAt": "2025-04-22T10:45:25.660Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "d01143ee-245f-4c5a-85d2-08f4414704e6",
      "number": 8,
      "content": "**CHÚA** đỡ dậy những ai bị bắt phải sấp mình;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.670Z",
      "updatedAt": "2025-04-22T10:45:25.670Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "1fe96278-5962-4010-b8fe-05de817a2a8b",
      "number": 8,
      "content": "**CHÚA** yêu mến người ngay lành;",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.681Z",
      "updatedAt": "2025-04-22T10:45:25.681Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "d8ca1a2b-9d7e-4767-86c6-3af5fcf6557f",
      "number": 9,
      "content": "**CHÚA** bảo vệ những kiều dân;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.692Z",
      "updatedAt": "2025-04-22T10:45:25.692Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "e4c8ce9f-a66d-41f5-8262-37b9ca47e5f9",
      "number": 9,
      "content": "Ngài nâng đỡ cô nhi và quả phụ;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.703Z",
      "updatedAt": "2025-04-22T10:45:25.703Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "33f9c5b0-ae62-4dfb-989c-8b26ad1c6d9c",
      "number": 9,
      "content": "Nhưng Ngài phá hỏng đường lối của những kẻ ác.",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.715Z",
      "updatedAt": "2025-04-22T10:45:25.715Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "2e214169-9e00-410d-80c2-582f419e1f4b",
      "number": 10,
      "content": "**CHÚA** sẽ trị vì đời đời;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.728Z",
      "updatedAt": "2025-04-22T10:45:25.728Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    },
    {
      "id": "7e8485f8-16ee-41d0-bb7f-184ec7eddf37",
      "number": 10,
      "content": "Hỡi Si-ôn, Đức Chúa Trời của ngươi sẽ trị vì đời đời.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:25.745Z",
      "updatedAt": "2025-04-22T10:45:25.745Z",
      "chapterId": "24b94b48-a802-425c-8f9f-f79206ef713c"
    }
  ],
