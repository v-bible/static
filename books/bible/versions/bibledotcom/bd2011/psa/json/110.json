{
  "chapter": {
    "id": "333d3e6c-8db9-4a3b-a9c1-76435c356120",
    "number": 110,
    "ref": "https://www.bible.com/bible/19/PSA.110.BD2011",
    "createdAt": "2025-04-24T01:29:07.179Z",
    "updatedAt": "2025-04-24T01:29:07.179Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "15001fb9-a92a-4dd1-a395-9f3332b38225",
      "number": 1,
      "content": "**CHÚA** phán với Chúa của tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.525Z",
      "updatedAt": "2025-04-24T01:57:41.525Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "cd18c84a-9f01-4667-aaae-1c177b8ec8c4",
      "number": 1,
      "content": "“Con hãy ngồi bên phải Ta,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.553Z",
      "updatedAt": "2025-04-24T01:57:41.553Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "d62a0dc4-758b-4392-bac6-259638298c55",
      "number": 1,
      "content": "Cho đến khi Ta đặt những kẻ thù của Con làm bệ chân Con.”",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.563Z",
      "updatedAt": "2025-04-24T01:57:41.563Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "a5557dbb-0bb1-4217-b13c-d7618ee3b157",
      "number": 2,
      "content": "“Từ Si-ôn **CHÚA** sẽ đưa vương trượng uy quyền của Con ra:",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.574Z",
      "updatedAt": "2025-04-24T01:57:41.574Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "a3294771-78d5-41bd-80e2-0ea93e61a91a",
      "number": 2,
      "content": "Hãy trị vì những kẻ thù của Con.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.585Z",
      "updatedAt": "2025-04-24T01:57:41.585Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "0290b318-c904-40a0-bdac-97527bee039a",
      "number": 3,
      "content": "Trong ngày Con biểu dương quyền lực,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.598Z",
      "updatedAt": "2025-04-24T01:57:41.598Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "1424b139-1733-474e-b9b6-8013284f221e",
      "number": 3,
      "content": "Dân tộc Con sẽ hăng hái xung phong;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.611Z",
      "updatedAt": "2025-04-24T01:57:41.611Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "45afa4f7-8d9c-4fc4-8289-af764f5ff53b",
      "number": 3,
      "content": "Họ sẽ tập họp thành đội ngũ chỉnh tề khi hừng đông ló dạng;",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.625Z",
      "updatedAt": "2025-04-24T01:57:41.625Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "c808667a-7d97-42cc-81b5-ad9dbf526d9d",
      "number": 3,
      "content": "Những trai trẻ sẽ quy tụ quanh Con",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.638Z",
      "updatedAt": "2025-04-24T01:57:41.638Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "1cfde82c-546b-4961-8224-026a88928bc5",
      "number": 3,
      "content": "Như sương phủ ban mai.”",
      "order": 4,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.652Z",
      "updatedAt": "2025-04-24T01:57:41.652Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "614eb53f-1a8e-4a79-be45-648bc931b507",
      "number": 4,
      "content": "**CHÚA** đã thề",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.665Z",
      "updatedAt": "2025-04-24T01:57:41.665Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "bc40adf8-1e8c-4e21-8488-ab57d00b23a6",
      "number": 4,
      "content": "Và sẽ không đổi ý của Ngài rằng,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.679Z",
      "updatedAt": "2025-04-24T01:57:41.679Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "29c69528-2482-4aa1-ab1f-6bb5e5f3cfcd",
      "number": 4,
      "content": "“Con là tư tế đời đời",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.693Z",
      "updatedAt": "2025-04-24T01:57:41.693Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "112722c6-78f0-4122-9778-0f679ba1ea2e",
      "number": 4,
      "content": "Theo Dòng Mên-chi-xê-đéc.”",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.707Z",
      "updatedAt": "2025-04-24T01:57:41.707Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "d51ab7be-eb16-466b-ac5e-36e3a53598c7",
      "number": 5,
      "content": "Chúa ở bên phải Con.",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.720Z",
      "updatedAt": "2025-04-24T01:57:41.720Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "3e3ce184-467a-4586-93c3-ddfce32a0a40",
      "number": 5,
      "content": "Ngài sẽ đập tan các vua trong ngày Ngài nổi giận.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.734Z",
      "updatedAt": "2025-04-24T01:57:41.734Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "99805523-9a71-42d9-8150-d9247b42aec0",
      "number": 6,
      "content": "Ngài sẽ đoán phạt các dân;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.751Z",
      "updatedAt": "2025-04-24T01:57:41.751Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "ddf18db5-2d8e-4965-bb3d-286bcd351b96",
      "number": 6,
      "content": "Ngài sẽ làm cho thây những kẻ ác chất cao thành đống;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.765Z",
      "updatedAt": "2025-04-24T01:57:41.765Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "68bbc221-87b7-48d2-917f-abf443ecdceb",
      "number": 6,
      "content": "Ngài sẽ đập tan các lãnh tụ của thế gian.",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.778Z",
      "updatedAt": "2025-04-24T01:57:41.778Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "c11dac8d-34d8-4af1-9d3b-94c6114aaf37",
      "number": 7,
      "content": "Ngài sẽ uống nước suối bên đường,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.792Z",
      "updatedAt": "2025-04-24T01:57:41.792Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    },
    {
      "id": "2adbb346-68c8-4fbc-aace-a0a88f68a32a",
      "number": 7,
      "content": "Rồi Ngài sẽ ngẩng đầu lên cách hiên ngang đắc thắng.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:57:41.806Z",
      "updatedAt": "2025-04-24T01:57:41.806Z",
      "chapterId": "333d3e6c-8db9-4a3b-a9c1-76435c356120"
    }
  ],
