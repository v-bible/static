{
  "chapter": {
    "id": "99cb389d-b976-4715-afa6-17f8a29d3412",
    "number": 8,
    "ref": "https://www.bible.com/bible/19/PSA.8.BD2011",
    "createdAt": "2025-04-24T01:29:05.858Z",
    "updatedAt": "2025-04-24T01:29:05.858Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "fc6fafaf-13a8-4930-ab36-cb6af38b0da6",
      "number": 1,
      "content": "Lạy **CHÚA**, Chúa của chúng con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.742Z",
      "updatedAt": "2025-04-24T01:51:16.742Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "ac7a133c-00ad-463d-9828-075424f90d40",
      "number": 1,
      "content": "Danh Ngài thật uy nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.762Z",
      "updatedAt": "2025-04-24T01:51:16.762Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "bb9910ae-3fe7-47be-b27a-f043bc19528b",
      "number": 1,
      "content": "Ngài đã đặt vinh quang Ngài trên các tầng trời!",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.772Z",
      "updatedAt": "2025-04-24T01:51:16.772Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "c4cfd667-0a2f-457d-9a90-68724e58b058",
      "number": 2,
      "content": "Từ miệng các trẻ thơ và các ấu nhi Ngài đã biểu lộ quyền năng,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.782Z",
      "updatedAt": "2025-04-24T01:51:16.782Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "787dfb38-fa48-4719-b56c-c89b1ea7618e",
      "number": 2,
      "content": "Để quân chống lại Ngài thấy rõ,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.792Z",
      "updatedAt": "2025-04-24T01:51:16.792Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "977ca473-38ef-4f82-a292-a58f2383df29",
      "number": 2,
      "content": "Để kẻ thù và kẻ báo thù phải im hơi bỏ cuộc.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.802Z",
      "updatedAt": "2025-04-24T01:51:16.802Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "7ed06677-a385-4f90-8f06-0da0142b7764",
      "number": 3,
      "content": "Khi con lặng ngắm cõi trời cao, công việc của ngón tay Ngài,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.813Z",
      "updatedAt": "2025-04-24T01:51:16.813Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "c08627f9-52d3-415e-a89e-0ab072056ebc",
      "number": 3,
      "content": "Mặt trăng và các ngôi sao mà Ngài đã đặt trên không trung;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.829Z",
      "updatedAt": "2025-04-24T01:51:16.829Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "80683fff-9f9f-440b-9c96-7e88d4156d3a",
      "number": 4,
      "content": "Loài người là gì mà Ngài phải quan tâm đến?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.840Z",
      "updatedAt": "2025-04-24T01:51:16.840Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "5d93b707-39de-4d6b-a593-1c412d76be52",
      "number": 4,
      "content": "Con của loài người là chi mà Ngài phải bận lòng?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.852Z",
      "updatedAt": "2025-04-24T01:51:16.852Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "0fb1dab1-e3b1-4f69-9650-eb91dafea3ab",
      "number": 5,
      "content": "Ngài đã dựng nên loài người thấp hơn các vị thần một chút;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.865Z",
      "updatedAt": "2025-04-24T01:51:16.865Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "ec02b4ee-b72b-41fb-83af-230e28076d33",
      "number": 5,
      "content": "Ngài đội trên đầu loài người vinh hiển và tôn trọng.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.891Z",
      "updatedAt": "2025-04-24T01:51:16.891Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "6088d325-5312-4e49-8cf4-a1870a48b3f4",
      "number": 6,
      "content": "Ngài cho loài người quản trị mọi vật do tay Ngài dựng nên;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.905Z",
      "updatedAt": "2025-04-24T01:51:16.905Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "00be0498-ae4d-4d72-9999-c43c53870d4a",
      "number": 6,
      "content": "Ngài bắt muôn vật phục dưới chân loài người,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.918Z",
      "updatedAt": "2025-04-24T01:51:16.918Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "c235d464-3b39-47cf-9de5-5126179b31ea",
      "number": 7,
      "content": "Tức tất cả chiên và bò,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.931Z",
      "updatedAt": "2025-04-24T01:51:16.931Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "aec97c18-174f-465c-9dac-065e9d5290ce",
      "number": 7,
      "content": "Cùng các thú vật ngoài đồng,",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.948Z",
      "updatedAt": "2025-04-24T01:51:16.948Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "115437c2-bd74-42f9-a75c-8499224da7a7",
      "number": 8,
      "content": "Các loài chim trên trời và mọi loài cá dưới biển,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.961Z",
      "updatedAt": "2025-04-24T01:51:16.961Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "d4667814-51e5-462b-a620-52b697983722",
      "number": 8,
      "content": "Tức mọi vật bơi lội trong các dòng nước biển.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.974Z",
      "updatedAt": "2025-04-24T01:51:16.974Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "be7ad07b-f332-49bc-9c93-325af0b79f75",
      "number": 9,
      "content": "Lạy **CHÚA**, Chúa của chúng con,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:16.988Z",
      "updatedAt": "2025-04-24T01:51:16.988Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    },
    {
      "id": "7b63df2e-7dcf-458b-81e0-324e613208f5",
      "number": 9,
      "content": "Danh Ngài thật uy nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:51:17.002Z",
      "updatedAt": "2025-04-24T01:51:17.002Z",
      "chapterId": "99cb389d-b976-4715-afa6-17f8a29d3412"
    }
  ],
