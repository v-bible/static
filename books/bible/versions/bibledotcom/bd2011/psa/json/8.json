  },
  "verses": [
    {
      "id": "14e41147-5e03-446c-bc40-1217351ca456",
      "number": 1,
      "content": "Lạy **CHÚA**, Chúa của chúng con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.492Z",
      "updatedAt": "2025-04-22T10:35:28.492Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "fc5a304f-464f-4c46-bcd7-311ebac5fc2f",
      "number": 9,
      "content": "Lạy **CHÚA**, Chúa của chúng con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.777Z",
      "updatedAt": "2025-04-22T10:35:28.777Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "d3e351b1-a9c7-4173-8fa4-afafc0844492",
      "number": 1,
      "content": "Danh Ngài thật uy nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.550Z",
      "updatedAt": "2025-04-22T10:35:28.550Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "2bf817f3-e351-4e74-9aca-bf72e7d4231c",
      "number": 9,
      "content": "Danh Ngài thật uy nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.787Z",
      "updatedAt": "2025-04-22T10:35:28.787Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "abbb4fdc-cc8f-4bbc-ad5b-1f58369e4699",
      "number": 1,
      "content": "Ngài đã đặt vinh quang Ngài trên các tầng trời!",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.562Z",
      "updatedAt": "2025-04-22T10:35:28.562Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "c3f937ef-6b20-419d-a1a3-42e05352b04c",
      "number": 2,
      "content": "Từ miệng các trẻ thơ và các ấu nhi Ngài đã biểu lộ quyền năng,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.576Z",
      "updatedAt": "2025-04-22T10:35:28.576Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "11e79548-2136-4d1a-a67a-0fa8bdd8316f",
      "number": 2,
      "content": "Để quân chống lại Ngài thấy rõ,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.589Z",
      "updatedAt": "2025-04-22T10:35:28.589Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "f7c542da-465e-4cd9-ac18-1675e32400ff",
      "number": 2,
      "content": "Để kẻ thù và kẻ báo thù phải im hơi bỏ cuộc.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.602Z",
      "updatedAt": "2025-04-22T10:35:28.602Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "6ad74b96-c2e3-46d0-85ea-9f9573de8447",
      "number": 3,
      "content": "Khi con lặng ngắm cõi trời cao, công việc của ngón tay Ngài,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.617Z",
      "updatedAt": "2025-04-22T10:35:28.617Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "b9e37a06-4564-48af-b308-586df7706626",
      "number": 3,
      "content": "Mặt trăng và các ngôi sao mà Ngài đã đặt trên không trung;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.631Z",
      "updatedAt": "2025-04-22T10:35:28.631Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "6e455426-3f43-4d8e-a5ec-4c1aaf68c764",
      "number": 4,
      "content": "Loài người là gì mà Ngài phải quan tâm đến?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.645Z",
      "updatedAt": "2025-04-22T10:35:28.645Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "d8f8ea09-086a-476c-ba6b-88775a69cc17",
      "number": 4,
      "content": "Con của loài người là chi mà Ngài phải bận lòng?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.656Z",
      "updatedAt": "2025-04-22T10:35:28.656Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "7e701284-97a7-42a8-b695-bb5635e3106b",
      "number": 5,
      "content": "Ngài đã dựng nên loài người thấp hơn các vị thần một chút;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.666Z",
      "updatedAt": "2025-04-22T10:35:28.666Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "c4468855-d852-4947-9154-59df7850be6c",
      "number": 5,
      "content": "Ngài đội trên đầu loài người vinh hiển và tôn trọng.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.692Z",
      "updatedAt": "2025-04-22T10:35:28.692Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "4d75e080-cf5e-4621-b0be-4cdd27ad1d3f",
      "number": 6,
      "content": "Ngài cho loài người quản trị mọi vật do tay Ngài dựng nên;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.703Z",
      "updatedAt": "2025-04-22T10:35:28.703Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "e7239ee7-ee5a-48a6-abf6-e7319611a5ab",
      "number": 6,
      "content": "Ngài bắt muôn vật phục dưới chân loài người,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.717Z",
      "updatedAt": "2025-04-22T10:35:28.717Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "a4a4d102-4e1c-4144-a68e-0cab4694c62d",
      "number": 7,
      "content": "Tức tất cả chiên và bò,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.733Z",
      "updatedAt": "2025-04-22T10:35:28.733Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "7742b3bd-f287-4ec1-a274-ce774703a0f5",
      "number": 7,
      "content": "Cùng các thú vật ngoài đồng,",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.743Z",
      "updatedAt": "2025-04-22T10:35:28.743Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "46d01fde-e7f3-40d8-9087-b2bf1e599a9c",
      "number": 8,
      "content": "Các loài chim trên trời và mọi loài cá dưới biển,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.756Z",
      "updatedAt": "2025-04-22T10:35:28.756Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    },
    {
      "id": "63bb8992-fbb8-4c3f-9f37-18d9be5c8378",
      "number": 8,
      "content": "Tức mọi vật bơi lội trong các dòng nước biển.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:35:28.766Z",
      "updatedAt": "2025-04-22T10:35:28.766Z",
      "chapterId": "92493251-43fc-4254-8a91-241a39675884"
    }
  ],
