  "verses": [
    {
      "id": "e628ca51-ef7a-4f48-ae91-f8c8f8143770",
      "number": 1,
      "content": "Hỡi tất cả tôi tớ **CHÚA**,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:33.028Z",
      "updatedAt": "2025-04-22T10:44:33.028Z",
      "chapterId": "a85616f0-0814-4ce5-af8c-541f32e48ebf"
    },
    {
      "id": "cdef8e0a-8c1e-402e-bc8f-40e44a22460d",
      "number": 1,
      "content": "Những người phục vụ trong nhà **CHÚA** ban đêm, xin hãy chúc tụng **CHÚA**.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:33.076Z",
      "updatedAt": "2025-04-22T10:44:33.076Z",
      "chapterId": "a85616f0-0814-4ce5-af8c-541f32e48ebf"
    },
    {
      "id": "0849e027-78fd-46f0-bcae-86afdeccb344",
      "number": 2,
      "content": "Xin đưa cao đôi tay quý vị lên trong nơi thánh",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:33.086Z",
      "updatedAt": "2025-04-22T10:44:33.086Z",
      "chapterId": "a85616f0-0814-4ce5-af8c-541f32e48ebf"
    },
    {
      "id": "f3d942f1-00db-4eff-8746-11cc80f3bbf5",
      "number": 2,
      "content": "Và chúc tụng **CHÚA**.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:33.098Z",
      "updatedAt": "2025-04-22T10:44:33.098Z",
      "chapterId": "a85616f0-0814-4ce5-af8c-541f32e48ebf"
    },
    {
      "id": "7576faa1-1bdb-46ef-acb2-c93ff75be4f1",
      "number": 3,
      "content": "Nguyện **CHÚA**, Đấng dựng nên trời và đất,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:33.111Z",
      "updatedAt": "2025-04-22T10:44:33.111Z",
      "chapterId": "a85616f0-0814-4ce5-af8c-541f32e48ebf"
    },
    {
      "id": "8d1ca9aa-62d6-4a9d-b74f-a0de97db9dd1",
      "number": 3,
      "content": "Ban phước cho quý vị từ Si-ôn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:44:33.124Z",
      "updatedAt": "2025-04-22T10:44:33.124Z",
      "chapterId": "a85616f0-0814-4ce5-af8c-541f32e48ebf"
    }
  ],
