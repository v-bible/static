  "verses": [
    {
      "id": "d65c9916-96d8-4ab0-8a37-532c10447992",
      "number": 1,
      "content": "Hỡi tất cả tôi tớ **CHÚA**,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:25.996Z",
      "updatedAt": "2025-04-24T01:59:25.996Z",
      "chapterId": "52c50518-1ee4-4541-848e-4c0cc4048f26"
    },
    {
      "id": "43f49ab5-3e0e-4909-b33f-ed7ff0186d45",
      "number": 1,
      "content": "Những người phục vụ trong nhà **CHÚA** ban đêm, xin hãy chúc tụng **CHÚA**.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:26.023Z",
      "updatedAt": "2025-04-24T01:59:26.023Z",
      "chapterId": "52c50518-1ee4-4541-848e-4c0cc4048f26"
    },
    {
      "id": "370dfe3a-7480-41ef-a725-79fa8e407014",
      "number": 2,
      "content": "Xin đưa cao đôi tay quý vị lên trong nơi thánh",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:26.032Z",
      "updatedAt": "2025-04-24T01:59:26.032Z",
      "chapterId": "52c50518-1ee4-4541-848e-4c0cc4048f26"
    },
    {
      "id": "a493210b-f28d-44d8-b2d5-ac340d8191f9",
      "number": 2,
      "content": "Và chúc tụng **CHÚA**.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:26.043Z",
      "updatedAt": "2025-04-24T01:59:26.043Z",
      "chapterId": "52c50518-1ee4-4541-848e-4c0cc4048f26"
    },
    {
      "id": "9281dc6d-72b8-4870-a9c0-156eefb9453e",
      "number": 3,
      "content": "Nguyện **CHÚA**, Đấng dựng nên trời và đất,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:26.054Z",
      "updatedAt": "2025-04-24T01:59:26.054Z",
      "chapterId": "52c50518-1ee4-4541-848e-4c0cc4048f26"
    },
    {
      "id": "1fa4acf6-e4a0-4962-b68a-9354f410cb60",
      "number": 3,
      "content": "Ban phước cho quý vị từ Si-ôn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:59:26.065Z",
      "updatedAt": "2025-04-24T01:59:26.065Z",
      "chapterId": "52c50518-1ee4-4541-848e-4c0cc4048f26"
    }
  ],
