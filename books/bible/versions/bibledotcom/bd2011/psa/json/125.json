{
  "chapter": {
    "id": "9616f7c4-8f08-4078-90aa-359502689c76",
    "number": 125,
    "ref": "https://www.bible.com/bible/19/PSA.125.BD2011",
    "createdAt": "2025-04-24T01:29:07.388Z",
    "updatedAt": "2025-04-24T01:29:07.388Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "1d301ac6-05b9-48dc-9bc1-be098307c91e",
      "number": 1,
      "content": "Phàm ai tin cậy **CHÚA** sẽ như Núi Si-ôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.317Z",
      "updatedAt": "2025-04-24T01:58:55.317Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "54754c62-3dbe-48ad-9d79-2dac6faff7a4",
      "number": 1,
      "content": "Chẳng hề rúng động nhưng cứ bền vững muôn đời.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.345Z",
      "updatedAt": "2025-04-24T01:58:55.345Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "52b1a8de-c101-4bb7-8da5-7d5c43f2f653",
      "number": 2,
      "content": "Như các rặng núi bao quanh Giê-ru-sa-lem thể nào,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.355Z",
      "updatedAt": "2025-04-24T01:58:55.355Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "9895be67-11c6-4e89-8cb4-a511a0b762f3",
      "number": 2,
      "content": "**CHÚA** bao quanh dân Ngài thể ấy,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.366Z",
      "updatedAt": "2025-04-24T01:58:55.366Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "c66ef632-6428-4df4-95fe-8d238e720ba1",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.378Z",
      "updatedAt": "2025-04-24T01:58:55.378Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "9d25427b-e772-439b-b9ad-61634317ad10",
      "number": 3,
      "content": "Vì gậy cầm quyền của kẻ ác không được phép hành sử",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.391Z",
      "updatedAt": "2025-04-24T01:58:55.391Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "87666060-d214-462c-9d32-89a4710dd1ae",
      "number": 3,
      "content": "Trên địa phận của người ngay lành,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.405Z",
      "updatedAt": "2025-04-24T01:58:55.405Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "4e4d61a8-d51a-4c6c-8711-77c66ced7ab3",
      "number": 3,
      "content": "Nên tay người ngay lành",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.418Z",
      "updatedAt": "2025-04-24T01:58:55.418Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "8f5448d3-8039-4370-b5ae-04cab4081957",
      "number": 3,
      "content": "Không bị buộc phải tham dự vào việc ác.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.432Z",
      "updatedAt": "2025-04-24T01:58:55.432Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "e7003f9d-904a-4685-a86c-e056a5d7bad1",
      "number": 4,
      "content": "**CHÚA** ôi, xin ban phước cho những người tốt,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.446Z",
      "updatedAt": "2025-04-24T01:58:55.446Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "dcb45814-0ca3-4d43-9655-26e82a671427",
      "number": 4,
      "content": "Và cho những người có lòng ngay thẳng thật thà.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.459Z",
      "updatedAt": "2025-04-24T01:58:55.459Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "610b0703-99a3-4a1a-b43c-0d30466b366a",
      "number": 5,
      "content": "Nhưng đối với những kẻ bỏ Ngài để đi theo con đường lươn lẹo,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.473Z",
      "updatedAt": "2025-04-24T01:58:55.473Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "db89b1f3-9a80-4a80-9b3c-20fc58c91e9d",
      "number": 5,
      "content": "Xin **CHÚA** trục xuất chúng chung với những kẻ làm điều ác.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.487Z",
      "updatedAt": "2025-04-24T01:58:55.487Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    },
    {
      "id": "be8f1354-a99f-43b6-b49e-32834170593a",
      "number": 5,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:55.500Z",
      "updatedAt": "2025-04-24T01:58:55.500Z",
      "chapterId": "9616f7c4-8f08-4078-90aa-359502689c76"
    }
  ],
