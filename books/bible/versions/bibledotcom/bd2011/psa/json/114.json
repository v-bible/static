{
  "chapter": {
    "id": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed",
    "number": 114,
    "ref": "https://www.bible.com/bible/19/PSA.114.BD2011",
    "createdAt": "2025-04-24T01:29:07.233Z",
    "updatedAt": "2025-04-24T01:29:07.233Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "6f8ceee1-44d0-43ea-9035-5db6065a61fa",
      "number": 1,
      "content": "Khi dân I-sơ-ra-ên ra khỏi Ai-cập,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.102Z",
      "updatedAt": "2025-04-24T01:58:07.102Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "dfb8f616-7096-4dec-a56e-3f91391f3d4a",
      "number": 1,
      "content": "Nhà Gia-cốp ra khỏi một dân nói một ngôn ngữ lạ,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.123Z",
      "updatedAt": "2025-04-24T01:58:07.123Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "89b8e085-9723-40ad-b432-7fb4f307156c",
      "number": 2,
      "content": "Giu-đa trở thành nơi thánh của Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.134Z",
      "updatedAt": "2025-04-24T01:58:07.134Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "8c1ea9dd-3872-4c2c-a0e7-729d9d6fdd4e",
      "number": 2,
      "content": "I-sơ-ra-ên trở nên vương quốc của Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.144Z",
      "updatedAt": "2025-04-24T01:58:07.144Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "8ff4f984-0c90-47b0-9747-db127d534228",
      "number": 3,
      "content": "Nước đại dương trông thấy thế liền hoảng kinh chạy trốn;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.155Z",
      "updatedAt": "2025-04-24T01:58:07.155Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "f9b17a08-ff98-4075-a940-786facdb2b57",
      "number": 3,
      "content": "Nước Sông Giô-đanh hỗn loạn chảy ngược về nguồn;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.165Z",
      "updatedAt": "2025-04-24T01:58:07.165Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "b75c8934-dcc5-4383-9448-7b64addc59f3",
      "number": 4,
      "content": "Các rặng núi nhảy lên như chiên đực;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.176Z",
      "updatedAt": "2025-04-24T01:58:07.176Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "d26de0b7-67c1-4f6c-91e2-647927bb3f39",
      "number": 4,
      "content": "Các dãy đồi nhảy cỡn như chiên con.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.188Z",
      "updatedAt": "2025-04-24T01:58:07.188Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "f73e48ac-b619-4588-bffe-88a1e5885672",
      "number": 5,
      "content": "Hỡi nước đại dương, điều gì đã khiến ngươi để ngươi chạy trốn?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.201Z",
      "updatedAt": "2025-04-24T01:58:07.201Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "bbca124f-04a3-4d14-882f-880bb6bca159",
      "number": 5,
      "content": "Hỡi nước Sông Giô-đanh, sao ngươi lại chảy ngược về nguồn?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.214Z",
      "updatedAt": "2025-04-24T01:58:07.214Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "01ff3055-1709-4853-a94c-6927512af156",
      "number": 6,
      "content": "Hỡi các rặng núi, sao các ngươi nhảy lên như chiên đực?",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.228Z",
      "updatedAt": "2025-04-24T01:58:07.228Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "0050a96a-ca7c-4cc7-8f81-b43dc5b8cf10",
      "number": 6,
      "content": "Hỡi các dãy đồi, sao các ngươi nhảy cỡn như chiên con?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.241Z",
      "updatedAt": "2025-04-24T01:58:07.241Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "ebe5e982-23b5-463e-a8e4-da07f594f60f",
      "number": 7,
      "content": "Hỡi trái đất, khá run rẩy trước mặt Chúa,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.254Z",
      "updatedAt": "2025-04-24T01:58:07.254Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "fff22e66-70bf-4b32-82e6-1f6c58ec7194",
      "number": 7,
      "content": "Tức trước thánh nhan Đức Chúa Trời của Gia-cốp,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.267Z",
      "updatedAt": "2025-04-24T01:58:07.267Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "45d6a446-7d6e-43c9-9dd7-641daa57cead",
      "number": 8,
      "content": "Đấng khiến nước từ một vầng đá tuôn ra thành ao hồ,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.279Z",
      "updatedAt": "2025-04-24T01:58:07.279Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    },
    {
      "id": "dbb7cdfa-2eae-49da-988c-534c1d3a09f6",
      "number": 8,
      "content": "Từ tảng đá hoa cương, dòng nước ngọt lai láng trào ra.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:07.292Z",
      "updatedAt": "2025-04-24T01:58:07.292Z",
      "chapterId": "a122c7ae-0ad7-44ef-8b40-1bd92ba7eeed"
    }
  ],
