{
  "chapter": {
    "id": "8529a192-8565-4184-93d6-f1e0051116ec",
    "number": 114,
    "ref": "https://www.bible.com/bible/19/PSA.114.BD2011",
    "createdAt": "2025-04-22T10:10:26.927Z",
    "updatedAt": "2025-04-22T10:10:26.927Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "fa84048e-0881-4d6a-8397-33e41bf1fe24",
      "number": 1,
      "content": "Khi dân I-sơ-ra-ên ra khỏi Ai-cập,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:11.855Z",
      "updatedAt": "2025-04-22T10:43:11.855Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "dedb8544-762a-40a9-b0df-35587758a627",
      "number": 1,
      "content": "Nhà Gia-cốp ra khỏi một dân nói một ngôn ngữ lạ,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:11.883Z",
      "updatedAt": "2025-04-22T10:43:11.883Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "45f370fd-0cae-4ee0-9645-676963bcf53b",
      "number": 2,
      "content": "Giu-đa trở thành nơi thánh của Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:11.894Z",
      "updatedAt": "2025-04-22T10:43:11.894Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "d71ff178-fdec-4f83-84d6-873a68434d1a",
      "number": 2,
      "content": "I-sơ-ra-ên trở nên vương quốc của Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:11.903Z",
      "updatedAt": "2025-04-22T10:43:11.903Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "353ea25b-34ad-4fc6-989f-1e1acc674273",
      "number": 3,
      "content": "Nước đại dương trông thấy thế liền hoảng kinh chạy trốn;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:11.914Z",
      "updatedAt": "2025-04-22T10:43:11.914Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "2eab0ad7-f2e1-48fd-8828-ec16bdf72bf8",
      "number": 3,
      "content": "Nước Sông Giô-đanh hỗn loạn chảy ngược về nguồn;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:11.926Z",
      "updatedAt": "2025-04-22T10:43:11.926Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "cc97d6bf-5360-4a40-b74b-c32ac5cc0106",
      "number": 4,
      "content": "Các rặng núi nhảy lên như chiên đực;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:11.939Z",
      "updatedAt": "2025-04-22T10:43:11.939Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "d30d409b-1cb7-43ad-a60b-ce30356062e3",
      "number": 4,
      "content": "Các dãy đồi nhảy cỡn như chiên con.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:11.952Z",
      "updatedAt": "2025-04-22T10:43:11.952Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "d945175d-7772-4bb3-9112-35101fae8c11",
      "number": 5,
      "content": "Hỡi nước đại dương, điều gì đã khiến ngươi để ngươi chạy trốn?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:11.965Z",
      "updatedAt": "2025-04-22T10:43:11.965Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "90cf1941-2018-48e4-8ec7-1f6d648f4315",
      "number": 5,
      "content": "Hỡi nước Sông Giô-đanh, sao ngươi lại chảy ngược về nguồn?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:11.979Z",
      "updatedAt": "2025-04-22T10:43:11.979Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "1c970aad-4c19-4939-ba58-f0c61965052d",
      "number": 6,
      "content": "Hỡi các rặng núi, sao các ngươi nhảy lên như chiên đực?",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:11.995Z",
      "updatedAt": "2025-04-22T10:43:11.995Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "c67d46c2-ed4b-4c27-a7c3-6e34784d0542",
      "number": 6,
      "content": "Hỡi các dãy đồi, sao các ngươi nhảy cỡn như chiên con?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:12.008Z",
      "updatedAt": "2025-04-22T10:43:12.008Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "788868a7-a42c-4500-ab4c-3234c06b836e",
      "number": 7,
      "content": "Hỡi trái đất, khá run rẩy trước mặt Chúa,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:12.022Z",
      "updatedAt": "2025-04-22T10:43:12.022Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "f27e49de-fcd2-4e9b-966e-050aba8ea994",
      "number": 7,
      "content": "Tức trước thánh nhan Đức Chúa Trời của Gia-cốp,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:12.036Z",
      "updatedAt": "2025-04-22T10:43:12.036Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "f9c3aa28-9160-45c6-ba05-2321b56dfcde",
      "number": 8,
      "content": "Đấng khiến nước từ một vầng đá tuôn ra thành ao hồ,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:12.049Z",
      "updatedAt": "2025-04-22T10:43:12.049Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    },
    {
      "id": "fe261cb9-557e-4a43-bcb9-58ebf4fb7c8d",
      "number": 8,
      "content": "Từ tảng đá hoa cương, dòng nước ngọt lai láng trào ra.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:12.063Z",
      "updatedAt": "2025-04-22T10:43:12.063Z",
      "chapterId": "8529a192-8565-4184-93d6-f1e0051116ec"
    }
  ],
