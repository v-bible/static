  "headings": [
    {
      "id": "774c0d1d-ac66-4e15-80bf-1b7eee8d3973",
      "content": "Lời Cầu Xin Giữa Cơn Hoạn Nạn",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T01:57:06.205Z",
      "updatedAt": "2025-04-24T01:57:06.205Z",
      "verseId": "1fe374f4-bb27-454e-bd21-bba16a3ec57f",
      "chapterId": "2501614a-5c48-4976-92a7-9db675307d2b"
    }
  ],
  "references": [],
  "psalmMetadata": [
    {
      "id": "85acc0bf-c42c-4d89-b7f5-cd22e8fe606c",
      "title": "Lời cầu nguyện của người gặp khốn khó, trong lúc ngã lòng người ấy dốc đổ lòng mình kêu van trước mặt CHÚA",
      "createdAt": "2025-04-24T01:57:07.804Z",
      "updatedAt": "2025-04-24T01:57:07.804Z",
      "chapterId": "2501614a-5c48-4976-92a7-9db675307d2b"
    }
  ]
}