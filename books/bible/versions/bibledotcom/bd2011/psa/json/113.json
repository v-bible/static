{
  "chapter": {
    "id": "9695006a-c11f-45bb-92b3-08868627b6e1",
    "number": 113,
    "ref": "https://www.bible.com/bible/19/PSA.113.BD2011",
    "createdAt": "2025-04-24T01:29:07.220Z",
    "updatedAt": "2025-04-24T01:29:07.220Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "715bb467-b19a-4730-8a3f-c3df9e9eb66d",
      "number": 1,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.144Z",
      "updatedAt": "2025-04-24T01:58:03.144Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "67f17872-5537-4c7f-8d66-a7fe9290facc",
      "number": 1,
      "content": "Hỡi các tôi tớ **CHÚA**, hãy ca ngợi Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.171Z",
      "updatedAt": "2025-04-24T01:58:03.171Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "6abcc802-1d28-4e44-b893-d7917dfec2ee",
      "number": 1,
      "content": "Hãy ca ngợi danh **CHÚA**.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.181Z",
      "updatedAt": "2025-04-24T01:58:03.181Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "3aeeaacf-fd30-4154-b909-cee6515ea1bb",
      "number": 2,
      "content": "Hãy chúc tụng danh **CHÚA**,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.192Z",
      "updatedAt": "2025-04-24T01:58:03.192Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "63556c77-c772-4a72-addd-88ed1796159a",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.205Z",
      "updatedAt": "2025-04-24T01:58:03.205Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "ed449435-982c-42e3-82d6-56944ee1f670",
      "number": 3,
      "content": "Từ bình minh cho đến hoàng hôn,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.215Z",
      "updatedAt": "2025-04-24T01:58:03.215Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "eb879d3c-a70f-4356-af68-7f0624e9c631",
      "number": 3,
      "content": "Danh **CHÚA** phải được tôn vinh ca ngợi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.226Z",
      "updatedAt": "2025-04-24T01:58:03.226Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "44ca026c-a70a-4c80-a3e9-c4edae5589c2",
      "number": 4,
      "content": "**CHÚA** được tôn cao hơn tất cả các dân các nước;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.237Z",
      "updatedAt": "2025-04-24T01:58:03.237Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "f4fe3c71-5a0d-4a79-8921-b105a0aaaf44",
      "number": 4,
      "content": "Vinh quang Ngài vượt trỗi hơn các tầng trời.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.249Z",
      "updatedAt": "2025-04-24T01:58:03.249Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "ecc20ef2-4cde-47ef-8764-c4d51665d985",
      "number": 5,
      "content": "Có ai giống như **CHÚA**, Đức Chúa Trời chúng ta,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.261Z",
      "updatedAt": "2025-04-24T01:58:03.261Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "74107c32-260d-4a08-ac28-db455e438678",
      "number": 5,
      "content": "Đấng ngự trên ngai cao cả,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.275Z",
      "updatedAt": "2025-04-24T01:58:03.275Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "c7d32b21-bc4e-468f-a552-80b62f944566",
      "number": 6,
      "content": "Lại tự hạ mình xuống,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.288Z",
      "updatedAt": "2025-04-24T01:58:03.288Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "2474f968-ba6f-48ad-aa8d-9cb9e899959c",
      "number": 6,
      "content": "Để đoái xem mọi vật trong bầu trời và trên mặt đất chăng?",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.302Z",
      "updatedAt": "2025-04-24T01:58:03.302Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "4056d2fd-3470-4f56-bd45-2693523ed318",
      "number": 7,
      "content": "Ngài nâng đỡ kẻ nghèo khổ ra khỏi chốn bụi tro,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.316Z",
      "updatedAt": "2025-04-24T01:58:03.316Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "bbc70a95-af66-43b2-8777-251d100e901f",
      "number": 7,
      "content": "Và nhấc đưa kẻ khó nghèo ra khỏi gò rác rến.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.329Z",
      "updatedAt": "2025-04-24T01:58:03.329Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "a421594d-479b-4ef8-95be-f7e4f605ce59",
      "number": 8,
      "content": "Ngài đặt họ ngồi ngang hàng với các bậc quan quyền,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.342Z",
      "updatedAt": "2025-04-24T01:58:03.342Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "b609e6e2-356c-4106-836b-5ef0c81b22a8",
      "number": 8,
      "content": "Tức với những người quyền thế của con dân Ngài.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.356Z",
      "updatedAt": "2025-04-24T01:58:03.356Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "6b6a68ed-36f0-44e5-a085-bcb0bddc65c5",
      "number": 9,
      "content": "Ngài làm cho người đàn bà son sẻ sống quạnh quẽ trong nhà",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.369Z",
      "updatedAt": "2025-04-24T01:58:03.369Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "a0d235c7-3bba-41d4-8f4b-5dddae817443",
      "number": 9,
      "content": "Thành người mẹ hiền vui vẻ của một đàn con.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.384Z",
      "updatedAt": "2025-04-24T01:58:03.384Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    },
    {
      "id": "763c4ec7-a8ad-42c9-8447-4bb8cbe41936",
      "number": 9,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:03.397Z",
      "updatedAt": "2025-04-24T01:58:03.397Z",
      "chapterId": "9695006a-c11f-45bb-92b3-08868627b6e1"
    }
  ],
