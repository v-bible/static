{
  "chapter": {
    "id": "712d9485-b930-4faf-a42a-20d80980fed2",
    "number": 61,
    "ref": "https://www.bible.com/bible/19/PSA.61.BD2011",
    "createdAt": "2025-04-22T10:10:26.273Z",
    "updatedAt": "2025-04-22T10:10:26.273Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "73e8cc30-65f4-4d77-96a4-f99aa518b584",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin nghe tiếng kêu cầu của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.784Z",
      "updatedAt": "2025-04-22T10:39:16.784Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "e8cc0bce-4b85-475b-b274-594b37ced915",
      "number": 1,
      "content": "Xin lắng nghe lời cầu nguyện của con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.832Z",
      "updatedAt": "2025-04-22T10:39:16.832Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "7b2335cb-a56a-42a0-8eb0-974e6596c26d",
      "number": 2,
      "content": "Từ nơi xa xôi ở tận cùng trái đất con kêu cầu Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.843Z",
      "updatedAt": "2025-04-22T10:39:16.843Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "bd250e3a-002d-4a8e-b240-e5cdae159052",
      "number": 2,
      "content": "Lòng con bị choáng ngợp vì biết bao nỗi lo sợ,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.855Z",
      "updatedAt": "2025-04-22T10:39:16.855Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "b838316f-5f33-4355-a6be-e0e19369deaa",
      "number": 2,
      "content": "Xin dẫn đưa con đến một vầng đá",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.871Z",
      "updatedAt": "2025-04-22T10:39:16.871Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "a39ade33-503c-43c1-adb7-2907b2d9536b",
      "number": 2,
      "content": "Cao lớn hơn con;",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.884Z",
      "updatedAt": "2025-04-22T10:39:16.884Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "c71542a9-61a5-4dbc-b9c0-dede970a1157",
      "number": 3,
      "content": "Vì Ngài là nơi ẩn náu của con,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.897Z",
      "updatedAt": "2025-04-22T10:39:16.897Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "a3c61de1-4a9d-4e7f-800e-c274dcb8a5c6",
      "number": 3,
      "content": "Một pháo tháp vững chắc để con đối phó với quân thù.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.911Z",
      "updatedAt": "2025-04-22T10:39:16.911Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "643f7119-b8bf-4c98-82d0-ea4e5118f655",
      "number": 4,
      "content": "Xin cho con ở trong nhà Ngài mãi mãi;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.927Z",
      "updatedAt": "2025-04-22T10:39:16.927Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "60d563c0-6c93-4985-a161-c8db2525d43c",
      "number": 4,
      "content": "Xin cho con nương náu dưới sự che chở của bóng cánh Ngài. (Sê-la)",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.940Z",
      "updatedAt": "2025-04-22T10:39:16.940Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "46137784-2b4e-4274-84e3-203a1fd57197",
      "number": 5,
      "content": "Đức Chúa Trời ôi, vì Ngài đã nghe lời hứa nguyện của con;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.954Z",
      "updatedAt": "2025-04-22T10:39:16.954Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "575a19d1-c52c-4f6d-ac95-6194f3ea1902",
      "number": 5,
      "content": "Ngài ban cho con cơ nghiệp dành cho những người kính sợ danh Ngài.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.967Z",
      "updatedAt": "2025-04-22T10:39:16.967Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "cae32a65-79f5-4022-b065-b431d0b94dad",
      "number": 6,
      "content": "Cầu xin Ngài gia tăng tuổi thọ cho vua;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.980Z",
      "updatedAt": "2025-04-22T10:39:16.980Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "79119a4d-a0d1-4222-95e6-b93f95e5cde6",
      "number": 6,
      "content": "Xin cho những năm của đời người kéo dài qua nhiều thế hệ.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:16.994Z",
      "updatedAt": "2025-04-22T10:39:16.994Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "e449dccc-f617-4467-91b6-7e0715c79c28",
      "number": 7,
      "content": "Nguyện người trị vì trước mặt Đức Chúa Trời mãi mãi;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:17.010Z",
      "updatedAt": "2025-04-22T10:39:17.010Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "c0cc8c38-18aa-403c-b041-0342db9cfaa3",
      "number": 7,
      "content": "Cầu xin Ngài ban tình thương và đức thành tín bảo vệ người.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:17.024Z",
      "updatedAt": "2025-04-22T10:39:17.024Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "3dc04c6a-6092-44c9-a266-735053e8f4b1",
      "number": 8,
      "content": "Như vậy con sẽ ca ngợi danh Ngài mãi mãi;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:17.040Z",
      "updatedAt": "2025-04-22T10:39:17.040Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    },
    {
      "id": "bef6edca-402a-436b-9a2d-46adff3f5291",
      "number": 8,
      "content": "Con sẽ giữ trọn những gì con đã hứa nguyện mỗi ngày.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:39:17.063Z",
      "updatedAt": "2025-04-22T10:39:17.063Z",
      "chapterId": "712d9485-b930-4faf-a42a-20d80980fed2"
    }
  ],
