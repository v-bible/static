{
  "chapter": {
    "id": "8ceb63d4-4782-4667-8a64-7c536c52b6b4",
    "number": 143,
    "ref": "https://www.bible.com/bible/19/PSA.143.BD2011",
    "createdAt": "2025-04-24T01:29:07.587Z",
    "updatedAt": "2025-04-24T01:29:07.587Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "387f21f8-ee00-4d99-b958-c7e650692e97",
      "number": 1,
      "content": "**CHÚA** ôi, xin lắng nghe lời cầu nguyện của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.361Z",
      "updatedAt": "2025-04-24T02:00:10.361Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "300e265e-fb92-495f-a4fd-0eee336dd1c5",
      "number": 1,
      "content": "Xin nghiêng tai nghe tiếng cầu xin của con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.390Z",
      "updatedAt": "2025-04-24T02:00:10.390Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "55cc45e4-6640-4d46-ae07-b5baaed1daf8",
      "number": 1,
      "content": "Xin lấy đức thành tín và đức công chính của Ngài nhậm lời con.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.404Z",
      "updatedAt": "2025-04-24T02:00:10.404Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "619263a1-d8d9-4f1e-a46e-762b44eb8628",
      "number": 2,
      "content": "Cầu xin Ngài đừng đem con, đầy tớ Ngài, ra xét đoán,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.418Z",
      "updatedAt": "2025-04-24T02:00:10.418Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "a155fa38-3ad7-4208-88a3-6e21529e4275",
      "number": 2,
      "content": "Vì trước mặt Ngài, chẳng người sống nào là ngay lành.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.429Z",
      "updatedAt": "2025-04-24T02:00:10.429Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "d6ad6a0f-d290-47c7-bd45-bcdeffcb8575",
      "number": 3,
      "content": "Kẻ thù của con đã truy đuổi con;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.443Z",
      "updatedAt": "2025-04-24T02:00:10.443Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "078cccdd-d53d-4f94-b1d6-fb167d8ce752",
      "number": 3,
      "content": "Nó giày đạp mạng sống con dưới đất;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.460Z",
      "updatedAt": "2025-04-24T02:00:10.460Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "9376f9d7-d413-4299-81aa-d58fe2f8dfbb",
      "number": 3,
      "content": "Nó làm cho con phải sống trong tối tăm, chẳng khác gì những kẻ đã chết từ lâu.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.476Z",
      "updatedAt": "2025-04-24T02:00:10.476Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "445ea52d-9df0-4e41-b3e3-6fab6771d28e",
      "number": 4,
      "content": "Bởi thế tâm linh con nao sờn trong con;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.486Z",
      "updatedAt": "2025-04-24T02:00:10.486Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "58513609-5c34-4b8b-8c92-bf0ac3183a9f",
      "number": 4,
      "content": "Lòng con tuyệt vọng trong con.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.502Z",
      "updatedAt": "2025-04-24T02:00:10.502Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "8101f779-a52b-48c5-841d-5625637388a1",
      "number": 5,
      "content": "Con nhớ lại những ngày xa xưa;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.516Z",
      "updatedAt": "2025-04-24T02:00:10.516Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "ea200200-66f3-4932-99d8-321c711424d6",
      "number": 5,
      "content": "Con suy gẫm mọi việc Ngài làm;",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.531Z",
      "updatedAt": "2025-04-24T02:00:10.531Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "36eb4d33-383a-4a43-a8a7-b67f18b11160",
      "number": 5,
      "content": "Con chiêm nghiệm những việc tay Ngài thực hiện.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.542Z",
      "updatedAt": "2025-04-24T02:00:10.542Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "e3cecca1-3bdc-410e-9ad0-3096fc433952",
      "number": 6,
      "content": "Con đưa hai tay lên hướng về Ngài cầu nguyện;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.556Z",
      "updatedAt": "2025-04-24T02:00:10.556Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "37da0aa5-0313-4c7e-9ec2-d0a2458d3eba",
      "number": 6,
      "content": "Linh hồn con khao khát Ngài như đất khô khao khát nước. (Sê-la)",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.566Z",
      "updatedAt": "2025-04-24T02:00:10.566Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "2d80bc0e-42a7-4290-b96c-e84e83837e28",
      "number": 7,
      "content": "Xin mau đáp lời con, **CHÚA** ôi,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.577Z",
      "updatedAt": "2025-04-24T02:00:10.577Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "2aafb7bc-514b-4e6e-980f-18614105289d",
      "number": 7,
      "content": "Vì tâm linh con đang sờn ngã.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.590Z",
      "updatedAt": "2025-04-24T02:00:10.590Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "ec40b4e2-e985-4c0a-bf5e-88a98fe3a200",
      "number": 7,
      "content": "Xin đừng ẩn mặt Ngài khỏi con,",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.600Z",
      "updatedAt": "2025-04-24T02:00:10.600Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "c0140b8b-c89d-4cac-b1e3-f1da966c3cc3",
      "number": 7,
      "content": "Kẻo con như những kẻ sắp xuống huyệt mả chăng.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.610Z",
      "updatedAt": "2025-04-24T02:00:10.610Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "ce04b734-c324-4ab4-965e-a62f9804fc9e",
      "number": 8,
      "content": "Xin cho con nghe tình thương của Ngài khi bình minh ló dạng,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.624Z",
      "updatedAt": "2025-04-24T02:00:10.624Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "06857b2b-882a-4faa-9730-8e5e119c5a8a",
      "number": 8,
      "content": "Vì con tin cậy nơi Ngài;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.638Z",
      "updatedAt": "2025-04-24T02:00:10.638Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "949cc609-dd05-47e5-be1a-cd53f80b34d1",
      "number": 8,
      "content": "Xin dạy con đường lối con nên đi,",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.653Z",
      "updatedAt": "2025-04-24T02:00:10.653Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "983931a6-489a-46db-9504-567e9ff8211d",
      "number": 8,
      "content": "Vì linh hồn con ngưỡng vọng Ngài.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.668Z",
      "updatedAt": "2025-04-24T02:00:10.668Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "a75fbf7d-7460-4d93-b503-c25b15676aa9",
      "number": 9,
      "content": "**CHÚA** ôi, xin giải cứu con khỏi những kẻ thù ghét con,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.682Z",
      "updatedAt": "2025-04-24T02:00:10.682Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "c5831ca3-2d90-48c7-b255-f0f1a0a594c4",
      "number": 9,
      "content": "Vì con nương náu mình nơi Ngài.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.695Z",
      "updatedAt": "2025-04-24T02:00:10.695Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "e7e49288-7cb7-41cb-9f2f-9b1730bf3ea8",
      "number": 10,
      "content": "Xin dạy con làm theo ý Ngài,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.711Z",
      "updatedAt": "2025-04-24T02:00:10.711Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "356cc193-e969-4132-992f-0e9721f2967c",
      "number": 10,
      "content": "Vì Ngài là Đức Chúa Trời của con;",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.728Z",
      "updatedAt": "2025-04-24T02:00:10.728Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "c2be23f7-1c4c-4d08-80ed-f54c633d4081",
      "number": 10,
      "content": "Nguyện Thần Linh thánh thiện của Ngài",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.743Z",
      "updatedAt": "2025-04-24T02:00:10.743Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "7a6de2f8-de87-4a29-bbec-fd7f5791cc23",
      "number": 10,
      "content": "Dẫn con vào đất phẳng bằng.",
      "order": 3,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.753Z",
      "updatedAt": "2025-04-24T02:00:10.753Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "b73dd565-bdd5-4ef6-8cb4-84ac84477365",
      "number": 11,
      "content": "Vì cớ danh Ngài, **CHÚA** ôi, xin phục hồi sinh lực cho con.",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.765Z",
      "updatedAt": "2025-04-24T02:00:10.765Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "9faf6a7d-6c27-44c2-8bc7-d29087c88d8b",
      "number": 11,
      "content": "Vì đức công chính của Ngài, xin đem linh hồn con ra khỏi cảnh gian truân.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.776Z",
      "updatedAt": "2025-04-24T02:00:10.776Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "d082e307-0cfa-4e9d-97f7-c58d4ec17ad3",
      "number": 12,
      "content": "Vì lòng thương xót của Ngài, xin tiêu diệt những kẻ thù của con,",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.791Z",
      "updatedAt": "2025-04-24T02:00:10.791Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "277882d1-57e8-4416-958e-2562a9600054",
      "number": 12,
      "content": "Và diệt mất tất cả những kẻ làm khổ linh hồn con,",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.804Z",
      "updatedAt": "2025-04-24T02:00:10.804Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    },
    {
      "id": "9e0f9d46-236c-4f5c-ac4b-4475e3ffc23b",
      "number": 12,
      "content": "Vì con là tôi tớ Ngài.",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:10.818Z",
      "updatedAt": "2025-04-24T02:00:10.818Z",
      "chapterId": "8ceb63d4-4782-4667-8a64-7c536c52b6b4"
    }
  ],
