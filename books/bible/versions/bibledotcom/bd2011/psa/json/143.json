{
  "chapter": {
    "id": "2cf72a0a-4ac9-4060-90b6-b267142d14f0",
    "number": 143,
    "ref": "https://www.bible.com/bible/19/PSA.143.BD2011",
    "createdAt": "2024-09-23T05:08:40.693Z",
    "updatedAt": "2024-09-23T05:08:40.693Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "3f6bcf8b-2100-4843-ae59-fc02d32956f5",
      "number": 1,
      "content": "**CHÚA** ôi, xin lắng nghe lời cầu nguyện của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.718Z",
      "updatedAt": "2024-09-23T05:43:30.718Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "927841a9-4784-4ec1-aa38-051812880856",
      "number": 1,
      "content": "Xin nghiêng tai nghe tiếng cầu xin của con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.739Z",
      "updatedAt": "2024-09-23T05:43:30.739Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "40120bea-76ef-4b1c-97fd-d6c5242f0248",
      "number": 1,
      "content": "Xin lấy đức thành tín và đức công chính của Ngài nhậm lời con.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.741Z",
      "updatedAt": "2024-09-23T05:43:30.741Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "3f57c275-156c-4181-aa78-df8b3aba1197",
      "number": 2,
      "content": "Cầu xin Ngài đừng đem con, đầy tớ Ngài, ra xét đoán,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.743Z",
      "updatedAt": "2024-09-23T05:43:30.743Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "70d807ce-b857-4f36-afc3-24b176f279d7",
      "number": 2,
      "content": "Vì trước mặt Ngài, chẳng người sống nào là ngay lành.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.745Z",
      "updatedAt": "2024-09-23T05:43:30.745Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "1d26283b-73c6-4dcc-9506-bf25bd6eea04",
      "number": 3,
      "content": "Kẻ thù của con đã truy đuổi con;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.747Z",
      "updatedAt": "2024-09-23T05:43:30.747Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "db3a393c-6ccd-4402-af8c-003c74170ffa",
      "number": 3,
      "content": "Nó giày đạp mạng sống con dưới đất;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.749Z",
      "updatedAt": "2024-09-23T05:43:30.749Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "512f9ef0-7d38-4fa6-8f88-55e366a51384",
      "number": 3,
      "content": "Nó làm cho con phải sống trong tối tăm, chẳng khác gì những kẻ đã chết từ lâu.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.753Z",
      "updatedAt": "2024-09-23T05:43:30.753Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "04e9dfea-e8b9-4ded-870a-6cb6e4f0c55a",
      "number": 4,
      "content": "Bởi thế tâm linh con nao sờn trong con;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.757Z",
      "updatedAt": "2024-09-23T05:43:30.757Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "e22bbfa9-fb92-43c4-8d6c-402706851b1c",
      "number": 4,
      "content": "Lòng con tuyệt vọng trong con.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.762Z",
      "updatedAt": "2024-09-23T05:43:30.762Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "ae7818cd-3ab7-4bc0-8813-18dbfbb965ae",
      "number": 5,
      "content": "Con nhớ lại những ngày xa xưa;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.767Z",
      "updatedAt": "2024-09-23T05:43:30.767Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "11ff6667-0cc3-418a-b149-5092a3732ca0",
      "number": 5,
      "content": "Con suy gẫm mọi việc Ngài làm;",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.772Z",
      "updatedAt": "2024-09-23T05:43:30.772Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "c0367df9-b8b8-45eb-8d40-0b9870af0d5a",
      "number": 5,
      "content": "Con chiêm nghiệm những việc tay Ngài thực hiện.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.776Z",
      "updatedAt": "2024-09-23T05:43:30.776Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "a1268870-5695-4186-8b25-1c93f9e3dbc5",
      "number": 6,
      "content": "Con đưa hai tay lên hướng về Ngài cầu nguyện;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.781Z",
      "updatedAt": "2024-09-23T05:43:30.781Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "72bf1302-75c1-4494-a99e-bb8d31f2bf75",
      "number": 6,
      "content": "Linh hồn con khao khát Ngài như đất khô khao khát nước. (Sê-la)",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.786Z",
      "updatedAt": "2024-09-23T05:43:30.786Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "e135058c-b4f2-4cfa-bd82-d9bb26cec9e8",
      "number": 7,
      "content": "Xin mau đáp lời con, **CHÚA** ôi,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.791Z",
      "updatedAt": "2024-09-23T05:43:30.791Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "58d43af8-b63d-4336-96bf-0279d42d7718",
      "number": 7,
      "content": "Vì tâm linh con đang sờn ngã.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.796Z",
      "updatedAt": "2024-09-23T05:43:30.796Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "bd5ae935-a387-4f05-965b-50a424c688b4",
      "number": 7,
      "content": "Xin đừng ẩn mặt Ngài khỏi con,",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.801Z",
      "updatedAt": "2024-09-23T05:43:30.801Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "ebe3adc1-370e-47f6-980e-163803b3edf5",
      "number": 7,
      "content": "Kẻo con như những kẻ sắp xuống huyệt mả chăng.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.809Z",
      "updatedAt": "2024-09-23T05:43:30.809Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "708e9ee9-883c-4c93-beb4-672d7b5cf11d",
      "number": 8,
      "content": "Xin cho con nghe tình thương của Ngài khi bình minh ló dạng,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.817Z",
      "updatedAt": "2024-09-23T05:43:30.817Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "111bd911-e7a5-48e9-a7c9-3a6897798d37",
      "number": 8,
      "content": "Vì con tin cậy nơi Ngài;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.822Z",
      "updatedAt": "2024-09-23T05:43:30.822Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "c1a10118-6dab-4cb0-8bab-2c0dc369bd41",
      "number": 8,
      "content": "Xin dạy con đường lối con nên đi,",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.827Z",
      "updatedAt": "2024-09-23T05:43:30.827Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "d5482b3d-142b-4c34-a634-129472b50ce2",
      "number": 8,
      "content": "Vì linh hồn con ngưỡng vọng Ngài.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.832Z",
      "updatedAt": "2024-09-23T05:43:30.832Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "e1e9a4d8-f82e-4903-ab8d-3f0f87087872",
      "number": 9,
      "content": "**CHÚA** ôi, xin giải cứu con khỏi những kẻ thù ghét con,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.836Z",
      "updatedAt": "2024-09-23T05:43:30.836Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "61be23a5-9c32-4c35-ad4c-1cdb98508dbd",
      "number": 9,
      "content": "Vì con nương náu mình nơi Ngài.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.841Z",
      "updatedAt": "2024-09-23T05:43:30.841Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "c70b6213-003c-4479-8c9d-66aabe328842",
      "number": 10,
      "content": "Xin dạy con làm theo ý Ngài,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.845Z",
      "updatedAt": "2024-09-23T05:43:30.845Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "dce86cd4-c8b3-4959-9ec6-9ec97448d9b4",
      "number": 10,
      "content": "Vì Ngài là Đức Chúa Trời của con;",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.850Z",
      "updatedAt": "2024-09-23T05:43:30.850Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "87b40ca2-d2da-4d56-9254-25ad79dfa594",
      "number": 10,
      "content": "Nguyện Thần Linh thánh thiện của Ngài",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.855Z",
      "updatedAt": "2024-09-23T05:43:30.855Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "81c924d0-a04a-463c-bb72-0035dcb8eb2d",
      "number": 10,
      "content": "Dẫn con vào đất phẳng bằng.",
      "order": 3,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.860Z",
      "updatedAt": "2024-09-23T05:43:30.860Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "8149d052-8bb4-428d-b4fd-ad244b47eaeb",
      "number": 11,
      "content": "Vì cớ danh Ngài, **CHÚA** ôi, xin phục hồi sinh lực cho con.",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.865Z",
      "updatedAt": "2024-09-23T05:43:30.865Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "4c853a9a-e0a7-44f8-ad94-6adc364349ea",
      "number": 11,
      "content": "Vì đức công chính của Ngài, xin đem linh hồn con ra khỏi cảnh gian truân.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.870Z",
      "updatedAt": "2024-09-23T05:43:30.870Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "bf695bc2-1eac-4799-8f62-97f63bbaa2cf",
      "number": 12,
      "content": "Vì lòng thương xót của Ngài, xin tiêu diệt những kẻ thù của con,",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.875Z",
      "updatedAt": "2024-09-23T05:43:30.875Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "b2116a08-1b1d-4de7-9a6a-279f0041343e",
      "number": 12,
      "content": "Và diệt mất tất cả những kẻ làm khổ linh hồn con,",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.881Z",
      "updatedAt": "2024-09-23T05:43:30.881Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    },
    {
      "id": "f970bae3-0587-4f46-b7bd-83128ffb10e9",
      "number": 12,
      "content": "Vì con là tôi tớ Ngài.",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:43:30.886Z",
      "updatedAt": "2024-09-23T05:43:30.886Z",
      "chapterId": "2cf72a0a-4ac9-4060-90b6-b267142d14f0"
    }
  ],
