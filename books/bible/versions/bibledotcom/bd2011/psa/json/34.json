    },
    {
      "id": "11a030c2-db3e-47fe-bdd6-23743f92eb62",
      "number": 22,
      "content": "**CHÚA** cứu chuộc mạng sống các tôi tớ Ngài;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:07.187Z",
      "updatedAt": "2025-04-24T01:53:07.187Z",
