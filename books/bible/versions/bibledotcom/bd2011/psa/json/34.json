    },
    {
      "id": "39085a02-c21e-450a-9d10-843a212edd68",
      "number": 22,
      "content": "Không người nào nương cậy Ngài sẽ bị kết tội.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
