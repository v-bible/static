    },
    {
      "id": "c4e43c32-79e8-463d-a1d0-6dea724c3334",
      "number": 21,
      "content": "Và những kẻ ghét người ngay lành sẽ bị kết tội.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
