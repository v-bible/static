    {
      "id": "df489cab-32e7-4915-b6a4-80bc8f5fb1ff",
      "number": 21,
      "content": "Điều ác sẽ giết kẻ ác,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:53:07.158Z",
