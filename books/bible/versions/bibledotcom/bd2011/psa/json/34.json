    },
    {
      "id": "2e51c057-4c41-430d-8424-6edaf9aa6144",
      "number": 21,
      "content": "Điều ác sẽ giết kẻ ác,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:47.354Z",
      "updatedAt": "2024-09-23T05:46:47.354Z",
      "chapterId": "a3ab84bf-b060-458c-b33d-4404c93a52ff"
    },
    {
      "id": "44817a66-d3b8-40ac-8eaf-9480a4444c9b",
      "number": 21,
      "content": "Và những kẻ ghét người ngay lành sẽ bị kết tội.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:47.358Z",
      "updatedAt": "2024-09-23T05:46:47.358Z",
      "chapterId": "a3ab84bf-b060-458c-b33d-4404c93a52ff"
    },
    {
      "id": "6b905597-0572-436c-9cfd-000fdb6b6dc0",
      "number": 22,
      "content": "**CHÚA** cứu chuộc mạng sống các tôi tớ Ngài;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:47.363Z",
      "updatedAt": "2024-09-23T05:46:47.363Z",
      "chapterId": "a3ab84bf-b060-458c-b33d-4404c93a52ff"
    },
    {
      "id": "9c88824b-3feb-48c1-9424-1f298d3c969a",
      "number": 22,
      "content": "Không người nào nương cậy Ngài sẽ bị kết tội.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:47.367Z",
      "updatedAt": "2024-09-23T05:46:47.367Z",
      "chapterId": "a3ab84bf-b060-458c-b33d-4404c93a52ff"
    }
  ],
