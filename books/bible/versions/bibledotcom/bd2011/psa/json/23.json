{
  "chapter": {
    "id": "7393c6a9-1a83-45c5-9cef-e75bce615b73",
    "number": 23,
    "ref": "https://www.bible.com/bible/19/PSA.23.BD2011",
    "createdAt": "2025-04-24T01:29:06.059Z",
    "updatedAt": "2025-04-24T01:29:06.059Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "ae19d28c-edfe-44a6-8dca-8ea191459a40",
      "number": 1,
      "content": "**CHÚA** là Đấng chăn giữ tôi, tôi sẽ chẳng thiếu thốn gì.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.557Z",
      "updatedAt": "2025-04-24T01:52:17.557Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "b7773847-6829-4bce-a773-60e2f40994a2",
      "number": 2,
      "content": "Ngài cho tôi nằm nghỉ trong đồng cỏ xanh tươi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.590Z",
      "updatedAt": "2025-04-24T01:52:17.590Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "0e733cfe-f149-4350-af8f-16a6f65cb5d8",
      "number": 2,
      "content": "Ngài dẫn tôi đến mé nước bình tịnh.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.600Z",
      "updatedAt": "2025-04-24T01:52:17.600Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "7c22562f-f9ef-4dd5-aa10-d2d555ae3ca7",
      "number": 3,
      "content": "Ngài phục hồi sức lực cho linh hồn tôi;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.612Z",
      "updatedAt": "2025-04-24T01:52:17.612Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "41a89f27-55ef-4bc0-801a-797a67e7e70a",
      "number": 3,
      "content": "Ngài dẫn tôi vào các lối công chính,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.624Z",
      "updatedAt": "2025-04-24T01:52:17.624Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "f25266e3-b0ed-4b47-9f2d-2febb3be357b",
      "number": 3,
      "content": "Vì cớ danh Ngài.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.636Z",
      "updatedAt": "2025-04-24T01:52:17.636Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "9d71c60e-34ec-4e36-a223-47def36bfebe",
      "number": 4,
      "content": "Thật vậy, dù khi con đi qua thung lũng rợp bóng tử thần,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.649Z",
      "updatedAt": "2025-04-24T01:52:17.649Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "32f615f3-a2d5-4291-9c97-04a30f2b318f",
      "number": 4,
      "content": "Con sẽ chẳng sợ tai họa nào,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.662Z",
      "updatedAt": "2025-04-24T01:52:17.662Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "5f401fe2-e501-4938-97b9-1f790ce12038",
      "number": 4,
      "content": "Vì Ngài ở với con;",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.674Z",
      "updatedAt": "2025-04-24T01:52:17.674Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "88cf4136-3ce8-42c9-ba3c-7e3b16641d9a",
      "number": 4,
      "content": "Cây trượng và cây gậy của Ngài",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.688Z",
      "updatedAt": "2025-04-24T01:52:17.688Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "987ee2dd-14f2-4828-9158-f95474c1d46c",
      "number": 4,
      "content": "An ủi con.",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.702Z",
      "updatedAt": "2025-04-24T01:52:17.702Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "6a6492d8-2028-4bdd-8f52-062ac39fa7c4",
      "number": 5,
      "content": "Ngài dọn bàn cho con",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.715Z",
      "updatedAt": "2025-04-24T01:52:17.715Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "d626fc0b-86a7-4632-ac4e-d8c7718344dd",
      "number": 5,
      "content": "Trước mặt những kẻ thù của con;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.734Z",
      "updatedAt": "2025-04-24T01:52:17.734Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "2df21488-0805-453b-a4a5-e47fdf48066d",
      "number": 5,
      "content": "Ngài xức dầu cho đầu con;",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.748Z",
      "updatedAt": "2025-04-24T01:52:17.748Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "5f5a7dd2-f59f-4d98-9f55-0d9a5261c2d2",
      "number": 5,
      "content": "Chén con đầy tràn.",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.762Z",
      "updatedAt": "2025-04-24T01:52:17.762Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "ed545e5d-9fae-4345-9582-03eab24cd71c",
      "number": 6,
      "content": "Quả thật, trọn đời tôi,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.775Z",
      "updatedAt": "2025-04-24T01:52:17.775Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "a7360cbd-6a65-421a-a2e0-e0045e593a93",
      "number": 6,
      "content": "Phước hạnh và ơn thương xót sẽ theo tôi;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.789Z",
      "updatedAt": "2025-04-24T01:52:17.789Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "99a79100-dd72-4f05-8837-23c2853b2bd2",
      "number": 6,
      "content": "Tôi sẽ ở trong nhà của **CHÚA**",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.803Z",
      "updatedAt": "2025-04-24T01:52:17.803Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    },
    {
      "id": "d00b17fe-8b3e-44c7-8e77-ba8186455e7d",
      "number": 6,
      "content": "Cho đến đời đời.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:52:17.820Z",
      "updatedAt": "2025-04-24T01:52:17.820Z",
      "chapterId": "7393c6a9-1a83-45c5-9cef-e75bce615b73"
    }
  ],
