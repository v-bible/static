{
  "chapter": {
    "id": "2c63afc0-af36-47a9-92f0-cb8653fd66e8",
    "number": 23,
    "ref": "https://www.bible.com/bible/19/PSA.23.BD2011",
    "createdAt": "2025-04-22T10:10:25.847Z",
    "updatedAt": "2025-04-22T10:10:25.847Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "445aa28a-8726-4295-bf26-fcec401cc887",
      "number": 1,
      "content": "**CHÚA** là Đấng chăn giữ tôi, tôi sẽ chẳng thiếu thốn gì.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.517Z",
      "updatedAt": "2025-04-22T10:36:23.517Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "126419ca-cb0b-446a-ae65-3f060b3199b4",
      "number": 2,
      "content": "Ngài cho tôi nằm nghỉ trong đồng cỏ xanh tươi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.585Z",
      "updatedAt": "2025-04-22T10:36:23.585Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "d1636b34-b180-43d2-bb34-b67f16ccc28a",
      "number": 2,
      "content": "Ngài dẫn tôi đến mé nước bình tịnh.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.602Z",
      "updatedAt": "2025-04-22T10:36:23.602Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "1b438989-ca15-4e37-b4bd-3ddaaf0496b2",
      "number": 3,
      "content": "Ngài phục hồi sức lực cho linh hồn tôi;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.619Z",
      "updatedAt": "2025-04-22T10:36:23.619Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "d7d4424f-3361-46a7-8c74-f5fe5df0e4a9",
      "number": 3,
      "content": "Ngài dẫn tôi vào các lối công chính,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.634Z",
      "updatedAt": "2025-04-22T10:36:23.634Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "becce9b4-20dc-4fe2-b643-63c91651f5bf",
      "number": 3,
      "content": "Vì cớ danh Ngài.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.645Z",
      "updatedAt": "2025-04-22T10:36:23.645Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "22b59388-e3c0-4f2e-8cbd-4a3deb577f00",
      "number": 4,
      "content": "Thật vậy, dù khi con đi qua thung lũng rợp bóng tử thần,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.656Z",
      "updatedAt": "2025-04-22T10:36:23.656Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "739cec37-4c1a-4535-ab58-8d106b9d8fcd",
      "number": 4,
      "content": "Con sẽ chẳng sợ tai họa nào,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.666Z",
      "updatedAt": "2025-04-22T10:36:23.666Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "efaf4cbc-d255-4542-99b5-6f3c7527450d",
      "number": 4,
      "content": "Vì Ngài ở với con;",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.677Z",
      "updatedAt": "2025-04-22T10:36:23.677Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "1da024d9-9961-4651-abec-2ca294fe3d1e",
      "number": 4,
      "content": "Cây trượng và cây gậy của Ngài",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.687Z",
      "updatedAt": "2025-04-22T10:36:23.687Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "8c6e31d5-c1f4-475d-8e7e-30d279c38600",
      "number": 4,
      "content": "An ủi con.",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.697Z",
      "updatedAt": "2025-04-22T10:36:23.697Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "f202fcf0-9de6-4ee5-a98d-284f54653ab8",
      "number": 5,
      "content": "Ngài dọn bàn cho con",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.712Z",
      "updatedAt": "2025-04-22T10:36:23.712Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "6cf0bbb5-ef41-4b85-8f09-b1553b8213da",
      "number": 5,
      "content": "Trước mặt những kẻ thù của con;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.728Z",
      "updatedAt": "2025-04-22T10:36:23.728Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "9b9d6d56-3efc-4a4d-8ae0-e4524930989d",
      "number": 5,
      "content": "Ngài xức dầu cho đầu con;",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.742Z",
      "updatedAt": "2025-04-22T10:36:23.742Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "a264cc08-9403-4ea8-b1d0-4c4a8f226d55",
      "number": 5,
      "content": "Chén con đầy tràn.",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.759Z",
      "updatedAt": "2025-04-22T10:36:23.759Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "ccd0f3de-f416-4045-a6db-233ba9430ad0",
      "number": 6,
      "content": "Quả thật, trọn đời tôi,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.773Z",
      "updatedAt": "2025-04-22T10:36:23.773Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "586439e9-20e1-413f-8239-3494f634f56e",
      "number": 6,
      "content": "Phước hạnh và ơn thương xót sẽ theo tôi;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.787Z",
      "updatedAt": "2025-04-22T10:36:23.787Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "bfa1d9d8-85ac-4fa8-97c6-876512a5a862",
      "number": 6,
      "content": "Tôi sẽ ở trong nhà của **CHÚA**",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.801Z",
      "updatedAt": "2025-04-22T10:36:23.801Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    },
    {
      "id": "4780441e-3acc-4d25-91ad-91cc49867605",
      "number": 6,
      "content": "Cho đến đời đời.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:36:23.814Z",
      "updatedAt": "2025-04-22T10:36:23.814Z",
      "chapterId": "2c63afc0-af36-47a9-92f0-cb8653fd66e8"
    }
  ],
