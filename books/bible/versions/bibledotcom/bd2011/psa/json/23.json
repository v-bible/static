{
  "chapter": {
    "id": "14f0950c-d82a-475d-8d85-60f283a8f9de",
    "number": 23,
    "ref": "https://www.bible.com/bible/19/PSA.23.BD2011",
    "createdAt": "2024-09-23T05:08:40.283Z",
    "updatedAt": "2024-09-23T05:08:40.283Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "a55ae96b-7349-467a-a45c-bcecff8a0a99",
      "number": 1,
      "content": "**CHÚA** là Đấng chăn giữ tôi, tôi sẽ chẳng thiếu thốn gì.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.161Z",
      "updatedAt": "2024-09-23T05:46:01.161Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "515f711e-a7cd-4cc5-b848-336a926c9bba",
      "number": 2,
      "content": "Ngài cho tôi nằm nghỉ trong đồng cỏ xanh tươi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.178Z",
      "updatedAt": "2024-09-23T05:46:01.178Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "b8e886b0-da76-4385-ae1e-2c4c393729a7",
      "number": 2,
      "content": "Ngài dẫn tôi đến mé nước bình tịnh.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.180Z",
      "updatedAt": "2024-09-23T05:46:01.180Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "289e1ce6-e2cd-471a-961e-6b8ab1fcb532",
      "number": 3,
      "content": "Ngài phục hồi sức lực cho linh hồn tôi;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.182Z",
      "updatedAt": "2024-09-23T05:46:01.182Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "f506a04a-51be-4fe4-b792-0b727b72e406",
      "number": 3,
      "content": "Ngài dẫn tôi vào các lối công chính,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.183Z",
      "updatedAt": "2024-09-23T05:46:01.183Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "7936a47e-c98f-4a81-930a-55458bf40503",
      "number": 3,
      "content": "Vì cớ danh Ngài.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.185Z",
      "updatedAt": "2024-09-23T05:46:01.185Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "24e682c9-8e8f-4114-91cc-8994df762a99",
      "number": 4,
      "content": "Thật vậy, dù khi con đi qua thung lũng rợp bóng tử thần,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.188Z",
      "updatedAt": "2024-09-23T05:46:01.188Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "9d08a3be-4aa4-4616-ab98-9862c14f8953",
      "number": 4,
      "content": "Con sẽ chẳng sợ tai họa nào,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.193Z",
      "updatedAt": "2024-09-23T05:46:01.193Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "ceb6f01c-c666-4600-9f8d-c807c0659f8b",
      "number": 4,
      "content": "Vì Ngài ở với con;",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.198Z",
      "updatedAt": "2024-09-23T05:46:01.198Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "885ab095-bf09-49e1-8c6d-bcba2844cf6a",
      "number": 4,
      "content": "Cây trượng và cây gậy của Ngài",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.203Z",
      "updatedAt": "2024-09-23T05:46:01.203Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "c61754f4-6888-4374-a428-895805b7a7e4",
      "number": 4,
      "content": "An ủi con.",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.208Z",
      "updatedAt": "2024-09-23T05:46:01.208Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "f05b3938-234f-4ab4-8033-865f45148e18",
      "number": 5,
      "content": "Ngài dọn bàn cho con",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.213Z",
      "updatedAt": "2024-09-23T05:46:01.213Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "c402224e-ecc8-46c8-9b26-0cec2ecbb7fc",
      "number": 5,
      "content": "Trước mặt những kẻ thù của con;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.218Z",
      "updatedAt": "2024-09-23T05:46:01.218Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "960ae8f4-58b5-44ae-a27c-ce85ea20a217",
      "number": 5,
      "content": "Ngài xức dầu cho đầu con;",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.222Z",
      "updatedAt": "2024-09-23T05:46:01.222Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "7531078c-4e8e-4058-98ea-23d7a1670abf",
      "number": 5,
      "content": "Chén con đầy tràn.",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.227Z",
      "updatedAt": "2024-09-23T05:46:01.227Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "1c6bc96f-d961-4f8c-95b1-33a5d5048917",
      "number": 6,
      "content": "Quả thật, trọn đời tôi,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.232Z",
      "updatedAt": "2024-09-23T05:46:01.232Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "cda5d9c1-4779-4e8a-bf7c-e4f6df37d8bb",
      "number": 6,
      "content": "Phước hạnh và ơn thương xót sẽ theo tôi;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.237Z",
      "updatedAt": "2024-09-23T05:46:01.237Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "26c0a137-4aa9-463b-a8cb-0fcd90907f44",
      "number": 6,
      "content": "Tôi sẽ ở trong nhà của **CHÚA**",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.242Z",
      "updatedAt": "2024-09-23T05:46:01.242Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    },
    {
      "id": "1ec20370-1653-4648-bc19-4553b2beb24e",
      "number": 6,
      "content": "Cho đến đời đời.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:46:01.247Z",
      "updatedAt": "2024-09-23T05:46:01.247Z",
      "chapterId": "14f0950c-d82a-475d-8d85-60f283a8f9de"
    }
  ],
