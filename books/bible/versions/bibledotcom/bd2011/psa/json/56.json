{
  "chapter": {
    "id": "9801b465-da46-46f6-b06a-14d62ece4e32",
    "number": 56,
    "ref": "https://www.bible.com/bible/19/PSA.56.BD2011",
    "createdAt": "2025-04-24T01:29:06.462Z",
    "updatedAt": "2025-04-24T01:29:06.462Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "6ca231eb-0945-4c22-9829-7e44e395ae24",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin thương xót con, vì người ta chà đạp con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.215Z",
      "updatedAt": "2025-04-24T01:54:30.215Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "e9010be1-ad8d-455d-975b-c08c2356f06d",
      "number": 1,
      "content": "Hằng ngày họ cứ gây sự và hà hiếp con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.249Z",
      "updatedAt": "2025-04-24T01:54:30.249Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "be8921fe-5a50-4ab5-8c37-0dfbb8cbce28",
      "number": 2,
      "content": "Quân thù con chà đạp con suốt ngày;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.262Z",
      "updatedAt": "2025-04-24T01:54:30.262Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "dae07a03-34db-4dee-a74f-566496d48547",
      "number": 2,
      "content": "Nhiều kẻ tấn công con thay, ôi Đấng Chí Cao.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.277Z",
      "updatedAt": "2025-04-24T01:54:30.277Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "f7a1c604-a440-4744-8914-8f998a690cc7",
      "number": 3,
      "content": "Mỗi khi con lo sợ,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.290Z",
      "updatedAt": "2025-04-24T01:54:30.290Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "305bf8fc-afec-49a0-8310-0f0200a0e60e",
      "number": 3,
      "content": "Con đặt trọn lòng tin cậy nơi Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.303Z",
      "updatedAt": "2025-04-24T01:54:30.303Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "525d45eb-cd28-499a-923f-e2d071054e70",
      "number": 4,
      "content": "Trong Đức Chúa Trời, tôi ca ngợi lời Ngài;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.317Z",
      "updatedAt": "2025-04-24T01:54:30.317Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "9e2277c5-991d-4383-8bc8-f3b0c10ad84f",
      "number": 4,
      "content": "Nơi Đức Chúa Trời, tôi đặt trọn niềm tin; tôi sẽ không sợ hãi;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.332Z",
      "updatedAt": "2025-04-24T01:54:30.332Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "52fd2d6f-6e91-473a-ab85-29903b91a054",
      "number": 4,
      "content": "Loài người làm chi tôi được?",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.345Z",
      "updatedAt": "2025-04-24T01:54:30.345Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "470738ef-f6b4-4bd6-b234-9199d5d9654e",
      "number": 5,
      "content": "Suốt ngày họ cứ bóp méo lời con;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.359Z",
      "updatedAt": "2025-04-24T01:54:30.359Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "4443e73d-e203-4e24-8386-90fa91b6a341",
      "number": 5,
      "content": "Họ luôn luôn nghĩ cách hại con.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.372Z",
      "updatedAt": "2025-04-24T01:54:30.372Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "3de326ea-fa53-4527-8214-e4f2126e840f",
      "number": 6,
      "content": "Họ tạo sự bài bác;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.386Z",
      "updatedAt": "2025-04-24T01:54:30.386Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "283ee120-52de-48fc-8c45-5b92a3dfbdb9",
      "number": 6,
      "content": "Họ ngấm ngầm phá hoại;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.400Z",
      "updatedAt": "2025-04-24T01:54:30.400Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "6354a053-ad1e-4e65-ba93-fe3e268e059d",
      "number": 6,
      "content": "Họ rình rập các bước con hầu chờ dịp lấy mạng con.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.414Z",
      "updatedAt": "2025-04-24T01:54:30.414Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "4ccb2faf-010c-4c5b-88df-7a84b483e82c",
      "number": 7,
      "content": "Họ có thể nhờ gian ác mình mà tránh khỏi bị phạt sao?",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.428Z",
      "updatedAt": "2025-04-24T01:54:30.428Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "b7cbab51-4e08-4856-8bd1-0ec3bf30817d",
      "number": 7,
      "content": "Đức Chúa Trời ôi, xin nổi thịnh nộ lên và trừng phạt họ.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.442Z",
      "updatedAt": "2025-04-24T01:54:30.442Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "66ceaaaa-6c97-438c-8474-80e4e7891200",
      "number": 8,
      "content": "Ngài đã dõi theo những bước lang thang trốn tránh của con;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.455Z",
      "updatedAt": "2025-04-24T01:54:30.455Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "42310635-84d9-4561-b368-6b3122fd81d5",
      "number": 8,
      "content": "Xin để nước mắt con trong ve của Ngài;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.469Z",
      "updatedAt": "2025-04-24T01:54:30.469Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "c65e6fba-cd8a-454d-a3f2-6f8dd6a4a866",
      "number": 8,
      "content": "Nước mắt con chẳng đã được ghi vào sách của Ngài sao?",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.482Z",
      "updatedAt": "2025-04-24T01:54:30.482Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "68962eac-7a8d-4061-ab41-98490cf77312",
      "number": 9,
      "content": "Những kẻ thù của con sẽ bỏ đi",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.496Z",
      "updatedAt": "2025-04-24T01:54:30.496Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "0f0b0360-b235-4544-911a-29dad42357ae",
      "number": 9,
      "content": "Trong ngày con kêu cầu;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.509Z",
      "updatedAt": "2025-04-24T01:54:30.509Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "a0c9223e-a8e5-4eba-869b-e332957b5dfe",
      "number": 9,
      "content": "Con biết rõ điều đó, vì Đức Chúa Trời binh vực con.",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.523Z",
      "updatedAt": "2025-04-24T01:54:30.523Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "7fc26a19-0b87-41c2-bd80-bb38ded7e199",
      "number": 10,
      "content": "Trong Đức Chúa Trời, tôi ca ngợi lời Ngài;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.536Z",
      "updatedAt": "2025-04-24T01:54:30.536Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "70ce4086-b0a1-421f-8e97-d31e5464c193",
      "number": 10,
      "content": "Trong **CHÚA**, tôi ca ngợi lời Ngài;",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.550Z",
      "updatedAt": "2025-04-24T01:54:30.550Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "0c5d1caa-011a-490f-bc6d-5f95d1a60b2a",
      "number": 11,
      "content": "Nơi Đức Chúa Trời, tôi đặt trọn niềm tin; tôi sẽ không sợ hãi;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.564Z",
      "updatedAt": "2025-04-24T01:54:30.564Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "a294a8a8-3f83-4e93-88a0-2f65a3ef48f6",
      "number": 11,
      "content": "Loài người làm chi tôi được?",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.577Z",
      "updatedAt": "2025-04-24T01:54:30.577Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "048a012f-58b7-4ad2-9096-43ead7bd8cc2",
      "number": 12,
      "content": "Đức Chúa Trời ôi, con sẽ làm những gì con đã thệ nguyện;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.591Z",
      "updatedAt": "2025-04-24T01:54:30.591Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "6a6834f9-7182-4ebb-9fbf-233e03f208b1",
      "number": 12,
      "content": "Con sẽ dâng của lễ tạ ơn lên Ngài.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.604Z",
      "updatedAt": "2025-04-24T01:54:30.604Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "a3417466-926b-4136-8191-f9c57d61d116",
      "number": 13,
      "content": "Vì chẳng phải Ngài đã giải cứu linh hồn con khỏi chết,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.618Z",
      "updatedAt": "2025-04-24T01:54:30.618Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "104e7a25-7b9f-4042-9d7c-6eeaebf2d4e2",
      "number": 13,
      "content": "Khiến chân con không bị vấp ngã,",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.633Z",
      "updatedAt": "2025-04-24T01:54:30.633Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "5d76dcb6-b59f-4b30-9680-38df60e13295",
      "number": 13,
      "content": "Để con có thể bước đi trước mặt Đức Chúa Trời",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.647Z",
      "updatedAt": "2025-04-24T01:54:30.647Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    },
    {
      "id": "1f9a4a5a-8228-4559-ada7-b68b8bf811cb",
      "number": 13,
      "content": "Trong ánh sáng của sự sống hay sao?",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:30.660Z",
      "updatedAt": "2025-04-24T01:54:30.660Z",
      "chapterId": "9801b465-da46-46f6-b06a-14d62ece4e32"
    }
  ],
