{
  "chapter": {
    "id": "dd59df7e-b383-4841-aeba-3e8a6b53f57b",
    "number": 68,
    "ref": "https://www.bible.com/bible/19/PSA.68.BD2011",
    "createdAt": "2025-04-24T01:29:06.614Z",
    "updatedAt": "2025-04-24T01:29:06.614Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "7d1474c9-bb67-47ce-a0e7-7df05c0660f8",
      "number": 1,
      "content": "Nguyện Đức Chúa Trời trỗi dậy; nguyện những kẻ chống nghịch Ngài tản lạc;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.712Z",
      "updatedAt": "2025-04-24T01:55:11.712Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "7ed49e49-a913-4d56-bda9-0f2be410a442",
      "number": 1,
      "content": "Nguyện những kẻ ghét Ngài chạy trốn trước mặt Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.732Z",
      "updatedAt": "2025-04-24T01:55:11.732Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "be40789b-040c-40c5-8aed-edf5b8e72caf",
      "number": 2,
      "content": "Như khói bị tan biến thể nào, nguyện chúng bị tan đi thể ấy;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.743Z",
      "updatedAt": "2025-04-24T01:55:11.743Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "7fa82ecc-ad28-4650-93d9-d315be186203",
      "number": 2,
      "content": "Như sáp bị tan chảy trước ngọn lửa thể nào,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.753Z",
      "updatedAt": "2025-04-24T01:55:11.753Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "ba09d74a-5db1-42ef-a18e-45b6cf55854a",
      "number": 2,
      "content": "Nguyện những kẻ ác bị diệt mất trước mặt Đức Chúa Trời thể ấy.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.765Z",
      "updatedAt": "2025-04-24T01:55:11.765Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "5e8a7706-667b-4d08-8ed7-e562bd17972b",
      "number": 3,
      "content": "Nguyện những người ngay lành hân hoan,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.776Z",
      "updatedAt": "2025-04-24T01:55:11.776Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "d411e698-810d-41a2-a8cf-ba73e5657473",
      "number": 3,
      "content": "Nguyện họ vui vẻ trước mặt Đức Chúa Trời;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.788Z",
      "updatedAt": "2025-04-24T01:55:11.788Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "2c258858-ebc3-48e2-b3a2-2a54b6c72638",
      "number": 3,
      "content": "Thật vậy nguyện họ hớn hở và vui mừng.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.801Z",
      "updatedAt": "2025-04-24T01:55:11.801Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "79f8c38b-267a-4f70-803f-185e5127734e",
      "number": 4,
      "content": "Hãy ca ngợi Đức Chúa Trời; hãy hát ca chúc tụng danh Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.816Z",
      "updatedAt": "2025-04-24T01:55:11.816Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "ae577ceb-20c7-4ee7-abcf-cb5f47d957bc",
      "number": 4,
      "content": "Khá tôn ngợi Đấng ngự trên các tầng mây;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.830Z",
      "updatedAt": "2025-04-24T01:55:11.830Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "c8ec04aa-127e-42de-8264-45684ceee43f",
      "number": 4,
      "content": "Danh Ngài là **CHÚA**;",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.857Z",
      "updatedAt": "2025-04-24T01:55:11.857Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "9e405455-0cac-4bb6-afbb-39b92daf3c1c",
      "number": 4,
      "content": "Khá reo vui trước mặt Ngài.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.871Z",
      "updatedAt": "2025-04-24T01:55:11.871Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "1fc641ca-3a28-4364-9995-ea72a78badcd",
      "number": 5,
      "content": "Cha kẻ mồ côi, Đấng binh vực người góa bụa,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.885Z",
      "updatedAt": "2025-04-24T01:55:11.885Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "da8503c4-7f5d-4c6d-b39f-b042e109fb9e",
      "number": 5,
      "content": "Chính là Đức Chúa Trời, Đấng ngự trong nơi thánh của Ngài.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.898Z",
      "updatedAt": "2025-04-24T01:55:11.898Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "c5e39fe2-1013-407f-b97c-11a770d0ee12",
      "number": 6,
      "content": "Đức Chúa Trời là Đấng làm cho kẻ cô đơn có mái ấm gia đình;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.912Z",
      "updatedAt": "2025-04-24T01:55:11.912Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "c973d4ab-80e7-429b-b6e9-5c619c91a138",
      "number": 6,
      "content": "Ngài dẫn đưa những kẻ bị tù đày đến nơi sung mãn;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.927Z",
      "updatedAt": "2025-04-24T01:55:11.927Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "424645cb-9dd9-4328-bad4-19bb6d45e454",
      "number": 6,
      "content": "Còn những kẻ bội nghịch phải ở trong xứ khô khan.",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.941Z",
      "updatedAt": "2025-04-24T01:55:11.941Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "cc7980fe-6382-4d84-beb6-2ee2a479ad9e",
      "number": 7,
      "content": "Đức Chúa Trời ôi, khi Ngài đi ra trước mặt con dân Ngài,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.955Z",
      "updatedAt": "2025-04-24T01:55:11.955Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "6ed44d05-20a6-465c-8c7a-6eaaf6632e69",
      "number": 7,
      "content": "Khi Ngài đi qua sa mạc, (Sê-la)",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.969Z",
      "updatedAt": "2025-04-24T01:55:11.969Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "290890d0-b6d7-4521-a3ee-522e5a28ba0f",
      "number": 8,
      "content": "Đất rúng động, các tầng trời ào ạt đổ cơn mưa,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.983Z",
      "updatedAt": "2025-04-24T01:55:11.983Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "a964b2b5-2967-4980-a8a6-92684105a19d",
      "number": 8,
      "content": "Trước mặt Đức Chúa Trời, Thần của Núi Si-nai,",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:11.995Z",
      "updatedAt": "2025-04-24T01:55:11.995Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "9d50f59b-5d4c-412f-9f78-acfc7de9847f",
      "number": 8,
      "content": "Trước mặt Đức Chúa Trời, Thần của I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.023Z",
      "updatedAt": "2025-04-24T01:55:12.023Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "7dd0360d-d2ec-40ef-982f-4755a448f374",
      "number": 9,
      "content": "Đức Chúa Trời ôi, Ngài đã ban mưa xuống cách dồi dào,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.036Z",
      "updatedAt": "2025-04-24T01:55:12.036Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "0d850c3d-2b65-4a87-a9c9-fd845a4373f5",
      "number": 9,
      "content": "Khi sản nghiệp của Ngài bị khô khan, Ngài đã bồi bổ nó lại.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.056Z",
      "updatedAt": "2025-04-24T01:55:12.056Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "3d472656-7349-428e-988d-93f8d1164cd6",
      "number": 10,
      "content": "Con dân Ngài đã định cư trong sản nghiệp đó;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.070Z",
      "updatedAt": "2025-04-24T01:55:12.070Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "bfce8855-e88c-4479-9b65-49aeddc3aa82",
      "number": 10,
      "content": "Đức Chúa Trời ôi, do lòng tốt của Ngài, Ngài đã chu cấp cho những người nghèo khó.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.084Z",
      "updatedAt": "2025-04-24T01:55:12.084Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "65dc0fb0-62d2-4571-af9c-ea2767194864",
      "number": 11,
      "content": "Khi Chúa ban lệnh,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.098Z",
      "updatedAt": "2025-04-24T01:55:12.098Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "a8b63929-2323-4ece-b633-7827046f5833",
      "number": 11,
      "content": "Những phụ nữ ra đi báo tin mừng đông vô số:",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.112Z",
      "updatedAt": "2025-04-24T01:55:12.112Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "57fe5b4c-d598-41c6-9fe2-a771007bdde9",
      "number": 12,
      "content": "“Các vua của các đạo quân bỏ chạy; họ chạy trốn.”",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.125Z",
      "updatedAt": "2025-04-24T01:55:12.125Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "d37256b5-44ca-4dd2-81f6-9024fefa7820",
      "number": 12,
      "content": "Những phụ nữ còn ở nhà chia chiến lợi phẩm–",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.139Z",
      "updatedAt": "2025-04-24T01:55:12.139Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "1d8eef96-44bf-4389-86a4-34ae55a2d2c3",
      "number": 13,
      "content": "Dù các ông nằm nghỉ giữa các ràn chiên–",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.153Z",
      "updatedAt": "2025-04-24T01:55:12.153Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "7469bbdc-2c3b-4b20-9670-ad2a7f177b19",
      "number": 13,
      "content": "Các chị em như những cánh bồ câu long lanh dát bạc,",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.166Z",
      "updatedAt": "2025-04-24T01:55:12.166Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "06972f73-fa09-4aeb-a757-306cb9eab4b4",
      "number": 13,
      "content": "Với bộ lông lóng lánh ánh vàng.",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.180Z",
      "updatedAt": "2025-04-24T01:55:12.180Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "33bb5439-a8b5-41e4-97b9-1ee9166fbc20",
      "number": 14,
      "content": "Khi Đấng Toàn Năng đánh văng các vua trong xứ,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.194Z",
      "updatedAt": "2025-04-24T01:55:12.194Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "09cb4ee9-3b06-44a7-affc-107fa6e33869",
      "number": 14,
      "content": "Khiến họ như tuyết rơi tơi tả ở Xanh-môn.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.207Z",
      "updatedAt": "2025-04-24T01:55:12.207Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "f76bd2e1-9b87-4a65-b111-d4f33536eb22",
      "number": 15,
      "content": "Núi Ba-san là núi của Đức Chúa Trời;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.220Z",
      "updatedAt": "2025-04-24T01:55:12.220Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "d5f63f35-780c-42e0-bb1d-aaeb205deb9d",
      "number": 15,
      "content": "Rặng Núi Ba-san quả có nhiều đỉnh cao chót vót.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.234Z",
      "updatedAt": "2025-04-24T01:55:12.234Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "e82b28da-af33-426b-84ee-858bfc54eff9",
      "number": 16,
      "content": "Hỡi những rặng núi cao có nhiều đỉnh,",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.248Z",
      "updatedAt": "2025-04-24T01:55:12.248Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "3fa6cbf6-a65d-4ccb-adc4-623c1de50dc4",
      "number": 16,
      "content": "Tại sao các ngươi ganh tị với ngọn núi Đức Chúa Trời đã chọn để làm nơi Ngài ngự?",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.261Z",
      "updatedAt": "2025-04-24T01:55:12.261Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "d22f0eb6-0550-49fb-8759-fae9d8f57473",
      "number": 16,
      "content": "Thật vậy **CHÚA** sẽ ngự tại đó đời đời.",
      "order": 2,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.275Z",
      "updatedAt": "2025-04-24T01:55:12.275Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "65a0086d-9f78-4f2f-a50b-8b285a0968d3",
      "number": 17,
      "content": "Các xe chiến mã của Đức Chúa Trời nhiều đến hằng vạn,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.288Z",
      "updatedAt": "2025-04-24T01:55:12.288Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "328a8d4b-afd4-4ceb-a187-d956d2d1063f",
      "number": 17,
      "content": "Thật là hằng ngàn và hằng vạn;",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.302Z",
      "updatedAt": "2025-04-24T01:55:12.302Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "1da53129-5aeb-4b09-b32f-257a3db93858",
      "number": 17,
      "content": "Chúa ngự giữa chúng như Ngài ngự trong nơi thánh của Ngài ở Si-nai.",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.319Z",
      "updatedAt": "2025-04-24T01:55:12.319Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "ba31fa1d-5ae4-4a16-a4a3-836942149999",
      "number": 18,
      "content": "Ngài đã lên nơi cao,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.333Z",
      "updatedAt": "2025-04-24T01:55:12.333Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "61feab9a-a45a-4cce-82df-100de22059c3",
      "number": 18,
      "content": "Ngài dẫn theo đoàn tù binh bị bắt đem đi lưu đày;",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.347Z",
      "updatedAt": "2025-04-24T01:55:12.347Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "d40bb469-0d60-4518-b272-2a80efa4b8fe",
      "number": 18,
      "content": "Ngài đã nhận những lễ vật của loài người,",
      "order": 2,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.360Z",
      "updatedAt": "2025-04-24T01:55:12.360Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "2743f52c-ca37-449c-83ea-ca72de452ad2",
      "number": 18,
      "content": "Ngay cả những lễ vật của những kẻ bội nghịch, để chứng tỏ rằng **CHÚA** Đức Chúa Trời cũng ở tại đó.",
      "order": 3,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.374Z",
      "updatedAt": "2025-04-24T01:55:12.374Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "add07346-44eb-4e4c-9200-4408a02eb974",
      "number": 19,
      "content": "Tôn ngợi Chúa,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.401Z",
      "updatedAt": "2025-04-24T01:55:12.401Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "548e0c39-e241-4845-9e74-48c713bc077a",
      "number": 19,
      "content": "Đấng hằng ngày cưu mang chúng ta;",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.414Z",
      "updatedAt": "2025-04-24T01:55:12.414Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "17aee6f5-b02b-4ea4-bc4a-2f56b6923ed0",
      "number": 19,
      "content": "Đức Chúa Trời là Đấng Giải Cứu của chúng ta. (Sê-la)",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.428Z",
      "updatedAt": "2025-04-24T01:55:12.428Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "d3e91a0d-4bd9-451e-87b7-02c8cb4a8235",
      "number": 20,
      "content": "Đức Chúa Trời chúng ta là Đức Chúa Trời giải cứu;",
      "order": 0,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.442Z",
      "updatedAt": "2025-04-24T01:55:12.442Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "fda3703c-53ee-4a18-9cda-7d3f67340add",
      "number": 20,
      "content": "Nhờ **CHÚA** Hằng Hữu, chúng ta được thoát chết.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.455Z",
      "updatedAt": "2025-04-24T01:55:12.455Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "952da9f3-feec-4d8a-a11c-d3636348a147",
      "number": 21,
      "content": "Chắc chắn Đức Chúa Trời sẽ đạp nát đầu kẻ thù của Ngài,",
      "order": 0,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.482Z",
      "updatedAt": "2025-04-24T01:55:12.482Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "ba5f200a-bc42-4200-a487-c46330d08662",
      "number": 21,
      "content": "Tức đầu tóc của những kẻ cứ miệt mài trong tội ác.",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.496Z",
      "updatedAt": "2025-04-24T01:55:12.496Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "e5f69976-8b62-45eb-8836-bb56315f11c4",
      "number": 22,
      "content": "Chúa phán,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.509Z",
      "updatedAt": "2025-04-24T01:55:12.509Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "8e20733a-b5e2-423b-8387-82709d73862e",
      "number": 22,
      "content": "“Từ Ba-san, Ta sẽ đem chúng về;",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.524Z",
      "updatedAt": "2025-04-24T01:55:12.524Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "0f1c2838-877e-4b14-9edc-bdff569aa831",
      "number": 22,
      "content": "Từ các đại dương sâu thẳm, Ta sẽ mang chúng về,",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.537Z",
      "updatedAt": "2025-04-24T01:55:12.537Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "92c664a2-4b27-4f7e-907a-0f94ad9e21ad",
      "number": 23,
      "content": "Để các ngươi có thể giẫm chân trên máu kẻ thù của mình,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.551Z",
      "updatedAt": "2025-04-24T01:55:12.551Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "a9f41543-c2bf-4cce-9b1a-bdb181cd52c5",
      "number": 23,
      "content": "Ngay cả lưỡi của chó các ngươi cũng sẽ được phần trên quân thù của các ngươi.”",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.564Z",
      "updatedAt": "2025-04-24T01:55:12.564Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "05505933-2966-453f-9b03-ab705d89398f",
      "number": 24,
      "content": "Đức Chúa Trời ôi, người ta đã chứng kiến lễ nghinh rước Ngài,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.578Z",
      "updatedAt": "2025-04-24T01:55:12.578Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "91559928-a092-43b6-a1b9-0c715189f975",
      "number": 24,
      "content": "Tức lễ nghinh rước Đức Chúa Trời của con, Vua của con, vào đền thánh.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.592Z",
      "updatedAt": "2025-04-24T01:55:12.592Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "75f2bbfe-cce5-435b-8773-421dfb8668c9",
      "number": 25,
      "content": "Ca đoàn đi trước ngợi ca, ban nhạc theo sau trỗi nhạc,",
      "order": 0,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.606Z",
      "updatedAt": "2025-04-24T01:55:12.606Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "66e7bb46-66da-4fd7-8a33-434a1cd4ffa1",
      "number": 25,
      "content": "Ở giữa là đoàn thiếu nữ đánh trống lục lạc nhịp nhàng.",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.619Z",
      "updatedAt": "2025-04-24T01:55:12.619Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "9b077342-6d9d-40dd-bfd5-eb78f1a68280",
      "number": 26,
      "content": "Hãy chúc tụng Đức Chúa Trời giữa những đoàn người đông đúc dự lễ,",
      "order": 0,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.632Z",
      "updatedAt": "2025-04-24T01:55:12.632Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "6a360abd-3b84-4877-bbb2-6175f73b7974",
      "number": 26,
      "content": "Hãy tôn ngợi **CHÚA**, hỡi dòng dõi của I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.646Z",
      "updatedAt": "2025-04-24T01:55:12.646Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "97f2ed52-b03d-4f83-aee5-ed006288be50",
      "number": 27,
      "content": "Kìa là Bên-gia-min, chi tộc trẻ nhất, đang dẫn đầu;",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.660Z",
      "updatedAt": "2025-04-24T01:55:12.660Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "9035f1e5-86b4-4aae-b086-6e447e421192",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Giu-đa và phái đoàn của họ;",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.674Z",
      "updatedAt": "2025-04-24T01:55:12.674Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "964e3d42-55be-4fb6-9a0a-359a1529c9dc",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Xê-bu-lun và các nhà quyền quý của Náp-ta-li.",
      "order": 2,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.687Z",
      "updatedAt": "2025-04-24T01:55:12.687Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "154b56d0-2829-4c85-9bcc-c2668cc0e8e8",
      "number": 28,
      "content": "Đức Chúa Trời ôi, xin thể hiện quyền năng Ngài;",
      "order": 0,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.701Z",
      "updatedAt": "2025-04-24T01:55:12.701Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "ca4af67d-83ee-407b-9d0f-86299b3eeef6",
      "number": 28,
      "content": "Đức Chúa Trời ôi, xin bày tỏ sức mạnh Ngài, như Ngài đã từng làm cho chúng con khi trước.",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.728Z",
      "updatedAt": "2025-04-24T01:55:12.728Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "520ac260-62c7-4ce3-a3bd-5a5cab5fcbc8",
      "number": 29,
      "content": "Vì đền thờ Ngài tọa lạc tại Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.742Z",
      "updatedAt": "2025-04-24T01:55:12.742Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "212bf683-e9ac-4d84-a072-398c60f5fbb1",
      "number": 29,
      "content": "Nên các vua sẽ đem các lễ vật đến dâng lên Ngài tại đó.",
      "order": 1,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.756Z",
      "updatedAt": "2025-04-24T01:55:12.756Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "69686039-7859-40fe-a84e-9865500a7654",
      "number": 30,
      "content": "Cầu xin Ngài trách phạt bầy thú hoang trong đám lau sậy,",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.769Z",
      "updatedAt": "2025-04-24T01:55:12.769Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "99be207a-ea82-444d-b90c-688a836ba631",
      "number": 30,
      "content": "Cùng đám bò đực ở giữa đàn bò con của các dân.",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.783Z",
      "updatedAt": "2025-04-24T01:55:12.783Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "fe5340b2-802c-4347-92ca-28a1d2bbe578",
      "number": 30,
      "content": "Xin Ngài bắt chúng phải đem bạc nén đến quỳ dâng;",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.797Z",
      "updatedAt": "2025-04-24T01:55:12.797Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "d7c98bb6-4016-4e62-99bb-40ccb326b741",
      "number": 30,
      "content": "Còn những dân hiếu chiến, xin Ngài làm cho chúng tản lạc khắp nơi.",
      "order": 3,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.810Z",
      "updatedAt": "2025-04-24T01:55:12.810Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "498da7d8-1edd-4558-a4cd-ba7da62bd619",
      "number": 31,
      "content": "Những đoàn người quyền thế sẽ từ Ai-cập đến thờ phượng;",
      "order": 0,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.824Z",
      "updatedAt": "2025-04-24T01:55:12.824Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "7b2d38a1-99b0-4096-bf2e-f0e32b3ee553",
      "number": 31,
      "content": "Người Ê-thi-ô-pi sẽ sớm đưa tay lên hướng về Đức Chúa Trời.",
      "order": 1,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.838Z",
      "updatedAt": "2025-04-24T01:55:12.838Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "9bb961f9-900e-4fed-ad9f-4bdd4034304d",
      "number": 32,
      "content": "Hỡi tất cả quốc gia trên thế giới, hãy ca tụng Đức Chúa Trời;",
      "order": 0,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.865Z",
      "updatedAt": "2025-04-24T01:55:12.865Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "97f26914-7406-41f3-86c3-44c24d4ec988",
      "number": 32,
      "content": "Hãy ca ngợi **CHÚA**, (Sê-la)",
      "order": 1,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.879Z",
      "updatedAt": "2025-04-24T01:55:12.879Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "a4f91bb5-8a6e-4d85-9918-56dd494ca045",
      "number": 33,
      "content": "Đấng ngự trên nơi chí cao của các tầng trời, tức các tầng trời đã được dựng nên từ thời thái cổ;",
      "order": 0,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.893Z",
      "updatedAt": "2025-04-24T01:55:12.893Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "9583ac25-8c59-47bb-9dd0-084d4f5cff0e",
      "number": 33,
      "content": "Kìa, Ngài cất tiếng nói, giọng Ngài thật oai hùng.",
      "order": 1,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.906Z",
      "updatedAt": "2025-04-24T01:55:12.906Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "01925a8f-d00a-402a-a665-45263b286d42",
      "number": 34,
      "content": "Khá biết rằng mọi quyền lực đều thuộc về Đức Chúa Trời;",
      "order": 0,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.921Z",
      "updatedAt": "2025-04-24T01:55:12.921Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "8c2a9043-00b4-479e-af8a-4f00b9bec26e",
      "number": 34,
      "content": "Nguyện uy nghi Ngài tỏa rạng trên I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.935Z",
      "updatedAt": "2025-04-24T01:55:12.935Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "04c6fcef-c1de-47ea-ae17-3c611bcb833c",
      "number": 34,
      "content": "Và quyền năng Ngài đầy dẫy khắp không trung.",
      "order": 2,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.949Z",
      "updatedAt": "2025-04-24T01:55:12.949Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "c965d28e-bb7f-467c-8649-89d61db09fc5",
      "number": 35,
      "content": "Đức Chúa Trời ôi, trong đền thánh Ngài, Ngài đáng kinh hãi biết bao!",
      "order": 0,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.962Z",
      "updatedAt": "2025-04-24T01:55:12.962Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "e4ed3295-0627-45f9-884b-9d76b62a319b",
      "number": 35,
      "content": "Chính Đức Chúa Trời của I-sơ-ra-ên đã ban sức mạnh và quyền lực cho con dân Ngài.",
      "order": 1,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.979Z",
      "updatedAt": "2025-04-24T01:55:12.979Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    },
    {
      "id": "e2dc436c-89a2-4c83-afdc-a4fd9c235e3d",
      "number": 35,
      "content": "Tôn ngợi Đức Chúa Trời!",
      "order": 2,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:12.993Z",
      "updatedAt": "2025-04-24T01:55:12.993Z",
      "chapterId": "dd59df7e-b383-4841-aeba-3e8a6b53f57b"
    }
  ],
