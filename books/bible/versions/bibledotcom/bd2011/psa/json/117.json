  "verses": [
    {
      "id": "0447e0db-f343-4883-9409-0e976a3a2461",
      "number": 1,
      "content": "Hỡi các nước, hãy ca ngợi **CHÚA**;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:19.149Z",
      "updatedAt": "2025-04-24T01:58:19.149Z",
      "chapterId": "c00b7776-9ef9-45d6-ae92-81dae7bc2a9d"
    },
    {
      "id": "792b08ed-af80-4a1a-97a4-d52fea6fe414",
      "number": 1,
      "content": "Hỡi muôn dân, khá tôn ngợi Ngài!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:19.177Z",
      "updatedAt": "2025-04-24T01:58:19.177Z",
      "chapterId": "c00b7776-9ef9-45d6-ae92-81dae7bc2a9d"
    },
    {
      "id": "423e7980-3184-4570-b14e-3163a2e481ec",
      "number": 2,
      "content": "Vì tình thương của Ngài đối với chúng ta rất lớn,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:19.187Z",
      "updatedAt": "2025-04-24T01:58:19.187Z",
      "chapterId": "c00b7776-9ef9-45d6-ae92-81dae7bc2a9d"
    },
    {
      "id": "bb42de1e-5815-42d9-9ab3-371080005aa2",
      "number": 2,
      "content": "Và đức thành tín của **CHÚA** hằng còn đến đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:19.198Z",
      "updatedAt": "2025-04-24T01:58:19.198Z",
      "chapterId": "c00b7776-9ef9-45d6-ae92-81dae7bc2a9d"
    },
    {
      "id": "185ebb17-d65f-4dda-8301-a32e0e342534",
      "number": 2,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:19.210Z",
      "updatedAt": "2025-04-24T01:58:19.210Z",
