  "verses": [
    {
      "id": "50cd9ffc-fdfa-4545-bb75-8229861b6da0",
      "number": 1,
      "content": "Hỡi các nước, hãy ca ngợi **CHÚA**;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:23.280Z",
      "updatedAt": "2025-04-22T10:43:23.280Z",
      "chapterId": "f34ef68b-74c2-49a5-bd96-3002344b6ee9"
    },
    {
      "id": "eefe00a4-9026-44a4-bc7b-665fdd50629d",
      "number": 1,
      "content": "Hỡi muôn dân, khá tôn ngợi Ngài!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:23.326Z",
      "updatedAt": "2025-04-22T10:43:23.326Z",
      "chapterId": "f34ef68b-74c2-49a5-bd96-3002344b6ee9"
    },
    {
      "id": "c353e977-17f1-4395-a789-e27ee308f956",
      "number": 2,
      "content": "Vì tình thương của Ngài đối với chúng ta rất lớn,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:23.338Z",
      "updatedAt": "2025-04-22T10:43:23.338Z",
      "chapterId": "f34ef68b-74c2-49a5-bd96-3002344b6ee9"
    },
    {
      "id": "548d843a-3fb3-43d7-9ec2-f1eab41ffa2b",
      "number": 2,
      "content": "Và đức thành tín của **CHÚA** hằng còn đến đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:23.347Z",
      "updatedAt": "2025-04-22T10:43:23.347Z",
      "chapterId": "f34ef68b-74c2-49a5-bd96-3002344b6ee9"
    },
    {
      "id": "ab6b2b90-bd26-4d67-8031-410e9b9cc152",
      "number": 2,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:23.357Z",
      "updatedAt": "2025-04-22T10:43:23.357Z",
      "chapterId": "f34ef68b-74c2-49a5-bd96-3002344b6ee9"
    }
  ],
