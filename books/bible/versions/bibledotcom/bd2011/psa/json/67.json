{
  "chapter": {
    "id": "1a86e09f-541c-41c4-90b9-7e287a9a95fa",
    "number": 67,
    "ref": "https://www.bible.com/bible/19/PSA.67.BD2011",
    "createdAt": "2025-04-24T01:29:06.604Z",
    "updatedAt": "2025-04-24T01:29:06.604Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "8fe0755d-795d-44fa-9a99-33bad217b62b",
      "number": 1,
      "content": "Cầu xin Đức Chúa Trời thương xót chúng con và ban phước cho chúng con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.542Z",
      "updatedAt": "2025-04-24T01:55:08.542Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "ddfae618-a4b4-4f05-a783-e6fd49059b55",
      "number": 1,
      "content": "Cầu xin Ngài chiếu sáng mặt Ngài trên chúng con, (Sê-la)",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.570Z",
      "updatedAt": "2025-04-24T01:55:08.570Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "69573273-a501-422d-a97c-fdf2d021a706",
      "number": 2,
      "content": "Để đường lối Ngài được biết đến trên đất,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.580Z",
      "updatedAt": "2025-04-24T01:55:08.580Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "0c91d88c-10e7-43da-9b0d-d4796c5df840",
      "number": 2,
      "content": "Và ơn cứu rỗi Ngài được phổ cập giữa các dân.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.591Z",
      "updatedAt": "2025-04-24T01:55:08.591Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "14bda5f8-92fa-429a-9524-3005909ff5f3",
      "number": 3,
      "content": "Đức Chúa Trời ôi, nguyện muôn dân ca ngợi Ngài;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.602Z",
      "updatedAt": "2025-04-24T01:55:08.602Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "25354982-fc97-462a-bd5a-3bf9d70bc113",
      "number": 3,
      "content": "Nguyện tất cả các dân ca ngợi Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.614Z",
      "updatedAt": "2025-04-24T01:55:08.614Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "dbdc1197-8a8a-41e2-8687-4b16fd4f7495",
      "number": 4,
      "content": "Nguyện các dân các nước vui mừng và hân hoan ca hát,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.625Z",
      "updatedAt": "2025-04-24T01:55:08.625Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "8f6d6628-c6ea-49c3-9eff-7ed593d19ea0",
      "number": 4,
      "content": "Vì Ngài xét đoán muôn dân cách công minh,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.638Z",
      "updatedAt": "2025-04-24T01:55:08.638Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "24f0ecfe-8bb7-43d2-a7b4-f0646ecf0990",
      "number": 4,
      "content": "Và lãnh đạo các quốc gia trên đất. (Sê-la)",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.652Z",
      "updatedAt": "2025-04-24T01:55:08.652Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "7942ab4e-c013-4ccd-ac23-6725b88f46ae",
      "number": 5,
      "content": "Đức Chúa Trời ôi, nguyện muôn dân ca ngợi Ngài;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.666Z",
      "updatedAt": "2025-04-24T01:55:08.666Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "0041232c-42b0-4c02-b8ae-f1ac84d7803b",
      "number": 5,
      "content": "Nguyện tất cả các dân ca ngợi Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.680Z",
      "updatedAt": "2025-04-24T01:55:08.680Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "73417ce4-d923-4291-9dda-6d1fff9e09b3",
      "number": 6,
      "content": "Đất sẽ sinh sản hoa màu;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.693Z",
      "updatedAt": "2025-04-24T01:55:08.693Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "c8def013-6286-42c1-ba95-9dec8aa54f7c",
      "number": 6,
      "content": "Đức Chúa Trời, chính Đức Chúa Trời của chúng ta, sẽ ban phước cho chúng ta.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.708Z",
      "updatedAt": "2025-04-24T01:55:08.708Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "1c6f6fdb-7640-40a3-8c75-6a406b681a12",
      "number": 7,
      "content": "Nguyện Đức Chúa Trời tiếp tục ban phước cho chúng ta,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.722Z",
      "updatedAt": "2025-04-24T01:55:08.722Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    },
    {
      "id": "726e8124-a4b2-4374-8187-ea17f6432649",
      "number": 7,
      "content": "Để mọi người ở tận cùng trái đất kính sợ Ngài.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:55:08.735Z",
      "updatedAt": "2025-04-24T01:55:08.735Z",
      "chapterId": "1a86e09f-541c-41c4-90b9-7e287a9a95fa"
    }
  ],
