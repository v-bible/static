{
  "chapter": {
    "id": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8",
    "number": 54,
    "ref": "https://www.bible.com/bible/19/PSA.54.BD2011",
    "createdAt": "2025-04-24T01:29:06.437Z",
    "updatedAt": "2025-04-24T01:29:06.437Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "7051dbd1-7002-48ac-8f85-08bc811bcd09",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin cứu con, bằng uy danh Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:22.985Z",
      "updatedAt": "2025-04-24T01:54:22.985Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "bddb857b-ecfd-43d2-a42c-16b0c91abf7e",
      "number": 1,
      "content": "Xin minh oan cho con, bằng quyền năng Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.013Z",
      "updatedAt": "2025-04-24T01:54:23.013Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "d20f5a55-7004-4a26-9f62-d64c289ddbb2",
      "number": 2,
      "content": "Đức Chúa Trời ôi, xin lắng nghe lời cầu nguyện của con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.024Z",
      "updatedAt": "2025-04-24T01:54:23.024Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "6268783a-7359-4897-8c97-4005931ed0d0",
      "number": 2,
      "content": "Xin nghiêng tai nghe tiếng của miệng con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.036Z",
      "updatedAt": "2025-04-24T01:54:23.036Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "8fc5b108-c60d-46af-b8da-afe843093bd8",
      "number": 3,
      "content": "Vì những kẻ xa lạ đã nổi lên chống lại con,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.049Z",
      "updatedAt": "2025-04-24T01:54:23.049Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "c7afc878-5dc0-4e77-adf9-939903200b70",
      "number": 3,
      "content": "Những kẻ hung bạo đã tìm cách hại mạng sống con;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.062Z",
      "updatedAt": "2025-04-24T01:54:23.062Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "9b783546-7829-442b-bdb8-b7a7a1ff80cd",
      "number": 3,
      "content": "Quả là chúng chẳng coi Đức Chúa Trời ra gì. (Sê-la)",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.079Z",
      "updatedAt": "2025-04-24T01:54:23.079Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "edc6b87d-5df5-493a-9769-f2cd6b2f5bea",
      "number": 4,
      "content": "Này, Đức Chúa Trời là Đấng giúp đỡ tôi;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.096Z",
      "updatedAt": "2025-04-24T01:54:23.096Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "585f4068-7ebd-4a25-a69b-a6eca6af9604",
      "number": 4,
      "content": "Chúa là Đấng gìn giữ mạng sống tôi.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.110Z",
      "updatedAt": "2025-04-24T01:54:23.110Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "b3049edf-642a-43c0-9df4-ceada2968118",
      "number": 5,
      "content": "Ngài sẽ báo trả điều ác trên kẻ thù tôi,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.123Z",
      "updatedAt": "2025-04-24T01:54:23.123Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "c37d8d72-8b00-4fbd-b670-506765e72f31",
      "number": 5,
      "content": "Tiêu diệt chúng vì đức thành tín Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.137Z",
      "updatedAt": "2025-04-24T01:54:23.137Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "26665942-5ff1-40ca-b9de-b8608e5f0970",
      "number": 6,
      "content": "Con sẽ đem của lễ lạc ý dâng lên Ngài với tất cả niềm vui;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.152Z",
      "updatedAt": "2025-04-24T01:54:23.152Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "67d85769-31a9-4eca-af33-b1306c99ad5f",
      "number": 6,
      "content": "Con sẽ cảm tạ danh Ngài, **CHÚA** ôi, vì đó là điều tốt đẹp,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.166Z",
      "updatedAt": "2025-04-24T01:54:23.166Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "faf463ac-74ce-4145-8473-d929aa8dc7a6",
      "number": 7,
      "content": "Vì Ngài đã giải cứu con khỏi mọi hoạn nạn,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.180Z",
      "updatedAt": "2025-04-24T01:54:23.180Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    },
    {
      "id": "2ab221e5-38ed-42b2-b4b1-7aa7e124beec",
      "number": 7,
      "content": "Và mắt con được nhìn thấy những kẻ thù của con bị bại.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:54:23.193Z",
      "updatedAt": "2025-04-24T01:54:23.193Z",
      "chapterId": "d9bc2f48-4e34-42e4-b7ef-b39ef74cefc8"
    }
  ],
