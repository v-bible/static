    },
    {
      "id": "e1ed09ef-4410-4186-ba32-4e78ea14af9b",
      "number": 19,
      "content": "Ngài tuyên bố lời Ngài cho Gia-cốp,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:08.868Z",
      "updatedAt": "2024-09-23T05:44:08.868Z",
      "chapterId": "c16a972c-3804-4c1e-a1b9-899710dca5d3"
    },
    {
      "id": "57de02b2-5702-4ace-87f4-62e229c4d82a",
      "number": 19,
      "content": "Luật lệ Ngài và mạng lịnh Ngài cho I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:08.871Z",
      "updatedAt": "2024-09-23T05:44:08.871Z",
      "chapterId": "c16a972c-3804-4c1e-a1b9-899710dca5d3"
    },
    {
      "id": "27c862c3-ffeb-487a-975a-1fb7f7da6c21",
      "number": 20,
      "content": "Ngài chẳng làm như vậy cho một dân nào khác;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:08.875Z",
      "updatedAt": "2024-09-23T05:44:08.875Z",
      "chapterId": "c16a972c-3804-4c1e-a1b9-899710dca5d3"
    },
    {
      "id": "b61eb343-b111-4aa2-bdd8-cb07a80ad1b5",
      "number": 20,
      "content": "Họ chẳng biết gì về các mạng lịnh của Ngài.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:08.878Z",
      "updatedAt": "2024-09-23T05:44:08.878Z",
