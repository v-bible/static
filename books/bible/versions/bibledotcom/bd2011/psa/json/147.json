{
  "chapter": {
    "id": "c865c8c5-bbc8-4d63-b242-90429f09f685",
    "number": 147,
    "ref": "https://www.bible.com/bible/19/PSA.147.BD2011",
    "createdAt": "2025-04-24T01:29:07.639Z",
    "updatedAt": "2025-04-24T01:29:07.639Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "cce3f40e-7905-4c60-a515-94ab3d5fc66f",
      "number": 1,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.800Z",
      "updatedAt": "2025-04-24T02:00:31.800Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "ce8418c7-478c-41b2-a60a-4b0ca11dc4bf",
      "number": 1,
      "content": "Ca hát chúc tụng Đức Chúa Trời chúng ta là điều tốt đẹp;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.825Z",
      "updatedAt": "2025-04-24T02:00:31.825Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "0e00a50b-3133-46fc-88a6-da47fde645e3",
      "number": 1,
      "content": "Vì tôn ngợi Ngài là điều vui thỏa và hợp lý dường nào!",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.836Z",
      "updatedAt": "2025-04-24T02:00:31.836Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "ececd3bf-e7d6-4ddf-ab77-f6709863ef84",
      "number": 2,
      "content": "**CHÚA** xây dựng lại Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.846Z",
      "updatedAt": "2025-04-24T02:00:31.846Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "040fb8b1-2a76-41aa-8306-a466c5b7706d",
      "number": 2,
      "content": "Ngài gom nhóm những người bị tản lạc của I-sơ-ra-ên trở về.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.861Z",
      "updatedAt": "2025-04-24T02:00:31.861Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "1edd0e7d-9d54-4689-86a4-edd9c2f90bfd",
      "number": 3,
      "content": "Ngài rịt lành những tâm hồn tan vỡ;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.874Z",
      "updatedAt": "2025-04-24T02:00:31.874Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "a6c5c1d0-cb26-4843-a813-ff00b89853da",
      "number": 3,
      "content": "Và băng bó các vết thương của họ.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.894Z",
      "updatedAt": "2025-04-24T02:00:31.894Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "7a994136-832a-41bd-a581-a42edced661a",
      "number": 4,
      "content": "Ngài định số lượng các vì sao;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.907Z",
      "updatedAt": "2025-04-24T02:00:31.907Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "c75ab06e-27d5-49bf-bc1e-2fc961b10992",
      "number": 4,
      "content": "Ngài gọi chúng theo tên riêng của từng vì sao một.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.920Z",
      "updatedAt": "2025-04-24T02:00:31.920Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "cee5a864-6615-4a46-8881-f3871752177c",
      "number": 5,
      "content": "**CHÚA** chúng ta vĩ đại biết bao!",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.933Z",
      "updatedAt": "2025-04-24T02:00:31.933Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "14f3760c-9ca0-4319-b722-215f199531ae",
      "number": 5,
      "content": "Quyền năng Ngài thật vô hạn. Kiến thức Ngài quả vô cùng.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.949Z",
      "updatedAt": "2025-04-24T02:00:31.949Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "2a35f26a-2bfa-44f9-8ba7-1d48f46e3394",
      "number": 6,
      "content": "**CHÚA** nhấc đưa những người nghèo khó lên cao,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.966Z",
      "updatedAt": "2025-04-24T02:00:31.966Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "f2dead1d-b09f-489e-832a-89a1f0b48d0e",
      "number": 6,
      "content": "Nhưng Ngài đánh đổ phường gian ác ngã nhào xuống đất.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.984Z",
      "updatedAt": "2025-04-24T02:00:31.984Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "6b7c62ed-ef47-4637-a3ff-d48e1a9323d2",
      "number": 7,
      "content": "Hãy dâng lên **CHÚA** những lời cảm tạ;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:31.997Z",
      "updatedAt": "2025-04-24T02:00:31.997Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "2b6f87a6-b04c-42d6-a6fc-fedae9b289c5",
      "number": 7,
      "content": "Hãy dùng nhạc khí ca ngợi Đức Chúa Trời.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.010Z",
      "updatedAt": "2025-04-24T02:00:32.010Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "77d9664d-fc43-43cf-bf0a-59ae76eddd01",
      "number": 8,
      "content": "Ngài bao phủ các tầng trời bằng mây;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.036Z",
      "updatedAt": "2025-04-24T02:00:32.036Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "8ba0ada0-8368-441b-8488-1c83f52bdc2d",
      "number": 8,
      "content": "Ngài chuẩn bị các trận mưa cho đất;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.049Z",
      "updatedAt": "2025-04-24T02:00:32.049Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "e37c617a-cb0d-4061-9e92-7324b312206e",
      "number": 8,
      "content": "Ngài khiến cỏ mọc trên các núi đồi.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.062Z",
      "updatedAt": "2025-04-24T02:00:32.062Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "ecbc6a18-1b64-4758-9ac5-06d71ca9ece1",
      "number": 9,
      "content": "Ngài ban thực phẩm cho muôn loài cầm thú;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.076Z",
      "updatedAt": "2025-04-24T02:00:32.076Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "cdd4e2cc-654f-49ed-aa51-089d8c15008e",
      "number": 9,
      "content": "Ngài ban thức ăn cho các quạ con đói bụng kêu gào.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.089Z",
      "updatedAt": "2025-04-24T02:00:32.089Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "f0953ca0-629b-4579-8568-edbb50077933",
      "number": 10,
      "content": "Ngài chẳng hài lòng về sức lực của ngựa chiến;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.102Z",
      "updatedAt": "2025-04-24T02:00:32.102Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "30bf72ef-677d-4d5a-ad1c-4a91649c72d1",
      "number": 10,
      "content": "Ngài không vui về đôi chân nhanh nhẹn của loài người;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.117Z",
      "updatedAt": "2025-04-24T02:00:32.117Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "ce2bf824-8091-4b04-a156-bb8231bd18d4",
      "number": 11,
      "content": "Nhưng **CHÚA** rất vui lòng về những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.132Z",
      "updatedAt": "2025-04-24T02:00:32.132Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "cb174951-0a45-4b50-b887-8e36e9c44169",
      "number": 11,
      "content": "Những người đặt hy vọng vào tình thương của Ngài.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.148Z",
      "updatedAt": "2025-04-24T02:00:32.148Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "42593974-c22b-4cad-b093-61f42b5980bc",
      "number": 12,
      "content": "Hỡi Giê-ru-sa-lem, hãy ca ngợi **CHÚA**!",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.160Z",
      "updatedAt": "2025-04-24T02:00:32.160Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "46d94ba8-8838-4e2d-834f-04d50eacf72e",
      "number": 12,
      "content": "Hỡi Si-ôn, hãy tôn ngợi Đức Chúa Trời!",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.173Z",
      "updatedAt": "2025-04-24T02:00:32.173Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "415771db-e174-46cf-bb3f-d7e22060e30e",
      "number": 13,
      "content": "Vì Ngài làm vững mạnh các song sắt ở các cổng thành của ngươi;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.183Z",
      "updatedAt": "2025-04-24T02:00:32.183Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "2a159a40-ce6f-450b-a272-313499531307",
      "number": 13,
      "content": "Ngài ban phước cho con dân ngươi sống trong ngươi.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.193Z",
      "updatedAt": "2025-04-24T02:00:32.193Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "f44ff5b1-8609-4d93-83a4-ac35c7265d35",
      "number": 14,
      "content": "Ngài ban hòa bình nơi các biên giới ngươi;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.203Z",
      "updatedAt": "2025-04-24T02:00:32.203Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "4831f734-b8ee-4304-bc36-933ab853031e",
      "number": 14,
      "content": "Ngài cho ngươi no thỏa bằng gạo ngon hảo hạng.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.213Z",
      "updatedAt": "2025-04-24T02:00:32.213Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "88b07cae-85d6-45e1-ba9b-8c0875703251",
      "number": 15,
      "content": "Ngài truyền lịnh của Ngài cho trái đất;",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.223Z",
      "updatedAt": "2025-04-24T02:00:32.223Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "930d8a07-4b45-4d88-b562-3afac0880693",
      "number": 15,
      "content": "Lời Ngài phán ra được cấp tốc truyền đi.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.233Z",
      "updatedAt": "2025-04-24T02:00:32.233Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "05530955-bc41-4ed3-b80f-6b133004c2aa",
      "number": 16,
      "content": "Ngài ban tuyết xuống phủ trắng như lông chiên;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.243Z",
      "updatedAt": "2025-04-24T02:00:32.243Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "783d0315-27a2-4bd4-80e9-7b0c81ea344f",
      "number": 16,
      "content": "Ngài rải sương giá ra như tro bụi.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.253Z",
      "updatedAt": "2025-04-24T02:00:32.253Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "34ca52b3-9014-4160-bfd2-d890ddd295f6",
      "number": 17,
      "content": "Ngài quăng băng giá ra từng mảng;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.263Z",
      "updatedAt": "2025-04-24T02:00:32.263Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "9db17b67-ae79-4bfa-bc1a-8cfeb85fca6b",
      "number": 17,
      "content": "Ai có thể chịu nổi cơn buốt giá của Ngài?",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.275Z",
      "updatedAt": "2025-04-24T02:00:32.275Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "737d358a-3f42-45f9-9f9a-c269a1929430",
      "number": 18,
      "content": "Ngài ban lịnh, chúng liền tan chảy;",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.285Z",
      "updatedAt": "2025-04-24T02:00:32.285Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "30d5d338-4204-4106-9d59-8ef72ec29112",
      "number": 18,
      "content": "Ngài khiến gió thổi qua, nước chảy xuống thành dòng.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.296Z",
      "updatedAt": "2025-04-24T02:00:32.296Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "767e3a61-6327-4bea-8946-225bedb3807c",
      "number": 19,
      "content": "Ngài tuyên bố lời Ngài cho Gia-cốp,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.308Z",
      "updatedAt": "2025-04-24T02:00:32.308Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "b30e8305-24ce-490d-b5fe-db6ba99d481c",
      "number": 19,
      "content": "Luật lệ Ngài và mạng lịnh Ngài cho I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.318Z",
      "updatedAt": "2025-04-24T02:00:32.318Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "80ddf15f-bdbf-4bf0-8672-a3c22deb986e",
      "number": 20,
      "content": "Ngài chẳng làm như vậy cho một dân nào khác;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.328Z",
      "updatedAt": "2025-04-24T02:00:32.328Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "7e9684d7-aeb0-413e-b30e-50555ca7eae4",
      "number": 20,
      "content": "Họ chẳng biết gì về các mạng lịnh của Ngài.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.338Z",
      "updatedAt": "2025-04-24T02:00:32.338Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    },
    {
      "id": "92bcd1e6-6fc3-4041-918b-ab6e0c3e1edc",
      "number": 20,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:00:32.349Z",
      "updatedAt": "2025-04-24T02:00:32.349Z",
      "chapterId": "c865c8c5-bbc8-4d63-b242-90429f09f685"
    }
