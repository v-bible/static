{
  "chapter": {
    "id": "6683ee82-8cf5-4d02-a927-8b5238245b8f",
    "number": 147,
    "ref": "https://www.bible.com/bible/19/PSA.147.BD2011",
    "createdAt": "2025-04-22T10:10:27.287Z",
    "updatedAt": "2025-04-22T10:10:27.287Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "fa0181a8-c115-4397-9299-156e78f51869",
      "number": 1,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.784Z",
      "updatedAt": "2025-04-22T10:45:29.784Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "39bbc9c4-adca-4dc7-9321-f90e83b36f58",
      "number": 20,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.380Z",
      "updatedAt": "2025-04-22T10:45:30.380Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "d8d6f0df-7d15-4d7d-bfb4-e4a097f7face",
      "number": 1,
      "content": "Ca hát chúc tụng Đức Chúa Trời chúng ta là điều tốt đẹp;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.846Z",
      "updatedAt": "2025-04-22T10:45:29.846Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "f5e51abb-72f8-412d-a37c-2c10820ee51f",
      "number": 1,
      "content": "Vì tôn ngợi Ngài là điều vui thỏa và hợp lý dường nào!",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.857Z",
      "updatedAt": "2025-04-22T10:45:29.857Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "ca5e8b94-6ab9-4ae4-96e2-ca8bf4564481",
      "number": 2,
      "content": "**CHÚA** xây dựng lại Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.870Z",
      "updatedAt": "2025-04-22T10:45:29.870Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "6850701c-f18a-4abb-b217-e1b6b563e0e3",
      "number": 2,
      "content": "Ngài gom nhóm những người bị tản lạc của I-sơ-ra-ên trở về.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.883Z",
      "updatedAt": "2025-04-22T10:45:29.883Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "b1e67b91-9954-4890-8ff2-814aae705e3e",
      "number": 3,
      "content": "Ngài rịt lành những tâm hồn tan vỡ;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.896Z",
      "updatedAt": "2025-04-22T10:45:29.896Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "f7d933da-8b69-4dee-a873-aa02f2e634d7",
      "number": 3,
      "content": "Và băng bó các vết thương của họ.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.909Z",
      "updatedAt": "2025-04-22T10:45:29.909Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "620330cf-28a3-43c8-ac96-ca4b0b498a5c",
      "number": 4,
      "content": "Ngài định số lượng các vì sao;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.926Z",
      "updatedAt": "2025-04-22T10:45:29.926Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "8300cb13-4d56-4155-87a6-4ff77b453239",
      "number": 4,
      "content": "Ngài gọi chúng theo tên riêng của từng vì sao một.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.943Z",
      "updatedAt": "2025-04-22T10:45:29.943Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "4144b68e-a5c6-43b7-8ddc-02225b64fb6a",
      "number": 5,
      "content": "**CHÚA** chúng ta vĩ đại biết bao!",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.956Z",
      "updatedAt": "2025-04-22T10:45:29.956Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "1a1645f5-85bf-4492-80bf-5b79dabcc19a",
      "number": 5,
      "content": "Quyền năng Ngài thật vô hạn. Kiến thức Ngài quả vô cùng.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.969Z",
      "updatedAt": "2025-04-22T10:45:29.969Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "19ba8654-7c79-43f0-9afd-3ca943d0f8e5",
      "number": 6,
      "content": "**CHÚA** nhấc đưa những người nghèo khó lên cao,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.981Z",
      "updatedAt": "2025-04-22T10:45:29.981Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "5d3c27b4-1aad-4d66-8365-759c3af292d5",
      "number": 6,
      "content": "Nhưng Ngài đánh đổ phường gian ác ngã nhào xuống đất.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:29.994Z",
      "updatedAt": "2025-04-22T10:45:29.994Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "b55b4224-b123-43de-b467-c3d8f88a9d09",
      "number": 7,
      "content": "Hãy dâng lên **CHÚA** những lời cảm tạ;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.005Z",
      "updatedAt": "2025-04-22T10:45:30.005Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "3a42672a-2bd1-4864-ac7f-3c4f110656b7",
      "number": 7,
      "content": "Hãy dùng nhạc khí ca ngợi Đức Chúa Trời.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.016Z",
      "updatedAt": "2025-04-22T10:45:30.016Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "8417afd5-5635-483a-b2e2-4ba11f978aab",
      "number": 8,
      "content": "Ngài bao phủ các tầng trời bằng mây;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.046Z",
      "updatedAt": "2025-04-22T10:45:30.046Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "6c9c4891-dfbb-4fdd-806e-c0fd0dbfbd80",
      "number": 8,
      "content": "Ngài chuẩn bị các trận mưa cho đất;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.059Z",
      "updatedAt": "2025-04-22T10:45:30.059Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "12950c4c-3ef2-4ddc-a356-d625e3976f2e",
      "number": 8,
      "content": "Ngài khiến cỏ mọc trên các núi đồi.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.073Z",
      "updatedAt": "2025-04-22T10:45:30.073Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "90c9f84e-e4f5-400c-9d2d-0b306b552431",
      "number": 9,
      "content": "Ngài ban thực phẩm cho muôn loài cầm thú;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.086Z",
      "updatedAt": "2025-04-22T10:45:30.086Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "042435bf-439f-4ab0-8407-fe6f875bf2bf",
      "number": 9,
      "content": "Ngài ban thức ăn cho các quạ con đói bụng kêu gào.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.099Z",
      "updatedAt": "2025-04-22T10:45:30.099Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "cd7f7c52-26d0-4088-850a-e86ebf110a94",
      "number": 10,
      "content": "Ngài chẳng hài lòng về sức lực của ngựa chiến;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.113Z",
      "updatedAt": "2025-04-22T10:45:30.113Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "c4adf258-c27f-4497-b39b-46068b94c6b7",
      "number": 10,
      "content": "Ngài không vui về đôi chân nhanh nhẹn của loài người;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.126Z",
      "updatedAt": "2025-04-22T10:45:30.126Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "adb84df8-3926-4332-999e-f6bc3f16ed56",
      "number": 11,
      "content": "Nhưng **CHÚA** rất vui lòng về những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.141Z",
      "updatedAt": "2025-04-22T10:45:30.141Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "b89fe6c5-9929-433c-a852-85b636c00dd8",
      "number": 11,
      "content": "Những người đặt hy vọng vào tình thương của Ngài.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.153Z",
      "updatedAt": "2025-04-22T10:45:30.153Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "8a921c74-68c8-4928-b803-b10fa656a8ab",
      "number": 12,
      "content": "Hỡi Giê-ru-sa-lem, hãy ca ngợi **CHÚA**!",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.163Z",
      "updatedAt": "2025-04-22T10:45:30.163Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "7d810041-e69d-4cdc-9594-e2084bf7cf35",
      "number": 12,
      "content": "Hỡi Si-ôn, hãy tôn ngợi Đức Chúa Trời!",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.173Z",
      "updatedAt": "2025-04-22T10:45:30.173Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "1957042e-5fc5-45cc-9e74-1a7bb3dc305f",
      "number": 13,
      "content": "Vì Ngài làm vững mạnh các song sắt ở các cổng thành của ngươi;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.183Z",
      "updatedAt": "2025-04-22T10:45:30.183Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "520a287d-0e3d-486f-871d-d8603b1c7b04",
      "number": 13,
      "content": "Ngài ban phước cho con dân ngươi sống trong ngươi.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.193Z",
      "updatedAt": "2025-04-22T10:45:30.193Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "dbc36028-4244-4a74-8703-7e79f33f0a0d",
      "number": 14,
      "content": "Ngài ban hòa bình nơi các biên giới ngươi;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.203Z",
      "updatedAt": "2025-04-22T10:45:30.203Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "968cc721-126c-487a-aefc-23436c639cb7",
      "number": 14,
      "content": "Ngài cho ngươi no thỏa bằng gạo ngon hảo hạng.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.213Z",
      "updatedAt": "2025-04-22T10:45:30.213Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "e5d1475b-271a-4ee1-b129-fd23473196d3",
      "number": 15,
      "content": "Ngài truyền lịnh của Ngài cho trái đất;",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.223Z",
      "updatedAt": "2025-04-22T10:45:30.223Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "3b071e57-4e83-4958-8c91-f153cd36551d",
      "number": 15,
      "content": "Lời Ngài phán ra được cấp tốc truyền đi.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.233Z",
      "updatedAt": "2025-04-22T10:45:30.233Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "d726105d-1224-4765-bf15-aff0bf5d3676",
      "number": 16,
      "content": "Ngài ban tuyết xuống phủ trắng như lông chiên;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.245Z",
      "updatedAt": "2025-04-22T10:45:30.245Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "a8cdcae8-f8de-4181-b9cb-3a046c016cfc",
      "number": 16,
      "content": "Ngài rải sương giá ra như tro bụi.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.259Z",
      "updatedAt": "2025-04-22T10:45:30.259Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "068db762-496d-4830-8ab6-359fd796ab66",
      "number": 17,
      "content": "Ngài quăng băng giá ra từng mảng;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.272Z",
      "updatedAt": "2025-04-22T10:45:30.272Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "8838b7d8-fdf7-4e24-b7fd-e88b7d390fe6",
      "number": 17,
      "content": "Ai có thể chịu nổi cơn buốt giá của Ngài?",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.286Z",
      "updatedAt": "2025-04-22T10:45:30.286Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "5a7ee67d-8b20-406a-9bce-8d53c25c0d0e",
      "number": 18,
      "content": "Ngài ban lịnh, chúng liền tan chảy;",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.299Z",
      "updatedAt": "2025-04-22T10:45:30.299Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "14247cf8-443f-4e1b-8994-66a09449c476",
      "number": 18,
      "content": "Ngài khiến gió thổi qua, nước chảy xuống thành dòng.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.313Z",
      "updatedAt": "2025-04-22T10:45:30.313Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "c283882b-6751-465f-8bc2-0750a3b59b5d",
      "number": 19,
      "content": "Ngài tuyên bố lời Ngài cho Gia-cốp,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.326Z",
      "updatedAt": "2025-04-22T10:45:30.326Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "8e4e0e74-9b01-4b34-8aff-6723326c2bb5",
      "number": 19,
      "content": "Luật lệ Ngài và mạng lịnh Ngài cho I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.340Z",
      "updatedAt": "2025-04-22T10:45:30.340Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "1fb2a2ba-291c-4d6f-8e68-6c37a85f48ca",
      "number": 20,
      "content": "Ngài chẳng làm như vậy cho một dân nào khác;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.353Z",
      "updatedAt": "2025-04-22T10:45:30.353Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    },
    {
      "id": "3e2e9fa5-f5e8-4b31-bb35-14930af52c05",
      "number": 20,
      "content": "Họ chẳng biết gì về các mạng lịnh của Ngài.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:45:30.366Z",
      "updatedAt": "2025-04-22T10:45:30.366Z",
      "chapterId": "6683ee82-8cf5-4d02-a927-8b5238245b8f"
    }
  ],
