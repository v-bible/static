{
  "chapter": {
    "id": "a41f6c19-6a62-4b30-8b8a-a2414c122046",
    "number": 120,
    "ref": "https://www.bible.com/bible/19/PSA.120.BD2011",
    "createdAt": "2025-04-22T10:10:27.001Z",
    "updatedAt": "2025-04-22T10:10:27.001Z",
    "bookId": "287f96f5-4a40-46df-b6a0-89edcd2581a9"
  },
  "verses": [
    {
      "id": "90754ae7-11b0-423e-aab1-20da85a19cb7",
      "number": 1,
      "content": "Trong cơn hoạn nạn tôi kêu cầu cùng **CHÚA**,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.753Z",
      "updatedAt": "2025-04-22T10:43:41.753Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "989c8312-cbd2-4b7e-89d0-de03b13248cd",
      "number": 1,
      "content": "Và Ngài đã đáp lời tôi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.814Z",
      "updatedAt": "2025-04-22T10:43:41.814Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "393abf1e-43a1-48c3-964f-456df8a19704",
      "number": 2,
      "content": "**CHÚA** ôi, xin giải cứu linh hồn con",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.825Z",
      "updatedAt": "2025-04-22T10:43:41.825Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "5e32f271-9f88-458f-9125-822652baa9b1",
      "number": 2,
      "content": "Khỏi môi dối trá,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.838Z",
      "updatedAt": "2025-04-22T10:43:41.838Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "57faae47-0a5a-484c-8b37-b65847c478d8",
      "number": 2,
      "content": "Và khỏi lưỡi dối lừa.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.851Z",
      "updatedAt": "2025-04-22T10:43:41.851Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "c3bf5c7d-c271-4177-b7f5-20636f836334",
      "number": 3,
      "content": "Hỡi lưỡi dối trá,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.864Z",
      "updatedAt": "2025-04-22T10:43:41.864Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "3cb5e178-d2e9-469e-8a3c-2085b75c54da",
      "number": 3,
      "content": "Ngài sẽ làm gì cho ngươi?",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.878Z",
      "updatedAt": "2025-04-22T10:43:41.878Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "5283d25f-1ae3-4b02-a21d-ce2524e44b2b",
      "number": 3,
      "content": "Ngài sẽ làm gì nữa cho ngươi?",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.891Z",
      "updatedAt": "2025-04-22T10:43:41.891Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "8804b6d8-847b-487e-b750-3e820795e796",
      "number": 4,
      "content": "Ngài sẽ dùng những tên nhọn của người dũng sĩ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.904Z",
      "updatedAt": "2025-04-22T10:43:41.904Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "4d25ab6a-c6b4-4a38-b3e1-dc9f4e80d996",
      "number": 4,
      "content": "Vàn hững than lửa hừng của bụi cây chổi cho ngươi.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.920Z",
      "updatedAt": "2025-04-22T10:43:41.920Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "caa9672a-9634-4a5e-a709-87b37bc83ef5",
      "number": 5,
      "content": "Khốn thay cho tôi vì tôi trú ngụ ở Mê-sếch,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.934Z",
      "updatedAt": "2025-04-22T10:43:41.934Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "6634be93-4961-4ff2-8ae5-a9b36ca4e2a7",
      "number": 5,
      "content": "Vì tôi cư ngụ giữa các trại của Kê-đa.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.947Z",
      "updatedAt": "2025-04-22T10:43:41.947Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "5f1fe9c7-160f-4db9-b418-1250961ed427",
      "number": 6,
      "content": "Linh hồn tôi đã sống quá lâu",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.960Z",
      "updatedAt": "2025-04-22T10:43:41.960Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "35aeb4af-b779-4510-8dae-3f0adc263521",
      "number": 6,
      "content": "Với những kẻ ghét hòa bình.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.976Z",
      "updatedAt": "2025-04-22T10:43:41.976Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "61a1c170-6d09-47d6-b4b4-9883407432f3",
      "number": 7,
      "content": "Tôi yêu chuộng hòa bình,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:41.989Z",
      "updatedAt": "2025-04-22T10:43:41.989Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    },
    {
      "id": "cfc5fa2b-4fc9-47b2-a1c3-907c9553f689",
      "number": 7,
      "content": "Nhưng khi tôi nói đến hòa bình, chúng chỉ muốn chiến tranh.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:43:42.002Z",
      "updatedAt": "2025-04-22T10:43:42.002Z",
      "chapterId": "a41f6c19-6a62-4b30-8b8a-a2414c122046"
    }
  ],
