{
  "chapter": {
    "id": "1d3912b3-d926-4f58-bff8-2cb1f7d25766",
    "number": 120,
    "ref": "https://www.bible.com/bible/19/PSA.120.BD2011",
    "createdAt": "2024-09-23T05:08:40.622Z",
    "updatedAt": "2024-09-23T05:08:40.622Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "f699bac7-d289-47aa-ba63-d7e67de9759f",
      "number": 1,
      "content": "Trong cơn hoạn nạn tôi kêu cầu cùng **CHÚA**,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.784Z",
      "updatedAt": "2024-09-23T05:54:36.784Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "4d0a1bec-862e-453b-b09c-972537ab8b85",
      "number": 1,
      "content": "Và Ngài đã đáp lời tôi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.812Z",
      "updatedAt": "2024-09-23T05:54:36.812Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "6d040fcb-6a0f-4eaf-b6ee-6e8d644d95ea",
      "number": 2,
      "content": "**CHÚA** ôi, xin giải cứu linh hồn con",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.817Z",
      "updatedAt": "2024-09-23T05:54:36.817Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "3c6c7a9a-a568-49ee-bdb6-3467389495df",
      "number": 2,
      "content": "Khỏi môi dối trá,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.822Z",
      "updatedAt": "2024-09-23T05:54:36.822Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "b6d05ca0-71f5-4b0d-a262-eb7ffdb7f13a",
      "number": 2,
      "content": "Và khỏi lưỡi dối lừa.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.827Z",
      "updatedAt": "2024-09-23T05:54:36.827Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "497d5181-cfe0-4437-a310-800779e296e5",
      "number": 3,
      "content": "Hỡi lưỡi dối trá,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.831Z",
      "updatedAt": "2024-09-23T05:54:36.831Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "92737836-214b-4ab8-94d8-132d471aa2fd",
      "number": 3,
      "content": "Ngài sẽ làm gì cho ngươi?",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.836Z",
      "updatedAt": "2024-09-23T05:54:36.836Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "f6d12c6f-9ee0-4c9e-b50d-b9b85d4a0784",
      "number": 3,
      "content": "Ngài sẽ làm gì nữa cho ngươi?",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.841Z",
      "updatedAt": "2024-09-23T05:54:36.841Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "3993168b-f582-435d-b395-20c3316a4e86",
      "number": 4,
      "content": "Ngài sẽ dùng những tên nhọn của người dũng sĩ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.846Z",
      "updatedAt": "2024-09-23T05:54:36.846Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "0eebbe2a-d5df-4997-8661-916b7e3b091a",
      "number": 4,
      "content": "Vàn hững than lửa hừng của bụi cây chổi cho ngươi.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.851Z",
      "updatedAt": "2024-09-23T05:54:36.851Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "963f52b3-0827-4d5c-bda0-a763128e6baa",
      "number": 5,
      "content": "Khốn thay cho tôi vì tôi trú ngụ ở Mê-sếch,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.856Z",
      "updatedAt": "2024-09-23T05:54:36.856Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "20ddd620-4db9-4b8d-a395-ba840f4a51f2",
      "number": 5,
      "content": "Vì tôi cư ngụ giữa các trại của Kê-đa.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.861Z",
      "updatedAt": "2024-09-23T05:54:36.861Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "9e85317e-5323-407b-92c7-a937f83c766e",
      "number": 6,
      "content": "Linh hồn tôi đã sống quá lâu",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.865Z",
      "updatedAt": "2024-09-23T05:54:36.865Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "ea0e0467-9f03-4a2f-b6e2-7fb45510112f",
      "number": 6,
      "content": "Với những kẻ ghét hòa bình.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.870Z",
      "updatedAt": "2024-09-23T05:54:36.870Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "81ff1039-e189-4199-afa9-b219a2e53d88",
      "number": 7,
      "content": "Tôi yêu chuộng hòa bình,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.875Z",
      "updatedAt": "2024-09-23T05:54:36.875Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    },
    {
      "id": "28236946-8c14-4af4-b22f-3aec171f4fc9",
      "number": 7,
      "content": "Nhưng khi tôi nói đến hòa bình, chúng chỉ muốn chiến tranh.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:36.880Z",
      "updatedAt": "2024-09-23T05:54:36.880Z",
      "chapterId": "1d3912b3-d926-4f58-bff8-2cb1f7d25766"
    }
  ],
