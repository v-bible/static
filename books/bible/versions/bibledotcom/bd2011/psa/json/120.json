{
  "chapter": {
    "id": "4731b268-2c25-436b-8242-d7d6068db1d7",
    "number": 120,
    "ref": "https://www.bible.com/bible/19/PSA.120.BD2011",
    "createdAt": "2025-04-24T01:29:07.320Z",
    "updatedAt": "2025-04-24T01:29:07.320Z",
    "bookId": "1be24f59-6084-4aae-9ca9-e1e79d1deaa1"
  },
  "verses": [
    {
      "id": "55054de3-b341-4bfe-b175-efbaf4b225e6",
      "number": 1,
      "content": "Trong cơn hoạn nạn tôi kêu cầu cùng **CHÚA**,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.377Z",
      "updatedAt": "2025-04-24T01:58:38.377Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "4247907f-8595-4052-a08d-eda14c1b0322",
      "number": 1,
      "content": "Và Ngài đã đáp lời tôi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.401Z",
      "updatedAt": "2025-04-24T01:58:38.401Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "06a6534e-2cbb-4062-a05d-ec97a2ab014a",
      "number": 2,
      "content": "**CHÚA** ôi, xin giải cứu linh hồn con",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.411Z",
      "updatedAt": "2025-04-24T01:58:38.411Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "38fcd472-8385-4308-a3ef-a1993e15c8e4",
      "number": 2,
      "content": "Khỏi môi dối trá,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.422Z",
      "updatedAt": "2025-04-24T01:58:38.422Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "34438dfc-81e2-4b6e-8c30-80dd50732f2f",
      "number": 2,
      "content": "Và khỏi lưỡi dối lừa.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.433Z",
      "updatedAt": "2025-04-24T01:58:38.433Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "f2a8c375-9add-4f3e-ba86-b05cbc2c5814",
      "number": 3,
      "content": "Hỡi lưỡi dối trá,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.443Z",
      "updatedAt": "2025-04-24T01:58:38.443Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "71209239-71fe-4474-9bc1-8c7c0a1ceb94",
      "number": 3,
      "content": "Ngài sẽ làm gì cho ngươi?",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.457Z",
      "updatedAt": "2025-04-24T01:58:38.457Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "c099f387-bdf1-4e25-8410-c8cf6f14862f",
      "number": 3,
      "content": "Ngài sẽ làm gì nữa cho ngươi?",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.468Z",
      "updatedAt": "2025-04-24T01:58:38.468Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "e64757bf-f6be-48cb-bd26-05e39310d5b9",
      "number": 4,
      "content": "Ngài sẽ dùng những tên nhọn của người dũng sĩ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.480Z",
      "updatedAt": "2025-04-24T01:58:38.480Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "ddef6397-1783-4465-9aae-d965d63ff3f4",
      "number": 4,
      "content": "Vàn hững than lửa hừng của bụi cây chổi cho ngươi.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.491Z",
      "updatedAt": "2025-04-24T01:58:38.491Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "8fe8f268-6487-42ec-851e-f3ffeff93422",
      "number": 5,
      "content": "Khốn thay cho tôi vì tôi trú ngụ ở Mê-sếch,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.503Z",
      "updatedAt": "2025-04-24T01:58:38.503Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "4a788819-00c4-4702-b240-c49b52e0ef3b",
      "number": 5,
      "content": "Vì tôi cư ngụ giữa các trại của Kê-đa.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.516Z",
      "updatedAt": "2025-04-24T01:58:38.516Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "20d8664c-fb20-46da-82a8-e8eabd38b23c",
      "number": 6,
      "content": "Linh hồn tôi đã sống quá lâu",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.529Z",
      "updatedAt": "2025-04-24T01:58:38.529Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "ffacaf39-34e6-47fb-9dcc-e19947128329",
      "number": 6,
      "content": "Với những kẻ ghét hòa bình.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.543Z",
      "updatedAt": "2025-04-24T01:58:38.543Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "d599ac49-9108-44bc-8686-7a3972ece3bf",
      "number": 7,
      "content": "Tôi yêu chuộng hòa bình,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.556Z",
      "updatedAt": "2025-04-24T01:58:38.556Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    },
    {
      "id": "63b676a1-5513-409c-a274-6106528e66db",
      "number": 7,
      "content": "Nhưng khi tôi nói đến hòa bình, chúng chỉ muốn chiến tranh.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T01:58:38.570Z",
      "updatedAt": "2025-04-24T01:58:38.570Z",
      "chapterId": "4731b268-2c25-436b-8242-d7d6068db1d7"
    }
  ],
