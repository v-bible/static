      "chapterId": "4695e766-00da-41a8-999e-8a3a12b19403"
    },
    {
      "id": "6efbc3a8-f15a-450a-ac39-41b18f8f084a",
      "content": "nghĩa là Giếng của Đấng Hằng Sống thấy tôi",
      "position": 53,
      "order": 2,
      "createdAt": "2025-04-24T01:30:05.619Z",
