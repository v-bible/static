      "chapterId": "7d761ef3-c005-4d7f-8f49-0d9c5e4d3087"
    },
    {
      "id": "cc985d49-5056-4f71-95c1-f1b6387cdbbc",
      "content": "Chúa Hóa Bánh cho Năm Ngàn Người Ăn",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T11:04:18.470Z",
      "updatedAt": "2025-04-22T11:04:18.470Z",
      "verseId": "69df0fb9-c2b6-4048-8bfa-c7b13e5b7c44",
      "chapterId": "7d761ef3-c005-4d7f-8f49-0d9c5e4d3087"
    },
    {
      "id": "c5ad74f3-d7b1-4e3e-bab4-2b1effc1d7eb",
      "content": "Chúa Chữa Bịnh ở Ghê-nê-sa-rết",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T11:04:18.862Z",
      "updatedAt": "2025-04-22T11:04:18.862Z",
      "verseId": "84ee1af3-176f-4fc2-82cf-50ff75678c2d",
      "chapterId": "7d761ef3-c005-4d7f-8f49-0d9c5e4d3087"
    },
    {
      "id": "01c75439-8a74-47ab-bcf9-a5e7e6710105",
      "content": "Giăng Báp-tít Qua Đời",
      "level": 1,
      "order": 0,
