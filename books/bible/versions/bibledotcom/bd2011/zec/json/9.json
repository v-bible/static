    {
      "id": "81cba80c-33ea-4a12-8ec2-f21e3ec3270c",
      "content": "Dân **CHÚA**Được Phục Hồi",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T02:15:33.147Z",
      "updatedAt": "2025-04-24T02:15:33.147Z",
