    },
    {
      "id": "655cb44d-101e-4785-85db-19e8dba65c82",
      "content": "nt: chê-rúp (số ít của chê-ru-bim)",
      "position": 36,
      "order": 1,
      "createdAt": "2024-09-23T05:30:12.118Z",
      "updatedAt": "2024-09-23T05:30:12.118Z",
      "verseId": "b0dab387-7f01-4827-9ca6-9f662b092837",
