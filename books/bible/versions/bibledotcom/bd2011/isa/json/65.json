{
  "chapter": {
    "id": "007964f0-6b03-4fa8-a18a-6add9145ac2b",
    "number": 65,
    "ref": "https://www.bible.com/bible/19/ISA.65.BD2011",
    "createdAt": "2025-04-22T10:10:28.798Z",
    "updatedAt": "2025-04-22T10:10:28.798Z",
    "bookId": "22c0d459-25e6-4624-85e4-8f3b415f7cc7"
  },
  "verses": [
    {
      "id": "2294ce38-cc93-44f9-b17b-1660a17ae96d",
      "number": 1,
      "content": "“Ta đã cho những kẻ chẳng thèm hỏi đến Ta được cầu xin Ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.687Z",
      "updatedAt": "2025-04-22T10:52:06.687Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "096ee219-e5e0-49fa-ba56-f1f92491e196",
      "number": 1,
      "content": "Ta đã cho những kẻ chẳng tìm kiếm Ta được tìm Ta.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.735Z",
      "updatedAt": "2025-04-22T10:52:06.735Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "2f0f04e6-3693-4175-9025-5fc99fd3b109",
      "number": 1,
      "content": "Ta nói với một dân chẳng thèm kêu cầu danh Ta rằng,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.745Z",
      "updatedAt": "2025-04-22T10:52:06.745Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "11621122-0207-4ef1-ba73-d874ac3853e7",
      "number": 1,
      "content": "‘Ta ở đây! Ta ở đây!’",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.756Z",
      "updatedAt": "2025-04-22T10:52:06.756Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "fa610e99-10cb-4286-895f-2e57f283b83a",
      "number": 2,
      "content": "Suốt ngày Ta dang hai tay",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.769Z",
      "updatedAt": "2025-04-22T10:52:06.769Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "1e2d0e84-302d-4709-ac58-240e944da187",
      "number": 2,
      "content": "Mời gọi một dân phản nghịch,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.782Z",
      "updatedAt": "2025-04-22T10:52:06.782Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "0a47beb4-d1b2-4b83-b4e1-fe670ba6a238",
      "number": 2,
      "content": "Một dân cứ bước theo đường lối gian tà,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.795Z",
      "updatedAt": "2025-04-22T10:52:06.795Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "e35e4118-cda6-4c1e-8aaf-ab058dabd312",
      "number": 2,
      "content": "Theo ý riêng mình,",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.808Z",
      "updatedAt": "2025-04-22T10:52:06.808Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "5c2b5e91-2535-4832-83eb-f7549070ae2d",
      "number": 3,
      "content": "Một dân không ngừng chọc giận",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.821Z",
      "updatedAt": "2025-04-22T10:52:06.821Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "8a5b86ff-08e4-4a61-8183-f69ee0f59698",
      "number": 3,
      "content": "Ngay vào mặt Ta,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.832Z",
      "updatedAt": "2025-04-22T10:52:06.832Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "3ecb658a-b850-4978-98ca-271458324bf0",
      "number": 3,
      "content": "Chúng dâng con vật hiến tế cho tà thần trong các vườn,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.844Z",
      "updatedAt": "2025-04-22T10:52:06.844Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "96ce0545-aa08-40e9-aa4b-d47fe6569377",
      "number": 3,
      "content": "Và dâng hương cho các thần tượng trên những bàn thờ bằng gạch;",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.855Z",
      "updatedAt": "2025-04-22T10:52:06.855Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "b8a7b355-26ec-4a36-a5d1-9ff5d91ffa9f",
      "number": 4,
      "content": "Chúng ngồi trầm tư mặc tưởng giữa các mồ mả,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.865Z",
      "updatedAt": "2025-04-22T10:52:06.865Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "12ebf6e4-735e-44dc-9791-1b6ab3617f48",
      "number": 4,
      "content": "Và thức thâu đêm tĩnh tọa nơi mộ địa âm u.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.875Z",
      "updatedAt": "2025-04-22T10:52:06.875Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "ea7a5537-74a4-4e1e-abcf-ba5f5af53b86",
      "number": 4,
      "content": "Chúng ăn thịt heo ô uế;",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.886Z",
      "updatedAt": "2025-04-22T10:52:06.886Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "3c0412bc-0f0d-41dd-be72-508a580c7116",
      "number": 4,
      "content": "Bát đĩa chúng chứa đầy các thức ăn gớm ghiếc.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.897Z",
      "updatedAt": "2025-04-22T10:52:06.897Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "3ec45dd6-c7a4-4940-9930-8db2f68095c9",
      "number": 5,
      "content": "Chúng là những kẻ đã nói,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.907Z",
      "updatedAt": "2025-04-22T10:52:06.907Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "4adbe227-2758-4049-a03f-8dc83b176ba3",
      "number": 5,
      "content": "‘Hãy đứng xa ra, chớ lại gần ta, vì ta thánh khiết hơn ngươi!’",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.919Z",
      "updatedAt": "2025-04-22T10:52:06.919Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "5c194425-9d25-4d67-a2bc-bc405788d85b",
      "number": 5,
      "content": "Những điều ấy làm cho khói giận của Ta bốc ra nơi lỗ mũi,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.932Z",
      "updatedAt": "2025-04-22T10:52:06.932Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "3d8d9fff-8ec5-4b96-a3fd-35aaf9018ab1",
      "number": 5,
      "content": "Như lửa hừng cháy hừng hực suốt ngày.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.945Z",
      "updatedAt": "2025-04-22T10:52:06.945Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "bd5eaa40-4d37-4b48-8cae-f9319614d2bb",
      "number": 6,
      "content": "Này, những việc ấy đã được ghi lại trước mặt Ta;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.961Z",
      "updatedAt": "2025-04-22T10:52:06.961Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "afeab1cd-a6df-4329-bc0c-922c632183f3",
      "number": 6,
      "content": "Ta sẽ không im lặng, nhưng sẽ báo trả,",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:06.978Z",
      "updatedAt": "2025-04-22T10:52:06.978Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "9b12282a-ef35-412f-bffc-09302668baf1",
      "number": 6,
      "content": "Ta sẽ báo trả ngay vào lòng chúng;",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.001Z",
      "updatedAt": "2025-04-22T10:52:07.001Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "52f11646-dfa0-49f5-b7b9-3e81a9683ecb",
      "number": 7,
      "content": "Ta sẽ báo trả tội của các ngươi và tội của tổ tiên các ngươi nữa,”",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.017Z",
      "updatedAt": "2025-04-22T10:52:07.017Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "d7ccc328-00f1-41c8-bca3-e0ef30c8ea17",
      "number": 7,
      "content": "**CHÚA** phán.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.030Z",
      "updatedAt": "2025-04-22T10:52:07.030Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "30837e28-bc84-425b-bcdf-79037e157d11",
      "number": 25,
      "content": "**CHÚA** phán.",
      "order": 5,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.157Z",
      "updatedAt": "2025-04-22T10:52:08.157Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "400168d1-f593-4924-97b3-f18acecd622e",
      "number": 7,
      "content": "“Chúng đã dâng hương cho các tà thần trên các núi;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.043Z",
      "updatedAt": "2025-04-22T10:52:07.043Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "d4de8f51-d415-40a9-83c0-3f6f8fa51233",
      "number": 7,
      "content": "Chúng đã xúc phạm đến Ta trên các ngọn đồi,",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.057Z",
      "updatedAt": "2025-04-22T10:52:07.057Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "21d47302-df70-483a-97c7-803e4145db60",
      "number": 7,
      "content": "Nên Ta sẽ lường theo những việc chúng làm khi trước,",
      "order": 4,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.070Z",
      "updatedAt": "2025-04-22T10:52:07.070Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "f7121dde-f71e-43ea-a97c-76ea128a7f75",
      "number": 7,
      "content": "Mà báo trả đích đáng vào lòng chúng.”",
      "order": 5,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.083Z",
      "updatedAt": "2025-04-22T10:52:07.083Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "8145c28b-66f8-46e3-968f-60a7adde451f",
      "number": 8,
      "content": "**CHÚA** phán thế này:",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.097Z",
      "updatedAt": "2025-04-22T10:52:07.097Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "b0cc6b9d-5740-4ece-bf3d-980f99c44c14",
      "number": 8,
      "content": "“Như rượu mới còn tìm thấy trong chùm nho héo,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.110Z",
      "updatedAt": "2025-04-22T10:52:07.110Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "5c292041-d938-43c6-b4c6-bda2dd048645",
      "number": 8,
      "content": "Người ta bảo rằng, ‘Đừng bỏ nó đi,",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.123Z",
      "updatedAt": "2025-04-22T10:52:07.123Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "6ae56a62-17fc-42fd-815b-bd15a3b9dc2b",
      "number": 8,
      "content": "Vì trong đó còn tìm thấy chút phước lành,’",
      "order": 3,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.136Z",
      "updatedAt": "2025-04-22T10:52:07.136Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "43a14f9b-268f-412e-8d37-12072730ce4e",
      "number": 8,
      "content": "Thì vì cớ các tôi tớ Ta, Ta sẽ làm như vậy;",
      "order": 4,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.153Z",
      "updatedAt": "2025-04-22T10:52:07.153Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "4c7fa977-7d00-4b90-87d5-aaccbc3a2e6f",
      "number": 8,
      "content": "Ta sẽ không tiêu diệt hết chúng đâu.",
      "order": 5,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.169Z",
      "updatedAt": "2025-04-22T10:52:07.169Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "b522a9f5-3837-4a6f-9e3f-291dbaf9ec46",
      "number": 9,
      "content": "Từ Gia-cốp Ta sẽ cho phát sinh một dòng dõi;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.182Z",
      "updatedAt": "2025-04-22T10:52:07.182Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "e13af18a-b2ae-48e5-868e-299bc49324d5",
      "number": 9,
      "content": "Từ Giu-đa sẽ ra một người thừa kế các núi thánh của Ta;",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.195Z",
      "updatedAt": "2025-04-22T10:52:07.195Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "e6d92e5f-9cc5-408f-b449-e49c356d0d0c",
      "number": 9,
      "content": "Người Ta chọn sẽ thừa hưởng cơ nghiệp đó;",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.209Z",
      "updatedAt": "2025-04-22T10:52:07.209Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "0756d19a-9bf2-4612-84c7-a439e833582a",
      "number": 9,
      "content": "Các tôi tớ Ta sẽ cư ngụ ở đó.",
      "order": 3,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.223Z",
      "updatedAt": "2025-04-22T10:52:07.223Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "1892d945-fe4b-4e68-8d7d-4210164fa493",
      "number": 10,
      "content": "Sa-rôn sẽ là đồng cỏ cho các bầy súc vật;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.237Z",
      "updatedAt": "2025-04-22T10:52:07.237Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "c24fdcd0-17df-4c8e-874a-7cc4b6fd4158",
      "number": 10,
      "content": "Thung lũng A-cơ sẽ là chỗ cho các đàn bò nằm,",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.250Z",
      "updatedAt": "2025-04-22T10:52:07.250Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "cad9e588-a5ae-436b-b776-bf80df2685a1",
      "number": 10,
      "content": "Vì dân Ta đã tìm kiếm Ta.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.263Z",
      "updatedAt": "2025-04-22T10:52:07.263Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "77680390-7056-49bc-b206-99e813696e7a",
      "number": 11,
      "content": "Nhưng hỡi các ngươi là những kẻ bỏ **CHÚA**,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.276Z",
      "updatedAt": "2025-04-22T10:52:07.276Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "350321d8-8681-441f-b522-c173ac1190fe",
      "number": 11,
      "content": "Những kẻ quên núi thánh của Ta,",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.290Z",
      "updatedAt": "2025-04-22T10:52:07.290Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "035a90f3-d92c-4de7-abd5-b6c489d385f7",
      "number": 11,
      "content": "Những kẻ lập bàn thờ cho Thần May Mắn,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.303Z",
      "updatedAt": "2025-04-22T10:52:07.303Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "f870636d-ec65-45cc-9dab-ee56b4084203",
      "number": 11,
      "content": "Dâng rượu pha cho Thần Định Mệnh,",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.330Z",
      "updatedAt": "2025-04-22T10:52:07.330Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "0bd390c9-c749-48c2-9e8a-1ca7cee04f2f",
      "number": 12,
      "content": "Vì thế Ta sẽ định phần số các ngươi cho gươm giáo;",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.357Z",
      "updatedAt": "2025-04-22T10:52:07.357Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "1ce476c4-4c14-40ae-97f4-909b63b92ba4",
      "number": 12,
      "content": "Tất cả các ngươi sẽ quỳ xuống trước kẻ giết mình,",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.370Z",
      "updatedAt": "2025-04-22T10:52:07.370Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "7749fc7c-cffe-4aa4-9c3a-e6080ab21710",
      "number": 12,
      "content": "Vì khi Ta gọi, các ngươi chẳng thèm đáp lại;",
      "order": 2,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.384Z",
      "updatedAt": "2025-04-22T10:52:07.384Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "be9245c6-3d4e-4870-8bb4-a1200578512b",
      "number": 12,
      "content": "Khi Ta phán, các ngươi chẳng muốn lắng nghe;",
      "order": 3,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.400Z",
      "updatedAt": "2025-04-22T10:52:07.400Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "cd35ffc1-30f5-46ab-9d38-609133f7adb1",
      "number": 12,
      "content": "Nhưng các ngươi cứ làm điều ác trước mắt Ta,",
      "order": 4,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.413Z",
      "updatedAt": "2025-04-22T10:52:07.413Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "38ae0cb7-9f3f-4b1b-8d50-5f693e064ef3",
      "number": 12,
      "content": "Và cứ chọn làm những điều Ta không thích.”",
      "order": 5,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.427Z",
      "updatedAt": "2025-04-22T10:52:07.427Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "f7179447-5d0a-4f14-935b-2ed798941fc4",
      "number": 13,
      "content": "Vì thế **CHÚA** Hằng Hữu phán thế này:",
      "order": 0,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.440Z",
      "updatedAt": "2025-04-22T10:52:07.440Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "033de7eb-0659-4ce2-81a1-4b770aa4a2f1",
      "number": 13,
      "content": "“Này, các tôi tớ Ta sẽ ăn,",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.454Z",
      "updatedAt": "2025-04-22T10:52:07.454Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "65e4160e-e4e4-4025-a930-c5016d411036",
      "number": 13,
      "content": "Nhưng các ngươi sẽ đói;",
      "order": 2,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.467Z",
      "updatedAt": "2025-04-22T10:52:07.467Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "e64df72d-9913-40ff-9052-577651680f5f",
      "number": 13,
      "content": "Này, các tôi tớ Ta sẽ uống,",
      "order": 3,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.480Z",
      "updatedAt": "2025-04-22T10:52:07.480Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "7238f624-b6a8-486f-b4b3-e3d9c6bbbae2",
      "number": 13,
      "content": "Nhưng các ngươi sẽ khát;",
      "order": 4,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.493Z",
      "updatedAt": "2025-04-22T10:52:07.493Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "a0dd7994-3816-4c8c-af93-73a3dff52a3a",
      "number": 13,
      "content": "Này, các tôi tớ Ta sẽ vui mừng,",
      "order": 5,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.507Z",
      "updatedAt": "2025-04-22T10:52:07.507Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "60425f39-3b4e-4347-919b-7afe217c2158",
      "number": 13,
      "content": "Nhưng các ngươi sẽ tủi hổ;",
      "order": 6,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.520Z",
      "updatedAt": "2025-04-22T10:52:07.520Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "8244f7de-f3b9-4e6b-bc96-59633abd1a4b",
      "number": 14,
      "content": "Này, các tôi tớ Ta sẽ ca hát vì tâm hồn vui vẻ,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.532Z",
      "updatedAt": "2025-04-22T10:52:07.532Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "e7182f7a-8f14-4ae6-86a7-e09c67323673",
      "number": 14,
      "content": "Nhưng các ngươi sẽ khóc than vì sầu thảm trong lòng,",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.545Z",
      "updatedAt": "2025-04-22T10:52:07.545Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "b8ad7a25-974e-48d7-8cf3-4d5089a98252",
      "number": 14,
      "content": "Và các ngươi sẽ khóc lóc vì tâm linh đau đớn.",
      "order": 2,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.558Z",
      "updatedAt": "2025-04-22T10:52:07.558Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "dc55dff3-a639-4d5c-9cde-dad440ae38ae",
      "number": 15,
      "content": "Các ngươi cũng sẽ được lưu danh, nhưng chỉ để làm lời nguyền rủa cho những kẻ Ta chọn,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.572Z",
      "updatedAt": "2025-04-22T10:52:07.572Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "d63852a9-b425-4f7c-a5d9-fafdd9268554",
      "number": 15,
      "content": "Vì **CHÚA** Hằng Hữu sẽ tiêu diệt ngươi,",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.584Z",
      "updatedAt": "2025-04-22T10:52:07.584Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "d9dcd708-7798-4216-aaac-6b39995f491b",
      "number": 15,
      "content": "Nhưng các tôi tớ Ngài, Ngài sẽ gọi họ bằng một danh khác.",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.595Z",
      "updatedAt": "2025-04-22T10:52:07.595Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "2923ba1d-03c2-4613-98e2-5777ae6e5eea",
      "number": 16,
      "content": "Vì vậy, ở trong xứ, ai cầu phúc cho mình",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.606Z",
      "updatedAt": "2025-04-22T10:52:07.606Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "a597b8be-9f46-421d-a3c8-afd111c7b5b3",
      "number": 16,
      "content": "Sẽ nhân danh Đức Chúa Trời của sự thật mà cầu phúc;",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.616Z",
      "updatedAt": "2025-04-22T10:52:07.616Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "84683735-1d7b-4b25-aad0-b84a19c5c359",
      "number": 16,
      "content": "Ở trong xứ, ai tuyên thệ",
      "order": 2,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.627Z",
      "updatedAt": "2025-04-22T10:52:07.627Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "ff1ed20d-5971-4678-b482-2a828d121519",
      "number": 16,
      "content": "Sẽ nhân danh Đức Chúa Trời của sự thật mà thề,",
      "order": 3,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.644Z",
      "updatedAt": "2025-04-22T10:52:07.644Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "c3f5b927-9d73-4b6b-b86f-1acdcf1e27c9",
      "number": 16,
      "content": "Vì những gian truân khi trước đã bị quên rồi,",
      "order": 4,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.655Z",
      "updatedAt": "2025-04-22T10:52:07.655Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "1817f41f-9b86-4510-8433-6d802edffd0a",
      "number": 16,
      "content": "Và vì chúng đã bị che khuất khỏi mắt Ta rồi.",
      "order": 5,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.668Z",
      "updatedAt": "2025-04-22T10:52:07.668Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "96b0e111-5c3b-4a63-8966-d73a1f498668",
      "number": 17,
      "content": "Vì này, Ta sẽ dựng nên",
      "order": 0,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.681Z",
      "updatedAt": "2025-04-22T10:52:07.681Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "cb864364-5732-4b1b-9c48-17ca627f276a",
      "number": 17,
      "content": "Trời mới và đất mới;",
      "order": 1,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.707Z",
      "updatedAt": "2025-04-22T10:52:07.707Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "a0d48d48-0f16-4c6e-9ec4-c91a374c1cf2",
      "number": 17,
      "content": "Những gì trước kia sẽ không còn được nhớ đến nữa,",
      "order": 2,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.720Z",
      "updatedAt": "2025-04-22T10:52:07.720Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "7c1e736b-7599-4620-95b7-b9c9dedf9390",
      "number": 17,
      "content": "Và cũng không còn đến trong trí nữa.",
      "order": 3,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.734Z",
      "updatedAt": "2025-04-22T10:52:07.734Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "0037ec9a-0d94-490b-83b7-c7eb23c71f3a",
      "number": 18,
      "content": "Nhưng hãy hân hoan và vui mừng mãi mãi",
      "order": 0,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.747Z",
      "updatedAt": "2025-04-22T10:52:07.747Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "faaac0fe-fd17-417c-97e1-2f0a16e7e180",
      "number": 18,
      "content": "Về những gì Ta tạo dựng;",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.760Z",
      "updatedAt": "2025-04-22T10:52:07.760Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "c0431035-6a41-403b-af1a-c22b3a47b93c",
      "number": 18,
      "content": "Vì này, Ta dựng nên Giê-ru-sa-lem để làm một niềm vui;",
      "order": 2,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.774Z",
      "updatedAt": "2025-04-22T10:52:07.774Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "41ab9b23-7683-4956-a05c-5224eda4dd01",
      "number": 18,
      "content": "Và dân thành ấy sẽ là một niềm vui thỏa.",
      "order": 3,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.787Z",
      "updatedAt": "2025-04-22T10:52:07.787Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "24a6cf38-847f-480e-acc1-3fbe9909aaf4",
      "number": 19,
      "content": "Ta sẽ vui vẻ vì Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.804Z",
      "updatedAt": "2025-04-22T10:52:07.804Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "9fcb1dd0-4441-43ef-ad3c-5140f48ba30d",
      "number": 19,
      "content": "Ta sẽ vui mừng vì dân Ta;",
      "order": 1,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.817Z",
      "updatedAt": "2025-04-22T10:52:07.817Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "8d0c799b-a18b-4262-994c-b5e1309abad4",
      "number": 19,
      "content": "Tiếng than khóc sẽ không còn nghe trong thành ấy;",
      "order": 2,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.830Z",
      "updatedAt": "2025-04-22T10:52:07.830Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "ff64aef3-31de-44dd-a4da-520f261d6876",
      "number": 19,
      "content": "Tiếng khóc than sẽ vĩnh viễn không còn.",
      "order": 3,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.843Z",
      "updatedAt": "2025-04-22T10:52:07.843Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "3788afad-ebd6-4d2f-97e9-203cba71082c",
      "number": 20,
      "content": "Trong thành ấy trẻ thơ sẽ không còn chết yểu;",
      "order": 0,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.857Z",
      "updatedAt": "2025-04-22T10:52:07.857Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "ba24590d-c8d3-473c-b5fc-1b7440522661",
      "number": 20,
      "content": "Không người già nào chẳng hưởng trọn tuổi thọ của mình;",
      "order": 1,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.870Z",
      "updatedAt": "2025-04-22T10:52:07.870Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "0623ef5d-200e-47c6-98e9-8f94186e89d7",
      "number": 20,
      "content": "Vì ai trăm tuổi mà qua đời sẽ bị xem là chết trẻ;",
      "order": 2,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.889Z",
      "updatedAt": "2025-04-22T10:52:07.889Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "2ee8384b-f7dd-4e2b-a28a-9c21afb2ade8",
      "number": 20,
      "content": "Người chưa được bách niên mà thất lộc sẽ xem như bị rủa nguyền.",
      "order": 3,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.903Z",
      "updatedAt": "2025-04-22T10:52:07.903Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "5823a95b-2e2d-4a19-8000-533690819fc6",
      "number": 21,
      "content": "Chúng sẽ xây nhà và an cư trong các nhà đó;",
      "order": 0,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.919Z",
      "updatedAt": "2025-04-22T10:52:07.919Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "03f76ca1-0fc9-450d-86ff-6c3e11908d60",
      "number": 21,
      "content": "Chúng sẽ trồng vườn nho và được ăn trái mình trồng.",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.932Z",
      "updatedAt": "2025-04-22T10:52:07.932Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "68f4bbc4-940f-4544-a0f3-f0363b630ae0",
      "number": 22,
      "content": "Chúng sẽ không xây nhà để bị người khác vào chiếm ở;",
      "order": 0,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.945Z",
      "updatedAt": "2025-04-22T10:52:07.945Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "ee899d19-7d15-432b-a361-0dc6ee02690b",
      "number": 22,
      "content": "Chúng sẽ không trồng cây để bị người khác đến chiếm ăn.",
      "order": 1,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.958Z",
      "updatedAt": "2025-04-22T10:52:07.958Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "f45f4414-1ce2-42a1-a150-5fae4e1300a4",
      "number": 22,
      "content": "Vì cây cối sống bao nhiêu tuổi, dân Ta cũng sẽ thọ bấy nhiêu.",
      "order": 2,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.972Z",
      "updatedAt": "2025-04-22T10:52:07.972Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "73eb16e8-b50e-4de7-bfd9-ab26317c10fb",
      "number": 22,
      "content": "Những kẻ Ta chọn sẽ hưởng lâu dài mọi kết quả do tay mình làm ra.",
      "order": 3,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:07.985Z",
      "updatedAt": "2025-04-22T10:52:07.985Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "1dd7a80d-ff04-485c-b0e7-e06699767623",
      "number": 23,
      "content": "Chúng sẽ không làm lụng cách luống công vô ích;",
      "order": 0,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.001Z",
      "updatedAt": "2025-04-22T10:52:08.001Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "77ad8fc6-1b5b-4767-b1d5-4902075a13bd",
      "number": 23,
      "content": "Chúng sinh con ra chẳng phải để chuốc lấy âu sầu,",
      "order": 1,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.021Z",
      "updatedAt": "2025-04-22T10:52:08.021Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "48ab3132-bf6d-4445-8d5b-a99bd97848d6",
      "number": 23,
      "content": "Vì con cháu chúng sẽ là những người được **CHÚA** ban phước,",
      "order": 2,
      "parNumber": 95,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.034Z",
      "updatedAt": "2025-04-22T10:52:08.034Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "94d5b080-83af-4dbb-b339-b71468b25c27",
      "number": 23,
      "content": "Và dòng dõi chúng cùng với chúng nữa.",
      "order": 3,
      "parNumber": 96,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.047Z",
      "updatedAt": "2025-04-22T10:52:08.047Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "67e64d52-0ecf-4f72-82f2-ce69c44805a2",
      "number": 24,
      "content": "Rồi đây, trước khi chúng cầu xin, Ta đã trả lời;",
      "order": 0,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.060Z",
      "updatedAt": "2025-04-22T10:52:08.060Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "328bafe4-b034-4b79-b163-d62f27a5a530",
      "number": 24,
      "content": "Khi chúng còn đang nói, Ta đã nghe rõ hết.",
      "order": 1,
      "parNumber": 98,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.073Z",
      "updatedAt": "2025-04-22T10:52:08.073Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "2dd87518-65d6-4ecb-98b3-c4462b49ee0b",
      "number": 25,
      "content": "Chó sói và chiên con sẽ ăn cỏ với nhau,",
      "order": 0,
      "parNumber": 99,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.086Z",
      "updatedAt": "2025-04-22T10:52:08.086Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "c84fac5c-705c-4541-ac74-7388d4ce9f46",
      "number": 25,
      "content": "Sư tử sẽ ăn rơm rạ như bò,",
      "order": 1,
      "parNumber": 100,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.104Z",
      "updatedAt": "2025-04-22T10:52:08.104Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "7ebcd7aa-b11c-4f88-8926-d7cc386202c6",
      "number": 25,
      "content": "Còn bụi đất sẽ là thức ăn của loài rắn;",
      "order": 2,
      "parNumber": 101,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.117Z",
      "updatedAt": "2025-04-22T10:52:08.117Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "0bff85bf-7868-4553-8350-8c5be9a9e9c5",
      "number": 25,
      "content": "Chúng sẽ không gây đau thương hay hủy diệt",
      "order": 3,
      "parNumber": 102,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.130Z",
      "updatedAt": "2025-04-22T10:52:08.130Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    },
    {
      "id": "c6c5074f-c01f-410f-9c68-0bd695610772",
      "number": 25,
      "content": "Trong khắp núi thánh của Ta,”",
      "order": 4,
      "parNumber": 103,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:52:08.144Z",
      "updatedAt": "2025-04-22T10:52:08.144Z",
      "chapterId": "007964f0-6b03-4fa8-a18a-6add9145ac2b"
    }
  ],
