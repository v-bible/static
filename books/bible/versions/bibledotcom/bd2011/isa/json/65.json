{
  "chapter": {
    "id": "d0973f4c-9176-4501-a71f-c86830ceff24",
    "number": 65,
    "ref": "https://www.bible.com/bible/19/ISA.65.BD2011",
    "createdAt": "2025-04-24T01:29:09.290Z",
    "updatedAt": "2025-04-24T01:29:09.290Z",
    "bookId": "be27ebcc-122e-43b3-8087-1ceb029435b3"
  },
  "verses": [
    {
      "id": "df97a047-abb0-4d2a-99a9-6aa6c4995f25",
      "number": 1,
      "content": "“Ta đã cho những kẻ chẳng thèm hỏi đến Ta được cầu xin Ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.188Z",
      "updatedAt": "2025-04-24T02:06:29.188Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "336d8688-823b-49c6-a721-b344ecf0ae95",
      "number": 1,
      "content": "Ta đã cho những kẻ chẳng tìm kiếm Ta được tìm Ta.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.209Z",
      "updatedAt": "2025-04-24T02:06:29.209Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "9aaea7ae-1136-4fa5-9486-45e04f428345",
      "number": 1,
      "content": "Ta nói với một dân chẳng thèm kêu cầu danh Ta rằng,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.219Z",
      "updatedAt": "2025-04-24T02:06:29.219Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "4c2cd108-7779-47da-9aa6-6881bc0682b9",
      "number": 1,
      "content": "‘Ta ở đây! Ta ở đây!’",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.228Z",
      "updatedAt": "2025-04-24T02:06:29.228Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "cc8dd2ae-fc82-4580-b49d-cb0d113d7b78",
      "number": 2,
      "content": "Suốt ngày Ta dang hai tay",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.238Z",
      "updatedAt": "2025-04-24T02:06:29.238Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "8a132306-0545-4dc3-83ea-1a48bfbc1435",
      "number": 2,
      "content": "Mời gọi một dân phản nghịch,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.248Z",
      "updatedAt": "2025-04-24T02:06:29.248Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "2c148e00-710a-47e5-a59b-51165a6dc301",
      "number": 2,
      "content": "Một dân cứ bước theo đường lối gian tà,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.258Z",
      "updatedAt": "2025-04-24T02:06:29.258Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "323adfdb-66b0-4fd2-a4c5-cc577af9e2a2",
      "number": 2,
      "content": "Theo ý riêng mình,",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.267Z",
      "updatedAt": "2025-04-24T02:06:29.267Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "9e94e70d-5130-4cc7-9ab2-d08152250454",
      "number": 3,
      "content": "Một dân không ngừng chọc giận",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.277Z",
      "updatedAt": "2025-04-24T02:06:29.277Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "0e3f9765-0a54-40d3-97a6-b28980a6c5db",
      "number": 3,
      "content": "Ngay vào mặt Ta,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.287Z",
      "updatedAt": "2025-04-24T02:06:29.287Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "cf019566-0772-401c-a1be-106e34ec6c45",
      "number": 3,
      "content": "Chúng dâng con vật hiến tế cho tà thần trong các vườn,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.297Z",
      "updatedAt": "2025-04-24T02:06:29.297Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "4d2e26cb-50a1-4ec6-af9e-f857c2d47bd9",
      "number": 3,
      "content": "Và dâng hương cho các thần tượng trên những bàn thờ bằng gạch;",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.306Z",
      "updatedAt": "2025-04-24T02:06:29.306Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "971e39a1-3897-4dcd-8a42-9772368ba06c",
      "number": 4,
      "content": "Chúng ngồi trầm tư mặc tưởng giữa các mồ mả,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.316Z",
      "updatedAt": "2025-04-24T02:06:29.316Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "56297587-4364-4fa6-aa4c-85128ad0b624",
      "number": 4,
      "content": "Và thức thâu đêm tĩnh tọa nơi mộ địa âm u.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.332Z",
      "updatedAt": "2025-04-24T02:06:29.332Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "ce7a2bee-85d0-414a-9a1f-e50f4810e0dc",
      "number": 4,
      "content": "Chúng ăn thịt heo ô uế;",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.342Z",
      "updatedAt": "2025-04-24T02:06:29.342Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "a89c06f3-70f3-47cc-98df-c50fadd28e2e",
      "number": 4,
      "content": "Bát đĩa chúng chứa đầy các thức ăn gớm ghiếc.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.351Z",
      "updatedAt": "2025-04-24T02:06:29.351Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "4fcbac8f-cc69-4e3a-aebc-64388f4cd9b2",
      "number": 5,
      "content": "Chúng là những kẻ đã nói,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.361Z",
      "updatedAt": "2025-04-24T02:06:29.361Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "4859a7ff-6751-48d8-bec1-af13e174279a",
      "number": 5,
      "content": "‘Hãy đứng xa ra, chớ lại gần ta, vì ta thánh khiết hơn ngươi!’",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.370Z",
      "updatedAt": "2025-04-24T02:06:29.370Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "913e9a53-5965-4ec8-b717-09d18153ddc9",
      "number": 5,
      "content": "Những điều ấy làm cho khói giận của Ta bốc ra nơi lỗ mũi,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.380Z",
      "updatedAt": "2025-04-24T02:06:29.380Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "2469278f-af16-4200-8e5d-850c683a8e2d",
      "number": 5,
      "content": "Như lửa hừng cháy hừng hực suốt ngày.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.389Z",
      "updatedAt": "2025-04-24T02:06:29.389Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "4c76f316-34a2-4f03-a1cc-0d6809120552",
      "number": 6,
      "content": "Này, những việc ấy đã được ghi lại trước mặt Ta;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.398Z",
      "updatedAt": "2025-04-24T02:06:29.398Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "c35fca57-64fb-43c9-8548-651ef7c30f05",
      "number": 6,
      "content": "Ta sẽ không im lặng, nhưng sẽ báo trả,",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.408Z",
      "updatedAt": "2025-04-24T02:06:29.408Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "83f596c8-50cf-4415-ae51-ca3f1a10d62e",
      "number": 6,
      "content": "Ta sẽ báo trả ngay vào lòng chúng;",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.417Z",
      "updatedAt": "2025-04-24T02:06:29.417Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "8e076b6f-0098-4cd0-b1ea-8038e2c82a92",
      "number": 7,
      "content": "Ta sẽ báo trả tội của các ngươi và tội của tổ tiên các ngươi nữa,”",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.427Z",
      "updatedAt": "2025-04-24T02:06:29.427Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "da8a29a7-cf47-4737-a258-2830e598d589",
      "number": 7,
      "content": "**CHÚA** phán.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.436Z",
      "updatedAt": "2025-04-24T02:06:29.436Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "19c96962-c00d-440c-bca6-7a38a6882af2",
      "number": 7,
      "content": "“Chúng đã dâng hương cho các tà thần trên các núi;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.446Z",
      "updatedAt": "2025-04-24T02:06:29.446Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "a5b0ffb7-17a6-4826-8d68-7462adfbf006",
      "number": 7,
      "content": "Chúng đã xúc phạm đến Ta trên các ngọn đồi,",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.457Z",
      "updatedAt": "2025-04-24T02:06:29.457Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "032b6e27-ce6b-4814-a9bf-3f953e638ea2",
      "number": 7,
      "content": "Nên Ta sẽ lường theo những việc chúng làm khi trước,",
      "order": 4,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.466Z",
      "updatedAt": "2025-04-24T02:06:29.466Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "94045d85-73df-4817-b687-58fceaddbd3f",
      "number": 7,
      "content": "Mà báo trả đích đáng vào lòng chúng.”",
      "order": 5,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.476Z",
      "updatedAt": "2025-04-24T02:06:29.476Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "67943664-5a32-4a48-8a72-d9ced6c9413b",
      "number": 8,
      "content": "**CHÚA** phán thế này:",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.486Z",
      "updatedAt": "2025-04-24T02:06:29.486Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "87323321-1382-494a-afb8-64826a060494",
      "number": 8,
      "content": "“Như rượu mới còn tìm thấy trong chùm nho héo,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.495Z",
      "updatedAt": "2025-04-24T02:06:29.495Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "72834a11-21b7-49ac-aeb1-62f27f4e7eda",
      "number": 8,
      "content": "Người ta bảo rằng, ‘Đừng bỏ nó đi,",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.505Z",
      "updatedAt": "2025-04-24T02:06:29.505Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "de1b365a-6407-40b5-9da5-d518a61defc2",
      "number": 8,
      "content": "Vì trong đó còn tìm thấy chút phước lành,’",
      "order": 3,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.515Z",
      "updatedAt": "2025-04-24T02:06:29.515Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "21bac7a0-8967-4524-abfe-31a8239bf062",
      "number": 8,
      "content": "Thì vì cớ các tôi tớ Ta, Ta sẽ làm như vậy;",
      "order": 4,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.525Z",
      "updatedAt": "2025-04-24T02:06:29.525Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "b84253a4-7b81-4d02-9f61-3738c73e1ec7",
      "number": 8,
      "content": "Ta sẽ không tiêu diệt hết chúng đâu.",
      "order": 5,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.535Z",
      "updatedAt": "2025-04-24T02:06:29.535Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "3af1805e-f9bd-4721-a386-f970d245dea9",
      "number": 9,
      "content": "Từ Gia-cốp Ta sẽ cho phát sinh một dòng dõi;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.544Z",
      "updatedAt": "2025-04-24T02:06:29.544Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "c28c1776-73c5-449e-ab8f-44f952ac68f0",
      "number": 9,
      "content": "Từ Giu-đa sẽ ra một người thừa kế các núi thánh của Ta;",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.554Z",
      "updatedAt": "2025-04-24T02:06:29.554Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "9c14c71a-aa3d-42e2-a1f3-aa3887f25276",
      "number": 9,
      "content": "Người Ta chọn sẽ thừa hưởng cơ nghiệp đó;",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.564Z",
      "updatedAt": "2025-04-24T02:06:29.564Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "b3bb5604-b196-49d4-b6b3-62de0004cfa7",
      "number": 9,
      "content": "Các tôi tớ Ta sẽ cư ngụ ở đó.",
      "order": 3,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.574Z",
      "updatedAt": "2025-04-24T02:06:29.574Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "e76ef86f-14c7-46a8-afa0-f62620a0c42b",
      "number": 10,
      "content": "Sa-rôn sẽ là đồng cỏ cho các bầy súc vật;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.583Z",
      "updatedAt": "2025-04-24T02:06:29.583Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "e5279a25-a3a6-44d4-91ac-eb2be52ba34e",
      "number": 10,
      "content": "Thung lũng A-cơ sẽ là chỗ cho các đàn bò nằm,",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.593Z",
      "updatedAt": "2025-04-24T02:06:29.593Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "544fd97f-1d36-44fb-bc8b-5f5134978a49",
      "number": 10,
      "content": "Vì dân Ta đã tìm kiếm Ta.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.603Z",
      "updatedAt": "2025-04-24T02:06:29.603Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "36ba26c4-e70a-40e9-91ce-c77094320c6b",
      "number": 11,
      "content": "Nhưng hỡi các ngươi là những kẻ bỏ **CHÚA**,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.612Z",
      "updatedAt": "2025-04-24T02:06:29.612Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "cfd1eefa-c49e-436b-91cd-37fbefe418ff",
      "number": 11,
      "content": "Những kẻ quên núi thánh của Ta,",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.621Z",
      "updatedAt": "2025-04-24T02:06:29.621Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "dedaa667-c36e-4373-ba00-99dbf066c101",
      "number": 11,
      "content": "Những kẻ lập bàn thờ cho Thần May Mắn,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.631Z",
      "updatedAt": "2025-04-24T02:06:29.631Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "58929ff4-4b84-4cc1-8c35-8a6df29e3ba3",
      "number": 11,
      "content": "Dâng rượu pha cho Thần Định Mệnh,",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.651Z",
      "updatedAt": "2025-04-24T02:06:29.651Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "60e2badf-d109-47b2-bbbd-f66bb8fa19de",
      "number": 12,
      "content": "Vì thế Ta sẽ định phần số các ngươi cho gươm giáo;",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.671Z",
      "updatedAt": "2025-04-24T02:06:29.671Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "45354de1-a7d4-468c-9c45-d7ec5177e4d3",
      "number": 12,
      "content": "Tất cả các ngươi sẽ quỳ xuống trước kẻ giết mình,",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.681Z",
      "updatedAt": "2025-04-24T02:06:29.681Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "f2cda0bd-a4c1-4a6e-9889-c9ef230c5940",
      "number": 12,
      "content": "Vì khi Ta gọi, các ngươi chẳng thèm đáp lại;",
      "order": 2,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.696Z",
      "updatedAt": "2025-04-24T02:06:29.696Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "3319ba80-a833-4f5d-a884-7196e079d7f6",
      "number": 12,
      "content": "Khi Ta phán, các ngươi chẳng muốn lắng nghe;",
      "order": 3,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.706Z",
      "updatedAt": "2025-04-24T02:06:29.706Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "923a00b5-327f-4594-a84b-b9ac55c86e80",
      "number": 12,
      "content": "Nhưng các ngươi cứ làm điều ác trước mắt Ta,",
      "order": 4,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.716Z",
      "updatedAt": "2025-04-24T02:06:29.716Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "55445cb0-3312-44e8-af7c-0e46e0667ec9",
      "number": 12,
      "content": "Và cứ chọn làm những điều Ta không thích.”",
      "order": 5,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.726Z",
      "updatedAt": "2025-04-24T02:06:29.726Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "dfba2ccd-76df-4487-bca0-48ee499469eb",
      "number": 13,
      "content": "Vì thế **CHÚA** Hằng Hữu phán thế này:",
      "order": 0,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.735Z",
      "updatedAt": "2025-04-24T02:06:29.735Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "6743b7e5-295b-4ed2-b9f4-26765b50cc96",
      "number": 13,
      "content": "“Này, các tôi tớ Ta sẽ ăn,",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.745Z",
      "updatedAt": "2025-04-24T02:06:29.745Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "cad325e2-6a65-49a3-8829-5dde31c274f2",
      "number": 13,
      "content": "Nhưng các ngươi sẽ đói;",
      "order": 2,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.754Z",
      "updatedAt": "2025-04-24T02:06:29.754Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "dfe4b288-a389-4558-b52d-dd8de4ef3cbb",
      "number": 13,
      "content": "Này, các tôi tớ Ta sẽ uống,",
      "order": 3,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.764Z",
      "updatedAt": "2025-04-24T02:06:29.764Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "fdb54aad-e27b-442f-9719-a111f510200e",
      "number": 13,
      "content": "Nhưng các ngươi sẽ khát;",
      "order": 4,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.773Z",
      "updatedAt": "2025-04-24T02:06:29.773Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "14807889-e26e-4565-bc5a-52c433831674",
      "number": 13,
      "content": "Này, các tôi tớ Ta sẽ vui mừng,",
      "order": 5,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.783Z",
      "updatedAt": "2025-04-24T02:06:29.783Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "3158600a-b852-4f33-98f2-1c2fbdf21ec9",
      "number": 13,
      "content": "Nhưng các ngươi sẽ tủi hổ;",
      "order": 6,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.796Z",
      "updatedAt": "2025-04-24T02:06:29.796Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "794d7061-b269-4151-afd1-2c0491f5360b",
      "number": 14,
      "content": "Này, các tôi tớ Ta sẽ ca hát vì tâm hồn vui vẻ,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.806Z",
      "updatedAt": "2025-04-24T02:06:29.806Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "f80f2585-e40b-4e4b-820c-ea8d75c5119c",
      "number": 14,
      "content": "Nhưng các ngươi sẽ khóc than vì sầu thảm trong lòng,",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.815Z",
      "updatedAt": "2025-04-24T02:06:29.815Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "81013c7e-d1ca-4bf2-addf-9ce402b2bc19",
      "number": 14,
      "content": "Và các ngươi sẽ khóc lóc vì tâm linh đau đớn.",
      "order": 2,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.824Z",
      "updatedAt": "2025-04-24T02:06:29.824Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "c006191c-28e0-4203-af5d-daee7c0d19ca",
      "number": 15,
      "content": "Các ngươi cũng sẽ được lưu danh, nhưng chỉ để làm lời nguyền rủa cho những kẻ Ta chọn,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.834Z",
      "updatedAt": "2025-04-24T02:06:29.834Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "8bfdc005-b44f-4d0b-9447-4693ac1170ea",
      "number": 15,
      "content": "Vì **CHÚA** Hằng Hữu sẽ tiêu diệt ngươi,",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.844Z",
      "updatedAt": "2025-04-24T02:06:29.844Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "bcad11eb-527a-4255-921a-428da5b9b97b",
      "number": 15,
      "content": "Nhưng các tôi tớ Ngài, Ngài sẽ gọi họ bằng một danh khác.",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.854Z",
      "updatedAt": "2025-04-24T02:06:29.854Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "8e9b4ce2-dead-45e3-89e2-a944b233fed4",
      "number": 16,
      "content": "Vì vậy, ở trong xứ, ai cầu phúc cho mình",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.864Z",
      "updatedAt": "2025-04-24T02:06:29.864Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "4fe9b554-2985-41c6-b7cd-b66b4afd09c2",
      "number": 16,
      "content": "Sẽ nhân danh Đức Chúa Trời của sự thật mà cầu phúc;",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.873Z",
      "updatedAt": "2025-04-24T02:06:29.873Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "c718ea57-419d-49e8-8422-a0c38d326e6e",
      "number": 16,
      "content": "Ở trong xứ, ai tuyên thệ",
      "order": 2,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.883Z",
      "updatedAt": "2025-04-24T02:06:29.883Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "f173a3c8-92f4-423d-9531-7b9612e84a04",
      "number": 16,
      "content": "Sẽ nhân danh Đức Chúa Trời của sự thật mà thề,",
      "order": 3,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.892Z",
      "updatedAt": "2025-04-24T02:06:29.892Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "932c2e77-4302-401e-b2eb-8b3d1291741a",
      "number": 16,
      "content": "Vì những gian truân khi trước đã bị quên rồi,",
      "order": 4,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.904Z",
      "updatedAt": "2025-04-24T02:06:29.904Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "2346c9ed-d9fa-4723-b511-d2774f445332",
      "number": 16,
      "content": "Và vì chúng đã bị che khuất khỏi mắt Ta rồi.",
      "order": 5,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.914Z",
      "updatedAt": "2025-04-24T02:06:29.914Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "c299754f-0d44-4cf7-9fa7-7429eb2df58c",
      "number": 17,
      "content": "Vì này, Ta sẽ dựng nên",
      "order": 0,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.924Z",
      "updatedAt": "2025-04-24T02:06:29.924Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "6b2d4b42-329e-4e0c-8322-d3eef661ee3f",
      "number": 17,
      "content": "Trời mới và đất mới;",
      "order": 1,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.943Z",
      "updatedAt": "2025-04-24T02:06:29.943Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "cdbd555a-429b-4afc-9284-f3507601566d",
      "number": 17,
      "content": "Những gì trước kia sẽ không còn được nhớ đến nữa,",
      "order": 2,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.952Z",
      "updatedAt": "2025-04-24T02:06:29.952Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "ec923b02-152d-43b0-b872-e9b825a16459",
      "number": 17,
      "content": "Và cũng không còn đến trong trí nữa.",
      "order": 3,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.962Z",
      "updatedAt": "2025-04-24T02:06:29.962Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "f581c624-e2bd-43a7-87ec-840561757921",
      "number": 18,
      "content": "Nhưng hãy hân hoan và vui mừng mãi mãi",
      "order": 0,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.971Z",
      "updatedAt": "2025-04-24T02:06:29.971Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "4b886fec-aa73-4a69-ab59-b48df6a90a19",
      "number": 18,
      "content": "Về những gì Ta tạo dựng;",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.981Z",
      "updatedAt": "2025-04-24T02:06:29.981Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "193fbf90-6eb3-47d4-8bd4-5deb51667c57",
      "number": 18,
      "content": "Vì này, Ta dựng nên Giê-ru-sa-lem để làm một niềm vui;",
      "order": 2,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:29.990Z",
      "updatedAt": "2025-04-24T02:06:29.990Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "18fe6b87-0161-4503-87a6-cf42f65d9566",
      "number": 18,
      "content": "Và dân thành ấy sẽ là một niềm vui thỏa.",
      "order": 3,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30Z",
      "updatedAt": "2025-04-24T02:06:30Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "84b8d32b-039b-4237-9b2f-dfaeb7dfc3f4",
      "number": 19,
      "content": "Ta sẽ vui vẻ vì Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.010Z",
      "updatedAt": "2025-04-24T02:06:30.010Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "8dfdd23b-b5e3-4ccb-bf30-cacad9807e16",
      "number": 19,
      "content": "Ta sẽ vui mừng vì dân Ta;",
      "order": 1,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.019Z",
      "updatedAt": "2025-04-24T02:06:30.019Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "3b816cef-235b-4910-bc82-eb9f1d6336e9",
      "number": 19,
      "content": "Tiếng than khóc sẽ không còn nghe trong thành ấy;",
      "order": 2,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.029Z",
      "updatedAt": "2025-04-24T02:06:30.029Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "257e5e8b-8126-42f1-89ca-89034eeec4ab",
      "number": 19,
      "content": "Tiếng khóc than sẽ vĩnh viễn không còn.",
      "order": 3,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.038Z",
      "updatedAt": "2025-04-24T02:06:30.038Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "49bb8f7a-84dd-4c0a-8893-b0d26b69afa7",
      "number": 20,
      "content": "Trong thành ấy trẻ thơ sẽ không còn chết yểu;",
      "order": 0,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.048Z",
      "updatedAt": "2025-04-24T02:06:30.048Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "31db019b-d248-40d9-82c8-a54ee78f7e84",
      "number": 20,
      "content": "Không người già nào chẳng hưởng trọn tuổi thọ của mình;",
      "order": 1,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.058Z",
      "updatedAt": "2025-04-24T02:06:30.058Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "9730bbad-fb38-4506-9a70-e62a9ce03e92",
      "number": 20,
      "content": "Vì ai trăm tuổi mà qua đời sẽ bị xem là chết trẻ;",
      "order": 2,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.068Z",
      "updatedAt": "2025-04-24T02:06:30.068Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "c7910e99-7fe1-401d-8caf-2f907504dd73",
      "number": 20,
      "content": "Người chưa được bách niên mà thất lộc sẽ xem như bị rủa nguyền.",
      "order": 3,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.077Z",
      "updatedAt": "2025-04-24T02:06:30.077Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "36915c08-9bec-4d51-ab1d-8141fa31a904",
      "number": 21,
      "content": "Chúng sẽ xây nhà và an cư trong các nhà đó;",
      "order": 0,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.087Z",
      "updatedAt": "2025-04-24T02:06:30.087Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "4ca37c7b-371a-4490-be5e-00e2fc2aa523",
      "number": 21,
      "content": "Chúng sẽ trồng vườn nho và được ăn trái mình trồng.",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.096Z",
      "updatedAt": "2025-04-24T02:06:30.096Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "8b48be02-f960-466c-ac32-8afa813c390e",
      "number": 22,
      "content": "Chúng sẽ không xây nhà để bị người khác vào chiếm ở;",
      "order": 0,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.106Z",
      "updatedAt": "2025-04-24T02:06:30.106Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "0c18af49-3eef-4f74-b4a7-081e9e79757d",
      "number": 22,
      "content": "Chúng sẽ không trồng cây để bị người khác đến chiếm ăn.",
      "order": 1,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.116Z",
      "updatedAt": "2025-04-24T02:06:30.116Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "a9458051-02ed-4a74-8e0a-ebcd25e17025",
      "number": 22,
      "content": "Vì cây cối sống bao nhiêu tuổi, dân Ta cũng sẽ thọ bấy nhiêu.",
      "order": 2,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.126Z",
      "updatedAt": "2025-04-24T02:06:30.126Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "80cf2a66-4e9e-4f19-b0be-8020c6c3ae98",
      "number": 22,
      "content": "Những kẻ Ta chọn sẽ hưởng lâu dài mọi kết quả do tay mình làm ra.",
      "order": 3,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.136Z",
      "updatedAt": "2025-04-24T02:06:30.136Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "4b2191ce-1831-43c1-b615-d405823d099c",
      "number": 23,
      "content": "Chúng sẽ không làm lụng cách luống công vô ích;",
      "order": 0,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.145Z",
      "updatedAt": "2025-04-24T02:06:30.145Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "c108ddc6-a18f-45c3-bc1c-a312b7995a2d",
      "number": 23,
      "content": "Chúng sinh con ra chẳng phải để chuốc lấy âu sầu,",
      "order": 1,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.159Z",
      "updatedAt": "2025-04-24T02:06:30.159Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "c369ea7a-52d6-4f89-ac2c-ca4d5aa8c7fa",
      "number": 23,
      "content": "Vì con cháu chúng sẽ là những người được **CHÚA** ban phước,",
      "order": 2,
      "parNumber": 95,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.168Z",
      "updatedAt": "2025-04-24T02:06:30.168Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "16e08b51-710e-4c37-bcf5-c70d0bd7d568",
      "number": 23,
      "content": "Và dòng dõi chúng cùng với chúng nữa.",
      "order": 3,
      "parNumber": 96,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.178Z",
      "updatedAt": "2025-04-24T02:06:30.178Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "d48c2447-0360-4d19-a9e4-93e8e0eb0231",
      "number": 24,
      "content": "Rồi đây, trước khi chúng cầu xin, Ta đã trả lời;",
      "order": 0,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.188Z",
      "updatedAt": "2025-04-24T02:06:30.188Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "b698dab7-bab3-4263-979a-c56affe44cf3",
      "number": 24,
      "content": "Khi chúng còn đang nói, Ta đã nghe rõ hết.",
      "order": 1,
      "parNumber": 98,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.201Z",
      "updatedAt": "2025-04-24T02:06:30.201Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "4a5e3d67-31ac-4bcf-9de0-5425ecc8c9af",
      "number": 25,
      "content": "Chó sói và chiên con sẽ ăn cỏ với nhau,",
      "order": 0,
      "parNumber": 99,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.211Z",
      "updatedAt": "2025-04-24T02:06:30.211Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "6ee36e3b-409e-424c-b86e-47063cb971b3",
      "number": 25,
      "content": "Sư tử sẽ ăn rơm rạ như bò,",
      "order": 1,
      "parNumber": 100,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.221Z",
      "updatedAt": "2025-04-24T02:06:30.221Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "0b737934-70d4-4f1f-ab28-7daa83ead8b3",
      "number": 25,
      "content": "Còn bụi đất sẽ là thức ăn của loài rắn;",
      "order": 2,
      "parNumber": 101,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.230Z",
      "updatedAt": "2025-04-24T02:06:30.230Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "dc86627c-7eef-42e9-a113-8564a4c93d5f",
      "number": 25,
      "content": "Chúng sẽ không gây đau thương hay hủy diệt",
      "order": 3,
      "parNumber": 102,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.240Z",
      "updatedAt": "2025-04-24T02:06:30.240Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "8452761c-a09c-4a48-bdaa-54035ebd87b1",
      "number": 25,
      "content": "Trong khắp núi thánh của Ta,”",
      "order": 4,
      "parNumber": 103,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.252Z",
      "updatedAt": "2025-04-24T02:06:30.252Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    },
    {
      "id": "e52005d6-df95-496e-a7c1-bc150f4c6480",
      "number": 25,
      "content": "**CHÚA** phán.",
      "order": 5,
      "parNumber": 104,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:06:30.262Z",
      "updatedAt": "2025-04-24T02:06:30.262Z",
      "chapterId": "d0973f4c-9176-4501-a71f-c86830ceff24"
    }
  ],
