{
  "chapter": {
    "id": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4",
    "number": 34,
    "ref": "https://www.bible.com/bible/19/ISA.34.BD2011",
    "createdAt": "2025-04-24T01:29:08.876Z",
    "updatedAt": "2025-04-24T01:29:08.876Z",
    "bookId": "be27ebcc-122e-43b3-8087-1ceb029435b3"
  },
  "verses": [
    {
      "id": "eb5b1ef3-17ea-4a3b-ac55-cc0c4fb76dd5",
      "number": 1,
      "content": "Hỡi các nước, hãy đến gần và nghe;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.381Z",
      "updatedAt": "2025-04-24T02:04:57.381Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "4c96ba9d-31b8-42ba-bd46-3e3e0301bd46",
      "number": 1,
      "content": "Hỡi các dân, hãy chú ý!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.408Z",
      "updatedAt": "2025-04-24T02:04:57.408Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "26258af8-9679-4994-acd8-6503950d5d6a",
      "number": 1,
      "content": "Hỡi trái đất và mọi loài sống trên đất,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.418Z",
      "updatedAt": "2025-04-24T02:04:57.418Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "170fed9c-9630-4d9c-86ac-d9b4ce6b9044",
      "number": 1,
      "content": "Hỡi thế gian và mọi vật ra từ thế gian, tất cả hãy lắng nghe;",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.428Z",
      "updatedAt": "2025-04-24T02:04:57.428Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "84d4363e-8cb1-47a5-8857-ea774007fb31",
      "number": 2,
      "content": "Vì cơn giận của **CHÚA** đang nghịch lại mọi quốc gia;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.438Z",
      "updatedAt": "2025-04-24T02:04:57.438Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "1fed706e-9c69-46ec-a547-8b3264151d3c",
      "number": 2,
      "content": "Cơn thịnh nộ của Ngài nghịch lại mọi đạo quân của họ.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.448Z",
      "updatedAt": "2025-04-24T02:04:57.448Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "70c5546b-c141-4f1f-bc4c-135ec760618d",
      "number": 2,
      "content": "Ngài muốn diệt hẳn họ, Ngài phó họ cho sự tàn sát.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.457Z",
      "updatedAt": "2025-04-24T02:04:57.457Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "e8af22b4-189e-4247-92d2-67f449886b94",
      "number": 3,
      "content": "Xác người bị giết của họ sẽ bị ném ra ngoài;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.468Z",
      "updatedAt": "2025-04-24T02:04:57.468Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "34b3768f-17ba-4fe2-a0c5-3d21f43b92fe",
      "number": 3,
      "content": "Mùi hôi thối của chúng sẽ bay lên nồng nặc.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.478Z",
      "updatedAt": "2025-04-24T02:04:57.478Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "6f2900c3-d039-4497-a47d-10c2401c209b",
      "number": 3,
      "content": "Máu người sẽ chảy lênh láng trên các núi.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.488Z",
      "updatedAt": "2025-04-24T02:04:57.488Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "b561355f-9c1f-40b4-aeb4-ab6f11fa2a87",
      "number": 4,
      "content": "Toàn thể đạo binh trên không gian sẽ tan rã;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.498Z",
      "updatedAt": "2025-04-24T02:04:57.498Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "22e09d86-fc42-4db5-8477-64175b8bd76d",
      "number": 4,
      "content": "Các tầng trời sẽ bị cuốn lại như một cuộn sách;",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.507Z",
      "updatedAt": "2025-04-24T02:04:57.507Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "25e0ad2b-39e5-4ab1-8f5b-a494332e54c9",
      "number": 4,
      "content": "Tất cả các đạo binh của họ sẽ ngã xuống,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.520Z",
      "updatedAt": "2025-04-24T02:04:57.520Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "26faf0d5-01b7-4c6d-8d09-4a23f2a0491f",
      "number": 4,
      "content": "Như lá nho rơi rụng,",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.530Z",
      "updatedAt": "2025-04-24T02:04:57.530Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "015af0c8-416f-4a82-b870-dad193377de5",
      "number": 4,
      "content": "Như lá vả lìa cành.",
      "order": 4,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.539Z",
      "updatedAt": "2025-04-24T02:04:57.539Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "cd968600-7a0c-4a83-b741-08e85e2303a1",
      "number": 5,
      "content": "Khi lưỡi gươm Ta đã say máu trên không gian,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.549Z",
      "updatedAt": "2025-04-24T02:04:57.549Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "64fc5249-7777-4c50-998c-026467bbc6a6",
      "number": 5,
      "content": "Này, nó sẽ giáng xuống trên Ê-đôm,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.568Z",
      "updatedAt": "2025-04-24T02:04:57.568Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "eeea20f3-2e51-4a8d-9a1b-9b69d118bac1",
      "number": 5,
      "content": "Tức giáng xuống trên dân Ta đã định phải bị đoán phạt.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.578Z",
      "updatedAt": "2025-04-24T02:04:57.578Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "51fb9e7a-9b3e-4971-92b8-74558658106b",
      "number": 6,
      "content": "Gươm của **CHÚA** dính đầy máu,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.588Z",
      "updatedAt": "2025-04-24T02:04:57.588Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "96c18c11-dfc6-426f-816f-29c58713f536",
      "number": 6,
      "content": "Mỡ bao kín lưỡi gươm,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.604Z",
      "updatedAt": "2025-04-24T02:04:57.604Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "c96e2ffa-9236-466c-8754-143fd5fadf07",
      "number": 6,
      "content": "Nào máu chiên, máu dê,",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.614Z",
      "updatedAt": "2025-04-24T02:04:57.614Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "28d3f374-25bd-4b40-bbc6-03bea1f52d21",
      "number": 6,
      "content": "Nào mỡ bọc trái cật của các chiên đực,",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.624Z",
      "updatedAt": "2025-04-24T02:04:57.624Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "d01b2c87-2b88-45d8-8178-4e1ff26b0609",
      "number": 6,
      "content": "Vì **CHÚA** có cuộc tế lễ ở Bô-ra,",
      "order": 4,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.633Z",
      "updatedAt": "2025-04-24T02:04:57.633Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "0d715c54-581f-438b-873e-1b5b72f31cf9",
      "number": 6,
      "content": "Một cuộc tàn sát lớn trong xứ Ê-đôm.",
      "order": 5,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.643Z",
      "updatedAt": "2025-04-24T02:04:57.643Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "149a52b0-d516-43e5-9d79-02341e40ebbd",
      "number": 7,
      "content": "Các bò rừng sẽ ngã xuống với chúng;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.653Z",
      "updatedAt": "2025-04-24T02:04:57.653Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "79e12221-98a0-4300-b566-d7e266c49afe",
      "number": 7,
      "content": "Các bò tơ và các bò đực mạnh cũng vậy;",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.663Z",
      "updatedAt": "2025-04-24T02:04:57.663Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "9ffa98c9-7859-476e-be3d-61e639fb2f37",
      "number": 7,
      "content": "Đất của chúng thấm đầy máu;",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.673Z",
      "updatedAt": "2025-04-24T02:04:57.673Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "f69b35d3-b925-4fc2-ac31-73e8d47486c6",
      "number": 7,
      "content": "Bụi của chúng lẫn lộn với mỡ khắp nơi.",
      "order": 3,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.683Z",
      "updatedAt": "2025-04-24T02:04:57.683Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "1f4a3012-fe90-461c-b1f3-3f3d55c4ea6d",
      "number": 8,
      "content": "Vì **CHÚA** sẽ có một ngày báo thù,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.692Z",
      "updatedAt": "2025-04-24T02:04:57.692Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "b7527035-3844-4bc4-b2ee-071eaccee4a0",
      "number": 8,
      "content": "Một năm báo trả để minh oan cho duyên cớ của Si-ôn.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.702Z",
      "updatedAt": "2025-04-24T02:04:57.702Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "d5c55e4a-457f-45d2-ae60-0693f4e8b165",
      "number": 9,
      "content": "Các sông suối của Ê-đôm sẽ biến thành hắc ín;",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.712Z",
      "updatedAt": "2025-04-24T02:04:57.712Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "965875d6-d1ce-434a-8395-a5791aca25a5",
      "number": 9,
      "content": "Bụi đất của nó sẽ hóa thành lưu huỳnh;",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.722Z",
      "updatedAt": "2025-04-24T02:04:57.722Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "3b84b86c-e9d6-487c-a52a-6d9002146df6",
      "number": 9,
      "content": "Đất cát của nó sẽ trở nên hắc ín âm ỉ cháy,",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.735Z",
      "updatedAt": "2025-04-24T02:04:57.735Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "42e2c09b-e5c7-4c6c-ad2b-81e2ec9ce49b",
      "number": 10,
      "content": "Cả ngày lẫn đêm nó cháy mãi không ngừng;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.745Z",
      "updatedAt": "2025-04-24T02:04:57.745Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "d45f9236-bb2d-482b-bab4-a1257e1199b8",
      "number": 10,
      "content": "Khói đen của nó cứ bay lên không dứt;",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.755Z",
      "updatedAt": "2025-04-24T02:04:57.755Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "1d914866-542f-4f91-b9d8-8cdb45f38762",
      "number": 10,
      "content": "Từ thế hệ này qua thế hệ khác nó nằm đó trong cảnh đổ nát hoang tàn;",
      "order": 2,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.765Z",
      "updatedAt": "2025-04-24T02:04:57.765Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "68d93862-b1b4-47e3-b57a-5a21cbf51437",
      "number": 10,
      "content": "Muôn đời sẽ không ai muốn đi ngang qua đó.",
      "order": 3,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.774Z",
      "updatedAt": "2025-04-24T02:04:57.774Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "4975e9a8-53ea-4cee-9a64-a62033437219",
      "number": 11,
      "content": "Diều hâu và nhím sẽ chiếm lấy xứ đó;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.784Z",
      "updatedAt": "2025-04-24T02:04:57.784Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "37020e15-6f54-4cc9-b7cc-49bc60000875",
      "number": 11,
      "content": "Cú và quạ sẽ ở tại đó;",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.794Z",
      "updatedAt": "2025-04-24T02:04:57.794Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "78db4ec9-f1c8-4d07-a2d1-54555727edbf",
      "number": 11,
      "content": "Trên xứ đó Ngài sẽ giăng một sợi dây của sự hỗn mang,",
      "order": 2,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.804Z",
      "updatedAt": "2025-04-24T02:04:57.804Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "e83861c4-96a0-466e-9a1e-5ce6353fbf83",
      "number": 11,
      "content": "Một dây dọi của sự trống không.",
      "order": 3,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.814Z",
      "updatedAt": "2025-04-24T02:04:57.814Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "573d7d39-190c-4dd1-a9e9-8e1b0f80ee61",
      "number": 12,
      "content": "Các nhà quý tộc của nó sẽ không còn gì để gọi là một vương quốc;",
      "order": 0,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.824Z",
      "updatedAt": "2025-04-24T02:04:57.824Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "b1040fb9-026c-48da-946c-a519ecb8ad4d",
      "number": 12,
      "content": "Tất cả người quyền quý của nó đều sẽ như không.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.834Z",
      "updatedAt": "2025-04-24T02:04:57.834Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "ab025e66-93f9-4b5f-a35a-b5575b6c855c",
      "number": 13,
      "content": "Gai góc sẽ mọc lên trong các lâu đài cung điện;",
      "order": 0,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.844Z",
      "updatedAt": "2025-04-24T02:04:57.844Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "2f05645c-bba9-4ef6-857e-90c4327bd2b1",
      "number": 13,
      "content": "Gai dại và bụi gai sẽ mọc trên các thành lũy xưa kia.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.854Z",
      "updatedAt": "2025-04-24T02:04:57.854Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "b58d4a24-4bd7-4d83-9a47-7df1e7e145e1",
      "number": 13,
      "content": "Nó sẽ trở nên nơi ở của chó rừng,",
      "order": 2,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.863Z",
      "updatedAt": "2025-04-24T02:04:57.863Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "c308c1f4-0b25-498a-8255-1df4352bd5b9",
      "number": 13,
      "content": "Một sân chơi của đà điểu.",
      "order": 3,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.873Z",
      "updatedAt": "2025-04-24T02:04:57.873Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "383e1df3-9bee-48bb-b7ad-1f61fb2d5ab2",
      "number": 14,
      "content": "Ở đó mèo rừng sẽ đụng độ với chó sói;",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.883Z",
      "updatedAt": "2025-04-24T02:04:57.883Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "beb19ab8-aaf5-4919-8c15-47c92c5b2a86",
      "number": 14,
      "content": "Dê rừng sẽ cất tiếng gọi nhau;",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.893Z",
      "updatedAt": "2025-04-24T02:04:57.893Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "2144f9fd-454d-41f5-9d77-40057d14d675",
      "number": 14,
      "content": "Các thú vật sống về đêm sẽ nghỉ ngơi tại đó;",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.902Z",
      "updatedAt": "2025-04-24T02:04:57.902Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "e37990a0-bb28-4e24-b8c6-471bdf07b4d7",
      "number": 14,
      "content": "Chúng sẽ tìm được một nơi nghỉ ngơi thích hợp cho mình.",
      "order": 3,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.912Z",
      "updatedAt": "2025-04-24T02:04:57.912Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "1427c9e5-3112-4ef1-89e8-b0f05da71c10",
      "number": 15,
      "content": "Chim cú sẽ làm tổ và đẻ trứng ở đó;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.922Z",
      "updatedAt": "2025-04-24T02:04:57.922Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "c4d908f7-895b-4038-b810-39c1830fbd3c",
      "number": 15,
      "content": "Chúng sẽ ấp trứng của chúng và ấp ủ đàn con dưới cánh;",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.935Z",
      "updatedAt": "2025-04-24T02:04:57.935Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "8cef9938-0352-48ca-8aab-647408f1fb28",
      "number": 15,
      "content": "Nơi đó diều hâu sẽ tụ lại với nhau;",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.945Z",
      "updatedAt": "2025-04-24T02:04:57.945Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "082bc674-a8e4-47d6-9bb4-8f0d0d8ef107",
      "number": 15,
      "content": "Con nào cũng có đôi có cặp.",
      "order": 3,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.955Z",
      "updatedAt": "2025-04-24T02:04:57.955Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "63ccc4c3-431b-4d7b-8a19-f3a8a4f93233",
      "number": 16,
      "content": "Hãy tìm trong sách của **CHÚA** và đọc xem:",
      "order": 0,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.967Z",
      "updatedAt": "2025-04-24T02:04:57.967Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "a38ddce2-379d-4183-9afa-3f7a3f30e899",
      "number": 16,
      "content": "Không một con nào trong các con này sẽ mất;",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.977Z",
      "updatedAt": "2025-04-24T02:04:57.977Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "6311b855-7561-471c-95b6-3f8c973414d9",
      "number": 16,
      "content": "Không con nào là không có cặp có đôi;",
      "order": 2,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.987Z",
      "updatedAt": "2025-04-24T02:04:57.987Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "c9a023e0-51d4-4cc4-8793-8a508e5d7d5b",
      "number": 16,
      "content": "Vì miệng Ngài đã truyền ra như vậy;",
      "order": 3,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:57.996Z",
      "updatedAt": "2025-04-24T02:04:57.996Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "6ec0fe0d-d7b4-45ce-b6e4-8ee694b431f7",
      "number": 16,
      "content": "Thần của Ngài đã tụ họp chúng về.",
      "order": 4,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:58.016Z",
      "updatedAt": "2025-04-24T02:04:58.016Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "8c831f90-fe18-4bad-a09f-7da7dae3c6a9",
      "number": 17,
      "content": "Ngài đã bốc thăm định phần cho chúng;",
      "order": 0,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:58.025Z",
      "updatedAt": "2025-04-24T02:04:58.025Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "b4633999-e140-43da-b26a-7f582dd0a7d5",
      "number": 17,
      "content": "Tay Ngài đã dùng dây đo chia phần cho chúng;",
      "order": 1,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:58.035Z",
      "updatedAt": "2025-04-24T02:04:58.035Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "37e5c0b4-01ff-4dc7-8a1a-42f982320638",
      "number": 17,
      "content": "Chúng sẽ ở tại đó muôn đời;",
      "order": 2,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:58.045Z",
      "updatedAt": "2025-04-24T02:04:58.045Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    },
    {
      "id": "9136843e-861a-4f3b-921a-ec145c2487eb",
      "number": 17,
      "content": "Chúng sẽ ngụ tại đó từ thế hệ này qua thế hệ khác.",
      "order": 3,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-24T02:04:58.055Z",
      "updatedAt": "2025-04-24T02:04:58.055Z",
      "chapterId": "ade8609e-dfa8-4caa-9dbb-1b7d57afd4d4"
    }
  ],
