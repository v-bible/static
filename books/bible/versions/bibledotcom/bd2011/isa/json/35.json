{
  "chapter": {
    "id": "69e40640-b3de-45b7-83b7-518247633ea9",
    "number": 35,
    "ref": "https://www.bible.com/bible/19/ISA.35.BD2011",
    "createdAt": "2025-04-22T10:10:28.430Z",
    "updatedAt": "2025-04-22T10:10:28.430Z",
    "bookId": "22c0d459-25e6-4624-85e4-8f3b415f7cc7"
  },
  "verses": [
    {
      "id": "9d9d46dd-1698-48d9-9ba8-cead1794ac64",
      "number": 1,
      "content": "Miền hoang dã và vùng đất khô cằn sẽ vui mừng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.311Z",
      "updatedAt": "2025-04-22T10:50:27.311Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "9c484482-fd15-424d-9fb9-63b1f972d12d",
      "number": 1,
      "content": "Sa mạc sẽ mừng rỡ và trổ hoa như hoa hồng nở rộ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.339Z",
      "updatedAt": "2025-04-22T10:50:27.339Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "42a994e2-fca5-4774-8aec-006d990bc590",
      "number": 2,
      "content": "Nó sẽ trổ hoa đầy dẫy khắp nơi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.349Z",
      "updatedAt": "2025-04-22T10:50:27.349Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "3b1d2906-e086-4a18-95bd-098c4fe935fb",
      "number": 2,
      "content": "Nó sẽ hân hoan vui mừng ca hát.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.360Z",
      "updatedAt": "2025-04-22T10:50:27.360Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "e32e3df8-2674-4cb9-8dd3-dc66b7478e78",
      "number": 2,
      "content": "Vinh hiển của Li-băng,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.370Z",
      "updatedAt": "2025-04-22T10:50:27.370Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "1857771a-5994-4194-b423-56317ee44573",
      "number": 2,
      "content": "Hùng vĩ của Cạt-mên, và rực rỡ của Sa-rôn đều được ban cho nó.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.382Z",
      "updatedAt": "2025-04-22T10:50:27.382Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "71f68c14-f8e6-40d6-8498-78f564729e23",
      "number": 2,
      "content": "Người ta sẽ thấy vinh hiển của **CHÚA**,",
      "order": 4,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.394Z",
      "updatedAt": "2025-04-22T10:50:27.394Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "11b4c4f7-4bd8-4fae-8bc1-b49e3fc7e682",
      "number": 2,
      "content": "Tức sự huy hoàng của Đức Chúa Trời chúng ta.",
      "order": 5,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.408Z",
      "updatedAt": "2025-04-22T10:50:27.408Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "3d0c1034-48f3-449e-93fa-d6a1c7d0fa8c",
      "number": 3,
      "content": "Hãy làm cho những cánh tay yếu đuối được mạnh mẽ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.421Z",
      "updatedAt": "2025-04-22T10:50:27.421Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "7cf958a8-c278-4b93-8619-06e460caf906",
      "number": 3,
      "content": "Những đầu gối run rẩy được vững vàng.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.434Z",
      "updatedAt": "2025-04-22T10:50:27.434Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "30a1e96c-9cd7-4a25-ba6d-70529dd3dee7",
      "number": 4,
      "content": "Hãy nói với những người có lòng sợ hãi,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.447Z",
      "updatedAt": "2025-04-22T10:50:27.447Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "1043d908-f477-4102-bd2b-55d49e3a383e",
      "number": 4,
      "content": "“Hãy mạnh mẽ lên, đừng sợ!",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.460Z",
      "updatedAt": "2025-04-22T10:50:27.460Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "6e422886-26e0-4d57-b1cd-716a33a3fb49",
      "number": 4,
      "content": "Này, Đức Chúa Trời của anh chị em",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.471Z",
      "updatedAt": "2025-04-22T10:50:27.471Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "149b008d-480f-45f3-ab7d-659d9ddf62cb",
      "number": 4,
      "content": "Sẽ đến để báo thù",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.482Z",
      "updatedAt": "2025-04-22T10:50:27.482Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "3f4f3562-c58f-46bb-bfa3-7fd5ef6b3f39",
      "number": 4,
      "content": "Bằng sự báo trả của Đức Chúa Trời;",
      "order": 4,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.492Z",
      "updatedAt": "2025-04-22T10:50:27.492Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "6f0a2581-10d0-47fa-a1ca-3cddd520fad9",
      "number": 4,
      "content": "Ngài sẽ đến và cứu anh chị em.”",
      "order": 5,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.502Z",
      "updatedAt": "2025-04-22T10:50:27.502Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "4bcbf8d5-79cb-4e86-aa69-820c10c01c2d",
      "number": 5,
      "content": "Bấy giờ mắt người mù sẽ mở ra,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.512Z",
      "updatedAt": "2025-04-22T10:50:27.512Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "9935faea-ba7c-407d-8378-95e64592e4da",
      "number": 5,
      "content": "Tai người điếc sẽ nghe rõ,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.522Z",
      "updatedAt": "2025-04-22T10:50:27.522Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "857982de-7aff-4558-afe8-e42e6b6e2ee6",
      "number": 6,
      "content": "Bấy giờ người què sẽ nhảy như nai rừng,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.532Z",
      "updatedAt": "2025-04-22T10:50:27.532Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "3fb4fa7e-e706-4113-a5e7-9356ec465ad1",
      "number": 6,
      "content": "Lưỡi người câm sẽ ca hát.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.542Z",
      "updatedAt": "2025-04-22T10:50:27.542Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "73ccb089-6ce4-46f6-bebc-e358bd597aa6",
      "number": 6,
      "content": "Vì nước đã phun lên trong đồng hoang khô hạn;",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.552Z",
      "updatedAt": "2025-04-22T10:50:27.552Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "cbcd6b6b-db33-4deb-9857-3a52518a1a6c",
      "number": 6,
      "content": "Sa mạc bây giờ đầy những suối nước trào tuôn.",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.562Z",
      "updatedAt": "2025-04-22T10:50:27.562Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "b584e8ef-89d5-44e1-9968-c5a1859db2b4",
      "number": 7,
      "content": "Miền đất khô cằn biến thành ao hồ ngập nước;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.578Z",
      "updatedAt": "2025-04-22T10:50:27.578Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "5b40aee9-719d-4f4f-b3b6-9fa31fd84ac8",
      "number": 7,
      "content": "Vùng đất khô khan đầy các suối nước tuôn dòng;",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.588Z",
      "updatedAt": "2025-04-22T10:50:27.588Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "2f9910c7-a772-45e4-b28c-4429796db234",
      "number": 7,
      "content": "Nơi ngày trước những chó rừng nằm ngủ,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.598Z",
      "updatedAt": "2025-04-22T10:50:27.598Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "76e2c5d5-6f18-42d6-9c33-81e9ea0afbac",
      "number": 7,
      "content": "Giờ cỏ xanh, lau, sậy mọc um tùm.",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.611Z",
      "updatedAt": "2025-04-22T10:50:27.611Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "37d22802-aafe-435f-bec5-184cd6f75184",
      "number": 8,
      "content": "Rồi ở đó người ta sẽ xây một đại lộ làm con đường di chuyển,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.621Z",
      "updatedAt": "2025-04-22T10:50:27.621Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "69f7ee80-fab0-456d-b0f8-68d304be0dd2",
      "number": 8,
      "content": "Và đặt tên là Đường Thánh;",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.631Z",
      "updatedAt": "2025-04-22T10:50:27.631Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "2ff3668f-0c9d-48aa-8ad1-9401c753c5af",
      "number": 8,
      "content": "Những kẻ ô uế sẽ không được đi trên đường ấy,",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.641Z",
      "updatedAt": "2025-04-22T10:50:27.641Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "7cfeb252-d0ab-4dc3-96fc-7ca15e68c226",
      "number": 8,
      "content": "Đường ấy chỉ dành cho những người thuộc về Chúa mà thôi.",
      "order": 3,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.651Z",
      "updatedAt": "2025-04-22T10:50:27.651Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "488e7be5-d8e9-486c-8fdf-12d139a25466",
      "number": 8,
      "content": "Những lữ khách điên rồ sẽ không được lang thang trên đường ấy.",
      "order": 4,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.671Z",
      "updatedAt": "2025-04-22T10:50:27.671Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "729d03f2-c2b5-43cd-93cb-2b9ab8f9adfa",
      "number": 9,
      "content": "Sư tử sẽ không đón đường ai trên đường ấy;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.680Z",
      "updatedAt": "2025-04-22T10:50:27.680Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "f9473473-aeab-4644-9324-d5495c6dea58",
      "number": 9,
      "content": "Các thú dữ sẽ không đi trên đường ấy;",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.690Z",
      "updatedAt": "2025-04-22T10:50:27.690Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "ffee029a-1b94-4dda-9920-6db766a05edb",
      "number": 9,
      "content": "Chẳng ai thấy chúng lảng vảng ở đó;",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.700Z",
      "updatedAt": "2025-04-22T10:50:27.700Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "cedb0020-0e06-4e5e-94fe-5b4a9dd8c7dd",
      "number": 9,
      "content": "Nhưng chỉ những người được cứu chuộc đi trên đường ấy.",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.710Z",
      "updatedAt": "2025-04-22T10:50:27.710Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "240e46aa-b8bb-47dd-b8b7-8b60e918faab",
      "number": 10,
      "content": "Những người được **CHÚA** cứu chuộc sẽ trở về;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.720Z",
      "updatedAt": "2025-04-22T10:50:27.720Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "fa253b04-d2d3-49c2-8ede-12786a537a53",
      "number": 10,
      "content": "Họ sẽ tiến về Si-ôn với tiếng hát vui mừng;",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.730Z",
      "updatedAt": "2025-04-22T10:50:27.730Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "14da00a3-38af-46c2-84ca-c17d201e5357",
      "number": 10,
      "content": "Niềm vui đời đời sẽ rạng ngời trên gương mặt họ,",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.741Z",
      "updatedAt": "2025-04-22T10:50:27.741Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "468a93a1-2f49-495b-9a2c-56315f9410be",
      "number": 10,
      "content": "Họ sẽ vui mừng và hớn hở,",
      "order": 3,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.752Z",
      "updatedAt": "2025-04-22T10:50:27.752Z",
      "chapterId": "69e40640-b3de-45b7-83b7-518247633ea9"
    },
    {
      "id": "3c7ec7f5-b5d1-448d-941a-09355830db0c",
      "number": 10,
      "content": "Sầu thảm và than thở sẽ trốn mất.",
      "order": 4,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2025-04-22T10:50:27.764Z",
      "updatedAt": "2025-04-22T10:50:27.764Z",
