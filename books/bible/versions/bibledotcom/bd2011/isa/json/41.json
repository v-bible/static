      "chapterId": "fa07c624-ded2-418f-8c6e-88e3b45139a0"
    },
    {
      "id": "25aafbe3-426e-4f2c-a46e-f27b8144eccf",
      "content": "I-sơ-ra-ên Chắc Chắn Được Cứu",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T02:05:18.195Z",
      "updatedAt": "2025-04-24T02:05:18.195Z",
