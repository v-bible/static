    },
    {
      "id": "4175c958-6035-4d90-9a26-b1cba1606085",
      "content": "Sự Hư Không của Thần Tượng",
      "order": 0,
      "createdAt": "2024-09-23T06:00:58.738Z",
      "updatedAt": "2024-09-23T06:00:58.738Z",
