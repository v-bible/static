  "headings": [
    {
      "id": "33016a27-8f18-4590-ab65-1dd3aee16ffc",
      "content": "Cơn Giận của **CHÚA** đối với Những Kẻ Lãnh Đạo Gian Ác",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-24T02:03:44.071Z",
