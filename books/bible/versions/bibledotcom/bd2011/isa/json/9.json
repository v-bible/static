      "chapterId": "d6481b00-5f3c-4a60-b761-1c32a1257300"
    },
    {
      "id": "38fe92bf-b96f-4d47-a2f0-d40e875fd674",
      "content": "****",
      "level": 1,
      "order": 1,
      "createdAt": "2025-04-22T10:48:55.888Z",
      "updatedAt": "2025-04-22T10:48:55.888Z",
