      "chapterId": "d6481b00-5f3c-4a60-b761-1c32a1257300"
    },
    {
      "id": "5df784b0-1b5d-4f83-9f3b-f249eb06bffb",
      "content": "CHÚA",
      "level": 1,
      "order": 2,
      "createdAt": "2025-04-22T10:48:55.905Z",
      "updatedAt": "2025-04-22T10:48:55.905Z",
      "verseId": "70ccc605-93d4-42b2-b7ba-9f96a989752b",
      "chapterId": "d6481b00-5f3c-4a60-b761-1c32a1257300"
    },
    {
      "id": "67d9e171-fff2-4255-9332-ac2362cb4281",
      "content": "đối với Những Kẻ Lãnh Đạo Gian Ác",
