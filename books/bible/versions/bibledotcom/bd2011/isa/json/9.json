    },
    {
      "id": "d272367c-5f47-4fdb-9f9d-15ff69e1437f",
      "content": "nt: ăn thịt của cánh tay mình",
      "position": 51,
      "order": 1,
      "createdAt": "2025-04-22T10:48:56.449Z",
      "updatedAt": "2025-04-22T10:48:56.449Z",
