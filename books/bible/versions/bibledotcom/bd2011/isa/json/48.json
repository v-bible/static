{
  "chapter": {
    "id": "024a6c87-54bd-4eed-a834-d49b4e501661",
    "number": 48,
    "ref": "https://www.bible.com/bible/19/ISA.48.BD2011",
    "createdAt": "2024-09-23T05:08:41.073Z",
    "updatedAt": "2024-09-23T05:08:41.073Z",
    "bookId": "655961d7-6793-43d1-be06-cff911774d08"
  },
  "verses": [
    {
      "id": "90fb0c0b-fb36-42e3-92f5-927d256fcf5b",
      "number": 1,
      "content": "Hãy nghe đây, hỡi nhà Gia-cốp;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.583Z",
      "updatedAt": "2024-09-23T06:01:22.583Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "fd047991-e7d9-4351-938c-f1afe6fb9191",
      "number": 1,
      "content": "Hỡi những kẻ được gọi bằng danh I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.593Z",
      "updatedAt": "2024-09-23T06:01:22.593Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "e73f49a6-3d9c-4fd4-ac49-4a36a4823c02",
      "number": 1,
      "content": "Và những người ra từ nguồn cội của Giu-đa,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.595Z",
      "updatedAt": "2024-09-23T06:01:22.595Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "fb6a0b1c-78da-4237-9b60-eda458674006",
      "number": 1,
      "content": "Những người lấy danh **CHÚA** mà thề,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.597Z",
      "updatedAt": "2024-09-23T06:01:22.597Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "531864cf-f487-496f-a7cf-84f38f1e1ceb",
      "number": 1,
      "content": "Cầu khẩn Đức Chúa Trời của I-sơ-ra-ên,",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.598Z",
      "updatedAt": "2024-09-23T06:01:22.598Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "dd73e693-e360-40cf-aa2b-79619043065f",
      "number": 1,
      "content": "Nhưng lại không thật lòng và ngay thẳng,",
      "order": 5,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.600Z",
      "updatedAt": "2024-09-23T06:01:22.600Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "6555e83d-5ea8-4170-89f2-52bd83c637c9",
      "number": 2,
      "content": "Dù chúng cho mình là dân của thành thánh,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.604Z",
      "updatedAt": "2024-09-23T06:01:22.604Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "8c23af0e-4c5e-4ddc-92e0-140a84797745",
      "number": 2,
      "content": "Và nương cậy vào Đức Chúa Trời của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.609Z",
      "updatedAt": "2024-09-23T06:01:22.609Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "6032dba9-7403-49bd-9857-0af66963b4a5",
      "number": 2,
      "content": "Tức Đấng có danh là **CHÚA** các đạo quân.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.613Z",
      "updatedAt": "2024-09-23T06:01:22.613Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "2afd41c4-291c-4b19-98f5-7cfdce5b9f09",
      "number": 3,
      "content": "Những chuyện đã qua Ta đã báo trước tự ngàn xưa;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.621Z",
      "updatedAt": "2024-09-23T06:01:22.621Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "070e6118-a3d7-4a93-aa5f-e8672bc89efe",
      "number": 3,
      "content": "Những điều ấy đã ra từ miệng Ta, và Ta làm cho người ta biết chúng,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.626Z",
      "updatedAt": "2024-09-23T06:01:22.626Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "376669f9-c3be-4079-8328-3105d03d4041",
      "number": 3,
      "content": "Rồi thình lình Ta thực hiện những điều ấy, và chúng đã xảy ra.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.630Z",
      "updatedAt": "2024-09-23T06:01:22.630Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "7f3023f7-1857-492d-961b-b1ef5473bbe7",
      "number": 4,
      "content": "Vì Ta biết ngươi là kẻ cứng đầu,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.634Z",
      "updatedAt": "2024-09-23T06:01:22.634Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "8dc3a080-d2bc-4d0d-b6a2-d1952dca1865",
      "number": 4,
      "content": "Gân cổ ngươi cứng như sắt,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.638Z",
      "updatedAt": "2024-09-23T06:01:22.638Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "c6dd80ec-a15a-43e7-b483-8af4b6932d0b",
      "number": 4,
      "content": "Trán của ngươi cứng như đồng,",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.642Z",
      "updatedAt": "2024-09-23T06:01:22.642Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "b08f29ac-8518-4775-a07a-e6a7c3b9996a",
      "number": 5,
      "content": "Nên ngay từ ban đầu Ta đã nói cho ngươi những điều ấy;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.645Z",
      "updatedAt": "2024-09-23T06:01:22.645Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "6ac4370e-70e5-48b9-9667-c04190e09399",
      "number": 5,
      "content": "Trước khi chúng xảy ra, Ta đã nói về chúng cho ngươi hay,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.648Z",
      "updatedAt": "2024-09-23T06:01:22.648Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "7cf28dce-6fb4-4494-ac8a-39cb31dab9bb",
      "number": 5,
      "content": "Để ngươi không thể nói, “Ấy là thần tượng của tôi đã làm những điều ấy;",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.650Z",
      "updatedAt": "2024-09-23T06:01:22.650Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "b48a865e-b94c-47ab-b750-7bbce3609ba6",
      "number": 5,
      "content": "Đó là tượng chạm của tôi và tượng đúc của tôi đã ra lịnh cho chúng phải xảy ra.”",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.653Z",
      "updatedAt": "2024-09-23T06:01:22.653Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "4f43aaaf-52c6-4d0c-9e20-86787945eb4c",
      "number": 6,
      "content": "Ngươi đã nghe rồi; ngươi đã thấy tất cả rồi;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.655Z",
      "updatedAt": "2024-09-23T06:01:22.655Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "98b6d664-5cce-4245-886c-358bb452e547",
      "number": 6,
      "content": "Sao ngươi không tuyên bố ra đi?",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.658Z",
      "updatedAt": "2024-09-23T06:01:22.658Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "4fbc102a-eca3-4364-b833-1356d43f395a",
      "number": 6,
      "content": "Từ nay Ta sẽ cho ngươi nghe những điều mới,",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.660Z",
      "updatedAt": "2024-09-23T06:01:22.660Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "5a400042-dfc6-4e32-b109-c7fae95776e2",
      "number": 6,
      "content": "Ngay cả những điều còn giữ kín mà ngươi chưa hề biết.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.663Z",
      "updatedAt": "2024-09-23T06:01:22.663Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "3db5590b-8f5b-4809-96ba-3e90c51f14f9",
      "number": 7,
      "content": "Đến bây giờ những điều ấy mới được tạo nên, Chứ chẳng phải chúng đã có sẵn từ lâu rồi đâu;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.665Z",
      "updatedAt": "2024-09-23T06:01:22.665Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "efdfd106-2ddc-42bc-96f5-f9843912ade4",
      "number": 7,
      "content": "Trước ngày này ngươi chưa bao giờ nghe về chúng,",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.668Z",
      "updatedAt": "2024-09-23T06:01:22.668Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "b8bb8995-37da-41ba-946c-7330403f278a",
      "number": 7,
      "content": "Để ngươi không thể nói, “Ồ tưởng gì chứ những điều đó tôi đã biết cả rồi.”",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.670Z",
      "updatedAt": "2024-09-23T06:01:22.670Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "4d4d76d9-9015-4b43-9c9b-625fb21256b3",
      "number": 8,
      "content": "Chắc chắn ngươi chưa nghe đến; Chắc chắn ngươi chưa biết đến;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.672Z",
      "updatedAt": "2024-09-23T06:01:22.672Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "028cf38b-e2c1-4f83-8e68-36fdafd41c35",
      "number": 8,
      "content": "Chắc chắn từ lâu tai ngươi chưa mở ra,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.675Z",
      "updatedAt": "2024-09-23T06:01:22.675Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "b2a5073a-af21-41fb-8453-5f9f51f49ef7",
      "number": 8,
      "content": "Vì Ta biết ngươi là kẻ phản bội,",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.677Z",
      "updatedAt": "2024-09-23T06:01:22.677Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "ea5d3674-3ae5-460c-8959-9342dbe6eb82",
      "number": 8,
      "content": "Ngươi mang tên là kẻ phản phúc từ lúc ngươi còn là một thai nhi.",
      "order": 3,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.679Z",
      "updatedAt": "2024-09-23T06:01:22.679Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "6c9decfe-917e-47b5-8f89-dbde29b5038a",
      "number": 9,
      "content": "Bấy lâu nay, vì danh Ta, Ta cam nén giận,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.682Z",
      "updatedAt": "2024-09-23T06:01:22.682Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "c09373e7-2498-4009-8753-8714c590d194",
      "number": 9,
      "content": "Vì sự ca ngợi dành cho Ta, Ta dằn lòng chịu đựng ngươi,",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.684Z",
      "updatedAt": "2024-09-23T06:01:22.684Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "c96c678d-e7cb-43ec-9273-14a34da64abe",
      "number": 9,
      "content": "Mà không tiêu diệt ngươi.",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.687Z",
      "updatedAt": "2024-09-23T06:01:22.687Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "7d050aba-621c-408a-a8b7-76a3ec841728",
      "number": 10,
      "content": "Này, Ta đã tinh luyện ngươi, nhưng không như người ta luyện bạc;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.689Z",
      "updatedAt": "2024-09-23T06:01:22.689Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "e938fa4d-49de-4740-a1ae-2cee46365275",
      "number": 10,
      "content": "Ta đã rèn luyện ngươi trong lò lửa khổ đau.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.692Z",
      "updatedAt": "2024-09-23T06:01:22.692Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "cf1e29fc-6e2b-4732-b73e-ca1fd7d0c6df",
      "number": 11,
      "content": "Nhưng nay vì cớ Ta, vì chính Ta, Ta sẽ ra tay hành động;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.694Z",
      "updatedAt": "2024-09-23T06:01:22.694Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "97ff82c9-b417-4cfa-a0e7-20282c7b234a",
      "number": 11,
      "content": "Chứ lẽ nào Ta cứ để cho danh Ta bị sỉ nhục hay sao?",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.697Z",
      "updatedAt": "2024-09-23T06:01:22.697Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "6c0f9b4c-63c0-4719-a6e6-8e11f3818885",
      "number": 11,
      "content": "Ta sẽ không nhường vinh hiển Ta cho một kẻ nào khác.",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.700Z",
      "updatedAt": "2024-09-23T06:01:22.700Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "3c6ddc09-ad3f-4c7c-ba9a-ac732ece01dc",
      "number": 12,
      "content": "Hỡi Gia-cốp, hãy lắng nghe Ta;",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.704Z",
      "updatedAt": "2024-09-23T06:01:22.704Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "bb5f3b6d-4145-4efe-9dc1-24e371b677f9",
      "number": 12,
      "content": "Hỡi I-sơ-ra-ên, người Ta đã gọi;",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.707Z",
      "updatedAt": "2024-09-23T06:01:22.707Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "529e465f-f442-451a-a661-0eaed68e87ba",
      "number": 12,
      "content": "Ta là Đấng ấy, Ta là Đấng Đầu Tiên,",
      "order": 2,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.712Z",
      "updatedAt": "2024-09-23T06:01:22.712Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "97c78a0f-a56f-4ea3-9df1-5737d86517dd",
      "number": 12,
      "content": "Ta cũng là Đấng Cuối Cùng.",
      "order": 3,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.716Z",
      "updatedAt": "2024-09-23T06:01:22.716Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "e70e1047-d8a8-43fd-a496-e2fdc6d20e64",
      "number": 13,
      "content": "Chính tay Ta đã đặt nền trái đất;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.720Z",
      "updatedAt": "2024-09-23T06:01:22.720Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "aaa51660-ee11-492f-948e-c03d9ccae324",
      "number": 13,
      "content": "Tay phải Ta đã trải rộng bầu trời;",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.724Z",
      "updatedAt": "2024-09-23T06:01:22.724Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "df23dc60-c94a-4e28-8e0d-fad650458ef4",
      "number": 13,
      "content": "Khi Ta gọi chúng,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.728Z",
      "updatedAt": "2024-09-23T06:01:22.728Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "dc7136bc-7a14-4428-8b52-fe1f71d90699",
      "number": 13,
      "content": "Chúng cùng nhau đứng dậy.",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.732Z",
      "updatedAt": "2024-09-23T06:01:22.732Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "7abee9c5-7115-4c8e-acb1-7ea4224d9205",
      "number": 14,
      "content": "Hỡi tất cả các ngươi, hãy họp lại và lắng nghe!",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.737Z",
      "updatedAt": "2024-09-23T06:01:22.737Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "1b952d49-1023-4920-8a9d-5f33b3aeb2e3",
      "number": 14,
      "content": "Ai trong vòng chúng đã công bố những điều ấy?",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.741Z",
      "updatedAt": "2024-09-23T06:01:22.741Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "6114cb4e-89e4-494a-8d1e-0924f93e9cfe",
      "number": 14,
      "content": "**CHÚA** yêu thương nó;",
      "order": 2,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.745Z",
      "updatedAt": "2024-09-23T06:01:22.745Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "616de32f-b444-4118-8a6b-e027782bb0db",
      "number": 14,
      "content": "Ngài sẽ thực hiện điều đẹp ý Ngài đối với Ba-by-lôn;",
      "order": 3,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.749Z",
      "updatedAt": "2024-09-23T06:01:22.749Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "d670bbda-4652-487a-b050-f66a4eb10dba",
      "number": 14,
      "content": "Ngài sẽ đưa tay ra nghịch lại người Canh-đê.",
      "order": 4,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.753Z",
      "updatedAt": "2024-09-23T06:01:22.753Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "59930db9-7a97-4ea6-9035-c5e2c920f8ff",
      "number": 15,
      "content": "Ta, chính Ta, đã phán; phải, Ta đã gọi nó;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.757Z",
      "updatedAt": "2024-09-23T06:01:22.757Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "85992f66-3453-4532-82eb-0d136b2ea168",
      "number": 15,
      "content": "Ta đã dẫn dắt nó, nên đường lối nó sẽ thành công thịnh vượng.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.761Z",
      "updatedAt": "2024-09-23T06:01:22.761Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "b1ac6a93-cdef-47da-a9f6-94ed421a8011",
      "number": 16,
      "content": "Hãy đến gần Ta và nghe rõ điều này:",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.766Z",
      "updatedAt": "2024-09-23T06:01:22.766Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "c24b4427-60ac-437d-b518-01206ef42568",
      "number": 16,
      "content": "Từ ban đầu, Ta không nói điều gì bí ẩn;",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.771Z",
      "updatedAt": "2024-09-23T06:01:22.771Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "e5a4cd20-396a-4bce-8ad4-6fc550b042d5",
      "number": 16,
      "content": "Lúc nó xảy ra, Ta đã có ở đó rồi.",
      "order": 2,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.776Z",
      "updatedAt": "2024-09-23T06:01:22.776Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "952db816-5f16-4940-b1d6-8d86554394c5",
      "number": 16,
      "content": "Và bây giờ **CHÚA** Hằng Hữu và Thần của Ngài đã sai tôi.",
      "order": 3,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.781Z",
      "updatedAt": "2024-09-23T06:01:22.781Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "2af4db5d-5471-4015-902f-82ecc3a7ee0f",
      "number": 17,
      "content": "**CHÚA**, Đấng Cứu Chuộc ngươi,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.785Z",
      "updatedAt": "2024-09-23T06:01:22.785Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "8e8f5420-b03f-49d1-b558-f51da31794a9",
      "number": 17,
      "content": "Đấng Thánh của I-sơ-ra-ên, phán thế này:",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.790Z",
      "updatedAt": "2024-09-23T06:01:22.790Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "2ee07d33-9c74-4e90-8062-a0c24a579d2d",
      "number": 17,
      "content": "Ta là **CHÚA**, Đức Chúa Trời của ngươi,",
      "order": 2,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.795Z",
      "updatedAt": "2024-09-23T06:01:22.795Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "15fbcb9f-3145-4251-9839-2a18922e6a50",
      "number": 17,
      "content": "Đấng dạy bảo ngươi để ngươi được phước,",
      "order": 3,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.799Z",
      "updatedAt": "2024-09-23T06:01:22.799Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "a51735cd-3695-4f77-9534-3670e3de2640",
      "number": 17,
      "content": "Đấng dẫn dắt ngươi vào đường lối ngươi nên đi.",
      "order": 4,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.804Z",
      "updatedAt": "2024-09-23T06:01:22.804Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "34fcd413-e116-4498-832b-92d9850d0fad",
      "number": 18,
      "content": "Ôi, ước chi ngươi chú ý đến các điều răn Ta,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.809Z",
      "updatedAt": "2024-09-23T06:01:22.809Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "10dd7cf1-6278-4f54-b0a5-ddee4647e981",
      "number": 18,
      "content": "Thì hòa bình thịnh vượng của ngươi sẽ còn mãi như dòng sông vô tận,",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.814Z",
      "updatedAt": "2024-09-23T06:01:22.814Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "c1273172-e992-4e87-a48f-c1cb0997c1e9",
      "number": 18,
      "content": "Sự thành công của ngươi sẽ không ngừng như sóng ở đại dương,",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.818Z",
      "updatedAt": "2024-09-23T06:01:22.818Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "c8a553b8-83a7-4d3c-a8da-83211264db0b",
      "number": 19,
      "content": "Con cháu ngươi sẽ đông như cát nơi bờ biển,",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.823Z",
      "updatedAt": "2024-09-23T06:01:22.823Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "37272058-eaa2-44a7-af84-1610d966ec9f",
      "number": 19,
      "content": "Dòng dõi ngươi sẽ nhiều tựa cát của đại dương,",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.828Z",
      "updatedAt": "2024-09-23T06:01:22.828Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "00f22712-aa14-4b3a-b440-13dde0eb8942",
      "number": 19,
      "content": "Danh ngươi sẽ không bao giờ bị xóa bỏ,",
      "order": 2,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.832Z",
      "updatedAt": "2024-09-23T06:01:22.832Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "0c87495b-85a0-4b63-af32-c5134ebbf5da",
      "number": 19,
      "content": "Và chẳng bao giờ bị diệt mất trước mặt Ta.",
      "order": 3,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.836Z",
      "updatedAt": "2024-09-23T06:01:22.836Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "f265be31-46cc-4975-acc0-860eb7cb1219",
      "number": 20,
      "content": "Hãy rời khỏi Ba-by-lôn, hãy trốn khỏi dân Canh-đê,",
      "order": 0,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.841Z",
      "updatedAt": "2024-09-23T06:01:22.841Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "ce5d6307-f2ab-4a94-90cf-7c9629009b2b",
      "number": 20,
      "content": "Hãy cất tiếng hát ca, hãy tuyên bố,",
      "order": 1,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.845Z",
      "updatedAt": "2024-09-23T06:01:22.845Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "26a20b8b-ec19-48e3-a696-d05a27c63b34",
      "number": 20,
      "content": "Hãy công bố điều ấy ra, hãy truyền tin ấy đến tận cùng trái đất,",
      "order": 2,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.849Z",
      "updatedAt": "2024-09-23T06:01:22.849Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "df937c0b-d51a-46ee-afa8-4732ecb644e3",
      "number": 20,
      "content": "Bảo rằng, “**CHÚA** cứu chuộc Gia-cốp đầy tớ Ngài.”",
      "order": 3,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.853Z",
      "updatedAt": "2024-09-23T06:01:22.853Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "a7ac481d-6628-4c7c-a509-65f7656edcdc",
      "number": 21,
      "content": "Dù Ngài dẫn họ đi qua sa mạc, họ cũng không khát nước,",
      "order": 0,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.858Z",
      "updatedAt": "2024-09-23T06:01:22.858Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "db8428f6-221a-4a34-94ab-85e44e6bb871",
      "number": 21,
      "content": "Vì Ngài khiến nước từ vầng đá chảy ra cho họ uống.",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.863Z",
      "updatedAt": "2024-09-23T06:01:22.863Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "fa6791f6-458c-4f4e-9f53-4311751f9887",
      "number": 21,
      "content": "Ngài chẻ vầng đá, và nước đã trào ra.",
      "order": 2,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.868Z",
      "updatedAt": "2024-09-23T06:01:22.868Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    },
    {
      "id": "17d30475-78f1-4ef8-b7f0-481996669401",
      "number": 22,
      "content": "Tuy nhiên phường gian tà sẽ không có bình an, **CHÚA** phán.",
      "order": 0,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:22.873Z",
      "updatedAt": "2024-09-23T06:01:22.873Z",
      "chapterId": "024a6c87-54bd-4eed-a834-d49b4e501661"
    }
  ],
