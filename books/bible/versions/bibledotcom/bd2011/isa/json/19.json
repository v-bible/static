      "chapterId": "ba3192c0-b979-48a4-a8f6-6cf2d087d2af"
    }
  ],
  "headings": [
    {
      "id": "833e8993-87bb-45ea-acdf-9d469347973a",
      "content": "Sứ Điệp Nghịch Lại Ai-cập",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T10:49:33.979Z",
      "updatedAt": "2025-04-22T10:49:33.979Z",
      "verseId": "4cceb401-e244-47f4-8fe7-c5c54af26a14",
      "chapterId": "ba3192c0-b979-48a4-a8f6-6cf2d087d2af"
    },
    {
      "id": "97298b5e-74f7-43e9-800b-c5bc597ebb16",
      "content": "Ai-cập, A-sy-ri, và I-sơ-ra-ên Được Phước",
      "level": 1,
      "order": 0,
      "createdAt": "2025-04-22T10:49:34.691Z",
      "updatedAt": "2025-04-22T10:49:34.691Z",
      "verseId": "c2037859-d760-4f62-ae28-85e0d752693f",
      "chapterId": "ba3192c0-b979-48a4-a8f6-6cf2d087d2af"
    }
  ],
