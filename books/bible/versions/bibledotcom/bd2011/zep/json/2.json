    },
    {
      "id": "2a023fe6-71df-4c54-be75-14f333dd4e93",
      "content": "Dân sống ở miền thượng lưu Sông Nin",
      "position": 28,
      "order": 1,
      "createdAt": "2025-04-24T02:14:52.670Z",
      "updatedAt": "2025-04-24T02:14:52.670Z",
