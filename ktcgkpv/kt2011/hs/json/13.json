    },
    {
      "id": "2442f53f-8dfa-4ece-bb35-374786271edb",
      "content": "Vương quyền chấm dứt",
      "order": 0,
      "createdAt": "2024-09-23T04:40:33.368Z",
      "updatedAt": "2024-09-23T04:40:33.368Z",
      "verseId": "f07dcfa7-ea59-4bcb-ba0c-7d8560922a41",
      "chapterId": "8bd2b88f-ff0b-4939-bce8-559c0eb3c4b2"
    },
    {
      "id": "bf3de91d-dcc2-4e22-bf46-b4c0dab7ee8b",
