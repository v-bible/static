    },
    {
      "id": "0848512f-e78a-4c01-87c6-3c48ed237178",
      "content": "Vương quyền chấm dứt",
      "order": 0,
      "createdAt": "2024-09-10T06:02:52.066Z",
      "updatedAt": "2024-09-10T06:02:52.066Z",
      "verseId": "f0b3f002-f29e-45ca-b672-c64f06de47d1",
