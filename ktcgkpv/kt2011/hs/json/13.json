    },
    {
      "id": "a3a7adf9-e2cc-4e79-aa7b-ddf991323ded",
      "content": "Sự sụp đổ không tránh được",
      "order": 0,
      "createdAt": "2024-09-10T06:02:52.094Z",
      "updatedAt": "2024-09-10T06:02:52.094Z",
