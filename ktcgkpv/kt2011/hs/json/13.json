    },
    {
      "id": "e226e563-c562-47dd-b2ca-07122c47efab",
      "content": "Vương quyền chấm dứt",
      "order": 0,
      "createdAt": "2024-09-17T10:33:42.851Z",
      "updatedAt": "2024-09-17T10:33:42.851Z",
      "verseId": "47ff3237-d02b-4848-973e-6a0e5fb3449d",
      "chapterId": "eff1754a-e965-4ada-bc28-b31518452ac4"
    },
    {
      "id": "870c35fc-5309-49b2-8cee-650271fb0db4",
      "content": "Sự sụp đổ không tránh được",
      "order": 0,
      "createdAt": "2024-09-17T10:33:42.914Z",
      "updatedAt": "2024-09-17T10:33:42.914Z",
      "verseId": "bd86e81e-9fbc-492a-be0a-364a78016325",
      "chapterId": "eff1754a-e965-4ada-bc28-b31518452ac4"
    }
  ],
