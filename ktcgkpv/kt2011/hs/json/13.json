    {
      "id": "14e018fc-7f66-4d60-8dac-78dae0c47de5",
      "content": "Trừng phạt tội vô ơn bạc nghĩa",
      "order": 0,
      "createdAt": "2024-09-17T10:33:42.772Z",
      "updatedAt": "2024-09-17T10:33:42.772Z",
