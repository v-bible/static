    },
    {
      "id": "e41ffc88-22b9-4bb1-b163-d38cbc3f979f",
      "content": "Trừng phạt tội ác tại Ba-an Pơ-o",
      "order": 0,
      "createdAt": "2024-09-17T10:34:09.680Z",
      "updatedAt": "2024-09-17T10:34:09.680Z",
