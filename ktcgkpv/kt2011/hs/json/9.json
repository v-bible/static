    },
    {
      "id": "de1d1ada-f21f-4708-8271-c0e4cc5cf621",
      "content": "Trừng phạt tội ác tại Ghin-gan",
      "order": 0,
      "createdAt": "2024-09-17T10:34:09.739Z",
      "updatedAt": "2024-09-17T10:34:09.739Z",
