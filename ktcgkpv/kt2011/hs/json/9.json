    },
    {
      "id": "6228e86f-e796-45b6-8816-27446596aa2d",
      "content": "Trừng phạt tội ác tại Ba-an Pơ-o",
      "order": 0,
      "createdAt": "2024-09-10T06:03:40.336Z",
      "updatedAt": "2024-09-10T06:03:40.336Z",
