      "chapterId": "80120a83-72d7-4fb7-af27-3c399801be07"
    },
    {
      "id": "b5594d43-e504-4286-a661-04f09bb3ee78",
      "content": "Trừng phạt tội ác tại Ghin-gan",
      "order": 0,
      "createdAt": "2024-09-10T06:03:40.473Z",
      "updatedAt": "2024-09-10T06:03:40.473Z",
      "verseId": "4fa11072-19b2-4ef7-803c-092a2161e423",
