    }
  ],
  "references": [
    {
      "id": "830ce50d-efeb-4f98-9633-3b24305a1517",
      "content": "Ds 22,36; Is 15–16; Gr 48; Ed 25,8-11; Xp 2,8-11",
      "position": 0,
      "order": 0,
      "createdAt": "2024-09-17T10:35:06.175Z",
      "updatedAt": "2024-09-17T10:35:06.175Z",
      "headingId": "63975689-78c5-4def-bd29-dfefda2198e6",
      "chapterId": "f8dd664e-f1da-4859-af3b-179f5e6482df"
    },
    {
      "id": "c065c882-1b54-4a80-9316-028fb7d8db64",
      "content": "Lv 26,14-15; Is 5,24; Gr 7,28",
      "position": 12,
      "order": 1,
