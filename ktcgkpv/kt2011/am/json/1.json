      "chapterId": "36e7bf8d-594a-44d4-a179-004dca554da6"
    },
    {
      "id": "a39ed44b-c0bd-48df-8a70-4f51c96f4c7f",
      "content": "Ga-da và Phi-li-tinh",
      "order": 0,
      "createdAt": "2024-09-10T06:07:02.634Z",
      "updatedAt": "2024-09-10T06:07:02.634Z",
