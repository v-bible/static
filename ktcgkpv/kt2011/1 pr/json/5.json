    },
    {
      "id": "d9ddc448-80b5-4fc2-a9ae-e351e39a97f1",
      "content": "X. Ep 6,11-13 ; Gc 4,7.",
      "position": 46,
      "order": 12,
      "createdAt": "2024-09-10T01:26:02.077Z",
      "updatedAt": "2024-09-10T01:26:02.077Z",
