      "chapterId": "e6d2bdfe-5ed9-43e3-9a59-83c0777b5774"
    },
    {
      "id": "cd78410c-3fbc-4ef5-b379-98a5d416f748",
      "content": "Tin cậy vào một mình Thiên Chúa",
      "order": 0,
      "createdAt": "2024-09-10T05:07:26.720Z",
      "updatedAt": "2024-09-10T05:07:26.720Z",
      "verseId": "b216b1c0-8182-4a8e-a190-0cec2100c3cc",
      "chapterId": "e6d2bdfe-5ed9-43e3-9a59-83c0777b5774"
    },
    {
      "id": "0b59d485-731a-41a0-b17a-3f05d3ed0fbb",
      "content": "Coi chừng người gian ác",
      "order": 0,
      "createdAt": "2024-09-10T05:07:26.863Z",
      "updatedAt": "2024-09-10T05:07:26.863Z",
      "verseId": "1e329d4b-d44f-4480-a462-f1f81f74dc22",
      "chapterId": "e6d2bdfe-5ed9-43e3-9a59-83c0777b5774"
    }
  ],
