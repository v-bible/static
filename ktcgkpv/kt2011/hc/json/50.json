    },
    {
      "id": "09d23889-929b-4b9c-99eb-724e84c9e01a",
      "content": "Chống lại ba dân tộc",
      "order": 0,
      "createdAt": "2024-09-23T04:34:50.141Z",
      "updatedAt": "2024-09-23T04:34:50.141Z",
