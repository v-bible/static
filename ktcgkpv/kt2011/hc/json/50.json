      "createdAt": "2024-09-17T10:11:22.237Z",
      "updatedAt": "2024-09-17T10:11:22.237Z",
      "verseId": "59743e2d-b11a-4176-876c-081852ad746f",
      "chapterId": "e54e236f-8a14-4add-a56d-4b933665d01b"
    },
    {
      "id": "f8614322-a054-4a61-adb8-bddda6413fe9",
      "content": "Chống lại ba dân tộc",
      "order": 0,
      "createdAt": "2024-09-17T10:11:22.291Z",
      "updatedAt": "2024-09-17T10:11:22.291Z",
