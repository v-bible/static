    },
    {
      "id": "b11a3aac-eaa8-44fd-9db9-db42e385ce2b",
      "content": "Chống tính kiêu ngạo",
      "order": 0,
      "createdAt": "2024-09-10T05:15:24.695Z",
      "updatedAt": "2024-09-10T05:15:24.695Z",
