    },
    {
      "id": "8c9df0fa-73f3-4324-a13f-0c7aaae8c754",
      "content": "Những người đáng trọng",
      "order": 0,
      "createdAt": "2024-09-10T05:15:24.899Z",
      "updatedAt": "2024-09-10T05:15:24.899Z",
      "verseId": "15c8c166-ef3f-446e-a46f-f6186e2a438f",
      "chapterId": "08d4edff-6fb5-49db-8f58-4cbc86291ac8"
    },
    {
      "id": "fe1c39ca-b052-4cd9-984f-fedac6e65341",
      "content": "Đức khiêm nhường và lòng chân thật",
      "order": 0,
      "createdAt": "2024-09-10T05:15:25.022Z",
      "updatedAt": "2024-09-10T05:15:25.022Z",
