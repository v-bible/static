    {
      "id": "2cdd742b-45a7-4585-b2cc-920c9120fdff",
      "content": "<i>Ở phía bắc</i> của chi tộc Giu-đa.",
      "position": 150,
      "order": 2,
      "createdAt": "2024-09-17T09:23:45.978Z",
