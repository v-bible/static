{
  "chapter": {
    "id": "e15791ac-c0d6-4ee1-bd40-74e8834a909e",
    "number": 26,
    "ref": "",
    "createdAt": "2024-09-17T08:41:12.688Z",
    "updatedAt": "2024-09-17T08:41:12.688Z",
    "bookId": "0dd63fc9-ff54-4f5e-9db1-ff7efa3e44ff"
  },
  "verses": [
    {
      "id": "97754ce4-065e-4470-a8e5-d24e5c72198b",
      "number": 1,
      "content": "Năm thứ mười một, ngày đầu tháng, có lời ĐỨC CHÚA phán với tôi rằng :",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:29:06.508Z",
      "updatedAt": "2024-09-17T10:29:06.508Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "1df9c2c9-f325-40ee-b18b-a0e61ae82cc5",
      "number": 2,
      "content": "Hỡi con người, vì Tia đã nói",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.528Z",
      "updatedAt": "2024-09-17T10:29:06.528Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "cb6e664a-209b-4f05-8594-c72dcf5a958c",
      "number": 2,
      "content": "chống lại Giê-ru-sa-lem :",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.533Z",
      "updatedAt": "2024-09-17T10:29:06.533Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "aa7f30a3-1a08-422e-a2a3-e870e10d33f9",
      "number": 2,
      "content": "“A ha !",
      "order": 2,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.537Z",
      "updatedAt": "2024-09-17T10:29:06.537Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "455bcd55-e7af-4e4d-a9ff-e32cfa0e97d2",
      "number": 2,
      "content": "Cửa của các dân đã tan nát ;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.546Z",
      "updatedAt": "2024-09-17T10:29:06.546Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "067fbba6-bf44-4c88-8e75-01a0a3c83bfc",
      "number": 2,
      "content": "đến lượt ta phú túc,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.551Z",
      "updatedAt": "2024-09-17T10:29:06.551Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "17c0af4f-b149-4163-a2ca-cf1da3c9f07e",
      "number": 2,
      "content": "còn nó phải tan hoang”,",
      "order": 5,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.555Z",
      "updatedAt": "2024-09-17T10:29:06.555Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "4d7e40c2-ea03-4e18-8a0b-c6f2ad8ba48d",
      "number": 3,
      "content": "nên Chúa Thượng",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.563Z",
      "updatedAt": "2024-09-17T10:29:06.563Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "149f08a1-e02a-482b-a33f-3fd1d229f2bb",
      "number": 3,
      "content": "là ĐỨC CHÚA phán thế này :",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.570Z",
      "updatedAt": "2024-09-17T10:29:06.570Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "4be6e07c-c280-4670-bbad-be01d474b971",
      "number": 3,
      "content": "Ta sẽ trừng phạt ngươi, hỡi Tia,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.574Z",
      "updatedAt": "2024-09-17T10:29:06.574Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "e1ea48e0-744c-4ea6-8b2f-c8636917e98e",
      "number": 3,
      "content": "sẽ làm cho nhiều dân tộc",
      "order": 3,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.584Z",
      "updatedAt": "2024-09-17T10:29:06.584Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "27c72534-b4b8-4e55-9383-e15d6dce7eaa",
      "number": 3,
      "content": "nổi lên chống lại ngươi",
      "order": 4,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.588Z",
      "updatedAt": "2024-09-17T10:29:06.588Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "a27bc3f1-63b7-4e40-b38c-466d49e23402",
      "number": 3,
      "content": "như biển khơi dậy sóng.",
      "order": 5,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.593Z",
      "updatedAt": "2024-09-17T10:29:06.593Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "1de6dd81-77d1-4aa1-82e9-10eaf6e68ed2",
      "number": 4,
      "content": "Chúng sẽ triệt hạ",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.597Z",
      "updatedAt": "2024-09-17T10:29:06.597Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "cfa088ef-8216-4540-a02b-26a2828743ec",
      "number": 4,
      "content": "tường luỹ thành Tia,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.602Z",
      "updatedAt": "2024-09-17T10:29:06.602Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "111ed559-b77d-41f8-bed7-1afe7772fa59",
      "number": 4,
      "content": "sẽ phá huỷ các tháp ;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.607Z",
      "updatedAt": "2024-09-17T10:29:06.607Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "3336ddd7-38ab-45ca-a0a8-51fbb7621c27",
      "number": 4,
      "content": "còn Ta sẽ quét bụi khỏi thành",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.611Z",
      "updatedAt": "2024-09-17T10:29:06.611Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "e68b4bed-6c9d-4c53-a6ee-ec9a2f908d35",
      "number": 4,
      "content": "và biến nó ra núi đá trọc.",
      "order": 4,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.616Z",
      "updatedAt": "2024-09-17T10:29:06.616Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "ae479f91-c7dd-4fde-a924-064c21b5e712",
      "number": 5,
      "content": "Nó sẽ thành chỗ phơi lưới,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.620Z",
      "updatedAt": "2024-09-17T10:29:06.620Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "0387ad0c-5a5e-4c9e-b5d1-f07359c0e330",
      "number": 5,
      "content": "giữa biển khơi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.625Z",
      "updatedAt": "2024-09-17T10:29:06.625Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "ea6def76-b1eb-47c7-aa62-30eed1135ef6",
      "number": 5,
      "content": "bởi vì chính Ta đã phán,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.630Z",
      "updatedAt": "2024-09-17T10:29:06.630Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "30111218-4522-4b7e-b0ac-b47318e23217",
      "number": 5,
      "content": "– sấm ngôn của Chúa Thượng",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.634Z",
      "updatedAt": "2024-09-17T10:29:06.634Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "7b1694bf-eccd-4ecd-9309-6700e62dc74d",
      "number": 5,
      "content": "là ĐỨC CHÚA.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.638Z",
      "updatedAt": "2024-09-17T10:29:06.638Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "6288988e-0920-413c-988e-981cae9c910e",
      "number": 5,
      "content": "Nó sẽ là mồi ngon cho các dân tộc.",
      "order": 5,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.643Z",
      "updatedAt": "2024-09-17T10:29:06.643Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "29b436b8-8066-4f08-9eaf-6edf830c023f",
      "number": 6,
      "content": "Các con gái của nó",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.648Z",
      "updatedAt": "2024-09-17T10:29:06.648Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "c6c59709-8a9d-4f4b-b260-51bc81558647",
      "number": 6,
      "content": "đang ở ngoài đồng",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.657Z",
      "updatedAt": "2024-09-17T10:29:06.657Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "1fee9609-0651-4202-a9f1-91ed3e8f653a",
      "number": 6,
      "content": "sẽ bị gươm tàn sát ;",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.661Z",
      "updatedAt": "2024-09-17T10:29:06.661Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "5ab2e577-44d1-4108-9ac3-eb8bd9fd5cf5",
      "number": 6,
      "content": "bấy giờ, chúng sẽ nhận biết",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.666Z",
      "updatedAt": "2024-09-17T10:29:06.666Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "3e45236b-d41e-41af-b91c-18f5dba1b55c",
      "number": 6,
      "content": "chính Ta là ĐỨC CHÚA.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.670Z",
      "updatedAt": "2024-09-17T10:29:06.670Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "f30e2e50-6028-4cb1-96ae-44243084eff3",
      "number": 7,
      "content": "Quả vậy, Chúa Thượng",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.675Z",
      "updatedAt": "2024-09-17T10:29:06.675Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "e35eb85a-7dde-4e60-b823-922273ac5c43",
      "number": 7,
      "content": "là ĐỨC CHÚA phán thế này :",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.679Z",
      "updatedAt": "2024-09-17T10:29:06.679Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "508ae7e3-ba6e-4022-a83e-cc3c95af6adf",
      "number": 7,
      "content": "Đây, từ phương bắc,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.683Z",
      "updatedAt": "2024-09-17T10:29:06.683Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "c515bdf6-4d25-40f0-9ea4-2d4ad35efc6f",
      "number": 7,
      "content": "Ta sắp dẫn đến Tia",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.687Z",
      "updatedAt": "2024-09-17T10:29:06.687Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "20deef66-638d-4122-836e-0430b571179d",
      "number": 7,
      "content": "Na-bu-cô-đô-nô-xo,",
      "order": 4,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.691Z",
      "updatedAt": "2024-09-17T10:29:06.691Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "629c4544-addf-4dec-9ca0-0c40db4eead4",
      "number": 7,
      "content": "vua Ba-by-lon, vua các vua,",
      "order": 5,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.699Z",
      "updatedAt": "2024-09-17T10:29:06.699Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "20e903bb-c031-477c-96cd-e9c5729682c8",
      "number": 7,
      "content": "cùng với ngựa,",
      "order": 6,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.703Z",
      "updatedAt": "2024-09-17T10:29:06.703Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "52879a8d-79d2-4ce3-a58f-4bb267690200",
      "number": 7,
      "content": "chiến xa và kỵ mã,",
      "order": 7,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.707Z",
      "updatedAt": "2024-09-17T10:29:06.707Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "75da8d91-c56b-412b-b0ad-bcbcd60f9c63",
      "number": 7,
      "content": "một đoàn lũ",
      "order": 8,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.712Z",
      "updatedAt": "2024-09-17T10:29:06.712Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "48bd3660-3a0e-4099-a8a4-e643f4a6e549",
      "number": 7,
      "content": "và một đám dân đông đảo.",
      "order": 9,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.716Z",
      "updatedAt": "2024-09-17T10:29:06.716Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "4ee20b4c-3982-4692-93bc-969ae7097e04",
      "number": 8,
      "content": "Các con gái của ngươi",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.725Z",
      "updatedAt": "2024-09-17T10:29:06.725Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "87c815c4-d970-4a9c-94d8-e0a4826e6a17",
      "number": 8,
      "content": "đang ở ngoài đồng,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.729Z",
      "updatedAt": "2024-09-17T10:29:06.729Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "3b18d178-dedb-4b28-80fb-7b9f2e012c29",
      "number": 8,
      "content": "nó sẽ dùng gươm tàn sát.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.733Z",
      "updatedAt": "2024-09-17T10:29:06.733Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "462b7845-0397-45d7-b259-3fd998315675",
      "number": 8,
      "content": "Nó sẽ đắp chiến luỹ, đắp ụ,",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.738Z",
      "updatedAt": "2024-09-17T10:29:06.738Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "9c3bb135-cdc4-4537-b37f-89067fc7229b",
      "number": 8,
      "content": "và dựng thuẫn chống lại ngươi.",
      "order": 4,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.750Z",
      "updatedAt": "2024-09-17T10:29:06.750Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "fbb5262c-97d3-4d55-bc01-f110e4346566",
      "number": 9,
      "content": "Nhắm vào các tường luỹ",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.755Z",
      "updatedAt": "2024-09-17T10:29:06.755Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "79cdd3cf-1645-44c6-a6f7-5eadec9f36e0",
      "number": 9,
      "content": "của ngươi, nó sẽ đặt máy phá",
