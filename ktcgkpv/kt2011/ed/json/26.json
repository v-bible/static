      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.854Z",
      "updatedAt": "2024-09-17T10:29:06.854Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
