{
  "chapter": {
    "id": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1",
    "number": 26,
    "ref": "",
    "createdAt": "2024-09-10T00:39:05.922Z",
    "updatedAt": "2024-09-10T00:39:05.922Z",
    "bookId": "816710ca-073b-4c13-b6c6-82e73a63961f"
  },
  "verses": [
    {
      "id": "4d54b184-253a-4526-9094-ddfda46ae04a",
      "number": 1,
      "content": "Năm thứ mười một, ngày đầu tháng, có lời ĐỨC CHÚA phán với tôi rằng :",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T05:26:56.406Z",
      "updatedAt": "2024-09-10T05:26:56.406Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "1808df89-4194-4c4d-88cb-c4b7fed33b92",
      "number": 2,
      "content": "Hỡi con người, vì Tia đã nói",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.422Z",
      "updatedAt": "2024-09-10T05:26:56.422Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "f6915747-8807-45f3-80ab-a6c9f2992b05",
      "number": 2,
      "content": "chống lại Giê-ru-sa-lem :",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.424Z",
      "updatedAt": "2024-09-10T05:26:56.424Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "e075493b-57be-410c-86bf-147e7756dd82",
      "number": 2,
      "content": "“A ha !",
      "order": 2,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.426Z",
      "updatedAt": "2024-09-10T05:26:56.426Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "fc40be2a-aec6-47d5-a925-983ffc1d31a2",
      "number": 2,
      "content": "Cửa của các dân đã tan nát ;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.430Z",
      "updatedAt": "2024-09-10T05:26:56.430Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "f60b0f64-65f6-4e11-aaa8-c65d48ade2f6",
      "number": 2,
      "content": "đến lượt ta phú túc,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.433Z",
      "updatedAt": "2024-09-10T05:26:56.433Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "22c6f303-ab64-4c95-a128-541b0c9fc253",
      "number": 2,
      "content": "còn nó phải tan hoang”,",
      "order": 5,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.435Z",
      "updatedAt": "2024-09-10T05:26:56.435Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "4e03b950-2f11-45f9-a15d-770a8477d42e",
      "number": 3,
      "content": "nên Chúa Thượng",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.437Z",
      "updatedAt": "2024-09-10T05:26:56.437Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "3f2d8e7b-06c7-4f62-953e-936d9b3793de",
      "number": 3,
      "content": "là ĐỨC CHÚA phán thế này :",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.439Z",
      "updatedAt": "2024-09-10T05:26:56.439Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "68d6baa3-cbef-43b0-bb23-bdef79ea2717",
      "number": 3,
      "content": "Ta sẽ trừng phạt ngươi, hỡi Tia,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.441Z",
      "updatedAt": "2024-09-10T05:26:56.441Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "a719d35a-086d-483b-997e-48634232fa49",
      "number": 3,
      "content": "sẽ làm cho nhiều dân tộc",
      "order": 3,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.445Z",
      "updatedAt": "2024-09-10T05:26:56.445Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "bf83d4b7-a943-4642-96d5-39ee81c5100d",
      "number": 3,
      "content": "nổi lên chống lại ngươi",
      "order": 4,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.447Z",
      "updatedAt": "2024-09-10T05:26:56.447Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "6216899f-96c0-4bec-8566-765c8486705b",
      "number": 3,
      "content": "như biển khơi dậy sóng.",
      "order": 5,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.449Z",
      "updatedAt": "2024-09-10T05:26:56.449Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "a9509680-a617-4adc-a81a-84bf1521f688",
      "number": 4,
      "content": "Chúng sẽ triệt hạ",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.451Z",
      "updatedAt": "2024-09-10T05:26:56.451Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "6ce70c56-a21d-453c-80b8-1c2b005e3684",
      "number": 4,
      "content": "tường luỹ thành Tia,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.454Z",
      "updatedAt": "2024-09-10T05:26:56.454Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "c18960b9-f1fe-4044-a080-eeb89da00062",
      "number": 4,
      "content": "sẽ phá huỷ các tháp ;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.456Z",
      "updatedAt": "2024-09-10T05:26:56.456Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "fe58f78c-3854-41db-b20d-ef02d4e83aab",
      "number": 4,
      "content": "còn Ta sẽ quét bụi khỏi thành",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.459Z",
      "updatedAt": "2024-09-10T05:26:56.459Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "564da467-0de9-4ff7-90d0-dff42c52ffdd",
      "number": 4,
      "content": "và biến nó ra núi đá trọc.",
      "order": 4,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.464Z",
      "updatedAt": "2024-09-10T05:26:56.464Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "a98fc826-81fc-4b24-8177-897937afa327",
      "number": 5,
      "content": "Nó sẽ thành chỗ phơi lưới,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.469Z",
      "updatedAt": "2024-09-10T05:26:56.469Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "15025e27-d409-4254-9e37-c585862646ed",
      "number": 5,
      "content": "giữa biển khơi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.474Z",
      "updatedAt": "2024-09-10T05:26:56.474Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "ae1ae3b1-faac-4f9d-9b3e-e5ef68c1b940",
      "number": 5,
      "content": "bởi vì chính Ta đã phán,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.479Z",
      "updatedAt": "2024-09-10T05:26:56.479Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "8e6ab36b-356b-496b-9bd7-47dd0df3c0e3",
      "number": 5,
      "content": "– sấm ngôn của Chúa Thượng",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.484Z",
      "updatedAt": "2024-09-10T05:26:56.484Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "9623f9c5-4150-4979-9591-8276c947463b",
      "number": 5,
      "content": "là ĐỨC CHÚA.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.488Z",
      "updatedAt": "2024-09-10T05:26:56.488Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "3ba6eb2d-b64e-4133-bcbf-835a993e1c3a",
      "number": 5,
      "content": "Nó sẽ là mồi ngon cho các dân tộc.",
      "order": 5,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.493Z",
      "updatedAt": "2024-09-10T05:26:56.493Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "74978786-3f59-4b93-a35b-bddce58e0334",
      "number": 6,
      "content": "Các con gái của nó",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.497Z",
      "updatedAt": "2024-09-10T05:26:56.497Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "9ae459b9-7531-4c37-8441-3b3f8bea05ce",
      "number": 6,
      "content": "đang ở ngoài đồng",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.507Z",
      "updatedAt": "2024-09-10T05:26:56.507Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "38f4e061-705f-4d4a-8430-324035a8c5d7",
      "number": 6,
      "content": "sẽ bị gươm tàn sát ;",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.512Z",
      "updatedAt": "2024-09-10T05:26:56.512Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "829e4753-34c0-4698-8322-6fbe04136160",
      "number": 6,
      "content": "bấy giờ, chúng sẽ nhận biết",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.519Z",
      "updatedAt": "2024-09-10T05:26:56.519Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "6481254d-22d8-4ce0-b27f-d35a1afa0928",
      "number": 6,
      "content": "chính Ta là ĐỨC CHÚA.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.523Z",
      "updatedAt": "2024-09-10T05:26:56.523Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "0df1fb8e-0ee7-42fd-8ffa-f1189e9aeade",
      "number": 7,
      "content": "Quả vậy, Chúa Thượng",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.528Z",
      "updatedAt": "2024-09-10T05:26:56.528Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "8b0819b6-157e-41e1-9f7c-cfb94d80d4dd",
      "number": 7,
      "content": "là ĐỨC CHÚA phán thế này :",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.534Z",
      "updatedAt": "2024-09-10T05:26:56.534Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "b70ee3f3-4d5e-472b-9b9d-6a7b0f18aca3",
      "number": 7,
      "content": "Đây, từ phương bắc,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.539Z",
      "updatedAt": "2024-09-10T05:26:56.539Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "4dee6675-0a93-4994-8622-d339c58dc33d",
      "number": 7,
      "content": "Ta sắp dẫn đến Tia",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.543Z",
      "updatedAt": "2024-09-10T05:26:56.543Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "e8881e12-290f-4ccd-b978-65237bb3eeac",
      "number": 7,
      "content": "Na-bu-cô-đô-nô-xo,",
      "order": 4,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.549Z",
      "updatedAt": "2024-09-10T05:26:56.549Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "d0921370-1762-4b1d-a33c-33daa9e33b24",
      "number": 7,
      "content": "vua Ba-by-lon, vua các vua,",
      "order": 5,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.559Z",
      "updatedAt": "2024-09-10T05:26:56.559Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "0f037c97-4638-45f8-9d6d-11e332668c2a",
      "number": 7,
      "content": "cùng với ngựa,",
      "order": 6,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.563Z",
      "updatedAt": "2024-09-10T05:26:56.563Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "f40e441d-b47d-4f9b-863d-25b7b5fcdaff",
      "number": 7,
      "content": "chiến xa và kỵ mã,",
      "order": 7,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.568Z",
      "updatedAt": "2024-09-10T05:26:56.568Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "7d2920ab-a574-472e-bb72-ecfd3c2adc3d",
      "number": 7,
      "content": "một đoàn lũ",
      "order": 8,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.573Z",
      "updatedAt": "2024-09-10T05:26:56.573Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "3596ef65-8fdd-4b28-a66e-c6fd86080d8a",
      "number": 7,
      "content": "và một đám dân đông đảo.",
      "order": 9,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.577Z",
      "updatedAt": "2024-09-10T05:26:56.577Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "8600751a-4351-45ca-87ca-40b8385baae9",
      "number": 8,
      "content": "Các con gái của ngươi",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.587Z",
      "updatedAt": "2024-09-10T05:26:56.587Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "ec8e2a34-5190-4f11-a3fe-d7a4a558d135",
      "number": 8,
      "content": "đang ở ngoài đồng,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.592Z",
      "updatedAt": "2024-09-10T05:26:56.592Z",
      "chapterId": "ddc7c55c-c7e3-4a94-b4c3-9fe740ab70b1"
    },
    {
      "id": "fab7377b-3420-42fb-98f0-b0972eabbde8",
      "number": 8,
      "content": "nó sẽ dùng gươm tàn sát.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T05:26:56.597Z",
