      "id": "dd79a305-1ac0-4850-8692-4bd23d0236f1",
      "number": 14,
      "content": "ngươi sẽ thành chỗ phơi lưới,",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.921Z",
      "updatedAt": "2024-09-17T10:29:06.921Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
