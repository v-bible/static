      "updatedAt": "2024-09-17T10:29:06.846Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "c5546a03-197b-446c-8ae2-338eeb17517d",
      "number": 12,
      "content": "hàng hoá của ngươi,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.850Z",
