      "createdAt": "2024-09-17T10:29:06.913Z",
      "updatedAt": "2024-09-17T10:29:06.913Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "caa63b70-1e48-4268-8282-2f649deb40ff",
      "number": 14,
      "content": "thành núi đá trọc ;",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.917Z",
      "updatedAt": "2024-09-17T10:29:06.917Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
