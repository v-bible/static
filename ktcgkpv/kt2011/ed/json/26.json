      "createdAt": "2024-09-17T10:29:06.768Z",
      "updatedAt": "2024-09-17T10:29:06.768Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "a3891f32-d0d6-4b82-83b7-1a98eae9ebcf",
      "number": 10,
      "content": "Ngựa của nó hàng hàng lớp lớp",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.772Z",
      "updatedAt": "2024-09-17T10:29:06.772Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "ac6b41b8-f6a9-4b99-b603-88a8a01ed18e",
      "number": 10,
      "content": "tung bụi che phủ ngươi ;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.776Z",
      "updatedAt": "2024-09-17T10:29:06.776Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "b52b9612-d6e5-46d2-af11-cc290a23f3b5",
      "number": 10,
      "content": "nghe tiếng vó câu,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:29:06.780Z",
      "updatedAt": "2024-09-17T10:29:06.780Z",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
    {
      "id": "2d5ebc3d-c38a-4faa-8106-6994dcb58a60",
      "number": 10,
      "content": "tiếng xe và xe trận,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
