    {
      "id": "04417fa3-983c-48b8-875e-6e72fbac5405",
      "content": "Is 24,8-9; Gr 25,10; Kh 18,22",
      "position": 14,
      "order": 4,
      "createdAt": "2024-09-17T10:29:06.891Z",
      "updatedAt": "2024-09-17T10:29:06.891Z",
      "verseId": "884320a4-7b1f-4bc3-995e-c073ae99f2ac",
      "chapterId": "e15791ac-c0d6-4ee1-bd40-74e8834a909e"
    },
