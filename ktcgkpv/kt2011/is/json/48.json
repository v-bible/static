      "chapterId": "11209ecc-3b26-4e6f-aee2-b3849f4edef9"
    },
    {
      "id": "023e79c9-c8f5-4c47-83eb-ed89f3673234",
      "content": "Vận mệnh Ít-ra-en",
      "order": 0,
      "createdAt": "2024-09-17T10:24:50.941Z",
      "updatedAt": "2024-09-17T10:24:50.941Z",
      "verseId": "5ff9787e-b2b6-4c3a-8abb-9efa8af79942",
      "chapterId": "11209ecc-3b26-4e6f-aee2-b3849f4edef9"
    },
    {
      "id": "199e6150-3809-4251-91c8-c0e3a38e4b25",
      "content": "Thời lưu đày chấm dứt",
      "order": 0,
      "createdAt": "2024-09-17T10:24:51.046Z",
      "updatedAt": "2024-09-17T10:24:51.046Z",
      "verseId": "432b7fd3-3910-4814-8497-36573061c16f",
      "chapterId": "11209ecc-3b26-4e6f-aee2-b3849f4edef9"
    }
  ],
