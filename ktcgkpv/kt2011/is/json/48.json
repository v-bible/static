      "chapterId": "f6198b4f-26df-4816-becf-c94fe0d6c833"
    },
    {
      "id": "24d7b293-c432-4203-a7da-2ec7a5c35172",
      "content": "Vận mệnh Ít-ra-en",
      "order": 0,
      "createdAt": "2024-09-10T04:44:39.291Z",
      "updatedAt": "2024-09-10T04:44:39.291Z",
      "verseId": "bc47256a-2879-4627-b915-72229720564e",
      "chapterId": "f6198b4f-26df-4816-becf-c94fe0d6c833"
    },
    {
      "id": "88425573-d1ac-4685-aa73-8ca4ec0f07be",
      "content": "Thời lưu đày chấm dứt",
      "order": 0,
      "createdAt": "2024-09-10T04:44:39.347Z",
      "updatedAt": "2024-09-10T04:44:39.347Z",
