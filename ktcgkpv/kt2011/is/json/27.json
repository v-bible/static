      "chapterId": "30afce1f-6a46-4939-82f1-903677686d07"
    },
    {
      "id": "182c85f6-4795-4ad5-b95e-9d2e054784bf",
      "content": "Người Ít-ra-en trở về",
      "order": 0,
      "createdAt": "2024-09-10T04:49:16.053Z",
      "updatedAt": "2024-09-10T04:49:16.053Z",
