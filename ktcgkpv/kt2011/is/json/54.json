      "chapterId": "1c071335-8d5c-452e-98dd-963a3cc2d490"
    },
    {
      "id": "de8564e0-69f8-4319-9002-c48af39c80e3",
      "content": "Is 26,15",
      "position": 12,
      "order": 3,
      "createdAt": "2024-09-23T04:43:34.867Z",
      "updatedAt": "2024-09-23T04:43:34.867Z",
      "verseId": "9e12efde-617e-43ad-81a0-781ca7e9bf85",
      "chapterId": "1c071335-8d5c-452e-98dd-963a3cc2d490"
    },
    {
