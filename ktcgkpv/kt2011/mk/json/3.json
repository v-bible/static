    },
    {
      "id": "8edc96d5-b471-4882-bb0c-f99099b38151",
      "content": "Gr 11,11; Ed 8,18",
      "position": 14,
      "order": 4,
      "createdAt": "2024-09-23T04:50:56.369Z",
      "updatedAt": "2024-09-23T04:50:56.369Z",
      "verseId": "8dbb2b1b-7f0a-4b00-b903-20e51c6058aa",
      "chapterId": "98073fe2-8a0f-42a9-bd59-e81e3d95938e"
    },
    {
      "id": "dae3e463-481e-4a78-a03f-e7ef92f3a5fe",
