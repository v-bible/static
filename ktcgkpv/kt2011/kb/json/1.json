    },
    {
      "id": "353af962-4ea8-4afb-824a-d7734a6fd6a5",
      "content": "I. CUỘC ĐỐI THOẠI GIỮA NGÔN SỨ VÀ THIÊN CHÚA",
      "order": 0,
      "createdAt": "2024-09-17T10:20:27.687Z",
      "updatedAt": "2024-09-17T10:20:27.687Z",
      "verseId": "48106a87-5a7a-437f-bbca-7d5a11628642",
      "chapterId": "9678a0e8-6eb5-461b-addc-9be9f0566d0e"
    },
    {
      "id": "666687dc-d1f7-48c9-8117-e1d917e350e5",
      "content": "Sấm ngôn đầu tiên : Dân Can-đê, một tai hoạ ĐỨC CHÚA gửi đến",
      "order": 0,
      "createdAt": "2024-09-17T10:20:27.771Z",
      "updatedAt": "2024-09-17T10:20:27.771Z",
      "verseId": "5ec9fa7a-a2da-45a9-81e9-0535713a9fb0",
      "chapterId": "9678a0e8-6eb5-461b-addc-9be9f0566d0e"
    },
    {
      "id": "ebd1d3c4-4017-4a03-a1d5-d69208774d3a",
      "content": "Lời phàn nàn thứ hai của ngôn sứ : Kẻ áp bức đòi tiền hối lộ",
      "order": 0,
      "createdAt": "2024-09-17T10:20:27.934Z",
      "updatedAt": "2024-09-17T10:20:27.934Z",
      "verseId": "b859e3da-ffa8-408a-89b8-e0f821fa7d03",
      "chapterId": "9678a0e8-6eb5-461b-addc-9be9f0566d0e"
    },
    {
      "id": "3ff06bdf-7146-4930-8c92-38b2c0b69544",
      "content": "Tên sách",
      "order": 1,
      "createdAt": "2024-09-17T10:20:27.681Z",
      "updatedAt": "2024-09-17T10:20:27.681Z",
      "verseId": "1e501402-17c1-4bcb-82f5-c3ce257eb305",
      "chapterId": "9678a0e8-6eb5-461b-addc-9be9f0566d0e"
    },
    {
      "id": "56e1c981-d338-40c8-ae7a-c5b5ec18d789",
      "content": "Lời phàn nàn thứ nhất của ngôn sứ : Sự công chính suy thoái",
      "order": 1,
      "createdAt": "2024-09-17T10:20:27.692Z",
      "updatedAt": "2024-09-17T10:20:27.692Z",
      "verseId": "48106a87-5a7a-437f-bbca-7d5a11628642",
      "chapterId": "9678a0e8-6eb5-461b-addc-9be9f0566d0e"
    }
  ],
