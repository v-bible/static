      "chapterId": "e601d39d-18ba-473b-b536-857627d818ad"
    },
    {
      "id": "b85aa0c9-0aa3-4a59-9962-dac4d44e3a3c",
      "content": "Lặp lại 20,16.",
      "position": 30,
      "order": 2,
      "createdAt": "2024-09-10T04:54:10.128Z",
      "updatedAt": "2024-09-10T04:54:10.128Z",
      "verseId": "9e057ee5-01f4-4dc8-a6cb-f7d4aa5369f0",
      "chapterId": "e601d39d-18ba-473b-b536-857627d818ad"
    },
    {
      "id": "7c88087a-c384-490c-aadd-874decd6e6a3",
      "content": "Câu này chống lại thái độ xun xoe giả hình, che giấu óc vụ lợi. Chúc lành ở đây phải hiểu là chào.",
      "position": 32,
      "order": 3,
      "createdAt": "2024-09-10T04:54:10.143Z",
      "updatedAt": "2024-09-10T04:54:10.143Z",
      "verseId": "05566f10-f43a-4342-9754-f5feb6824610",
      "chapterId": "e601d39d-18ba-473b-b536-857627d818ad"
    },
    {
      "id": "9c59e440-42b3-48e2-98b2-b8ade42e6f4b",
      "content": "ds : <i>trái tim</i>.",
      "position": 6,
      "order": 4,
      "createdAt": "2024-09-10T04:54:10.199Z",
      "updatedAt": "2024-09-10T04:54:10.199Z",
      "verseId": "7c57b560-514d-4325-92c9-96838ca1ea17",
      "chapterId": "e601d39d-18ba-473b-b536-857627d818ad"
    },
    {
      "id": "4f6888ce-710a-463c-b1ee-0f4d7771f50a",
      "content": "ds : <i>con người được thử bằng tiếng khen</i>.",
      "position": 37,
      "order": 5,
      "createdAt": "2024-09-10T04:54:10.239Z",
      "updatedAt": "2024-09-10T04:54:10.239Z",
      "verseId": "41712b1f-2d0b-4879-8d3d-611bbb5fcb9d",
      "chapterId": "e601d39d-18ba-473b-b536-857627d818ad"
    },
    {
      "id": "379736cd-fa9d-4d0b-a950-b0ecdcb814e2",
      "content": "Cc. 23-27 khuyến khích việc chăn nuôi như nguồn lợi bảo đảm cho gia đình.",
      "position": 30,
      "order": 6,
      "createdAt": "2024-09-10T04:54:10.269Z",
      "updatedAt": "2024-09-10T04:54:10.269Z",
      "verseId": "0b54a8bf-0a8b-40a8-bb6e-57a0a6977bb2",
      "chapterId": "e601d39d-18ba-473b-b536-857627d818ad"
    },
    {
      "id": "3e7bccd7-edcd-4b00-b68a-8c4b4edd1ff0",
      "content": "ds : <i>triều thiên</i>.",
      "position": 12,
      "order": 7,
      "createdAt": "2024-09-10T04:54:10.285Z",
      "updatedAt": "2024-09-10T04:54:10.285Z",
      "verseId": "c6daa7ba-e2e1-4486-ae53-e5a404ff1279",
      "chapterId": "e601d39d-18ba-473b-b536-857627d818ad"
    }
  ],
