    },
    {
      "id": "c3e00362-dc62-45ee-a22d-5fcf0f6fe4bb",
      "content": "Lặp lại 22,3.",
      "position": 36,
      "order": 1,
      "createdAt": "2024-09-10T04:54:10.098Z",
      "updatedAt": "2024-09-10T04:54:10.098Z",
