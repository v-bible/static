      "chapterId": "f500052f-fda2-480a-be0e-5586cdaede8a"
    },
    {
      "id": "25f4430a-2277-4f22-a249-d758c6ae3b49",
      "content": "Lặp lại 22,3.",
      "position": 36,
      "order": 1,
      "createdAt": "2024-09-17T10:06:26.425Z",
      "updatedAt": "2024-09-17T10:06:26.425Z",
