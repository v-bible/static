      "chapterId": "268135fc-b4c6-41cc-b1dd-6ea1822ce6bc"
    },
    {
      "id": "3a9e03c5-4083-4011-8237-3d1e1996569d",
      "content": "Cn 10,28",
      "position": 23,
      "order": 3,
      "createdAt": "2024-09-23T04:26:21.666Z",
      "updatedAt": "2024-09-23T04:26:21.666Z",
      "verseId": "16109b8c-d80a-42ed-a001-40bdb5b04b49",
      "chapterId": "268135fc-b4c6-41cc-b1dd-6ea1822ce6bc"
    },
