    },
    {
      "id": "72ee3368-dbdb-42e7-9c0d-51c0eaded41f",
      "content": "Cn 11,1",
      "position": 0,
      "order": 5,
      "createdAt": "2024-09-17T10:08:05.112Z",
      "updatedAt": "2024-09-17T10:08:05.112Z",
      "verseId": "30984bbb-c701-4cff-a63c-4286b4052713",
      "chapterId": "8b88c838-850f-4899-a52f-cf6b36f1096e"
    },
