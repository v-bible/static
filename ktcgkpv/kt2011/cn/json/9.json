    },
    {
      "id": "e91f6934-a6ef-4270-b76c-aab6acbce901",
      "content": "<i>Nhà</i> của Mụ Khờ Dại chẳng giống nhà của Đức Khôn Ngoan chút nào.",
      "position": 19,
      "order": 5,
      "createdAt": "2024-09-10T04:54:20.544Z",
      "updatedAt": "2024-09-10T04:54:20.544Z",
      "verseId": "5fc23176-84f0-40dc-bee4-c05b0cab5209",
      "chapterId": "311bff7d-76b2-4988-8e7d-8ac7ca7129c8"
    },
    {
      "id": "b7f883d6-ab65-406a-81d8-b5e11bb8be61",
      "content": "Mụ hành động như người phụ nữ trắc nết ở 7,8-13.",
      "position": 27,
      "order": 6,
      "createdAt": "2024-09-10T04:54:20.561Z",
      "updatedAt": "2024-09-10T04:54:20.561Z",
      "verseId": "ca81f14a-ba45-4407-b9ad-0a763a74e914",
      "chapterId": "311bff7d-76b2-4988-8e7d-8ac7ca7129c8"
    },
    {
      "id": "2d56c915-3129-4ad4-9aaa-a770a3b2c337",
      "content": "<i>Lén lút, vụng trộm</i> lại là cái bả hấp dẫn những người nhẹ dạ.",
      "position": 46,
      "order": 7,
      "createdAt": "2024-09-10T04:54:20.580Z",
      "updatedAt": "2024-09-10T04:54:20.580Z",
      "verseId": "bff5f839-7857-464c-8103-a58042cabd8b",
      "chapterId": "311bff7d-76b2-4988-8e7d-8ac7ca7129c8"
    }
  ],
