    },
    {
      "id": "9c588af3-7340-4aed-ac6b-bef7b81d8515",
      "content": "Cn 1,7",
      "position": 30,
      "order": 1,
      "createdAt": "2024-09-17T10:05:26.929Z",
      "updatedAt": "2024-09-17T10:05:26.929Z",
      "verseId": "e4f4634f-3c63-4de5-9349-42ae9727449f",
      "chapterId": "4fea08af-cd5c-46f8-91ee-0b7c4f5917ef"
    },
    {
      "id": "5dc24986-5f1e-463d-8843-94b680f81e2e",
      "content": "Cn 6,19",
      "position": 0,
      "order": 2,
      "createdAt": "2024-09-17T10:05:26.945Z",
      "updatedAt": "2024-09-17T10:05:26.945Z",
      "verseId": "f7fa0dc6-ddf1-4f3e-a3c3-8bedf623e097",
      "chapterId": "4fea08af-cd5c-46f8-91ee-0b7c4f5917ef"
    },
    {
      "id": "68a25260-d87d-4ff8-81e7-f7fd02438960",
      "content": "Cn 16,25",
