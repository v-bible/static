      "order": 0,
      "createdAt": "2024-09-10T05:00:13.440Z",
      "updatedAt": "2024-09-10T05:00:13.440Z",
      "verseId": "6a517356-b430-41b5-aa37-25ae5275c345",
      "chapterId": "8671dd2d-b838-44eb-92ff-b1d4868d4b56"
    },
    {
      "id": "b617fbcd-53a9-414a-b700-7f1d02b1d475",
      "content": "Cn 1,7",
      "position": 30,
      "order": 1,
      "createdAt": "2024-09-10T05:00:13.448Z",
      "updatedAt": "2024-09-10T05:00:13.448Z",
      "verseId": "a2699218-6260-4b0f-8b4a-7123c650deac",
      "chapterId": "8671dd2d-b838-44eb-92ff-b1d4868d4b56"
    },
    {
      "id": "99f67424-a07b-4300-ab16-3188e1a52712",
      "content": "Cn 6,19",
      "position": 0,
      "order": 2,
      "createdAt": "2024-09-10T05:00:13.464Z",
      "updatedAt": "2024-09-10T05:00:13.464Z",
      "verseId": "e55bd0e9-36d6-404a-8e12-fb67d1010f21",
      "chapterId": "8671dd2d-b838-44eb-92ff-b1d4868d4b56"
    },
    {
      "id": "e070e5a0-a69c-4d88-856d-68c9ca780cf8",
      "content": "Cn 16,25",
      "position": 0,
      "order": 3,
      "createdAt": "2024-09-10T05:00:13.502Z",
      "updatedAt": "2024-09-10T05:00:13.502Z",
      "verseId": "beaf50a7-aaa7-48e1-9766-3656efec6142",
      "chapterId": "8671dd2d-b838-44eb-92ff-b1d4868d4b56"
    },
    {
