      "chapterId": "4e01b132-2550-475e-a812-241a127a63c3"
    },
    {
      "id": "140af784-418a-4999-9fe7-edc0d18e6784",
      "content": "2. ÔNG GIU-ĐA",
      "order": 0,
      "createdAt": "2024-09-17T09:38:31.858Z",
      "updatedAt": "2024-09-17T09:38:31.858Z",
      "verseId": "41116c58-2590-4a0e-bb87-ad49d5e6f64f",
      "chapterId": "4e01b132-2550-475e-a812-241a127a63c3"
    },
    {
      "id": "8b10e0cc-1adc-4103-86d4-da44a8fd707f",
      "content": "Dòng họ Giu-đa",
      "order": 0,
      "createdAt": "2024-09-17T09:38:31.868Z",
      "updatedAt": "2024-09-17T09:38:31.868Z",
      "verseId": "5542874e-2d56-4118-b8e9-09cb4b75e9fa",
      "chapterId": "4e01b132-2550-475e-a812-241a127a63c3"
    },
    {
      "id": "17042697-d794-47c8-b670-540e8047afea",
      "content": "Nguồn gốc vua Đa-vít",
      "order": 0,
      "createdAt": "2024-09-17T09:38:31.896Z",
      "updatedAt": "2024-09-17T09:38:31.896Z",
      "verseId": "67a49af7-1099-4c15-b277-16d69b871c9c",
      "chapterId": "4e01b132-2550-475e-a812-241a127a63c3"
    },
    {
      "id": "c36ee1bb-e800-4a10-a2a9-842fd5f6d622",
      "content": "Ngành ông Ca-lếp",
      "order": 0,
      "createdAt": "2024-09-17T09:38:31.922Z",
      "updatedAt": "2024-09-17T09:38:31.922Z",
      "verseId": "c18e88b9-9738-4f05-b175-8a0bb8af0440",
      "chapterId": "4e01b132-2550-475e-a812-241a127a63c3"
    },
    {
      "id": "a214edb7-1990-427f-9cf8-f7f091189d24",
      "content": "Ngành ông Giơ-rác-mơ-ên",
      "order": 0,
      "createdAt": "2024-09-17T09:38:31.967Z",
      "updatedAt": "2024-09-17T09:38:31.967Z",
      "verseId": "02e942cd-0419-4cc7-bc32-8c4387adce7e",
      "chapterId": "4e01b132-2550-475e-a812-241a127a63c3"
    },
    {
      "id": "f244668d-93bf-4e79-a3c2-af2de39e0a35",
      "content": "Ngành ông Ca-lếp",
      "order": 0,
      "createdAt": "2024-09-17T09:38:32.050Z",
      "updatedAt": "2024-09-17T09:38:32.050Z",
      "verseId": "f24cab98-3a1c-48ff-8fc2-2267e93b839b",
      "chapterId": "4e01b132-2550-475e-a812-241a127a63c3"
    },
    {
      "id": "263eec1c-6883-4dcc-b6d1-ea5767944209",
      "content": "Các con của ông Ít-ra-en",
      "order": 1,
