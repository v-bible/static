{
  "chapter": {
    "id": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6",
    "number": 15,
    "ref": "",
    "createdAt": "2024-09-17T08:41:12.491Z",
    "updatedAt": "2024-09-17T08:41:12.491Z",
