  },
  "verses": [
    {
      "id": "86168497-fa4d-4704-adef-4937d0772388",
      "number": 1,
      "content": "Bấy giờ ông Mô-sê cùng với con cái Ít-ra-en hát mừng ĐỨC CHÚA bài ca sau đây. Họ ca rằng :",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:10:53.037Z",
      "updatedAt": "2024-09-17T09:10:53.037Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "9f2d2816-d91b-4bcb-bb08-205b30dae27c",
      "number": 1,
      "content": "“Tôi xin hát mừng CHÚA,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.050Z",
      "updatedAt": "2024-09-17T09:10:53.050Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "9e589581-0a59-448c-a0c9-731841c71b0e",
      "number": 1,
      "content": "Đấng cao cả uy hùng :",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.052Z",
      "updatedAt": "2024-09-17T09:10:53.052Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "d57bfdb5-22c1-4c6e-94f7-b0fcd795e166",
      "number": 1,
      "content": "Kỵ binh cùng chiến mã,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.054Z",
      "updatedAt": "2024-09-17T09:10:53.054Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "20adb9fb-029a-46bb-adf7-25c9a31da978",
      "number": 1,
      "content": "Người xô xuống đại dương.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.056Z",
      "updatedAt": "2024-09-17T09:10:53.056Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "6f2e40e8-7a2d-4d04-92c9-489f2887b5f4",
      "number": 2,
      "content": "CHÚA là sức mạnh tôi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.058Z",
      "updatedAt": "2024-09-17T09:10:53.058Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "dbe46044-661b-42d1-8b3a-59f35cca9581",
      "number": 2,
      "content": "là Đấng tôi ca ngợi,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.062Z",
      "updatedAt": "2024-09-17T09:10:53.062Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "9cf284b7-15a8-4fc3-a7a2-beaf3a25537f",
      "number": 2,
      "content": "chính Người cứu độ tôi.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.064Z",
      "updatedAt": "2024-09-17T09:10:53.064Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "103b2b21-b42f-4670-a195-58c09e01fcac",
      "number": 2,
      "content": "Người là Chúa tôi thờ,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.066Z",
      "updatedAt": "2024-09-17T09:10:53.066Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "fbf2e5b9-ee93-4fa8-887d-360778e466d2",
      "number": 2,
      "content": "xin dâng lời vinh chúc,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.068Z",
      "updatedAt": "2024-09-17T09:10:53.068Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "347d89d1-5674-44ac-91a1-270dead103be",
      "number": 2,
      "content": "Người là Chúa tổ tiên,",
      "order": 5,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.070Z",
      "updatedAt": "2024-09-17T09:10:53.070Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "6e118448-b80d-4bb5-99ef-5e0f4994e115",
      "number": 2,
      "content": "xin mừng câu tán tụng.",
      "order": 6,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.072Z",
      "updatedAt": "2024-09-17T09:10:53.072Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "e107daf2-e9a7-4940-8974-ab486a502662",
      "number": 3,
      "content": "Người là trang chiến binh,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.075Z",
      "updatedAt": "2024-09-17T09:10:53.075Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "f1c7e5d5-4c01-498a-aada-cde09f12d343",
      "number": 3,
      "content": "danh Người là ‘ĐỨC CHÚA !’",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.077Z",
      "updatedAt": "2024-09-17T09:10:53.077Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "1871d609-c22b-4624-ac38-f0fb09fa0e60",
      "number": 4,
      "content": "Xa mã Pha-ra-ô,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.081Z",
      "updatedAt": "2024-09-17T09:10:53.081Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "34a0494a-3d48-42f7-b4ce-041d5a7869c5",
      "number": 4,
      "content": "Người xô xuống lòng biển,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.083Z",
      "updatedAt": "2024-09-17T09:10:53.083Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "03b54d51-98cb-45b2-a771-a37ee4b2f5ff",
      "number": 4,
      "content": "tướng dũng với binh hùng",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.086Z",
      "updatedAt": "2024-09-17T09:10:53.086Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "38b86791-b4c0-4810-96be-632b2de0111c",
      "number": 4,
      "content": "chết chìm trong Biển Sậy.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.088Z",
      "updatedAt": "2024-09-17T09:10:53.088Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "0736fa25-d4c7-46ca-9ef2-127a122ad1b1",
      "number": 5,
      "content": "Vực thẳm vùi lấp chúng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.090Z",
      "updatedAt": "2024-09-17T09:10:53.090Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "9bdba85f-96f1-4449-b974-153322ceb1e5",
      "number": 5,
      "content": "chúng chìm xuống nước sâu",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.091Z",
      "updatedAt": "2024-09-17T09:10:53.091Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "0c037cf1-6b25-4db9-a256-ffd37da8ab0d",
      "number": 5,
      "content": "chẳng khác nào hòn đá.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.095Z",
      "updatedAt": "2024-09-17T09:10:53.095Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "b5fc017d-7c63-4eca-92a5-0ca9e66177fc",
      "number": 6,
      "content": "Lạy CHÚA, tay hữu Ngài",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.097Z",
      "updatedAt": "2024-09-17T09:10:53.097Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "16fdbcb7-6d30-4e53-96d7-f14f19d47fc8",
      "number": 6,
      "content": "đã biểu dương sức mạnh.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.099Z",
      "updatedAt": "2024-09-17T09:10:53.099Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "6ace6261-2a11-4e71-8a82-795f4630ca3e",
      "number": 6,
      "content": "Tay hữu Ngài, lạy CHÚA,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.101Z",
      "updatedAt": "2024-09-17T09:10:53.101Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "f93aaff5-a168-40a7-b28f-4d7333f423e1",
      "number": 6,
      "content": "đã nghiền nát địch quân.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.103Z",
      "updatedAt": "2024-09-17T09:10:53.103Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "9ccf497a-b5d0-4196-b983-f46a1906a662",
      "number": 7,
      "content": "Lấy dũng lực oai hùng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.104Z",
      "updatedAt": "2024-09-17T09:10:53.104Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "3a2ffee4-bb54-4b17-9904-2ea7fe6ba3ff",
      "number": 7,
      "content": "Chúa quật ngã đối phương ;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.106Z",
      "updatedAt": "2024-09-17T09:10:53.106Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "03c01a20-4ee2-4c17-8cc4-83fdcd5e1bfd",
      "number": 7,
      "content": "Ngài nổi cơn thịnh nộ,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.108Z",
      "updatedAt": "2024-09-17T09:10:53.108Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "7e20478d-385c-4637-9007-3aed3f806e63",
      "number": 7,
      "content": "thiêu chúng cháy như rơm.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.110Z",
      "updatedAt": "2024-09-17T09:10:53.110Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "ee943141-5ad4-4071-ad74-25421077df35",
      "number": 8,
      "content": "Nộ khí Ngài, lạy Chúa,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.114Z",
      "updatedAt": "2024-09-17T09:10:53.114Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "6029fe05-36eb-4773-af71-e0126f7582fe",
      "number": 8,
      "content": "đã khiến nước dâng lên,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.116Z",
      "updatedAt": "2024-09-17T09:10:53.116Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "c935358d-fa5e-4145-a61e-9ffeab24cc38",
      "number": 8,
      "content": "sóng trùng dương dồn lại",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.118Z",
      "updatedAt": "2024-09-17T09:10:53.118Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "bc20859e-d21b-444c-bce9-648cc2be4350",
      "number": 8,
      "content": "dựng đứng như tường thành ;",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.120Z",
      "updatedAt": "2024-09-17T09:10:53.120Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "adba04d7-8888-4c9a-8d0a-33c0f5c1bb6c",
      "number": 8,
      "content": "giữa lòng biển thẳm sâu,",
      "order": 4,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.122Z",
      "updatedAt": "2024-09-17T09:10:53.122Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "cab37e5f-b80b-4aef-8e6f-ef5dabb91c46",
      "number": 8,
      "content": "nước bỗng đâu ngừng chảy.",
      "order": 5,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.124Z",
      "updatedAt": "2024-09-17T09:10:53.124Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "cc152864-e47a-4425-9c08-b1bdf92753e0",
      "number": 9,
      "content": "Địch quân tự nhủ rằng :",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.127Z",
      "updatedAt": "2024-09-17T09:10:53.127Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "432f0c25-bf8f-406e-abbc-7863f9004035",
      "number": 9,
      "content": "‘Ta đuổi theo bắt lấy,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.129Z",
      "updatedAt": "2024-09-17T09:10:53.129Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "88934648-b16c-47ac-a41e-55d0c006e535",
      "number": 9,
      "content": "chiến lợi phẩm đem chia,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.132Z",
      "updatedAt": "2024-09-17T09:10:53.132Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "daace21d-1bb5-4865-9d04-94fcd36ae21e",
      "number": 9,
      "content": "mới no lòng thoả dạ ;",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.135Z",
      "updatedAt": "2024-09-17T09:10:53.135Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "8743305d-3bdd-4c5c-a286-4e6c05d974f7",
      "number": 9,
      "content": "ta tuốt lưỡi gươm trần,",
      "order": 4,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.138Z",
      "updatedAt": "2024-09-17T09:10:53.138Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "ab3a1bd4-816c-4cf5-abd9-30ff939d9fa4",
      "number": 9,
      "content": "cứ thẳng tay tiêu diệt.’",
      "order": 5,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.141Z",
      "updatedAt": "2024-09-17T09:10:53.141Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
      "id": "cbcfe91e-4bab-4811-89a9-6de5f1886061",
      "number": 10,
      "content": "Ngài hà hơi nổi gió,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:10:53.144Z",
      "updatedAt": "2024-09-17T09:10:53.144Z",
      "chapterId": "8bcd0ea0-c8ef-425e-bdd7-705205ae01e6"
    },
    {
