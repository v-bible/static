{
  "chapter": {
    "id": "69489e6e-b9ae-40e0-bb88-eb5d76764be1",
    "number": 15,
    "ref": "",
    "createdAt": "2024-09-10T00:39:05.720Z",
    "updatedAt": "2024-09-10T00:39:05.720Z",
    "bookId": "dd6fba3f-4218-4d80-9222-1d95652f68d6"
  },
  "verses": [
    {
      "id": "3d6e0810-19cd-4fd1-8e71-c8436932d5c4",
      "number": 1,
      "content": "Bấy giờ ông Mô-sê cùng với con cái Ít-ra-en hát mừng ĐỨC CHÚA bài ca sau đây. Họ ca rằng :",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T02:15:20.380Z",
      "updatedAt": "2024-09-10T02:15:20.380Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "bd79e33a-4793-4614-bf23-f7e05ba7c278",
      "number": 1,
      "content": "“Tôi xin hát mừng CHÚA,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.394Z",
      "updatedAt": "2024-09-10T02:15:20.394Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "90248ccc-bba5-4465-a32d-6c36d4f0a032",
      "number": 1,
      "content": "Đấng cao cả uy hùng :",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.396Z",
      "updatedAt": "2024-09-10T02:15:20.396Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "c25d597c-c5f5-40ed-876a-5e91a9563c42",
      "number": 1,
      "content": "Kỵ binh cùng chiến mã,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.398Z",
      "updatedAt": "2024-09-10T02:15:20.398Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "755c3012-6ccb-4c4b-b424-ac24a5022d13",
      "number": 1,
      "content": "Người xô xuống đại dương.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.400Z",
      "updatedAt": "2024-09-10T02:15:20.400Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "962d9a64-f9cf-4b14-b23a-47ca17274ae2",
      "number": 2,
      "content": "CHÚA là sức mạnh tôi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.402Z",
      "updatedAt": "2024-09-10T02:15:20.402Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "0e166384-0fd6-4003-860a-6cf62a8b859b",
      "number": 2,
      "content": "là Đấng tôi ca ngợi,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.405Z",
      "updatedAt": "2024-09-10T02:15:20.405Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "1032be1c-f559-4cb1-8c63-2b98e1a1030f",
      "number": 2,
      "content": "chính Người cứu độ tôi.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.407Z",
      "updatedAt": "2024-09-10T02:15:20.407Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "3c46aca2-bb20-4789-8aca-7b161155cce8",
      "number": 2,
      "content": "Người là Chúa tôi thờ,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.409Z",
      "updatedAt": "2024-09-10T02:15:20.409Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "3a1c04cb-7681-476b-bc5e-f014393224e3",
      "number": 2,
      "content": "xin dâng lời vinh chúc,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.411Z",
      "updatedAt": "2024-09-10T02:15:20.411Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "275a1707-478b-4637-b256-758e1c00af19",
      "number": 2,
      "content": "Người là Chúa tổ tiên,",
      "order": 5,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.413Z",
      "updatedAt": "2024-09-10T02:15:20.413Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "b22f58da-bd35-44fb-8dd8-f0cddcf150ea",
      "number": 2,
      "content": "xin mừng câu tán tụng.",
      "order": 6,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.415Z",
      "updatedAt": "2024-09-10T02:15:20.415Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "faca0072-6ed7-47ee-96b2-5be1466220e5",
      "number": 3,
      "content": "Người là trang chiến binh,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.417Z",
      "updatedAt": "2024-09-10T02:15:20.417Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "94b0acab-9228-4c47-a19e-2cbb04c518b7",
      "number": 3,
      "content": "danh Người là ‘ĐỨC CHÚA !’",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.419Z",
      "updatedAt": "2024-09-10T02:15:20.419Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "4597f5e3-a713-4e14-a0cf-dce9bca10110",
      "number": 4,
      "content": "Xa mã Pha-ra-ô,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.423Z",
      "updatedAt": "2024-09-10T02:15:20.423Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "280c70fb-0cd9-41f6-b584-62a4a60370d6",
      "number": 4,
      "content": "Người xô xuống lòng biển,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.425Z",
      "updatedAt": "2024-09-10T02:15:20.425Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "cd326496-056d-4a7b-abd9-0a16f551ea67",
      "number": 4,
      "content": "tướng dũng với binh hùng",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.427Z",
      "updatedAt": "2024-09-10T02:15:20.427Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "d870aa13-b549-48a3-af2e-0b4d75b2fd78",
      "number": 4,
      "content": "chết chìm trong Biển Sậy.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.429Z",
      "updatedAt": "2024-09-10T02:15:20.429Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "2b65b7b3-dd1e-4485-8f56-a91220e6a040",
      "number": 5,
      "content": "Vực thẳm vùi lấp chúng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.431Z",
      "updatedAt": "2024-09-10T02:15:20.431Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "6fdfa493-15db-42b1-b209-fdcb3bb3c372",
      "number": 5,
      "content": "chúng chìm xuống nước sâu",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.433Z",
      "updatedAt": "2024-09-10T02:15:20.433Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "d55f002d-53ed-4429-8f43-b016b6c0384c",
      "number": 5,
      "content": "chẳng khác nào hòn đá.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.437Z",
      "updatedAt": "2024-09-10T02:15:20.437Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "a3b33c48-23df-48b4-af48-f66c52502549",
      "number": 6,
      "content": "Lạy CHÚA, tay hữu Ngài",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.439Z",
      "updatedAt": "2024-09-10T02:15:20.439Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "aa8cf03d-7390-49a2-8582-8361ee1e5682",
      "number": 6,
      "content": "đã biểu dương sức mạnh.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.443Z",
      "updatedAt": "2024-09-10T02:15:20.443Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "29b2d365-bed4-469f-b2bd-1ee5d1add192",
      "number": 6,
      "content": "Tay hữu Ngài, lạy CHÚA,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.447Z",
      "updatedAt": "2024-09-10T02:15:20.447Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "f03f43fe-c817-477b-87eb-adfd3e047b77",
      "number": 6,
      "content": "đã nghiền nát địch quân.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.452Z",
      "updatedAt": "2024-09-10T02:15:20.452Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "af3f37dd-d503-48be-b05e-4a7496c3c1ed",
      "number": 7,
      "content": "Lấy dũng lực oai hùng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.457Z",
      "updatedAt": "2024-09-10T02:15:20.457Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "3a55f599-1292-424d-82d9-3afc94e783e5",
      "number": 7,
      "content": "Chúa quật ngã đối phương ;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.462Z",
      "updatedAt": "2024-09-10T02:15:20.462Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "b7ab6c0b-c8a2-4cb7-b147-993e4929f6f8",
      "number": 7,
      "content": "Ngài nổi cơn thịnh nộ,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.466Z",
      "updatedAt": "2024-09-10T02:15:20.466Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "b4c10d05-17b9-4deb-8701-b8a78dea6d21",
      "number": 7,
      "content": "thiêu chúng cháy như rơm.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.471Z",
      "updatedAt": "2024-09-10T02:15:20.471Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "6e8b1588-3781-49f7-8673-2ec590623b73",
      "number": 8,
      "content": "Nộ khí Ngài, lạy Chúa,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.481Z",
      "updatedAt": "2024-09-10T02:15:20.481Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "3ac49cae-4b30-4244-82ba-cc05617ec85d",
      "number": 8,
      "content": "đã khiến nước dâng lên,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.486Z",
      "updatedAt": "2024-09-10T02:15:20.486Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "36eb1a23-bca4-4059-8d59-cd0b7c7b2b64",
      "number": 8,
      "content": "sóng trùng dương dồn lại",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.490Z",
      "updatedAt": "2024-09-10T02:15:20.490Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "9cbc3ff4-d2e6-4b4d-8c0d-7f1fecdc42b0",
      "number": 8,
      "content": "dựng đứng như tường thành ;",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.495Z",
      "updatedAt": "2024-09-10T02:15:20.495Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "641cd092-d6b6-4395-84f6-cfd4391862a3",
      "number": 8,
      "content": "giữa lòng biển thẳm sâu,",
      "order": 4,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.500Z",
      "updatedAt": "2024-09-10T02:15:20.500Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "cb021a06-5d46-47c9-97af-2e23e095ffe6",
      "number": 8,
      "content": "nước bỗng đâu ngừng chảy.",
      "order": 5,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.505Z",
      "updatedAt": "2024-09-10T02:15:20.505Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "50669648-5661-48b4-b4a9-9c3dc68e334d",
      "number": 9,
      "content": "Địch quân tự nhủ rằng :",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.509Z",
      "updatedAt": "2024-09-10T02:15:20.509Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "2c82ecf3-fc81-442a-9886-6f079a8c11fe",
      "number": 9,
      "content": "‘Ta đuổi theo bắt lấy,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.514Z",
      "updatedAt": "2024-09-10T02:15:20.514Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "8affb7b1-52e5-4faa-8d2a-a2c18742f2e6",
      "number": 9,
      "content": "chiến lợi phẩm đem chia,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.519Z",
      "updatedAt": "2024-09-10T02:15:20.519Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "62c51831-1112-4bd9-9837-eb0e51e735fc",
      "number": 9,
      "content": "mới no lòng thoả dạ ;",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.524Z",
      "updatedAt": "2024-09-10T02:15:20.524Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "05c818b6-6707-4498-acd6-905381269133",
      "number": 9,
      "content": "ta tuốt lưỡi gươm trần,",
      "order": 4,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.529Z",
      "updatedAt": "2024-09-10T02:15:20.529Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "43a00759-cd31-4ed6-9f3d-f1a1e69c005b",
      "number": 9,
      "content": "cứ thẳng tay tiêu diệt.’",
      "order": 5,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.534Z",
      "updatedAt": "2024-09-10T02:15:20.534Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
      "id": "56f1fe0d-ce01-4e19-b359-389f7ad8a3ad",
      "number": 10,
      "content": "Ngài hà hơi nổi gió,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T02:15:20.540Z",
      "updatedAt": "2024-09-10T02:15:20.540Z",
      "chapterId": "69489e6e-b9ae-40e0-bb88-eb5d76764be1"
    },
    {
