    },
    {
      "id": "acc55aeb-6e17-400c-a4e7-d4cb2f59b3c6",
      "number": 1,
      "content": "A-lép",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.417Z",
      "updatedAt": "2024-09-17T10:00:40.417Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "2ced05b0-8455-4b88-8c02-14d3f235b155",
      "number": 1,
      "content": "Tôi xin hết lòng cảm tạ CHÚA,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.422Z",
      "updatedAt": "2024-09-17T10:00:40.422Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "45fdd90c-888d-4392-bdb4-97a01e88fa78",
      "number": 1,
      "content": "Bết",
      "order": 3,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.424Z",
      "updatedAt": "2024-09-17T10:00:40.424Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "bd0bae40-d8d0-42da-b229-9ed2a1bdf15f",
      "number": 1,
      "content": "trong cộng đoàn chính nhân, giữa lòng đại hội.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.426Z",
      "updatedAt": "2024-09-17T10:00:40.426Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "b5f26286-454a-477f-a52a-43642aba6738",
      "number": 2,
      "content": "Ghi-men",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.429Z",
      "updatedAt": "2024-09-17T10:00:40.429Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "0de3893f-1fec-4f75-8e73-1ceef39f2416",
      "number": 2,
      "content": "Việc CHÚA làm quả thật lớn lao,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.434Z",
      "updatedAt": "2024-09-17T10:00:40.434Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "d21511c1-abd2-4503-ad74-9601cf845c7e",
      "number": 2,
      "content": "Đa-lét",
      "order": 2,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.439Z",
      "updatedAt": "2024-09-17T10:00:40.439Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "2d161a20-00cd-4b18-9212-1c6f55a34a5c",
      "number": 2,
      "content": "người mộ mến ra công tìm hiểu.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.443Z",
      "updatedAt": "2024-09-17T10:00:40.443Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "d44520b9-84ec-4555-9999-e11a210c0c68",
      "number": 3,
      "content": "Hê",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.447Z",
      "updatedAt": "2024-09-17T10:00:40.447Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "2d11c405-df1d-4ca4-98f1-f77de0250f51",
      "number": 3,
      "content": "Sự nghiệp Chúa oai phong hiển hách,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.452Z",
      "updatedAt": "2024-09-17T10:00:40.452Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "98f96322-746f-451c-8eb1-0e68ead62043",
      "number": 3,
      "content": "Vau",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.457Z",
      "updatedAt": "2024-09-17T10:00:40.457Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "81644897-5ec3-4b26-95b5-329254ecdcb5",
      "number": 3,
      "content": "đức công chính của Người tồn tại thiên thu.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.462Z",
      "updatedAt": "2024-09-17T10:00:40.462Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "183fa3d9-e9f1-459a-9f2e-a6e409f6880f",
      "number": 4,
      "content": "Da-in",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.472Z",
      "updatedAt": "2024-09-17T10:00:40.472Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "ff5c36fb-d60b-4f93-9cf0-e904266f8f99",
      "number": 4,
      "content": "Chúa đã truyền tưởng niệm những kỳ công của Người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.476Z",
      "updatedAt": "2024-09-17T10:00:40.476Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "5899cc22-3941-4122-bee0-23fc5e31cf80",
      "number": 4,
      "content": "Khết",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.481Z",
      "updatedAt": "2024-09-17T10:00:40.481Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "bd1752cc-b85c-49e7-b398-daa79ec3f75f",
      "number": 4,
      "content": "CHÚA là Đấng từ bi nhân hậu.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.485Z",
      "updatedAt": "2024-09-17T10:00:40.485Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "a350caff-8077-4950-a8fd-e2c1520011ed",
      "number": 5,
      "content": "Tết",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.490Z",
      "updatedAt": "2024-09-17T10:00:40.490Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "b586860e-90b8-4b16-a642-9bc8a9c0e78e",
      "number": 5,
      "content": "Ai kính sợ Người, Người ban phát của ăn ;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.495Z",
      "updatedAt": "2024-09-17T10:00:40.495Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "8e1a6f1d-bc5f-4617-be45-77e2b6dbdd7b",
      "number": 5,
      "content": "Giốt",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.504Z",
      "updatedAt": "2024-09-17T10:00:40.504Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "2a41838f-c473-4c3f-8fed-9469d70a7971",
      "number": 5,
      "content": "giao ước đã lập ra, muôn đời Người nhớ mãi.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.509Z",
      "updatedAt": "2024-09-17T10:00:40.509Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "eb1098c6-5241-4f64-8d2d-b099feda949c",
      "number": 6,
      "content": "Cáp",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.514Z",
      "updatedAt": "2024-09-17T10:00:40.514Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "def44791-c55f-4df0-84b0-dab3b297d6ef",
      "number": 6,
      "content": "Sức mạnh việc Người làm, Người cho dân thấy rõ,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.519Z",
      "updatedAt": "2024-09-17T10:00:40.519Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "032ee333-78fd-4bed-ac3c-b3e6d34d0492",
      "number": 6,
      "content": "La-mét",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.523Z",
      "updatedAt": "2024-09-17T10:00:40.523Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "b5c1a5e9-141b-4288-baff-517f48818d0e",
      "number": 6,
      "content": "khi tặng ban cho họ gia sản của chư dân.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.527Z",
      "updatedAt": "2024-09-17T10:00:40.527Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "9710cc53-77ff-47d7-b66c-9a15aa592e39",
      "number": 7,
      "content": "Mêm",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.532Z",
      "updatedAt": "2024-09-17T10:00:40.532Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "b84f77b2-0179-436d-b964-15b262d65097",
      "number": 7,
      "content": "Những công trình tay Chúa thực hiện",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.537Z",
      "updatedAt": "2024-09-17T10:00:40.537Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "91426dfa-1bdf-40cb-b604-77c84649f1b8",
      "number": 7,
      "content": "quả là chân thật và công minh.Nun",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.542Z",
      "updatedAt": "2024-09-17T10:00:40.542Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "c5bb8d7d-27da-45e6-aaef-a62524c7d4d8",
      "number": 7,
      "content": "Huấn lệnh Người ban đều chắc chắn,",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.546Z",
      "updatedAt": "2024-09-17T10:00:40.546Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "11c86a35-23af-4dd4-bcda-c161ef069547",
      "number": 8,
      "content": "Xa-méc",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.551Z",
      "updatedAt": "2024-09-17T10:00:40.551Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "33034c47-281c-449c-84c7-7da6582a7a2f",
      "number": 8,
      "content": "bền vững đến muôn đời muôn thuở,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.556Z",
      "updatedAt": "2024-09-17T10:00:40.556Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "fa993f9a-b406-46a2-a3e6-b47bebf6875f",
      "number": 8,
      "content": "A-in",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.561Z",
      "updatedAt": "2024-09-17T10:00:40.561Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "3296a3e1-b68b-4b48-8565-7995657fac18",
      "number": 8,
      "content": "căn cứ vào sự thật lẽ ngay.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.566Z",
      "updatedAt": "2024-09-17T10:00:40.566Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "3e98c34d-76a2-4576-bd6c-d7f8336bef2e",
      "number": 9,
      "content": "Pê",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.570Z",
      "updatedAt": "2024-09-17T10:00:40.570Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "f048aa3c-821f-49ca-86e9-d5de3bb3cda1",
      "number": 9,
      "content": "Người đem lại cho dân ơn giải thoát,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.575Z",
      "updatedAt": "2024-09-17T10:00:40.575Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "9b376e0f-e748-4383-b44d-b7d80f69974a",
      "number": 9,
      "content": "Xa-đê",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.580Z",
      "updatedAt": "2024-09-17T10:00:40.580Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "1d00c2b6-0e9d-41fe-ba07-05fd105e3464",
      "number": 9,
      "content": "thiết lập giao ước đến muôn đời.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.584Z",
      "updatedAt": "2024-09-17T10:00:40.584Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "f8e62102-44d2-4b81-8658-51ac1b7cf75e",
      "number": 9,
      "content": "Cốp",
      "order": 4,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.589Z",
      "updatedAt": "2024-09-17T10:00:40.589Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "d028286b-6f64-4144-a9a6-4c548c5a487d",
      "number": 9,
      "content": "Tôn danh Người thánh thiêng khả uý.",
      "order": 5,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.593Z",
      "updatedAt": "2024-09-17T10:00:40.593Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "ee05f14b-a564-42d2-9355-5bf4cbd33af9",
      "number": 10,
      "content": "Rết",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.598Z",
      "updatedAt": "2024-09-17T10:00:40.598Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "4235f317-2f97-4593-97a6-7fb822355de1",
      "number": 10,
      "content": "Kính sợ CHÚA là đầu mối khôn ngoan.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.602Z",
      "updatedAt": "2024-09-17T10:00:40.602Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "fab30237-0a1a-4421-902d-8a69332f7e85",
      "number": 10,
      "content": "Sin",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:40.616Z",
      "updatedAt": "2024-09-17T10:00:40.616Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
      "id": "cd533436-1820-4bad-af28-92296c03cc41",
      "number": 10,
      "content": "Sáng suốt thay kẻ thực hành như vậy.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.620Z",
      "updatedAt": "2024-09-17T10:00:40.620Z",
      "chapterId": "abeecdc7-35ab-4449-be3f-5327c87c6dce"
    },
    {
