      "number": 1,
      "content": "Ha-lê-lui-a.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:40.407Z",
      "updatedAt": "2024-09-17T10:00:40.407Z",
