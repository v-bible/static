    {
      "id": "af51c32d-93cd-4417-805c-4b731f0cec06",
      "number": 1,
      "content": "Tôi xin hết lòng cảm tạ CHÚA,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.486Z",
      "updatedAt": "2024-09-10T04:38:02.486Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "17f4e4c8-0f00-4464-9a62-382b8f1a8d9c",
      "number": 1,
      "content": "Bết",
      "order": 3,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.488Z",
      "updatedAt": "2024-09-10T04:38:02.488Z",
