    },
    {
      "id": "fa9e4fc8-8f56-4ba0-9f7b-d08223c942f6",
      "number": 1,
      "content": "trong cộng đoàn chính nhân, giữa lòng đại hội.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.490Z",
      "updatedAt": "2024-09-10T04:38:02.490Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "28ba86e4-5043-42e0-9b25-3656aac806f3",
      "number": 2,
      "content": "Ghi-men",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.492Z",
      "updatedAt": "2024-09-10T04:38:02.492Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "24164072-9565-4157-bc6e-416333a47106",
      "number": 2,
      "content": "Việc CHÚA làm quả thật lớn lao,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.493Z",
      "updatedAt": "2024-09-10T04:38:02.493Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "e7b1f867-4cb6-4290-b130-af16587bcc20",
      "number": 2,
      "content": "Đa-lét",
      "order": 2,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.495Z",
      "updatedAt": "2024-09-10T04:38:02.495Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "35e8cddc-3c09-4c70-a215-3644e2e24cc7",
      "number": 2,
      "content": "người mộ mến ra công tìm hiểu.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.497Z",
      "updatedAt": "2024-09-10T04:38:02.497Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "5ea82652-e67c-493f-9851-5d10820a82ab",
      "number": 3,
      "content": "Hê",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.499Z",
      "updatedAt": "2024-09-10T04:38:02.499Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "fb952da7-0c6e-4ad4-aa12-e99c51e770e1",
      "number": 3,
      "content": "Sự nghiệp Chúa oai phong hiển hách,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.501Z",
      "updatedAt": "2024-09-10T04:38:02.501Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "0bdf42dd-0ee7-453e-8404-ecc0d02936eb",
      "number": 3,
      "content": "Vau",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.503Z",
      "updatedAt": "2024-09-10T04:38:02.503Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "7dfd79e3-da27-4ec5-92c1-f614ab5e7252",
      "number": 3,
      "content": "đức công chính của Người tồn tại thiên thu.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.505Z",
      "updatedAt": "2024-09-10T04:38:02.505Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "c4c87670-444e-4902-8fe8-57e072c0b9f9",
      "number": 4,
      "content": "Da-in",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.509Z",
      "updatedAt": "2024-09-10T04:38:02.509Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "eb124259-94a5-45d0-85cc-d75f6edd7733",
      "number": 4,
      "content": "Chúa đã truyền tưởng niệm những kỳ công của Người.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.511Z",
      "updatedAt": "2024-09-10T04:38:02.511Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "d1f3b635-09b2-4518-8009-3879850c7901",
      "number": 4,
      "content": "Khết",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.513Z",
      "updatedAt": "2024-09-10T04:38:02.513Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "bc72663d-5cf6-48e5-a975-a6da648f583e",
      "number": 4,
      "content": "CHÚA là Đấng từ bi nhân hậu.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.515Z",
      "updatedAt": "2024-09-10T04:38:02.515Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "7d26a619-e2c1-4697-932e-49600b7af586",
      "number": 5,
      "content": "Tết",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.517Z",
      "updatedAt": "2024-09-10T04:38:02.517Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "6e9db6f2-f4fd-4100-89f2-0efbebf32b06",
      "number": 5,
      "content": "Ai kính sợ Người, Người ban phát của ăn ;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.519Z",
      "updatedAt": "2024-09-10T04:38:02.519Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "7c5c6355-7c1f-4b98-a9fd-c931cbca7056",
      "number": 5,
      "content": "Giốt",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.523Z",
      "updatedAt": "2024-09-10T04:38:02.523Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "586dba8c-2ad0-402f-862b-21f562857bbd",
      "number": 5,
      "content": "giao ước đã lập ra, muôn đời Người nhớ mãi.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.525Z",
      "updatedAt": "2024-09-10T04:38:02.525Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "74886004-d196-4583-9f6f-c2bd2d611eb2",
      "number": 6,
      "content": "Cáp",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.534Z",
      "updatedAt": "2024-09-10T04:38:02.534Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "333b4355-0414-4538-841b-0975b03834ce",
      "number": 6,
      "content": "Sức mạnh việc Người làm, Người cho dân thấy rõ,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.544Z",
      "updatedAt": "2024-09-10T04:38:02.544Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "385fe59e-ae2c-4c5b-a15d-d82f2aa7f547",
      "number": 6,
      "content": "La-mét",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.553Z",
      "updatedAt": "2024-09-10T04:38:02.553Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "6a29ae3d-d427-443f-a0f8-de686523cb4e",
      "number": 6,
      "content": "khi tặng ban cho họ gia sản của chư dân.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.562Z",
      "updatedAt": "2024-09-10T04:38:02.562Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "9511b4e7-4fb7-441d-837f-8b1de8d58ba2",
      "number": 7,
      "content": "Mêm",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.572Z",
      "updatedAt": "2024-09-10T04:38:02.572Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "c192a893-93b9-4aff-b8b9-6b97bc546944",
      "number": 7,
      "content": "Những công trình tay Chúa thực hiện",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.581Z",
      "updatedAt": "2024-09-10T04:38:02.581Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "9fde8e10-7460-45fc-b06a-d2e1c50e7fd7",
      "number": 7,
      "content": "quả là chân thật và công minh.Nun",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.590Z",
      "updatedAt": "2024-09-10T04:38:02.590Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "c1ced95c-3ecd-40d2-8cca-0bcd173c647e",
      "number": 7,
      "content": "Huấn lệnh Người ban đều chắc chắn,",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.600Z",
      "updatedAt": "2024-09-10T04:38:02.600Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "653c3a9a-5961-4410-bfa2-661abf3446f0",
      "number": 8,
      "content": "Xa-méc",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.609Z",
      "updatedAt": "2024-09-10T04:38:02.609Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "95a6af6f-b0b8-41e6-80be-3919115b61df",
      "number": 8,
      "content": "bền vững đến muôn đời muôn thuở,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.618Z",
      "updatedAt": "2024-09-10T04:38:02.618Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "cff07e95-0d99-4793-ae88-9789cee32967",
      "number": 8,
      "content": "A-in",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.627Z",
      "updatedAt": "2024-09-10T04:38:02.627Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "2547d0f8-c0ca-4df3-8ba9-f2d5c24c016f",
      "number": 8,
      "content": "căn cứ vào sự thật lẽ ngay.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.637Z",
      "updatedAt": "2024-09-10T04:38:02.637Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "3bb24f6a-16a4-4e1f-a1cc-add710e3a0c6",
      "number": 9,
      "content": "Pê",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.646Z",
      "updatedAt": "2024-09-10T04:38:02.646Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "dbf48f3d-0e1c-4c28-acdc-a27c77af7ba3",
      "number": 9,
      "content": "Người đem lại cho dân ơn giải thoát,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.656Z",
      "updatedAt": "2024-09-10T04:38:02.656Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "15eb2167-05ae-4fbd-ab28-cb66fcc5a8c6",
      "number": 9,
      "content": "Xa-đê",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.673Z",
      "updatedAt": "2024-09-10T04:38:02.673Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "a8e06a4b-4e4f-47e0-a36e-60133557d250",
      "number": 9,
      "content": "thiết lập giao ước đến muôn đời.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.683Z",
      "updatedAt": "2024-09-10T04:38:02.683Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "14ac98e8-0874-4bbf-b779-798133fc6fa0",
      "number": 9,
      "content": "Cốp",
      "order": 4,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.701Z",
      "updatedAt": "2024-09-10T04:38:02.701Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "beb03061-bbd1-477f-8ce7-2b59417afcdf",
      "number": 9,
      "content": "Tôn danh Người thánh thiêng khả uý.",
      "order": 5,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.710Z",
      "updatedAt": "2024-09-10T04:38:02.710Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "21e3f94f-a43a-4cf2-a2ca-63fc22785bb0",
      "number": 10,
      "content": "Rết",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.719Z",
      "updatedAt": "2024-09-10T04:38:02.719Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "9c7c1fbb-a125-4277-b313-76ea7ed773ea",
      "number": 10,
      "content": "Kính sợ CHÚA là đầu mối khôn ngoan.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.730Z",
      "updatedAt": "2024-09-10T04:38:02.730Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "0d9d6f5e-9b06-4ae0-842e-1c2bf969249e",
      "number": 10,
      "content": "Sin",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:38:02.742Z",
      "updatedAt": "2024-09-10T04:38:02.742Z",
      "chapterId": "8ff3df2c-8d61-4f30-a2f7-98230be793e1"
    },
    {
      "id": "5d1c70c5-3bb1-4631-be70-838b0f255db8",
      "number": 10,
      "content": "Sáng suốt thay kẻ thực hành như vậy.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:38:02.748Z",
