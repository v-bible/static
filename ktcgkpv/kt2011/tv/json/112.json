    },
    {
      "id": "35043025-c9a8-4f5b-8a0d-71705cb36e23",
      "number": 1,
      "content": "A-lép",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.583Z",
      "updatedAt": "2024-09-23T04:18:00.583Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "135b6904-9623-4188-970c-e71cde929dbe",
      "number": 1,
      "content": "Hạnh phúc thay, người kính sợ CHÚA,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.588Z",
      "updatedAt": "2024-09-23T04:18:00.588Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "3d6a3e18-ac99-4fec-ab8a-d4bdb69526c0",
      "number": 1,
      "content": "Bết",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.590Z",
      "updatedAt": "2024-09-23T04:18:00.590Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "944502ea-61dc-48bb-8bec-3e656d614dfd",
      "number": 1,
      "content": "những ưa cùng thích mệnh lệnh Chúa truyền ban.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.593Z",
      "updatedAt": "2024-09-23T04:18:00.593Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "733feca7-a973-4a09-814f-160e1c11bcdf",
      "number": 2,
      "content": "Ghi-men",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.595Z",
      "updatedAt": "2024-09-23T04:18:00.595Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "3e8df928-0d6e-4d6a-a8eb-0f3e8f42cbcb",
      "number": 2,
      "content": "Trên mặt đất, con cháu của họ sẽ hùng cường,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.597Z",
      "updatedAt": "2024-09-23T04:18:00.597Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "2f75930c-add7-41ad-ae3f-05e174111d42",
      "number": 2,
      "content": "Đa-lét",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.599Z",
      "updatedAt": "2024-09-23T04:18:00.599Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "34241b4c-b396-4598-8307-48b5ff60f5ba",
      "number": 2,
      "content": "dòng dõi kẻ ngay lành được Chúa thương giáng phúc.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.601Z",
      "updatedAt": "2024-09-23T04:18:00.601Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "912cc651-88bc-47fd-a071-63936a35c1ff",
      "number": 3,
      "content": "Hê",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.603Z",
      "updatedAt": "2024-09-23T04:18:00.603Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "c205c1a8-4d85-4907-ab79-5cc71db2982b",
      "number": 3,
      "content": "Gia đình họ phú quý giàu sang,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.605Z",
      "updatedAt": "2024-09-23T04:18:00.605Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "78fabe93-c72b-4b73-b6a1-232d42545a81",
      "number": 3,
      "content": "Vau",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.606Z",
      "updatedAt": "2024-09-23T04:18:00.606Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "c8ee03dc-767d-4e87-a34c-0397c89ba8fd",
      "number": 3,
      "content": "đức công chính của họ tồn tại muôn đời.",
      "order": 3,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.608Z",
      "updatedAt": "2024-09-23T04:18:00.608Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "e076d82b-9364-44c3-8b19-e50d3d6f72ce",
      "number": 4,
      "content": "Da-in",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.612Z",
      "updatedAt": "2024-09-23T04:18:00.612Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "95d6c688-230f-4814-b646-7b3f02972558",
      "number": 4,
      "content": "Giữa tối tăm bừng lên một ánh sáng",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.614Z",
      "updatedAt": "2024-09-23T04:18:00.614Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "4e2dcd0f-f3a5-4df3-a051-4a5fd48c517f",
      "number": 4,
      "content": "chiếu rọi kẻ ngay lành :",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.616Z",
      "updatedAt": "2024-09-23T04:18:00.616Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "aa4b171c-3d48-426d-a57f-d382cb338baa",
      "number": 4,
      "content": "Khết",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.618Z",
      "updatedAt": "2024-09-23T04:18:00.618Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "0772815f-a8e0-45f5-ba61-c2f09954e9ee",
      "number": 4,
      "content": "đó là người từ bi nhân hậu và công chính.",
      "order": 4,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.620Z",
      "updatedAt": "2024-09-23T04:18:00.620Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "8c99c6f6-4521-4930-9b8c-4bc450c84c56",
      "number": 5,
      "content": "Tết",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.627Z",
      "updatedAt": "2024-09-23T04:18:00.627Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "970b0968-a055-4422-ba72-e61d32b79315",
      "number": 5,
      "content": "Phúc thay người biết cảm thương và cho vay mượn,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.633Z",
      "updatedAt": "2024-09-23T04:18:00.633Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "29233cfa-dc6b-48ca-a9d4-e98e7cae451d",
      "number": 5,
      "content": "Giốt",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.642Z",
      "updatedAt": "2024-09-23T04:18:00.642Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "b1089931-5069-40a7-9873-9a921a467177",
      "number": 5,
      "content": "biết theo lẽ phải mà thu xếp công việc mình.",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.646Z",
      "updatedAt": "2024-09-23T04:18:00.646Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "c6e95d8f-4f45-4f22-ae10-57e13d0b212e",
      "number": 6,
      "content": "Cáp",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.650Z",
      "updatedAt": "2024-09-23T04:18:00.650Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "2877713b-ba8c-45b4-9892-a72a934ff13e",
      "number": 6,
      "content": "Họ sẽ không bao giờ lay chuyển,",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.654Z",
      "updatedAt": "2024-09-23T04:18:00.654Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "71d0d985-c88e-4cfb-9e3b-8c46eb560434",
      "number": 6,
      "content": "La-mét",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.659Z",
      "updatedAt": "2024-09-23T04:18:00.659Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "684ea5e4-d995-45af-9fb2-61c13165cc3a",
      "number": 6,
      "content": "thiên hạ muôn đời còn tưởng nhớ chính nhân.",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.663Z",
      "updatedAt": "2024-09-23T04:18:00.663Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "1eb089c9-859e-41a0-91c1-d9b16dff981a",
      "number": 7,
      "content": "Mêm",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.668Z",
      "updatedAt": "2024-09-23T04:18:00.668Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "3339ae0b-0943-44a4-b89c-fadf26e3618f",
      "number": 7,
      "content": "Họ không lo phải nghe tin dữ,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.672Z",
      "updatedAt": "2024-09-23T04:18:00.672Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "8cee9fce-717c-45b2-8caf-d50954a92747",
      "number": 7,
      "content": "[Nun]()",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.676Z",
      "updatedAt": "2024-09-23T04:18:00.676Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "b6849b72-a80b-4206-bfe3-152c064f9937",
      "number": 7,
      "content": "hằng an tâm và tin cậy CHÚA,",
      "order": 3,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.681Z",
      "updatedAt": "2024-09-23T04:18:00.681Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "fb817d28-cdf5-4d89-a61f-adae4c3655cb",
      "number": 8,
      "content": "Xa-méc",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.685Z",
      "updatedAt": "2024-09-23T04:18:00.685Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "4accbe00-0b04-4e55-bb39-60aa54be0b89",
      "number": 8,
      "content": "luôn vững lòng không sợ hãi chi",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.690Z",
      "updatedAt": "2024-09-23T04:18:00.690Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "7f105341-cb67-4923-9623-30971020a674",
      "number": 8,
      "content": "A-in",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.694Z",
      "updatedAt": "2024-09-23T04:18:00.694Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "6fdea6fb-16cc-4bec-afa6-d04b9efaeab5",
      "number": 8,
      "content": "và rốt cuộc coi khinh lũ địch thù.",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.699Z",
      "updatedAt": "2024-09-23T04:18:00.699Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "e35d8b70-ba95-4c88-810d-7004ca474d17",
      "number": 9,
      "content": "Pê",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.703Z",
      "updatedAt": "2024-09-23T04:18:00.703Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "17425cbd-7ca7-4572-975f-3e2f2165c84c",
      "number": 9,
      "content": "Kẻ túng nghèo, họ rộng tay làm phúc,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.708Z",
      "updatedAt": "2024-09-23T04:18:00.708Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "b32a7734-9ee1-4ce9-a720-4d5ae55242d7",
      "number": 9,
      "content": "Xa-đê",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.712Z",
      "updatedAt": "2024-09-23T04:18:00.712Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
