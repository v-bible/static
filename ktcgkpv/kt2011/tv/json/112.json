  "verses": [
    {
      "id": "b14ac939-4df6-455c-85c2-938170614f88",
      "number": 1,
      "content": "Ha-lê-lui-a.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.733Z",
      "updatedAt": "2024-09-17T09:56:26.733Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "c6dc28ea-a62c-4c90-a000-1516d87018c0",
      "number": 1,
      "content": "A-lép",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.741Z",
      "updatedAt": "2024-09-17T09:56:26.741Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "0db48ca8-7681-455b-a5c2-d19e0b187b27",
      "number": 1,
      "content": "Hạnh phúc thay, người kính sợ CHÚA,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.743Z",
      "updatedAt": "2024-09-17T09:56:26.743Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "521aa7c1-67d3-4932-a294-713b65a8f283",
      "number": 1,
      "content": "Bết",
      "order": 3,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.746Z",
      "updatedAt": "2024-09-17T09:56:26.746Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "8a5625f2-b8b0-443c-8147-d4a0632dfa95",
      "number": 1,
      "content": "những ưa cùng thích mệnh lệnh Chúa truyền ban.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.748Z",
      "updatedAt": "2024-09-17T09:56:26.748Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "0bf5eff4-a8a2-4d1e-9f56-a87769812168",
      "number": 2,
      "content": "Ghi-men",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.750Z",
      "updatedAt": "2024-09-17T09:56:26.750Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "325307a9-487e-4892-8b6c-7867fce477aa",
      "number": 2,
      "content": "Trên mặt đất, con cháu của họ sẽ hùng cường,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.752Z",
      "updatedAt": "2024-09-17T09:56:26.752Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "c7f3c246-bff5-415c-8fa3-447f417365be",
      "number": 2,
      "content": "Đa-lét",
      "order": 2,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.754Z",
      "updatedAt": "2024-09-17T09:56:26.754Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "75c820a1-267a-429d-871a-bc9f9a011024",
      "number": 2,
      "content": "dòng dõi kẻ ngay lành được Chúa thương giáng phúc.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.756Z",
      "updatedAt": "2024-09-17T09:56:26.756Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "e51748e0-310a-4bf4-bc8f-1507d25701e6",
      "number": 3,
      "content": "Hê",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.758Z",
      "updatedAt": "2024-09-17T09:56:26.758Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "3b69ff8f-403d-4f64-9fe4-a8e48c34a621",
      "number": 3,
      "content": "Gia đình họ phú quý giàu sang,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.760Z",
      "updatedAt": "2024-09-17T09:56:26.760Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "48ae549c-ae0a-4afe-beba-87d8de5bf28b",
      "number": 3,
      "content": "Vau",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.763Z",
      "updatedAt": "2024-09-17T09:56:26.763Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "124b4ba9-965c-40c6-90ca-d1d810248d21",
      "number": 3,
      "content": "đức công chính của họ tồn tại muôn đời.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.765Z",
      "updatedAt": "2024-09-17T09:56:26.765Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "53359a1a-8fac-4892-980d-9c14134e90e9",
      "number": 4,
      "content": "Da-in",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.769Z",
      "updatedAt": "2024-09-17T09:56:26.769Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "28136b54-b70e-450f-a0f5-01ba5d8565e0",
      "number": 4,
      "content": "Giữa tối tăm bừng lên một ánh sáng",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.771Z",
      "updatedAt": "2024-09-17T09:56:26.771Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "001cd5ac-1c87-4f4d-a23b-97c9e087673e",
      "number": 4,
      "content": "chiếu rọi kẻ ngay lành :",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.773Z",
      "updatedAt": "2024-09-17T09:56:26.773Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "19abf6fa-906f-4bab-bd64-c06a61b978f7",
      "number": 4,
      "content": "Khết",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.775Z",
      "updatedAt": "2024-09-17T09:56:26.775Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "424d478a-061d-43c1-8188-ac8ede6b8fbb",
      "number": 4,
      "content": "đó là người từ bi nhân hậu và công chính.",
      "order": 4,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.780Z",
      "updatedAt": "2024-09-17T09:56:26.780Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "060902f7-55fa-4c92-96d3-3608fb43cdd1",
      "number": 5,
      "content": "Tết",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.790Z",
      "updatedAt": "2024-09-17T09:56:26.790Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "b84f468c-d7c9-4b51-87bf-f077a191d441",
      "number": 5,
      "content": "Phúc thay người biết cảm thương và cho vay mượn,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.794Z",
      "updatedAt": "2024-09-17T09:56:26.794Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "f7a67558-285d-48b4-88ca-3d3db42e4cb7",
      "number": 5,
      "content": "Giốt",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.800Z",
      "updatedAt": "2024-09-17T09:56:26.800Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "160f4dca-fd8d-461f-a570-f4fb3ec49a4c",
      "number": 5,
      "content": "biết theo lẽ phải mà thu xếp công việc mình.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.803Z",
      "updatedAt": "2024-09-17T09:56:26.803Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "f2e2483c-34a2-4484-81e6-382a036ae26d",
      "number": 6,
      "content": "Cáp",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.806Z",
      "updatedAt": "2024-09-17T09:56:26.806Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "75a973d8-6371-4ed2-b4e3-d9b77d243ae2",
      "number": 6,
      "content": "Họ sẽ không bao giờ lay chuyển,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.808Z",
      "updatedAt": "2024-09-17T09:56:26.808Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "d20b2feb-8c95-46cf-8cc4-1db21dbcfd37",
      "number": 6,
      "content": "La-mét",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.811Z",
      "updatedAt": "2024-09-17T09:56:26.811Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "2552d51d-e7f5-48c5-9236-8aa70cf71d2d",
      "number": 6,
      "content": "thiên hạ muôn đời còn tưởng nhớ chính nhân.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.815Z",
      "updatedAt": "2024-09-17T09:56:26.815Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "6f2101fc-42ff-4ecf-a0dc-c97b846b9c5d",
      "number": 7,
      "content": "Mêm",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.817Z",
      "updatedAt": "2024-09-17T09:56:26.817Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "d37cd0c7-ada0-4f88-92e0-b925c4b5ea7b",
      "number": 7,
      "content": "Họ không lo phải nghe tin dữ,Nun",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.819Z",
      "updatedAt": "2024-09-17T09:56:26.819Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "8b04864a-e512-42be-8865-371e56b2e790",
      "number": 7,
      "content": "hằng an tâm và tin cậy CHÚA,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.821Z",
      "updatedAt": "2024-09-17T09:56:26.821Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "e5d74abe-51f0-48ca-b5dd-df8b784b26b1",
      "number": 8,
      "content": "Xa-méc",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.823Z",
      "updatedAt": "2024-09-17T09:56:26.823Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "df9ed52e-4621-4ddc-818a-7d1ac3a0ec8d",
      "number": 8,
      "content": "luôn vững lòng không sợ hãi chi",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.825Z",
      "updatedAt": "2024-09-17T09:56:26.825Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "919b43f4-7742-49fd-b667-43a3d594e3c2",
      "number": 8,
      "content": "A-in",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.828Z",
      "updatedAt": "2024-09-17T09:56:26.828Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "029057a4-b071-49b6-bc1f-55c82f49402c",
      "number": 8,
      "content": "và rốt cuộc coi khinh lũ địch thù.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.831Z",
      "updatedAt": "2024-09-17T09:56:26.831Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "dda07f9a-2130-4be9-a955-4ff10c374f92",
      "number": 9,
      "content": "Pê",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.833Z",
      "updatedAt": "2024-09-17T09:56:26.833Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "d882d618-eddd-4b68-9f34-9879fcd21b48",
      "number": 9,
      "content": "Kẻ túng nghèo, họ rộng tay làm phúc,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.835Z",
      "updatedAt": "2024-09-17T09:56:26.835Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "0afadd72-81df-41ca-9acc-7ba249bab0bf",
      "number": 9,
      "content": "Xa-đê",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.837Z",
      "updatedAt": "2024-09-17T09:56:26.837Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "955cb9ac-c9bf-40ee-8c21-53d563719af7",
      "number": 9,
      "content": "đức công chính của họ tồn tại muôn đời,",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.839Z",
      "updatedAt": "2024-09-17T09:56:26.839Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "eb514e55-a29e-4aa4-9e1f-8cd452bacb22",
      "number": 9,
      "content": "Cốp",
      "order": 4,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.841Z",
      "updatedAt": "2024-09-17T09:56:26.841Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "7a2bff7f-351f-4001-b8ac-816df58c6290",
      "number": 9,
      "content": "uy thế họ vươn cao rực rỡ.",
      "order": 5,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.844Z",
      "updatedAt": "2024-09-17T09:56:26.844Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "2a6bb39a-2617-4420-9091-faa716dc4e7f",
      "number": 10,
      "content": "Rết",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.848Z",
      "updatedAt": "2024-09-17T09:56:26.848Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "0740f137-29de-416a-9990-71c9f1e08cdf",
      "number": 10,
      "content": "Thấy cảnh này ác nhân tức giận,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.850Z",
      "updatedAt": "2024-09-17T09:56:26.850Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "c42da7b0-e4fa-4384-8312-0bac5d121e72",
      "number": 10,
      "content": "Sin",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T09:56:26.852Z",
      "updatedAt": "2024-09-17T09:56:26.852Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "8c4778b7-9890-4fe4-a8cc-503bd6c6f8d8",
      "number": 10,
      "content": "nghiến răng nghiến lợi và mòn mỏi héo hon,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:26.854Z",
      "updatedAt": "2024-09-17T09:56:26.854Z",
      "chapterId": "544bb368-0e65-49e3-9b49-89422e16dc71"
    },
    {
      "id": "937e9965-7067-49a8-9327-3a3df468ed70",
