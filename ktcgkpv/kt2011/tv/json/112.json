  "verses": [
    {
      "id": "61eed457-d0ae-4040-bcf5-781fe2042ff0",
      "number": 1,
      "content": "Ha-lê-lui-a.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.025Z",
      "updatedAt": "2024-09-10T04:33:57.025Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "3c83de8a-fe41-4c1e-b3be-d9cde03459f3",
      "number": 1,
      "content": "A-lép",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.039Z",
      "updatedAt": "2024-09-10T04:33:57.039Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "49c658e9-25dd-4a1e-9ba5-90f7a1e049fb",
      "number": 1,
      "content": "Hạnh phúc thay, người kính sợ CHÚA,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.041Z",
      "updatedAt": "2024-09-10T04:33:57.041Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "34ddcf4e-6149-459c-93e9-d090bfbb0442",
      "number": 1,
      "content": "Bết",
      "order": 3,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.044Z",
      "updatedAt": "2024-09-10T04:33:57.044Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "111227fd-24d4-42f5-a00d-27c939f3a836",
      "number": 1,
      "content": "những ưa cùng thích mệnh lệnh Chúa truyền ban.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.046Z",
      "updatedAt": "2024-09-10T04:33:57.046Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "93a51cc9-5568-4f80-beba-da4c43027684",
      "number": 2,
      "content": "Ghi-men",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.048Z",
      "updatedAt": "2024-09-10T04:33:57.048Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "5017a3ef-bec7-4719-882c-6ff560b0ccdc",
      "number": 2,
      "content": "Trên mặt đất, con cháu của họ sẽ hùng cường,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.050Z",
      "updatedAt": "2024-09-10T04:33:57.050Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "2835fc84-eae5-481f-8142-57807f4a6c81",
      "number": 2,
      "content": "Đa-lét",
      "order": 2,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.052Z",
      "updatedAt": "2024-09-10T04:33:57.052Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "ba502611-9bdc-40e5-83c7-c93ff561c50b",
      "number": 2,
      "content": "dòng dõi kẻ ngay lành được Chúa thương giáng phúc.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.054Z",
      "updatedAt": "2024-09-10T04:33:57.054Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "71ce2a8f-2129-4426-9a0a-403f21252c9b",
      "number": 3,
      "content": "Hê",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.057Z",
      "updatedAt": "2024-09-10T04:33:57.057Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "95b93822-d899-44ab-9b05-705ff385b823",
      "number": 3,
      "content": "Gia đình họ phú quý giàu sang,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.059Z",
      "updatedAt": "2024-09-10T04:33:57.059Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "f3263473-e583-46bf-bc1b-1a129e67665b",
      "number": 3,
      "content": "Vau",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.061Z",
      "updatedAt": "2024-09-10T04:33:57.061Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "b9cf1e3c-7bd6-4461-b241-2ac89ea756f3",
      "number": 3,
      "content": "đức công chính của họ tồn tại muôn đời.",
      "order": 3,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.063Z",
      "updatedAt": "2024-09-10T04:33:57.063Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "a74362ab-74ce-464a-8d1c-9219f0d5c8d8",
      "number": 4,
      "content": "Da-in",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.067Z",
      "updatedAt": "2024-09-10T04:33:57.067Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "b44dd6eb-5832-49e8-bf85-fbed4f7c6614",
      "number": 4,
      "content": "Giữa tối tăm bừng lên một ánh sáng",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.069Z",
      "updatedAt": "2024-09-10T04:33:57.069Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "6c59185f-e536-45ec-8fa7-a93e3adfd7db",
      "number": 4,
      "content": "chiếu rọi kẻ ngay lành :",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.071Z",
      "updatedAt": "2024-09-10T04:33:57.071Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "88485c75-519a-4ab7-a53a-0c55595e0823",
      "number": 4,
      "content": "Khết",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.074Z",
      "updatedAt": "2024-09-10T04:33:57.074Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "bce64d70-07ef-4af1-b53d-cac2e32c0ab8",
      "number": 4,
      "content": "đó là người từ bi nhân hậu và công chính.",
      "order": 4,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.076Z",
      "updatedAt": "2024-09-10T04:33:57.076Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "fa90edff-0149-4764-913b-7a9a5364ee88",
      "number": 5,
      "content": "Tết",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.080Z",
      "updatedAt": "2024-09-10T04:33:57.080Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "7e78c0ff-09f7-4ce9-9ef9-3835136706f0",
      "number": 5,
      "content": "Phúc thay người biết cảm thương và cho vay mượn,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.082Z",
      "updatedAt": "2024-09-10T04:33:57.082Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "ab79f0c9-a63f-4bbf-8fd8-0d37d11f3974",
      "number": 5,
      "content": "Giốt",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.087Z",
      "updatedAt": "2024-09-10T04:33:57.087Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "cb2fdb87-07b4-4e03-bc6b-78d02584247d",
      "number": 5,
      "content": "biết theo lẽ phải mà thu xếp công việc mình.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.089Z",
      "updatedAt": "2024-09-10T04:33:57.089Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "c849e3f6-7e31-4081-aec0-77bc55c88df0",
      "number": 6,
      "content": "Cáp",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.091Z",
      "updatedAt": "2024-09-10T04:33:57.091Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "b715c0c7-a3a4-450d-98b8-13d9baecea8d",
      "number": 6,
      "content": "Họ sẽ không bao giờ lay chuyển,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.093Z",
      "updatedAt": "2024-09-10T04:33:57.093Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "c65a104b-c4d1-4e98-abbf-73a2b011389f",
      "number": 6,
      "content": "La-mét",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.096Z",
      "updatedAt": "2024-09-10T04:33:57.096Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "3d4498a2-43e2-41e5-a53c-b1065124e742",
      "number": 6,
      "content": "thiên hạ muôn đời còn tưởng nhớ chính nhân.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.098Z",
      "updatedAt": "2024-09-10T04:33:57.098Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "f740783b-5ec5-418a-a414-77d420f32287",
      "number": 7,
      "content": "Mêm",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.100Z",
      "updatedAt": "2024-09-10T04:33:57.100Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "7c04bff9-d583-46a1-a475-2c86ccb66c7c",
      "number": 7,
      "content": "Họ không lo phải nghe tin dữ,Nun",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.102Z",
      "updatedAt": "2024-09-10T04:33:57.102Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "944bb740-827b-4d5a-9356-2cf587ba0756",
      "number": 7,
      "content": "hằng an tâm và tin cậy CHÚA,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.104Z",
      "updatedAt": "2024-09-10T04:33:57.104Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "67111645-7b18-4b83-8511-508c5177b3ca",
      "number": 8,
      "content": "Xa-méc",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.106Z",
      "updatedAt": "2024-09-10T04:33:57.106Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "b9b57c54-cda6-4ce8-8480-a8311522607c",
      "number": 8,
      "content": "luôn vững lòng không sợ hãi chi",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.108Z",
      "updatedAt": "2024-09-10T04:33:57.108Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "e21b0416-fc45-4799-8f16-819555d8c901",
      "number": 8,
      "content": "A-in",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.111Z",
      "updatedAt": "2024-09-10T04:33:57.111Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "edffe3ea-7964-418b-a773-fdc14b4e43a4",
      "number": 8,
      "content": "và rốt cuộc coi khinh lũ địch thù.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.113Z",
      "updatedAt": "2024-09-10T04:33:57.113Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "76820d40-7e75-448e-857d-86413300bf47",
      "number": 9,
      "content": "Pê",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.115Z",
      "updatedAt": "2024-09-10T04:33:57.115Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "333fd8af-d991-4ddb-bd5e-c446f4172c36",
      "number": 9,
      "content": "Kẻ túng nghèo, họ rộng tay làm phúc,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.117Z",
      "updatedAt": "2024-09-10T04:33:57.117Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "410a62de-18dc-4362-9f04-d8ffdcfb652e",
      "number": 9,
      "content": "Xa-đê",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.119Z",
      "updatedAt": "2024-09-10T04:33:57.119Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "d3b47615-0004-426b-9210-1084130a4aea",
      "number": 9,
      "content": "đức công chính của họ tồn tại muôn đời,",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.121Z",
      "updatedAt": "2024-09-10T04:33:57.121Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "02c1c989-8526-4317-aa0d-ee916a1adb89",
      "number": 9,
      "content": "Cốp",
      "order": 4,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.124Z",
      "updatedAt": "2024-09-10T04:33:57.124Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "8e480895-e937-4a7a-976a-862214642f36",
      "number": 9,
      "content": "uy thế họ vươn cao rực rỡ.",
      "order": 5,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.126Z",
      "updatedAt": "2024-09-10T04:33:57.126Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "69ed7dd3-97f5-4269-8a10-030f890bddda",
      "number": 10,
      "content": "Rết",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.130Z",
      "updatedAt": "2024-09-10T04:33:57.130Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "27c72e8a-0264-491e-bf9e-a5aa31922f01",
      "number": 10,
      "content": "Thấy cảnh này ác nhân tức giận,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:33:57.132Z",
      "updatedAt": "2024-09-10T04:33:57.132Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "6c494537-85cd-448a-bad0-23835066f11b",
      "number": 10,
      "content": "Sin",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:33:57.134Z",
      "updatedAt": "2024-09-10T04:33:57.134Z",
      "chapterId": "1f91222c-1709-4f87-a192-d2dda928c70b"
    },
    {
      "id": "c77e73c7-35b9-4b11-a775-fbf396bb7a3f",
      "number": 10,
      "content": "nghiến răng nghiến lợi và mòn mỏi héo hon,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
