      "id": "58d8ff1c-b367-479d-9f43-02db9a05e40a",
      "number": 9,
      "content": "đức công chính của họ tồn tại muôn đời,",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.716Z",
      "updatedAt": "2024-09-23T04:18:00.716Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "d818ca95-477e-4857-920b-ad1b345bdc67",
      "number": 9,
      "content": "Cốp",
      "order": 4,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.721Z",
      "updatedAt": "2024-09-23T04:18:00.721Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "c3d2a29e-2588-40a2-b87d-baddc198bbd2",
      "number": 9,
      "content": "uy thế họ vươn cao rực rỡ.",
      "order": 5,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:00.725Z",
      "updatedAt": "2024-09-23T04:18:00.725Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "841c7da8-584c-4d03-98be-c7a1f18e91eb",
      "number": 10,
      "content": "Rết",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:00.734Z",
      "updatedAt": "2024-09-23T04:18:00.734Z",
      "chapterId": "bd8ae07e-36a0-42aa-bbf4-e89c1a05f04e"
    },
    {
      "id": "0f1126b1-00b5-477c-a7fd-717617b59f07",
