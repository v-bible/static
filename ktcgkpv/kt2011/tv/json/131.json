  },
  "verses": [
    {
      "id": "17381231-7c36-45b1-acc9-9b077b7ad1cb",
      "number": 1,
      "content": "*Ca khúc lên Đền. Của vua [Đa-vít]().*",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:21:47.032Z",
      "updatedAt": "2024-09-23T04:21:47.032Z",
      "chapterId": "7e72a0f2-2b81-4c50-886e-77808690376b"
    },
    {
      "id": "60a7a931-1f42-4d1b-ba17-d5aba59b8e1f",
      "number": 1,
      "content": "Lòng con chẳng dám tự cao,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:21:47.044Z",
      "updatedAt": "2024-09-23T04:21:47.044Z",
      "chapterId": "7e72a0f2-2b81-4c50-886e-77808690376b"
    },
    {
      "id": "e48bb2d2-456d-4bb5-88ba-c4561b0ce527",
      "number": 1,
      "content": "mắt con chẳng dám tự hào, CHÚA ơi !",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:21:47.047Z",
      "updatedAt": "2024-09-23T04:21:47.047Z",
      "chapterId": "7e72a0f2-2b81-4c50-886e-77808690376b"
    },
    {
      "id": "ec56e05c-6e32-4f08-b820-0292063e904d",
      "number": 1,
      "content": "Đường cao vọng, chẳng đời nào bước,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:21:47.051Z",
      "updatedAt": "2024-09-23T04:21:47.051Z",
      "chapterId": "7e72a0f2-2b81-4c50-886e-77808690376b"
    },
    {
      "id": "98f14be5-27e3-4753-bf05-6e4fc9f7315e",
      "number": 1,
      "content": "việc diệu kỳ vượt sức, chẳng cầu ;",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:21:47.055Z",
      "updatedAt": "2024-09-23T04:21:47.055Z",
      "chapterId": "7e72a0f2-2b81-4c50-886e-77808690376b"
    },
    {
      "id": "66866f6a-9ec9-417c-a261-e4b12bf71f8b",
      "number": 2,
      "content": "hồn con, con vẫn trước sau",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:21:47.058Z",
      "updatedAt": "2024-09-23T04:21:47.058Z",
      "chapterId": "7e72a0f2-2b81-4c50-886e-77808690376b"
    },
    {
      "id": "e45f117c-4de4-4799-adaf-98bce2e0489c",
      "number": 2,
      "content": "giữ cho thinh lặng, giữ sao thanh bình.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:21:47.062Z",
      "updatedAt": "2024-09-23T04:21:47.062Z",
      "chapterId": "7e72a0f2-2b81-4c50-886e-77808690376b"
    },
    {
      "id": "77c20758-afce-4bc1-ace4-479184ec7501",
      "number": 2,
      "content": "Như trẻ thơ nép mình lòng mẹ,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:21:47.065Z",
      "updatedAt": "2024-09-23T04:21:47.065Z",
      "chapterId": "7e72a0f2-2b81-4c50-886e-77808690376b"
    },
    {
      "id": "083e212b-bace-4ea1-b169-97b8d55d92b9",
      "number": 2,
      "content": "trong con, hồn lặng lẽ an vui.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:21:47.071Z",
      "updatedAt": "2024-09-23T04:21:47.071Z",
      "chapterId": "7e72a0f2-2b81-4c50-886e-77808690376b"
    },
    {
      "id": "2349929a-3d3b-4967-ac82-40109e84196c",
      "number": 3,
      "content": "Cậy vào CHÚA, [Ít-ra-en]() ơi,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:21:47.077Z",
      "updatedAt": "2024-09-23T04:21:47.077Z",
      "chapterId": "7e72a0f2-2b81-4c50-886e-77808690376b"
    },
    {
      "id": "243d9345-af63-4eb9-ad0a-f537c9082fb3",
      "number": 3,
      "content": "từ nay đến mãi muôn đời muôn năm.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:21:47.090Z",
      "updatedAt": "2024-09-23T04:21:47.090Z",
      "chapterId": "7e72a0f2-2b81-4c50-886e-77808690376b"
    }
  ],
