  "verses": [
    {
      "id": "d11d5a2a-c116-4e26-a50d-1597021872bb",
      "number": 1,
      "content": "Muôn nước hỡi, nào ca ngợi CHÚA,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:31:37.490Z",
      "updatedAt": "2024-09-10T04:31:37.490Z",
      "chapterId": "41610c98-0b06-44c5-90bc-3ba4d48f8931"
    },
    {
      "id": "98f94fc3-97bd-46ec-845d-4f694d2f5f03",
      "number": 1,
      "content": "ngàn dân ơi, hãy chúc tụng Người !",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:31:37.507Z",
      "updatedAt": "2024-09-10T04:31:37.507Z",
      "chapterId": "41610c98-0b06-44c5-90bc-3ba4d48f8931"
    },
    {
      "id": "f9d01f5b-f74b-49df-aabc-f901d2748910",
      "number": 2,
      "content": "Vì tình Chúa thương ta thật là mãnh liệt,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:31:37.512Z",
      "updatedAt": "2024-09-10T04:31:37.512Z",
      "chapterId": "41610c98-0b06-44c5-90bc-3ba4d48f8931"
    },
    {
      "id": "914073cd-11bc-4bcc-bda8-08cae01e2863",
      "number": 2,
      "content": "lòng thành tín của Người bền vững muôn năm. Ha-lê-lui-a.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:31:37.517Z",
      "updatedAt": "2024-09-10T04:31:37.517Z",
      "chapterId": "41610c98-0b06-44c5-90bc-3ba4d48f8931"
    }
  ],
