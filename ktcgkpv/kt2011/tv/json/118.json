  "verses": [
    {
      "id": "1048d27f-869d-4cea-81c2-fd036e46f6e2",
      "number": 1,
      "content": "Hãy tạ ơn CHÚA vì Chúa nhân từ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.465Z",
      "updatedAt": "2024-09-17T09:56:55.465Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "a84621a8-9bb3-417f-aca7-474eb06dd6b1",
      "number": 1,
      "content": "muôn ngàn đời Chúa vẫn trọn tình thương.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.480Z",
      "updatedAt": "2024-09-17T09:56:55.480Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "c4dc8c88-c676-45bc-b15f-e4c1fad33e0f",
      "number": 2,
      "content": "Ít-ra-en hãy nói lên rằng :",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.482Z",
      "updatedAt": "2024-09-17T09:56:55.482Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "01bcfdc1-bf10-4702-9530-916f9620e5e4",
      "number": 2,
      "content": "muôn ngàn đời Chúa vẫn trọn tình thương.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.486Z",
      "updatedAt": "2024-09-17T09:56:55.486Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "63e4e464-96db-45a5-8457-c4b8f9fe4cc1",
      "number": 3,
      "content": "Nhà A-ha-ron hãy nói lên rằng :",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.488Z",
      "updatedAt": "2024-09-17T09:56:55.488Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "1916947e-dee8-4b4a-bcf1-3d8408b5b395",
      "number": 3,
      "content": "muôn ngàn đời Chúa vẫn trọn tình thương.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.490Z",
      "updatedAt": "2024-09-17T09:56:55.490Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "3f33395b-0507-4506-8c88-f1a2138c68e4",
      "number": 4,
      "content": "Ai kính sợ CHÚA hãy nói lên rằng :",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.492Z",
      "updatedAt": "2024-09-17T09:56:55.492Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "d2429c80-40fc-4ab1-8a15-8df600eee094",
      "number": 4,
      "content": "muôn ngàn đời Chúa vẫn trọn tình thương.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.494Z",
      "updatedAt": "2024-09-17T09:56:55.494Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "d94a26a4-57bb-4155-b31e-5e3115e7abb3",
      "number": 5,
      "content": "Giữa cảnh gian truân, tôi đã kêu cầu CHÚA,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.496Z",
      "updatedAt": "2024-09-17T09:56:55.496Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "7f87eb4a-f200-4b90-804a-e9b9313e85de",
      "number": 5,
      "content": "CHÚA đáp lời và giải thoát tôi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.498Z",
      "updatedAt": "2024-09-17T09:56:55.498Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "0c0d5256-e0bd-4986-ae7d-6a3d2eb33a0e",
      "number": 6,
      "content": "Có CHÚA ở cùng tôi, tôi chẳng sợ gì,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.500Z",
      "updatedAt": "2024-09-17T09:56:55.500Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "d282888e-0406-49d1-a115-08f0ecf2f065",
      "number": 6,
      "content": "hỏi người đời làm chi tôi được ?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.504Z",
      "updatedAt": "2024-09-17T09:56:55.504Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "eb24627b-4545-48cc-964a-5abc2d7ab3f6",
      "number": 7,
      "content": "Có CHÚA ở cùng tôi mà bênh đỡ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.506Z",
      "updatedAt": "2024-09-17T09:56:55.506Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "b8e2c6e2-3e28-4d76-aad4-84d839fbd59d",
      "number": 7,
      "content": "tôi dám nghênh lũ địch thù tôi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.508Z",
      "updatedAt": "2024-09-17T09:56:55.508Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "c9a53e4d-26ae-4ccf-a727-ab69d9651827",
      "number": 8,
      "content": "Ẩn thân bên cạnh CHÚA TRỜI,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.510Z",
      "updatedAt": "2024-09-17T09:56:55.510Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "8fb94fef-c5de-4a0f-b535-3627411b1d0d",
      "number": 8,
      "content": "thì hơn tin cậy ở người trần gian.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.512Z",
      "updatedAt": "2024-09-17T09:56:55.512Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "fc4e177f-be57-4f2e-ab51-471777dfd272",
      "number": 9,
      "content": "Cậy vào thần thế vua quan,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.514Z",
      "updatedAt": "2024-09-17T09:56:55.514Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "4b511472-885b-4104-b4ff-2caf5a4d2e65",
      "number": 9,
      "content": "chẳng bằng ẩn náu ở bên CHÚA TRỜI.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.516Z",
      "updatedAt": "2024-09-17T09:56:55.516Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "768db18e-15ce-4f50-a302-23a15179a602",
      "number": 10,
      "content": "Chư dân xúm lại bủa vây tôi,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.518Z",
      "updatedAt": "2024-09-17T09:56:55.518Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "e06e2054-bc1f-48b7-b9e0-342611863b6c",
      "number": 10,
      "content": "nhờ danh CHÚA, tôi đã trừ diệt chúng.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.522Z",
      "updatedAt": "2024-09-17T09:56:55.522Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "228a9b71-0af5-42d8-9cca-814a0ea1ae81",
      "number": 11,
      "content": "Chúng bủa vây tôi, trùng điệp tư bề,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.524Z",
      "updatedAt": "2024-09-17T09:56:55.524Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "19ab2e2f-f69b-482e-9494-85da37bb61c3",
      "number": 11,
      "content": "nhờ danh CHÚA, tôi đã trừ diệt chúng.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.527Z",
      "updatedAt": "2024-09-17T09:56:55.527Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "f5c65cbd-ed95-4713-bfb8-30ec59ecaf00",
      "number": 12,
      "content": "Chúng bủa vây tôi như thể bầy ong,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.529Z",
      "updatedAt": "2024-09-17T09:56:55.529Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "4c522403-5834-42ac-9dce-5bdead895851",
      "number": 12,
      "content": "chúng như lửa bụi gai cháy bừng bừng,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.531Z",
      "updatedAt": "2024-09-17T09:56:55.531Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "09e73352-a04f-47a1-b4b1-7a37317dcfda",
      "number": 12,
      "content": "nhờ danh CHÚA, tôi đã trừ diệt chúng.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.532Z",
      "updatedAt": "2024-09-17T09:56:55.532Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "911fcc23-ca86-49df-90e1-639affc4c522",
      "number": 13,
      "content": "Chúng xô đẩy tôi, xô cho ngã,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.534Z",
      "updatedAt": "2024-09-17T09:56:55.534Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "7b32e6da-7a89-471a-9bcd-b7d2651f819b",
      "number": 13,
      "content": "nhưng CHÚA đã phù trợ thân này.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.536Z",
      "updatedAt": "2024-09-17T09:56:55.536Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "5eb2733e-6cf4-45f9-aa1c-e1b2bd317e46",
      "number": 14,
      "content": "CHÚA là sức mạnh tôi, là Đấng tôi ca ngợi,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.538Z",
      "updatedAt": "2024-09-17T09:56:55.538Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "a9922cdf-f2aa-420c-bb59-7cd69d388b54",
      "number": 14,
      "content": "chính Người cứu độ tôi.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.542Z",
      "updatedAt": "2024-09-17T09:56:55.542Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "a1616e99-38f1-4ca7-a1d9-93e8e964ca56",
      "number": 15,
      "content": "Kìa nghe tiếng reo mừng chiến thắng",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.544Z",
      "updatedAt": "2024-09-17T09:56:55.544Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "59c79029-ee8c-4eb0-8715-029ace74dbd5",
      "number": 15,
      "content": "trong doanh trại chính nhân :",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.546Z",
      "updatedAt": "2024-09-17T09:56:55.546Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "87a85290-08cb-423c-b3a9-ee96f87b327d",
      "number": 16,
      "content": "“Tay hữu CHÚA đã ra oai thần lực, tay hữu CHÚA giơ cao,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.548Z",
      "updatedAt": "2024-09-17T09:56:55.548Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "a7ac08cb-0b51-4dfc-a834-b533ab1a55aa",
      "number": 16,
      "content": "tay hữu CHÚA đã ra oai thần lực.”",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.552Z",
      "updatedAt": "2024-09-17T09:56:55.552Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "beb5e31e-bcd4-4e1c-82d6-2613f4f15cc5",
      "number": 17,
      "content": "Tôi không phải chết, nhưng tôi sẽ sống,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.554Z",
      "updatedAt": "2024-09-17T09:56:55.554Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "47428092-311e-49ef-b532-ea4a3d208aa8",
      "number": 17,
      "content": "để loan báo những công việc CHÚA làm.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.556Z",
      "updatedAt": "2024-09-17T09:56:55.556Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "541d52a8-6f33-40bc-ae4c-592be9acef69",
      "number": 18,
      "content": "Sửa phạt tôi, vâng CHÚA sửa phạt tôi,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.558Z",
      "updatedAt": "2024-09-17T09:56:55.558Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "34e40e9e-5c94-4fec-a980-3005512ec8c3",
      "number": 18,
      "content": "nhưng không nỡ để tôi phải chết.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.560Z",
      "updatedAt": "2024-09-17T09:56:55.560Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "88180435-ca99-48a8-97a7-71f1339aa2d8",
      "number": 19,
      "content": "Xin mở cửa công chính cho tôi",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.563Z",
      "updatedAt": "2024-09-17T09:56:55.563Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "26ae3dd6-2fc2-4e49-adbd-590dc2cedd15",
      "number": 19,
      "content": "để tôi vào tạ ơn ĐỨC CHÚA.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.567Z",
      "updatedAt": "2024-09-17T09:56:55.567Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "49a48823-d680-4623-abe3-8bc1dd00ad66",
      "number": 20,
      "content": "Đây là cửa dẫn vào nơi CHÚA ngự,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.571Z",
      "updatedAt": "2024-09-17T09:56:55.571Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "a3203704-5814-455a-98f3-a40cb78f55be",
      "number": 20,
      "content": "chỉ những người công chính mới được qua.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.575Z",
      "updatedAt": "2024-09-17T09:56:55.575Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "e043b947-958f-4f43-9165-5fcb2979b8fd",
      "number": 21,
      "content": "Lạy Chúa, con xin tạ ơn Ngài",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.579Z",
      "updatedAt": "2024-09-17T09:56:55.579Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "f3ff2afd-cdcd-4dab-ba97-b46af83807a0",
      "number": 21,
      "content": "vì đã đáp lời con và thương cứu độ.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.583Z",
      "updatedAt": "2024-09-17T09:56:55.583Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "71c5b1f7-ee2a-4681-8d3f-fc8a0948dca9",
      "number": 22,
      "content": "Tảng đá thợ xây nhà loại bỏ",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.586Z",
      "updatedAt": "2024-09-17T09:56:55.586Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "bdb689b5-68a2-472a-b485-30ff23fd9dd8",
      "number": 22,
      "content": "lại trở nên đá tảng góc tường.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.590Z",
      "updatedAt": "2024-09-17T09:56:55.590Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "ce345198-705e-4df7-a5bb-2a2c6b1d12b1",
      "number": 23,
      "content": "Đó chính là công trình của CHÚA,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.594Z",
      "updatedAt": "2024-09-17T09:56:55.594Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "3fdb4e12-da2f-41f4-be6a-922f8f6cf8ed",
      "number": 23,
      "content": "công trình kỳ diệu trước mắt chúng ta.",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.598Z",
      "updatedAt": "2024-09-17T09:56:55.598Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "05804fa0-4b10-4294-bed0-f8564b36656c",
      "number": 24,
      "content": "Đây là ngày CHÚA đã làm ra,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.602Z",
      "updatedAt": "2024-09-17T09:56:55.602Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "b53a8b74-2bc3-446f-a61e-c96839df9944",
      "number": 24,
      "content": "nào ta hãy vui mừng hoan hỷ.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.606Z",
      "updatedAt": "2024-09-17T09:56:55.606Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "912a363a-bd54-4541-90d5-5f7d2d97e7a3",
      "number": 25,
      "content": "Lạy CHÚA, xin ban ơn cứu độ,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.610Z",
      "updatedAt": "2024-09-17T09:56:55.610Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "2ec0c483-cb13-42d4-89b7-73c60f1b79d8",
      "number": 25,
      "content": "lạy CHÚA, xin thương giúp thành công.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.618Z",
      "updatedAt": "2024-09-17T09:56:55.618Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "ac69c963-3e1b-4391-9734-b2e3c79c047c",
      "number": 26,
      "content": "Nguyện xin Chúa tuôn đổ phúc lành",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.622Z",
      "updatedAt": "2024-09-17T09:56:55.622Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "cfb78b42-c471-442d-adad-1ec6c1b57621",
      "number": 26,
      "content": "cho người tiến vào đây nhân danh CHÚA.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.630Z",
      "updatedAt": "2024-09-17T09:56:55.630Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "418c9049-b2be-404e-9717-425554df297a",
      "number": 26,
      "content": "Từ nhà CHÚA, chúng tôi chúc lành cho anh em.",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.634Z",
      "updatedAt": "2024-09-17T09:56:55.634Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "60bdb24f-3a31-4113-9a91-1d23a9c39450",
      "number": 27,
      "content": "ĐỨC CHÚA là Thượng Đế, Người giãi sáng trên ta.",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.641Z",
      "updatedAt": "2024-09-17T09:56:55.641Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "d811905e-4054-4c86-badc-70ef9d44008f",
      "number": 27,
      "content": "Tay cầm lá, hãy sắp thành đám rước đến bên cạnh bàn thờ.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.645Z",
      "updatedAt": "2024-09-17T09:56:55.645Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "0d0d03a7-9d8e-4a55-9ebc-f5ebfc721ba8",
      "number": 28,
      "content": "Lạy Chúa, chính Ngài là Thiên Chúa của con,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.649Z",
      "updatedAt": "2024-09-17T09:56:55.649Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "4c3c6b76-1701-4ade-9a71-fcb749adc591",
      "number": 28,
      "content": "xin dâng Ngài muôn câu cảm tạ ;",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.654Z",
      "updatedAt": "2024-09-17T09:56:55.654Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "7034b9d5-4277-4cb3-9bfc-337e2a24fb27",
      "number": 28,
      "content": "lạy Thiên Chúa con thờ, xin dâng Ngài vạn tiếng tôn vinh.",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.658Z",
      "updatedAt": "2024-09-17T09:56:55.658Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "325532ba-50ea-4219-936c-1b51992d0200",
      "number": 29,
      "content": "Hãy tạ ơn CHÚA vì Chúa nhân từ,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.661Z",
      "updatedAt": "2024-09-17T09:56:55.661Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "fe780160-47e4-4980-8d90-8e4c7c0a2f3e",
      "number": 29,
      "content": "muôn ngàn đời Chúa vẫn trọn tình thương.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T09:56:55.669Z",
      "updatedAt": "2024-09-17T09:56:55.669Z",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    }
  ],
  "footnotes": [
    {
      "id": "e58ef1bb-77cf-43f4-bbcf-8e14ea63e452",
      "content": "Cộng đoàn sử dụng <i>Tv tạ ơn này của tập thể</i> lúc đi kiệu vào Đền Thờ để tạ ơn Đức Chúa, sau khi được giải thoát khỏi một hiểm hoạ lớn. Các tư tế chúc lành cho cộng đoàn, rồi chuẩn bị dâng lễ tế tạ ơn. Tv này là Tv cuối cùng của nhóm Tv Ha-len (các Tv 113 – 118). Đây là một cách phân chia Tv : <b>1</b>) Cc. 1-4 ; <b>2</b>) Cc. 5-9 ; <b>3</b>) Cc. 10-21 ; <b>4</b>) Cc. 22-25 ; <b>5</b>) Cc. 26-27 ; <b>6</b>) C.28 ; <b>7</b>) C.29.",
      "position": 39,
      "order": 0,
      "createdAt": "2024-09-17T09:56:55.476Z",
      "updatedAt": "2024-09-17T09:56:55.476Z",
      "headingId": "17175ed5-527e-4f16-a3e9-dc4ecf5472d6",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "8eceb4ee-b078-403d-af80-bad6d28bfd8d",
      "content": "Trong cc. 2-4, ba nhóm lần lượt được kêu mời tạ ơn Đức Chúa như trong Tv 115,9-11 và Tv 135,19-20.",
      "position": 25,
      "order": 1,
      "createdAt": "2024-09-17T09:56:55.484Z",
      "updatedAt": "2024-09-17T09:56:55.484Z",
      "verseId": "c4dc8c88-c676-45bc-b15f-e4c1fad33e0f",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "226b8d35-618e-4f13-b405-cde77fd0eae7",
      "content": "<i>Xin ban ơn cứu độ</i> – “<span class=\"hebrew-translit\">hôšî`â nä</span>” (tiếng Híp-ri) = “Hosanna” (phiên âm ra tiếng La-tinh) trở thành một tiếng reo mừng, một lời tung hô (x. Mt 21,9 ; Mc 11,9 ; Ga 12,13).",
      "position": 27,
      "order": 2,
      "createdAt": "2024-09-17T09:56:55.614Z",
      "updatedAt": "2024-09-17T09:56:55.614Z",
      "verseId": "912a363a-bd54-4541-90d5-5f7d2d97e7a3",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    },
    {
      "id": "dd1dfb57-ccf1-4ce3-ae1c-9f9fc4ad5545",
      "content": "Nội dung của cc. 26-27 : các tư tế chúc lành cho đám đông, rồi mời gọi họ chuẩn bị dâng lễ tế tạ ơn.",
      "position": 33,
      "order": 3,
      "createdAt": "2024-09-17T09:56:55.626Z",
      "updatedAt": "2024-09-17T09:56:55.626Z",
      "verseId": "ac69c963-3e1b-4391-9734-b2e3c79c047c",
      "chapterId": "246e1626-4317-40ba-a872-bb04ad2d688a"
    }
  ],
