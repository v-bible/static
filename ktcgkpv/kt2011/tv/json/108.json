{
  "chapter": {
    "id": "e65f6d27-96db-4f24-8775-159c0804a766",
    "number": 108,
    "ref": "",
    "createdAt": "2024-09-10T00:39:05.893Z",
    "updatedAt": "2024-09-10T00:39:05.893Z",
    "bookId": "36ce7190-e479-4be3-9624-b27d78763f56"
  },
  "verses": [
    {
      "id": "037c01e0-aa3e-4edb-af9a-cdf2aaf44d82",
      "number": 1,
      "content": "Thánh ca. Thánh vịnh. Của vua Đa-vít.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:28:37.069Z",
      "updatedAt": "2024-09-10T04:28:37.069Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "9bd0b803-2076-4f9f-9aee-361ea2c623f7",
      "number": 2,
      "content": "Con vững dạ an lòng, lạy Thiên Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.076Z",
      "updatedAt": "2024-09-10T04:28:37.076Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "79153078-2451-4ce8-bb51-41243bb803ab",
      "number": 2,
      "content": "này con xin đàn hát xướng ca với tất cả tâm hồn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.080Z",
      "updatedAt": "2024-09-10T04:28:37.080Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "c5fcae90-b75c-49ed-8f31-65e3babb3c6d",
      "number": 3,
      "content": "Thức dậy đi, hỡi đàn sắt đàn cầm,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.082Z",
      "updatedAt": "2024-09-10T04:28:37.082Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "bc005473-3853-4908-9d28-d5554d229873",
      "number": 3,
      "content": "tôi còn đánh thức cả bình minh.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.084Z",
      "updatedAt": "2024-09-10T04:28:37.084Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "f5a253d1-e251-4e13-a481-53b5917e2497",
      "number": 4,
      "content": "Lạy THIÊN CHÚA, trước mặt chư dân, con dâng lời cảm tạ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.088Z",
      "updatedAt": "2024-09-10T04:28:37.088Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "79b74cc6-667b-4955-87ea-57d8221d60d4",
      "number": 4,
      "content": "giữa lòng muôn nước, con đàn hát xướng ca.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.098Z",
      "updatedAt": "2024-09-10T04:28:37.098Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "4f7c237a-165f-4d9f-a5a8-4d31aa47d3d9",
      "number": 5,
      "content": "Vì tình thương của Ngài cao ngất trời xanh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.102Z",
      "updatedAt": "2024-09-10T04:28:37.102Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "0418b04b-d43a-4cb3-8977-9e26ef1cf8b2",
      "number": 5,
      "content": "lòng thành tín vượt ngàn mây thẳm.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.107Z",
      "updatedAt": "2024-09-10T04:28:37.107Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "abc08b3c-82e4-4477-b46d-38069ef0d450",
      "number": 6,
      "content": "Lạy Thiên Chúa, xin biểu thị uy phong khắp cõi trời",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.112Z",
      "updatedAt": "2024-09-10T04:28:37.112Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "cf9133a7-9c7e-4e6f-9bee-f9ea47a292cf",
      "number": 6,
      "content": "và chiếu toả vinh quang đầy mặt đất.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.117Z",
      "updatedAt": "2024-09-10T04:28:37.117Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "1d0f4c28-94e0-4302-a25a-13c0cf2240a2",
      "number": 7,
      "content": "Xin ra tay cứu độ, và đáp lời chúng con,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.122Z",
      "updatedAt": "2024-09-10T04:28:37.122Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "aca7cdca-783a-4952-9811-f4217af8aa52",
      "number": 7,
      "content": "để những kẻ Ngài yêu thương được ơn giải thoát.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.136Z",
      "updatedAt": "2024-09-10T04:28:37.136Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "22e4f661-75f2-4a8c-bd74-c49388e1e049",
      "number": 8,
      "content": "Trong thánh điện, Chúa Trời tuyên phán :",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.141Z",
      "updatedAt": "2024-09-10T04:28:37.141Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "446cf7a0-87de-4947-b787-a63ca03600eb",
      "number": 8,
      "content": "Khấp khởi mừng, Ta chia cắt Si-khem,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.146Z",
      "updatedAt": "2024-09-10T04:28:37.146Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "5852a4f3-0152-43cf-9af1-7ce1094fa39c",
      "number": 8,
      "content": "Ta đo đạc cả đồng bằng Xúc-cốt.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.150Z",
      "updatedAt": "2024-09-10T04:28:37.150Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "f10d296f-53e1-4f7e-b140-266f893c2ff4",
      "number": 9,
      "content": "Ga-la-át về Ta, Mơ-na-se cũng về Ta,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.155Z",
      "updatedAt": "2024-09-10T04:28:37.155Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "300a8a4d-3747-4289-82ae-827264de856b",
      "number": 9,
      "content": "Ta lấy Ép-ra-im làm mũ chiến đội đầu,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.160Z",
      "updatedAt": "2024-09-10T04:28:37.160Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "b15f10d2-6cf0-4ab4-9ae2-30803495e5f8",
      "number": 9,
      "content": "lấy Giu-đa làm cây gậy chỉ huy.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.165Z",
      "updatedAt": "2024-09-10T04:28:37.165Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "4b50033c-da2f-4b25-9af1-448233dea0f8",
      "number": 10,
      "content": "“Mô-áp là chậu Ta tắm rửa, Ê-đôm là chỗ Ta liệng giày,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.170Z",
      "updatedAt": "2024-09-10T04:28:37.170Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "37b1fd5a-814a-4646-953a-19a1eb7eb4d4",
      "number": 10,
      "content": "đánh bại Phi-li-tinh, Ta reo mừng thắng trận.”",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.179Z",
      "updatedAt": "2024-09-10T04:28:37.179Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "e6414543-1fcd-45fa-b945-fa43faa8e234",
      "number": 11,
      "content": "Lạy Thiên Chúa, ai sẽ đưa con vào thành kiên cố,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.184Z",
      "updatedAt": "2024-09-10T04:28:37.184Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "fe121e8c-0cd7-4ff5-bcad-6b3de8c0c338",
      "number": 11,
      "content": "ai sẽ dắt con đến tận Ê-đôm ?",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.188Z",
      "updatedAt": "2024-09-10T04:28:37.188Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "24ef6664-8d19-440b-aee6-ed727266f20f",
      "number": 12,
      "content": "Há chẳng phải chính Ngài,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.193Z",
      "updatedAt": "2024-09-10T04:28:37.193Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "c545b061-5446-4c35-b660-ba5705a6a18c",
      "number": 12,
      "content": "Đấng đã từng ruồng bỏ chúng con,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.198Z",
      "updatedAt": "2024-09-10T04:28:37.198Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "cae246c9-8a23-497b-9bc0-74d8be8ea3b8",
      "number": 12,
      "content": "chẳng còn dẫn ba quân xuất trận ?",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.203Z",
      "updatedAt": "2024-09-10T04:28:37.203Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "a8583b2b-63a1-466d-9b33-d428d3be9d01",
      "number": 13,
      "content": "Xin Ngài thương cứu viện, cho chúng con thoát khỏi quân thù,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.208Z",
      "updatedAt": "2024-09-10T04:28:37.208Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "346fe29d-be16-43ae-a0c1-1f831e7d22db",
      "number": 13,
      "content": "chứ người phàm giúp đỡ chỉ bằng không.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.214Z",
      "updatedAt": "2024-09-10T04:28:37.214Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "ba1d3acf-5210-4976-8235-677eb1758e8a",
      "number": 14,
      "content": "Có Thiên Chúa, ta sẽ trổ tài oanh liệt,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.219Z",
      "updatedAt": "2024-09-10T04:28:37.219Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    },
    {
      "id": "7251ef62-4265-414d-8f2a-496a7cdec548",
      "number": 14,
      "content": "chính Người chà đạp kẻ thù ta.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:28:37.223Z",
      "updatedAt": "2024-09-10T04:28:37.223Z",
      "chapterId": "e65f6d27-96db-4f24-8775-159c0804a766"
    }
  ],
