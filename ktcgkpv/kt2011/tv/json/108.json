{
  "chapter": {
    "id": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9",
    "number": 108,
    "ref": "",
    "createdAt": "2024-09-17T08:41:12.650Z",
    "updatedAt": "2024-09-17T08:41:12.650Z",
    "bookId": "d770eae8-a8d2-4a9b-89b0-d3fe47cc3846"
  },
  "verses": [
    {
      "id": "6f10eabf-b03b-434d-b1be-1e9dad46aa42",
      "number": 1,
      "content": "Thánh ca. Thánh vịnh. Của vua Đa-vít.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:01:22.971Z",
      "updatedAt": "2024-09-17T10:01:22.971Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "f3ea971a-d851-4cf0-81a0-2890d214af7b",
      "number": 2,
      "content": "Con vững dạ an lòng, lạy Thiên Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:22.977Z",
      "updatedAt": "2024-09-17T10:01:22.977Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "8c16d250-8203-4996-b6e2-84e515da8e6f",
      "number": 2,
      "content": "này con xin đàn hát xướng ca với tất cả tâm hồn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:22.981Z",
      "updatedAt": "2024-09-17T10:01:22.981Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "d5fb5593-ffd8-4095-8306-fc786e552e96",
      "number": 3,
      "content": "Thức dậy đi, hỡi đàn sắt đàn cầm,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:22.983Z",
      "updatedAt": "2024-09-17T10:01:22.983Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "ec900614-f519-45cf-8a61-f94488164399",
      "number": 3,
      "content": "tôi còn đánh thức cả bình minh.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:22.985Z",
      "updatedAt": "2024-09-17T10:01:22.985Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "11b68148-9e97-4043-b36b-911b30a1bda7",
      "number": 4,
      "content": "Lạy THIÊN CHÚA, trước mặt chư dân, con dâng lời cảm tạ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:22.987Z",
      "updatedAt": "2024-09-17T10:01:22.987Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "7ab9f8bb-5461-42f9-9b32-e01bd211cb18",
      "number": 4,
      "content": "giữa lòng muôn nước, con đàn hát xướng ca.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:22.991Z",
      "updatedAt": "2024-09-17T10:01:22.991Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "1834ad06-6c02-4dcf-b38e-9ca0f34f7eae",
      "number": 5,
      "content": "Vì tình thương của Ngài cao ngất trời xanh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:22.993Z",
      "updatedAt": "2024-09-17T10:01:22.993Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "a5eeac95-b4a9-4bd3-8bc6-efe5c5d8e2dc",
      "number": 5,
      "content": "lòng thành tín vượt ngàn mây thẳm.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:22.995Z",
      "updatedAt": "2024-09-17T10:01:22.995Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "47f067a6-d447-4bd8-8ce1-74a6e16d64d4",
      "number": 6,
      "content": "Lạy Thiên Chúa, xin biểu thị uy phong khắp cõi trời",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:22.997Z",
      "updatedAt": "2024-09-17T10:01:22.997Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "b3ab683f-4283-41f0-a3e3-2023a5aeb3ad",
      "number": 6,
      "content": "và chiếu toả vinh quang đầy mặt đất.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23Z",
      "updatedAt": "2024-09-17T10:01:23Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "a537897a-83ac-446a-9b2b-dfb84790d246",
      "number": 7,
      "content": "Xin ra tay cứu độ, và đáp lời chúng con,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.002Z",
      "updatedAt": "2024-09-17T10:01:23.002Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "b2a713c3-e38e-4c49-9af9-091c64d0b6c6",
      "number": 7,
      "content": "để những kẻ Ngài yêu thương được ơn giải thoát.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.008Z",
      "updatedAt": "2024-09-17T10:01:23.008Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "fab6f8cf-4acd-4e5f-b8f6-c76232364df7",
      "number": 8,
      "content": "Trong thánh điện, Chúa Trời tuyên phán :",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.010Z",
      "updatedAt": "2024-09-17T10:01:23.010Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "22d32508-53c5-473c-9791-f30fcdf6e51e",
      "number": 8,
      "content": "Khấp khởi mừng, Ta chia cắt Si-khem,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.012Z",
      "updatedAt": "2024-09-17T10:01:23.012Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "c9d6db03-d5d6-420a-9bf6-21105a2c59af",
      "number": 8,
      "content": "Ta đo đạc cả đồng bằng Xúc-cốt.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.014Z",
      "updatedAt": "2024-09-17T10:01:23.014Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "6acca6d2-e27f-4f01-bbfc-6684e7a90a89",
      "number": 9,
      "content": "Ga-la-át về Ta, Mơ-na-se cũng về Ta,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.016Z",
      "updatedAt": "2024-09-17T10:01:23.016Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "0771e71d-bf1d-4484-b7c6-c948d8f9edfd",
      "number": 9,
      "content": "Ta lấy Ép-ra-im làm mũ chiến đội đầu,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.019Z",
      "updatedAt": "2024-09-17T10:01:23.019Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "da6e46f1-be73-410d-9cca-0b43051bb875",
      "number": 9,
      "content": "lấy Giu-đa làm cây gậy chỉ huy.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.021Z",
      "updatedAt": "2024-09-17T10:01:23.021Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "b568675a-7ffa-4591-9147-529edda43c81",
      "number": 10,
      "content": "“Mô-áp là chậu Ta tắm rửa, Ê-đôm là chỗ Ta liệng giày,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.024Z",
      "updatedAt": "2024-09-17T10:01:23.024Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "8b70e680-92c9-4fe9-b0d9-164057fbaf84",
      "number": 10,
      "content": "đánh bại Phi-li-tinh, Ta reo mừng thắng trận.”",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.029Z",
      "updatedAt": "2024-09-17T10:01:23.029Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "6b9fc5b0-0284-4d51-b4cf-1bbe305a454f",
      "number": 11,
      "content": "Lạy Thiên Chúa, ai sẽ đưa con vào thành kiên cố,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.032Z",
      "updatedAt": "2024-09-17T10:01:23.032Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "aaade593-8cc9-49e9-9fb2-2f8a26b2141c",
      "number": 11,
      "content": "ai sẽ dắt con đến tận Ê-đôm ?",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.035Z",
      "updatedAt": "2024-09-17T10:01:23.035Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "16eca75c-9a64-4454-a26b-1dcdf78b62b5",
      "number": 12,
      "content": "Há chẳng phải chính Ngài,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.038Z",
      "updatedAt": "2024-09-17T10:01:23.038Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "d7efdc38-6dc3-41d3-850b-3d41f0313226",
      "number": 12,
      "content": "Đấng đã từng ruồng bỏ chúng con,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.041Z",
      "updatedAt": "2024-09-17T10:01:23.041Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "0e2b6320-9adf-4862-9f1a-3fe9dc73c033",
      "number": 12,
      "content": "chẳng còn dẫn ba quân xuất trận ?",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.045Z",
      "updatedAt": "2024-09-17T10:01:23.045Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "027e5bc1-27fb-46d2-8cea-9ed74f96bc06",
      "number": 13,
      "content": "Xin Ngài thương cứu viện, cho chúng con thoát khỏi quân thù,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.048Z",
      "updatedAt": "2024-09-17T10:01:23.048Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "ebf187f6-e0db-435e-aea2-fed07142b9b3",
      "number": 13,
      "content": "chứ người phàm giúp đỡ chỉ bằng không.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.055Z",
      "updatedAt": "2024-09-17T10:01:23.055Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "7f18115b-ffb7-469a-a688-d39c2406e88a",
      "number": 14,
      "content": "Có Thiên Chúa, ta sẽ trổ tài oanh liệt,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.060Z",
      "updatedAt": "2024-09-17T10:01:23.060Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    },
    {
      "id": "b04c7493-df17-4907-a61b-c70ef6024795",
      "number": 14,
      "content": "chính Người chà đạp kẻ thù ta.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:01:23.064Z",
      "updatedAt": "2024-09-17T10:01:23.064Z",
      "chapterId": "467a0d6f-1cf8-495b-b4ad-c71975d3c6b9"
    }
  ],
