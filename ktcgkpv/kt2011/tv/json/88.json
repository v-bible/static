{
  "chapter": {
    "id": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3",
    "number": 88,
    "ref": "",
    "createdAt": "2024-09-17T08:41:12.649Z",
    "updatedAt": "2024-09-17T08:41:12.649Z",
    "bookId": "d770eae8-a8d2-4a9b-89b0-d3fe47cc3846"
  },
  "verses": [
    {
      "id": "999db3f3-f55d-41ac-a2bc-c213e6d28d07",
      "number": 1,
      "content": "Thánh ca. Thánh vịnh. Của con cái ông Cô-rắc. Phần ca trưởng. Lúc bệnh hoạn. Lúc sầu muộn. Thi khúc. Của ông Hê-man, người Ét-ra.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-17T10:00:13.234Z",
      "updatedAt": "2024-09-17T10:00:13.234Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "1f9d8177-d7bb-4075-b084-a6d95df36505",
      "number": 2,
      "content": "Lạy CHÚA là Thiên Chúa cứu độ con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.241Z",
      "updatedAt": "2024-09-17T10:00:13.241Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "58be4151-45ff-4ee4-8148-355bc2b01061",
      "number": 2,
      "content": "trước Thánh Nhan, đêm ngày con kêu cứu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.243Z",
      "updatedAt": "2024-09-17T10:00:13.243Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "539beb74-18d2-445a-83c1-7624b4954a6e",
      "number": 3,
      "content": "Nguyện cho lời kinh vọng tới Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.245Z",
      "updatedAt": "2024-09-17T10:00:13.245Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "c1232851-2978-4a26-a95c-950d68b8700f",
      "number": 3,
      "content": "xin lắng nghe tiếng lòng thổn thức.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.247Z",
      "updatedAt": "2024-09-17T10:00:13.247Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "4c78ddf0-a28d-4be6-a369-5c3e02a20efe",
      "number": 4,
      "content": "Vì hồn con ngập tràn đau khổ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.249Z",
      "updatedAt": "2024-09-17T10:00:13.249Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "a681bb74-0f02-4bd3-928a-eeedd0d2ca07",
      "number": 4,
      "content": "mạng sống con âm phủ gần kề,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.252Z",
      "updatedAt": "2024-09-17T10:00:13.252Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "e6eeb044-512a-4a2b-92d9-569340bf83f6",
      "number": 5,
      "content": "thân kể như đã vào phần mộ,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.258Z",
      "updatedAt": "2024-09-17T10:00:13.258Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "cd584d88-cca6-49e9-84bc-2e985b66e6de",
      "number": 5,
      "content": "ví tựa người kiệt sức còn chi !",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.260Z",
      "updatedAt": "2024-09-17T10:00:13.260Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "d07930eb-d49c-4039-9c22-84dc5a3a6f44",
      "number": 6,
      "content": "Con nằm đây giữa bao người chết,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.263Z",
      "updatedAt": "2024-09-17T10:00:13.263Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "ea6c7762-002d-460b-b87a-a7584f6be90d",
      "number": 6,
      "content": "như các tử thi vùi trong mồ mả",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.265Z",
      "updatedAt": "2024-09-17T10:00:13.265Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "98dad89a-0522-482f-ac98-653cdfbbaaab",
      "number": 6,
      "content": "đã bị Chúa quên đi, và không được tay Ngài săn sóc.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.267Z",
      "updatedAt": "2024-09-17T10:00:13.267Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "d6abd5b8-6295-467a-b568-e6a7d24b0f7a",
      "number": 7,
      "content": "Chúa hạ con xuống tận đáy huyệt sâu,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.269Z",
      "updatedAt": "2024-09-17T10:00:13.269Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "3e026f53-60a1-40cb-9ec3-7eebe65b7b03",
      "number": 7,
      "content": "giữa chốn tối tăm, giữa lòng vực thẳm.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.271Z",
      "updatedAt": "2024-09-17T10:00:13.271Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "ba5c27a3-96da-4618-b110-f04c16c03221",
      "number": 8,
      "content": "Cơn giận Chúa đè nặng thân con",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.273Z",
      "updatedAt": "2024-09-17T10:00:13.273Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "29ffc8e3-36a2-4312-a15d-30e75d49fecd",
      "number": 8,
      "content": "như sóng cồn xô đẩy dập vùi.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.275Z",
      "updatedAt": "2024-09-17T10:00:13.275Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "5fbe5bef-4afb-463e-b95e-c77024223e6a",
      "number": 9,
      "content": "Chúa làm cho bạn bè xa lánh",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.280Z",
      "updatedAt": "2024-09-17T10:00:13.280Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "6beefca4-335f-45e5-ae35-1a19f52cc117",
      "number": 9,
      "content": "và coi con như đồ ghê tởm.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.282Z",
      "updatedAt": "2024-09-17T10:00:13.282Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "2ef52ed0-4f01-4674-aabf-a0e853e63744",
      "number": 9,
      "content": "Con bị giam cầm không thể thoát ra,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.284Z",
      "updatedAt": "2024-09-17T10:00:13.284Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "9464753c-1fff-4807-bb2f-24e0fdfe5e3d",
      "number": 10,
      "content": "mắt mờ đi vì quá nhiều đau khổ.",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.287Z",
      "updatedAt": "2024-09-17T10:00:13.287Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "2c11f105-f50f-47e1-99c3-8513e3e7681a",
      "number": 10,
      "content": "Lạy CHÚA, suốt cả ngày con kêu lên Chúa",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.289Z",
      "updatedAt": "2024-09-17T10:00:13.289Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "b3fe323d-1e23-4f29-b5dc-ef6f0bb536c1",
      "number": 10,
      "content": "và giơ tay hướng thẳng về Ngài.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.292Z",
      "updatedAt": "2024-09-17T10:00:13.292Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "73736fed-d0f5-4dc6-b259-ad599aedb817",
      "number": 11,
      "content": "Chúa đâu làm phép lạ cho người đã mạng vong,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.294Z",
      "updatedAt": "2024-09-17T10:00:13.294Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "3c989643-8400-4033-9a8b-3bae3928d3a8",
      "number": 11,
      "content": "âm hồn đâu trỗi dậy ca tụng Chúa bao giờ ?",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.296Z",
      "updatedAt": "2024-09-17T10:00:13.296Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "1c0dcebc-9d07-4799-94dd-8b25e460c2a8",
      "number": 12,
      "content": "Trong mồ mả, ai nói về tình thương của Chúa ?",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.298Z",
      "updatedAt": "2024-09-17T10:00:13.298Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "f9243d62-d11f-49b5-a564-c9f2394db0d5",
      "number": 12,
      "content": "Cõi âm ty, ai kể lại lòng thành tín của Ngài ?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.302Z",
      "updatedAt": "2024-09-17T10:00:13.302Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "04747581-0805-4656-8c26-4fe0833b24b9",
      "number": 13,
      "content": "Những kỳ công Chúa, nơi tối tăm ai rõ ?",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.307Z",
      "updatedAt": "2024-09-17T10:00:13.307Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "598495bc-c35d-4048-a5b4-0cc64f7dedb8",
      "number": 13,
      "content": "Đức công chính Ngài, chốn quên lãng ai hay ?",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.309Z",
      "updatedAt": "2024-09-17T10:00:13.309Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "084f8f26-68d8-427d-8fec-ee8bfeb018c3",
      "number": 14,
      "content": "Phần con đây, con kêu lên Ngài, lạy CHÚA,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.311Z",
      "updatedAt": "2024-09-17T10:00:13.311Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "e637cef0-fa96-49fc-b213-9393311adac6",
      "number": 14,
      "content": "mới tinh sương đã chờ chực nguyện xin.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.313Z",
      "updatedAt": "2024-09-17T10:00:13.313Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "304ee2fd-aef0-455c-bc68-72397c19812a",
      "number": 15,
      "content": "Lạy CHÚA, thân con đây, Chúa nỡ nào ruồng rẫy,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.315Z",
      "updatedAt": "2024-09-17T10:00:13.315Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "687c96f2-79af-4885-b3c3-9c8051cc63a1",
      "number": 15,
      "content": "ẩn mặt đi mà chẳng đoái hoài.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.317Z",
      "updatedAt": "2024-09-17T10:00:13.317Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "4f5f604b-81e5-4fe3-8cca-7e7781ae822b",
      "number": 16,
      "content": "Từ thuở bé, con khổ đã nhiều và luôn ngắc ngoải,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.319Z",
      "updatedAt": "2024-09-17T10:00:13.319Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "ad907bf1-ea16-4c5d-9be8-63f03bacd168",
      "number": 16,
      "content": "Chúa làm con kinh hãi, con hoá ra thẫn thờ.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.321Z",
      "updatedAt": "2024-09-17T10:00:13.321Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "6e42de68-3722-458e-ad45-55931263b148",
      "number": 17,
      "content": "Bao cơn thịnh nộ, Ngài đổ ngập thân con,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.323Z",
      "updatedAt": "2024-09-17T10:00:13.323Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "348dec57-a4d9-46ae-8035-dd1eee80de57",
      "number": 17,
      "content": "bấy nỗi kinh hoàng khiến con rời rã.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.325Z",
      "updatedAt": "2024-09-17T10:00:13.325Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "177c7ccd-82ce-4315-8022-934bae642c18",
      "number": 18,
      "content": "Bủa vây con suốt ngày ngần ấy thứ,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.327Z",
      "updatedAt": "2024-09-17T10:00:13.327Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "8fba0956-e613-4e01-aa77-7e9c654d037c",
      "number": 18,
      "content": "dồn dập tư bề như nước bao la.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.329Z",
      "updatedAt": "2024-09-17T10:00:13.329Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "43d22363-a50a-4cec-9b79-14e51aa601c2",
      "number": 19,
      "content": "Cận thân Chúa khiến lìa xa,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.332Z",
      "updatedAt": "2024-09-17T10:00:13.332Z",
      "chapterId": "15b7b9a9-bdc6-438e-84e7-1ecb78c306d3"
    },
    {
      "id": "66d93d63-e785-49e0-a365-3683ae1194b4",
      "number": 19,
      "content": "chung quanh bầu bạn chỉ là bóng đêm.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-17T10:00:13.337Z",
      "updatedAt": "2024-09-17T10:00:13.337Z",
