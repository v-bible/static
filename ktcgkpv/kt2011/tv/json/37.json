{
  "chapter": {
    "id": "a0d1a633-3212-4cd5-a34c-8bb8db263ada",
    "number": 37,
    "ref": "",
    "createdAt": "2024-09-10T00:39:05.892Z",
    "updatedAt": "2024-09-10T00:39:05.892Z",
    "bookId": "36ce7190-e479-4be3-9624-b27d78763f56"
  },
  "verses": [
    {
      "id": "0f9393b3-08b1-4c9e-be61-90ef7a6b980c",
      "number": 1,
      "content": "Của vua Đa-vít.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.527Z",
      "updatedAt": "2024-09-10T04:24:33.527Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "7ff38578-49dd-47a0-837b-08455dc86f7c",
      "number": 1,
      "content": "A-lép",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.542Z",
      "updatedAt": "2024-09-10T04:24:33.542Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "5c83ab87-5ac3-4995-a90b-347f18429041",
      "number": 1,
      "content": "Bạn đừng nổi giận vì quân gian ác,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.544Z",
      "updatedAt": "2024-09-10T04:24:33.544Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "0b72f38c-e711-4bec-9c1d-2f4524bd1675",
      "number": 1,
      "content": "chớ phân bì với kẻ bất lương,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.554Z",
      "updatedAt": "2024-09-10T04:24:33.554Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "c316bae4-284a-4001-b631-686f96ab4693",
      "number": 2,
      "content": "vì chúng tựa cỏ hoang mau úa",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.557Z",
      "updatedAt": "2024-09-10T04:24:33.557Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "9d395b0d-8c02-43d8-a9b0-74e7e41bbc10",
      "number": 2,
      "content": "và giống như thanh thảo chóng tàn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.561Z",
      "updatedAt": "2024-09-10T04:24:33.561Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "c33015af-fe3a-4654-9ebd-cacf680572ca",
      "number": 3,
      "content": "Bết",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.564Z",
      "updatedAt": "2024-09-10T04:24:33.564Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "08932a25-6002-4663-ae73-12ecb31e5b1b",
      "number": 3,
      "content": "Cứ tin tưởng vào CHÚA và làm điều thiện,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.566Z",
      "updatedAt": "2024-09-10T04:24:33.566Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "38bb2d27-ba66-4323-902a-a9ab49c3abef",
      "number": 3,
      "content": "thì sẽ được ở trong đất nước và sống yên hàn.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.568Z",
      "updatedAt": "2024-09-10T04:24:33.568Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "8f077a87-66a1-4c46-9d3a-fd203f63575b",
      "number": 4,
      "content": "Hãy lấy CHÚA làm niềm vui của bạn,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.572Z",
      "updatedAt": "2024-09-10T04:24:33.572Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "24ef350f-4dfd-4d13-91c7-37d42a0a0b39",
      "number": 4,
      "content": "Người sẽ cho được phỉ chí toại lòng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.575Z",
      "updatedAt": "2024-09-10T04:24:33.575Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "f86b54e5-17ac-4b73-a1e8-5bd06d5856e0",
      "number": 5,
      "content": "Ghi-men",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.577Z",
      "updatedAt": "2024-09-10T04:24:33.577Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "95665336-9782-4ff3-a6cf-d725754053a3",
      "number": 5,
      "content": "Hãy ký thác đường đời cho CHÚA,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.579Z",
      "updatedAt": "2024-09-10T04:24:33.579Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "437c9243-16dc-4744-97f3-378b324b3f15",
      "number": 5,
      "content": "tin tưởng vào Người, Người sẽ ra tay.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.581Z",
      "updatedAt": "2024-09-10T04:24:33.581Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "9675fc91-2b3c-4220-9943-719e421241dd",
      "number": 6,
      "content": "Chính nghĩa bạn, Chúa sẽ làm rực rỡ tựa bình minh,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.584Z",
      "updatedAt": "2024-09-10T04:24:33.584Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "ce281038-a87a-4662-a46e-61a72b6af180",
      "number": 6,
      "content": "công lý bạn, Người sẽ cho huy hoàng như chính ngọ.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.586Z",
      "updatedAt": "2024-09-10T04:24:33.586Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "99de4a6e-6b9e-4619-9092-962957a35902",
      "number": 7,
      "content": "Đa-lét",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.588Z",
      "updatedAt": "2024-09-10T04:24:33.588Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "7b34cdef-70cc-4ab3-a3b6-16143dfb2963",
      "number": 7,
      "content": "Hãy lặng thinh trước mặt CHÚA và đợi trông Người.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.590Z",
      "updatedAt": "2024-09-10T04:24:33.590Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "e23f3884-dbd5-4773-bce9-fbdb921c002b",
      "number": 7,
      "content": "Bạn chẳng nên nổi giận",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.603Z",
      "updatedAt": "2024-09-10T04:24:33.603Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "547b6cfa-45fa-47e3-a39f-f920d2fed2ea",
      "number": 7,
      "content": "với kẻ được thành công hay với người xảo trá.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.605Z",
      "updatedAt": "2024-09-10T04:24:33.605Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "325149a2-482b-4428-98c3-b3ad03875268",
      "number": 8,
      "content": "Hê",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.615Z",
      "updatedAt": "2024-09-10T04:24:33.615Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "d41f4049-41a2-443b-aa18-012b56342dbd",
      "number": 8,
      "content": "Dừng cơn phẫn nộ và chớ mãi nổi xung,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.618Z",
      "updatedAt": "2024-09-10T04:24:33.618Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "2dcf1fe1-c13c-4d07-ac20-c99c60e763ef",
      "number": 8,
      "content": "đừng nổi giận kẻo sinh ra tội lỗi,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.620Z",
      "updatedAt": "2024-09-10T04:24:33.620Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "ea8e798f-65e1-46d3-bd69-d6cc2e1c7a30",
      "number": 9,
      "content": "vì bọn gian ác sẽ bị diệt trừ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.622Z",
      "updatedAt": "2024-09-10T04:24:33.622Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "e7a4c741-845f-43e1-b31d-8185a54e63a8",
      "number": 9,
      "content": "còn người trông đợi CHÚA, sẽ được đất hứa làm gia nghiệp.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.624Z",
      "updatedAt": "2024-09-10T04:24:33.624Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "39d85baa-d2ed-4a44-9eba-259af0d9c05f",
      "number": 10,
      "content": "Vau",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.627Z",
      "updatedAt": "2024-09-10T04:24:33.627Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "7846ea0b-da81-499b-9465-10018d89037a",
      "number": 10,
      "content": "Ít lâu nữa ác nhân sẽ chẳng còn,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.629Z",
      "updatedAt": "2024-09-10T04:24:33.629Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "0d72748c-f07f-40cb-8a3a-dc51e2755bd2",
      "number": 10,
      "content": "đến chỗ xưa cũng không tìm thấy hắn.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.631Z",
      "updatedAt": "2024-09-10T04:24:33.631Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "41adba60-56bf-4aab-a9de-417a9fca338a",
      "number": 11,
      "content": "Còn kẻ nghèo hèn được đất hứa làm gia nghiệp",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.633Z",
      "updatedAt": "2024-09-10T04:24:33.633Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "08a572d4-3f03-4735-9c66-afd8f24e7451",
      "number": 11,
      "content": "và vui hưởng cảnh an lạc chan hoà.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.638Z",
      "updatedAt": "2024-09-10T04:24:33.638Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "90ea1f9d-0537-4cb6-bb0f-6ee8044e1c2f",
      "number": 12,
      "content": "Da-in",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.640Z",
      "updatedAt": "2024-09-10T04:24:33.640Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "129d8be8-df37-4cf0-bbae-7bfb3ac323b4",
      "number": 12,
      "content": "Kẻ gian ác mưu hại người công chính, và hậm hực nghiến răng.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.642Z",
      "updatedAt": "2024-09-10T04:24:33.642Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "16abe5a2-68d1-41ac-8fca-4da4e1ba7083",
      "number": 13,
      "content": "Nhưng Chúa cười nhạo nó, vì thấy nó đã đến ngày tàn.",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.645Z",
      "updatedAt": "2024-09-10T04:24:33.645Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "3da015ba-311f-4997-8154-11cdb23fdc53",
      "number": 14,
      "content": "Khết",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.647Z",
      "updatedAt": "2024-09-10T04:24:33.647Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "459d604f-9fd3-42c8-8d69-c0b3b6c7ade1",
      "number": 14,
      "content": "Phường ác nhân tuốt gươm giương nỏ",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.649Z",
      "updatedAt": "2024-09-10T04:24:33.649Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "a840bd85-027e-48a1-9d1b-cbb0a95aebdd",
      "number": 14,
      "content": "nhằm hạ kẻ nghèo hèn, và giết người chính trực ;",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.651Z",
      "updatedAt": "2024-09-10T04:24:33.651Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "905636bd-47f6-4181-b8f3-c2859c7869b6",
      "number": 15,
      "content": "nhưng gươm chúng lại đâm vào tim chúng, và nỏ gẫy tan tành.",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.653Z",
      "updatedAt": "2024-09-10T04:24:33.653Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "cbb9a9ed-6069-4a21-b748-1f52025a218b",
      "number": 16,
      "content": "Tết",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.657Z",
      "updatedAt": "2024-09-10T04:24:33.657Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "31a8bb65-08d1-492a-8162-b97f7cfca46d",
      "number": 16,
      "content": "Ít tiền ít của mà là người công chính",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.659Z",
      "updatedAt": "2024-09-10T04:24:33.659Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "35c2d96c-f8be-4109-b0b6-35c0cadc276e",
      "number": 16,
      "content": "hơn nhiều vàng nhiều bạc mà là kẻ ác nhân.",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.663Z",
      "updatedAt": "2024-09-10T04:24:33.663Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "ccfab4e1-de13-4df9-8cca-38ef2b44c5a2",
      "number": 17,
      "content": "Vì cánh tay bọn ác nhân sẽ bị bẻ gẫy,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.665Z",
      "updatedAt": "2024-09-10T04:24:33.665Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "337d3531-9ac0-401a-a41e-db3f8b9d2277",
      "number": 17,
      "content": "còn người công chính được CHÚA độ trì.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.668Z",
      "updatedAt": "2024-09-10T04:24:33.668Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "c675d91b-a5be-423c-a3ce-a8d757e28637",
      "number": 18,
      "content": "Giốt",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.669Z",
      "updatedAt": "2024-09-10T04:24:33.669Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "01cb2284-ed9e-477a-86da-ab2293a249d0",
      "number": 18,
      "content": "CHÚA chăm sóc cuộc đời người thiện hảo,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.672Z",
      "updatedAt": "2024-09-10T04:24:33.672Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "00e3861e-5f04-4af2-aa2b-afb22948c912",
      "number": 18,
      "content": "gia nghiệp họ tồn tại đến muôn năm.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.674Z",
      "updatedAt": "2024-09-10T04:24:33.674Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "5ab41a1c-d840-46ef-9226-e2aac511be87",
      "number": 19,
      "content": "Buổi gian nan họ không hề hổ thẹn,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.676Z",
      "updatedAt": "2024-09-10T04:24:33.676Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "f5ee4d62-8119-4596-b48a-52d87b495ee1",
      "number": 19,
      "content": "ngày đói kém lại được no đầy.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.678Z",
      "updatedAt": "2024-09-10T04:24:33.678Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "c0c8b4dc-9bbd-4be4-b362-5addb96c4eb3",
      "number": 20,
      "content": "Cáp",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.680Z",
      "updatedAt": "2024-09-10T04:24:33.680Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "ffbe0a95-9557-4714-810b-c2cb81287442",
      "number": 20,
      "content": "Còn quân gian ác sẽ phải tiêu vong ;",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.682Z",
      "updatedAt": "2024-09-10T04:24:33.682Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "866fbd3d-15b9-426a-ab4b-52f9db7b101d",
      "number": 20,
      "content": "những kẻ thù của CHÚA",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.684Z",
      "updatedAt": "2024-09-10T04:24:33.684Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "9e76d6bb-0d57-4b50-919e-f68287b3bd82",
      "number": 20,
      "content": "sẽ tan đi như cỏ nội hoa đồng, tan đi thành mây khói.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.686Z",
      "updatedAt": "2024-09-10T04:24:33.686Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "89629ac6-948c-4434-8403-28f4687b387d",
      "number": 21,
      "content": "La-mét",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.692Z",
      "updatedAt": "2024-09-10T04:24:33.692Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "62001c2d-dbb7-4f62-89a2-ac76d889dfa8",
      "number": 21,
      "content": "Kẻ gian ác vay mà không trả,",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.697Z",
      "updatedAt": "2024-09-10T04:24:33.697Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "b021c7ae-319a-4785-b324-6ccf1a8a0792",
      "number": 21,
      "content": "người công chính thông cảm và cho không.",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.700Z",
      "updatedAt": "2024-09-10T04:24:33.700Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "c6a5cb83-c936-4444-ba50-ae11d7ccfb7a",
      "number": 22,
      "content": "Người Chúa chúc lành được đất hứa làm gia nghiệp,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.702Z",
      "updatedAt": "2024-09-10T04:24:33.702Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "5735aa85-60fc-4246-81f5-08f8af779803",
      "number": 22,
      "content": "kẻ Chúa nguyền rủa sẽ bị diệt trừ.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.704Z",
      "updatedAt": "2024-09-10T04:24:33.704Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "01bac7b6-2e5e-4231-8bd9-bd7bc39f2cd8",
      "number": 23,
      "content": "Mêm",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.707Z",
      "updatedAt": "2024-09-10T04:24:33.707Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "a0b37bd0-33c8-41ca-9e89-efe5dd25adc6",
      "number": 23,
      "content": "CHÚA giúp con người bước đi vững chãi,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.710Z",
      "updatedAt": "2024-09-10T04:24:33.710Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "4221518f-5c0f-499b-ae68-e875fc033092",
      "number": 23,
      "content": "ưa chuộng đường lối họ dõi theo.",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.715Z",
      "updatedAt": "2024-09-10T04:24:33.715Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "a1896e65-b752-4b3b-89cc-e9b4dbe1266c",
      "number": 24,
      "content": "Dầu họ có vấp cũng không ngã gục,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.718Z",
      "updatedAt": "2024-09-10T04:24:33.718Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "feed9fab-5c18-41f0-ab2d-842108f4346a",
      "number": 24,
      "content": "bởi vì đã có CHÚA cầm tay.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.721Z",
      "updatedAt": "2024-09-10T04:24:33.721Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "b1c3b010-3168-4db8-bd17-9c332bff4a59",
      "number": 25,
      "content": "Nun",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.724Z",
      "updatedAt": "2024-09-10T04:24:33.724Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "c3edc680-d80e-4552-aa23-0ed7f3ba89bb",
      "number": 25,
      "content": "Từ nhỏ dại tới nay tôi già cả,",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.727Z",
      "updatedAt": "2024-09-10T04:24:33.727Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "ad38f113-af5e-49a4-9240-5e14094a88b3",
      "number": 25,
      "content": "chưa thấy người công chính bị bỏ rơi,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.731Z",
      "updatedAt": "2024-09-10T04:24:33.731Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "c11f797c-ee4c-46d4-9140-35594cc37568",
      "number": 25,
      "content": "hoặc dòng giống phải ăn mày thiên hạ.",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.735Z",
      "updatedAt": "2024-09-10T04:24:33.735Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "0c6e0958-3955-4be2-85b9-28c5d2d51e68",
      "number": 26,
      "content": "Ngày ngày họ thông cảm và cho mượn cho vay,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.739Z",
      "updatedAt": "2024-09-10T04:24:33.739Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "c6530d97-1219-426f-9e4f-32e43ae18381",
      "number": 26,
      "content": "dòng giống mai sau hưởng phúc lành.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.743Z",
      "updatedAt": "2024-09-10T04:24:33.743Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "16ccd9ce-7d1c-404a-9c58-d8ade7a6f36e",
      "number": 27,
      "content": "Hãy làm lành lánh dữ, bạn sẽ được một nơi ở muôn đời.",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.747Z",
      "updatedAt": "2024-09-10T04:24:33.747Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "2207f56a-ab50-4daa-b8b3-d8e358e8ee6d",
      "number": 28,
      "content": "Bởi vì CHÚA yêu thích điều chính trực,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.751Z",
      "updatedAt": "2024-09-10T04:24:33.751Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "ca48cbfb-6add-4f99-96e2-174743bc2c0e",
      "number": 28,
      "content": "chẳng bỏ rơi những bậc hiếu trung.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.754Z",
      "updatedAt": "2024-09-10T04:24:33.754Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "bd2769e8-1ec3-4376-bb5e-36aab35707a8",
      "number": 28,
      "content": "A-in",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.758Z",
      "updatedAt": "2024-09-10T04:24:33.758Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "078747ca-018f-4f23-b4a3-9155732ba5d5",
      "number": 28,
      "content": "Quân bất chính sẽ hoàn toàn bị tiêu diệt,",
      "order": 3,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.761Z",
      "updatedAt": "2024-09-10T04:24:33.761Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "b8f80147-9044-4753-971a-d8ca707e39e4",
      "number": 28,
      "content": "dòng giống ác nhân rồi cũng phải tru di.",
      "order": 4,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.764Z",
      "updatedAt": "2024-09-10T04:24:33.764Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "b18b79eb-170a-45fb-9824-83f6e9031eba",
      "number": 29,
      "content": "Còn người công chính được đất hứa làm gia nghiệp,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.767Z",
      "updatedAt": "2024-09-10T04:24:33.767Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "ec542f07-0e37-4eee-a273-574e9e296345",
      "number": 29,
      "content": "và định cư tại đó mãi muôn đời.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.770Z",
      "updatedAt": "2024-09-10T04:24:33.770Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "5ff5d22a-bc88-4e53-a96e-5285464f0f17",
      "number": 30,
      "content": "Pê",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.773Z",
      "updatedAt": "2024-09-10T04:24:33.773Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "9b4da957-f331-4f2f-aede-458e267d92f5",
      "number": 30,
      "content": "Miệng người công chính niệm lẽ khôn ngoan,",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.775Z",
      "updatedAt": "2024-09-10T04:24:33.775Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "13f4e3f0-7b51-4070-bd0b-63c5bb8f49ce",
      "number": 30,
      "content": "và lưỡi họ nói lên điều chính trực.",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.778Z",
      "updatedAt": "2024-09-10T04:24:33.778Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "5c336318-7181-4567-8c12-a69461a3df76",
      "number": 31,
      "content": "Luật Thiên Chúa, họ ghi tạc vào lòng,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.781Z",
      "updatedAt": "2024-09-10T04:24:33.781Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "f07e5565-6e19-491c-9155-5de09a4ab15a",
      "number": 31,
      "content": "bước chân đi không hề lảo đảo.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.786Z",
      "updatedAt": "2024-09-10T04:24:33.786Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "46765105-f606-4f00-8eb5-d3f24fcc10a1",
      "number": 32,
      "content": "Xa-đê",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.789Z",
      "updatedAt": "2024-09-10T04:24:33.789Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "048dbd84-1e0e-491d-9245-4dfb13f14f12",
      "number": 32,
      "content": "Kẻ gian ác rình mò, mưu giết người công chính.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.793Z",
      "updatedAt": "2024-09-10T04:24:33.793Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "dae8a560-cde4-4505-887d-66c5606bfce9",
      "number": 33,
      "content": "Nhưng CHÚA không để họ lọt vào tay ác nhân,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.795Z",
      "updatedAt": "2024-09-10T04:24:33.795Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "81f74902-0653-4f65-9356-659e6591498f",
      "number": 33,
      "content": "không để bị kết án khi phải ra trước toà.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.798Z",
      "updatedAt": "2024-09-10T04:24:33.798Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "7f6e85f7-e21f-46c3-bda5-4bd25ec01c6c",
      "number": 34,
      "content": "Cốp",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.800Z",
      "updatedAt": "2024-09-10T04:24:33.800Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "abac3e05-bb8e-4821-9e1b-7dd9f659e916",
      "number": 34,
      "content": "Hãy trông chờ CHÚA, giữ vững đường lối Người,",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.804Z",
      "updatedAt": "2024-09-10T04:24:33.804Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "eead803d-21ba-453c-8f6f-f47c5d26494a",
      "number": 34,
      "content": "rồi Người sẽ cất nhắc bạn lên",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.807Z",
      "updatedAt": "2024-09-10T04:24:33.807Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "9fbbd19e-9c51-4e85-addb-ef7b594f6d21",
      "number": 34,
      "content": "và ban đất hứa làm gia nghiệp",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.809Z",
      "updatedAt": "2024-09-10T04:24:33.809Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "3166daff-a928-47d3-b6d7-530b6ff1c48e",
      "number": 34,
      "content": "bạn sẽ nhìn thấy ác nhân bị diệt trừ.",
      "order": 4,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.811Z",
      "updatedAt": "2024-09-10T04:24:33.811Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "09ca9187-a906-4c40-be46-6fb9f36cda63",
      "number": 35,
      "content": "Rết",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.814Z",
      "updatedAt": "2024-09-10T04:24:33.814Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "3e99dc6f-d573-42f6-a33d-556774f5f9cc",
      "number": 35,
      "content": "Tôi đã thấy ác nhân ngạo ngược,",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.816Z",
      "updatedAt": "2024-09-10T04:24:33.816Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "ae2993f4-599c-42cf-91c4-17dff8dc0151",
      "number": 35,
      "content": "nó vươn mình tựa hương bá xanh tươi.",
      "order": 2,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.820Z",
      "updatedAt": "2024-09-10T04:24:33.820Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "c5f945c1-3fd6-4050-8211-19262c003647",
      "number": 36,
      "content": "Nay trở lại, nó không còn nữa,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.823Z",
      "updatedAt": "2024-09-10T04:24:33.823Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "a970d89d-d9bc-4c9c-a528-237793861b06",
      "number": 36,
      "content": "tìm kiếm hoài mà chẳng thấy tăm hơi.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.825Z",
      "updatedAt": "2024-09-10T04:24:33.825Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "bc5735ea-b868-428e-a433-295f1c3b0f6a",
      "number": 37,
      "content": "Sin",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.828Z",
      "updatedAt": "2024-09-10T04:24:33.828Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "1a252b9a-ffd5-4457-96a8-1d6ec12c623a",
      "number": 37,
      "content": "Hãy xem kẻ thiện toàn, cứ nhìn người chính trực,",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.830Z",
      "updatedAt": "2024-09-10T04:24:33.830Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "a38d08a6-fbe8-4716-a601-a9ed4998d914",
      "number": 37,
      "content": "ai hiếu hoà sẽ được con dòng cháu giống.",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.834Z",
      "updatedAt": "2024-09-10T04:24:33.834Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "156775b9-d354-44ea-b11e-345583bf9790",
      "number": 38,
      "content": "Quân tội lỗi đều bị diệt trừ,",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.836Z",
      "updatedAt": "2024-09-10T04:24:33.836Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "3dc818f0-5d4e-42a0-954b-55addc38c112",
      "number": 38,
      "content": "nòi ác nhân rồi cũng phải tru di.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.839Z",
      "updatedAt": "2024-09-10T04:24:33.839Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "e607685f-1beb-40b7-908d-689ca44a2461",
      "number": 39,
      "content": "Tau",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-10T04:24:33.842Z",
      "updatedAt": "2024-09-10T04:24:33.842Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "037ab7f2-4531-4252-b1ee-f23d0ebef149",
      "number": 39,
      "content": "Còn chính nhân được CHÚA thương cứu độ",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.845Z",
      "updatedAt": "2024-09-10T04:24:33.845Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "6eeb2652-3f2e-42bd-b887-442efdb976d1",
      "number": 39,
      "content": "và bảo vệ chở che trong buổi ngặt nghèo.",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.848Z",
      "updatedAt": "2024-09-10T04:24:33.848Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "c950b8b0-e5f4-45c2-bd3b-688161c5e9dc",
      "number": 40,
      "content": "CHÚA phù trợ và Người giải thoát,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.851Z",
      "updatedAt": "2024-09-10T04:24:33.851Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "ad1a3d39-f67b-4951-b2ad-99d1e07f63f4",
      "number": 40,
      "content": "giải thoát khỏi ác nhân và thương cứu độ,",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.854Z",
      "updatedAt": "2024-09-10T04:24:33.854Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "e155864a-9cc0-4e78-a574-9939c939a937",
      "number": 40,
      "content": "bởi vì họ ẩn náu bên Người.",
      "order": 2,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-10T04:24:33.856Z",
      "updatedAt": "2024-09-10T04:24:33.856Z",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    }
  ],
  "footnotes": [
    {
      "id": "a05621c7-da38-4cf3-a3cc-16c6f413a1ae",
      "content": "Thánh vịnh này là <i>một Thánh vịnh giáo huấn theo mẫu tự</i>. Tác giả phân biệt ở đây hai nhóm : kẻ dữ và người lành. Mặc dầu có sự phân biệt rõ ràng giữa hai nhóm, khó mà tìm ra thứ tự các ý tưởng được triển khai trong Tv này. Tuy nhiên, độc giả có thể bắt được ý tưởng chính sau đây : đứng trước cảnh ác nhân hoành hành, tác giả khuyên người lành cứ bình tĩnh và an tâm, bởi vì họ sẽ được <i>đất hứa làm gia nghiệp</i> (cc. 9.11a.22a.29a.34c), vì ở đó họ sẽ sống yên hàn (c.3), được vui hưởng cảnh an lạc chan hoà (c.11b), được no đầy trong ngày đói kém (c.19b), vì có Thiên Chúa can thiệp. Quả thật, Thiên Chúa phá tan mưu đồ của ác nhân (c.15), không hề bỏ rơi ai bao giờ (c.28) ; Người sẽ che chở phù trì (cc. 5b.17b.23-24.33.39-40), giải thoát và cứu độ (cc. 39-40) ; Người sẽ bảo vệ sự sống và dòng dõi của người lành (c.37), sẽ cho họ được phỉ chí toại lòng (c.4).",
      "position": 35,
      "order": 0,
      "createdAt": "2024-09-10T04:24:33.539Z",
      "updatedAt": "2024-09-10T04:24:33.539Z",
      "headingId": "dacb94df-c938-4035-bdd6-1c843b7b3222",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "3ca5015b-a3f1-4820-b4a1-79257a0bf8f7",
      "content": "<i>Đất nước</i> ở đây là xứ Pa-lét-tin (x. cc. 9.11a.22a.29a.34c). <i>Sống yên hàn</i> ở đó và được hưởng dùng của cải và tài nguyên phong phú : đó là một trong những ý tưởng chính của Tv này.",
      "position": 28,
      "order": 1,
      "createdAt": "2024-09-10T04:24:33.570Z",
      "updatedAt": "2024-09-10T04:24:33.570Z",
      "verseId": "38bb2d27-ba66-4323-902a-a9ab49c3abef",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    },
    {
      "id": "a5b8c885-9dcd-4762-85e8-8500de31f636",
      "content": "Ác nhân gây hại cho bản thân bằng chính phương tiện họ muốn dùng để hại người khác (<i>gậy ông lại đập lưng ông</i>). X. Tv 7,16-17 ; 9,16 ; 57,7 ; cũng x. Cn 5,22 ; 14,32 ; 26,27 ; G 4,8 ; Gv 10,8 ; Hc 27,25 ; Kn 11,16 ; 12,23 ; 18,4 ; Hs 8,7 ; 10,13 ...",
      "position": 38,
      "order": 2,
      "createdAt": "2024-09-10T04:24:33.655Z",
      "updatedAt": "2024-09-10T04:24:33.655Z",
      "verseId": "905636bd-47f6-4181-b8f3-c2859c7869b6",
      "chapterId": "a0d1a633-3212-4cd5-a34c-8bb8db263ada"
    }
  ],
