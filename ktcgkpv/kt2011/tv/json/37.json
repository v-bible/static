{
  "chapter": {
    "id": "65410336-ba11-49c3-b623-a09cb93a1d17",
    "number": 37,
    "ref": "",
    "createdAt": "2024-09-23T03:08:02.366Z",
    "updatedAt": "2024-09-23T03:08:02.366Z",
    "bookId": "0fb0b0bf-be48-4502-bb85-e165626fcd15"
  },
  "verses": [
    {
      "id": "503e1cec-7d2c-4186-916c-3c64fcd49db6",
      "number": 1,
      "content": "*Của vua [Đa-vít]().*",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:01.913Z",
      "updatedAt": "2024-09-23T04:13:01.913Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "ee84f2e4-3597-4ec2-865d-25fa1d125edb",
      "number": 1,
      "content": "A-lép",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:01.927Z",
      "updatedAt": "2024-09-23T04:13:01.927Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "d3345021-b587-4e9a-be41-daaa84b87db5",
      "number": 1,
      "content": "Bạn đừng nổi giận vì quân gian ác,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.929Z",
      "updatedAt": "2024-09-23T04:13:01.929Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "e7fc3c02-3059-4649-ad9e-13581ec9fa4e",
      "number": 1,
      "content": "chớ phân bì với kẻ bất lương,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.931Z",
      "updatedAt": "2024-09-23T04:13:01.931Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "13677483-3c4b-46e4-a6bc-0442c560e8a9",
      "number": 2,
      "content": "vì chúng tựa cỏ hoang mau úa",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.933Z",
      "updatedAt": "2024-09-23T04:13:01.933Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "90fbfb28-1e2f-4b5f-98d9-d195675d8b62",
      "number": 2,
      "content": "và giống như thanh thảo chóng tàn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.937Z",
      "updatedAt": "2024-09-23T04:13:01.937Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "05368d51-4d45-4047-a7a0-31276efef5f6",
      "number": 3,
      "content": "Bết",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:01.940Z",
      "updatedAt": "2024-09-23T04:13:01.940Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "575383d7-3926-439d-9ed1-ea9baa444be7",
      "number": 3,
      "content": "Cứ tin tưởng vào CHÚA và làm điều thiện,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.943Z",
      "updatedAt": "2024-09-23T04:13:01.943Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "3b7fe855-7308-46b1-b2f9-13b9efa5671d",
      "number": 3,
      "content": "thì sẽ được ở trong đất nước và sống yên hàn.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.947Z",
      "updatedAt": "2024-09-23T04:13:01.947Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "cc93b4ea-00c8-48e5-b505-527463c4e667",
      "number": 4,
      "content": "Hãy lấy CHÚA làm niềm vui của bạn,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.955Z",
      "updatedAt": "2024-09-23T04:13:01.955Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "d5bed02e-13fb-4237-8635-46eda73f4f67",
      "number": 4,
      "content": "Người sẽ cho được phỉ chí toại lòng.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.960Z",
      "updatedAt": "2024-09-23T04:13:01.960Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "9bd4a96f-5e19-4038-9e75-290e38d93bd5",
      "number": 5,
      "content": "Ghi-men",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:01.964Z",
      "updatedAt": "2024-09-23T04:13:01.964Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "486fbe6d-03a8-4b09-b07b-67da8a1830e7",
      "number": 5,
      "content": "Hãy ký thác đường đời cho CHÚA,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.968Z",
      "updatedAt": "2024-09-23T04:13:01.968Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "2f32abe7-b3d4-445a-ab14-54e2851f80c5",
      "number": 5,
      "content": "tin tưởng vào Người, Người sẽ ra tay.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.973Z",
      "updatedAt": "2024-09-23T04:13:01.973Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "7ab352db-1db9-4f87-bf1a-c4acdbe2352e",
      "number": 6,
      "content": "Chính nghĩa bạn, Chúa sẽ làm rực rỡ tựa bình minh,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.977Z",
      "updatedAt": "2024-09-23T04:13:01.977Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "1c20100d-3683-4853-90d8-f820368f63e2",
      "number": 6,
      "content": "công lý bạn, Người sẽ cho huy hoàng như chính ngọ.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.981Z",
      "updatedAt": "2024-09-23T04:13:01.981Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "d5215127-74c1-4b49-857b-2636fd2dfc22",
      "number": 7,
      "content": "Đa-lét",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:01.985Z",
      "updatedAt": "2024-09-23T04:13:01.985Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "83c518bf-0148-4b6a-a9e1-a095489866b8",
      "number": 7,
      "content": "Hãy lặng thinh trước mặt CHÚA và đợi trông Người.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.990Z",
      "updatedAt": "2024-09-23T04:13:01.990Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "bf3324c2-e71b-4b40-89dd-45ac7126881a",
      "number": 7,
      "content": "Bạn chẳng nên nổi giận",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:01.998Z",
      "updatedAt": "2024-09-23T04:13:01.998Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "fa322081-965e-4edf-8e8e-b68e4449ea92",
      "number": 7,
      "content": "với kẻ được thành công hay với người xảo trá.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.002Z",
      "updatedAt": "2024-09-23T04:13:02.002Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "c6b1f404-7c63-476e-bc81-1c1a3c5ddb8a",
      "number": 8,
      "content": "Hê",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.007Z",
      "updatedAt": "2024-09-23T04:13:02.007Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "da2f53de-add0-4f4e-b16f-db8326a161c5",
      "number": 8,
      "content": "Dừng cơn phẫn nộ và chớ mãi nổi xung,",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.011Z",
      "updatedAt": "2024-09-23T04:13:02.011Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "b467fd27-2f88-4f0f-95d4-ba1f33ed9b77",
      "number": 8,
      "content": "đừng nổi giận kẻo sinh ra tội lỗi,",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.015Z",
      "updatedAt": "2024-09-23T04:13:02.015Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "7479225e-861a-45ef-933c-e892ee93c57e",
      "number": 9,
      "content": "vì bọn gian ác sẽ bị diệt trừ,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.019Z",
      "updatedAt": "2024-09-23T04:13:02.019Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "d9f39d24-ba94-4c99-a59c-2323917fe1b8",
      "number": 9,
      "content": "còn người trông đợi CHÚA, sẽ được đất hứa làm gia nghiệp.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.023Z",
      "updatedAt": "2024-09-23T04:13:02.023Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "26543429-626b-4d41-a2f7-265e2f3cd146",
      "number": 10,
      "content": "Vau",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.027Z",
      "updatedAt": "2024-09-23T04:13:02.027Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "d2b2488b-2f0f-4d6e-bc07-ecd2b267e2da",
      "number": 10,
      "content": "Ít lâu nữa ác nhân sẽ chẳng còn,",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.031Z",
      "updatedAt": "2024-09-23T04:13:02.031Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "71c543b1-1072-4a8a-ad31-6b5d4f186b4e",
      "number": 10,
      "content": "đến chỗ xưa cũng không tìm thấy hắn.",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.035Z",
      "updatedAt": "2024-09-23T04:13:02.035Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "dea44a34-ea5e-4039-aac4-ff85d4690e25",
      "number": 11,
      "content": "Còn kẻ nghèo hèn được đất hứa làm gia nghiệp",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.040Z",
      "updatedAt": "2024-09-23T04:13:02.040Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "0c02e478-2444-47e6-b1fc-c3a175a5908d",
      "number": 11,
      "content": "và vui hưởng cảnh an lạc chan hoà.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.048Z",
      "updatedAt": "2024-09-23T04:13:02.048Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "b144bd88-1cb9-40d6-b593-f263d6e55a1a",
      "number": 12,
      "content": "Da-in",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.052Z",
      "updatedAt": "2024-09-23T04:13:02.052Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "18734c00-0c3f-41d1-81cb-ce706b70886a",
      "number": 12,
      "content": "Kẻ gian ác mưu hại người công chính, và hậm hực nghiến răng.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.056Z",
      "updatedAt": "2024-09-23T04:13:02.056Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "a912580d-23fe-48c9-a555-91a6f9a4e98e",
      "number": 13,
      "content": "Nhưng Chúa cười nhạo nó, vì thấy nó đã đến ngày tàn.",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.060Z",
      "updatedAt": "2024-09-23T04:13:02.060Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "1df1bb53-7a19-48fa-b1ee-f08125270ff1",
      "number": 14,
      "content": "Khết",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.065Z",
      "updatedAt": "2024-09-23T04:13:02.065Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "3f90665d-31f7-4d0a-92bb-308f25278abb",
      "number": 14,
      "content": "Phường ác nhân tuốt gươm giương nỏ",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.069Z",
      "updatedAt": "2024-09-23T04:13:02.069Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "5797d0e1-c3ad-4420-b95d-bc77ee7dd67c",
      "number": 14,
      "content": "nhằm hạ kẻ nghèo hèn, và giết người chính trực ;",
      "order": 2,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.073Z",
      "updatedAt": "2024-09-23T04:13:02.073Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "9148c035-2245-4926-8d94-da5c04441f22",
      "number": 15,
      "content": "nhưng gươm chúng lại đâm vào tim chúng, và nỏ gẫy tan tành.",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.077Z",
      "updatedAt": "2024-09-23T04:13:02.077Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "417f96ce-3ad0-47ed-9cdb-21c995fdd8db",
      "number": 16,
      "content": "Tết",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.085Z",
      "updatedAt": "2024-09-23T04:13:02.085Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "9b46fdcd-f8a2-4850-a5ac-8f4776c5421a",
      "number": 16,
      "content": "Ít tiền ít của mà là người công chính",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.090Z",
      "updatedAt": "2024-09-23T04:13:02.090Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "262e0ebb-7bdf-47bd-9285-c1e12e5b048e",
      "number": 16,
      "content": "hơn nhiều vàng nhiều bạc mà là kẻ ác nhân.",
      "order": 2,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.098Z",
      "updatedAt": "2024-09-23T04:13:02.098Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "0ffe3500-a247-414f-b0f8-b61a7bb58cb0",
      "number": 17,
      "content": "Vì cánh tay bọn ác nhân sẽ bị bẻ gẫy,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.102Z",
      "updatedAt": "2024-09-23T04:13:02.102Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "3dc0a670-7b2e-4275-b06c-c27fe4a6a16c",
      "number": 17,
      "content": "còn người công chính được CHÚA độ trì.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.106Z",
      "updatedAt": "2024-09-23T04:13:02.106Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "7cdd5e5e-2bf2-4609-900a-beb791019b64",
      "number": 18,
      "content": "Giốt",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.111Z",
      "updatedAt": "2024-09-23T04:13:02.111Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "584768a0-c227-4703-aa44-70f5b6e90261",
      "number": 18,
      "content": "CHÚA chăm sóc cuộc đời người thiện hảo,",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.115Z",
      "updatedAt": "2024-09-23T04:13:02.115Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "f3d78b45-f23e-4210-ae01-bd6fbd71e464",
      "number": 18,
      "content": "gia nghiệp họ tồn tại đến muôn năm.",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.119Z",
      "updatedAt": "2024-09-23T04:13:02.119Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "e46c7ab1-fb40-4e03-a732-61929c78aa7e",
      "number": 19,
      "content": "Buổi gian nan họ không hề hổ thẹn,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.124Z",
      "updatedAt": "2024-09-23T04:13:02.124Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "57cfa048-6b68-401a-84b3-ae0c5dadb962",
      "number": 19,
      "content": "ngày đói kém lại được no đầy.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.128Z",
      "updatedAt": "2024-09-23T04:13:02.128Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "ab86de5e-b5c9-4965-be00-5ef9a7bdbcf3",
      "number": 20,
      "content": "Cáp",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.132Z",
      "updatedAt": "2024-09-23T04:13:02.132Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "826cbd59-4f4d-4432-a197-542f587360b8",
      "number": 20,
      "content": "Còn quân gian ác sẽ phải tiêu vong ;",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.136Z",
      "updatedAt": "2024-09-23T04:13:02.136Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "82cc2bca-736d-47c4-b7e7-7c7aedf8da59",
      "number": 20,
      "content": "những kẻ thù của CHÚA",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.140Z",
      "updatedAt": "2024-09-23T04:13:02.140Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "cc354e23-4b29-480e-bfe7-50dccab733c9",
      "number": 20,
      "content": "sẽ tan đi như cỏ nội hoa đồng, tan đi thành mây khói.",
      "order": 3,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.144Z",
      "updatedAt": "2024-09-23T04:13:02.144Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "63d91f25-9ab6-4f95-97fb-6548d0d47871",
      "number": 21,
      "content": "La-mét",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.148Z",
      "updatedAt": "2024-09-23T04:13:02.148Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "8b9f65fc-0627-4411-a272-e438c434715c",
      "number": 21,
      "content": "Kẻ gian ác vay mà không trả,",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.152Z",
      "updatedAt": "2024-09-23T04:13:02.152Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "fdd2aae9-efd5-4a3b-ae32-a1e457eae7fc",
      "number": 21,
      "content": "người công chính thông cảm và cho không.",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.156Z",
      "updatedAt": "2024-09-23T04:13:02.156Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "30e5cc2d-08f8-4105-b798-fa7406e72d0b",
      "number": 22,
      "content": "Người Chúa chúc lành được đất hứa làm gia nghiệp,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.160Z",
      "updatedAt": "2024-09-23T04:13:02.160Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "785f15ed-35a2-4c24-9233-c926bca12376",
      "number": 22,
      "content": "kẻ Chúa nguyền rủa sẽ bị diệt trừ.",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.165Z",
      "updatedAt": "2024-09-23T04:13:02.165Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "8a3a695f-051d-4a47-836a-648ce1f3810e",
      "number": 23,
      "content": "Mêm",
      "order": 0,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.169Z",
      "updatedAt": "2024-09-23T04:13:02.169Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "1935804b-c731-4ea2-a1fc-5ea51124d82a",
      "number": 23,
      "content": "CHÚA giúp con người bước đi vững chãi,",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.173Z",
      "updatedAt": "2024-09-23T04:13:02.173Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "5a9387f6-0007-4cca-be29-db298ac51570",
      "number": 23,
      "content": "ưa chuộng đường lối họ dõi theo.",
      "order": 2,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.182Z",
      "updatedAt": "2024-09-23T04:13:02.182Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "ff7ff233-62df-495a-b906-39afb900bccf",
      "number": 24,
      "content": "Dầu họ có vấp cũng không ngã gục,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.186Z",
      "updatedAt": "2024-09-23T04:13:02.186Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "4feb9025-4bf2-4978-8604-6a9570973cb3",
      "number": 24,
      "content": "bởi vì đã có CHÚA cầm tay.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.190Z",
      "updatedAt": "2024-09-23T04:13:02.190Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "b3e27286-ff2e-4dde-82dd-cc02ddee4380",
      "number": 25,
      "content": "[Nun]()",
      "order": 0,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.194Z",
      "updatedAt": "2024-09-23T04:13:02.194Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "0e8742d9-70b9-484c-a4e4-b531b7671b9c",
      "number": 25,
      "content": "Từ nhỏ dại tới nay tôi già cả,",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.202Z",
      "updatedAt": "2024-09-23T04:13:02.202Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "cb018014-ce43-427f-8fd5-49663eb48806",
      "number": 25,
      "content": "chưa thấy người công chính bị bỏ rơi,",
      "order": 2,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.209Z",
      "updatedAt": "2024-09-23T04:13:02.209Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "5bbd4e03-27cb-4421-a0e8-3cd0f56ca447",
      "number": 25,
      "content": "hoặc dòng giống phải ăn mày thiên hạ.",
      "order": 3,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.216Z",
      "updatedAt": "2024-09-23T04:13:02.216Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "7730ad3b-93f6-4b72-b599-9e30e2d79b99",
      "number": 26,
      "content": "Ngày ngày họ thông cảm và cho mượn cho vay,",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.224Z",
      "updatedAt": "2024-09-23T04:13:02.224Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "8c9a6976-d517-4b37-ad98-1089f880976a",
      "number": 26,
      "content": "dòng giống mai sau hưởng phúc lành.",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.228Z",
      "updatedAt": "2024-09-23T04:13:02.228Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "31934895-aa58-4ecf-9599-6b4d7d5f53ca",
      "number": 27,
      "content": "Hãy làm lành lánh dữ, bạn sẽ được một nơi ở muôn đời.",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.232Z",
      "updatedAt": "2024-09-23T04:13:02.232Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "1222e6e8-51b6-4e0b-bf8c-96b30e7c2c89",
      "number": 28,
      "content": "Bởi vì CHÚA yêu thích điều chính trực,",
      "order": 0,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.236Z",
      "updatedAt": "2024-09-23T04:13:02.236Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "7b520e6b-404c-463a-81c6-54d679606f5d",
      "number": 28,
      "content": "chẳng bỏ rơi những bậc hiếu trung.",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.241Z",
      "updatedAt": "2024-09-23T04:13:02.241Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "eca0e85b-829e-4e08-8561-b34b1468e62b",
      "number": 28,
      "content": "A-in",
      "order": 2,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.245Z",
      "updatedAt": "2024-09-23T04:13:02.245Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "096a7942-08f0-4869-a735-2aa77da6fd9f",
      "number": 28,
      "content": "Quân bất chính sẽ hoàn toàn bị tiêu diệt,",
      "order": 3,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.249Z",
      "updatedAt": "2024-09-23T04:13:02.249Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "315c9a63-99fe-4efa-ab11-e6474cf3251b",
      "number": 28,
      "content": "dòng giống ác nhân rồi cũng phải tru di.",
      "order": 4,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.253Z",
      "updatedAt": "2024-09-23T04:13:02.253Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "b21af074-201f-4e17-93b3-50cf251f7835",
      "number": 29,
      "content": "Còn người công chính được đất hứa làm gia nghiệp,",
      "order": 0,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.258Z",
      "updatedAt": "2024-09-23T04:13:02.258Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "4b11ad6a-8b42-4d46-9404-6a24cffd5d37",
      "number": 29,
      "content": "và định cư tại đó mãi muôn đời.",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.262Z",
      "updatedAt": "2024-09-23T04:13:02.262Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "0dfb0a34-8832-4efe-8e2b-9fd40b01e08a",
      "number": 30,
      "content": "Pê",
      "order": 0,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.267Z",
      "updatedAt": "2024-09-23T04:13:02.267Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "06dba9a9-e909-46ac-a6cf-3c7df61f5f08",
      "number": 30,
      "content": "Miệng người công chính niệm lẽ khôn ngoan,",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.271Z",
      "updatedAt": "2024-09-23T04:13:02.271Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "f0786084-1cc7-4988-804e-18f9d36edea1",
      "number": 30,
      "content": "và lưỡi họ nói lên điều chính trực.",
      "order": 2,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.275Z",
      "updatedAt": "2024-09-23T04:13:02.275Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "27f8d70b-e8a1-49e3-a704-a5f2aa181fd4",
      "number": 31,
      "content": "Luật Thiên Chúa, họ ghi tạc vào lòng,",
      "order": 0,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.279Z",
      "updatedAt": "2024-09-23T04:13:02.279Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "03e9167e-e9c6-4220-b08a-29a5b94d1b3e",
      "number": 31,
      "content": "bước chân đi không hề lảo đảo.",
      "order": 1,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.287Z",
      "updatedAt": "2024-09-23T04:13:02.287Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "21f6730b-bf34-47cb-a0dd-c9929ab5de1a",
      "number": 32,
      "content": "Xa-đê",
      "order": 0,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.291Z",
      "updatedAt": "2024-09-23T04:13:02.291Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "c633cd17-daa0-4bc4-8864-44d4f0ad7ca9",
      "number": 32,
      "content": "Kẻ gian ác rình mò, mưu giết người công chính.",
      "order": 1,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.296Z",
      "updatedAt": "2024-09-23T04:13:02.296Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "af6f626d-7e00-4b16-9630-b1c767c48745",
      "number": 33,
      "content": "Nhưng CHÚA không để họ lọt vào tay ác nhân,",
      "order": 0,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.300Z",
      "updatedAt": "2024-09-23T04:13:02.300Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "8ef07f41-6bab-4032-af17-9b538b173b11",
      "number": 33,
      "content": "không để bị kết án khi phải ra trước toà.",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.304Z",
      "updatedAt": "2024-09-23T04:13:02.304Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "4959364d-1034-4553-9cce-cc0dbb1f1696",
      "number": 34,
      "content": "Cốp",
      "order": 0,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.308Z",
      "updatedAt": "2024-09-23T04:13:02.308Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "016cbf50-ab6e-438d-a059-951c24436934",
      "number": 34,
      "content": "Hãy trông chờ CHÚA, giữ vững đường lối Người,",
      "order": 1,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.312Z",
      "updatedAt": "2024-09-23T04:13:02.312Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "144cc238-af61-4dae-b9c4-f313e184054a",
      "number": 34,
      "content": "rồi Người sẽ cất nhắc bạn lên",
      "order": 2,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.316Z",
      "updatedAt": "2024-09-23T04:13:02.316Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "be133a2e-3b0f-4439-b87d-358fa6b03d44",
      "number": 34,
      "content": "và ban đất hứa làm gia nghiệp",
      "order": 3,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.321Z",
      "updatedAt": "2024-09-23T04:13:02.321Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "7c17c5a1-55c8-4ef3-9726-b448ef438a1c",
      "number": 34,
      "content": "bạn sẽ nhìn thấy ác nhân bị diệt trừ.",
      "order": 4,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.325Z",
      "updatedAt": "2024-09-23T04:13:02.325Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "12241610-1f93-4dd5-a118-05fba5c7a69b",
      "number": 35,
      "content": "Rết",
      "order": 0,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.329Z",
      "updatedAt": "2024-09-23T04:13:02.329Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "3d5cdf75-c966-4a27-9aa6-12dfdb50fd8e",
      "number": 35,
      "content": "Tôi đã thấy ác nhân ngạo ngược,",
      "order": 1,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.334Z",
      "updatedAt": "2024-09-23T04:13:02.334Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "059abbd5-4e6c-4ae0-9e5c-0e2a613cafe9",
      "number": 35,
      "content": "nó vươn mình tựa hương bá xanh tươi.",
      "order": 2,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.342Z",
      "updatedAt": "2024-09-23T04:13:02.342Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "591c9668-23d8-4f72-9cf8-a6895df47d8f",
      "number": 36,
      "content": "Nay trở lại, nó không còn nữa,",
      "order": 0,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.346Z",
      "updatedAt": "2024-09-23T04:13:02.346Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "fa0946ef-205b-40f8-9417-7a56b1b0de4a",
      "number": 36,
      "content": "tìm kiếm hoài mà chẳng thấy tăm hơi.",
      "order": 1,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.350Z",
      "updatedAt": "2024-09-23T04:13:02.350Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "b050e181-0ac0-43fe-9b27-73b6c384645c",
      "number": 37,
      "content": "Sin",
      "order": 0,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.355Z",
      "updatedAt": "2024-09-23T04:13:02.355Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "c6c8c08e-b73e-427d-bc9e-467dca739815",
      "number": 37,
      "content": "Hãy xem kẻ thiện toàn, cứ nhìn người chính trực,",
      "order": 1,
      "parNumber": 95,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.359Z",
      "updatedAt": "2024-09-23T04:13:02.359Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "31f7cf8e-ee63-4c35-a7a3-728c76853d87",
      "number": 37,
      "content": "ai hiếu hoà sẽ được con dòng cháu giống.",
      "order": 2,
      "parNumber": 96,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.364Z",
      "updatedAt": "2024-09-23T04:13:02.364Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "df04e87a-9338-4482-a298-e7fbddb7e882",
      "number": 38,
      "content": "Quân tội lỗi đều bị diệt trừ,",
      "order": 0,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.368Z",
      "updatedAt": "2024-09-23T04:13:02.368Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "87e80675-c3ae-4e11-90c3-f2101551b6ec",
      "number": 38,
      "content": "nòi ác nhân rồi cũng phải tru di.",
      "order": 1,
      "parNumber": 98,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.373Z",
      "updatedAt": "2024-09-23T04:13:02.373Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "24df43a4-1c9f-4084-9000-36ba0fb95969",
      "number": 39,
      "content": "Tau",
      "order": 0,
      "parNumber": 99,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:13:02.377Z",
      "updatedAt": "2024-09-23T04:13:02.377Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "77f2d7b2-ed66-4b25-be98-c3d140303c23",
      "number": 39,
      "content": "Còn chính nhân được CHÚA thương cứu độ",
      "order": 1,
      "parNumber": 100,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.381Z",
      "updatedAt": "2024-09-23T04:13:02.381Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "55452d0f-2d59-451a-b94e-2310636d5686",
      "number": 39,
      "content": "và bảo vệ chở che trong buổi ngặt nghèo.",
      "order": 2,
      "parNumber": 101,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.385Z",
      "updatedAt": "2024-09-23T04:13:02.385Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "c537067a-30c2-4b48-99fc-07dcc1478690",
      "number": 40,
      "content": "CHÚA phù trợ và Người giải thoát,",
      "order": 0,
      "parNumber": 102,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.390Z",
      "updatedAt": "2024-09-23T04:13:02.390Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "b5c82fab-97c9-4e0b-a574-18dab7d8d9f4",
      "number": 40,
      "content": "giải thoát khỏi ác nhân và thương cứu độ,",
      "order": 1,
      "parNumber": 103,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.394Z",
      "updatedAt": "2024-09-23T04:13:02.394Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "0f47a3bb-9ff8-4731-9218-39eb73b7b35c",
      "number": 40,
      "content": "bởi vì họ ẩn náu bên Người.",
      "order": 2,
      "parNumber": 104,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:13:02.398Z",
      "updatedAt": "2024-09-23T04:13:02.398Z",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    }
  ],
  "footnotes": [
    {
      "id": "b372d81b-cc37-4b5c-8696-0dd76a473cbd",
      "content": "Thánh vịnh này là *một Thánh vịnh giáo huấn theo mẫu tự*. Tác giả phân biệt ở đây hai nhóm : kẻ dữ và người lành. Mặc dầu có sự phân biệt rõ ràng giữa hai nhóm, khó mà tìm ra thứ tự các ý tưởng được triển khai trong Tv này. Tuy nhiên, độc giả có thể bắt được ý tưởng chính sau đây : đứng trước cảnh ác nhân hoành hành, tác giả khuyên người lành cứ bình tĩnh và an tâm, bởi vì họ sẽ được *đất hứa làm gia nghiệp* (cc. 9.11a.22a.29a.34c), vì ở đó họ sẽ sống yên hàn (c.3), được vui hưởng cảnh an lạc chan hoà (c.11b), được no đầy trong ngày đói kém (c.19b), vì có Thiên Chúa can thiệp. Quả thật, Thiên Chúa phá tan mưu đồ của ác nhân (c.15), không hề bỏ rơi ai bao giờ (c.28) ; Người sẽ che chở phù trì (cc. 5b.17b.23-24.33.39-40), giải thoát và cứu độ (cc. 39-40) ; Người sẽ bảo vệ sự sống và dòng dõi của người lành (c.37), sẽ cho họ được phỉ chí toại lòng (c.4).",
      "position": 35,
      "order": 0,
      "createdAt": "2024-09-23T04:13:01.925Z",
      "updatedAt": "2024-09-23T04:13:01.925Z",
      "headingId": "aaf564eb-10bc-4aba-abaa-f169bbcf3f95",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "9a35541e-ed21-4cdb-b38c-b56eaed91c49",
      "content": "*Đất nước* ở đây là xứ Pa-lét-tin (x. cc. 9.11a.22a.29a.34c). *Sống yên hàn* ở đó và được hưởng dùng của cải và tài nguyên phong phú : đó là một trong những ý tưởng chính của Tv này.",
      "position": 28,
      "order": 1,
      "createdAt": "2024-09-23T04:13:01.951Z",
      "updatedAt": "2024-09-23T04:13:01.951Z",
      "verseId": "3b7fe855-7308-46b1-b2f9-13b9efa5671d",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    },
    {
      "id": "f8a0f051-e350-4a64-a684-5e773858e977",
      "content": "Ác nhân gây hại cho bản thân bằng chính phương tiện họ muốn dùng để hại người khác (*gậy ông lại đập lưng ông*). X. Tv 7,16-17 ; 9,16 ; 57,7 ; cũng x. Cn 5,22 ; 14,32 ; 26,27 ; G 4,8 ; Gv 10,8 ; Hc 27,25 ; Kn 11,16 ; 12,23 ; 18,4 ; Hs 8,7 ; 10,13 ...",
      "position": 38,
      "order": 2,
      "createdAt": "2024-09-23T04:13:02.081Z",
      "updatedAt": "2024-09-23T04:13:02.081Z",
      "verseId": "9148c035-2245-4926-8d94-da5c04441f22",
      "chapterId": "65410336-ba11-49c3-b623-a09cb93a1d17"
    }
  ],
