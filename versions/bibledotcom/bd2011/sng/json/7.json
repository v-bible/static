{
  "chapter": {
    "id": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535",
    "number": 7,
    "ref": "https://www.bible.com/bible/19/SNG.7.BD2011",
    "createdAt": "2024-09-23T05:08:40.879Z",
    "updatedAt": "2024-09-23T05:08:40.879Z",
    "bookId": "0b53553a-3777-44ec-a8d7-7b9f7dbf679a"
  },
  "verses": [
    {
      "id": "1eb130cb-abfc-4f71-bc24-07bc5f96f5bc",
      "number": 1,
      "content": "Công nương hỡi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.729Z",
      "updatedAt": "2024-09-23T05:58:08.729Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "2a2aabc8-5885-4aef-b85d-589672193128",
      "number": 1,
      "content": "Hai bàn chân em thật xinh đẹp trong đôi hài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.738Z",
      "updatedAt": "2024-09-23T05:58:08.738Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "8cc6b60d-0b63-42b6-91e2-3181b5dbe0b4",
      "number": 1,
      "content": "Đường cong của cặp đùi em giống như đôi trụ ngọc thạch",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.741Z",
      "updatedAt": "2024-09-23T05:58:08.741Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "0595a866-fa84-4a5d-8d92-25a0e7a6fc87",
      "number": 1,
      "content": "Được bàn tay nghệ thuật tài khéo tạc nên.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.744Z",
      "updatedAt": "2024-09-23T05:58:08.744Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "7d985ba0-2771-496f-8fc6-e020f2b23b6d",
      "number": 2,
      "content": "Rốn em tròn như chung rượu,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.746Z",
      "updatedAt": "2024-09-23T05:58:08.746Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "b7e3edc3-a58b-41cf-8de4-731501039534",
      "number": 2,
      "content": "Không bao giờ cạn hết rượu pha.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.747Z",
      "updatedAt": "2024-09-23T05:58:08.747Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "6657f3cf-98c9-4b0f-850b-04d92944f766",
      "number": 2,
      "content": "Bụng em như một gò lúa mạch,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.749Z",
      "updatedAt": "2024-09-23T05:58:08.749Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "39b94524-1daa-4366-bc53-f7c5eb4acb2c",
      "number": 2,
      "content": "Bao quanh bằng các hoa huệ.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.751Z",
      "updatedAt": "2024-09-23T05:58:08.751Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "46c3e4b7-a20a-4c13-93e5-2cf0440adb59",
      "number": 3,
      "content": "Đôi gò bồng đảo em như một cặp linh dương con,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.753Z",
      "updatedAt": "2024-09-23T05:58:08.753Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "3660ae0b-cca9-40a9-918b-294a9bdaa10a",
      "number": 3,
      "content": "Cặp con sinh đôi của một linh dương mẹ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.755Z",
      "updatedAt": "2024-09-23T05:58:08.755Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "261c1a16-d049-469f-a639-592da794fccc",
      "number": 4,
      "content": "Cổ em như một tháp ngà,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.757Z",
      "updatedAt": "2024-09-23T05:58:08.757Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "ca71203a-e6f5-4a52-beea-ea5a7a99150a",
      "number": 4,
      "content": "Mắt em như các hồ nước ở Hếch-bôn,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.759Z",
      "updatedAt": "2024-09-23T05:58:08.759Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "7dd79d2a-2c2f-4ea0-b5f7-c5e2b7bd1dd2",
      "number": 4,
      "content": "Bên Cổng Bát Ra-bim.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.761Z",
      "updatedAt": "2024-09-23T05:58:08.761Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "46de45f1-a0fc-41ec-abcf-ecb83da9dba7",
      "number": 4,
      "content": "Mũi em như Tháp Li-băng,",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.763Z",
      "updatedAt": "2024-09-23T05:58:08.763Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "579710f5-7c72-4e9e-bd22-b3801947dc35",
      "number": 4,
      "content": "Hướng nhìn về Đa-mách.",
      "order": 4,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.765Z",
      "updatedAt": "2024-09-23T05:58:08.765Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "638b5023-05f2-4796-9f20-415525f8d77e",
      "number": 5,
      "content": "Đầu em trên thân hình em như Núi Cạt-mên.",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.768Z",
      "updatedAt": "2024-09-23T05:58:08.768Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "6946e1c1-df3c-44b9-a660-f26079baf977",
      "number": 5,
      "content": "Tóc em như những giải lụa hồng",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.772Z",
      "updatedAt": "2024-09-23T05:58:08.772Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "f3688537-3016-4289-bc50-d8a79cea9299",
      "number": 5,
      "content": "Mà nhà vua đã bị trói buộc vào đó.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.777Z",
      "updatedAt": "2024-09-23T05:58:08.777Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "2d62ac4b-3e3c-4150-bb77-291ce6e569cf",
      "number": 6,
      "content": "Em xinh đẹp quá và duyên dáng biết bao;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.782Z",
      "updatedAt": "2024-09-23T05:58:08.782Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "35ac2344-a7a0-47d5-9285-3913d04f0906",
      "number": 6,
      "content": "Người yêu hỡi, em quả là tuyệt đẹp.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.787Z",
      "updatedAt": "2024-09-23T05:58:08.787Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "d749a255-0edf-49d0-80e0-8e88eef59f5c",
      "number": 7,
      "content": "Dáng em như một cây chà là,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.792Z",
      "updatedAt": "2024-09-23T05:58:08.792Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "2f84c46f-9050-4547-b872-cfa6f2f5e878",
      "number": 7,
      "content": "Đôi gò bồng đảo em như một chùm trái ngọt ngon.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.796Z",
      "updatedAt": "2024-09-23T05:58:08.796Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "97cc50f0-494a-4339-b232-739d65b8ae88",
      "number": 8,
      "content": "Anh nói, anh sẽ trèo lên cây chà là đó,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.801Z",
      "updatedAt": "2024-09-23T05:58:08.801Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "2d8ecb96-78bf-4e49-bee1-2e8677988ae8",
      "number": 8,
      "content": "Hái trái nó, và ăn.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.806Z",
      "updatedAt": "2024-09-23T05:58:08.806Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "a4a3b49c-abc5-492a-bfbb-dc60b391b411",
      "number": 8,
      "content": "Ôi, ước gì đôi gò bồng đảo em như chùm nho chín mọng,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.811Z",
      "updatedAt": "2024-09-23T05:58:08.811Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "d94b5d36-0398-4249-a92c-86fc46a1bca9",
      "number": 8,
      "content": "Hơi thở em như những trái táo thơm ngon,",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.817Z",
      "updatedAt": "2024-09-23T05:58:08.817Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "cf38a31b-d6df-49da-8691-8304addd3672",
      "number": 9,
      "content": "Và miệng em như rượu nồng hảo hạng.",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.822Z",
      "updatedAt": "2024-09-23T05:58:08.822Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "f3fd31e4-d005-4111-9c6e-1704f6ce96af",
      "number": 9,
      "content": "Nguyện rượu ấy rót vào miệng người yêu của tôi,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.827Z",
      "updatedAt": "2024-09-23T05:58:08.827Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "b85e9e6f-f44c-4022-8dc8-9cf978f58f94",
      "number": 9,
      "content": "Chảy nhẹ nhàng qua bờ môi thiếp ngủ và lẻn vào làm tê tái chân răng.",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.836Z",
      "updatedAt": "2024-09-23T05:58:08.836Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "9d8d8c46-964c-4a18-933a-2742ad3eaee3",
      "number": 10,
      "content": "Tôi thuộc về người yêu của tôi,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.841Z",
      "updatedAt": "2024-09-23T05:58:08.841Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "1c9d847b-77ff-4ef5-bbbc-8e9e6cd4b13a",
      "number": 10,
      "content": "Sự thèm khát của chàng sẽ do tôi làm thỏa mãn.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.845Z",
      "updatedAt": "2024-09-23T05:58:08.845Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "7939c08c-4ff5-4b5f-ae02-00018d3f6bd0",
      "number": 11,
      "content": "Hãy đến, người yêu của em;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.850Z",
      "updatedAt": "2024-09-23T05:58:08.850Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "7abdef12-2f22-4e36-9b54-000cc771c28e",
      "number": 11,
      "content": "Chúng ta hãy đi về miền quê,",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.855Z",
      "updatedAt": "2024-09-23T05:58:08.855Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "76dff013-0d32-4eea-bd89-1315c9fb7de6",
      "number": 11,
      "content": "Đêm nay chúng mình sẽ ngủ lại trong làng.",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.860Z",
      "updatedAt": "2024-09-23T05:58:08.860Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "849018d9-921e-4a58-be9f-485e5280b4e3",
      "number": 12,
      "content": "Sáng mai chúng ta sẽ dậy sớm đi vào vườn nho,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.864Z",
      "updatedAt": "2024-09-23T05:58:08.864Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "f3aa0c54-7b09-4691-829a-71799b1f15da",
      "number": 12,
      "content": "Để xem các nhành nho đã đâm chồi nẩy lộc,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.869Z",
      "updatedAt": "2024-09-23T05:58:08.869Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "a1de0d49-b230-466d-a546-a02889803676",
      "number": 12,
      "content": "Hoa nho đã nở,",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.874Z",
      "updatedAt": "2024-09-23T05:58:08.874Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "314dde63-710d-4cb5-8bd9-2dc6b44097ae",
      "number": 12,
      "content": "Và thạch lựu đã trổ hoa hay chưa,",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.879Z",
      "updatedAt": "2024-09-23T05:58:08.879Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "c00857fb-9705-4420-bf12-399868b75e3e",
      "number": 12,
      "content": "Rồi ở đó em sẽ dâng tặng anh tình yêu của em.",
      "order": 4,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.885Z",
      "updatedAt": "2024-09-23T05:58:08.885Z",
      "chapterId": "1125b01a-9aad-4b8f-a2c7-dae0c41ab535"
    },
    {
      "id": "ebed8ee4-0cca-40ee-a9d3-232e0d2ed6ec",
      "number": 13,
      "content": "Trái phong già đã phảng phất mùi thơm,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:58:08.890Z",
      "updatedAt": "2024-09-23T05:58:08.890Z",
