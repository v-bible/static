{
  "chapter": {
    "id": "fe04ee15-f3a4-4d70-b347-95eeb411c28d",
    "number": 4,
    "ref": "https://www.bible.com/bible/19/SNG.4.BD2011",
    "createdAt": "2024-09-23T05:08:40.868Z",
    "updatedAt": "2024-09-23T05:08:40.868Z",
    "bookId": "0b53553a-3777-44ec-a8d7-7b9f7dbf679a"
  },
  "verses": [
    {
      "id": "b9e315ae-3e5b-4fff-88e1-ca03db096d36",
      "number": 1,
      "content": "Em đẹp quá, em yêu!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.686Z",
      "updatedAt": "2024-09-23T05:57:58.686Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "095a9979-a636-4f39-b495-5b42b36f5e5b",
      "number": 1,
      "content": "Em đẹp quá!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.700Z",
      "updatedAt": "2024-09-23T05:57:58.700Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "ba6b6eb3-3393-435b-ab4d-8b3f871dbbe2",
      "number": 1,
      "content": "Sau màn the che mặt,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.703Z",
      "updatedAt": "2024-09-23T05:57:58.703Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "13c200e4-04ae-4b40-a19b-0a6c3b88f276",
      "number": 1,
      "content": "Đôi mắt em như một cặp bồ câu;",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.708Z",
      "updatedAt": "2024-09-23T05:57:58.708Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "98f88cdc-467c-411c-a081-398eb532e95c",
      "number": 1,
      "content": "Mái tóc em bồng bềnh như một đàn sơn dương",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.712Z",
      "updatedAt": "2024-09-23T05:57:58.712Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "5961c336-aa3b-4420-b33e-61f4151c79e9",
      "number": 1,
      "content": "Đang từ triền núi Ghi-lê-át đi xuống.",
      "order": 5,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.717Z",
      "updatedAt": "2024-09-23T05:57:58.717Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "a28a6d1e-391f-4611-b5b8-c09bafb51496",
      "number": 2,
      "content": "Răng em trắng đẹp như một đàn chiên",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.721Z",
      "updatedAt": "2024-09-23T05:57:58.721Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "f2521eb2-a91d-4f2b-9545-dc57dc4eef19",
      "number": 2,
      "content": "Vừa mới được hớt lông, tắm dưới suối đi lên,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.727Z",
      "updatedAt": "2024-09-23T05:57:58.727Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "5c40919a-d159-4301-a19d-a3f1b7ac4b62",
      "number": 2,
      "content": "Tất cả đều là cặp đôi,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.731Z",
      "updatedAt": "2024-09-23T05:57:58.731Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "4b121321-5eaa-45e1-b519-1d07c8080e03",
      "number": 2,
      "content": "Không chiếc nào đơn lẻ.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.736Z",
      "updatedAt": "2024-09-23T05:57:58.736Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "fb297b7a-b824-4a18-ade5-21a143041949",
      "number": 3,
      "content": "Môi em mọng đỏ như tao chỉ hồng,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.740Z",
      "updatedAt": "2024-09-23T05:57:58.740Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "b46535ef-c1e5-4f89-9f23-59c81f39d74a",
      "number": 3,
      "content": "Miệng em duyên dáng thật đáng yêu.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.745Z",
      "updatedAt": "2024-09-23T05:57:58.745Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "a3b89c6a-2588-45eb-b859-b4fd8f4bbd26",
      "number": 3,
      "content": "Má em đỏ hây hây như nửa quả thạch lựu,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.750Z",
      "updatedAt": "2024-09-23T05:57:58.750Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "29dcf673-6661-44df-ab65-efebf56f279d",
      "number": 3,
      "content": "Thấp thoáng sau màn the che mặt.",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.755Z",
      "updatedAt": "2024-09-23T05:57:58.755Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "d8982f34-dc15-4e0c-87c8-6073adf5383c",
      "number": 4,
      "content": "Cổ em xinh đẹp như Tháp Đa-vít,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.761Z",
      "updatedAt": "2024-09-23T05:57:58.761Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "0372842a-cf6a-442d-b237-25ea88240bd2",
      "number": 4,
      "content": "Được xây dựng để chứa binh khí,",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.766Z",
      "updatedAt": "2024-09-23T05:57:58.766Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "cc6b7d24-27cb-4801-bd6e-fb4a299466c6",
      "number": 4,
      "content": "Trên đó treo một ngàn cái khiên,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.771Z",
      "updatedAt": "2024-09-23T05:57:58.771Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "ff51c78b-4d3d-4e3e-a3e2-35fe1dd6bbe1",
      "number": 4,
      "content": "Đó là khiên của những dũng sĩ.",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.777Z",
      "updatedAt": "2024-09-23T05:57:58.777Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "49b9eafe-3aaf-4b02-82b8-5c85b6319d08",
      "number": 5,
      "content": "Đôi gò bồng đảo em như một cặp linh dương con,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.782Z",
      "updatedAt": "2024-09-23T05:57:58.782Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "c5b3c8e9-3029-48f8-a2f5-455aa19eb0b5",
      "number": 5,
      "content": "Cặp con sinh đôi của một linh dương mẹ,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.787Z",
      "updatedAt": "2024-09-23T05:57:58.787Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "656aed05-d29d-47af-b193-e0e5cf7a6332",
      "number": 5,
      "content": "Đang gặm cỏ non giữa thung lũng hoa huệ.",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.792Z",
      "updatedAt": "2024-09-23T05:57:58.792Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "b50bfd1c-f357-475d-8985-80bb46b52af7",
      "number": 6,
      "content": "Trước khi bình minh đến",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.797Z",
      "updatedAt": "2024-09-23T05:57:58.797Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "66ac0616-9a30-44ed-b035-abcfa296e5f4",
      "number": 6,
      "content": "Và bóng tối biến đi,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.803Z",
      "updatedAt": "2024-09-23T05:57:58.803Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "449a6013-bb69-4b89-9767-79456b45a3ad",
      "number": 6,
      "content": "Anh sẽ đến núi mộc dược",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.808Z",
      "updatedAt": "2024-09-23T05:57:58.808Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "d9e84ef0-bb17-4e8f-bacd-d23ee3a9afc6",
      "number": 6,
      "content": "Và đến đồi nhũ hương.",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.813Z",
      "updatedAt": "2024-09-23T05:57:58.813Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "38074ecd-74b3-44f7-a644-7aac35d9ea39",
      "number": 7,
      "content": "Em yêu ơi, toàn thân em đẹp tuyệt trần,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.818Z",
      "updatedAt": "2024-09-23T05:57:58.818Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "b8a87d4b-820a-4ea8-9562-23300d70f21e",
      "number": 7,
      "content": "Không chỗ nào có thể chê được.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.823Z",
      "updatedAt": "2024-09-23T05:57:58.823Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "359742a7-5ab3-4e3d-9aeb-80d192555fd6",
      "number": 8,
      "content": "Hỡi hôn thê của anh, hãy cùng anh rời khỏi Li-băng,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.828Z",
      "updatedAt": "2024-09-23T05:57:58.828Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "7992c4de-d7e2-4e67-8ebb-19b9ddaf664f",
      "number": 8,
      "content": "Hãy cùng anh rời khỏi Li-băng,",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.833Z",
      "updatedAt": "2024-09-23T05:57:58.833Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "d870a4e2-1974-4ad7-a461-689c6c1f53ec",
      "number": 8,
      "content": "Hãy cùng nhau ngắm cảnh bình minh từ đỉnh A-ma-na,",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.838Z",
      "updatedAt": "2024-09-23T05:57:58.838Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "83c59b4b-ecfa-4153-8a27-fe3f541357ed",
      "number": 8,
      "content": "Từ đỉnh Sê-nia và từ đỉnh Hẹt-môn,",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.842Z",
      "updatedAt": "2024-09-23T05:57:58.842Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "3b828763-06cf-4b97-a8c5-f30f8d133584",
      "number": 8,
      "content": "Từ các hang sư tử",
      "order": 4,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.847Z",
      "updatedAt": "2024-09-23T05:57:58.847Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "811bca5b-db65-41e1-b33c-81618af95b3d",
      "number": 8,
      "content": "Và từ những núi hùm beo.",
      "order": 5,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.853Z",
      "updatedAt": "2024-09-23T05:57:58.853Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "3fe18af6-7751-4d27-bc01-4244b8b4499b",
      "number": 9,
      "content": "Em đã chiếm đoạt tim anh rồi, hỡi em gái của anh, hôn thê của anh;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.858Z",
      "updatedAt": "2024-09-23T05:57:58.858Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "db36f84b-bbce-44d5-84ac-041e40fd2297",
      "number": 9,
      "content": "Em đã cướp mất hồn anh bằng một cái liếc mắt của em,",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.863Z",
      "updatedAt": "2024-09-23T05:57:58.863Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "8b761724-ce3c-4d84-95f2-46f259456e82",
      "number": 9,
      "content": "Bằng một dây chuyền đeo cổ của em.",
      "order": 2,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.869Z",
      "updatedAt": "2024-09-23T05:57:58.869Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "a5b6db2f-d550-4dc4-9c68-384941c13221",
      "number": 10,
      "content": "Tình yêu của em thật tuyệt vời, hỡi em gái của anh, hôn thê của anh,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.874Z",
      "updatedAt": "2024-09-23T05:57:58.874Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "502bb3f4-e0c2-4573-a915-c7e47606c106",
      "number": 10,
      "content": "Tình yêu của em thật nồng nàn hơn rượu;",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.879Z",
      "updatedAt": "2024-09-23T05:57:58.879Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "61e1531b-e979-428a-b07c-e1af2f2b5f65",
      "number": 10,
      "content": "Mùi nước hoa từ người em làm sảng khoái hơn mọi thứ hương thơm.",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.884Z",
      "updatedAt": "2024-09-23T05:57:58.884Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "1b75ae4d-7fb1-448f-b1d8-1ef4fb3bacee",
      "number": 11,
      "content": "Môi em ngọt lịm như mật nhểu, hỡi hôn thê của anh;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.889Z",
      "updatedAt": "2024-09-23T05:57:58.889Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "13547be7-82d7-4a18-969a-89305cd10800",
      "number": 11,
      "content": "Dưới lưỡi em là mật ngọt và sữa ngon.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.894Z",
      "updatedAt": "2024-09-23T05:57:58.894Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "3d70730e-7fec-47bb-8355-2ac225358a49",
      "number": 11,
      "content": "Mùi thơm từ y phục em tỏa ra như mùi thơm trên Núi Li-băng.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.900Z",
      "updatedAt": "2024-09-23T05:57:58.900Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "9b271294-7c5a-4706-be2f-ebc4a4eba02c",
      "number": 12,
      "content": "Em là mảnh vườn bị khóa kín, hỡi em gái của anh, hôn thê của anh,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.905Z",
      "updatedAt": "2024-09-23T05:57:58.905Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "9a74e808-6baf-4dac-bbc7-ca9b94e2a9cd",
      "number": 12,
      "content": "Em là suối nước bị rào ngăn, là mạch nước bị niêm phong, chưa hề có ai uống.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.910Z",
      "updatedAt": "2024-09-23T05:57:58.910Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "f8d24854-55f3-45e5-b869-ca33edd14cfc",
      "number": 13,
      "content": "Vườn cây của em là một vườn đầy thạch lựu,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.915Z",
      "updatedAt": "2024-09-23T05:57:58.915Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "72742be8-f05d-4dab-a96a-31b33999c85c",
      "number": 13,
      "content": "Đầy những hoa thơm trái ngọt,",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.920Z",
      "updatedAt": "2024-09-23T05:57:58.920Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "fdfbc525-80d3-45e3-a6ce-c96e38397633",
      "number": 13,
      "content": "Nào phụng tiên và cam tùng,",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.925Z",
      "updatedAt": "2024-09-23T05:57:58.925Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "96120038-2852-4baf-aa51-7873732e3462",
      "number": 14,
      "content": "Cam tùng và hoàng khương, đinh hương và nhục quế,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.930Z",
      "updatedAt": "2024-09-23T05:57:58.930Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "ced1ee2b-9a67-40bf-965b-671d884a5a38",
      "number": 14,
      "content": "Cùng mọi cây nhũ hương,",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.935Z",
      "updatedAt": "2024-09-23T05:57:58.935Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "bc66d28f-e08a-49ca-aa07-309683a4dfbf",
      "number": 14,
      "content": "Mộc dược và trầm hương,",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.940Z",
      "updatedAt": "2024-09-23T05:57:58.940Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "cdc0e83f-6164-49b3-89ed-d1e7af0d9f1a",
      "number": 14,
      "content": "Với tất cả những cây sản xuất các hương liệu thượng hạng.",
      "order": 3,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.946Z",
      "updatedAt": "2024-09-23T05:57:58.946Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "c078cef6-d594-48fb-91e0-04e825d18dbc",
      "number": 15,
      "content": "Em là suối nước trong vườn,",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.950Z",
      "updatedAt": "2024-09-23T05:57:58.950Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "18e6aa4f-82e2-42d1-8035-57a13508050d",
      "number": 15,
      "content": "Là giếng nước trào tuôn, tuôn dòng từ Li-băng chảy xuống.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.956Z",
      "updatedAt": "2024-09-23T05:57:58.956Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "bb1354ab-33a8-4e33-9fde-039c7260c4ab",
      "number": 16,
      "content": "Hãy nổi dậy, hỡi gió từ phương bắc.",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.960Z",
      "updatedAt": "2024-09-23T05:57:58.960Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "65898b31-fd19-4a62-a6a2-4cebf1c51df2",
      "number": 16,
      "content": "Hãy đến đây, hỡi gió từ phương nam.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.970Z",
      "updatedAt": "2024-09-23T05:57:58.970Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "e1ba665e-28f3-49da-8d7a-088b504c2f5a",
      "number": 16,
      "content": "Hãy thổi trên mảnh vườn của ta,",
      "order": 2,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.975Z",
      "updatedAt": "2024-09-23T05:57:58.975Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "85f33334-a044-4112-9edf-edfb2ea4a093",
      "number": 16,
      "content": "Để hương thơm của nó tỏa lan ra ngoài;",
      "order": 3,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.980Z",
      "updatedAt": "2024-09-23T05:57:58.980Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "1c7f53e4-3df6-4b20-952b-9fe075f6e349",
      "number": 16,
      "content": "Để người yêu của ta đến trong vườn của chàng,",
      "order": 4,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.985Z",
      "updatedAt": "2024-09-23T05:57:58.985Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    },
    {
      "id": "95edaf2a-5d04-49b5-824b-ba5f695b98b6",
      "number": 16,
      "content": "Hầu thưởng thức những trái ngọt ngon của nó.",
      "order": 5,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:58.989Z",
      "updatedAt": "2024-09-23T05:57:58.989Z",
      "chapterId": "fe04ee15-f3a4-4d70-b347-95eeb411c28d"
    }
  ],
