  "headings": [
    {
      "id": "5f97ec72-9522-4858-91fd-501f7d5c66f6",
      "content": "Ê-li-pha Cho Rằng Sự Gian Ác của Gióp Rất Lớn",
      "order": 0,
      "createdAt": "2024-09-23T05:41:36.373Z",
      "updatedAt": "2024-09-23T05:41:36.373Z",
      "verseId": "3a924fec-abaf-4184-85c3-de54b4ec6009",
      "chapterId": "db3c9087-502f-44e2-97ee-4991bf3632d0"
    }
  ],
