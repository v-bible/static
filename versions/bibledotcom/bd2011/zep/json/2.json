    },
    {
      "id": "bebac2e4-94b3-4518-80e8-a83daae02cd3",
      "content": "Dân sống ở miền thượng lưu Sông Nin",
      "position": 28,
      "order": 1,
      "createdAt": "2024-09-23T06:11:55.108Z",
      "updatedAt": "2024-09-23T06:11:55.108Z",
