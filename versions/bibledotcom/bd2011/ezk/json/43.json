      "chapterId": "0d16021a-8531-477d-a36e-1dd9f68a3864"
    },
    {
      "id": "03634fb6-b8d9-4c1f-8c37-7b73e45f503a",
      "content": "nt: 1 cubit đặc biệt, 58cm hay gần 6 tấc",
      "position": 196,
      "order": 1,
      "createdAt": "2024-09-23T06:07:59.415Z",
