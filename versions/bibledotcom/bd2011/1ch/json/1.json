{
  "chapter": {
    "id": "ac1b847a-eabb-41af-8cab-47ed1595d953",
    "number": 1,
    "ref": "https://www.bible.com/bible/19/1CH.1.BD2011",
    "createdAt": "2024-09-23T05:08:39.725Z",
    "updatedAt": "2024-09-23T05:08:39.725Z",
    "bookId": "bdd5c4e9-b842-4c74-b957-733181270031"
  },
  "verses": [
    {
      "id": "b262ae6c-4d07-473c-ace1-f1cf929d6502",
      "number": 1,
      "content": "A-đam, Sết, Ê-nốt,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.794Z",
      "updatedAt": "2024-09-23T05:33:02.794Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "c2d65ba7-8ae1-4c64-8050-b7c9dc18ecde",
      "number": 2,
      "content": "Kê-nan, Ma-ha-la-lên, Gia-rết,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.806Z",
      "updatedAt": "2024-09-23T05:33:02.806Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "1c47bf46-205b-4d95-9e1e-76e33cda63f2",
      "number": 3,
      "content": "Ê-nóc, Mê-thu-sê-la, La-méc,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.808Z",
      "updatedAt": "2024-09-23T05:33:02.808Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "ee4420cf-18fb-40c4-9fc6-db62836fe2d2",
      "number": 4,
      "content": "Nô-ê; các con của Nô-ê là Sem, Cham, và Gia-phết.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.809Z",
      "updatedAt": "2024-09-23T05:33:02.809Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "bf8c7f50-5d93-4684-88c7-61ac60f97b0a",
      "number": 5,
      "content": "Dòng dõi của Gia-phết: Gô-me, Ma-gót, Ma-đai, Gia-van, Tu-banh, Mê-sếch, và Ti-ra.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.811Z",
      "updatedAt": "2024-09-23T05:33:02.811Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "f1e68872-0a22-4d36-9bfb-d13a401ca07c",
      "number": 6,
      "content": "Dòng dõi của Gô-me: Ách-kê-na, Đi-phát, và Tô-gạc-ma.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.813Z",
      "updatedAt": "2024-09-23T05:33:02.813Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "6bc55930-effb-4b4d-b6cc-4a2bca86b4a5",
      "number": 7,
      "content": "Dòng dõi của Gia-van: Ê-li-sa, Tạt-si, Kít-tim, và Rô-đa-nim.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.818Z",
      "updatedAt": "2024-09-23T05:33:02.818Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "9ac69131-8c9e-490a-865f-3075e549b919",
      "number": 8,
      "content": "Dòng dõi của Cham: Cút, Ai-cập, Pút, và Ca-na-an.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.827Z",
      "updatedAt": "2024-09-23T05:33:02.827Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "49ba4e1e-2de1-4025-a66f-61fc92e3741f",
      "number": 9,
      "content": "Dòng dõi của Cút: Sê-ba, Ha-vi-la, Sáp-ta, Ra-a-ma, và Sáp-tê-ca.",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.831Z",
      "updatedAt": "2024-09-23T05:33:02.831Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "5c5aca81-1723-425a-a94c-de5fb58ff7fd",
      "number": 9,
      "content": "Dòng dõi của Ra-a-ma: Shê-ba và Đê-đan.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.835Z",
      "updatedAt": "2024-09-23T05:33:02.835Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "3da990ed-6470-4d56-8532-12362cc60bcc",
      "number": 10,
      "content": "Cút là cha của Nim-rốt; ông ấy là anh hùng đầu tiên trên mặt đất.",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.840Z",
      "updatedAt": "2024-09-23T05:33:02.840Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "5036d39a-817b-4783-b932-0870392d95f1",
      "number": 11,
      "content": "Ai-cập là cha của các dân Lút, A-nam, Lê-háp, Náp-tu,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.844Z",
      "updatedAt": "2024-09-23T05:33:02.844Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "2c0dd276-621f-4903-9df4-b96794a4e83d",
      "number": 12,
      "content": "Pát-ru, Các-lu (từ dân ấy mà ra dân Phi-li-tin), và dân Cáp-tô.",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.848Z",
      "updatedAt": "2024-09-23T05:33:02.848Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "75d07d65-292f-4d1d-ab35-852cc7999fb0",
      "number": 13,
      "content": "Ca-na-an là cha của Si-đôn con đầu lòng ông ấy, và Hết,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.857Z",
      "updatedAt": "2024-09-23T05:33:02.857Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "ff8305c7-5147-4d27-a973-eb955197f039",
      "number": 14,
      "content": "dân Giê-bu-si, dân A-mô-ri, dân Ghi-ga-si,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.865Z",
      "updatedAt": "2024-09-23T05:33:02.865Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "c4557ead-e8cf-41ed-ad56-2d76eb2800a6",
      "number": 15,
      "content": "dân Hi-vi, dân Ạc-ki, dân Si-ni,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.870Z",
      "updatedAt": "2024-09-23T05:33:02.870Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "1b77e1e7-a7c9-4130-9893-8a8b9eb1d489",
      "number": 16,
      "content": "dân Ạc-va-đi, dân Xê-ma-ri, và dân Ha-ma-thi.",
      "order": 0,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.875Z",
      "updatedAt": "2024-09-23T05:33:02.875Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "7e3d04e2-cbae-448e-a907-7de5a7400b5b",
      "number": 17,
      "content": "Dòng dõi của Sem: Ê-lam, Át-sua, Ạc-pác-sát, Lút, A-ram, U-xơ, Hun, Ghê-the, và Mê-sếch.",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.882Z",
      "updatedAt": "2024-09-23T05:33:02.882Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "7e6bcc6a-9b76-463d-9d00-9ce2f8f2bba2",
      "number": 18,
      "content": "Ạc-pác-sát là cha của Sê-la. Sê-la là cha của Ê-be.",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.890Z",
      "updatedAt": "2024-09-23T05:33:02.890Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "3c0b2815-1fad-4188-9129-c2b9619e3c11",
      "number": 19,
      "content": "Ê-be có hai con trai. Tên của người anh là Pê-léc, vì trong thời của ông đất bị chia ra, và tên của người em là Giốc-tan.",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.895Z",
      "updatedAt": "2024-09-23T05:33:02.895Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "ed206b26-9d45-42e0-bde0-1907e83f5479",
      "number": 20,
      "content": "Giốc-tan là cha của Anh-mô-đát, Sê-lép, Ha-xa-ma-vết, Giê-ra,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.899Z",
      "updatedAt": "2024-09-23T05:33:02.899Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "bab03ea7-9bec-42d7-943c-deddb96a12b2",
      "number": 21,
      "content": "Ha-đô-ram, U-xanh, Điết-la,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.903Z",
      "updatedAt": "2024-09-23T05:33:02.903Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "7488ea63-4e69-4fb3-a2f1-1d1d9e8e64e3",
      "number": 22,
      "content": "Ê-banh, A-bi-ma-ên, Sê-ba,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.907Z",
      "updatedAt": "2024-09-23T05:33:02.907Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "8cb076b4-80d4-4749-a2b5-290633d19c80",
      "number": 23,
      "content": "Ô-phia, Ha-vi-la, và Giô-báp. Tất cả những người ấy đều là con cháu của Giốc-tan.",
      "order": 0,
      "parNumber": 13,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.916Z",
      "updatedAt": "2024-09-23T05:33:02.916Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "1a1ffc02-bb19-4659-adbe-10227b7f2503",
      "number": 24,
      "content": "Sem, Ạc-pác-sát, Sê-la,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.920Z",
      "updatedAt": "2024-09-23T05:33:02.920Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "e9ad582c-5723-46b5-8ae0-52ee3a6dfd7e",
      "number": 25,
      "content": "Ê-be, Pê-léc, Rê-u,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.924Z",
      "updatedAt": "2024-09-23T05:33:02.924Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "9225f8e8-a076-45a6-b01b-162e5616f077",
      "number": 26,
      "content": "Sê-rúc, Na-hô, Tê-ra,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.928Z",
      "updatedAt": "2024-09-23T05:33:02.928Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "c83dad5f-60ef-4989-a383-84763f22fee6",
      "number": 27,
      "content": "Áp-ram, người về sau được đổi tên thành Áp-ra-ham.",
      "order": 0,
      "parNumber": 14,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.932Z",
      "updatedAt": "2024-09-23T05:33:02.932Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "1c94441c-c4d8-4e50-97a7-fe932d960c3f",
      "number": 28,
      "content": "Các con trai của Áp-ra-ham là I-sác và Ích-ma-ên.",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.937Z",
      "updatedAt": "2024-09-23T05:33:02.937Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "3510dad6-a3b8-45ff-a1b2-8e874b997932",
      "number": 29,
      "content": "Đây dòng dõi của họ: Con đầu lòng của Ích-ma-ên là Nê-ba-giốt, kế đến là Kê-đa, Át-bê-ên, Míp-sam,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.944Z",
      "updatedAt": "2024-09-23T05:33:02.944Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "d29ae93f-6b03-4706-8501-7300ceeebc47",
      "number": 30,
      "content": "Mích-ma, Đu-ma, Mát-sa, Ha-đát, Tê-ma,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.948Z",
      "updatedAt": "2024-09-23T05:33:02.948Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "97a2de81-c19c-45d0-ac88-bbb0f1fe7ad0",
      "number": 31,
      "content": "Giê-tua, Na-phích, và Kê-đê-ma. Đó là các con trai của Ích-ma-ên.",
      "order": 0,
      "parNumber": 16,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.952Z",
      "updatedAt": "2024-09-23T05:33:02.952Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "2db89127-1440-48fc-9203-b22e4a02c53c",
      "number": 32,
      "content": "Các con trai do bà Kê-tu-ra, vợ kế của Áp-ra-ham, sinh cho ông là Xim-ran, Giốc-san, Mê-đan, Mi-đi-an, Ích-bách, và Su-a.",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.956Z",
      "updatedAt": "2024-09-23T05:33:02.956Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "f8fabb24-a0e2-464a-bcac-987798581ac8",
      "number": 32,
      "content": "Các con trai của Giốc-san là Shê-ba và Đê-đan.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.960Z",
      "updatedAt": "2024-09-23T05:33:02.960Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "b03fa96a-6851-4f53-836a-f789fc4fae66",
      "number": 33,
      "content": "Các con trai của Mi-đi-an là Ê-pha, Ê-phe, Ha-nốc, A-bi-đa, và Ên-đa-a. Tất cả những người ấy là con cháu của bà Kê-tu-ra.",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.964Z",
      "updatedAt": "2024-09-23T05:33:02.964Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "cf0ce9ae-ab91-4459-9d60-57db91c075aa",
      "number": 34,
      "content": "Áp-ra-ham sinh I-sác. Các con của I-sác là Ê-sau và I-sơ-ra-ên.",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.968Z",
      "updatedAt": "2024-09-23T05:33:02.968Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "909e44ee-f880-47b3-a19b-0dc936e3d07b",
      "number": 35,
      "content": "Các con của Ê-sau là Ê-li-pha, Rê-u-ên, Giê-úc, Gia-lam, và Cô-ra.",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.972Z",
      "updatedAt": "2024-09-23T05:33:02.972Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "3f8de5f8-405b-4296-9d9d-ed13f476ceee",
      "number": 36,
      "content": "Các con của Ê-li-pha là Tê-man, Ô-ma, Xê-phô, Ga-tam, Kê-na, Tim-na, và A-ma-léc.",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.976Z",
      "updatedAt": "2024-09-23T05:33:02.976Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "85eefa36-5976-4494-8138-0f2db57ce7fb",
      "number": 37,
      "content": "Các con của Rê-u-ên là Na-hát, Xê-ra, Sam-ma, và Mít-xa.",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.980Z",
      "updatedAt": "2024-09-23T05:33:02.980Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "c3f62023-dad4-46ee-a19d-e40e514ecdf2",
      "number": 38,
      "content": "Các con của Sê-i-rơ là Lô-tan, Sô-banh, Xi-bê-ôn, A-na, Đi-sôn, Ê-xe, và Đi-san.",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.984Z",
      "updatedAt": "2024-09-23T05:33:02.984Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "793f8daa-a57d-47eb-8a64-2c82edbe07f4",
      "number": 39,
      "content": "Các con của Lô-tan là Hô-ri và Hô-mam. Em gái của Lô-tan là Tim-na.",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.988Z",
      "updatedAt": "2024-09-23T05:33:02.988Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "522c4d5c-77f6-4ecc-9ca8-0b7b0ac64b8d",
      "number": 40,
      "content": "Các con của Sô-banh là A-li-an, Ma-na-hát, Ê-banh, Sê-phi, và Ô-nam.",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.992Z",
      "updatedAt": "2024-09-23T05:33:02.992Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "951eec4c-e8ac-47d5-bbeb-c2a79ac56471",
      "number": 40,
      "content": "Các con của Xi-bê-ôn là Ai-a và A-na.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:02.997Z",
      "updatedAt": "2024-09-23T05:33:02.997Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "bdc37cd3-ef87-4dfe-bfac-e64a603d158c",
      "number": 41,
      "content": "Con của A-na là Đi-sôn.",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.002Z",
      "updatedAt": "2024-09-23T05:33:03.002Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "7e6b4816-3ca2-478d-9f06-f083befbcd27",
      "number": 41,
      "content": "Các con của Đi-sôn là Ham-ran, Ếch-ban, Ít-ran, và Kê-ran.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.006Z",
      "updatedAt": "2024-09-23T05:33:03.006Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "f33c4ad4-8c0f-4928-bdb7-827a273090b9",
      "number": 42,
      "content": "Các con của Ê-xe là Bin-han, Xa-a-van, và Gia-a-can.",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.010Z",
      "updatedAt": "2024-09-23T05:33:03.010Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "bc96b09b-520a-44e1-b575-04607a09ae63",
      "number": 42,
      "content": "Các con của Đi-san là U-xơ và A-ran.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.018Z",
      "updatedAt": "2024-09-23T05:33:03.018Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "8abfbdd0-b217-4cc5-ba4e-91e9611f27f2",
      "number": 43,
      "content": "Đây là tên các vua đã trị vì trong đất Ê-đôm trước khi có một vua nào trị vì trên dân I-sơ-ra-ên:",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.022Z",
      "updatedAt": "2024-09-23T05:33:03.022Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "934038be-6bc6-453c-b86d-a23cef9197b5",
      "number": 43,
      "content": "Bê-la con của Bê-ô; tên của kinh đô vua ấy là Đin-ha-ba.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.026Z",
      "updatedAt": "2024-09-23T05:33:03.026Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "9df11df8-296f-4154-a042-470f898aee5e",
      "number": 44,
      "content": "Sau khi Bê-la băng hà, Giô-báp con trai Xê-ra ở Bô-ra thế vị.",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.030Z",
      "updatedAt": "2024-09-23T05:33:03.030Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "09d2752f-4469-4f8f-a5eb-7ca3ab7bb631",
      "number": 45,
      "content": "Sau khi Giô-báp băng hà, Hu-sam quê ở vùng Tê-man thế vị.",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.034Z",
      "updatedAt": "2024-09-23T05:33:03.034Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "0cf591fa-46f9-4339-bf99-c4ba3b84b0d2",
      "number": 46,
      "content": "Sau khi Hu-sam băng hà, Ha-đát con của Bê-đát, người đã đánh bại dân Mi-đi-an trong đồng bằng Mô-áp, thế vị; tên của kinh đô vua ấy là A-vít.",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.038Z",
      "updatedAt": "2024-09-23T05:33:03.038Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "85c56e1c-f202-4469-9867-da7b9ea05db7",
      "number": 47,
      "content": "Sau khi Ha-đát băng hà, Sam-la con của Mách-rê-ca thế vị.",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.043Z",
      "updatedAt": "2024-09-23T05:33:03.043Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "3d5feeb7-2f7f-4ce8-bd25-117756e921df",
      "number": 48,
      "content": "Sau khi Sam-la băng hà, Sa-un quê ở Rê-hô-bốt, một địa danh nằm bên bờ sông Ơ-phơ-rát, thế vị.",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.047Z",
      "updatedAt": "2024-09-23T05:33:03.047Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "0913619a-a917-4f7f-b4e8-713c0e5e8e2d",
      "number": 49,
      "content": "Sau khi Sa-un băng hà, Ba-anh Ha-nan con của Ách-bơ thế vị.",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.051Z",
      "updatedAt": "2024-09-23T05:33:03.051Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "269a6d02-6386-4337-bfc0-d8d69d0c9b3e",
      "number": 50,
      "content": "Sau khi Ba-anh Ha-nan băng hà, Ha-đát thế vị; tên của kinh đô vua ấy là Pa-i. Vợ của vua ấy tên là Mê-hê-ta-bên. Bà ấy là ái nữ của Mát-rết, ái nữ của Mê Xa-háp.",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.055Z",
      "updatedAt": "2024-09-23T05:33:03.055Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "f24b1baf-adfe-49cf-bf21-dc8569424978",
      "number": 51,
      "content": "Sau đó Ha-đát băng hà.",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.059Z",
      "updatedAt": "2024-09-23T05:33:03.059Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "3700dbb7-c489-45e5-a794-bdc1c893d0cb",
      "number": 51,
      "content": "Các tộc trưởng của dân Ê-đôm là tộc trưởng Tim-na, tộc trưởng A-li-a, tộc trưởng Giê-thết,",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.063Z",
      "updatedAt": "2024-09-23T05:33:03.063Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "e7ec0670-f4c4-4cce-bb50-4b97a2791ca5",
      "number": 52,
      "content": "tộc trưởng Ô-hô-li-ba-ma, tộc trưởng Ê-la, tộc trưởng Pi-nôn,",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.071Z",
      "updatedAt": "2024-09-23T05:33:03.071Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "547a92b8-f47c-43d2-aac8-5a0f5d73d2d8",
      "number": 53,
      "content": "tộc trưởng Kê-na, tộc trưởng Tê-man, tộc trưởng Míp-xa,",
      "order": 0,
      "parNumber": 41,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.075Z",
      "updatedAt": "2024-09-23T05:33:03.075Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    },
    {
      "id": "e1f8425c-aaa4-48b1-9129-6410cab81d82",
      "number": 54,
      "content": "tộc trưởng Mạc-đi-ên, và tộc trưởng I-ram. Đó là các tộc trưởng của dân Ê-đôm.",
      "order": 0,
      "parNumber": 41,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-09-23T05:33:03.079Z",
      "updatedAt": "2024-09-23T05:33:03.079Z",
      "chapterId": "ac1b847a-eabb-41af-8cab-47ed1595d953"
    }
  ],
