{
  "chapter": {
    "id": "638d849e-b24c-46e2-9e7b-2834904e5e81",
    "number": 5,
    "ref": "https://www.bible.com/bible/19/MIC.5.BD2011",
    "createdAt": "2024-09-23T05:08:41.672Z",
    "updatedAt": "2024-09-23T05:08:41.672Z",
    "bookId": "94b75632-8b49-4c62-b5c1-c2721b11afe0"
  },
  "verses": [
    {
      "id": "3d393a8a-54b0-41f1-8fad-34814a0e60ff",
      "number": 1,
      "content": "Bây giờ, hỡi Ái Nữ của quân đội, hãy tập họp lại thành đội ngũ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.267Z",
      "updatedAt": "2024-09-23T06:11:11.267Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "f4f1fc69-2a5d-45c7-b52e-208dc9857e76",
      "number": 1,
      "content": "Quân thù đang vây hãm chúng ta,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.276Z",
      "updatedAt": "2024-09-23T06:11:11.276Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "5ba06e4a-7bb1-4e34-8850-0b63f5ad76f6",
      "number": 1,
      "content": "Chúng sẽ dùng gậy",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.278Z",
      "updatedAt": "2024-09-23T06:11:11.278Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "f302047e-a7d0-43d8-9da5-cc7f42ee4ddd",
      "number": 1,
      "content": "Đánh vào má người lãnh đạo I-sơ-ra-ên.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.281Z",
      "updatedAt": "2024-09-23T06:11:11.281Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "870612b1-dbef-4489-8eea-aa50696c1cb0",
      "number": 2,
      "content": "“Này ngươi, hỡi Bết-lê-hem ở Ép-ra-tha,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.285Z",
      "updatedAt": "2024-09-23T06:11:11.285Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "f3d12f88-12d9-4bdf-ba5c-74284776de63",
      "number": 2,
      "content": "Dù ngươi nhỏ bé không ra gì giữa các thị tộc của Giu-đa,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.288Z",
      "updatedAt": "2024-09-23T06:11:11.288Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "7b25605e-89c4-40ce-98fd-66cb09e0bba8",
      "number": 2,
      "content": "Nhưng từ ngươi sẽ ra cho Ta một người,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.290Z",
      "updatedAt": "2024-09-23T06:11:11.290Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "e8c2fcc9-53e5-4ff6-875d-134778a25df0",
      "number": 2,
      "content": "Một người sẽ trị vì trên I-sơ-ra-ên.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.292Z",
      "updatedAt": "2024-09-23T06:11:11.292Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "517871a5-7a12-49a3-a347-cfe12df1be16",
      "number": 2,
      "content": "Gốc gác của Người đã có từ ngàn xưa,",
      "order": 4,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.294Z",
      "updatedAt": "2024-09-23T06:11:11.294Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "4e2e6bf9-f269-4d48-917b-7a3ff8a4f8b7",
      "number": 2,
      "content": "Từ vô cực trong quá khứ.”",
      "order": 5,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.296Z",
      "updatedAt": "2024-09-23T06:11:11.296Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "21e67270-7d42-4637-a291-b42b26debefa",
      "number": 3,
      "content": "Vì thế Ngài sẽ bỏ mặc họ một thời gian,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.298Z",
      "updatedAt": "2024-09-23T06:11:11.298Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "1e5bc2bb-b0f2-4c99-818a-3b2eb7e89e95",
      "number": 3,
      "content": "Cho đến khi người đàn bà lâm bồn đã sinh con;",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.300Z",
      "updatedAt": "2024-09-23T06:11:11.300Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "2c950444-b898-4fab-bdf7-888d68c66abf",
      "number": 3,
      "content": "Bấy giờ những kẻ cùng huyết thống của người còn sống sót",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.303Z",
      "updatedAt": "2024-09-23T06:11:11.303Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "482b3c75-92cb-46d1-9b74-edc81ce137f9",
      "number": 3,
      "content": "Sẽ trở về quê hương đoàn tụ với dân I-sơ-ra-ên.",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.308Z",
      "updatedAt": "2024-09-23T06:11:11.308Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "c82e274f-f68f-4d6c-93ef-f2b31bca3901",
      "number": 4,
      "content": "Người sẽ đứng dậy chăn bầy của mình bằng quyền lực của **CHÚA**,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.313Z",
      "updatedAt": "2024-09-23T06:11:11.313Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "c2ef7aa2-9e51-4df2-a558-beebe21d1baf",
      "number": 4,
      "content": "Bằng uy danh của **CHÚA**, Đức Chúa Trời của Người.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.317Z",
      "updatedAt": "2024-09-23T06:11:11.317Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "8ad2d69a-26f2-4df8-b7d2-075e89c9584f",
      "number": 4,
      "content": "Bấy giờ thiên hạ sẽ được an cư lạc nghiệp,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.322Z",
      "updatedAt": "2024-09-23T06:11:11.322Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "c9f32eb1-8823-4993-96ac-d147b11c8518",
      "number": 4,
      "content": "Vì khi ấy quyền lực của Người sẽ trải rộng đến mọi nơi tận cùng trái đất.",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.328Z",
      "updatedAt": "2024-09-23T06:11:11.328Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "c9d6baca-9eb4-4b67-b971-71393c5eb653",
      "number": 5,
      "content": "Chính Người sẽ đem lại hòa bình.",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.333Z",
      "updatedAt": "2024-09-23T06:11:11.333Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "2b041e86-3fff-4ee5-b728-2f5fc25e8c73",
      "number": 5,
      "content": "Nếu quân A-sy-ri xâm lăng đất nước chúng ta",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.338Z",
      "updatedAt": "2024-09-23T06:11:11.338Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "cf44366f-8604-410c-b349-37f67ca7e872",
      "number": 5,
      "content": "Và bước chân đến các lâu đài của chúng ta,",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.343Z",
      "updatedAt": "2024-09-23T06:11:11.343Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "e16cd8e5-3aa8-40a1-83a9-a704898b2a0b",
      "number": 5,
      "content": "Chúng ta sẽ lập bảy người chăn",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.348Z",
      "updatedAt": "2024-09-23T06:11:11.348Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "ed3f570f-1d32-4090-a748-8c9c17c17386",
      "number": 5,
      "content": "Và tám người lãnh đạo để đối phó với chúng.",
      "order": 4,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.353Z",
      "updatedAt": "2024-09-23T06:11:11.353Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "3451a8c1-fe5b-4717-b22c-19e66ed047ea",
      "number": 6,
      "content": "Họ sẽ đánh bại xứ A-sy-ri bằng gươm;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.358Z",
      "updatedAt": "2024-09-23T06:11:11.358Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "db2527cc-32d3-418b-ab77-4fb0b2b90c60",
      "number": 6,
      "content": "Họ sẽ tiến vào đất của Nim-rốt bằng các cổng chính;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.363Z",
      "updatedAt": "2024-09-23T06:11:11.363Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "be26fdd4-3fc6-4ea8-beb0-bc233ce1bb1b",
      "number": 6,
      "content": "Người sẽ cứu chúng ta khỏi dân A-sy-ri,",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.368Z",
      "updatedAt": "2024-09-23T06:11:11.368Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "36a871c5-3e9b-4d75-be34-835a8ed6b715",
      "number": 6,
      "content": "Nếu chúng đến đất nước chúng ta",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.373Z",
      "updatedAt": "2024-09-23T06:11:11.373Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "27a6e7c0-2797-49c6-805e-6b161ba8a599",
      "number": 6,
      "content": "Hay bước chân vào biên giới chúng ta.",
      "order": 4,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.377Z",
      "updatedAt": "2024-09-23T06:11:11.377Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "67a8a6f5-b167-4970-aeef-0bb188b9c003",
      "number": 7,
      "content": "Bấy giờ những người còn sót lại của Gia-cốp,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.383Z",
      "updatedAt": "2024-09-23T06:11:11.383Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "b2679c49-105d-42cd-96e6-166e8f06ffb9",
      "number": 7,
      "content": "Những người đang sống giữa các dân,",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.393Z",
      "updatedAt": "2024-09-23T06:11:11.393Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "8abb770e-e779-41be-b13f-9626f876b3a2",
      "number": 7,
      "content": "Sẽ như sương móc đến từ **CHÚA**,",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.398Z",
      "updatedAt": "2024-09-23T06:11:11.398Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "23970f41-2d7f-4902-93b6-8407b247b19d",
      "number": 7,
      "content": "Như mưa rào rơi tự do trên cỏ,",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.404Z",
      "updatedAt": "2024-09-23T06:11:11.404Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "2fa6220a-309f-4889-bae6-eae339464799",
      "number": 7,
      "content": "Không bị lệ thuộc vào người nào,",
      "order": 4,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.409Z",
      "updatedAt": "2024-09-23T06:11:11.409Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "f1179309-c472-4d2b-8e24-bb8585a68ba0",
      "number": 7,
      "content": "Không phải đợi chờ để được phép của ai.",
      "order": 5,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.414Z",
      "updatedAt": "2024-09-23T06:11:11.414Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "8056d858-837f-4c4c-b1a5-fbcfedf5c434",
      "number": 8,
      "content": "Lúc ấy những người còn sót lại của Gia-cốp",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.419Z",
      "updatedAt": "2024-09-23T06:11:11.419Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "01b8389a-31ca-4e86-a0a1-ed08f4e92317",
      "number": 8,
      "content": "Ở giữa các quốc gia",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.424Z",
      "updatedAt": "2024-09-23T06:11:11.424Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "efe7ff26-a62e-4c15-b702-902252ec2c8b",
      "number": 8,
      "content": "Sẽ như sư tử ở giữa các thú rừng,",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.428Z",
      "updatedAt": "2024-09-23T06:11:11.428Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "f914a5ff-7538-43a6-9ce7-728805a3bbcf",
      "number": 8,
      "content": "Như sư tử tơ ở giữa các đàn chiên,",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.433Z",
      "updatedAt": "2024-09-23T06:11:11.433Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "7cf8eb83-1e91-42a7-b33a-407e315b51b0",
      "number": 8,
      "content": "Nếu nó đi qua, nó sẽ giày đạp và xé xác các con mồi ra từng mảnh,",
      "order": 4,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.438Z",
      "updatedAt": "2024-09-23T06:11:11.438Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "d1886090-0a68-4c64-848d-b7ca6f323594",
      "number": 8,
      "content": "Không ai cứu chúng được.",
      "order": 5,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.442Z",
      "updatedAt": "2024-09-23T06:11:11.442Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "875c2052-6be8-4d5f-afc8-9e9d67dde9f1",
      "number": 9,
      "content": "“Tay của ngươi sẽ đưa lên đè bẹp những kẻ chống lại ngươi,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.447Z",
      "updatedAt": "2024-09-23T06:11:11.447Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "23d777f5-7a72-41e7-9215-48ace35a9978",
      "number": 9,
      "content": "Tất cả kẻ thù của ngươi sẽ bị tiêu diệt.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.451Z",
      "updatedAt": "2024-09-23T06:11:11.451Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "a0925213-cbae-4068-9d6e-b76df2e02533",
      "number": 10,
      "content": "Trong ngày ấy,” **CHÚA** phán,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.456Z",
      "updatedAt": "2024-09-23T06:11:11.456Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "a6da45ee-6218-4ad1-9013-0ec73a92a503",
      "number": 10,
      "content": "“Ta sẽ tiêu diệt các ngựa chiến giữa ngươi,",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.460Z",
      "updatedAt": "2024-09-23T06:11:11.460Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "0a8f1ac3-c8fd-4999-b64a-240a2cb85b4d",
      "number": 10,
      "content": "Ta sẽ hủy diệt các xe chiến mã của ngươi.",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.465Z",
      "updatedAt": "2024-09-23T06:11:11.465Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "27b370e7-a52b-4aa0-9d8a-01d28c785587",
      "number": 11,
      "content": "Ta sẽ hủy phá các thành trong đất của ngươi,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.470Z",
      "updatedAt": "2024-09-23T06:11:11.470Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "42688c15-10cb-4e69-b0a2-32d93eaf5e9f",
      "number": 11,
      "content": "Ta sẽ làm sụp đổ tất cả các lâu đài kiên cố.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.475Z",
      "updatedAt": "2024-09-23T06:11:11.475Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "f81b8e76-4118-4090-a841-f15148c8afdf",
      "number": 12,
      "content": "Ta sẽ diệt trừ bọn phù thủy thầy pháp khỏi tay ngươi,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.480Z",
      "updatedAt": "2024-09-23T06:11:11.480Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "f0986104-34c7-4845-9c3a-15a7818bc6d3",
      "number": 12,
      "content": "Ngươi sẽ không còn những kẻ đoán mò bói tướng.",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.485Z",
      "updatedAt": "2024-09-23T06:11:11.485Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "a8a89c0c-e1e6-46ef-af2c-d92d6febf83f",
      "number": 13,
      "content": "Ta sẽ tiêu diệt các tượng chạm của ngươi",
      "order": 0,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.490Z",
      "updatedAt": "2024-09-23T06:11:11.490Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "ba4bcb20-f5ed-4f5e-b8f5-fb69e8fba16f",
      "number": 13,
      "content": "Và các trụ thờ của ngươi khỏi giữa ngươi,",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.496Z",
      "updatedAt": "2024-09-23T06:11:11.496Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "58536ce9-dc92-4bac-acf5-97eda75259c7",
      "number": 13,
      "content": "Ngươi sẽ không quỳ lạy trước các tác phẩm",
      "order": 2,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.500Z",
      "updatedAt": "2024-09-23T06:11:11.500Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "d54b021f-231d-4985-be59-f217687dd1ab",
      "number": 13,
      "content": "Do tay ngươi làm ra nữa.",
      "order": 3,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.505Z",
      "updatedAt": "2024-09-23T06:11:11.505Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "6c8c413a-6186-472b-98ff-eb94cdfce26b",
      "number": 14,
      "content": "Ta sẽ cho nhổ lên tận gốc các trụ thờ Nữ Thần A-sê-ra,",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.510Z",
      "updatedAt": "2024-09-23T06:11:11.510Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "a1dac2e0-e080-44fc-aa47-03ed0ba9acd0",
      "number": 14,
      "content": "Và tiêu diệt các thành thờ thần tượng của ngươi.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.515Z",
      "updatedAt": "2024-09-23T06:11:11.515Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "c64b9738-42e3-42f8-9d98-e378f7fb044d",
      "number": 15,
      "content": "Trong cơn giận và cơn thịnh nộ,",
      "order": 0,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.521Z",
      "updatedAt": "2024-09-23T06:11:11.521Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    },
    {
      "id": "2eac5b82-17f9-4d7e-8678-f986cbc5a865",
      "number": 15,
      "content": "Ta sẽ thực hiện cuộc báo thù những dân không vâng lời Ta.”",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:11:11.525Z",
      "updatedAt": "2024-09-23T06:11:11.525Z",
      "chapterId": "638d849e-b24c-46e2-9e7b-2834904e5e81"
    }
  ],
