    },
    {
      "id": "0eea47e7-4c81-4f6a-b7f6-3c201403d024",
      "content": "Dân Sa-phia thời xưa nổi tiếng là mặc đẹp",
      "position": 13,
      "order": 1,
      "createdAt": "2024-09-23T06:10:50.193Z",
      "updatedAt": "2024-09-23T06:10:50.193Z",
      "verseId": "a18e7470-5b7a-45e7-b602-c718f4e1603f",
      "chapterId": "4f0ed55b-1931-474a-8215-cc6626178bbc"
    },
    {
      "id": "34ef4972-c4a4-4e58-bea1-831dfe6f0561",
      "content": "Xa-a-nan có nghĩa là “ra đi”",
      "position": 14,
      "order": 2,
      "createdAt": "2024-09-23T06:10:50.214Z",
      "updatedAt": "2024-09-23T06:10:50.214Z",
      "verseId": "72d09354-179f-41c2-9d76-d096469bf1c9",
      "chapterId": "4f0ed55b-1931-474a-8215-cc6626178bbc"
    },
    {
      "id": "066cdfa6-4131-4d05-87bf-8f729f943efb",
      "content": "Bết Ê-xên có nghĩa là “nhà chật”",
      "position": 15,
      "order": 3,
      "createdAt": "2024-09-23T06:10:50.228Z",
      "updatedAt": "2024-09-23T06:10:50.228Z",
      "verseId": "7e6fb7a9-c2e5-4fa7-890f-682aa2c018d6",
      "chapterId": "4f0ed55b-1931-474a-8215-cc6626178bbc"
    },
    {
      "id": "b816c63d-9f86-4b04-8f64-5d16ba75bdad",
      "content": "nt: của ngươi",
      "position": 21,
      "order": 4,
      "createdAt": "2024-09-23T06:10:50.238Z",
      "updatedAt": "2024-09-23T06:10:50.238Z",
      "verseId": "f34c68e8-a6e0-4cd2-8f08-fb696bde146a",
      "chapterId": "4f0ed55b-1931-474a-8215-cc6626178bbc"
    },
    {
      "id": "de6070f7-7a03-4858-9721-a47f3b715774",
      "content": "Ách-xíp có nghĩa là “dối gạt”",
      "position": 53,
      "order": 5,
      "createdAt": "2024-09-23T06:10:50.308Z",
      "updatedAt": "2024-09-23T06:10:50.308Z",
      "verseId": "21c33ead-cd9e-4823-901c-9fd702c75e18",
      "chapterId": "4f0ed55b-1931-474a-8215-cc6626178bbc"
    },
    {
      "id": "32e99f26-116e-48e8-8beb-f3e2acd385db",
      "content": "Ma-rê-sa có nghĩa là “kẻ chinh phục”",
      "position": 20,
      "order": 6,
      "createdAt": "2024-09-23T06:10:50.322Z",
      "updatedAt": "2024-09-23T06:10:50.322Z",
      "verseId": "c27bd48a-d889-4278-9859-419441b9b5d2",
      "chapterId": "4f0ed55b-1931-474a-8215-cc6626178bbc"
    }
  ],
