  },
  "verses": [
    {
      "id": "e68f0d73-83ae-477f-9c67-406d150cdeff",
      "number": 1,
      "content": "Lạy **CHÚA**, Chúa của chúng con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:58.988Z",
      "updatedAt": "2024-09-23T05:44:58.988Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "20834458-3f96-4911-952a-d1a9bb23fd9a",
      "number": 9,
      "content": "Lạy **CHÚA**, Chúa của chúng con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.088Z",
      "updatedAt": "2024-09-23T05:44:59.088Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "d1acb208-38c4-4636-a367-0efd2291b0a4",
      "number": 9,
      "content": "Danh Ngài thật uy nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.091Z",
      "updatedAt": "2024-09-23T05:44:59.091Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "10ce0ac8-d32e-443c-9b85-d0a7e53659e4",
      "number": 1,
      "content": "Danh Ngài thật uy nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.019Z",
      "updatedAt": "2024-09-23T05:44:59.019Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "e6ad9f66-424f-436f-8ecf-4db8fd5544cf",
      "number": 1,
      "content": "Ngài đã đặt vinh quang Ngài trên các tầng trời!",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.024Z",
      "updatedAt": "2024-09-23T05:44:59.024Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "5c4045a9-b709-4526-9e7d-c1eced65e388",
      "number": 2,
      "content": "Từ miệng các trẻ thơ và các ấu nhi Ngài đã biểu lộ quyền năng,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.028Z",
      "updatedAt": "2024-09-23T05:44:59.028Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "0973077f-9915-4970-b5bf-c049aa857b7f",
      "number": 2,
      "content": "Để quân chống lại Ngài thấy rõ,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.032Z",
      "updatedAt": "2024-09-23T05:44:59.032Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "cf82b6cd-2f34-445b-b3a7-4a499f86a668",
      "number": 2,
      "content": "Để kẻ thù và kẻ báo thù phải im hơi bỏ cuộc.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.037Z",
      "updatedAt": "2024-09-23T05:44:59.037Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "6d19ba15-064b-4aa2-b403-20eb54487dce",
      "number": 3,
      "content": "Khi con lặng ngắm cõi trời cao, công việc của ngón tay Ngài,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.041Z",
      "updatedAt": "2024-09-23T05:44:59.041Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "7abce61f-0c87-41c3-b04f-de9ce9018bf1",
      "number": 3,
      "content": "Mặt trăng và các ngôi sao mà Ngài đã đặt trên không trung;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.046Z",
      "updatedAt": "2024-09-23T05:44:59.046Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "d41be862-6874-4560-9208-f1480083dc32",
      "number": 4,
      "content": "Loài người là gì mà Ngài phải quan tâm đến?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.050Z",
      "updatedAt": "2024-09-23T05:44:59.050Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "2fec7409-28ec-41b4-89dd-f8061665ebbc",
      "number": 4,
      "content": "Con của loài người là chi mà Ngài phải bận lòng?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.055Z",
      "updatedAt": "2024-09-23T05:44:59.055Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "fbeb29cd-3214-4635-97a2-66841e523d44",
      "number": 5,
      "content": "Ngài đã dựng nên loài người thấp hơn các vị thần một chút;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.059Z",
      "updatedAt": "2024-09-23T05:44:59.059Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "ad577951-eafe-4960-be9f-14f7092180ef",
      "number": 5,
      "content": "Ngài đội trên đầu loài người vinh hiển và tôn trọng.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.067Z",
      "updatedAt": "2024-09-23T05:44:59.067Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "1d5e0992-3243-4d12-ad5e-8ba1507de482",
      "number": 6,
      "content": "Ngài cho loài người quản trị mọi vật do tay Ngài dựng nên;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.071Z",
      "updatedAt": "2024-09-23T05:44:59.071Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "85b47038-239f-4250-8d46-04a8a0516242",
      "number": 6,
      "content": "Ngài bắt muôn vật phục dưới chân loài người,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.073Z",
      "updatedAt": "2024-09-23T05:44:59.073Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "668f22d1-91cf-48ea-9a59-bcb762bdbd60",
      "number": 7,
      "content": "Tức tất cả chiên và bò,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.076Z",
      "updatedAt": "2024-09-23T05:44:59.076Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "6da788b0-5f76-427b-b1d7-92c614771f33",
      "number": 7,
      "content": "Cùng các thú vật ngoài đồng,",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.079Z",
      "updatedAt": "2024-09-23T05:44:59.079Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "15c13e6a-9d74-4c2a-82bd-8b116d86aff5",
      "number": 8,
      "content": "Các loài chim trên trời và mọi loài cá dưới biển,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.082Z",
      "updatedAt": "2024-09-23T05:44:59.082Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    },
    {
      "id": "8fc08a6a-4605-44c6-b48e-309702b59c88",
      "number": 8,
      "content": "Tức mọi vật bơi lội trong các dòng nước biển.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:59.085Z",
      "updatedAt": "2024-09-23T05:44:59.085Z",
      "chapterId": "ddf5fc34-eecf-48bb-a5f0-3e4aff73ead3"
    }
  ],
