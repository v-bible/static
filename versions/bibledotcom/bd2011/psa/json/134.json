  "verses": [
    {
      "id": "461ef65d-3aad-4068-81d1-020de6df1c7e",
      "number": 1,
      "content": "Hỡi tất cả tôi tớ **CHÚA**,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:46.005Z",
      "updatedAt": "2024-09-23T05:42:46.005Z",
      "chapterId": "def811af-de4b-429d-afbd-ec20a0594068"
    },
    {
      "id": "9358b993-1c40-4036-8257-288409d56bd2",
      "number": 1,
      "content": "Những người phục vụ trong nhà **CHÚA** ban đêm, xin hãy chúc tụng **CHÚA**.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:46.021Z",
      "updatedAt": "2024-09-23T05:42:46.021Z",
      "chapterId": "def811af-de4b-429d-afbd-ec20a0594068"
    },
    {
      "id": "e54be2eb-db48-4b23-b2a1-74fdadd7940c",
      "number": 2,
      "content": "Xin đưa cao đôi tay quý vị lên trong nơi thánh",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:46.023Z",
      "updatedAt": "2024-09-23T05:42:46.023Z",
      "chapterId": "def811af-de4b-429d-afbd-ec20a0594068"
    },
    {
      "id": "2610d648-c38d-49ba-981f-09b6cecfa367",
      "number": 2,
      "content": "Và chúc tụng **CHÚA**.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:46.025Z",
      "updatedAt": "2024-09-23T05:42:46.025Z",
      "chapterId": "def811af-de4b-429d-afbd-ec20a0594068"
    },
    {
      "id": "afbfa4ef-3752-46a5-96c1-dd03827b5c64",
      "number": 3,
      "content": "Nguyện **CHÚA**, Đấng dựng nên trời và đất,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:46.027Z",
      "updatedAt": "2024-09-23T05:42:46.027Z",
      "chapterId": "def811af-de4b-429d-afbd-ec20a0594068"
    },
    {
      "id": "25496377-bc94-4902-8733-ac6c7bd8d258",
      "number": 3,
      "content": "Ban phước cho quý vị từ Si-ôn.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:46.029Z",
      "updatedAt": "2024-09-23T05:42:46.029Z",
      "chapterId": "def811af-de4b-429d-afbd-ec20a0594068"
    }
  ],
