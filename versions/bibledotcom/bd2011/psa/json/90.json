{
  "chapter": {
    "id": "026b1e88-e87d-497e-9d6e-2f4dbd228a38",
    "number": 90,
    "ref": "https://www.bible.com/bible/19/PSA.90.BD2011",
    "createdAt": "2024-09-23T05:08:40.522Z",
    "updatedAt": "2024-09-23T05:08:40.522Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "916d6a01-7dda-460f-85dc-f1003c473a64",
      "number": 1,
      "content": "Lạy Chúa, Ngài là nơi cư trú của chúng con",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.905Z",
      "updatedAt": "2024-09-23T05:51:50.905Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "184c6585-5392-433c-8566-92d1c7c81753",
      "number": 1,
      "content": "Trải qua mọi thế hệ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.916Z",
      "updatedAt": "2024-09-23T05:51:50.916Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "73e49016-94bf-4c23-9285-e397fed16e53",
      "number": 2,
      "content": "Trước khi núi non thành hình,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.918Z",
      "updatedAt": "2024-09-23T05:51:50.918Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "b61f1b82-7fc6-4b5b-b980-3b3533299eb9",
      "number": 2,
      "content": "Trước khi Ngài tạo thành trái đất và thế gian,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.920Z",
      "updatedAt": "2024-09-23T05:51:50.920Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "2f3dd11b-bddb-40af-b885-f6259c0174f1",
      "number": 2,
      "content": "Từ vô cực quá khứ đến vô cực tương lai Ngài là Đức Chúa Trời.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.922Z",
      "updatedAt": "2024-09-23T05:51:50.922Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "e4d1b7c2-afa4-400b-abc8-1484e2dfaebd",
      "number": 3,
      "content": "Ngài làm cho loài người trở về cát bụi và phán rằng,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.924Z",
      "updatedAt": "2024-09-23T05:51:50.924Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "242ba195-14d2-4b0f-a0d7-3a1529946105",
      "number": 3,
      "content": "“Hỡi con cái loài người, hãy trở về.”",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.926Z",
      "updatedAt": "2024-09-23T05:51:50.926Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "e529fd57-df39-43e6-b01d-6d773747b85f",
      "number": 4,
      "content": "Vì một ngàn năm trước mắt Ngài",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.929Z",
      "updatedAt": "2024-09-23T05:51:50.929Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "d0e0af68-8617-4253-9b7a-5de9fc9b5f6f",
      "number": 4,
      "content": "Tựa như ngày hôm qua, đã qua rồi,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.933Z",
      "updatedAt": "2024-09-23T05:51:50.933Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "835fe15c-3536-49bf-9754-b705f496ccf3",
      "number": 4,
      "content": "Hoặc như một canh đêm.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.938Z",
      "updatedAt": "2024-09-23T05:51:50.938Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "ca336b09-fa79-4b20-9be2-358c6251f993",
      "number": 5,
      "content": "Ngài làm cho đời người trôi nhanh như dòng nước lũ, thoáng qua như một giấc ngủ.",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.943Z",
      "updatedAt": "2024-09-23T05:51:50.943Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "7ff0f63a-dd27-4d5d-bdbd-bd2ce0d7fd68",
      "number": 5,
      "content": "Đời người như cỏ mọc ban mai,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.948Z",
      "updatedAt": "2024-09-23T05:51:50.948Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "79915b4f-1603-4268-9fdb-e101db2678e4",
      "number": 6,
      "content": "Sáng sớm chúng mọc lên và xanh tươi,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.952Z",
      "updatedAt": "2024-09-23T05:51:50.952Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "e07fb17c-4bd3-4e69-83df-df87e2766248",
      "number": 6,
      "content": "Chiều lại chúng héo khô và tàn tạ.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.957Z",
      "updatedAt": "2024-09-23T05:51:50.957Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "36e0349c-2db1-4f42-9cdd-853572939e56",
      "number": 7,
      "content": "Thật vậy đời sống chúng con chóng tàn vì cơn giận của Ngài.",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.965Z",
      "updatedAt": "2024-09-23T05:51:50.965Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "5c9f2ed2-9e54-4b90-a964-5defb8fdcff6",
      "number": 7,
      "content": "Chúng con kinh hãi vì cơn thịnh nộ của Ngài.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.970Z",
      "updatedAt": "2024-09-23T05:51:50.970Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "83cdc0e3-2b5e-47ec-8698-d390720491c2",
      "number": 8,
      "content": "Ngài đã để những tội lỗi chúng con trước mặt Ngài,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.975Z",
      "updatedAt": "2024-09-23T05:51:50.975Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "8e2d6729-3377-4f14-8550-043fee6dace7",
      "number": 8,
      "content": "Các tội bí mật chúng con trong ánh sáng của thánh nhan Ngài.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.980Z",
      "updatedAt": "2024-09-23T05:51:50.980Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "730e9f05-2b82-4af9-960a-076fddbe6f02",
      "number": 9,
      "content": "Những ngày chúng con qua đi dưới cơn giận của Ngài;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.984Z",
      "updatedAt": "2024-09-23T05:51:50.984Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "405b6658-4bdc-451c-9fae-4dc23ab3cc9b",
      "number": 9,
      "content": "Những năm chúng con kết thúc như một tiếng thở dài.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.989Z",
      "updatedAt": "2024-09-23T05:51:50.989Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "21c124e6-82f4-4f1d-b2b6-ec0a930db2ec",
      "number": 10,
      "content": "Tuổi tác chúng con đến được bảy mươi,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.993Z",
      "updatedAt": "2024-09-23T05:51:50.993Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "b956f8bd-6702-4fcd-a9eb-57b49add9103",
      "number": 10,
      "content": "Còn nếu mạnh khỏe thì đến tám mươi,",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:50.998Z",
      "updatedAt": "2024-09-23T05:51:50.998Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "4db4fe73-ce6f-44c0-b5b6-d17537c836a6",
      "number": 10,
      "content": "Nhưng niềm kiêu hãnh của chúng chẳng qua là lao khổ và buồn thảm;",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.003Z",
      "updatedAt": "2024-09-23T05:51:51.003Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "09ad0a6e-d94d-44f6-bc6d-1897cd978deb",
      "number": 10,
      "content": "Vì đời sống chóng qua, rồi chúng con bay mất đi.",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.008Z",
      "updatedAt": "2024-09-23T05:51:51.008Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "846482e5-3d29-43dd-8d1d-7c826acbc690",
      "number": 11,
      "content": "Ai biết được sức mạnh của cơn giận Ngài?",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.012Z",
      "updatedAt": "2024-09-23T05:51:51.012Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "04008df5-1e45-4561-b2c2-aef27ed03aa4",
      "number": 11,
      "content": "Cơn thịnh nộ Ngài lớn bao nhiêu, sự khiếp sợ Ngài càng lớn bấy nhiêu.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.017Z",
      "updatedAt": "2024-09-23T05:51:51.017Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "ab2cb44f-5522-45a6-8a9e-576a9eac26f6",
      "number": 12,
      "content": "Xin dạy chúng con biết đếm số ngày chúng con,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.022Z",
      "updatedAt": "2024-09-23T05:51:51.022Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "5cc822fc-496d-47e4-91e2-a22e6f3d0778",
      "number": 12,
      "content": "Để chúng con được lòng khôn ngoan.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.027Z",
      "updatedAt": "2024-09-23T05:51:51.027Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "6a04486a-d3dc-448b-bebc-8a5a9e15ac46",
      "number": 13,
      "content": "Xin trở lại, **CHÚA** ôi; Ngài còn giận chúng con đến bao lâu nữa?",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.032Z",
      "updatedAt": "2024-09-23T05:51:51.032Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "eb2c7ad9-9fda-41da-b4d3-ac2dc1dfa0ac",
      "number": 13,
      "content": "Xin dủ lòng thương xót các tôi tớ Ngài.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.037Z",
      "updatedAt": "2024-09-23T05:51:51.037Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "f978fbd5-3fac-4b31-93f2-3ff71e97a868",
      "number": 14,
      "content": "Mỗi buổi sáng xin cho chúng con được no nê bằng ơn thương xót của Ngài,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.042Z",
      "updatedAt": "2024-09-23T05:51:51.042Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "b0637e82-e97f-430f-b5a9-f70b81ebed4e",
      "number": 14,
      "content": "Để chúng con được hân hoan và vui thỏa trọn những ngày của mình.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.048Z",
      "updatedAt": "2024-09-23T05:51:51.048Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "90acfc01-5c01-4407-8423-7f4e79d19cfc",
      "number": 15,
      "content": "Xin ban cho chúng con niềm vui để bù lại những ngày Ngài phó chúng con cho hoạn nạn,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.053Z",
      "updatedAt": "2024-09-23T05:51:51.053Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "0fa4e5ac-aa8c-4a8a-b040-54e0f088089a",
      "number": 15,
      "content": "Và những năm chúng con thấy nỗi bất hạnh.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.058Z",
      "updatedAt": "2024-09-23T05:51:51.058Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "395bd9bc-d9f9-46b2-a3cc-285e7aadfba2",
      "number": 16,
      "content": "Nguyện công việc của Ngài được tỏ ra cho các tôi tớ Ngài.",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.062Z",
      "updatedAt": "2024-09-23T05:51:51.062Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "bbcec998-5bc7-4726-9062-0cc5954d0ca8",
      "number": 16,
      "content": "Nguyện vinh quang của Ngài tỏa rạng trên con cháu họ.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.067Z",
      "updatedAt": "2024-09-23T05:51:51.067Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "64f4c65a-b915-4eaf-b776-9807accad12b",
      "number": 17,
      "content": "Nguyện phước hạnh của **Chúa**, Đức Chúa Trời của chúng con, tuôn đổ trên chúng con.",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.072Z",
      "updatedAt": "2024-09-23T05:51:51.072Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "058a9588-568e-4208-8399-b2154041ba00",
      "number": 17,
      "content": "Cầu xin Ngài lập vững công việc của tay chúng con;",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:51.076Z",
      "updatedAt": "2024-09-23T05:51:51.076Z",
      "chapterId": "026b1e88-e87d-497e-9d6e-2f4dbd228a38"
    },
    {
      "id": "4244b836-e230-42bd-92d0-02822233f346",
      "number": 17,
      "content": "Thật vậy xin lập vững công việc của tay chúng con.",
      "order": 2,
      "parNumber": 40,
