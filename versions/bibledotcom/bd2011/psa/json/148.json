{
  "chapter": {
    "id": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520",
    "number": 148,
    "ref": "https://www.bible.com/bible/19/PSA.148.BD2011",
    "createdAt": "2024-09-23T05:08:40.708Z",
    "updatedAt": "2024-09-23T05:08:40.708Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "31861458-bd66-424d-9fbc-58b6c2439619",
      "number": 1,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.055Z",
      "updatedAt": "2024-09-23T05:44:21.055Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "69fd8852-a554-493f-8440-398fb1ba280d",
      "number": 14,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 3,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.219Z",
      "updatedAt": "2024-09-23T05:44:21.219Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "9dab8a5c-8efa-4575-9e3d-0b9481bc811c",
      "number": 1,
      "content": "Hãy ca ngợi **CHÚA** trên các tầng trời!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.070Z",
      "updatedAt": "2024-09-23T05:44:21.070Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "73af96e0-f2f8-4ece-9a33-1657b09f5bdb",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong cõi trời cao!",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.072Z",
      "updatedAt": "2024-09-23T05:44:21.072Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "13ad6c47-f903-4cb2-b86c-a00d1964285d",
      "number": 2,
      "content": "Hãy ca ngợi Ngài, hỡi tất cả các thiên sứ của Ngài!",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.076Z",
      "updatedAt": "2024-09-23T05:44:21.076Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "03422c29-7544-492e-b781-d3dcd880369b",
      "number": 2,
      "content": "Hãy ca ngợi Ngài, hỡi tất cả các thiên binh của Ngài!",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.080Z",
      "updatedAt": "2024-09-23T05:44:21.080Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "c6e546ce-822c-48a4-9ab8-af36e5d01302",
      "number": 3,
      "content": "Hãy ca ngợi Ngài, hỡi mặt trời và mặt trăng!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.085Z",
      "updatedAt": "2024-09-23T05:44:21.085Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "b56cb633-4fba-49f0-b08a-7ec2e33e4c7c",
      "number": 3,
      "content": "Hãy ca ngợi Ngài, hỡi tất cả các ngươi, những vì sao sáng!",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.090Z",
      "updatedAt": "2024-09-23T05:44:21.090Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "de7faeca-7c71-4498-959d-77fd6e65a402",
      "number": 4,
      "content": "Hãy ca ngợi Ngài, hỡi các ngươi, các tầng trời cao thăm thẳm!",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.095Z",
      "updatedAt": "2024-09-23T05:44:21.095Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "b9e3ae11-8509-41c3-8d5a-1d83e90d755f",
      "number": 4,
      "content": "Và hỡi các ngươi, các nguồn nước trên các tầng trời!",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.100Z",
      "updatedAt": "2024-09-23T05:44:21.100Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "7828f08c-5ef6-4a30-b04d-bb49b7dcb05a",
      "number": 13,
      "content": "Tất cả hãy ca ngợi danh **CHÚA**,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.190Z",
      "updatedAt": "2024-09-23T05:44:21.190Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "99fcc09b-e7a2-4860-959c-2519950e5165",
      "number": 5,
      "content": "Tất cả hãy ca ngợi danh **CHÚA**,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.105Z",
      "updatedAt": "2024-09-23T05:44:21.105Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "00063c9c-4ff4-4f06-84ce-a657ae3d76a7",
      "number": 5,
      "content": "Vì Ngài ra lịnh, mọi vật được dựng nên.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.110Z",
      "updatedAt": "2024-09-23T05:44:21.110Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "89973434-f40e-471b-b951-9f4e7ee58308",
      "number": 6,
      "content": "Ngài thiết lập chúng, để chúng trường tồn mãi mãi;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.114Z",
      "updatedAt": "2024-09-23T05:44:21.114Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "6a4d8b4d-8de9-4ee1-ae3d-8af2b8ed1712",
      "number": 6,
      "content": "Ngài ban một định luật, và chúng không bao giờ bị đào thải.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.119Z",
      "updatedAt": "2024-09-23T05:44:21.119Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "600bad1f-cd9a-4d6e-aca3-39ebbe967bc3",
      "number": 7,
      "content": "Hãy ca ngợi **CHÚA** ở dưới đất,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.124Z",
      "updatedAt": "2024-09-23T05:44:21.124Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "314ad9b1-d56e-4ba7-85ef-43001b9ac3a3",
      "number": 7,
      "content": "Hỡi các thủy quái và các vực sâu,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.129Z",
      "updatedAt": "2024-09-23T05:44:21.129Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "d0814318-7eef-4dc0-ba1a-4acae6839b69",
      "number": 8,
      "content": "Hỡi lửa và mưa đá, hỡi tuyết và mây,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.140Z",
      "updatedAt": "2024-09-23T05:44:21.140Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "53a7ba9f-6c84-45fb-b84c-b8f92c8a62b5",
      "number": 8,
      "content": "Hỡi gió bão luôn vâng lời Ngài,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.145Z",
      "updatedAt": "2024-09-23T05:44:21.145Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "5e076207-af35-400f-ac3f-e2808025feab",
      "number": 9,
      "content": "Hỡi các núi non và mọi đồi nổng,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.151Z",
      "updatedAt": "2024-09-23T05:44:21.151Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "4dcffdfb-83fb-4a26-ac98-d69e0bc07ddd",
      "number": 9,
      "content": "Hỡi những cây ra trái và những cây bá hương,",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.156Z",
      "updatedAt": "2024-09-23T05:44:21.156Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "2d1b23e6-df2c-4bdb-8937-225c18b5633d",
      "number": 10,
      "content": "Hỡi những thú rừng và mọi loài súc vật,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.161Z",
      "updatedAt": "2024-09-23T05:44:21.161Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "843b8b9b-3a0c-477b-a411-d05f64877257",
      "number": 10,
      "content": "Hỡi những côn trùng và những điểu thú,",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.166Z",
      "updatedAt": "2024-09-23T05:44:21.166Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "778b9a24-3df8-44ef-bb78-8fcad1994f0c",
      "number": 11,
      "content": "Hỡi các vua và mọi dân trên đất,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.171Z",
      "updatedAt": "2024-09-23T05:44:21.171Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "abb879c7-5cbb-4bbe-95c1-e9c0fc268915",
      "number": 11,
      "content": "Hỡi các lãnh chúa và các lãnh tụ trên thế gian,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.176Z",
      "updatedAt": "2024-09-23T05:44:21.176Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "306b8284-1747-48e4-9164-411f80a1a1fb",
      "number": 12,
      "content": "Hỡi tất cả thanh thiếu niên nam nữ,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.181Z",
      "updatedAt": "2024-09-23T05:44:21.181Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "9b5905c0-8d43-45af-8318-c47610126388",
      "number": 12,
      "content": "Hỡi quý cụ lão thành và hỡi những trẻ còn thơ.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.186Z",
      "updatedAt": "2024-09-23T05:44:21.186Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "bc2b19d7-e19e-4c13-9e4a-df208c099155",
      "number": 13,
      "content": "Vì chỉ có danh Ngài mới đáng được tôn cao;",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.195Z",
      "updatedAt": "2024-09-23T05:44:21.195Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "198bf9ba-56bd-4673-b84a-ec8262a0e8fe",
      "number": 13,
      "content": "Vinh quang Ngài vượt trỗi trên cả đất và trời.",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.200Z",
      "updatedAt": "2024-09-23T05:44:21.200Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "cc30872b-6a03-403e-b223-05999fe02587",
      "number": 14,
      "content": "Ngài nhấc cao sừng của dân Ngài lên,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.204Z",
      "updatedAt": "2024-09-23T05:44:21.204Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "04c26a2c-754d-43c8-9f00-b90dc815080d",
      "number": 14,
      "content": "Nên hãy ca ngợi Ngài, hỡi tất cả thánh dân Ngài,",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.209Z",
      "updatedAt": "2024-09-23T05:44:21.209Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    },
    {
      "id": "cee16d42-1ece-4ee0-90dd-74de4e042cbb",
      "number": 14,
      "content": "Con cháu của I-sơ-ra-ên, một dân gần gũi bên Ngài.",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:21.214Z",
      "updatedAt": "2024-09-23T05:44:21.214Z",
      "chapterId": "951c97d4-f660-4bbc-a8fc-01ac8b5cf520"
    }
  ],
