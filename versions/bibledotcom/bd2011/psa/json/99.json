    },
    {
      "id": "bd3db95b-f8fd-4b26-8aab-2ed9d2657d94",
      "content": "CHÚA",
      "order": 2,
      "createdAt": "2024-09-23T05:52:41.842Z",
      "updatedAt": "2024-09-23T05:52:41.842Z",
      "verseId": "90af576a-b4ce-4369-884c-74bbcc4fdf53",
      "chapterId": "60168921-4022-4f45-b2d5-1aa8329d4185"
    },
    {
      "id": "30fdd57b-b290-42f2-8053-70ff0f6633bb",
      "content": "Vì Sự Gắn Bó của Ngài với I-sơ-ra-ên",
      "order": 3,
      "createdAt": "2024-09-23T05:52:41.844Z",
      "updatedAt": "2024-09-23T05:52:41.844Z",
