{
  "chapter": {
    "id": "47851d35-437a-4f2d-aec8-42df670e968a",
    "number": 84,
    "ref": "https://www.bible.com/bible/19/PSA.84.BD2011",
    "createdAt": "2024-09-23T05:08:40.506Z",
    "updatedAt": "2024-09-23T05:08:40.506Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "4390a1ff-6425-420a-a4c0-fb7f017a9443",
      "number": 12,
      "content": "Lạy **CHÚA** của các đạo quân,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.764Z",
      "updatedAt": "2024-09-23T05:51:17.764Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "08173764-0f3d-4dcf-b5d7-21f0118d0664",
      "number": 1,
      "content": "Lạy **CHÚA** của các đạo quân,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.573Z",
      "updatedAt": "2024-09-23T05:51:17.573Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "13682107-2e5d-4c0d-a67c-659cd9631e46",
      "number": 1,
      "content": "Chỗ ở của Ngài thật đáng yêu mến thay!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.591Z",
      "updatedAt": "2024-09-23T05:51:17.591Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "43c8c22d-9d74-48b2-aa3d-11b92ab5a4f1",
      "number": 2,
      "content": "Linh hồn con khao khát và thèm muốn",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.596Z",
      "updatedAt": "2024-09-23T05:51:17.596Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "87ffae6f-cc28-4ec4-9e24-a2587ec2eacf",
      "number": 2,
      "content": "Được vào sân của nhà **CHÚA** biết bao.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.601Z",
      "updatedAt": "2024-09-23T05:51:17.601Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "e82ce84d-d41a-4a7e-a07f-f15053b654a9",
      "number": 2,
      "content": "Lòng con và thân xác con ước ao được về đó",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.606Z",
      "updatedAt": "2024-09-23T05:51:17.606Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "b8f7a5cf-7fbc-453e-986c-d835fb61899c",
      "number": 2,
      "content": "Để reo mừng chúc tụng Đức Chúa Trời hằng sống biết dường nào.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.611Z",
      "updatedAt": "2024-09-23T05:51:17.611Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "6deade3c-bff1-40a5-9c9b-678e8c83baf5",
      "number": 3,
      "content": "Ngay cả con chim sẻ còn kiếm được một chỗ ở,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.615Z",
      "updatedAt": "2024-09-23T05:51:17.615Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "5c9361e5-2f8e-4523-807f-d7d2baa2bf06",
      "number": 3,
      "content": "Con chim én còn có nơi để làm tổ",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.620Z",
      "updatedAt": "2024-09-23T05:51:17.620Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "e2f25141-2c5b-4e84-b18b-41f81e5e86e3",
      "number": 3,
      "content": "Cho con nó gần bàn thờ Ngài,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.625Z",
      "updatedAt": "2024-09-23T05:51:17.625Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "49b416af-97c0-44b8-a1b9-d33de7f62674",
      "number": 3,
      "content": "Ôi **CHÚA** của các đạo quân,",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.630Z",
      "updatedAt": "2024-09-23T05:51:17.630Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "923cbe4f-e727-4b3d-a485-74d5e4d2188c",
      "number": 3,
      "content": "Vua của con và Đức Chúa Trời của con.",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.635Z",
      "updatedAt": "2024-09-23T05:51:17.635Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "5acae425-7087-4c6f-b2fe-5757cc37e34d",
      "number": 4,
      "content": "Phước cho người nào được ở trong nhà Ngài!",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.640Z",
      "updatedAt": "2024-09-23T05:51:17.640Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "eea149c2-2719-4ccb-9ac6-e938cb7a4419",
      "number": 4,
      "content": "Họ sẽ được ca ngợi Ngài mãi mãi. (Sê-la)",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.645Z",
      "updatedAt": "2024-09-23T05:51:17.645Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "1a63935c-443e-48cd-bc08-902d3e72dc05",
      "number": 5,
      "content": "Phước thay cho người cậy Ngài làm sức mạnh mình,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.650Z",
      "updatedAt": "2024-09-23T05:51:17.650Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "f2268da4-bd36-40b5-8e0c-94cdf87a08c5",
      "number": 5,
      "content": "Và để lòng mình vào cuộc hành trình về nhà Ngài.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.655Z",
      "updatedAt": "2024-09-23T05:51:17.655Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "d482b00a-6878-4bf8-a4b5-79280c8a0331",
      "number": 6,
      "content": "Khi họ đi qua thung lũng buồn đau than khóc,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.660Z",
      "updatedAt": "2024-09-23T05:51:17.660Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "cb166c60-fcd1-485c-b06d-7a7c345af1b6",
      "number": 6,
      "content": "Họ sẽ biến nó thành nơi có những suối nước trào tuôn,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.664Z",
      "updatedAt": "2024-09-23T05:51:17.664Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "02aa0439-251f-4366-942d-3f8509a4dd24",
      "number": 6,
      "content": "Như mưa đầu mùa biến nơi ấy thành vùng có nhiều vũng nước.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.669Z",
      "updatedAt": "2024-09-23T05:51:17.669Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "856c33e0-2521-4c00-9a84-06e1b6b04613",
      "number": 7,
      "content": "Họ càng đi tới, sức lực họ càng tăng lên,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.675Z",
      "updatedAt": "2024-09-23T05:51:17.675Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "cbb72835-c9b2-4597-85b3-75061974c3d6",
      "number": 7,
      "content": "Cho đến khi mọi người ra mắt Đức Chúa Trời tại Si-ôn.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.680Z",
      "updatedAt": "2024-09-23T05:51:17.680Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "2b3c1b59-e6fe-4be7-9482-455b366a42d3",
      "number": 8,
      "content": "Lạy **CHÚA**, Đức Chúa Trời của các đạo quân, xin lắng nghe lời cầu nguyện của con;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.685Z",
      "updatedAt": "2024-09-23T05:51:17.685Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "14a3ced7-5146-4a1a-bd96-0e93aee562c9",
      "number": 8,
      "content": "Đức Chúa Trời của Gia-cốp ôi, xin nghiêng tai nghe tiếng cầu xin của con. (Sê-la)",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.690Z",
      "updatedAt": "2024-09-23T05:51:17.690Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "5f63c757-f0bb-48d5-a28b-c785f39850c2",
      "number": 9,
      "content": "Đức Chúa Trời ôi, xin đoái xem người lãnh đạo của chúng con;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.696Z",
      "updatedAt": "2024-09-23T05:51:17.696Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "b0dc73de-6f2a-44b1-a462-65e1febab435",
      "number": 9,
      "content": "Xin cúi nhìn vào mặt người được xức dầu của Ngài.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.706Z",
      "updatedAt": "2024-09-23T05:51:17.706Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "f063a9d0-2c82-4130-84ba-fd211307b4d4",
      "number": 10,
      "content": "Vì một ngày được ở trong các sân của nhà Ngài",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.711Z",
      "updatedAt": "2024-09-23T05:51:17.711Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "b9df7c8b-501c-4e93-986b-460f2c6fe104",
      "number": 10,
      "content": "Thật đáng hơn cả ngàn ngày ở những nơi khác.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.717Z",
      "updatedAt": "2024-09-23T05:51:17.717Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "a0be7467-7d97-457c-bea0-8c2933ed1f72",
      "number": 10,
      "content": "Tôi thà đứng giữ cửa trong nhà Đức Chúa Trời của tôi,",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.722Z",
      "updatedAt": "2024-09-23T05:51:17.722Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "ae14152f-82ae-4520-abcd-5bd080a3d35b",
      "number": 10,
      "content": "Còn hơn ở trong biệt thự của những kẻ gian ác.",
      "order": 3,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.727Z",
      "updatedAt": "2024-09-23T05:51:17.727Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "191c7712-24ca-4abb-b015-50af2e957240",
      "number": 11,
      "content": "Vì **CHÚA** Đức Chúa Trời là mặt trời và thuẫn khiên;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.738Z",
      "updatedAt": "2024-09-23T05:51:17.738Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "21898ffd-4295-4d9e-8196-ec29ce91e8cb",
      "number": 11,
      "content": "**CHÚA** sẽ ban ân sủng và vinh hiển;",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.749Z",
      "updatedAt": "2024-09-23T05:51:17.749Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "532520c9-1878-44b3-be38-2a65a49a8bd2",
      "number": 11,
      "content": "Ngài chẳng giữ lại phước hạnh nào",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.754Z",
      "updatedAt": "2024-09-23T05:51:17.754Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "10c50cda-c0ae-4490-b417-c7a1142f6eb7",
      "number": 11,
      "content": "Đối với người ăn ở ngay lành.",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.759Z",
      "updatedAt": "2024-09-23T05:51:17.759Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    },
    {
      "id": "bb7a0787-2924-457c-9c63-448281bbdf45",
      "number": 12,
      "content": "Phước thay cho người tin cậy Ngài!",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:17.769Z",
      "updatedAt": "2024-09-23T05:51:17.769Z",
      "chapterId": "47851d35-437a-4f2d-aec8-42df670e968a"
    }
  ],
