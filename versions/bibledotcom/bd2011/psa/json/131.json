  "verses": [
    {
      "id": "37f15bb5-4db5-489e-8f82-0a7ed20fa67b",
      "number": 1,
      "content": "**CHÚA** ôi, lòng con không dám tự cao;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:23.309Z",
      "updatedAt": "2024-09-23T05:55:23.309Z",
      "chapterId": "9658bc67-5aee-4f9b-b994-cb556bd59b43"
    },
    {
      "id": "d69b759e-ac6e-4dfd-9c2f-b4bc74cf0ef2",
      "number": 1,
      "content": "Mắt con chẳng dám kiêu kỳ;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:23.340Z",
      "updatedAt": "2024-09-23T05:55:23.340Z",
      "chapterId": "9658bc67-5aee-4f9b-b994-cb556bd59b43"
    },
    {
      "id": "205ce3b7-6a0b-4fc0-a17f-25cffb170ba3",
      "number": 1,
      "content": "Con không dám tham dự vào những việc lớn lao quá sức,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:23.345Z",
      "updatedAt": "2024-09-23T05:55:23.345Z",
      "chapterId": "9658bc67-5aee-4f9b-b994-cb556bd59b43"
    },
    {
      "id": "4a50e7c8-ea14-4b40-ad0e-d65d123ee74f",
      "number": 1,
      "content": "Hay những việc vượt quá khả năng mình.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:23.350Z",
      "updatedAt": "2024-09-23T05:55:23.350Z",
      "chapterId": "9658bc67-5aee-4f9b-b994-cb556bd59b43"
    },
    {
      "id": "c821440c-c58e-401e-9808-a5f72c03d1e3",
      "number": 2,
      "content": "Nhưng con đã tự trấn tĩnh và làm cho linh hồn mình lắng dịu,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:23.355Z",
      "updatedAt": "2024-09-23T05:55:23.355Z",
      "chapterId": "9658bc67-5aee-4f9b-b994-cb556bd59b43"
    },
    {
      "id": "0c95447d-e5a4-4e5a-973a-1a54d180de78",
      "number": 2,
      "content": "Như trẻ thơ no sữa nép mình bên lòng mẹ;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:23.360Z",
      "updatedAt": "2024-09-23T05:55:23.360Z",
      "chapterId": "9658bc67-5aee-4f9b-b994-cb556bd59b43"
    },
    {
      "id": "2f0eb9c5-059b-4407-943d-4a115aacea77",
      "number": 2,
      "content": "Linh hồn con ở trong con như một em bé vừa no sữa mẹ.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:23.364Z",
      "updatedAt": "2024-09-23T05:55:23.364Z",
      "chapterId": "9658bc67-5aee-4f9b-b994-cb556bd59b43"
    },
    {
      "id": "7c8903ae-4266-4490-9877-8bd69881af3c",
      "number": 3,
      "content": "Hỡi I-sơ-ra-ên, hãy hy vọng nơi **CHÚA**,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:23.369Z",
      "updatedAt": "2024-09-23T05:55:23.369Z",
      "chapterId": "9658bc67-5aee-4f9b-b994-cb556bd59b43"
    },
    {
      "id": "87a84e54-b560-43b8-9373-973004153f71",
      "number": 3,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:23.374Z",
      "updatedAt": "2024-09-23T05:55:23.374Z",
      "chapterId": "9658bc67-5aee-4f9b-b994-cb556bd59b43"
    }
  ],
