{
  "chapter": {
    "id": "7fcd7733-b204-41b5-9ebd-93193295a2e3",
    "number": 61,
    "ref": "https://www.bible.com/bible/19/PSA.61.BD2011",
    "createdAt": "2024-09-23T05:08:40.425Z",
    "updatedAt": "2024-09-23T05:08:40.425Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "f9ce2d73-d2aa-41ae-b05d-dac102b680d2",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin nghe tiếng kêu cầu của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.339Z",
      "updatedAt": "2024-09-23T05:48:59.339Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "38a3dc72-d652-4f5b-b395-07c9018083d8",
      "number": 1,
      "content": "Xin lắng nghe lời cầu nguyện của con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.364Z",
      "updatedAt": "2024-09-23T05:48:59.364Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "28ccafa0-935c-4264-a412-7205b2f7102f",
      "number": 2,
      "content": "Từ nơi xa xôi ở tận cùng trái đất con kêu cầu Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.367Z",
      "updatedAt": "2024-09-23T05:48:59.367Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "4855fbd0-68ab-4029-abf7-d20358cea71f",
      "number": 2,
      "content": "Lòng con bị choáng ngợp vì biết bao nỗi lo sợ,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.371Z",
      "updatedAt": "2024-09-23T05:48:59.371Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "e4682a0d-5b07-4024-9809-8cc07cbbef21",
      "number": 2,
      "content": "Xin dẫn đưa con đến một vầng đá",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.376Z",
      "updatedAt": "2024-09-23T05:48:59.376Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "4eabc44e-2e1e-4291-ba95-11b424ceb2e9",
      "number": 2,
      "content": "Cao lớn hơn con;",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.380Z",
      "updatedAt": "2024-09-23T05:48:59.380Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "3432cf03-88d6-41a1-bbc2-73ee26de9e84",
      "number": 3,
      "content": "Vì Ngài là nơi ẩn náu của con,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.385Z",
      "updatedAt": "2024-09-23T05:48:59.385Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "a81cd5d5-a599-4ba7-98e5-efe39e093a55",
      "number": 3,
      "content": "Một pháo tháp vững chắc để con đối phó với quân thù.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.389Z",
      "updatedAt": "2024-09-23T05:48:59.389Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "5e4db494-f871-4655-9fec-7171a5e45dad",
      "number": 4,
      "content": "Xin cho con ở trong nhà Ngài mãi mãi;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.394Z",
      "updatedAt": "2024-09-23T05:48:59.394Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "f17b6cca-82a8-43bd-a205-8e4a49f388e0",
      "number": 4,
      "content": "Xin cho con nương náu dưới sự che chở của bóng cánh Ngài. (Sê-la)",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.398Z",
      "updatedAt": "2024-09-23T05:48:59.398Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "4b1a3086-6ae4-4681-8713-4e30e2235cfd",
      "number": 5,
      "content": "Đức Chúa Trời ôi, vì Ngài đã nghe lời hứa nguyện của con;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.403Z",
      "updatedAt": "2024-09-23T05:48:59.403Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "bcb3d821-4e31-4720-89d7-0fdabdde0be7",
      "number": 5,
      "content": "Ngài ban cho con cơ nghiệp dành cho những người kính sợ danh Ngài.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.407Z",
      "updatedAt": "2024-09-23T05:48:59.407Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "ebd7fa59-0228-437c-bcc1-9dc318ccb402",
      "number": 6,
      "content": "Cầu xin Ngài gia tăng tuổi thọ cho vua;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.412Z",
      "updatedAt": "2024-09-23T05:48:59.412Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "6e480fbc-e4ee-496d-860a-ee125b5f0ebb",
      "number": 6,
      "content": "Xin cho những năm của đời người kéo dài qua nhiều thế hệ.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.416Z",
      "updatedAt": "2024-09-23T05:48:59.416Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "7dc9db4e-df0a-4e7b-b1bc-c870ac382b7f",
      "number": 7,
      "content": "Nguyện người trị vì trước mặt Đức Chúa Trời mãi mãi;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.421Z",
      "updatedAt": "2024-09-23T05:48:59.421Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "7b65b234-f919-47bf-8f55-51f545e74f28",
      "number": 7,
      "content": "Cầu xin Ngài ban tình thương và đức thành tín bảo vệ người.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.425Z",
      "updatedAt": "2024-09-23T05:48:59.425Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "e862fc8b-f251-4efa-b316-6e4044ebdf43",
      "number": 8,
      "content": "Như vậy con sẽ ca ngợi danh Ngài mãi mãi;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.430Z",
      "updatedAt": "2024-09-23T05:48:59.430Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    },
    {
      "id": "91380387-652d-43cf-8a59-6544e5dea4dd",
      "number": 8,
      "content": "Con sẽ giữ trọn những gì con đã hứa nguyện mỗi ngày.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:59.435Z",
      "updatedAt": "2024-09-23T05:48:59.435Z",
      "chapterId": "7fcd7733-b204-41b5-9ebd-93193295a2e3"
    }
  ],
