{
  "chapter": {
    "id": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c",
    "number": 123,
    "ref": "https://www.bible.com/bible/19/PSA.123.BD2011",
    "createdAt": "2024-09-23T05:08:40.631Z",
    "updatedAt": "2024-09-23T05:08:40.631Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "73fbd8fa-27b8-4f78-897a-f4316d5bb28e",
      "number": 1,
      "content": "Lạy Đấng ngự trên các tầng trời,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.909Z",
      "updatedAt": "2024-09-23T05:54:49.909Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "aad0a1e0-a561-4f3e-8db9-8345abe6f32c",
      "number": 1,
      "content": "Con ngước mắt lên hướng về Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.940Z",
      "updatedAt": "2024-09-23T05:54:49.940Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "421a1db0-b73f-438b-95bd-5b1080154e82",
      "number": 2,
      "content": "Kìa, như mắt các đầy tớ",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.945Z",
      "updatedAt": "2024-09-23T05:54:49.945Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "49157bed-fb66-478c-8877-ab907d5b8fbe",
      "number": 2,
      "content": "Trông nơi tay ông chủ họ thể nào,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.950Z",
      "updatedAt": "2024-09-23T05:54:49.950Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "dd4c0fa1-fc39-4055-9bb0-9daec5a3e91f",
      "number": 2,
      "content": "Như mắt người tớ gái",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.954Z",
      "updatedAt": "2024-09-23T05:54:49.954Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "31f0a89c-2647-4d06-979c-68282507fd65",
      "number": 2,
      "content": "Trông nơi tay bà chủ nàng thể nào,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.959Z",
      "updatedAt": "2024-09-23T05:54:49.959Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "5b1d670c-665b-45bd-830c-827c7433b111",
      "number": 2,
      "content": "Mắt chúng con cũng vọng trông nơi **CHÚA**, Đức Chúa Trời chúng con, thể ấy,",
      "order": 4,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.964Z",
      "updatedAt": "2024-09-23T05:54:49.964Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "2dc0cde4-3f00-460f-928f-792380c5813c",
      "number": 2,
      "content": "Cho đến khi Ngài bày tỏ ơn thương xót đối với chúng con.",
      "order": 5,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.969Z",
      "updatedAt": "2024-09-23T05:54:49.969Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "5772104c-7e00-4e1e-97df-705621d6df90",
      "number": 3,
      "content": "Xin thương xót chúng con, **CHÚA** ôi, xin thương xót chúng con,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.973Z",
      "updatedAt": "2024-09-23T05:54:49.973Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "618e507f-179f-4280-9fe7-daa15c4d038c",
      "number": 3,
      "content": "Vì chúng con bị khinh khi quá đỗi.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.978Z",
      "updatedAt": "2024-09-23T05:54:49.978Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "19dc9e42-cac2-47a1-91e8-174d9dffb379",
      "number": 4,
      "content": "Linh hồn chúng con quá ngao ngán",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.983Z",
      "updatedAt": "2024-09-23T05:54:49.983Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "dc492496-300a-4135-aa47-c354ede9bb22",
      "number": 4,
      "content": "Những lời nhạo báng của đám người kiêu ngạo,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.987Z",
      "updatedAt": "2024-09-23T05:54:49.987Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    },
    {
      "id": "6cfbaa0d-8857-441a-8de3-68b8ac15246e",
      "number": 4,
      "content": "Cùng với giọng khinh người của những kẻ kiêu căng.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:49.992Z",
      "updatedAt": "2024-09-23T05:54:49.992Z",
      "chapterId": "0f90dd07-6073-4e8b-9cc8-6f0b3851bd5c"
    }
  ],
