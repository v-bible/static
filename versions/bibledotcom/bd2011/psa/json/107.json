{
  "chapter": {
    "id": "9a9008da-119a-4bd5-abc5-daf81afca328",
    "number": 107,
    "ref": "https://www.bible.com/bible/19/PSA.107.BD2011",
    "createdAt": "2024-09-23T05:08:40.586Z",
    "updatedAt": "2024-09-23T05:08:40.586Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "ad1ae74b-977b-48a3-a39c-3a562d4a1841",
      "number": 1,
      "content": "Hãy cảm tạ **CHÚA**, vì Ngài thật tốt,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.116Z",
      "updatedAt": "2024-09-23T05:53:26.116Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "4bdca3a2-1868-42c7-8a9e-e11cd5f7d487",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.149Z",
      "updatedAt": "2024-09-23T05:53:26.149Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "384988a4-2e46-4c4a-aeb2-9169c4643e2d",
      "number": 2,
      "content": "Nguyện những người được cứu chuộc của **CHÚA** nói như vậy,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.153Z",
      "updatedAt": "2024-09-23T05:53:26.153Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "e9522d15-d527-4a7a-a3ee-b992e546474a",
      "number": 2,
      "content": "Những người được Ngài cứu khỏi tay kẻ thù,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.158Z",
      "updatedAt": "2024-09-23T05:53:26.158Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "58ea6daa-8fc8-452a-b504-cff3d8211256",
      "number": 3,
      "content": "Rồi gom nhóm họ từ các nước trở về;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.163Z",
      "updatedAt": "2024-09-23T05:53:26.163Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "2a9c20a4-e097-4d51-bb92-ffbb32338ef6",
      "number": 3,
      "content": "Từ phương đông và phương tây,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.169Z",
      "updatedAt": "2024-09-23T05:53:26.169Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "121a45c8-9373-43d3-9632-8ef59dc5bdc4",
      "number": 3,
      "content": "Phương bắc và phương nam.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.174Z",
      "updatedAt": "2024-09-23T05:53:26.174Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "b344f188-d7ea-458e-b838-c4ab704f6277",
      "number": 4,
      "content": "Họ lang thang trong đồng hoang, trong nơi hoang vắng;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.179Z",
      "updatedAt": "2024-09-23T05:53:26.179Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "4fb9b928-1530-451c-bf43-c0340741bde0",
      "number": 4,
      "content": "Họ chẳng tìm được đường về một thành để ở.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.184Z",
      "updatedAt": "2024-09-23T05:53:26.184Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "3f83b67d-c202-4829-ac26-a28929d8e280",
      "number": 5,
      "content": "Họ đói khát;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.189Z",
      "updatedAt": "2024-09-23T05:53:26.189Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "166f7a71-1200-49d7-85bb-205d519b20b9",
      "number": 5,
      "content": "Linh hồn họ mòn mỏi nao sờn.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.194Z",
      "updatedAt": "2024-09-23T05:53:26.194Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "1defd15a-983e-41ff-8970-67b74670ce48",
      "number": 19,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với **CHÚA**,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.333Z",
      "updatedAt": "2024-09-23T05:53:26.333Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "eea1550f-26ae-485d-ba76-c9c535bb5bc9",
      "number": 13,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với **CHÚA**,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.271Z",
      "updatedAt": "2024-09-23T05:53:26.271Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "0132089a-c20a-447c-aeb0-678ea643c557",
      "number": 6,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với **CHÚA**,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.198Z",
      "updatedAt": "2024-09-23T05:53:26.198Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "177bcadd-dd46-4dd7-a3ee-845bd14b7d65",
      "number": 28,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với **CHÚA**,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.420Z",
      "updatedAt": "2024-09-23T05:53:26.420Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "5e8ad099-adbc-40b3-875e-16ad708581dd",
      "number": 28,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.425Z",
      "updatedAt": "2024-09-23T05:53:26.425Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "4454e314-cea8-49ce-89e3-efa0ac8d050a",
      "number": 6,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.204Z",
      "updatedAt": "2024-09-23T05:53:26.204Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "31fc1b36-8acd-491d-83d3-085873a1a2eb",
      "number": 13,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.276Z",
      "updatedAt": "2024-09-23T05:53:26.276Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "5ee22870-ac9e-4d00-a37e-8149b0e736b4",
      "number": 19,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.339Z",
      "updatedAt": "2024-09-23T05:53:26.339Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "e379b185-6d60-479f-93c2-1bf2a56477b5",
      "number": 7,
      "content": "Ngài dẫn đưa họ vào đúng con đường phải đi,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.209Z",
      "updatedAt": "2024-09-23T05:53:26.209Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "1b86bc6d-d9ca-4be8-8869-a8e736aae9b0",
      "number": 7,
      "content": "Để họ đến một thành, một nơi để ở.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.214Z",
      "updatedAt": "2024-09-23T05:53:26.214Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "91d5a2c7-be10-4be0-a819-5e0b678730db",
      "number": 21,
      "content": "Hãy để họ cảm tạ **CHÚA** vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.355Z",
      "updatedAt": "2024-09-23T05:53:26.355Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "9b85cc9b-0b42-4004-8f7a-d19acc5583c8",
      "number": 8,
      "content": "Hãy để họ cảm tạ **CHÚA** vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.219Z",
      "updatedAt": "2024-09-23T05:53:26.219Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "3c808691-1c44-4b2b-afcc-c35a13792ff7",
      "number": 15,
      "content": "Hãy để họ cảm tạ **CHÚA** vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.292Z",
      "updatedAt": "2024-09-23T05:53:26.292Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "6f994b43-da43-4750-a063-70df6ea374bc",
      "number": 31,
      "content": "Hãy để họ cảm tạ **CHÚA** vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.448Z",
      "updatedAt": "2024-09-23T05:53:26.448Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "4b2e86c7-15dc-4298-ab21-b76535263fbd",
      "number": 8,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.225Z",
      "updatedAt": "2024-09-23T05:53:26.225Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "e452d55f-8c01-4a2a-890d-1f9ef48ea5c2",
      "number": 9,
      "content": "Vì Ngài làm thỏa mãn linh hồn khao khát,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.230Z",
      "updatedAt": "2024-09-23T05:53:26.230Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "2c0a94f8-c036-4b1c-8143-c9a78b402c9d",
      "number": 9,
      "content": "Và làm no nê linh hồn đói lả bằng các thức ăn ngon.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.235Z",
      "updatedAt": "2024-09-23T05:53:26.235Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "f9c7c93e-faae-4ad2-aa64-f832ae2ccf35",
      "number": 10,
      "content": "Có những kẻ sống trong tối tăm và trong bóng tử thần,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.240Z",
      "updatedAt": "2024-09-23T05:53:26.240Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "7b2de2c7-6263-4796-b6db-abc4eda71a71",
      "number": 10,
      "content": "Bị giam cầm trong khổ đau và xiềng xích,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.245Z",
      "updatedAt": "2024-09-23T05:53:26.245Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "34f45658-387f-4d05-9778-3b367207daf5",
      "number": 11,
      "content": "Bởi vì họ chống lại những lời của Đức Chúa Trời,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.250Z",
      "updatedAt": "2024-09-23T05:53:26.250Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "3b16c39f-6747-4f51-9658-564461b4102a",
      "number": 11,
      "content": "Và khinh bỉ lời chỉ bảo của Đấng Chí Cao.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.255Z",
      "updatedAt": "2024-09-23T05:53:26.255Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "8efc85d7-1e50-4da9-97a7-c544e2e4bfb3",
      "number": 12,
      "content": "Vì thế Ngài lấy gian lao hạ lòng họ xuống;",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.260Z",
      "updatedAt": "2024-09-23T05:53:26.260Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "ef7b4723-07c7-4df4-8ded-51f4c00b6288",
      "number": 12,
      "content": "Họ bị ngã nhào và chẳng ai giúp họ đứng lên.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.265Z",
      "updatedAt": "2024-09-23T05:53:26.265Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "5dff9b65-f136-472d-b095-28f464b024a9",
      "number": 14,
      "content": "Ngài đem họ ra khỏi cảnh tối tăm và bóng tử thần,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.281Z",
      "updatedAt": "2024-09-23T05:53:26.281Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "5902f4cb-ac9e-4bc7-930f-e256c664d402",
      "number": 14,
      "content": "Và đập tan xiềng xích gông cùm trói buộc họ.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.287Z",
      "updatedAt": "2024-09-23T05:53:26.287Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "ef688894-fdb4-425c-bd61-fe971b5c1059",
      "number": 15,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người,",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.297Z",
      "updatedAt": "2024-09-23T05:53:26.297Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "0f7ac6b7-7361-49a5-a528-05f67c45f3ab",
      "number": 16,
      "content": "Vì Ngài phá tan các cổng bằng đồng,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.302Z",
      "updatedAt": "2024-09-23T05:53:26.302Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "29cae5ab-9009-4944-b33c-b398a7bb9603",
      "number": 16,
      "content": "Và Ngài cắt rời những song bằng sắt.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.307Z",
      "updatedAt": "2024-09-23T05:53:26.307Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "261bb45d-6639-4872-96f4-bb279467e9d2",
      "number": 17,
      "content": "Có những kẻ cứ bội nghịch nên hóa ra điên dại,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.312Z",
      "updatedAt": "2024-09-23T05:53:26.312Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "dc4dae2c-672e-4237-91f7-9cacfae27dec",
      "number": 17,
      "content": "Và vì tội lỗi mình nên phải chuốc lấy khổ đau.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.318Z",
      "updatedAt": "2024-09-23T05:53:26.318Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "a09941d9-e377-4325-885d-8d8a6a7ff35e",
      "number": 18,
      "content": "Linh hồn họ gớm ghê mọi thực phẩm,",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.323Z",
      "updatedAt": "2024-09-23T05:53:26.323Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "0d523ceb-9f43-4b29-a3b8-e40d0bfc5cab",
      "number": 18,
      "content": "Và họ bị kéo gần đến cổng của tử thần.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.328Z",
      "updatedAt": "2024-09-23T05:53:26.328Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "ca40d6a2-b2f8-49f6-9bed-51f08a7dd549",
      "number": 20,
      "content": "Ngài ban lời Ngài và chữa lành họ;",
      "order": 0,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.345Z",
      "updatedAt": "2024-09-23T05:53:26.345Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "ee7194f0-59ab-4399-829a-82ec362291f7",
      "number": 20,
      "content": "Và giải cứu họ khỏi miệng của mộ phần.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.350Z",
      "updatedAt": "2024-09-23T05:53:26.350Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "50477ea2-0d33-4469-a509-73813a72bde0",
      "number": 21,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.360Z",
      "updatedAt": "2024-09-23T05:53:26.360Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "ae651fd5-59a2-4736-a57d-323a8e952b34",
      "number": 31,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.453Z",
      "updatedAt": "2024-09-23T05:53:26.453Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "fd0593aa-265c-4cdf-90e7-cf4116f0044c",
      "number": 22,
      "content": "Cũng hãy để họ dâng những của lễ cảm tạ lên Ngài,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.365Z",
      "updatedAt": "2024-09-23T05:53:26.365Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "7b728800-c60e-4b2d-8602-8d0f00198cce",
      "number": 22,
      "content": "Và thuật lại các công việc Ngài bằng những lời ca ngợi vui mừng.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.370Z",
      "updatedAt": "2024-09-23T05:53:26.370Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "cb98449a-2a78-4cfb-9314-3c5d874fe1af",
      "number": 23,
      "content": "Có những kẻ xuống tàu vượt đại dương thăm thẳm,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.375Z",
      "updatedAt": "2024-09-23T05:53:26.375Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "7adc7214-798b-459e-af37-06ea3b2f7040",
      "number": 23,
      "content": "Những người làm ăn trên biển cả mênh mông;",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.380Z",
      "updatedAt": "2024-09-23T05:53:26.380Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "c0eb7f45-eae0-4ce5-a665-02407652175d",
      "number": 24,
      "content": "Họ trông thấy những việc làm của **CHÚA**,",
      "order": 0,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.384Z",
      "updatedAt": "2024-09-23T05:53:26.384Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "55f2e1cd-23dc-45d3-a11d-163bd19d1862",
      "number": 24,
      "content": "Và những việc lạ lùng của Ngài ở giữa biển sâu.",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.389Z",
      "updatedAt": "2024-09-23T05:53:26.389Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "ff806aae-21d4-4949-ba45-c8dbc137c1da",
      "number": 25,
      "content": "Vì Ngài phán một tiếng, bão tố liền nổi lên cuồn cuộn,",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.393Z",
      "updatedAt": "2024-09-23T05:53:26.393Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "7269593b-1596-4c49-a242-ccee81b70407",
      "number": 25,
      "content": "Cuồng phong nâng cao những ngọn sóng kinh hồn.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.398Z",
      "updatedAt": "2024-09-23T05:53:26.398Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "a3165032-97ad-424e-9d0b-678b44a4d31e",
      "number": 26,
      "content": "Sóng cả dâng cao đến tận trời, rồi trụt sâu xuống vực thẳm;",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.403Z",
      "updatedAt": "2024-09-23T05:53:26.403Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "b7a168ad-9bf6-4988-bcd3-01b95f62daa5",
      "number": 26,
      "content": "Giữa cảnh khốn cùng họ hồn phi phách tán.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.407Z",
      "updatedAt": "2024-09-23T05:53:26.407Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "13530864-226d-42b7-b4b1-610b6d84cba9",
      "number": 27,
      "content": "Họ bị quay cuồng điên đảo tựa người say;",
      "order": 0,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.412Z",
      "updatedAt": "2024-09-23T05:53:26.412Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "5fcd991c-8e12-44b9-b199-0bc5cf4be906",
      "number": 27,
      "content": "Mọi tài trí khôn ngoan đều biến mất.",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.416Z",
      "updatedAt": "2024-09-23T05:53:26.416Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "bc2fba30-28b8-46ab-b584-b4267e895907",
      "number": 29,
      "content": "Ngài khiến bão tố yên lặng như tờ;",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.429Z",
      "updatedAt": "2024-09-23T05:53:26.429Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "b52b23ac-fd62-4f7e-97d2-7c205a6418da",
      "number": 29,
      "content": "Những ngọn sóng biển lặng im phăng phắc.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.434Z",
      "updatedAt": "2024-09-23T05:53:26.434Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "99a98231-9fb7-43e0-a3b2-fdaefc54644d",
      "number": 30,
      "content": "Họ rất đỗi vui mừng vì biển êm gió lặng;",
      "order": 0,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.438Z",
      "updatedAt": "2024-09-23T05:53:26.438Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "0c53efa0-9277-4416-84e4-93ef4c63f23d",
      "number": 30,
      "content": "Ngài lại dẫn đưa họ đến hải cảng họ ước mong.",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.444Z",
      "updatedAt": "2024-09-23T05:53:26.444Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "529b42f9-cd2a-4016-9e0e-3ebd6fda53e6",
      "number": 32,
      "content": "Hãy để họ tôn cao Ngài giữa đại hội đông người,",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.458Z",
      "updatedAt": "2024-09-23T05:53:26.458Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "ed57a53c-cadc-406c-80ca-03d446968a3b",
      "number": 32,
      "content": "Và ca tụng Ngài giữa hội đồng trưởng lão.",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.462Z",
      "updatedAt": "2024-09-23T05:53:26.462Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "35b4bcc7-73cc-4c8d-9024-91ccf2589bc5",
      "number": 33,
      "content": "Ngài biến các dòng sông thành sa mạc hoang vu,",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.467Z",
      "updatedAt": "2024-09-23T05:53:26.467Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "818dc4b1-5c98-4da7-a86e-1cbafc85776e",
      "number": 33,
      "content": "Các dòng suối thành đất khô cằn cỗi,",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.471Z",
      "updatedAt": "2024-09-23T05:53:26.471Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "feddef54-85a2-45ad-8cc6-40d73bf83b1e",
      "number": 34,
      "content": "Đất đai màu mỡ thành đồng chua nước mặn,",
      "order": 0,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.476Z",
      "updatedAt": "2024-09-23T05:53:26.476Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "6d3b5a72-e2dd-4f92-8ab0-134cb94bc6c9",
      "number": 34,
      "content": "Vì sự gian ác của dân cư ở đó.",
      "order": 1,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.480Z",
      "updatedAt": "2024-09-23T05:53:26.480Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "095688a2-8908-4491-b91d-62bf3baa1a20",
      "number": 35,
      "content": "Ngài biến đồng hoang thành đất có ao hồ,",
      "order": 0,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.485Z",
      "updatedAt": "2024-09-23T05:53:26.485Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "0542965f-1559-438c-9671-90147f70ac6f",
      "number": 35,
      "content": "Và xứ khô cằn thành nơi có các dòng nước ngọt;",
      "order": 1,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.489Z",
      "updatedAt": "2024-09-23T05:53:26.489Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "273623f5-45ff-4f9a-98a1-7ee09dba8804",
      "number": 36,
      "content": "Ngài đã cho những kẻ đói khát đến cư ngụ tại đó,",
      "order": 0,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.494Z",
      "updatedAt": "2024-09-23T05:53:26.494Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "c362057f-d5d7-4e25-8b88-d0424df56877",
      "number": 36,
      "content": "Để họ xây dựng một thành mà an cư lạc nghiệp;",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.500Z",
      "updatedAt": "2024-09-23T05:53:26.500Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "5006bd09-0ccf-4c78-bdac-904a075b97af",
      "number": 37,
      "content": "Họ gieo hạt ngoài đồng và trồng tỉa vườn nho,",
      "order": 0,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.505Z",
      "updatedAt": "2024-09-23T05:53:26.505Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "c4d24498-c8c6-4bd1-909c-ab2791255902",
      "number": 37,
      "content": "Rồi thu hoạch hoa màu sung túc.",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.510Z",
      "updatedAt": "2024-09-23T05:53:26.510Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "dc626004-be1e-458d-9029-05d0b37bb540",
      "number": 38,
      "content": "Ngài ban phước cho họ, và dân số họ gia tăng;",
      "order": 0,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.515Z",
      "updatedAt": "2024-09-23T05:53:26.515Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "f0940a39-88af-4130-bd7f-a17dd231dab4",
      "number": 38,
      "content": "Ngài chẳng để cho bầy súc vật của họ bị giảm thiểu.",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.520Z",
      "updatedAt": "2024-09-23T05:53:26.520Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "4d34c11c-ff32-48d6-8bb0-99c2813f3a31",
      "number": 39,
      "content": "Có lúc họ bị tiêu hao lụn bại, phải cúi đầu khuất phục ngoại bang,",
      "order": 0,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.525Z",
      "updatedAt": "2024-09-23T05:53:26.525Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "eedca43a-847f-4b73-be95-30a5a921d9ab",
      "number": 39,
      "content": "Bị áp bức, khổ đau, và sầu não.",
      "order": 1,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.529Z",
      "updatedAt": "2024-09-23T05:53:26.529Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "78112ff7-b356-427f-86aa-64178d7697c6",
      "number": 40,
      "content": "Ngài trút đổ ô nhục trên những người quyền quý của họ,",
      "order": 0,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.535Z",
      "updatedAt": "2024-09-23T05:53:26.535Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "34b223a5-871b-4aca-b9aa-aa3736a13d62",
      "number": 40,
      "content": "Khiến họ lang thang trong nơi hoang vắng không đường.",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.540Z",
      "updatedAt": "2024-09-23T05:53:26.540Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "b8da404a-afdf-4431-9d2c-9d205da49d1a",
      "number": 41,
      "content": "Nhưng Ngài lại cất những người nghèo ra khỏi cảnh lầm than,",
      "order": 0,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.544Z",
      "updatedAt": "2024-09-23T05:53:26.544Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "1a4a8f1e-3fd0-4aea-b7e7-902530d82d78",
      "number": 41,
      "content": "Và cho con cháu họ được gia tăng đông đúc.",
      "order": 1,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.549Z",
      "updatedAt": "2024-09-23T05:53:26.549Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "f2cfd040-6971-4f77-99ac-627c8dec1161",
      "number": 42,
      "content": "Những người ngay lành thấy thế bèn vui mừng;",
      "order": 0,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.553Z",
      "updatedAt": "2024-09-23T05:53:26.553Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "ebe0baf4-8f11-4369-a191-5961e9d4c877",
      "number": 42,
      "content": "Còn những kẻ gian ác thảy đều câm miệng lại.",
      "order": 1,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.558Z",
      "updatedAt": "2024-09-23T05:53:26.558Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "2f17e209-d47a-4b92-b6f9-f2985327c8f0",
      "number": 43,
      "content": "Hỡi ai là người khôn ngoan, hãy suy gẫm những điều ấy,",
      "order": 0,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.563Z",
      "updatedAt": "2024-09-23T05:53:26.563Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    },
    {
      "id": "b8f024ee-34f8-48ab-a00b-a6df1b3a5bc3",
      "number": 43,
      "content": "Rồi sẽ hiểu được thế nào là tình thương của **CHÚA**.",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:26.567Z",
      "updatedAt": "2024-09-23T05:53:26.567Z",
      "chapterId": "9a9008da-119a-4bd5-abc5-daf81afca328"
    }
  ],
