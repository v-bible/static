{
  "chapter": {
    "id": "a2ea1063-9c9c-480f-8119-9fed88becdb8",
    "number": 1,
    "ref": "https://www.bible.com/bible/19/PSA.1.BD2011",
    "createdAt": "2024-09-23T05:08:40.213Z",
    "updatedAt": "2024-09-23T05:08:40.213Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "3c974a0a-ac23-4eb7-ba34-ba5ca01c46ba",
      "number": 1,
      "content": "Phước cho người",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.243Z",
      "updatedAt": "2024-09-23T05:44:33.243Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "71ee912d-1e05-45ac-b6f1-2f7038d69d1c",
      "number": 1,
      "content": "Không đi theo kế kẻ gian ác,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.256Z",
      "updatedAt": "2024-09-23T05:44:33.256Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "d801d40f-c1b6-4e38-b355-f7cb94544f39",
      "number": 1,
      "content": "Không đứng chung đường với phường tội lỗi,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.261Z",
      "updatedAt": "2024-09-23T05:44:33.261Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "8ba6c6aa-03ad-4167-a322-384ca97e26e9",
      "number": 1,
      "content": "Không ngồi cùng chỗ với đồ nhạo báng,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.267Z",
      "updatedAt": "2024-09-23T05:44:33.267Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "e19f166f-ab02-44e0-9899-536795437b21",
      "number": 2,
      "content": "Nhưng vui thích về luật pháp của **CHÚA**,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.272Z",
      "updatedAt": "2024-09-23T05:44:33.272Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "825b52d9-2235-48f1-a580-536af7caf902",
      "number": 2,
      "content": "Và suy gẫm luật pháp ấy ngày và đêm.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.276Z",
      "updatedAt": "2024-09-23T05:44:33.276Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "9fa25924-5802-4b5b-8bab-1db214c3cb4d",
      "number": 3,
      "content": "Người ấy sẽ như cây",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.281Z",
      "updatedAt": "2024-09-23T05:44:33.281Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "2330faaf-12f3-48b4-a912-1f8e61e1e9e8",
      "number": 3,
      "content": "Trồng gần dòng nước,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.287Z",
      "updatedAt": "2024-09-23T05:44:33.287Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "b0e5e8a8-6084-4ba4-b2bd-47a3b41558a5",
      "number": 3,
      "content": "Cứ đến mùa được hoa quả đầy cành,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.292Z",
      "updatedAt": "2024-09-23T05:44:33.292Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "0386a968-94a4-45a7-943e-ca53b0a6575d",
      "number": 3,
      "content": "Lá vẫn xanh chứ chẳng hề héo úa;",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.297Z",
      "updatedAt": "2024-09-23T05:44:33.297Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "dcdd61eb-b086-4a84-8d4d-9d00948bc7ac",
      "number": 3,
      "content": "Mọi sự người ấy làm đều sẽ thành công.",
      "order": 4,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.302Z",
      "updatedAt": "2024-09-23T05:44:33.302Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "71c8c075-7f43-4272-94c7-d49e1cba30f3",
      "number": 4,
      "content": "Những kẻ gian ác sẽ không được như vậy,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.307Z",
      "updatedAt": "2024-09-23T05:44:33.307Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "8cd08cfc-7573-4986-a858-6b4547227cdb",
      "number": 4,
      "content": "Nhưng chúng chẳng khác gì rác rến bị gió đùa đi.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.312Z",
      "updatedAt": "2024-09-23T05:44:33.312Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "e53e085b-a037-424d-af5c-8e327451ef65",
      "number": 5,
      "content": "Vì thế những kẻ gian ác sẽ chẳng đứng nổi trong ngày phán xét;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.316Z",
      "updatedAt": "2024-09-23T05:44:33.316Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "0958f021-6ee8-4aba-b3b1-297b1aaac249",
      "number": 5,
      "content": "Những kẻ tội lỗi sẽ không được vào hội người ngay lành;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.321Z",
      "updatedAt": "2024-09-23T05:44:33.321Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "f8237997-129b-448c-8ce0-9b015ce4a3aa",
      "number": 6,
      "content": "Vì **CHÚA** biết rõ đường lối người ngay lành,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.326Z",
      "updatedAt": "2024-09-23T05:44:33.326Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    },
    {
      "id": "3eaac704-86c3-4121-928a-a805e81d39e3",
      "number": 6,
      "content": "Còn đường lối kẻ gian ác sẽ dẫn đến diệt vong.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:33.332Z",
      "updatedAt": "2024-09-23T05:44:33.332Z",
      "chapterId": "a2ea1063-9c9c-480f-8119-9fed88becdb8"
    }
  ],
