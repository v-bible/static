  },
  "verses": [
    {
      "id": "43c3432e-5d7a-4d5a-a476-27ddb9dd2f0d",
      "number": 1,
      "content": "Kìa, tốt đẹp thay và phước hạnh thay",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:31.167Z",
      "updatedAt": "2024-09-23T05:55:31.167Z",
      "chapterId": "18985ce6-66f2-403e-93ef-8916661b00f5"
    },
    {
      "id": "b78f621e-1fc7-4846-8de5-8515fe4eb43a",
      "number": 1,
      "content": "Khi anh chị em sống hòa thuận với nhau!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:31.178Z",
      "updatedAt": "2024-09-23T05:55:31.178Z",
      "chapterId": "18985ce6-66f2-403e-93ef-8916661b00f5"
    },
    {
      "id": "08f38de9-d0e4-4089-9382-81a85319100b",
      "number": 2,
      "content": "Điều ấy chẳng khác gì dầu quý báu từ trên đầu chảy xuống,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:31.182Z",
      "updatedAt": "2024-09-23T05:55:31.182Z",
      "chapterId": "18985ce6-66f2-403e-93ef-8916661b00f5"
    },
    {
      "id": "c28477ac-7a67-4149-9a7d-83e14c5f0eae",
      "number": 2,
      "content": "Chảy xuống râu,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:31.184Z",
      "updatedAt": "2024-09-23T05:55:31.184Z",
      "chapterId": "18985ce6-66f2-403e-93ef-8916661b00f5"
    },
    {
      "id": "ec0db4a7-219e-4531-a1f7-c2092ba3dfb0",
      "number": 2,
      "content": "Chảy xuống râu của A-rôn,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:31.185Z",
      "updatedAt": "2024-09-23T05:55:31.185Z",
      "chapterId": "18985ce6-66f2-403e-93ef-8916661b00f5"
    },
    {
      "id": "57ccde24-e8cb-404c-bbe8-85fb25714428",
      "number": 2,
      "content": "Rồi tiếp tục chảy xuống vạt áo ông.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:31.189Z",
      "updatedAt": "2024-09-23T05:55:31.189Z",
      "chapterId": "18985ce6-66f2-403e-93ef-8916661b00f5"
    },
    {
      "id": "b036e714-608b-416e-be3a-0ca0b3c16676",
      "number": 3,
      "content": "Điều ấy giống như sương móc từ Núi Hẹt-môn",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:31.193Z",
      "updatedAt": "2024-09-23T05:55:31.193Z",
      "chapterId": "18985ce6-66f2-403e-93ef-8916661b00f5"
    },
    {
      "id": "1c4e1b3c-9878-4f30-9d2d-de15653c9232",
      "number": 3,
      "content": "Sa xuống các rặng núi Si-ôn,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:31.198Z",
      "updatedAt": "2024-09-23T05:55:31.198Z",
      "chapterId": "18985ce6-66f2-403e-93ef-8916661b00f5"
    },
    {
      "id": "bf8ff76c-940e-4052-bcca-af97f82c4c5e",
      "number": 3,
      "content": "Vì tại đó **CHÚA** ban phước,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:31.203Z",
      "updatedAt": "2024-09-23T05:55:31.203Z",
      "chapterId": "18985ce6-66f2-403e-93ef-8916661b00f5"
    },
    {
      "id": "8146710e-669f-4854-a512-55d87b8b06f1",
      "number": 3,
      "content": "Phước của sự sống đời đời.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:31.207Z",
      "updatedAt": "2024-09-23T05:55:31.207Z",
      "chapterId": "18985ce6-66f2-403e-93ef-8916661b00f5"
    }
  ],
