{
  "chapter": {
    "id": "18985ce6-66f2-403e-93ef-8916661b00f5",
    "number": 133,
    "ref": "https://www.bible.com/bible/19/PSA.133.BD2011",
    "createdAt": "2024-09-23T05:08:40.667Z",
