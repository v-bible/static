  "verses": [
    {
      "id": "98163a59-e299-49c8-a4e0-a8eb99dbb0fc",
      "number": 1,
      "content": "Hỡi các nước, hãy ca ngợi **CHÚA**;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:18.159Z",
      "updatedAt": "2024-09-23T05:54:18.159Z",
      "chapterId": "5766882b-53b3-4d47-aae2-e569852583b2"
    },
    {
      "id": "98f21046-6ed6-4308-837f-d70f6754ab92",
      "number": 1,
      "content": "Hỡi muôn dân, khá tôn ngợi Ngài!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:18.174Z",
      "updatedAt": "2024-09-23T05:54:18.174Z",
      "chapterId": "5766882b-53b3-4d47-aae2-e569852583b2"
    },
    {
      "id": "80fb3209-b1ce-4138-acaf-86dfa48b311d",
      "number": 2,
      "content": "Vì tình thương của Ngài đối với chúng ta rất lớn,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:18.176Z",
      "updatedAt": "2024-09-23T05:54:18.176Z",
      "chapterId": "5766882b-53b3-4d47-aae2-e569852583b2"
    },
    {
      "id": "64dd159d-1700-4307-b591-50c6fc828590",
      "number": 2,
      "content": "Và đức thành tín của **CHÚA** hằng còn đến đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:18.180Z",
      "updatedAt": "2024-09-23T05:54:18.180Z",
      "chapterId": "5766882b-53b3-4d47-aae2-e569852583b2"
    },
    {
      "id": "ce24c8b4-7d7c-47ff-9756-c58c526bb125",
      "number": 2,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:18.182Z",
      "updatedAt": "2024-09-23T05:54:18.182Z",
      "chapterId": "5766882b-53b3-4d47-aae2-e569852583b2"
    }
  ],
