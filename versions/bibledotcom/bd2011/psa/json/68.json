{
  "chapter": {
    "id": "01569ef1-1d05-4ce4-b711-a1209385ae4c",
    "number": 68,
    "ref": "https://www.bible.com/bible/19/PSA.68.BD2011",
    "createdAt": "2024-09-23T05:08:40.446Z",
    "updatedAt": "2024-09-23T05:08:40.446Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "79217659-a2e2-4646-b7dc-28a3dd762e2c",
      "number": 1,
      "content": "Nguyện Đức Chúa Trời trỗi dậy; nguyện những kẻ chống nghịch Ngài tản lạc;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.716Z",
      "updatedAt": "2024-09-23T05:49:38.716Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "9ba75cbe-a63f-46b7-8d56-1679329e1014",
      "number": 1,
      "content": "Nguyện những kẻ ghét Ngài chạy trốn trước mặt Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.728Z",
      "updatedAt": "2024-09-23T05:49:38.728Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "4b2d8432-668d-4ff8-94d1-98f6cc130862",
      "number": 2,
      "content": "Như khói bị tan biến thể nào, nguyện chúng bị tan đi thể ấy;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.731Z",
      "updatedAt": "2024-09-23T05:49:38.731Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "7f991522-bebf-4a81-a9c7-2e78effa9b10",
      "number": 2,
      "content": "Như sáp bị tan chảy trước ngọn lửa thể nào,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.733Z",
      "updatedAt": "2024-09-23T05:49:38.733Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "a516cf3c-cac7-40c3-8564-ccedd80104d7",
      "number": 2,
      "content": "Nguyện những kẻ ác bị diệt mất trước mặt Đức Chúa Trời thể ấy.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.735Z",
      "updatedAt": "2024-09-23T05:49:38.735Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "a1aa6272-95b9-4fda-94db-7d47318119f7",
      "number": 3,
      "content": "Nguyện những người ngay lành hân hoan,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.738Z",
      "updatedAt": "2024-09-23T05:49:38.738Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "4648596a-d80c-4651-b477-d6796b1bd109",
      "number": 3,
      "content": "Nguyện họ vui vẻ trước mặt Đức Chúa Trời;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.743Z",
      "updatedAt": "2024-09-23T05:49:38.743Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "b444acbb-6f6e-4e2b-af3c-de55406f6f99",
      "number": 3,
      "content": "Thật vậy nguyện họ hớn hở và vui mừng.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.748Z",
      "updatedAt": "2024-09-23T05:49:38.748Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "6e8eb54a-e9f9-4877-ba7a-ed7f6d5cb9ea",
      "number": 4,
      "content": "Hãy ca ngợi Đức Chúa Trời; hãy hát ca chúc tụng danh Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.753Z",
      "updatedAt": "2024-09-23T05:49:38.753Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "ae3e76c7-ec07-4490-8538-1a7c49990a0f",
      "number": 4,
      "content": "Khá tôn ngợi Đấng ngự trên các tầng mây;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.757Z",
      "updatedAt": "2024-09-23T05:49:38.757Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "e5aed539-f1ae-4414-a1af-9e832a3e0291",
      "number": 4,
      "content": "Danh Ngài là **CHÚA**;",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.767Z",
      "updatedAt": "2024-09-23T05:49:38.767Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "db4c313a-e72e-45ba-bdd1-1febc24f3c83",
      "number": 4,
      "content": "Khá reo vui trước mặt Ngài.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.772Z",
      "updatedAt": "2024-09-23T05:49:38.772Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "1382a364-8869-4f62-bda7-518d7d596eb4",
      "number": 5,
      "content": "Cha kẻ mồ côi, Đấng binh vực người góa bụa,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.776Z",
      "updatedAt": "2024-09-23T05:49:38.776Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "d72f9d8f-bb42-4882-9c74-2851d071795b",
      "number": 5,
      "content": "Chính là Đức Chúa Trời, Đấng ngự trong nơi thánh của Ngài.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.781Z",
      "updatedAt": "2024-09-23T05:49:38.781Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "9140890c-03b7-4fb5-9fd2-28e8bae152f6",
      "number": 6,
      "content": "Đức Chúa Trời là Đấng làm cho kẻ cô đơn có mái ấm gia đình;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.786Z",
      "updatedAt": "2024-09-23T05:49:38.786Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "1ae0c7fd-0c8b-4960-8d48-ecc4425d15d5",
      "number": 6,
      "content": "Ngài dẫn đưa những kẻ bị tù đày đến nơi sung mãn;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.791Z",
      "updatedAt": "2024-09-23T05:49:38.791Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "a1e7164c-77b0-487d-83df-d4d496472a68",
      "number": 6,
      "content": "Còn những kẻ bội nghịch phải ở trong xứ khô khan.",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.795Z",
      "updatedAt": "2024-09-23T05:49:38.795Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "65fd0f60-90c0-4573-9857-67f879810f49",
      "number": 7,
      "content": "Đức Chúa Trời ôi, khi Ngài đi ra trước mặt con dân Ngài,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.800Z",
      "updatedAt": "2024-09-23T05:49:38.800Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "7b4ee606-561d-4718-a250-b3e0e62aa527",
      "number": 7,
      "content": "Khi Ngài đi qua sa mạc, (Sê-la)",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.805Z",
      "updatedAt": "2024-09-23T05:49:38.805Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "feb0f746-608e-4db3-ab67-f21536a70fe7",
      "number": 8,
      "content": "Đất rúng động, các tầng trời ào ạt đổ cơn mưa,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.809Z",
      "updatedAt": "2024-09-23T05:49:38.809Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "ac2f2253-e3da-4152-83a7-ead23879dfae",
      "number": 8,
      "content": "Trước mặt Đức Chúa Trời, Thần của Núi Si-nai,",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.814Z",
      "updatedAt": "2024-09-23T05:49:38.814Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "0b7d8152-c663-4c67-a84e-a55fe4f8f76d",
      "number": 8,
      "content": "Trước mặt Đức Chúa Trời, Thần của I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.823Z",
      "updatedAt": "2024-09-23T05:49:38.823Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "bbbb0625-6c79-4fdb-b6d3-0372d08ecc48",
      "number": 9,
      "content": "Đức Chúa Trời ôi, Ngài đã ban mưa xuống cách dồi dào,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.828Z",
      "updatedAt": "2024-09-23T05:49:38.828Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "2bde569e-60c2-4caa-b333-5d6205746bc0",
      "number": 9,
      "content": "Khi sản nghiệp của Ngài bị khô khan, Ngài đã bồi bổ nó lại.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.833Z",
      "updatedAt": "2024-09-23T05:49:38.833Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "a59b3712-25c8-47e6-939c-14299ab91030",
      "number": 10,
      "content": "Con dân Ngài đã định cư trong sản nghiệp đó;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.838Z",
      "updatedAt": "2024-09-23T05:49:38.838Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "03b2d912-1afc-48db-9bac-63a3637f30d0",
      "number": 10,
      "content": "Đức Chúa Trời ôi, do lòng tốt của Ngài, Ngài đã chu cấp cho những người nghèo khó.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.842Z",
      "updatedAt": "2024-09-23T05:49:38.842Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "244e72da-9dd5-41d6-bdd0-48e1d74cf392",
      "number": 11,
      "content": "Khi Chúa ban lệnh,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.847Z",
      "updatedAt": "2024-09-23T05:49:38.847Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "a8788ad3-48b6-4a54-b476-95e7baa69646",
      "number": 11,
      "content": "Những phụ nữ ra đi báo tin mừng đông vô số:",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.852Z",
      "updatedAt": "2024-09-23T05:49:38.852Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "1ae07482-507d-4cbd-b91d-98a7d11c2190",
      "number": 12,
      "content": "“Các vua của các đạo quân bỏ chạy; họ chạy trốn.”",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.856Z",
      "updatedAt": "2024-09-23T05:49:38.856Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "7e65e7ca-a7d1-4c44-9e90-ea88ed89c372",
      "number": 12,
      "content": "Những phụ nữ còn ở nhà chia chiến lợi phẩm–",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.862Z",
      "updatedAt": "2024-09-23T05:49:38.862Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "090ac139-0bb5-4113-89cc-ec1ceb1b15a5",
      "number": 13,
      "content": "Dù các ông nằm nghỉ giữa các ràn chiên–",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.867Z",
      "updatedAt": "2024-09-23T05:49:38.867Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "c5ed69b2-f456-4fec-ba91-40066abb1c55",
      "number": 13,
      "content": "Các chị em như những cánh bồ câu long lanh dát bạc,",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.872Z",
      "updatedAt": "2024-09-23T05:49:38.872Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "e37be170-0e41-48ae-a597-b23c09359392",
      "number": 13,
      "content": "Với bộ lông lóng lánh ánh vàng.",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.877Z",
      "updatedAt": "2024-09-23T05:49:38.877Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "d7ae33d2-1741-4f9e-a336-1f40929a5aa1",
      "number": 14,
      "content": "Khi Đấng Toàn Năng đánh văng các vua trong xứ,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.882Z",
      "updatedAt": "2024-09-23T05:49:38.882Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "6b68c16b-79ac-462f-b4e4-94b61a347298",
      "number": 14,
      "content": "Khiến họ như tuyết rơi tơi tả ở Xanh-môn.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.887Z",
      "updatedAt": "2024-09-23T05:49:38.887Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "593ca662-bbbd-4d79-b7b6-adc6bc3f33c3",
      "number": 15,
      "content": "Núi Ba-san là núi của Đức Chúa Trời;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.893Z",
      "updatedAt": "2024-09-23T05:49:38.893Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "713553c0-decc-416b-aaf4-ec06242cc729",
      "number": 15,
      "content": "Rặng Núi Ba-san quả có nhiều đỉnh cao chót vót.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.898Z",
      "updatedAt": "2024-09-23T05:49:38.898Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "e017df79-aa9b-464d-92a7-688b32e16e48",
      "number": 16,
      "content": "Hỡi những rặng núi cao có nhiều đỉnh,",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.903Z",
      "updatedAt": "2024-09-23T05:49:38.903Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "01be1b65-8006-479e-9bc8-e02d156818a6",
      "number": 16,
      "content": "Tại sao các ngươi ganh tị với ngọn núi Đức Chúa Trời đã chọn để làm nơi Ngài ngự?",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.908Z",
      "updatedAt": "2024-09-23T05:49:38.908Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "716937de-b6ad-49c1-ac44-33e17ac33ebf",
      "number": 16,
      "content": "Thật vậy **CHÚA** sẽ ngự tại đó đời đời.",
      "order": 2,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.912Z",
      "updatedAt": "2024-09-23T05:49:38.912Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "339c07af-53b0-4e42-9a84-f4b0d3221749",
      "number": 17,
      "content": "Các xe chiến mã của Đức Chúa Trời nhiều đến hằng vạn,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.917Z",
      "updatedAt": "2024-09-23T05:49:38.917Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "591aa990-ce0d-4755-b1de-5f5a54986409",
      "number": 17,
      "content": "Thật là hằng ngàn và hằng vạn;",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.922Z",
      "updatedAt": "2024-09-23T05:49:38.922Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "67994c24-e94f-4efc-b6c7-4b049918b3b2",
      "number": 17,
      "content": "Chúa ngự giữa chúng như Ngài ngự trong nơi thánh của Ngài ở Si-nai.",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.928Z",
      "updatedAt": "2024-09-23T05:49:38.928Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "ccb9aaa0-bd70-46b4-b0ad-c16441b78906",
      "number": 18,
      "content": "Ngài đã lên nơi cao,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.933Z",
      "updatedAt": "2024-09-23T05:49:38.933Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "44c15abb-b331-4cb8-8243-4d923e509a8b",
      "number": 18,
      "content": "Ngài dẫn theo đoàn tù binh bị bắt đem đi lưu đày;",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.939Z",
      "updatedAt": "2024-09-23T05:49:38.939Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "ae4d2705-6494-4bb8-8384-8bed8772d54d",
      "number": 18,
      "content": "Ngài đã nhận những lễ vật của loài người,",
      "order": 2,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.944Z",
      "updatedAt": "2024-09-23T05:49:38.944Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "e69bacd8-ce78-4f12-b6d9-0959014b76b4",
      "number": 18,
      "content": "Ngay cả những lễ vật của những kẻ bội nghịch, để chứng tỏ rằng **CHÚA** Đức Chúa Trời cũng ở tại đó.",
      "order": 3,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.950Z",
      "updatedAt": "2024-09-23T05:49:38.950Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "fcc3ed82-ef59-4942-b28c-1feb5ec13297",
      "number": 19,
      "content": "Tôn ngợi Chúa,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.960Z",
      "updatedAt": "2024-09-23T05:49:38.960Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "88dbf09d-bc84-44a1-9c5e-e1dc36cfff13",
      "number": 19,
      "content": "Đấng hằng ngày cưu mang chúng ta;",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.965Z",
      "updatedAt": "2024-09-23T05:49:38.965Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "1b5d72c0-73e0-4c85-b399-4593ce045d29",
      "number": 19,
      "content": "Đức Chúa Trời là Đấng Giải Cứu của chúng ta. (Sê-la)",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.971Z",
      "updatedAt": "2024-09-23T05:49:38.971Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "b7b7cb7e-9e85-4ebb-837c-68e51f6ff66c",
      "number": 20,
      "content": "Đức Chúa Trời chúng ta là Đức Chúa Trời giải cứu;",
      "order": 0,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.976Z",
      "updatedAt": "2024-09-23T05:49:38.976Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "cdaf745a-c7b9-4a9a-bbf9-73a010963a15",
      "number": 20,
      "content": "Nhờ **CHÚA** Hằng Hữu, chúng ta được thoát chết.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.981Z",
      "updatedAt": "2024-09-23T05:49:38.981Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "73080d6c-e426-4525-8631-22544c40aaf2",
      "number": 21,
      "content": "Chắc chắn Đức Chúa Trời sẽ đạp nát đầu kẻ thù của Ngài,",
      "order": 0,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.991Z",
      "updatedAt": "2024-09-23T05:49:38.991Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "cf574c28-4508-4afe-a5ac-42051c9b204c",
      "number": 21,
      "content": "Tức đầu tóc của những kẻ cứ miệt mài trong tội ác.",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:38.996Z",
      "updatedAt": "2024-09-23T05:49:38.996Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "27c03b99-c816-4d96-9371-963caad9156d",
      "number": 22,
      "content": "Chúa phán,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.001Z",
      "updatedAt": "2024-09-23T05:49:39.001Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "da605c49-faa7-4100-820a-f5d58e98352d",
      "number": 22,
      "content": "“Từ Ba-san, Ta sẽ đem chúng về;",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.007Z",
      "updatedAt": "2024-09-23T05:49:39.007Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "3e4fe594-cf9f-49df-bd1a-7cb2a1ea864b",
      "number": 22,
      "content": "Từ các đại dương sâu thẳm, Ta sẽ mang chúng về,",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.012Z",
      "updatedAt": "2024-09-23T05:49:39.012Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "c7e06a8f-75b4-443a-b049-d85f90179687",
      "number": 23,
      "content": "Để các ngươi có thể giẫm chân trên máu kẻ thù của mình,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.017Z",
      "updatedAt": "2024-09-23T05:49:39.017Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "89d286be-d7a3-4404-9a40-48475b144034",
      "number": 23,
      "content": "Ngay cả lưỡi của chó các ngươi cũng sẽ được phần trên quân thù của các ngươi.”",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.023Z",
      "updatedAt": "2024-09-23T05:49:39.023Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "b5edf24b-6390-4279-a5b3-786ce301fa19",
      "number": 24,
      "content": "Đức Chúa Trời ôi, người ta đã chứng kiến lễ nghinh rước Ngài,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.028Z",
      "updatedAt": "2024-09-23T05:49:39.028Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "cd3e4344-ea67-425e-8cbe-70d169123d17",
      "number": 24,
      "content": "Tức lễ nghinh rước Đức Chúa Trời của con, Vua của con, vào đền thánh.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.033Z",
      "updatedAt": "2024-09-23T05:49:39.033Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "47404695-7cbe-48d4-99e2-e70fe735ed8c",
      "number": 25,
      "content": "Ca đoàn đi trước ngợi ca, ban nhạc theo sau trỗi nhạc,",
      "order": 0,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.038Z",
      "updatedAt": "2024-09-23T05:49:39.038Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "2df3b3df-0235-4406-a5e6-f625254749ac",
      "number": 25,
      "content": "Ở giữa là đoàn thiếu nữ đánh trống lục lạc nhịp nhàng.",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.043Z",
      "updatedAt": "2024-09-23T05:49:39.043Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "ea9aaf68-597c-4ed2-881f-00ac65a7885e",
      "number": 26,
      "content": "Hãy chúc tụng Đức Chúa Trời giữa những đoàn người đông đúc dự lễ,",
      "order": 0,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.048Z",
      "updatedAt": "2024-09-23T05:49:39.048Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "b377c15d-0d04-4c1a-8a94-048deedd5fc4",
      "number": 26,
      "content": "Hãy tôn ngợi **CHÚA**, hỡi dòng dõi của I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.053Z",
      "updatedAt": "2024-09-23T05:49:39.053Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "f1f0416a-f30d-4798-8435-3218361e5ab0",
      "number": 27,
      "content": "Kìa là Bên-gia-min, chi tộc trẻ nhất, đang dẫn đầu;",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.058Z",
      "updatedAt": "2024-09-23T05:49:39.058Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "fe8184b0-2ae3-4338-900a-9d1ef87710ac",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Giu-đa và phái đoàn của họ;",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.063Z",
      "updatedAt": "2024-09-23T05:49:39.063Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "427bab2c-b8bc-48e4-92b0-ae3205055b39",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Xê-bu-lun và các nhà quyền quý của Náp-ta-li.",
      "order": 2,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.068Z",
      "updatedAt": "2024-09-23T05:49:39.068Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "8335459c-0f7e-4106-85e4-0b0813a134d8",
      "number": 28,
      "content": "Đức Chúa Trời ôi, xin thể hiện quyền năng Ngài;",
      "order": 0,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.073Z",
      "updatedAt": "2024-09-23T05:49:39.073Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "358c1c80-bd92-4620-a4fa-d9e960b90ed7",
      "number": 28,
      "content": "Đức Chúa Trời ôi, xin bày tỏ sức mạnh Ngài, như Ngài đã từng làm cho chúng con khi trước.",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.082Z",
      "updatedAt": "2024-09-23T05:49:39.082Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "dd05d6b1-4ed8-48e6-ab66-0229a1e25156",
      "number": 29,
      "content": "Vì đền thờ Ngài tọa lạc tại Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.087Z",
      "updatedAt": "2024-09-23T05:49:39.087Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "85c19fb2-c16c-4a6d-9e3e-fbcc502f8cdd",
      "number": 29,
      "content": "Nên các vua sẽ đem các lễ vật đến dâng lên Ngài tại đó.",
      "order": 1,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.092Z",
      "updatedAt": "2024-09-23T05:49:39.092Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "9e929eb1-86bb-45bb-ae5a-cc2b163f4787",
      "number": 30,
      "content": "Cầu xin Ngài trách phạt bầy thú hoang trong đám lau sậy,",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.097Z",
      "updatedAt": "2024-09-23T05:49:39.097Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "4e175fb9-4618-40ef-9087-9f6ac038b9bc",
      "number": 30,
      "content": "Cùng đám bò đực ở giữa đàn bò con của các dân.",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.102Z",
      "updatedAt": "2024-09-23T05:49:39.102Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "448e6e69-d1f1-4ffd-af2d-57eab3cc8098",
      "number": 30,
      "content": "Xin Ngài bắt chúng phải đem bạc nén đến quỳ dâng;",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.108Z",
      "updatedAt": "2024-09-23T05:49:39.108Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "1e1ca057-3c30-4ddd-a66a-9b98b9db5d68",
      "number": 30,
      "content": "Còn những dân hiếu chiến, xin Ngài làm cho chúng tản lạc khắp nơi.",
      "order": 3,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.112Z",
      "updatedAt": "2024-09-23T05:49:39.112Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "1337eab9-a3be-4fb8-8c4e-f92dd41d0ef9",
      "number": 31,
      "content": "Những đoàn người quyền thế sẽ từ Ai-cập đến thờ phượng;",
      "order": 0,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.116Z",
      "updatedAt": "2024-09-23T05:49:39.116Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "f6c980d4-2642-4fc8-bbe6-2a438574cf17",
      "number": 31,
      "content": "Người Ê-thi-ô-pi sẽ sớm đưa tay lên hướng về Đức Chúa Trời.",
      "order": 1,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.119Z",
      "updatedAt": "2024-09-23T05:49:39.119Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "55c5b9eb-2450-43df-b1d3-5fb1ea772c5d",
      "number": 32,
      "content": "Hỡi tất cả quốc gia trên thế giới, hãy ca tụng Đức Chúa Trời;",
      "order": 0,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.125Z",
      "updatedAt": "2024-09-23T05:49:39.125Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "5c6792cf-d0a0-4fec-a7e0-576095f2fa76",
      "number": 32,
      "content": "Hãy ca ngợi **CHÚA**, (Sê-la)",
      "order": 1,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.128Z",
      "updatedAt": "2024-09-23T05:49:39.128Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "cf3c4908-48ce-4816-b7d9-4b7ae325f07a",
      "number": 33,
      "content": "Đấng ngự trên nơi chí cao của các tầng trời, tức các tầng trời đã được dựng nên từ thời thái cổ;",
      "order": 0,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.131Z",
      "updatedAt": "2024-09-23T05:49:39.131Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "8a655a19-687c-447f-a79b-131783592ddc",
      "number": 33,
      "content": "Kìa, Ngài cất tiếng nói, giọng Ngài thật oai hùng.",
      "order": 1,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.133Z",
      "updatedAt": "2024-09-23T05:49:39.133Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "820ba45b-a39b-494d-971e-7c206332a0c3",
      "number": 34,
      "content": "Khá biết rằng mọi quyền lực đều thuộc về Đức Chúa Trời;",
      "order": 0,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.136Z",
      "updatedAt": "2024-09-23T05:49:39.136Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "5f10db0a-8308-472f-8d1a-0e648c000337",
      "number": 34,
      "content": "Nguyện uy nghi Ngài tỏa rạng trên I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.138Z",
      "updatedAt": "2024-09-23T05:49:39.138Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "23c22880-3a6a-4631-a98d-2d1248298159",
      "number": 34,
      "content": "Và quyền năng Ngài đầy dẫy khắp không trung.",
      "order": 2,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.141Z",
      "updatedAt": "2024-09-23T05:49:39.141Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "8e0ea308-016f-45a7-873f-9d1e880836a3",
      "number": 35,
      "content": "Đức Chúa Trời ôi, trong đền thánh Ngài, Ngài đáng kinh hãi biết bao!",
      "order": 0,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.144Z",
      "updatedAt": "2024-09-23T05:49:39.144Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "b32f7a44-36cd-477f-8232-a43592d3fd0a",
      "number": 35,
      "content": "Chính Đức Chúa Trời của I-sơ-ra-ên đã ban sức mạnh và quyền lực cho con dân Ngài.",
      "order": 1,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.146Z",
      "updatedAt": "2024-09-23T05:49:39.146Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    },
    {
      "id": "60c1c302-3959-4a43-85cd-7b224912ae52",
      "number": 35,
      "content": "Tôn ngợi Đức Chúa Trời!",
      "order": 2,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:39.149Z",
      "updatedAt": "2024-09-23T05:49:39.149Z",
      "chapterId": "01569ef1-1d05-4ce4-b711-a1209385ae4c"
    }
  ],
