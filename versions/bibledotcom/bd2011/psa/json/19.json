{
  "chapter": {
    "id": "9490653d-3d04-4513-9ddb-1627b2f2096f",
    "number": 19,
    "ref": "https://www.bible.com/bible/19/PSA.19.BD2011",
    "createdAt": "2024-09-23T05:08:40.268Z",
    "updatedAt": "2024-09-23T05:08:40.268Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "4c2e9043-bde7-47b4-be59-22e72cac7fb8",
      "number": 1,
      "content": "Các tầng trời rao truyền vinh hiển Đức Chúa Trời;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.647Z",
      "updatedAt": "2024-09-23T05:45:42.647Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "3dcd4a27-9889-4fb1-a69d-e04f13a139d5",
      "number": 1,
      "content": "Bầu trời công bố công việc tay Ngài làm.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.658Z",
      "updatedAt": "2024-09-23T05:45:42.658Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "cce392f8-9a75-408a-8f2f-009b30bb9116",
      "number": 2,
      "content": "Ngày ngày truyền rao sứ điệp;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.660Z",
      "updatedAt": "2024-09-23T05:45:42.660Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "ed395794-ebfd-4baf-b6f4-9079c80747aa",
      "number": 2,
      "content": "Đêm đêm bày tỏ tri thức.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.662Z",
      "updatedAt": "2024-09-23T05:45:42.662Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "5a5d22c5-c125-45e0-ad7a-87b89ba58938",
      "number": 3,
      "content": "Không dùng lời, chẳng cần tiếng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.664Z",
      "updatedAt": "2024-09-23T05:45:42.664Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "063fb205-534e-4070-99b6-4c2360e3ae29",
      "number": 3,
      "content": "Chẳng ai nghe giọng nói chúng ra sao;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.666Z",
      "updatedAt": "2024-09-23T05:45:42.666Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "6a3c6a7d-7ed0-454b-8093-149763d51470",
      "number": 4,
      "content": "Nhưng âm thanh chúng đã lan truyền khắp đất,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.668Z",
      "updatedAt": "2024-09-23T05:45:42.668Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "1cd760a3-9043-46d3-9287-2bcbdaa65284",
      "number": 4,
      "content": "Lời chúng đã vang ra đến cùng tận địa cầu.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.671Z",
      "updatedAt": "2024-09-23T05:45:42.671Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "8028d547-e7db-4510-af19-028c4683c7b5",
      "number": 4,
      "content": "Ngài đã dựng lều cho mặt trời trong chúng.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.673Z",
      "updatedAt": "2024-09-23T05:45:42.673Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "5555afbc-011d-4b38-b15e-de73c10df6aa",
      "number": 5,
      "content": "Mặt trời như một tân lang ra khỏi loan phòng,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.675Z",
      "updatedAt": "2024-09-23T05:45:42.675Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "1b5d8f65-2d3a-44d6-9dd0-086ddd941569",
      "number": 5,
      "content": "Như một lực sĩ hân hoan chạy trên tuyến đường mình.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.677Z",
      "updatedAt": "2024-09-23T05:45:42.677Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "9aca8511-0615-494a-aad9-25c31f93a395",
      "number": 6,
      "content": "Mặt trời mọc lên từ phương trời này,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.679Z",
      "updatedAt": "2024-09-23T05:45:42.679Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "13116571-f2fa-4fc3-acb0-176891a2f7be",
      "number": 6,
      "content": "Chạy vòng đến phương trời kia;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.682Z",
      "updatedAt": "2024-09-23T05:45:42.682Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "58784a98-dbcb-4a43-8b12-4b39b3fd4c25",
      "number": 6,
      "content": "Không vật gì tránh khỏi hơi nóng của nó được.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.684Z",
      "updatedAt": "2024-09-23T05:45:42.684Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "596b9027-66e8-4f64-97b9-6b834fdc5926",
      "number": 7,
      "content": "Luật pháp của **CHÚA** thật trọn vẹn,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.686Z",
      "updatedAt": "2024-09-23T05:45:42.686Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "b91f3226-d0a7-4dde-902d-1e8362f3e37c",
      "number": 7,
      "content": "Làm linh hồn hồi sức;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.688Z",
      "updatedAt": "2024-09-23T05:45:42.688Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "a5613abb-8cfe-470a-8212-5f70527a8f11",
      "number": 7,
      "content": "Quy tắc của **CHÚA** thật vững chắc,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.690Z",
      "updatedAt": "2024-09-23T05:45:42.690Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "c3d177c3-96fe-4a10-b083-5088cde2497f",
      "number": 7,
      "content": "Khiến kẻ dại khôn ra.",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.693Z",
      "updatedAt": "2024-09-23T05:45:42.693Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "fc18da28-09d9-419a-842c-7eb8dd0c2af4",
      "number": 8,
      "content": "Luật lệ của **CHÚA** thật đúng đắn,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.695Z",
      "updatedAt": "2024-09-23T05:45:42.695Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "5e73f91f-c046-4d89-bd5a-3a6bf629b0c1",
      "number": 8,
      "content": "Làm cho lòng vui vẻ;",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.697Z",
      "updatedAt": "2024-09-23T05:45:42.697Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "0a7a5d7b-e8fd-4654-b78b-bd24de0142e9",
      "number": 8,
      "content": "Điều răn của **CHÚA** thật minh bạch,",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.700Z",
      "updatedAt": "2024-09-23T05:45:42.700Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "63a14206-0ee2-45b2-b1c7-15d7dd37b715",
      "number": 8,
      "content": "Khiến mắt lòng thấy rõ.",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.702Z",
      "updatedAt": "2024-09-23T05:45:42.702Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "4f8fa514-e0a1-4d2b-a552-1bf97d300c23",
      "number": 9,
      "content": "Sự kính sợ **CHÚA** thật trong sạch,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.705Z",
      "updatedAt": "2024-09-23T05:45:42.705Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "b8ec26dd-6b30-4707-a4a5-b2687ab85ae0",
      "number": 9,
      "content": "Nên còn lại đời đời;",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.710Z",
      "updatedAt": "2024-09-23T05:45:42.710Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "6f441c81-e071-4922-9a9d-02f6bb236636",
      "number": 9,
      "content": "Phán quyết của **CHÚA** thật chính đáng,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.713Z",
      "updatedAt": "2024-09-23T05:45:42.713Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "15bea1e7-90ae-42f2-b6c1-f260f054d8ee",
      "number": 9,
      "content": "Thảy đều công bình cả.",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.717Z",
      "updatedAt": "2024-09-23T05:45:42.717Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "eeb35da3-d9ab-47e8-ac64-faae06f69fd9",
      "number": 10,
      "content": "Những điều ấy quý hơn vàng,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.722Z",
      "updatedAt": "2024-09-23T05:45:42.722Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "926fc68a-cb97-4713-95b7-7477ccf7b467",
      "number": 10,
      "content": "Thật báu hơn vàng ròng;",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.728Z",
      "updatedAt": "2024-09-23T05:45:42.728Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "57b843f4-ae5d-4d84-95c7-14f6ec2ccb60",
      "number": 10,
      "content": "Chúng ngọt hơn mật,",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.733Z",
      "updatedAt": "2024-09-23T05:45:42.733Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "21b11a57-19d1-43a3-9bbc-794dfe7b94e3",
      "number": 10,
      "content": "Thật ngọt hơn mật nhểu từ tàng ong.",
      "order": 3,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.738Z",
      "updatedAt": "2024-09-23T05:45:42.738Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "3fd8d0f2-fb26-4c35-9c2c-43e004edb085",
      "number": 11,
      "content": "Hơn nữa nhờ những điều ấy, đầy tớ Ngài được cảnh giác;",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.744Z",
      "updatedAt": "2024-09-23T05:45:42.744Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "d15bd2ca-e7cf-4acc-b581-b6e9ef798d25",
      "number": 11,
      "content": "Nhờ giữ những điều ấy, tôi tớ Ngài được ban thưởng lớn lao.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.749Z",
      "updatedAt": "2024-09-23T05:45:42.749Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "abe49ce3-f38d-4abd-89b0-73c30cc1a2cf",
      "number": 12,
      "content": "Ai có thể biết hết mọi lầm lỗi của mình chăng?",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.755Z",
      "updatedAt": "2024-09-23T05:45:42.755Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "b4fae0d9-1d35-44a0-b805-f608056ee3bd",
      "number": 12,
      "content": "Cầu xin Ngài tha thứ con vì những lỗi lầm con không biết.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.760Z",
      "updatedAt": "2024-09-23T05:45:42.760Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "417d9c40-a687-4b22-b65d-b9b96a1f1c7c",
      "number": 13,
      "content": "Xin giữ tôi tớ Ngài khỏi cố ý phạm tội;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.765Z",
      "updatedAt": "2024-09-23T05:45:42.765Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "d6f8b1ea-c3a5-43ee-8ea5-2b764709d2dd",
      "number": 13,
      "content": "Xin đừng để tội lỗi cầm quyền trên con;",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.769Z",
      "updatedAt": "2024-09-23T05:45:42.769Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "c24e4d3a-9872-4117-89d3-998122648231",
      "number": 13,
      "content": "Để con sẽ không có chỗ nào đáng trách,",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.775Z",
      "updatedAt": "2024-09-23T05:45:42.775Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "0c9331ec-6401-48b2-b940-8bf0a9aeb6e6",
      "number": 13,
      "content": "Và con sẽ không phạm tội trọng.",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.780Z",
      "updatedAt": "2024-09-23T05:45:42.780Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "572a3dff-ed1c-47f6-bc9d-349574d2826f",
      "number": 14,
      "content": "Nguyện lời nói của miệng con và sự suy gẫm của lòng con",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.785Z",
      "updatedAt": "2024-09-23T05:45:42.785Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "4bd0768e-b094-4304-a7b3-f7641035ddc2",
      "number": 14,
      "content": "Được đẹp ý Ngài,",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:45:42.790Z",
      "updatedAt": "2024-09-23T05:45:42.790Z",
      "chapterId": "9490653d-3d04-4513-9ddb-1627b2f2096f"
    },
    {
      "id": "42cb01e5-4c9f-4297-a8d8-f7046488e580",
      "number": 14,
