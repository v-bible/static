    },
    {
      "id": "1e15e898-ae9c-4fa9-b37b-73f05371e140",
      "content": "CHÚA",
      "order": 2,
      "createdAt": "2024-09-23T05:53:32.589Z",
      "updatedAt": "2024-09-23T05:53:32.589Z",
      "verseId": "5a3308fc-7f6f-44cc-b85b-b9bfa640025b",
      "chapterId": "835e2982-2ef6-47ad-a269-a654bafcc12d"
    },
    {
      "id": "981089e1-e1a6-4386-a216-23800a63977c",
      "content": "và Cầu Xin Ngài Ban Chiến Thắng",
      "order": 3,
      "createdAt": "2024-09-23T05:53:32.591Z",
      "updatedAt": "2024-09-23T05:53:32.591Z",
