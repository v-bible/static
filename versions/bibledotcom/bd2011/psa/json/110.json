{
  "chapter": {
    "id": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632",
    "number": 110,
    "ref": "https://www.bible.com/bible/19/PSA.110.BD2011",
    "createdAt": "2024-09-23T05:08:40.594Z",
    "updatedAt": "2024-09-23T05:08:40.594Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "59f4900d-ac90-4d59-af19-e46aa1364ccd",
      "number": 1,
      "content": "**CHÚA** phán với Chúa của tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.398Z",
      "updatedAt": "2024-09-23T05:53:43.398Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "f825539d-2149-4113-a246-1b50c4b32ad2",
      "number": 1,
      "content": "“Con hãy ngồi bên phải Ta,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.417Z",
      "updatedAt": "2024-09-23T05:53:43.417Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "e2e61a53-1bb0-45c9-b260-8719229a3ea7",
      "number": 1,
      "content": "Cho đến khi Ta đặt những kẻ thù của Con làm bệ chân Con.”",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.422Z",
      "updatedAt": "2024-09-23T05:53:43.422Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "5aa71c69-8423-4537-ad38-936fb953e2d4",
      "number": 2,
      "content": "“Từ Si-ôn **CHÚA** sẽ đưa vương trượng uy quyền của Con ra:",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.427Z",
      "updatedAt": "2024-09-23T05:53:43.427Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "6401c334-eed4-4824-9396-4fc9a7289b1b",
      "number": 2,
      "content": "Hãy trị vì những kẻ thù của Con.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.432Z",
      "updatedAt": "2024-09-23T05:53:43.432Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "26bafce4-51a4-47e9-bd04-6dca34534eff",
      "number": 3,
      "content": "Trong ngày Con biểu dương quyền lực,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.436Z",
      "updatedAt": "2024-09-23T05:53:43.436Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "bd764826-fc7e-4808-8985-345a8decdcc5",
      "number": 3,
      "content": "Dân tộc Con sẽ hăng hái xung phong;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.442Z",
      "updatedAt": "2024-09-23T05:53:43.442Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "0a555ded-6aa2-4f99-b1b1-b49fd2c4239c",
      "number": 3,
      "content": "Họ sẽ tập họp thành đội ngũ chỉnh tề khi hừng đông ló dạng;",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.446Z",
      "updatedAt": "2024-09-23T05:53:43.446Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "ced8e3c9-3ba4-4e7c-81eb-81c3a404e077",
      "number": 3,
      "content": "Những trai trẻ sẽ quy tụ quanh Con",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.451Z",
      "updatedAt": "2024-09-23T05:53:43.451Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "3728afc0-80a0-48d4-8201-73f5af6b6523",
      "number": 3,
      "content": "Như sương phủ ban mai.”",
      "order": 4,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.456Z",
      "updatedAt": "2024-09-23T05:53:43.456Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "15429fcd-c709-4150-8084-e9b2e876d440",
      "number": 4,
      "content": "**CHÚA** đã thề",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.465Z",
      "updatedAt": "2024-09-23T05:53:43.465Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "05ec4768-0080-4fc9-93af-f800561ccf56",
      "number": 4,
      "content": "Và sẽ không đổi ý của Ngài rằng,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.473Z",
      "updatedAt": "2024-09-23T05:53:43.473Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "3748377e-f914-45c7-b413-08a5ab6d3b10",
      "number": 4,
      "content": "“Con là tư tế đời đời",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.478Z",
      "updatedAt": "2024-09-23T05:53:43.478Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "03019718-a38e-49d4-ae3e-4573b183e77f",
      "number": 4,
      "content": "Theo Dòng Mên-chi-xê-đéc.”",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.483Z",
      "updatedAt": "2024-09-23T05:53:43.483Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "41708a9a-36d3-4e45-8ba9-17986df6c515",
      "number": 5,
      "content": "Chúa ở bên phải Con.",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.488Z",
      "updatedAt": "2024-09-23T05:53:43.488Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "40487120-05dd-4b87-9a88-d123a1e8e80a",
      "number": 5,
      "content": "Ngài sẽ đập tan các vua trong ngày Ngài nổi giận.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.493Z",
      "updatedAt": "2024-09-23T05:53:43.493Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "4593a567-3137-4015-9bde-921c61e3947f",
      "number": 6,
      "content": "Ngài sẽ đoán phạt các dân;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.498Z",
      "updatedAt": "2024-09-23T05:53:43.498Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "6aa3f61d-a669-4e9c-a821-51bcc6c67ec5",
      "number": 6,
      "content": "Ngài sẽ làm cho thây những kẻ ác chất cao thành đống;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.503Z",
      "updatedAt": "2024-09-23T05:53:43.503Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "7ad93f97-3be2-4557-92c3-23a462f884b7",
      "number": 6,
      "content": "Ngài sẽ đập tan các lãnh tụ của thế gian.",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.507Z",
      "updatedAt": "2024-09-23T05:53:43.507Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "a4ceda45-3130-48a9-856f-04323554fe04",
      "number": 7,
      "content": "Ngài sẽ uống nước suối bên đường,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.513Z",
      "updatedAt": "2024-09-23T05:53:43.513Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    },
    {
      "id": "47593cc5-c41d-4da2-9476-8526dfcff6aa",
      "number": 7,
      "content": "Rồi Ngài sẽ ngẩng đầu lên cách hiên ngang đắc thắng.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:43.518Z",
      "updatedAt": "2024-09-23T05:53:43.518Z",
      "chapterId": "5fb64bc5-79db-49b1-b56f-1bbfe9d55632"
    }
  ],
