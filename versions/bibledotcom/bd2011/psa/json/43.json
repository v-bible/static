{
  "chapter": {
    "id": "34754f00-0225-41c3-9a9f-b52f4de34c77",
    "number": 43,
    "ref": "https://www.bible.com/bible/19/PSA.43.BD2011",
    "createdAt": "2024-09-23T05:08:40.342Z",
    "updatedAt": "2024-09-23T05:08:40.342Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "9f5e158e-a13b-4f8d-b9f2-e6d022d6faf7",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin minh oan cho con và binh vực duyên cớ con",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.190Z",
      "updatedAt": "2024-09-23T05:47:26.190Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "3928ec20-65ba-43c3-a92e-62a96d91d56c",
      "number": 1,
      "content": "Trước một dân vô đạo;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.220Z",
      "updatedAt": "2024-09-23T05:47:26.220Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "9a51fd56-c389-46d7-a09c-fb1bca1827e1",
      "number": 1,
      "content": "Xin giải cứu con",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.225Z",
      "updatedAt": "2024-09-23T05:47:26.225Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "fd66301e-02d0-48a7-819c-9d89afa0666c",
      "number": 1,
      "content": "Khỏi kẻ gian dối và bất công,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.230Z",
      "updatedAt": "2024-09-23T05:47:26.230Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "d2e62380-a7f0-44d5-8b5a-8b8a29163ee2",
      "number": 2,
      "content": "Vì Đức Chúa Trời ôi, Ngài là sức mạnh của con.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.234Z",
      "updatedAt": "2024-09-23T05:47:26.234Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "9bf658f4-ba24-4e5e-a256-0b512470d804",
      "number": 2,
      "content": "Sao Ngài nỡ lìa bỏ con?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.239Z",
      "updatedAt": "2024-09-23T05:47:26.239Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "0f90e8b1-0282-4c8a-b49a-84713e280e4f",
      "number": 2,
      "content": "Sao con cứ phải than khóc",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.244Z",
      "updatedAt": "2024-09-23T05:47:26.244Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "c30a80f3-df49-4002-888c-63a4d2cbdd83",
      "number": 2,
      "content": "Vì bị kẻ thù áp bức mãi thế này?",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.249Z",
      "updatedAt": "2024-09-23T05:47:26.249Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "a8f415cb-d149-4b12-93dc-5354fd01bf3e",
      "number": 3,
      "content": "Ôi, xin ban ánh sáng và chân lý của Ngài",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.254Z",
      "updatedAt": "2024-09-23T05:47:26.254Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "26125c4f-44ee-4c8a-b8af-d4e865b70703",
      "number": 3,
      "content": "Để dẫn dắt con;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.258Z",
      "updatedAt": "2024-09-23T05:47:26.258Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "ebc34c2b-4a43-431d-9493-3aecd7f36841",
      "number": 3,
      "content": "Nguyện chúng dẫn đưa con đến núi thánh của Ngài,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.263Z",
      "updatedAt": "2024-09-23T05:47:26.263Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "7eb786fe-e0e8-4011-b23a-b19ff374730d",
      "number": 3,
      "content": "Và đến đền thờ Ngài,",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.269Z",
      "updatedAt": "2024-09-23T05:47:26.269Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "38f9a60d-115e-439e-b401-1fe106041a23",
      "number": 4,
      "content": "Để con sẽ đến bàn thờ của Đức Chúa Trời,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.280Z",
      "updatedAt": "2024-09-23T05:47:26.280Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "0960e32c-ecaf-4591-aef8-ba9189bd6e4b",
      "number": 4,
      "content": "Đến với Đức Chúa Trời, niềm vui khôn tả của con,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.286Z",
      "updatedAt": "2024-09-23T05:47:26.286Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "7c59c037-29ed-4d2f-88cf-26b0ece6473a",
      "number": 4,
      "content": "Để con hòa theo tiếng đàn mà ca ngợi Ngài,",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.291Z",
      "updatedAt": "2024-09-23T05:47:26.291Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "073e5195-9d47-4ee8-a669-c852e6771bfd",
      "number": 4,
      "content": "Lạy Đức Chúa Trời, Thần của con.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.301Z",
      "updatedAt": "2024-09-23T05:47:26.301Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "519c71b9-c897-4bca-8f43-9e7dad9ce40c",
      "number": 5,
      "content": "Hỡi linh hồn ta, cớ sao ngươi buồn bã?",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.312Z",
      "updatedAt": "2024-09-23T05:47:26.312Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "19f79022-2625-4a85-abd3-0de00a207672",
      "number": 5,
      "content": "Vì sao ngươi than thở trong ta?",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.317Z",
      "updatedAt": "2024-09-23T05:47:26.317Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "5fb9c1a0-79cd-4a6f-8efd-adc013243455",
      "number": 5,
      "content": "Hãy tin cậy Đức Chúa Trời, vì ta còn phải ca ngợi Ngài,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.322Z",
      "updatedAt": "2024-09-23T05:47:26.322Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    },
    {
      "id": "eed9700d-02cb-4ea7-b8a9-ca0579740b29",
      "number": 5,
      "content": "Đấng Giải Cứu của ta và Đức Chúa Trời của ta.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:47:26.327Z",
      "updatedAt": "2024-09-23T05:47:26.327Z",
      "chapterId": "34754f00-0225-41c3-9a9f-b52f4de34c77"
    }
  ],
