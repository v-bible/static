{
  "chapter": {
    "id": "63adfed6-a826-4510-b655-a3d28f3ab7aa",
    "number": 57,
    "ref": "https://www.bible.com/bible/19/PSA.57.BD2011",
    "createdAt": "2024-09-23T05:08:40.411Z",
    "updatedAt": "2024-09-23T05:08:40.411Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "0d96cfb0-4d8e-4ff3-9bbb-1b54407c51ac",
      "number": 1,
      "content": "Xin thương xót con, Đức Chúa Trời ôi, xin thương xót con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.103Z",
      "updatedAt": "2024-09-23T05:48:36.103Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "041eaf1e-c5ba-47ad-b6f6-27630bf1f986",
      "number": 1,
      "content": "Vì linh hồn con tin cậy Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.121Z",
      "updatedAt": "2024-09-23T05:48:36.121Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "71ea12a2-1b77-4a51-bf50-8f4ec58bda87",
      "number": 1,
      "content": "Thật vậy con nương náu mình dưới bóng cánh Ngài",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.123Z",
      "updatedAt": "2024-09-23T05:48:36.123Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "614dee2c-6392-4e0e-bc10-f20ef936d119",
      "number": 1,
      "content": "Cho đến khi những cơn hủy diệt đã qua.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.125Z",
      "updatedAt": "2024-09-23T05:48:36.125Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "152bf092-9f6b-47f3-9cda-9b95d01a5f24",
      "number": 2,
      "content": "Tôi kêu cầu với Đức Chúa Trời Tối Cao,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.127Z",
      "updatedAt": "2024-09-23T05:48:36.127Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "df39c544-f392-4c4c-9c08-17195aed64a4",
      "number": 2,
      "content": "Tôi kêu cầu với Đức Chúa Trời, Đấng thực hiện ý định của Ngài cho tôi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.131Z",
      "updatedAt": "2024-09-23T05:48:36.131Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "99774a0d-ad5e-4ff3-9413-504c1a73c06c",
      "number": 3,
      "content": "Từ trời Ngài ban ơn và cứu giúp tôi;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.134Z",
      "updatedAt": "2024-09-23T05:48:36.134Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "d862ed63-fb15-49f9-9371-79ee81d6f1c8",
      "number": 3,
      "content": "Ngài làm cho những kẻ chà đạp tôi phải hổ thẹn. (Sê-la)",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.136Z",
      "updatedAt": "2024-09-23T05:48:36.136Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "50599bbd-beb2-4b50-8ef9-8cc10e018beb",
      "number": 3,
      "content": "Đức Chúa Trời sẽ thể hiện tình thương và đức thành tín của Ngài.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.138Z",
      "updatedAt": "2024-09-23T05:48:36.138Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "830f2016-4182-437b-a917-25f64f507fda",
      "number": 4,
      "content": "Linh hồn tôi ở giữa một bầy sư tử;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.140Z",
      "updatedAt": "2024-09-23T05:48:36.140Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "5cf13a05-0137-4930-b5bc-0d482e5b90df",
      "number": 4,
      "content": "Tôi nằm giữa những kẻ hung dữ như lửa chực thiêu đốt,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.143Z",
      "updatedAt": "2024-09-23T05:48:36.143Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "cefebaf4-62ac-48d2-a993-8ec1bfd10468",
      "number": 4,
      "content": "Những kẻ có răng như giáo mác và tên nhọn,",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.145Z",
      "updatedAt": "2024-09-23T05:48:36.145Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "f7a09c4c-e04f-4297-8b65-239a67fb6636",
      "number": 4,
      "content": "Và lưỡi chúng sắc bén như gươm đao.",
      "order": 3,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.147Z",
      "updatedAt": "2024-09-23T05:48:36.147Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "f7922a51-2828-4bc0-b707-f60c6f8de21c",
      "number": 5,
      "content": "Đức Chúa Trời ôi, nguyện Ngài được tôn cao trên khắp trời!",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.149Z",
      "updatedAt": "2024-09-23T05:48:36.149Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "9331c209-5a9b-4a7f-b39b-9f52dcd46cca",
      "number": 5,
      "content": "Nguyện vinh hiển Ngài luôn trỗi cao trên khắp đất!",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.152Z",
      "updatedAt": "2024-09-23T05:48:36.152Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "4613099d-100a-4390-8af3-0b1b50a4a23e",
      "number": 6,
      "content": "Chúng đã giăng lưới để đón bắt các bước chân con,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.155Z",
      "updatedAt": "2024-09-23T05:48:36.155Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "0e6aa393-cac4-4d28-929d-91065dbaeb4f",
      "number": 6,
      "content": "Khiến linh hồn con phải cúi xuống lo âu;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.160Z",
      "updatedAt": "2024-09-23T05:48:36.160Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "39f8175c-8c88-4c53-a5ba-320216ee0621",
      "number": 6,
      "content": "Chúng đã đào hầm đằng trước con,",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.164Z",
      "updatedAt": "2024-09-23T05:48:36.164Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "a245a04f-3b2a-42bf-a592-daf04a795ba1",
      "number": 6,
      "content": "Nhưng chúng đã bị sa ngay vào giữa hầm đó! (Sê-la)",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.169Z",
      "updatedAt": "2024-09-23T05:48:36.169Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "d80e955a-cfb1-4b09-b185-1c7bbe346b6a",
      "number": 7,
      "content": "Lòng con vững rồi, Đức Chúa Trời ôi,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.175Z",
      "updatedAt": "2024-09-23T05:48:36.175Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "5b5f6aa5-203f-4eb3-b3f6-b74e52083f7b",
      "number": 7,
      "content": "Lòng con vững rồi;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.180Z",
      "updatedAt": "2024-09-23T05:48:36.180Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "79d3e42c-8937-4b2b-a7b1-eca346143378",
      "number": 7,
      "content": "Con sẽ ca hát, thật vậy, con sẽ ca hát chúc tụng Ngài.",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.186Z",
      "updatedAt": "2024-09-23T05:48:36.186Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "a6cefcf8-b3ce-417c-94e0-849f2204bd39",
      "number": 8,
      "content": "Hỡi linh hồn ta, hãy thức dậy!",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.191Z",
      "updatedAt": "2024-09-23T05:48:36.191Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "f3ab5485-6bdc-4588-98c6-9c108026f7d4",
      "number": 8,
      "content": "Hỡi đàn lia và hạc cầm, hãy trỗi lên!",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.197Z",
      "updatedAt": "2024-09-23T05:48:36.197Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "4ce7e849-9581-4783-bd48-6077113cc965",
      "number": 8,
      "content": "Tôi sẽ thức dậy từ lúc bình minh để ca ngợi Ngài!",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.202Z",
      "updatedAt": "2024-09-23T05:48:36.202Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "9f772a25-78bf-428d-af54-d59d6006357d",
      "number": 9,
      "content": "Chúa ôi, con sẽ cảm tạ Ngài giữa mọi người;",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.207Z",
      "updatedAt": "2024-09-23T05:48:36.207Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "d91f54b6-86b2-47b0-b1b6-8f78f9d95083",
      "number": 9,
      "content": "Con sẽ hát ca tôn ngợi Ngài giữa các dân,",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.213Z",
      "updatedAt": "2024-09-23T05:48:36.213Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "8673645c-eb61-49d3-893e-ee8b68c6df0d",
      "number": 10,
      "content": "Vì tình thương của Ngài cao đến tận trời,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.218Z",
      "updatedAt": "2024-09-23T05:48:36.218Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "12f5741f-46a9-4eb4-a57b-400b82141087",
      "number": 10,
      "content": "Đức thành tín của Ngài vượt quá các tầng mây.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.224Z",
      "updatedAt": "2024-09-23T05:48:36.224Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "abefd584-81be-481d-b756-db8dc05fe3b4",
      "number": 11,
      "content": "Đức Chúa Trời ôi, nguyện Ngài được tôn cao trên các tầng trời!",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.229Z",
      "updatedAt": "2024-09-23T05:48:36.229Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    },
    {
      "id": "86d44170-0e67-4266-9d69-fe5d78ea253a",
      "number": 11,
      "content": "Nguyện vinh hiển Ngài trỗi cao trên khắp đất!",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:36.234Z",
      "updatedAt": "2024-09-23T05:48:36.234Z",
      "chapterId": "63adfed6-a826-4510-b655-a3d28f3ab7aa"
    }
  ],
