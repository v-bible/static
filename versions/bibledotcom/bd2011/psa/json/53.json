{
  "chapter": {
    "id": "8d379af3-0e99-4a92-aa14-7711a848ad0d",
    "number": 53,
    "ref": "https://www.bible.com/bible/19/PSA.53.BD2011",
    "createdAt": "2024-09-23T05:08:40.395Z",
    "updatedAt": "2024-09-23T05:08:40.395Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "379ceff0-fb64-4f68-846e-4b655203910f",
      "number": 1,
      "content": "Kẻ dại nói trong lòng rằng, “Không có Đức Chúa Trời!”",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.841Z",
      "updatedAt": "2024-09-23T05:48:11.841Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "e80ed6b3-5f74-452d-8b4f-1d4d10704545",
      "number": 1,
      "content": "Chúng nó đều bại hoại; chúng nó phạm những tội lỗi đáng ghê tởm;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.852Z",
      "updatedAt": "2024-09-23T05:48:11.852Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "d2256fed-61ec-47e9-9919-802ce455488b",
      "number": 1,
      "content": "Chẳng ai làm điều thiện.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.854Z",
      "updatedAt": "2024-09-23T05:48:11.854Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "4e75bc6a-3983-4e97-9c51-d35a94579145",
      "number": 2,
      "content": "Đức Chúa Trời từ trời",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.856Z",
      "updatedAt": "2024-09-23T05:48:11.856Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "73dab414-f5c3-46b6-b70d-149aaa0bc971",
      "number": 2,
      "content": "Nhìn xuống con cái loài người,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.858Z",
      "updatedAt": "2024-09-23T05:48:11.858Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "a62a4965-d34f-4740-abaf-670e42e58776",
      "number": 2,
      "content": "Xem có ai là người trí thức,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.860Z",
      "updatedAt": "2024-09-23T05:48:11.860Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "7149e19d-638c-4d02-a889-4ffef39800b6",
      "number": 2,
      "content": "Biết tìm kiếm Đức Chúa Trời chăng.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.862Z",
      "updatedAt": "2024-09-23T05:48:11.862Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "b50ce582-053e-456d-89f8-4e0b30a73535",
      "number": 3,
      "content": "Họ thảy đều bại hoại;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.864Z",
      "updatedAt": "2024-09-23T05:48:11.864Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "649a7e5c-1e54-49ce-b3c4-567e2c268a2a",
      "number": 3,
      "content": "Họ cùng nhau trở nên ô uế;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.866Z",
      "updatedAt": "2024-09-23T05:48:11.866Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "231e8f69-55f7-4552-8c50-c8e521cb7684",
      "number": 3,
      "content": "Chẳng có người nào làm điều thiện,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.869Z",
      "updatedAt": "2024-09-23T05:48:11.869Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "f46bfb9a-d9ac-46b4-a6b4-2bba53689a7c",
      "number": 3,
      "content": "Dẫu một người cũng không.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.874Z",
      "updatedAt": "2024-09-23T05:48:11.874Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "6db19ccf-95a3-443c-b2b4-f1285934191d",
      "number": 4,
      "content": "Tại sao những kẻ làm ác chẳng hiểu biết?",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.878Z",
      "updatedAt": "2024-09-23T05:48:11.878Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "0e6c539c-6ba1-4574-8414-b9a777a8c9c5",
      "number": 4,
      "content": "Tại sao chúng cứ ăn nuốt dân tôi như thể ăn bánh?",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.883Z",
      "updatedAt": "2024-09-23T05:48:11.883Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "2eb61e4e-2d38-4433-bee8-409a9064c1cf",
      "number": 4,
      "content": "Tại sao chúng chẳng kêu cầu Đức Chúa Trời?",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.888Z",
      "updatedAt": "2024-09-23T05:48:11.888Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "e4961845-f33a-4899-983a-1e77a4b6d24b",
      "number": 5,
      "content": "Chúng sẽ rất sợ hãi dầu chẳng có gì đáng sợ,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.892Z",
      "updatedAt": "2024-09-23T05:48:11.892Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "50f98314-0b51-4bf7-8ca2-686b19c1f332",
      "number": 5,
      "content": "Vì Đức Chúa Trời sẽ rải xương những kẻ vây hãm bạn;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.897Z",
      "updatedAt": "2024-09-23T05:48:11.897Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "9c08fe99-cd6c-4303-b8ee-5ab71bfac2d5",
      "number": 5,
      "content": "Bạn làm cho chúng phải hổ thẹn,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.902Z",
      "updatedAt": "2024-09-23T05:48:11.902Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "24e22753-4943-4b45-9496-3e63fc563486",
      "number": 5,
      "content": "Vì Đức Chúa Trời đã loại bỏ chúng.",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.907Z",
      "updatedAt": "2024-09-23T05:48:11.907Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "639b7e8b-3051-41d9-9382-441375b177e3",
      "number": 6,
      "content": "Kìa, ơn cứu rỗi của I-sơ-ra-ên đến từ Si-ôn!",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.912Z",
      "updatedAt": "2024-09-23T05:48:11.912Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "621a6e82-3b87-4322-afd7-890e733928b7",
      "number": 6,
      "content": "Khi Đức Chúa Trời tái lập vận mạng của con dân Ngài,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.916Z",
      "updatedAt": "2024-09-23T05:48:11.916Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    },
    {
      "id": "d25ee3cf-4a46-45bc-a598-e14f9e4cc5cb",
      "number": 6,
      "content": "Gia-cốp sẽ mừng rỡ, I-sơ-ra-ên sẽ vui mừng.",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:11.926Z",
      "updatedAt": "2024-09-23T05:48:11.926Z",
      "chapterId": "8d379af3-0e99-4a92-aa14-7711a848ad0d"
    }
  ],
