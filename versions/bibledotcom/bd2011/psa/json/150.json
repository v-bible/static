  "verses": [
    {
      "id": "c4edffb7-3acd-458a-8a5f-14495b9d958f",
      "number": 1,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.703Z",
      "updatedAt": "2024-09-23T05:44:29.703Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "9a6929fc-e3c2-4cb5-9e3b-c1a510d7f452",
      "number": 6,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.774Z",
      "updatedAt": "2024-09-23T05:44:29.774Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "ef0b2b98-bbac-4592-9580-ffb5a14ee1db",
      "number": 1,
      "content": "Hãy ca ngợi Đức Chúa Trời trong nơi thánh Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.725Z",
      "updatedAt": "2024-09-23T05:44:29.725Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "790658c5-865d-483a-94c9-cf847aea061c",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong bầu trời lớn lao vô tận của Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.729Z",
      "updatedAt": "2024-09-23T05:44:29.729Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "186023f9-e644-4d3e-8e2b-acb75e8f0636",
      "number": 2,
      "content": "Hãy ca ngợi Ngài về những việc quyền năng Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.733Z",
      "updatedAt": "2024-09-23T05:44:29.733Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "682e172e-2aba-4f2e-be3c-b190a365e56c",
      "number": 2,
      "content": "Hãy ca ngợi Ngài theo sự lớn lao vĩ đại của Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.737Z",
      "updatedAt": "2024-09-23T05:44:29.737Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "5145123f-f1ca-472a-a55d-1b5e720a28c4",
      "number": 3,
      "content": "Hãy dùng tiếng kèn ca ngợi Ngài;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.741Z",
      "updatedAt": "2024-09-23T05:44:29.741Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "abe042ad-fcf7-4fe8-ad8e-9f21e239342d",
      "number": 3,
      "content": "Hãy dùng đàn lia và hạc cầm ca ngợi Ngài;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.746Z",
      "updatedAt": "2024-09-23T05:44:29.746Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "3e82fcc7-d94b-4b8a-8ecc-ed806082326a",
      "number": 4,
      "content": "Hãy dùng trống lục lạc và điệu vũ ca ngợi Ngài;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.750Z",
      "updatedAt": "2024-09-23T05:44:29.750Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "4100c260-6ca1-439b-9546-c946e068caef",
      "number": 4,
      "content": "Hãy dùng các nhạc khí bằng dây và ống sáo ca ngợi Ngài;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.755Z",
      "updatedAt": "2024-09-23T05:44:29.755Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "dbdabff7-06fa-4a92-8c50-104f8cd92710",
      "number": 5,
      "content": "Hãy dùng các phèng la vang tiếng ca ngợi Ngài;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.760Z",
      "updatedAt": "2024-09-23T05:44:29.760Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "8b021014-7047-4985-90b9-f19522bf97ae",
      "number": 5,
      "content": "Hãy dùng các khánh chiêng vang rền ca ngợi Ngài;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.765Z",
      "updatedAt": "2024-09-23T05:44:29.765Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    },
    {
      "id": "100c52fb-31cb-4cc4-8ea0-0eb935a0a1e7",
      "number": 6,
      "content": "Phàm vật chi thở, hãy ca ngợi **CHÚA**.",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:29.769Z",
      "updatedAt": "2024-09-23T05:44:29.769Z",
      "chapterId": "774d2dc5-5199-442e-91d6-c64bedb44113"
    }
  ],
