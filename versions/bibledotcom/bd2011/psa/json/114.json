{
  "chapter": {
    "id": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4",
    "number": 114,
    "ref": "https://www.bible.com/bible/19/PSA.114.BD2011",
    "createdAt": "2024-09-23T05:08:40.606Z",
    "updatedAt": "2024-09-23T05:08:40.606Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "5d80e2b9-32e6-49ff-a4f7-1185591045b4",
      "number": 1,
      "content": "Khi dân I-sơ-ra-ên ra khỏi Ai-cập,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.667Z",
      "updatedAt": "2024-09-23T05:54:05.667Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "de05b5e9-429c-4609-b6c4-3d42d1f4c5fd",
      "number": 1,
      "content": "Nhà Gia-cốp ra khỏi một dân nói một ngôn ngữ lạ,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.678Z",
      "updatedAt": "2024-09-23T05:54:05.678Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "2280e4fe-d29f-42c0-bbf2-12f01a291005",
      "number": 2,
      "content": "Giu-đa trở thành nơi thánh của Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.680Z",
      "updatedAt": "2024-09-23T05:54:05.680Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "6dd19175-1547-485e-a6d0-b1ba1ddb59c8",
      "number": 2,
      "content": "I-sơ-ra-ên trở nên vương quốc của Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.683Z",
      "updatedAt": "2024-09-23T05:54:05.683Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "9e13beb2-85e2-46be-9258-6fbda379acca",
      "number": 3,
      "content": "Nước đại dương trông thấy thế liền hoảng kinh chạy trốn;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.685Z",
      "updatedAt": "2024-09-23T05:54:05.685Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "fbc41921-a470-4be6-bacd-ac98f6703bd9",
      "number": 3,
      "content": "Nước Sông Giô-đanh hỗn loạn chảy ngược về nguồn;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.687Z",
      "updatedAt": "2024-09-23T05:54:05.687Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "b013975a-a533-4c24-99f1-47069d219eb8",
      "number": 4,
      "content": "Các rặng núi nhảy lên như chiên đực;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.689Z",
      "updatedAt": "2024-09-23T05:54:05.689Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "11ce4c21-6b56-4f1e-acd1-97959a6c7fa8",
      "number": 4,
      "content": "Các dãy đồi nhảy cỡn như chiên con.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.692Z",
      "updatedAt": "2024-09-23T05:54:05.692Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "8a8264f8-cb41-4887-8efa-640848a28b5e",
      "number": 5,
      "content": "Hỡi nước đại dương, điều gì đã khiến ngươi để ngươi chạy trốn?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.694Z",
      "updatedAt": "2024-09-23T05:54:05.694Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "5cbe200c-a3cc-47d0-972b-d59d6b38bd9f",
      "number": 5,
      "content": "Hỡi nước Sông Giô-đanh, sao ngươi lại chảy ngược về nguồn?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.696Z",
      "updatedAt": "2024-09-23T05:54:05.696Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "0e28ecf8-c873-4386-b383-3a2b01de1e07",
      "number": 6,
      "content": "Hỡi các rặng núi, sao các ngươi nhảy lên như chiên đực?",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.699Z",
      "updatedAt": "2024-09-23T05:54:05.699Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "0e7e22f8-41fa-49a6-be32-e448bdb73281",
      "number": 6,
      "content": "Hỡi các dãy đồi, sao các ngươi nhảy cỡn như chiên con?",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.703Z",
      "updatedAt": "2024-09-23T05:54:05.703Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "6a157d72-dacc-4910-a55b-b4a5d9342391",
      "number": 7,
      "content": "Hỡi trái đất, khá run rẩy trước mặt Chúa,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.707Z",
      "updatedAt": "2024-09-23T05:54:05.707Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "c0f0d7ce-27c0-4994-9a70-5786afbfdd46",
      "number": 7,
      "content": "Tức trước thánh nhan Đức Chúa Trời của Gia-cốp,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.712Z",
      "updatedAt": "2024-09-23T05:54:05.712Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "81f86438-d66a-4d2a-a0d0-e605acecaefd",
      "number": 8,
      "content": "Đấng khiến nước từ một vầng đá tuôn ra thành ao hồ,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.717Z",
      "updatedAt": "2024-09-23T05:54:05.717Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    },
    {
      "id": "d61e7d18-5f6a-4bf2-9c53-53a6ebbe9a57",
      "number": 8,
      "content": "Từ tảng đá hoa cương, dòng nước ngọt lai láng trào ra.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:54:05.722Z",
      "updatedAt": "2024-09-23T05:54:05.722Z",
      "chapterId": "4ff8e2db-2764-4dd0-b369-35c9dcce17e4"
    }
  ],
