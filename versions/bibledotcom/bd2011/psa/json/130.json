{
  "chapter": {
    "id": "70c8246d-7463-44ef-b1d9-9beb93c3aac3",
    "number": 130,
    "ref": "https://www.bible.com/bible/19/PSA.130.BD2011",
    "createdAt": "2024-09-23T05:08:40.651Z",
    "updatedAt": "2024-09-23T05:08:40.651Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "a00e25db-8759-4577-9607-21e6295ef43a",
      "number": 1,
      "content": "Lạy **CHÚA**, từ chốn vực sâu con kêu cầu Ngài.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.077Z",
      "updatedAt": "2024-09-23T05:55:19.077Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "561a6d7b-e1e7-458b-8778-0903582c945c",
      "number": 2,
      "content": "Chúa ôi, xin nghe tiếng cầu xin của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.092Z",
      "updatedAt": "2024-09-23T05:55:19.092Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "a87e0873-4311-457c-bc5b-b66bb2ec4764",
      "number": 2,
      "content": "Xin lắng tai nghe",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.094Z",
      "updatedAt": "2024-09-23T05:55:19.094Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "5c9c926a-e6fc-47f3-927d-379f47af4061",
      "number": 2,
      "content": "Lời cầu nguyện của con.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.096Z",
      "updatedAt": "2024-09-23T05:55:19.096Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "b8862ce3-ff38-4d26-80ef-4069adc0a0dd",
      "number": 3,
      "content": "Lạy **CHÚA**, nếu Ngài ghi nhớ mọi tội lỗi chúng con,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.098Z",
      "updatedAt": "2024-09-23T05:55:19.098Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "1d049bc1-ce54-4242-ae3d-74e93c8b65af",
      "number": 3,
      "content": "Chúa ôi, còn ai đứng nổi trước mặt Ngài?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.099Z",
      "updatedAt": "2024-09-23T05:55:19.099Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "1028888d-8d49-443e-a257-60448eaaff9b",
      "number": 4,
      "content": "Nhưng vì Ngài ban ơn tha thứ,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.101Z",
      "updatedAt": "2024-09-23T05:55:19.101Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "78e19d08-cdb8-45cc-a06a-b3b46bae94c3",
      "number": 4,
      "content": "Nên chúng con càng kính sợ Ngài hơn.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.103Z",
      "updatedAt": "2024-09-23T05:55:19.103Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "3bec9516-57e9-48a0-adbb-c48a7d434085",
      "number": 5,
      "content": "Tôi trông đợi **CHÚA**, linh hồn tôi trông đợi Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.105Z",
      "updatedAt": "2024-09-23T05:55:19.105Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "1929d373-8479-43e8-8150-677b1779eec7",
      "number": 5,
      "content": "Tôi đặt trọn hy vọng nơi lời Ngài.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.107Z",
      "updatedAt": "2024-09-23T05:55:19.107Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "523591b1-e00f-4829-a259-fad065182510",
      "number": 6,
      "content": "Linh hồn tôi trông đợi Chúa",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.109Z",
      "updatedAt": "2024-09-23T05:55:19.109Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "62d2379f-fc71-4b3c-b52d-6846b88bca74",
      "number": 6,
      "content": "Hơn người lính canh trông đợi sáng,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.111Z",
      "updatedAt": "2024-09-23T05:55:19.111Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "94b7c9d4-2ec2-42c3-94b2-f42317f5b235",
      "number": 6,
      "content": "Thật hơn người lính canh trông đợi sáng.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.113Z",
      "updatedAt": "2024-09-23T05:55:19.113Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "56c793b2-f127-4d78-8922-974a7784bf6e",
      "number": 7,
      "content": "Hỡi I-sơ-ra-ên, hãy đặt trọn hy vọng nơi **CHÚA**,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.114Z",
      "updatedAt": "2024-09-23T05:55:19.114Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "5da388fd-d97c-40a1-bb92-8d1c437f4829",
      "number": 7,
      "content": "Vì nơi **CHÚA** có tình thương;",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.116Z",
      "updatedAt": "2024-09-23T05:55:19.116Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "a3d227c7-1356-41df-bdb5-223510499943",
      "number": 7,
      "content": "Ngài ban ơn cứu chuộc cách dồi dào.",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.118Z",
      "updatedAt": "2024-09-23T05:55:19.118Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "8fd3ec4b-d3c3-400a-8a4c-6828008a0691",
      "number": 8,
      "content": "Ngài sẽ cứu chuộc I-sơ-ra-ên",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.120Z",
      "updatedAt": "2024-09-23T05:55:19.120Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    },
    {
      "id": "9c96966b-978b-4f92-9e41-2cadada83089",
      "number": 8,
      "content": "Khỏi mọi tội lỗi của họ.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:19.122Z",
      "updatedAt": "2024-09-23T05:55:19.122Z",
      "chapterId": "70c8246d-7463-44ef-b1d9-9beb93c3aac3"
    }
  ],
