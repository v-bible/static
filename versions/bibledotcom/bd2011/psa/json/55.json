    {
      "id": "faeacfc8-160c-4e9b-b981-7e4bd6c270b5",
      "content": "Cho Kẻ Phản Bội Bị Tiêu Diệt",
      "order": 3,
      "createdAt": "2024-09-23T05:48:24.269Z",
