  "headings": [
    {
      "id": "a9b27a93-3647-4bc4-a401-1d41d51a7d82",
      "content": "Lòng Tốt của Đức Chúa Trời Đối Với Sự Lầm Lạc của Dân I-sơ-ra-ên",
      "order": 0,
      "createdAt": "2024-09-23T05:50:57.744Z",
      "updatedAt": "2024-09-23T05:50:57.744Z",
