{
  "chapter": {
    "id": "2b7e7924-d858-4c07-a106-329df5613a58",
    "number": 81,
    "ref": "https://www.bible.com/bible/19/PSA.81.BD2011",
    "createdAt": "2024-09-23T05:08:40.495Z",
    "updatedAt": "2024-09-23T05:08:40.495Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "61141875-9f30-425a-93a9-2c6d4ae0ab47",
      "number": 1,
      "content": "Hãy cất tiếng ca ngợi Đức Chúa Trời, sức mạnh của chúng ta;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.738Z",
      "updatedAt": "2024-09-23T05:50:57.738Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "c41523e0-67a7-4cb2-b074-583a2684396a",
      "number": 1,
      "content": "Hãy lớn tiếng vui vẻ ca tụng Đức Chúa Trời của Gia-cốp.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.748Z",
      "updatedAt": "2024-09-23T05:50:57.748Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "355bc2fc-a679-409a-878b-40535f3ac6d2",
      "number": 2,
      "content": "Hãy trỗi nhạc, hãy đánh trống lục lạc,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.753Z",
      "updatedAt": "2024-09-23T05:50:57.753Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "6db2d25c-7f27-4d97-954f-12c7b92dbb85",
      "number": 2,
      "content": "Hãy dùng đàn lia và hạc cầm hòa tấu với nhau.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.758Z",
      "updatedAt": "2024-09-23T05:50:57.758Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "3a639ef7-6cc5-4896-8eba-a835479d6e9d",
      "number": 3,
      "content": "Hãy thổi kèn vang lên vào Ngày Mồng Một,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.764Z",
      "updatedAt": "2024-09-23T05:50:57.764Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "c7b36fd6-342f-4c95-a28c-9445447e1dff",
      "number": 3,
      "content": "Vào Ngày Rằm, và vào các ngày lễ trọng thể của chúng ta,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.773Z",
      "updatedAt": "2024-09-23T05:50:57.773Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "33ea0bcc-14f3-4a18-883d-7f30d4c807dc",
      "number": 4,
      "content": "Vì ấy là một luật định cho I-sơ-ra-ên,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.782Z",
      "updatedAt": "2024-09-23T05:50:57.782Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "161ddba4-7df9-4ed2-aea5-dec30879994b",
      "number": 4,
      "content": "Một mạng lịnh của Đức Chúa Trời của Gia-cốp.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.787Z",
      "updatedAt": "2024-09-23T05:50:57.787Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "ccf5f1d6-e614-438e-a34d-8b0a7a3a63f7",
      "number": 5,
      "content": "Ngài đã lập điều ấy để làm một chứng tích giữa Giô-sép,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.793Z",
      "updatedAt": "2024-09-23T05:50:57.793Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "0e2697d1-1561-4b48-9ceb-1486627d84ab",
      "number": 5,
      "content": "Khi Ngài đi trừng phạt trên đất Ai-cập,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.798Z",
      "updatedAt": "2024-09-23T05:50:57.798Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "9db5dfd9-1f9d-4308-aaae-754595556a2d",
      "number": 5,
      "content": "Nơi tôi nghe một ngôn ngữ tôi chẳng hiểu.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.805Z",
      "updatedAt": "2024-09-23T05:50:57.805Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "551fe306-ad4f-4e1c-84a9-632c45d338d9",
      "number": 6,
      "content": "“Ta đã cất gánh nặng của kẻ làm nô lệ khỏi vai nó;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.810Z",
      "updatedAt": "2024-09-23T05:50:57.810Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "8cbfb754-dd55-4e23-b3a2-ac8761ff3bea",
      "number": 6,
      "content": "Đôi tay nó đã khỏi bưng thúng giỏ của kẻ lao nô.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.815Z",
      "updatedAt": "2024-09-23T05:50:57.815Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "f97b8bcd-ebba-4c31-b056-9ea612b0354f",
      "number": 7,
      "content": "Trong cơn hoạn nạn ngươi đã kêu cầu, và Ta đã giải cứu ngươi;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.821Z",
      "updatedAt": "2024-09-23T05:50:57.821Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "e8629df9-9695-4fed-8bc5-abff2c4c6126",
      "number": 7,
      "content": "Từ trời cao huyền bí, nơi sấm sét phát ra, Ta đã đáp lời ngươi,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.825Z",
      "updatedAt": "2024-09-23T05:50:57.825Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "0d340972-eefe-4b25-8739-876ddaa2b66e",
      "number": 7,
      "content": "Ta đã thử lòng ngươi qua việc thiếu nước ở Mê-ri-ba. (Sê-la)",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.831Z",
      "updatedAt": "2024-09-23T05:50:57.831Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "cfa813b3-c948-44fb-8de4-4064ecd1438b",
      "number": 8,
      "content": "Hỡi dân Ta, hãy nghe, Ta khuyến cáo các ngươi;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.836Z",
      "updatedAt": "2024-09-23T05:50:57.836Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "26601cb1-c766-4a1f-81a1-80e9e1caec66",
      "number": 8,
      "content": "Hỡi I-sơ-ra-ên, ước gì ngươi chịu nghe Ta!",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.840Z",
      "updatedAt": "2024-09-23T05:50:57.840Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "e96df8b4-ac84-4df7-8bc2-e93c564c7802",
      "number": 9,
      "content": "Ở giữa ngươi chớ có thần lạ nào;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.845Z",
      "updatedAt": "2024-09-23T05:50:57.845Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "0d00ea43-dab4-4d9b-89ac-c503cee56a7f",
      "number": 9,
      "content": "Ngươi chớ thờ lạy bất cứ thần nào khác.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.850Z",
      "updatedAt": "2024-09-23T05:50:57.850Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "2d65da29-6b8a-4e1c-8ecc-d0afbcd4c214",
      "number": 10,
      "content": "Ta là **CHÚA**, Đức Chúa Trời của ngươi,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.854Z",
      "updatedAt": "2024-09-23T05:50:57.854Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "65aec13e-9241-4938-871a-51746aa76e1d",
      "number": 10,
      "content": "Đấng đã đem ngươi lên từ đất Ai-cập;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.864Z",
      "updatedAt": "2024-09-23T05:50:57.864Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "e7d2c736-87f7-429b-ad01-8114a278f77b",
      "number": 10,
      "content": "Hãy mở rộng miệng ngươi ra để Ta cho nó được no đầy.",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.869Z",
      "updatedAt": "2024-09-23T05:50:57.869Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "433315b5-c367-43a7-8645-446ed993e52f",
      "number": 11,
      "content": "Nhưng dân Ta chẳng nghe tiếng Ta;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.874Z",
      "updatedAt": "2024-09-23T05:50:57.874Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "5c95e679-c0ca-4e74-b76d-2ff633414132",
      "number": 11,
      "content": "I-sơ-ra-ên chẳng vâng lời Ta;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.879Z",
      "updatedAt": "2024-09-23T05:50:57.879Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "8a5847a0-a445-4a34-ace1-7b9198012dce",
      "number": 12,
      "content": "Nên Ta đã phó chúng cho tính bướng bỉnh của lòng chúng,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.885Z",
      "updatedAt": "2024-09-23T05:50:57.885Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "36aa72f7-090c-498c-b5aa-d6d04564438a",
      "number": 12,
      "content": "Để chúng cứ đi theo mưu ý của chúng.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.889Z",
      "updatedAt": "2024-09-23T05:50:57.889Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "c078ed4b-2930-4584-90d4-89f5ecb3ed4d",
      "number": 13,
      "content": "Ôi, ước gì dân Ta chịu nghe Ta;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.895Z",
      "updatedAt": "2024-09-23T05:50:57.895Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "c353ea0a-ebbd-4a13-b58a-85cc606d6036",
      "number": 13,
      "content": "Phải chi I-sơ-ra-ên cứ đi trong đường lối Ta.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.900Z",
      "updatedAt": "2024-09-23T05:50:57.900Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "942813f2-6cc8-4dc5-9444-25110aebf1e0",
      "number": 14,
      "content": "Chắc hẳn Ta đã mau lẹ triệt hạ những kẻ thù của chúng rồi,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.905Z",
      "updatedAt": "2024-09-23T05:50:57.905Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "bc6f528e-8e52-4d26-96d8-be15ef050878",
      "number": 14,
      "content": "Và dùng tay Ta đối phó những kẻ chống nghịch chúng rồi.”",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.910Z",
      "updatedAt": "2024-09-23T05:50:57.910Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "47b0a535-2aa2-48e9-bd41-452930a75a67",
      "number": 15,
      "content": "Nguyện những kẻ ghét **CHÚA** phải phủ phục trước mặt Ngài,",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.915Z",
      "updatedAt": "2024-09-23T05:50:57.915Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "1626574a-c734-49c2-83fd-afca888c3dc1",
      "number": 15,
      "content": "Và số phận của chúng phải như thế đời đời.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.920Z",
      "updatedAt": "2024-09-23T05:50:57.920Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "2c61934f-85e8-4ef8-9a56-20f23233aa46",
      "number": 16,
      "content": "Nhưng nguyện Ngài nuôi con dân Ngài bằng gạo thơm ngon nhất,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.925Z",
      "updatedAt": "2024-09-23T05:50:57.925Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    },
    {
      "id": "6da0275a-ffaf-4d71-bf3c-5346d4c2fc5e",
      "number": 16,
      "content": "Và “Ta cho ngươi no thỏa bằng mật ngọt lấy ra từ vầng đá.”",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:50:57.929Z",
      "updatedAt": "2024-09-23T05:50:57.929Z",
      "chapterId": "2b7e7924-d858-4c07-a106-329df5613a58"
    }
  ],
