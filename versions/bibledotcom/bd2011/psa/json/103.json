{
  "chapter": {
    "id": "086b978b-f73e-4f80-9a68-d8f096b800b3",
    "number": 103,
    "ref": "https://www.bible.com/bible/19/PSA.103.BD2011",
    "createdAt": "2024-09-23T05:08:40.559Z",
    "updatedAt": "2024-09-23T05:08:40.559Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "a6dc0aab-342e-400a-a01e-29f70d089879",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi **CHÚA**;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.119Z",
      "updatedAt": "2024-09-23T05:53:04.119Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "ec2be5d9-3780-430b-9e6c-33dcee8ae073",
      "number": 1,
      "content": "Hỡi tất cả những gì ở trong ta,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.135Z",
      "updatedAt": "2024-09-23T05:53:04.135Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "26253785-6f84-4230-b0e5-749328b0c0f7",
      "number": 1,
      "content": "Hãy ca tụng danh thánh của Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.137Z",
      "updatedAt": "2024-09-23T05:53:04.137Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "1b8345ea-416a-44e7-a878-105d17f30744",
      "number": 2,
      "content": "Hỡi linh hồn ta, hãy ca ngợi **CHÚA**,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.141Z",
      "updatedAt": "2024-09-23T05:53:04.141Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "2f23091d-99b0-459d-ade0-db37a84d119e",
      "number": 2,
      "content": "Chớ quên mọi phước hạnh của Ngài,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.146Z",
      "updatedAt": "2024-09-23T05:53:04.146Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "e80a1370-6ac7-468c-8c5d-de73b2a7cced",
      "number": 3,
      "content": "Đấng tha thứ mọi tội lỗi ngươi;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.150Z",
      "updatedAt": "2024-09-23T05:53:04.150Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "4cffc642-7a14-4903-bc96-526a165a3b08",
      "number": 3,
      "content": "Đấng chữa lành mọi bịnh tật ngươi;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.155Z",
      "updatedAt": "2024-09-23T05:53:04.155Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "d1a416d2-997c-42c4-a1fd-7c34dab6da30",
      "number": 4,
      "content": "Đấng cứu chuộc mạng sống ngươi thoát khỏi mộ phần;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.160Z",
      "updatedAt": "2024-09-23T05:53:04.160Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "7fc9c505-3525-4acf-8d30-9d8eda2168b1",
      "number": 4,
      "content": "Đấng đội trên đầu ngươi tình thương và lòng thương xót;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.165Z",
      "updatedAt": "2024-09-23T05:53:04.165Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "ebac32fd-bb3f-4f88-8cac-0969939ffd32",
      "number": 5,
      "content": "Đấng cho ngươi no thỏa với thức ăn ngon,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.170Z",
      "updatedAt": "2024-09-23T05:53:04.170Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "8ecdf553-4fc9-4aba-9025-35f1c6500cc6",
      "number": 5,
      "content": "Nhờ đó sức lực ngươi trẻ lại như sức của phượng hoàng.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.174Z",
      "updatedAt": "2024-09-23T05:53:04.174Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "68c32be7-db61-46bd-9488-47dfd34e4c57",
      "number": 6,
      "content": "**CHÚA** thi hành sự công chính",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.180Z",
      "updatedAt": "2024-09-23T05:53:04.180Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "602f9246-0b3b-43c0-9e21-c32d35dd0a55",
      "number": 6,
      "content": "Và công lý cho mọi người bị áp bức.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.185Z",
      "updatedAt": "2024-09-23T05:53:04.185Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "03ec41b4-87d5-4a92-92b6-9bf19dc6fa35",
      "number": 7,
      "content": "Ngài bày tỏ cho Môi-se các đường lối Ngài,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.190Z",
      "updatedAt": "2024-09-23T05:53:04.190Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "98787e3d-a921-4038-b9df-82c7f1abf481",
      "number": 7,
      "content": "Cho dân I-sơ-ra-ên các công việc Ngài.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.194Z",
      "updatedAt": "2024-09-23T05:53:04.194Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "806e559f-bba4-4b8a-b7c2-0f5adacc9d59",
      "number": 8,
      "content": "**CHÚA** hay thương xót và khoan dung độ lượng,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.199Z",
      "updatedAt": "2024-09-23T05:53:04.199Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "3d5ea853-96c0-4c0a-8ef5-2d5cd0fdb518",
      "number": 8,
      "content": "Chậm nóng giận và đầy dẫy tình thương.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.204Z",
      "updatedAt": "2024-09-23T05:53:04.204Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "b1f451ff-fc04-426c-a5fe-f9cbac262a31",
      "number": 9,
      "content": "Ngài không bắt tội luôn luôn;",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.209Z",
      "updatedAt": "2024-09-23T05:53:04.209Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "4eb30dd1-0241-41b8-a5b8-577e8271ce6a",
      "number": 9,
      "content": "Ngài cũng chẳng giữ lòng giận đến đời đời.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.214Z",
      "updatedAt": "2024-09-23T05:53:04.214Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "04482336-9baa-41d2-853f-ab8c3ff12270",
      "number": 10,
      "content": "Ngài không đối xử với chúng ta theo các tội của chúng ta;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.220Z",
      "updatedAt": "2024-09-23T05:53:04.220Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "1851a701-1b13-4f41-b4af-764b49c0fa29",
      "number": 10,
      "content": "Ngài cũng không báo trả chúng ta theo các lỗi của chúng ta.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.225Z",
      "updatedAt": "2024-09-23T05:53:04.225Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "ecb7ac5d-e163-4b20-b0c7-5c81405cedd2",
      "number": 11,
      "content": "Vì các tầng trời cao hơn đất bao nhiêu,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.231Z",
      "updatedAt": "2024-09-23T05:53:04.231Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "681d7b70-e965-4dda-a3aa-c218835d9c4f",
      "number": 11,
      "content": "Tình thương của Ngài đối với những người kính sợ Ngài cũng cao lớn bấy nhiêu.",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.236Z",
      "updatedAt": "2024-09-23T05:53:04.236Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "6226ce21-445c-4bac-a6d4-dec8dd0abb63",
      "number": 12,
      "content": "Phương đông xa cách phương tây bao nhiêu,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.240Z",
      "updatedAt": "2024-09-23T05:53:04.240Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "60237dac-6685-4e02-ba27-30787cd0a307",
      "number": 12,
      "content": "Ngài đem những vi phạm của chúng ta xa khỏi chúng ta cũng bấy nhiêu.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.245Z",
      "updatedAt": "2024-09-23T05:53:04.245Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "70a74aa3-55aa-4d90-8be1-94094364e001",
      "number": 13,
      "content": "Như người cha thương xót các con mình thể nào,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.250Z",
      "updatedAt": "2024-09-23T05:53:04.250Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "328f142b-2067-42ae-a4cf-bbcc445bae0d",
      "number": 13,
      "content": "**CHÚA** thương xót những người kính sợ Ngài cũng thể ấy.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.255Z",
      "updatedAt": "2024-09-23T05:53:04.255Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "fd8ff6a0-1889-458f-99f5-d13a653b3625",
      "number": 14,
      "content": "Vì Ngài biết rõ chúng ta được dựng nên bằng những gì;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.260Z",
      "updatedAt": "2024-09-23T05:53:04.260Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "7be5527a-6fbe-490c-9431-81b2050a0f79",
      "number": 14,
      "content": "Ngài nhớ lại rằng chúng ta là bụi đất.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.265Z",
      "updatedAt": "2024-09-23T05:53:04.265Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "12d01f8f-3b3f-42c5-9abd-e755489821b4",
      "number": 15,
      "content": "Về loài người, đời người như ngọn cỏ;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.270Z",
      "updatedAt": "2024-09-23T05:53:04.270Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "278a2b4c-d2a9-41b2-9ebf-3e3a2a8d8cf5",
      "number": 15,
      "content": "Nó thắm tươi như hoa dại ngoài đồng.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.275Z",
      "updatedAt": "2024-09-23T05:53:04.275Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "c5566b8e-061e-4a70-9a69-88b6d0ee1622",
      "number": 16,
      "content": "Một cơn gió thổi qua, kìa, nó chẳng còn;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.280Z",
      "updatedAt": "2024-09-23T05:53:04.280Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "6d98067e-a8a2-4ab2-b451-663327341be2",
      "number": 16,
      "content": "Chỗ nó mọc lên cũng không còn biết nó nữa.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.285Z",
      "updatedAt": "2024-09-23T05:53:04.285Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "677713c1-8d14-4926-9911-004da99a9527",
      "number": 17,
      "content": "Nhưng tình thương của **CHÚA** thì còn mãi mãi cho những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.290Z",
      "updatedAt": "2024-09-23T05:53:04.290Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "0e9f4e57-76f4-44cc-b2b7-0719b5b09ded",
      "number": 17,
      "content": "Và đức công chính Ngài dành cho con cháu chắt chít họ,",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.295Z",
      "updatedAt": "2024-09-23T05:53:04.295Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "f50b117b-7d63-40fb-ba0d-18c94b477d79",
      "number": 18,
      "content": "Tức cho những người giữ giao ước Ngài,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.299Z",
      "updatedAt": "2024-09-23T05:53:04.299Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "f133cbdb-1c2f-47fa-87cc-20a6b6968ce9",
      "number": 18,
      "content": "Và cho những người nhớ đến các điều răn Ngài mà làm theo.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.304Z",
      "updatedAt": "2024-09-23T05:53:04.304Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "6eeca688-1ec1-4eb4-aea1-e04bd3b079d8",
      "number": 19,
      "content": "**CHÚA** thiết lập ngai Ngài trên các tầng trời;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.309Z",
      "updatedAt": "2024-09-23T05:53:04.309Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "f6320ea6-c948-4316-9574-45dde9f8d14a",
      "number": 19,
      "content": "Vương quyền Ngài bao trùm cả vũ trụ.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.314Z",
      "updatedAt": "2024-09-23T05:53:04.314Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "b00d5db1-16a4-43c9-8cb6-05fa0f5416bb",
      "number": 20,
      "content": "Hãy ca ngợi **CHÚA**, hỡi các thiên sứ của Ngài,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.319Z",
      "updatedAt": "2024-09-23T05:53:04.319Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "a5d186fa-3bfd-4d3e-a2f7-4c3ca103ff1b",
      "number": 20,
      "content": "Tức những đấng dũng mãnh siêu phàm, những đấng thi hành mệnh lệnh Ngài,",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.323Z",
      "updatedAt": "2024-09-23T05:53:04.323Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "37feb632-2722-4a0e-a416-30077781e008",
      "number": 20,
      "content": "Những đấng vâng theo lời Ngài!",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.328Z",
      "updatedAt": "2024-09-23T05:53:04.328Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "71902cda-4524-47a5-bc56-93c5606c0bd4",
      "number": 21,
      "content": "Hãy ca ngợi **CHÚA**, hỡi tất cả các đạo quân của Ngài,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.333Z",
      "updatedAt": "2024-09-23T05:53:04.333Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "50e41404-e19b-44e3-b260-98b54dace0cf",
      "number": 21,
      "content": "Tức các đấng phục vụ Ngài, làm theo thánh ý Ngài.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.338Z",
      "updatedAt": "2024-09-23T05:53:04.338Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "8f6f9f84-1c66-4f9b-9eb1-d62c80095cd7",
      "number": 22,
      "content": "Hãy ca ngợi **CHÚA**, hỡi tất cả các tạo vật của Ngài,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.342Z",
      "updatedAt": "2024-09-23T05:53:04.342Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "0408280a-739a-4f2b-a409-a4c1bc383543",
      "number": 22,
      "content": "Ở mọi nơi dưới quyền trị vì của Ngài!",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.347Z",
      "updatedAt": "2024-09-23T05:53:04.347Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    },
    {
      "id": "66516258-f97d-4733-aed3-97a4a78de7d9",
      "number": 22,
      "content": "Hãy ca ngợi **CHÚA**, hỡi linh hồn ta!",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:53:04.352Z",
      "updatedAt": "2024-09-23T05:53:04.352Z",
      "chapterId": "086b978b-f73e-4f80-9a68-d8f096b800b3"
    }
  ],
