    }
  ],
  "headings": [
    {
      "id": "c856de89-b6fd-4365-bf44-eb99ca69371f",
      "content": "Sự Trị Vì của Đấng Được Xức Dầu",
      "order": 0,
      "createdAt": "2024-09-23T05:44:36.696Z",
      "updatedAt": "2024-09-23T05:44:36.696Z",
