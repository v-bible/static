    {
      "id": "f192441e-f8df-4e62-bd86-0511058f5630",
      "content": "Những Lời Khuyên Bảo Khác của Người Khôn Ngoan",
      "order": 0,
      "createdAt": "2024-09-23T05:56:49.817Z",
      "updatedAt": "2024-09-23T05:56:49.817Z",
