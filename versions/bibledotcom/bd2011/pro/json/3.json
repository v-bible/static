    },
    {
      "id": "0f168f08-8dde-4c8e-b46f-34d8b1dacb62",
      "content": "Sự Khôn Ngoan trong Công Cuộc Sáng Tạo",
      "order": 0,
      "createdAt": "2024-09-23T05:55:42.345Z",
      "updatedAt": "2024-09-23T05:55:42.345Z",
