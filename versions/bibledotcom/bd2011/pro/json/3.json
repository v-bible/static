{
  "chapter": {
    "id": "a527286c-8606-44fc-9d1b-e2aa518fe4f1",
    "number": 3,
    "ref": "https://www.bible.com/bible/19/PRO.3.BD2011",
    "createdAt": "2024-09-23T05:08:40.725Z",
    "updatedAt": "2024-09-23T05:08:40.725Z",
    "bookId": "2cf03725-457c-4fab-86fe-f08be1e90c98"
  },
  "verses": [
    {
      "id": "f9ab353b-27ca-4e20-822c-24cd961be617",
      "number": 1,
      "content": "Con ơi, chớ quên luật pháp ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.215Z",
      "updatedAt": "2024-09-23T05:55:42.215Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "a3ddd3ac-376f-4c21-925f-3a2dea9bc4e6",
      "number": 1,
      "content": "Nhưng hãy để lòng con gìn giữ mệnh lệnh ta;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.227Z",
      "updatedAt": "2024-09-23T05:55:42.227Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "17a88f95-85b2-4954-953e-c8277e17948f",
      "number": 2,
      "content": "Vì chúng sẽ làm gia tăng số ngày và số năm của đời con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.229Z",
      "updatedAt": "2024-09-23T05:55:42.229Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "14714f4b-53cc-40d8-87d6-1c0fb9a40178",
      "number": 2,
      "content": "Chúng sẽ đem lại cho con bình an thịnh vượng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.231Z",
      "updatedAt": "2024-09-23T05:55:42.231Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "32bbcc0b-2d34-42fa-a25c-c18f46a805a6",
      "number": 3,
      "content": "Đừng để lòng thương xót và tính chân thật lìa khỏi con;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.233Z",
      "updatedAt": "2024-09-23T05:55:42.233Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "dab6e668-e961-4e64-9000-d4cd3a77b2e1",
      "number": 3,
      "content": "Hãy buộc chúng quanh cổ con;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.235Z",
      "updatedAt": "2024-09-23T05:55:42.235Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "f8e8dc50-ca56-40a3-bffb-5b92a30e0fdc",
      "number": 3,
      "content": "Hãy ghi tạc chúng vào bia lòng con.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.238Z",
      "updatedAt": "2024-09-23T05:55:42.238Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "34b685ff-30db-4b61-9f09-3a931ee05121",
      "number": 4,
      "content": "Vì như thế con sẽ được ơn và được tiếng tốt,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.240Z",
      "updatedAt": "2024-09-23T05:55:42.240Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "bd5274c9-6b7e-4765-8bbb-7d2bfbb5b0e8",
      "number": 4,
      "content": "Trước mặt Đức Chúa Trời và luôn cả người ta.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.242Z",
      "updatedAt": "2024-09-23T05:55:42.242Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "4795a4d6-f146-4e41-9ab2-7b6fc487d521",
      "number": 5,
      "content": "Hãy hết lòng tin cậy **CHÚA**;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.244Z",
      "updatedAt": "2024-09-23T05:55:42.244Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "8875d355-5e76-46e2-b48a-99bcdb6ed54e",
      "number": 5,
      "content": "Chớ cậy vào sự thông sáng của con;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.247Z",
      "updatedAt": "2024-09-23T05:55:42.247Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "23278163-190b-4af9-b78d-f2b1e41b7c69",
      "number": 6,
      "content": "Phàm trong các việc làm của con, khá nhận biết Ngài,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.249Z",
      "updatedAt": "2024-09-23T05:55:42.249Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "9187a369-c6be-4585-8a6b-b7d046fd5025",
      "number": 6,
      "content": "Thì Ngài sẽ chỉ dẫn các lối của con.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.252Z",
      "updatedAt": "2024-09-23T05:55:42.252Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "4a20a1c3-897f-4067-8168-1e5e0f3bbded",
      "number": 7,
      "content": "Đừng cho mình là khôn ngoan theo mắt mình;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.254Z",
      "updatedAt": "2024-09-23T05:55:42.254Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "03437d1b-2788-4189-8cd9-5090268b2188",
      "number": 7,
      "content": "Hãy kính sợ **CHÚA** và lánh xa điều ác.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.256Z",
      "updatedAt": "2024-09-23T05:55:42.256Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "66efe34d-4216-423e-881d-64031d681644",
      "number": 8,
      "content": "Như vậy thân xác con sẽ được mạnh khỏe,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.258Z",
      "updatedAt": "2024-09-23T05:55:42.258Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "1eacb87b-6548-4b69-b773-7fce9d9c06d4",
      "number": 8,
      "content": "Và xương cốt con sẽ được cứng cáp vững vàng.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.261Z",
      "updatedAt": "2024-09-23T05:55:42.261Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "de501cb2-7f70-441f-bb03-a52c4af3a65a",
      "number": 9,
      "content": "Hãy dùng của cải con để tôn vinh **CHÚA**,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.263Z",
      "updatedAt": "2024-09-23T05:55:42.263Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "8d24dc2a-b498-4d44-98eb-70a300c5aa69",
      "number": 9,
      "content": "Hãy dâng các hoa lợi đầu mùa của con để cảm tạ Ngài.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.265Z",
      "updatedAt": "2024-09-23T05:55:42.265Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "fb64d76b-770f-4c7f-9750-0bbd7ec44ebd",
      "number": 10,
      "content": "Như vậy các vựa lẫm của con sẽ tràn đầy lúa thóc,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.268Z",
      "updatedAt": "2024-09-23T05:55:42.268Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "70523983-a33d-4253-a243-ac91e6cfd254",
      "number": 10,
      "content": "Các thùng rượu của con rượu mới sẽ tràn đầy.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.270Z",
      "updatedAt": "2024-09-23T05:55:42.270Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "291537d8-6144-429d-ad5c-5a8f5b1c370a",
      "number": 11,
      "content": "Con ơi, chớ coi thường sự sửa phạt của **CHÚA**,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.273Z",
      "updatedAt": "2024-09-23T05:55:42.273Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "cd916b1b-0129-4ffb-aef1-357653507365",
      "number": 11,
      "content": "Đừng bất mãn trước sự quở trách của Ngài;",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.275Z",
      "updatedAt": "2024-09-23T05:55:42.275Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "66afe647-9bd0-400f-b1cb-7b45522f3842",
      "number": 12,
      "content": "Vì ai được **CHÚA** thương thì Ngài quở trách,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.278Z",
      "updatedAt": "2024-09-23T05:55:42.278Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "d539002f-f49b-44f9-9bd0-af5feb8faaad",
      "number": 12,
      "content": "Như cha thương con mình nên trách phạt khuyên răn.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.281Z",
      "updatedAt": "2024-09-23T05:55:42.281Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "b734756f-5523-4245-b694-f1b51a81abc5",
      "number": 13,
      "content": "Phước thay cho người tìm được sự khôn ngoan,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.285Z",
      "updatedAt": "2024-09-23T05:55:42.285Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "8a35e537-9c9d-4776-89ab-9f74513b724f",
      "number": 13,
      "content": "Và cho người có được sự thông sáng.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.292Z",
      "updatedAt": "2024-09-23T05:55:42.292Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "2b6e3ee6-2ec8-4f5c-afe6-281ad59a4b19",
      "number": 14,
      "content": "Vì lợi ích của khôn ngoan trỗi hơn tiền bạc;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.296Z",
      "updatedAt": "2024-09-23T05:55:42.296Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "2e65dfa3-1543-4b0c-a4ff-c35f6ad63089",
      "number": 14,
      "content": "Ích lợi của nàng lấn át cả vàng ròng.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.300Z",
      "updatedAt": "2024-09-23T05:55:42.300Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "fab22466-21b7-433f-9daf-c36d11da86ee",
      "number": 15,
      "content": "Nàng quý báu hơn châu ngọc;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.305Z",
      "updatedAt": "2024-09-23T05:55:42.305Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "f37e7c55-8a36-4301-8034-d35d5e93d189",
      "number": 15,
      "content": "Chẳng có vật gì con ưa thích có thể sánh được với nàng.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.309Z",
      "updatedAt": "2024-09-23T05:55:42.309Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "04b344a1-f2d2-4acb-a2cb-1116d1da2802",
      "number": 16,
      "content": "Tay phải nàng phát ban trường thọ;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.313Z",
      "updatedAt": "2024-09-23T05:55:42.313Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "6a45cd02-f635-4ad4-ae9e-3e69c0ae1326",
      "number": 16,
      "content": "Tay trái nàng cho giàu có lẫn hiển vinh.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.317Z",
      "updatedAt": "2024-09-23T05:55:42.317Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "aaff25da-3f97-4c36-a87b-053091e2c65f",
      "number": 17,
      "content": "Con đường nàng là con đường vui thỏa;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.322Z",
      "updatedAt": "2024-09-23T05:55:42.322Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "5965b73f-701d-4f6d-bebc-886871ef157b",
      "number": 17,
      "content": "Mọi nẻo nàng là mọi ngả bình an.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.327Z",
      "updatedAt": "2024-09-23T05:55:42.327Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "490440c4-8493-411f-90da-b14d936a2882",
      "number": 18,
      "content": "Khôn ngoan là cây sự sống cho người nào nắm giữ được;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.332Z",
      "updatedAt": "2024-09-23T05:55:42.332Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "468416d8-95ef-47ec-9831-cf4b99b9d2ce",
      "number": 18,
      "content": "Ai giữ chặt nàng sẽ được phước lâu dài.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.336Z",
      "updatedAt": "2024-09-23T05:55:42.336Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "928609c9-2f6c-4fb8-ad6c-9d924cbbba04",
      "number": 19,
      "content": "**CHÚA** dùng sự khôn ngoan để dựng nên trái đất;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.341Z",
      "updatedAt": "2024-09-23T05:55:42.341Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "d5ee42fa-2eac-4f2e-a38c-e63427f6733c",
      "number": 19,
      "content": "Ngài dùng sự thông sáng để tạo lập các tầng trời;",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.350Z",
      "updatedAt": "2024-09-23T05:55:42.350Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "3e3df862-b7f4-42f4-a91a-d756656d3d7f",
      "number": 20,
      "content": "Nhờ tri thức của Ngài các vực sâu mở rộng;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.355Z",
      "updatedAt": "2024-09-23T05:55:42.355Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "3f91c1b2-dd6f-4fe1-881e-0f4b91ff4658",
      "number": 20,
      "content": "Những áng mây mù nhỏ những giọt sương đêm.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.361Z",
      "updatedAt": "2024-09-23T05:55:42.361Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "59f6768c-d050-4f41-986f-2c88be51a0f0",
      "number": 21,
      "content": "Con ơi, đừng để những điều này vuột khỏi tầm mắt con:",
      "order": 0,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.365Z",
      "updatedAt": "2024-09-23T05:55:42.365Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "d6668706-5b7e-49dd-844a-5c902379e247",
      "number": 21,
      "content": "Hãy giữ lấy sự khôn ngoan và sự cẩn trọng.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.374Z",
      "updatedAt": "2024-09-23T05:55:42.374Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "d37bd97c-ca27-442e-a899-783c7ba620eb",
      "number": 22,
      "content": "Những điều ấy sẽ là sức sống của linh hồn con,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.380Z",
      "updatedAt": "2024-09-23T05:55:42.380Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "e0c0e012-37d3-42b5-9831-ba45a1054247",
      "number": 22,
      "content": "Và là những đồ trang sức nơi cổ con.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.385Z",
      "updatedAt": "2024-09-23T05:55:42.385Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "3165cc62-060e-4492-b398-918e0d1e2004",
      "number": 23,
      "content": "Nhờ những điều ấy đường con đi sẽ an toàn bảo đảm,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.390Z",
      "updatedAt": "2024-09-23T05:55:42.390Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "a59d82bb-811e-4cfb-97a7-6124342cdd90",
      "number": 23,
      "content": "Bước chân con sẽ chẳng vấp phạm bao giờ.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.394Z",
      "updatedAt": "2024-09-23T05:55:42.394Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "862014ec-ad8a-4299-82e3-be811203bcca",
      "number": 24,
      "content": "Lúc nằm nghỉ con sẽ không lo sợ;",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.399Z",
      "updatedAt": "2024-09-23T05:55:42.399Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "704999fc-5ed5-4778-b065-e2c51ae21acb",
      "number": 24,
      "content": "Khi đặt lưng con sẽ an giấc ngủ ngon.",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.403Z",
      "updatedAt": "2024-09-23T05:55:42.403Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "fa75a584-9e78-4e82-a50c-af7ff499a793",
      "number": 25,
      "content": "Đừng sợ hãi nỗi kinh hoàng xảy ra bất chợt;",
      "order": 0,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.408Z",
      "updatedAt": "2024-09-23T05:55:42.408Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "2c013307-2188-44f6-a259-43659c50a7e7",
      "number": 25,
      "content": "Đừng âu lo khi đợt phá hoại của kẻ ác đến gần;",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.412Z",
      "updatedAt": "2024-09-23T05:55:42.412Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "5932a90b-03c3-4b25-a471-2b1b48e75cfc",
      "number": 26,
      "content": "Vì **CHÚA** sẽ là nguồn trông cậy của con;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.417Z",
      "updatedAt": "2024-09-23T05:55:42.417Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "5c9f03fe-13f2-4bd1-9b1a-163080a05c99",
      "number": 26,
      "content": "Ngài sẽ giữ chân con khỏi sa vào bẫy.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.422Z",
      "updatedAt": "2024-09-23T05:55:42.422Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "4ed5c900-a94b-4b84-acbf-635c48ed025b",
      "number": 27,
      "content": "Đừng từ chối làm việc thiện cho những người đáng giúp,",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.426Z",
      "updatedAt": "2024-09-23T05:55:42.426Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "9a92b156-65af-47d2-b293-b18bd0c43305",
      "number": 27,
      "content": "Nếu con có khả năng, hãy giúp đỡ người ta.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.430Z",
      "updatedAt": "2024-09-23T05:55:42.430Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "1095c521-89aa-4cae-9057-ee5c32f70471",
      "number": 28,
      "content": "Con đừng nói với người lân cận, “Về đi, mai trở lại, tôi sẽ cho,”",
      "order": 0,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.435Z",
      "updatedAt": "2024-09-23T05:55:42.435Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "dedd58fb-4637-4fc8-a3e7-f2f5d6b5a059",
      "number": 28,
      "content": "Trong khi con có sẵn điều người ấy đang cần.",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.439Z",
      "updatedAt": "2024-09-23T05:55:42.439Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "882b071a-edce-4fc7-91bf-dde036716b67",
      "number": 29,
      "content": "Con chớ lập mưu hại người lân cận,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.443Z",
      "updatedAt": "2024-09-23T05:55:42.443Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "d65f8091-ab2a-4d91-9c21-ce59b2775286",
      "number": 29,
      "content": "Người ở gần và tin tưởng nơi con.",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.447Z",
      "updatedAt": "2024-09-23T05:55:42.447Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "b9758345-a9a9-4692-8a89-89888b84d203",
      "number": 30,
      "content": "Con đừng gây sự với ai vô cớ,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.451Z",
      "updatedAt": "2024-09-23T05:55:42.451Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "fafbd2b2-e54a-49bc-86ac-6b866830747c",
      "number": 30,
      "content": "Khi không ai gây cho con thiệt hại điều gì.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.455Z",
      "updatedAt": "2024-09-23T05:55:42.455Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "eddf925a-d84e-40eb-a2d4-0211f2c3438a",
      "number": 31,
      "content": "Đừng ganh tị với quân tàn bạo;",
      "order": 0,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.459Z",
      "updatedAt": "2024-09-23T05:55:42.459Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "fb2a3e50-d172-4d4a-8d6c-8eeb60f120c1",
      "number": 31,
      "content": "Chớ chọn đường nào chúng chọn lấy để đi;",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.464Z",
      "updatedAt": "2024-09-23T05:55:42.464Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "299f8c69-85b4-4430-a410-ef72d08dde81",
      "number": 32,
      "content": "Vì **CHÚA** ghê tởm kẻ gian tà tội lỗi,",
      "order": 0,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.468Z",
      "updatedAt": "2024-09-23T05:55:42.468Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "c66f3b7e-d6d3-44d1-adc8-13e727f4bf59",
      "number": 32,
      "content": "Nhưng Ngài kết bạn tâm giao với người chính trực ngay lành.",
      "order": 1,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.472Z",
      "updatedAt": "2024-09-23T05:55:42.472Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "b84a4298-153a-43c3-a31b-6d2cd9e566f3",
      "number": 33,
      "content": "Sự nguyền rủa của **CHÚA** giáng trên nhà kẻ gian ác,",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.476Z",
      "updatedAt": "2024-09-23T05:55:42.476Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "d5251ff9-b4b8-4732-918c-62b819a02b5c",
      "number": 33,
      "content": "Nhưng phước hạnh Ngài ban cho chỗ ở người ngay lành.",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.480Z",
      "updatedAt": "2024-09-23T05:55:42.480Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "301bd123-2f94-4afd-8372-a5d85922998c",
      "number": 34,
      "content": "Ngài chế giễu phường chê bai nhạo báng,",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.484Z",
      "updatedAt": "2024-09-23T05:55:42.484Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "d500fdfe-99f1-4fde-b41f-f8242f0bdccf",
      "number": 34,
      "content": "Nhưng Ngài ban ơn cho người thật sự khiêm nhường.",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.489Z",
      "updatedAt": "2024-09-23T05:55:42.489Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "2c16b10a-bdca-49eb-a785-220b4fd0cd77",
      "number": 35,
      "content": "Người khôn ngoan sẽ thừa hưởng những hiển vinh tôn trọng,",
      "order": 0,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.493Z",
      "updatedAt": "2024-09-23T05:55:42.493Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    },
    {
      "id": "2ebc7607-500d-4ca5-ba32-c292c65fa8e7",
      "number": 35,
      "content": "Còn kẻ điên rồ bị tống cho nhục nhã ê chề.",
      "order": 1,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:42.497Z",
      "updatedAt": "2024-09-23T05:55:42.497Z",
      "chapterId": "a527286c-8606-44fc-9d1b-e2aa518fe4f1"
    }
  ],
