{
  "chapter": {
    "id": "fddb9046-0c69-4651-9c1c-e85f1aa80038",
    "number": 13,
    "ref": "https://www.bible.com/bible/19/PRO.13.BD2011",
    "createdAt": "2024-09-23T05:08:40.754Z",
    "updatedAt": "2024-09-23T05:08:40.754Z",
    "bookId": "2cf03725-457c-4fab-86fe-f08be1e90c98"
  },
  "verses": [
    {
      "id": "8337b2eb-dd4a-41ff-9735-fd0701dd551c",
      "number": 1,
      "content": "Con khôn ngoan nghe lời cha khuyên dạy,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.247Z",
      "updatedAt": "2024-09-23T05:56:14.247Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "cfc51799-eeaf-4d85-89b2-a41d45b09e49",
      "number": 1,
      "content": "Còn đứa nhạo báng không nghe lời quở trách của ai.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.256Z",
      "updatedAt": "2024-09-23T05:56:14.256Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "6f2e1ac2-f63e-4c9b-9aa8-9aaf475bb738",
      "number": 2,
      "content": "Người tốt sẽ hưởng bông trái lời mình nói,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.259Z",
      "updatedAt": "2024-09-23T05:56:14.259Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "48901c57-fa0e-4cf5-b0f1-66db3411ca40",
      "number": 2,
      "content": "Còn kẻ bội bạc phải chuốc lấy bạo tàn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.264Z",
      "updatedAt": "2024-09-23T05:56:14.264Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "b8ce907f-9856-499c-bce0-17d97b086c74",
      "number": 3,
      "content": "Ai giữ miệng mình giữ được mạng sống mình,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.269Z",
      "updatedAt": "2024-09-23T05:56:14.269Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "c0f0c9af-76c8-4512-bda7-95ebe0ac2aae",
      "number": 3,
      "content": "Kẻ nói nhiều ắt bại hoại chẳng sai.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.274Z",
      "updatedAt": "2024-09-23T05:56:14.274Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "52d8d7c3-0de1-405f-b249-56e2b903b8d3",
      "number": 4,
      "content": "Kẻ biếng nhác đói rũ mà không có gì ăn đỡ bụng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.278Z",
      "updatedAt": "2024-09-23T05:56:14.278Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "6b5c13f6-e978-4194-ad93-62ad49cfe869",
      "number": 4,
      "content": "Còn người siêng năng lúc nào thực phẩm cũng dư thừa.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.283Z",
      "updatedAt": "2024-09-23T05:56:14.283Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "731172fa-d004-482e-97d4-e072af7d2ee4",
      "number": 5,
      "content": "Người ngay lành ghét những gì gian dối,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.287Z",
      "updatedAt": "2024-09-23T05:56:14.287Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "b2b131c2-aeb2-4e67-8678-9401ee211289",
      "number": 5,
      "content": "Còn kẻ ác gian mang đến xấu hổ và ô nhơ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.292Z",
      "updatedAt": "2024-09-23T05:56:14.292Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "20fd73a0-fa3d-4cb7-81dc-83b29b70233d",
      "number": 6,
      "content": "Đức công chính bảo hộ người theo đường chân chính,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.296Z",
      "updatedAt": "2024-09-23T05:56:14.296Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "bf1d0ecc-d291-4cc1-9590-d0ab6ff7e1b2",
      "number": 6,
      "content": "Còn sự gian tà lật đổ kẻ ác gian.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.300Z",
      "updatedAt": "2024-09-23T05:56:14.300Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "a78ba95a-6320-4de7-8d1c-af196b2349c1",
      "number": 7,
      "content": "Có kẻ làm ra vẻ ta đây giàu có mà chẳng có gì cả,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.305Z",
      "updatedAt": "2024-09-23T05:56:14.305Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "2dcc1eaa-e465-4fdb-bff1-ea7d551122ab",
      "number": 7,
      "content": "Cũng có người làm ra vẻ nghèo nàn nhưng lại có của cải rất nhiều.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.309Z",
      "updatedAt": "2024-09-23T05:56:14.309Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "4daa7fe0-bba9-4a22-80e8-9fc1011f2ccc",
      "number": 8,
      "content": "Của cải nhiều có thể là tiền để chờ chuộc mạng,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.313Z",
      "updatedAt": "2024-09-23T05:56:14.313Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "b9dcf386-0fe3-49ab-b64b-26a35059864a",
      "number": 8,
      "content": "Còn người nghèo nàn chẳng nghe ai hăm dọa tiếng nào.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.318Z",
      "updatedAt": "2024-09-23T05:56:14.318Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "39ec1b5d-7dc4-4dfe-8d49-023a8dae6f6d",
      "number": 9,
      "content": "Ánh sáng người ngay lành luôn tỏa soi rực rỡ,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.322Z",
      "updatedAt": "2024-09-23T05:56:14.322Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "78e721d0-ab1a-4e3c-9080-4db1b592c49e",
      "number": 9,
      "content": "Nhưng đèn kẻ ác gian bị dập tắt mịt mờ.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.326Z",
      "updatedAt": "2024-09-23T05:56:14.326Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "20bd86c8-7a86-4161-86f3-c66eed52c751",
      "number": 10,
      "content": "Sự kiêu ngạo chỉ gây nên tranh cãi;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.330Z",
      "updatedAt": "2024-09-23T05:56:14.330Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "9bea50e5-ebb1-4e2b-93f5-5b8b0e2511e4",
      "number": 10,
      "content": "Người khôn ngoan biết ghi nhận lời khuyên.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.334Z",
      "updatedAt": "2024-09-23T05:56:14.334Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "e9cce47e-78d1-4d0c-9fed-e8f5c46499a7",
      "number": 11,
      "content": "Của cải nhờ lừa dối tinh vi để giàu mau sẽ chóng mất;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.339Z",
      "updatedAt": "2024-09-23T05:56:14.339Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "be14ff56-b405-4582-92fa-f6b6f7207500",
      "number": 11,
      "content": "Nhưng người chăm chỉ làm ăn sẽ giàu vững từ từ.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.343Z",
      "updatedAt": "2024-09-23T05:56:14.343Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "e6c58614-028a-413c-9eae-54b78a777f21",
      "number": 12,
      "content": "Trì hoãn niềm hy vọng làm cho lòng đau đớn,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.347Z",
      "updatedAt": "2024-09-23T05:56:14.347Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "3d829c17-88b6-464e-91d0-6b89bea6e8ee",
      "number": 12,
      "content": "Ao ước được thành là cây sự sống xanh tươi.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.351Z",
      "updatedAt": "2024-09-23T05:56:14.351Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "3c2cbec0-58b2-4a52-8356-240f597edecb",
      "number": 13,
      "content": "Kẻ khinh dể lời hằng sống chuốc cho mình sự hủy diệt,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.355Z",
      "updatedAt": "2024-09-23T05:56:14.355Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "cd451bb4-8ae6-47bb-8c61-1c301a501194",
      "number": 13,
      "content": "Người quý trọng điều răn chắc sẽ được thưởng ban.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.359Z",
      "updatedAt": "2024-09-23T05:56:14.359Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "4cd00bc6-61b8-451e-b868-6204a6dc1e22",
      "number": 14,
      "content": "Sự dạy dỗ của người khôn ngoan chính là nguồn sự sống,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.363Z",
      "updatedAt": "2024-09-23T05:56:14.363Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "f46a7036-0e4f-455f-9e75-77a224682628",
      "number": 14,
      "content": "Kẻ vâng theo sẽ không sa vào bẫy tử vong.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.368Z",
      "updatedAt": "2024-09-23T05:56:14.368Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "3585dbc7-e4a1-4e87-bb17-08801f3005bc",
      "number": 15,
      "content": "Nhận định sáng suốt luôn mang về ân huệ,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.372Z",
      "updatedAt": "2024-09-23T05:56:14.372Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "d35fc69f-2e3b-4e73-bf61-58a2dbfa6913",
      "number": 15,
      "content": "Còn đường kẻ bất trung gặp khốn đốn rất nhiều.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.376Z",
      "updatedAt": "2024-09-23T05:56:14.376Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "57de7daf-40ce-4965-b0fe-275c22a37ee0",
      "number": 16,
      "content": "Người khôn ngoan nắm vững tin tức trước khi hành động,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.380Z",
      "updatedAt": "2024-09-23T05:56:14.380Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "86529713-f5eb-4070-a87a-3e5347860dcd",
      "number": 16,
      "content": "Kẻ dại khờ hấp tấp phô bày sự dại dột mình ra.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.384Z",
      "updatedAt": "2024-09-23T05:56:14.384Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "aa9276aa-8a33-4dce-852e-71355fb2c28a",
      "number": 17,
      "content": "Sứ giả gian hùng gây thêm điều rắc rối,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.392Z",
      "updatedAt": "2024-09-23T05:56:14.392Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "fa1fbe2e-146b-46de-98e5-a2abc5773ab3",
      "number": 17,
      "content": "Sứ giả trung thành hóa giải những khó khăn.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.396Z",
      "updatedAt": "2024-09-23T05:56:14.396Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "13c017f3-d243-47e7-8ce8-0971c5d4a72e",
      "number": 18,
      "content": "Khinh dể lời khuyên dạy chuốc lấy bần cùng và nhục nhã,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.400Z",
      "updatedAt": "2024-09-23T05:56:14.400Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "ba21c794-00ee-4712-8d9f-d73013b2f933",
      "number": 18,
      "content": "Nhưng ai chịu nghe lời quở trách sẽ được tôn trọng vinh danh.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.405Z",
      "updatedAt": "2024-09-23T05:56:14.405Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "6526b84e-d1c9-4a54-ade4-ca0a247c273f",
      "number": 19,
      "content": "Mơ ước được thành làm tâm hồn khoan khoái;",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.409Z",
      "updatedAt": "2024-09-23T05:56:14.409Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "df6f6649-28a6-48ed-a3f0-1e5e2153bf27",
      "number": 19,
      "content": "Lìa bỏ gian tà là điều kẻ điên dại gớm ghê.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.414Z",
      "updatedAt": "2024-09-23T05:56:14.414Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "1e0081f4-4587-43e2-bd45-d075aed96311",
      "number": 20,
      "content": "Ai giao thiệp với người khôn ngoan sẽ học được khôn ngoan,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.419Z",
      "updatedAt": "2024-09-23T05:56:14.419Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "3eefbec2-57fa-4218-9ae6-8c3a337053df",
      "number": 20,
      "content": "Ai làm bạn với kẻ điên dại sẽ có ngày mang họa.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.424Z",
      "updatedAt": "2024-09-23T05:56:14.424Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "83845a81-f91b-4028-b274-152253f1cf29",
      "number": 21,
      "content": "Nỗi bất hạnh luôn đuổi theo kẻ tội lỗi,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.429Z",
      "updatedAt": "2024-09-23T05:56:14.429Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "ba341eb8-ddc5-4ff2-8f89-100a5945cb82",
      "number": 21,
      "content": "Sự phồn vinh là phần thưởng của người ngay lành.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.433Z",
      "updatedAt": "2024-09-23T05:56:14.433Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "e0a67bf1-a28e-4cef-ad7b-f6e920501a69",
      "number": 22,
      "content": "Người tốt để gia tài lại cho con cháu,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.437Z",
      "updatedAt": "2024-09-23T05:56:14.437Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "0fe7ef83-0298-4811-a3be-85ff88c4e085",
      "number": 22,
      "content": "Nhưng tội nhân tích trữ của cải để lại cho người ngay lành.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.442Z",
      "updatedAt": "2024-09-23T05:56:14.442Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "cb45c53d-7150-411a-9712-323f8401b560",
      "number": 23,
      "content": "Ruộng của người nghèo trúng mùa và mang lại nhiều hoa lợi,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.447Z",
      "updatedAt": "2024-09-23T05:56:14.447Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "d4cd8d79-fcf1-4c7f-88ed-c8a885d7b4f8",
      "number": 23,
      "content": "Nhưng sự bất công cướp sạch những hoa lợi đó đi.",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.451Z",
      "updatedAt": "2024-09-23T05:56:14.451Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "29e6e290-0c6a-4e8d-bc96-68e37161847e",
      "number": 24,
      "content": "Ai kiêng cữ roi vọt là không thương con cái,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.455Z",
      "updatedAt": "2024-09-23T05:56:14.455Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "3fb10fd2-2c69-4ac1-ad14-026b81d0efe2",
      "number": 24,
      "content": "Người thương con chăm chỉ sửa trị con mình.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.459Z",
      "updatedAt": "2024-09-23T05:56:14.459Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "de9659e7-8670-4e29-9824-44847fb91d7e",
      "number": 25,
      "content": "Người ngay lành có đầy đủ thức ăn thức uống,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.464Z",
      "updatedAt": "2024-09-23T05:56:14.464Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    },
    {
      "id": "42564b51-4477-4c44-aad7-de5f197c8185",
      "number": 25,
      "content": "Kẻ ác gian bụng cứ đói triền miên.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:56:14.469Z",
      "updatedAt": "2024-09-23T05:56:14.469Z",
      "chapterId": "fddb9046-0c69-4651-9c1c-e85f1aa80038"
    }
  ],
