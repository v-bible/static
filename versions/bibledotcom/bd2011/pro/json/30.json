    },
    {
      "id": "79012d62-6a22-4a69-9af0-c895753707cb",
      "number": 32,
      "content": "Hãy lấy tay bụm miệng mình lại.",
      "order": 2,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:06.390Z",
      "updatedAt": "2024-09-23T05:57:06.390Z",
      "chapterId": "04a312a8-d81e-43ae-8ddd-eefbf5948a13"
    },
    {
      "id": "8a533b7d-b0c9-4aa6-b59f-f3dca61df2cf",
      "number": 33,
      "content": "Vì ép sữa sẽ làm ra bơ,",
      "order": 0,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:06.392Z",
      "updatedAt": "2024-09-23T05:57:06.392Z",
      "chapterId": "04a312a8-d81e-43ae-8ddd-eefbf5948a13"
    },
    {
      "id": "bc625262-2527-4684-bd5a-08b7ffab2482",
      "number": 33,
      "content": "Và đánh vào mũi sẽ làm cho chảy máu,",
      "order": 1,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:57:06.395Z",
      "updatedAt": "2024-09-23T05:57:06.395Z",
      "chapterId": "04a312a8-d81e-43ae-8ddd-eefbf5948a13"
    },
    {
      "id": "6b7a7142-0c41-4769-b09c-423e705a2038",
      "number": 33,
      "content": "Cũng vậy chọc cho giận sẽ sinh ra tranh cạnh.",
      "order": 2,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
