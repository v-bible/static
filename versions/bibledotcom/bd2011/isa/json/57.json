{
  "chapter": {
    "id": "37c0182e-7805-47a9-9bba-c45609e08671",
    "number": 57,
    "ref": "https://www.bible.com/bible/19/ISA.57.BD2011",
    "createdAt": "2024-09-23T05:08:41.103Z",
    "updatedAt": "2024-09-23T05:08:41.103Z",
    "bookId": "655961d7-6793-43d1-be06-cff911774d08"
  },
  "verses": [
    {
      "id": "a3e48b63-f232-44f5-ba93-426cde1efc4a",
      "number": 1,
      "content": "Người công chính qua đời",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.640Z",
      "updatedAt": "2024-09-23T06:01:53.640Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "bd4a1cb9-3741-4a17-934a-6ffcb49fee55",
      "number": 1,
      "content": "Mà chẳng ai quan tâm đến;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.652Z",
      "updatedAt": "2024-09-23T06:01:53.652Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "30b2770c-ccb1-453c-9f30-d65a4d266a65",
      "number": 1,
      "content": "Người đạo đức bị cất đi",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.657Z",
      "updatedAt": "2024-09-23T06:01:53.657Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "0a9b8d47-525f-48c5-91f8-e26cbdb5917a",
      "number": 1,
      "content": "Mà không ai nghĩ rằng,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.662Z",
      "updatedAt": "2024-09-23T06:01:53.662Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "8a565890-b4c1-4fcd-8c95-11bc24ebdf6b",
      "number": 1,
      "content": "Người công chính được cất đi là để tránh khỏi tai họa.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.666Z",
      "updatedAt": "2024-09-23T06:01:53.666Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "d1f7d9ad-fd10-45fc-9e92-58679a46910d",
      "number": 2,
      "content": "Người ấy sẽ đi vào cõi bình an;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.672Z",
      "updatedAt": "2024-09-23T06:01:53.672Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "44c44b10-2fd0-4dd9-a987-9c55516f1f67",
      "number": 2,
      "content": "Ai bước đi trong sự chính trực",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.676Z",
      "updatedAt": "2024-09-23T06:01:53.676Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "d304129a-965a-49df-b263-82712ca74f7a",
      "number": 2,
      "content": "Sẽ được nghỉ ngơi trên giường mình.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.681Z",
      "updatedAt": "2024-09-23T06:01:53.681Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "cbd3efba-465b-4d83-b1ec-4baaa5ad05c8",
      "number": 3,
      "content": "“Còn các ngươi, hãy đến đây,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.686Z",
      "updatedAt": "2024-09-23T06:01:53.686Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "c763d1f0-bb2d-4742-9f31-00bef0d08ae6",
      "number": 3,
      "content": "Hỡi con cái của mụ phù thủy,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.691Z",
      "updatedAt": "2024-09-23T06:01:53.691Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "f1c75231-7b52-49f5-a8d2-de4c0c46111c",
      "number": 3,
      "content": "Hỡi dòng dõi của kẻ ngoại tình và phường đĩ điếm.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.696Z",
      "updatedAt": "2024-09-23T06:01:53.696Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "b08f2dcc-9b7e-4821-8276-a01a2ad0047d",
      "number": 4,
      "content": "Các ngươi cười nhạo ai?",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.700Z",
      "updatedAt": "2024-09-23T06:01:53.700Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "b72d63dd-583e-4482-a84a-965d332c5433",
      "number": 4,
      "content": "Các ngươi mở rộng miệng",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.706Z",
      "updatedAt": "2024-09-23T06:01:53.706Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "f1d437a5-db9b-4e80-b4d3-ae048e365f5e",
      "number": 4,
      "content": "Và le lưỡi chống lại ai?",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.710Z",
      "updatedAt": "2024-09-23T06:01:53.710Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "92d3162e-d167-4325-b34a-46c76489ed86",
      "number": 4,
      "content": "Há chẳng phải các ngươi là con cái của tội lỗi,",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.716Z",
      "updatedAt": "2024-09-23T06:01:53.716Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "3d81e13d-af54-451f-9186-98c2251e2744",
      "number": 4,
      "content": "Và dòng dõi của dối trá sao?",
      "order": 4,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.720Z",
      "updatedAt": "2024-09-23T06:01:53.720Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "e443423d-57b9-4828-bcf6-cba03c25cc3f",
      "number": 5,
      "content": "Há chẳng phải các ngươi đã hành dâm với các thần tượng",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.725Z",
      "updatedAt": "2024-09-23T06:01:53.725Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "4228c277-f353-4dee-93ac-649d88605d77",
      "number": 5,
      "content": "Dưới mọi cây cổ thụ,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.730Z",
      "updatedAt": "2024-09-23T06:01:53.730Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "1f2b1439-4857-4eff-ad8d-7d651bb48980",
      "number": 5,
      "content": "Các ngươi giết các con thơ mình trong các thung lũng,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.736Z",
      "updatedAt": "2024-09-23T06:01:53.736Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "bcba9eb5-bde8-4f63-a5b3-ba5606538057",
      "number": 5,
      "content": "Trong các hốc đá để tế thần sao?",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.741Z",
      "updatedAt": "2024-09-23T06:01:53.741Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "0752cd59-54d3-4c95-bbad-12839a807387",
      "number": 6,
      "content": "Nằm giữa các viên đá trơn tru trong khe suối sẽ là phần của ngươi,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.746Z",
      "updatedAt": "2024-09-23T06:01:53.746Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "4abec108-92f8-4fa7-95d0-db92308383e6",
      "number": 6,
      "content": "Chúng, chính chúng, là phần của ngươi;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.751Z",
      "updatedAt": "2024-09-23T06:01:53.751Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "6932187e-c924-4045-9a94-858d1bb636c8",
      "number": 6,
      "content": "Ngay cả những vật vô tri như thế mà ngươi cũng dâng lễ quán cho chúng,",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.756Z",
      "updatedAt": "2024-09-23T06:01:53.756Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "016689b1-e440-4f8f-98a2-f8b6a65091cb",
      "number": 6,
      "content": "Ngươi lấy lúa gạo làm của lễ chay dâng cho chúng,",
      "order": 3,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.761Z",
      "updatedAt": "2024-09-23T06:01:53.761Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "c11aa4d5-4e28-41a0-aaf8-270f21f59c49",
      "number": 6,
      "content": "Ta há có thể cứ thản nhiên trước những hành vi như thế hay sao?",
      "order": 4,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.766Z",
      "updatedAt": "2024-09-23T06:01:53.766Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "f24c0de4-df0f-4c08-b844-3d4664b90a7c",
      "number": 7,
      "content": "Ngươi đã kê giường cho mình trên đỉnh núi cao chót vót;",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.771Z",
      "updatedAt": "2024-09-23T06:01:53.771Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "c40cd93b-e740-41c8-918a-0ebcf036231b",
      "number": 7,
      "content": "Ngươi đã lên tận nơi cao như thế",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.776Z",
      "updatedAt": "2024-09-23T06:01:53.776Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "a5d08281-1a86-4707-a2d3-654354ca5638",
      "number": 7,
      "content": "Để dâng con vật hiến tế cho các thần tượng.",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.780Z",
      "updatedAt": "2024-09-23T06:01:53.780Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "0a2e86ff-c299-4fe8-b107-fd943194b20c",
      "number": 8,
      "content": "Sau các cánh cửa và sau các cột cửa,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.785Z",
      "updatedAt": "2024-09-23T06:01:53.785Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "c33f6407-4930-4468-a897-af85cf25c963",
      "number": 8,
      "content": "Ngươi đã lưu dấu kỷ niệm mình,",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.789Z",
      "updatedAt": "2024-09-23T06:01:53.789Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "4db6b417-487c-4c4d-9619-0789a5ce412f",
      "number": 8,
      "content": "Tại đó ngươi đã phản bội Ta.",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.794Z",
      "updatedAt": "2024-09-23T06:01:53.794Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "ef47fd84-2af3-40c8-b3f7-20d34158db29",
      "number": 8,
      "content": "Ngươi đã cởi bỏ y phục mình, để lên giường với các thần tượng;",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.799Z",
      "updatedAt": "2024-09-23T06:01:53.799Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "11a0f836-a80e-4a35-bfc8-317a7c76bd0d",
      "number": 8,
      "content": "Ngươi đã nới rộng giường của ngươi;",
      "order": 4,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.803Z",
      "updatedAt": "2024-09-23T06:01:53.803Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "d6ab49ed-3c01-430c-bf54-a9898909f3ca",
      "number": 8,
      "content": "Ngươi đã kết ước với chúng;",
      "order": 5,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.809Z",
      "updatedAt": "2024-09-23T06:01:53.809Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "13a61960-f86f-4f56-991c-be12e4e46bc3",
      "number": 8,
      "content": "Ngươi đã yêu mến giường của chúng,",
      "order": 6,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.813Z",
      "updatedAt": "2024-09-23T06:01:53.813Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "1321ce15-bf03-4b98-8b45-e5126010cfd6",
      "number": 8,
      "content": "Và ngươi đã ngắm nhìn chúng trần trụi lõa lồ.",
      "order": 7,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.818Z",
      "updatedAt": "2024-09-23T06:01:53.818Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "a87f07d4-23c8-4582-a638-21fef795eedc",
      "number": 9,
      "content": "Ngươi đã đem dầu và rất nhiều dầu thơm",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.824Z",
      "updatedAt": "2024-09-23T06:01:53.824Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "5f7c3bf7-c4c0-4b70-afa6-ba1f0486130a",
      "number": 9,
      "content": "Đến dâng cho Mô-lếch;",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.828Z",
      "updatedAt": "2024-09-23T06:01:53.828Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "84a1c417-ea25-4fd0-ab5c-c3b578e3dd2c",
      "number": 9,
      "content": "Ngươi đã sai các sứ giả đến tận phương xa,",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.838Z",
      "updatedAt": "2024-09-23T06:01:53.838Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "8c04caa1-8f68-44f1-94c1-c4bc01d6f1da",
      "number": 9,
      "content": "Và đã phái chúng xuống tận âm phủ.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.845Z",
      "updatedAt": "2024-09-23T06:01:53.845Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "06788969-b39c-42d5-8638-5cce8fe54af6",
      "number": 10,
      "content": "Ngươi đã mệt mỏi vì hành trình dài đằng đẵng,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.849Z",
      "updatedAt": "2024-09-23T06:01:53.849Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "7151b4e1-3fef-4861-855a-3088a0920338",
      "number": 10,
      "content": "Nhưng ngươi chẳng than rằng, ‘Thật uổng công phí sức!’",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.854Z",
      "updatedAt": "2024-09-23T06:01:53.854Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "6d9f7802-025d-44ae-ab6a-1857cd9ad6a4",
      "number": 10,
      "content": "Ngươi cho rằng tay mình thừa sức làm điều mình muốn,",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.860Z",
      "updatedAt": "2024-09-23T06:01:53.860Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "0189f354-5cd4-4378-9fef-23e446cc674d",
      "number": 10,
      "content": "Nên ngươi chẳng ngại mà cứ làm.",
      "order": 3,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.865Z",
      "updatedAt": "2024-09-23T06:01:53.865Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "19dc46fe-0585-43b0-8c26-0f2bf792f7f5",
      "number": 11,
      "content": "Ngươi nào có kính nể ai hoặc kính sợ ai,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.869Z",
      "updatedAt": "2024-09-23T06:01:53.869Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "e0de60d5-c783-4065-a13f-3304329c3e96",
      "number": 11,
      "content": "Có phải vì thế mà ngươi đã nói dối,",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.874Z",
      "updatedAt": "2024-09-23T06:01:53.874Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "ded6a739-4ed9-4786-8a9d-e70dc3f53252",
      "number": 11,
      "content": "Chẳng nhớ Ta,",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.879Z",
      "updatedAt": "2024-09-23T06:01:53.879Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "8780a9fd-b896-4a67-98e1-a433b9577211",
      "number": 11,
      "content": "Và không quan tâm gì đến Ta chăng?",
      "order": 3,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.884Z",
      "updatedAt": "2024-09-23T06:01:53.884Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "263da01a-cfef-494d-b201-2d88ed2450d4",
      "number": 11,
      "content": "Có phải vì lâu lắm rồi Ta đã cố nhịn,",
      "order": 4,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.889Z",
      "updatedAt": "2024-09-23T06:01:53.889Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "b78b81b5-181e-49d3-a8be-ede408f7bae8",
      "number": 11,
      "content": "Nên ngươi không kính sợ Ta chăng?",
      "order": 5,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.894Z",
      "updatedAt": "2024-09-23T06:01:53.894Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "7fa610a3-d4df-4d69-84b7-556ba78e4873",
      "number": 12,
      "content": "Ta sẽ công bố những gì công chính của ngươi ra, cùng các việc làm của ngươi nữa,",
      "order": 0,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.899Z",
      "updatedAt": "2024-09-23T06:01:53.899Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "b47f6fab-3639-4355-8f90-a64f19476dc2",
      "number": 12,
      "content": "Nhưng chúng sẽ chẳng giúp được gì cho ngươi đâu.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.904Z",
      "updatedAt": "2024-09-23T06:01:53.904Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "56e84e1d-c25e-49ef-a7bf-b4a62ff91c8e",
      "number": 13,
      "content": "Khi ngươi kêu cầu, hãy để các thần tượng ngươi thờ cúng giải cứu ngươi.",
      "order": 0,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.909Z",
      "updatedAt": "2024-09-23T06:01:53.909Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "06623e66-0e76-44de-9930-c9a50cc460e2",
      "number": 13,
      "content": "Kìa chỉ một cơn gió thổi qua,",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.914Z",
      "updatedAt": "2024-09-23T06:01:53.914Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "f7b869b5-d0a0-4b40-b57b-ac6b042b3ee0",
      "number": 13,
      "content": "Một làn hơi nhẹ lướt qua, cũng đủ sức cuốn tất cả chúng bay mất.",
      "order": 2,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.920Z",
      "updatedAt": "2024-09-23T06:01:53.920Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "eeea1178-3139-4eff-8859-0b8eeda394c1",
      "number": 13,
      "content": "Nhưng ai để lòng tin cậy Ta sẽ được đất làm cơ nghiệp,",
      "order": 3,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.925Z",
      "updatedAt": "2024-09-23T06:01:53.925Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "01a90310-ac56-4216-bf49-e1f6af38b123",
      "number": 13,
      "content": "Và sẽ thừa hưởng núi thánh của Ta.”",
      "order": 4,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.929Z",
      "updatedAt": "2024-09-23T06:01:53.929Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "8de4b3aa-0cb0-42f8-8ced-cb61714cfac4",
      "number": 14,
      "content": "Có lời phán rằng,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.934Z",
      "updatedAt": "2024-09-23T06:01:53.934Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "6d06d1cc-df19-4a71-a005-6542df25c70d",
      "number": 14,
      "content": "“Hãy đắp đường! Hãy đắp đường! Hãy sửa soạn đường.",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.947Z",
      "updatedAt": "2024-09-23T06:01:53.947Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "4511d978-7a04-4002-b842-86b1b0bd162e",
      "number": 14,
      "content": "Hãy dẹp bỏ mọi chướng ngại trên đường của dân Ta,”",
      "order": 2,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.955Z",
      "updatedAt": "2024-09-23T06:01:53.955Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "e951ee42-32fd-4e9c-a118-539fa81d29e0",
      "number": 15,
      "content": "Vì Đấng Chí Cao và Chí Tôn,",
      "order": 0,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.960Z",
      "updatedAt": "2024-09-23T06:01:53.960Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "5fdf9792-aed6-475d-a491-2d2c0690410b",
      "number": 15,
      "content": "Đấng ngự trong cõi vĩnh hằng, Đấng mang danh là Đấng Thánh, phán thế này:",
      "order": 1,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.966Z",
      "updatedAt": "2024-09-23T06:01:53.966Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "0f59583f-88d8-46a6-8b41-209dbcbe302b",
      "number": 15,
      "content": "“Dù Ta ngự trong cõi chí cao và chí thánh,",
      "order": 2,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.970Z",
      "updatedAt": "2024-09-23T06:01:53.970Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "3c1cff47-c5c9-48db-8458-95f5fbe931d6",
      "number": 15,
      "content": "Nhưng Ta vẫn ở với người có lòng tan vỡ và khiêm cung,",
      "order": 3,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.975Z",
      "updatedAt": "2024-09-23T06:01:53.975Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "d22fe397-481d-4486-b32c-cf33bd86f975",
      "number": 15,
      "content": "Để làm hồi sinh tinh thần của người khiêm tốn,",
      "order": 4,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.980Z",
      "updatedAt": "2024-09-23T06:01:53.980Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "0896eda8-b8a9-4299-ae03-70ca944e8143",
      "number": 15,
      "content": "Và làm hồi sinh tấm lòng của người tan vỡ,",
      "order": 5,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.985Z",
      "updatedAt": "2024-09-23T06:01:53.985Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "302e564f-20f9-4396-b6e5-f2b2ff0556d4",
      "number": 16,
      "content": "Vì Ta sẽ không buộc tội đến đời đời,",
      "order": 0,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.990Z",
      "updatedAt": "2024-09-23T06:01:53.990Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "b1f0a926-370f-4b23-804c-ec6c1d014b99",
      "number": 16,
      "content": "Ta sẽ không giận buồn mãi mãi,",
      "order": 1,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:53.995Z",
      "updatedAt": "2024-09-23T06:01:53.995Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "db469f44-9872-4b7e-b288-2126f66d9b10",
      "number": 16,
      "content": "Kẻo linh khí và các linh hồn Ta đã dựng nên",
      "order": 2,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54Z",
      "updatedAt": "2024-09-23T06:01:54Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "f45c3fba-ebb4-4f19-924b-90c372615244",
      "number": 16,
      "content": "Phải tàn tạ trước mặt Ta chăng.",
      "order": 3,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54.015Z",
      "updatedAt": "2024-09-23T06:01:54.015Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "72b05029-0418-4898-b302-bcc262296943",
      "number": 17,
      "content": "Vì tội tham lam ích kỷ của nó, Ta đã giận và đánh phạt nó;",
      "order": 0,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54.020Z",
      "updatedAt": "2024-09-23T06:01:54.020Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "73d11fc7-eda4-4ea3-aa91-c468995f08f4",
      "number": 17,
      "content": "Ta đã ẩn mặt với nó và giận nó;",
      "order": 1,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54.025Z",
      "updatedAt": "2024-09-23T06:01:54.025Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "8664175d-dc29-43da-a85b-07799d3efa5b",
      "number": 17,
      "content": "Thế mà nó vẫn trở lại con đường lòng nó muốn đi theo.",
      "order": 2,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54.029Z",
      "updatedAt": "2024-09-23T06:01:54.029Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "f64c693a-ae32-44af-923e-d79b3273c8c3",
      "number": 18,
      "content": "Ta đã thấy rõ các đường lối nó, dầu vậy Ta sẽ chữa lành nó;",
      "order": 0,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54.034Z",
      "updatedAt": "2024-09-23T06:01:54.034Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "95e078b9-0dd0-454a-ae6d-94dc6223c154",
      "number": 18,
      "content": "Ta sẽ dẫn dắt nó và phục hồi niềm an ủi cho nó và những kẻ than khóc của nó.",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54.040Z",
      "updatedAt": "2024-09-23T06:01:54.040Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "55b7bf82-15ed-43c2-a9bf-505b4f10968d",
      "number": 19,
      "content": "Ta sẽ làm cho môi miệng chúng thốt lên những lời ca ngợi.",
      "order": 0,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54.045Z",
      "updatedAt": "2024-09-23T06:01:54.045Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "daa18508-866c-4a92-9948-48d68510bb9b",
      "number": 19,
      "content": "Hòa bình thịnh vượng! Hòa bình thịnh vượng đến với những kẻ ở xa và những người ở gần,” **CHÚA** phán,",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54.049Z",
      "updatedAt": "2024-09-23T06:01:54.049Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "93b477f4-61dc-4919-84ad-8bd02cca0772",
      "number": 19,
      "content": "“Rồi Ta sẽ chữa lành nó.”",
      "order": 2,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54.055Z",
      "updatedAt": "2024-09-23T06:01:54.055Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "08db5a61-bfcf-48f3-b091-f3a49a429a76",
      "number": 20,
      "content": "Nhưng phường gian ác giống như biển động;",
      "order": 0,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54.060Z",
      "updatedAt": "2024-09-23T06:01:54.060Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "1a6aa570-df9f-44dd-a115-690011004f05",
      "number": 20,
      "content": "Vì chúng không thể lặng yên;",
      "order": 1,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:01:54.065Z",
      "updatedAt": "2024-09-23T06:01:54.065Z",
      "chapterId": "37c0182e-7805-47a9-9bba-c45609e08671"
    },
    {
      "id": "e8b52e78-b199-4332-9e95-10e513da4488",
      "number": 20,
      "content": "Chúng giống như sóng biển, cứ cuộn lên, đẩy ra bùn, và làm vẩn đục.",
      "order": 2,
      "parNumber": 80,
      "parIndex": 0,
