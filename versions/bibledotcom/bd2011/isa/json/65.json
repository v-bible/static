{
  "chapter": {
    "id": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f",
    "number": 65,
    "ref": "https://www.bible.com/bible/19/ISA.65.BD2011",
    "createdAt": "2024-09-23T05:08:41.128Z",
    "updatedAt": "2024-09-23T05:08:41.128Z",
    "bookId": "655961d7-6793-43d1-be06-cff911774d08"
  },
  "verses": [
    {
      "id": "56b15c16-033e-4553-80c4-96c144fdb828",
      "number": 1,
      "content": "“Ta đã cho những kẻ chẳng thèm hỏi đến Ta được cầu xin Ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.036Z",
      "updatedAt": "2024-09-23T06:02:21.036Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "a286cddc-504d-4eeb-b6a8-39552d63edfe",
      "number": 1,
      "content": "Ta đã cho những kẻ chẳng tìm kiếm Ta được tìm Ta.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.051Z",
      "updatedAt": "2024-09-23T06:02:21.051Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "afebc586-f8cf-444a-8350-333e071b7004",
      "number": 1,
      "content": "Ta nói với một dân chẳng thèm kêu cầu danh Ta rằng,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.053Z",
      "updatedAt": "2024-09-23T06:02:21.053Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "42a0253f-8b44-4f00-a46c-bb0e93ab1d86",
      "number": 1,
      "content": "‘Ta ở đây! Ta ở đây!’",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.055Z",
      "updatedAt": "2024-09-23T06:02:21.055Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "d99d5719-cb61-47e2-9149-b246cd94ae13",
      "number": 2,
      "content": "Suốt ngày Ta dang hai tay",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.057Z",
      "updatedAt": "2024-09-23T06:02:21.057Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "2821f84c-907a-440f-b0b0-e7e43d5913f4",
      "number": 2,
      "content": "Mời gọi một dân phản nghịch,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.059Z",
      "updatedAt": "2024-09-23T06:02:21.059Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "e1374c46-10da-4a7d-b22c-b8339bfe942b",
      "number": 2,
      "content": "Một dân cứ bước theo đường lối gian tà,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.061Z",
      "updatedAt": "2024-09-23T06:02:21.061Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "b62125df-6aee-4e1d-a0c0-561cb521fded",
      "number": 2,
      "content": "Theo ý riêng mình,",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.063Z",
      "updatedAt": "2024-09-23T06:02:21.063Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "73cb2a51-ac74-4255-b5e7-b14eed1487e5",
      "number": 3,
      "content": "Một dân không ngừng chọc giận",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.065Z",
      "updatedAt": "2024-09-23T06:02:21.065Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "f6a4db2b-da93-4ea6-b3d3-07e4b6be0e4a",
      "number": 3,
      "content": "Ngay vào mặt Ta,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.068Z",
      "updatedAt": "2024-09-23T06:02:21.068Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "dce912ba-2a77-4df9-8a50-1a046de0d8d7",
      "number": 3,
      "content": "Chúng dâng con vật hiến tế cho tà thần trong các vườn,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.070Z",
      "updatedAt": "2024-09-23T06:02:21.070Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "8a69dbbe-b10f-4631-924f-61cb37b91feb",
      "number": 3,
      "content": "Và dâng hương cho các thần tượng trên những bàn thờ bằng gạch;",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.072Z",
      "updatedAt": "2024-09-23T06:02:21.072Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "2cd025f3-6440-4bba-9cad-7a34628e64d5",
      "number": 4,
      "content": "Chúng ngồi trầm tư mặc tưởng giữa các mồ mả,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.075Z",
      "updatedAt": "2024-09-23T06:02:21.075Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "5cb4d5d4-3990-490d-85b1-a0d4172e8e62",
      "number": 4,
      "content": "Và thức thâu đêm tĩnh tọa nơi mộ địa âm u.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.077Z",
      "updatedAt": "2024-09-23T06:02:21.077Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "d92ac369-1060-4289-b7c3-a99b2210c2ed",
      "number": 4,
      "content": "Chúng ăn thịt heo ô uế;",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.080Z",
      "updatedAt": "2024-09-23T06:02:21.080Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "384f6a48-9cdf-4fc8-8563-ec54f48e1995",
      "number": 4,
      "content": "Bát đĩa chúng chứa đầy các thức ăn gớm ghiếc.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.083Z",
      "updatedAt": "2024-09-23T06:02:21.083Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "aa1a4653-186f-49bc-be1b-6e5d0a32a2bf",
      "number": 5,
      "content": "Chúng là những kẻ đã nói,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.086Z",
      "updatedAt": "2024-09-23T06:02:21.086Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "d827f585-1eca-4886-b621-d427ae6c644a",
      "number": 5,
      "content": "‘Hãy đứng xa ra, chớ lại gần ta, vì ta thánh khiết hơn ngươi!’",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.091Z",
      "updatedAt": "2024-09-23T06:02:21.091Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "79de8399-2dc5-4744-9cab-927a11659803",
      "number": 5,
      "content": "Những điều ấy làm cho khói giận của Ta bốc ra nơi lỗ mũi,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.095Z",
      "updatedAt": "2024-09-23T06:02:21.095Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "92b5269b-9c40-42cc-a1c6-147a02bab71d",
      "number": 5,
      "content": "Như lửa hừng cháy hừng hực suốt ngày.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.099Z",
      "updatedAt": "2024-09-23T06:02:21.099Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "16b9e422-e4a3-4352-b701-608c4ad134ca",
      "number": 6,
      "content": "Này, những việc ấy đã được ghi lại trước mặt Ta;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.104Z",
      "updatedAt": "2024-09-23T06:02:21.104Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "34ba6228-5bf4-41e3-93d8-aa959e3958ef",
      "number": 6,
      "content": "Ta sẽ không im lặng, nhưng sẽ báo trả,",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.108Z",
      "updatedAt": "2024-09-23T06:02:21.108Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "7d3a8965-d383-4b32-9753-88715b5f4559",
      "number": 6,
      "content": "Ta sẽ báo trả ngay vào lòng chúng;",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.116Z",
      "updatedAt": "2024-09-23T06:02:21.116Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "3a785a28-c992-49f7-9c19-5ac51d8046c6",
      "number": 7,
      "content": "Ta sẽ báo trả tội của các ngươi và tội của tổ tiên các ngươi nữa,”",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.121Z",
      "updatedAt": "2024-09-23T06:02:21.121Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "c0577cfc-dfeb-46ca-91cf-6409bd65ba9a",
      "number": 7,
      "content": "**CHÚA** phán.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.126Z",
      "updatedAt": "2024-09-23T06:02:21.126Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "1dfe693b-7f91-451a-b799-4bd4b7ab9a05",
      "number": 25,
      "content": "**CHÚA** phán.",
      "order": 5,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.561Z",
      "updatedAt": "2024-09-23T06:02:21.561Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "fd9542f7-5662-4e0d-9e80-c246a9cd1219",
      "number": 7,
      "content": "“Chúng đã dâng hương cho các tà thần trên các núi;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.132Z",
      "updatedAt": "2024-09-23T06:02:21.132Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "e4189654-0779-4145-b7a9-298881cee797",
      "number": 7,
      "content": "Chúng đã xúc phạm đến Ta trên các ngọn đồi,",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.137Z",
      "updatedAt": "2024-09-23T06:02:21.137Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "8786ba55-6d54-4649-b053-79bc844bcf5f",
      "number": 7,
      "content": "Nên Ta sẽ lường theo những việc chúng làm khi trước,",
      "order": 4,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.142Z",
      "updatedAt": "2024-09-23T06:02:21.142Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "3b5d8577-27cd-44df-b2b6-bc37c5d9abfc",
      "number": 7,
      "content": "Mà báo trả đích đáng vào lòng chúng.”",
      "order": 5,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.147Z",
      "updatedAt": "2024-09-23T06:02:21.147Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "c8c38b02-2549-49c5-89b8-7515a7840d65",
      "number": 8,
      "content": "**CHÚA** phán thế này:",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.151Z",
      "updatedAt": "2024-09-23T06:02:21.151Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "a1ecc2c5-ca61-45ae-bb29-46ad7d0fa705",
      "number": 8,
      "content": "“Như rượu mới còn tìm thấy trong chùm nho héo,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.156Z",
      "updatedAt": "2024-09-23T06:02:21.156Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "2f306a12-fc64-4134-8ccc-d17ba8e0b650",
      "number": 8,
      "content": "Người ta bảo rằng, ‘Đừng bỏ nó đi,",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.161Z",
      "updatedAt": "2024-09-23T06:02:21.161Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "ed425107-5840-4071-8d48-f64f41679ac1",
      "number": 8,
      "content": "Vì trong đó còn tìm thấy chút phước lành,’",
      "order": 3,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.166Z",
      "updatedAt": "2024-09-23T06:02:21.166Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "b81209d0-224a-4c15-9236-912438e0ff1f",
      "number": 8,
      "content": "Thì vì cớ các tôi tớ Ta, Ta sẽ làm như vậy;",
      "order": 4,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.172Z",
      "updatedAt": "2024-09-23T06:02:21.172Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "6b708df3-758f-4618-9bb0-0fafd597b03a",
      "number": 8,
      "content": "Ta sẽ không tiêu diệt hết chúng đâu.",
      "order": 5,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.177Z",
      "updatedAt": "2024-09-23T06:02:21.177Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "ccb8b0d5-2d54-4765-8987-31ef079af590",
      "number": 9,
      "content": "Từ Gia-cốp Ta sẽ cho phát sinh một dòng dõi;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.182Z",
      "updatedAt": "2024-09-23T06:02:21.182Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "6a17ec48-48d5-4378-9006-70c9192837ca",
      "number": 9,
      "content": "Từ Giu-đa sẽ ra một người thừa kế các núi thánh của Ta;",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.187Z",
      "updatedAt": "2024-09-23T06:02:21.187Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "d1f7e229-24a6-43b4-96fd-46116844cf88",
      "number": 9,
      "content": "Người Ta chọn sẽ thừa hưởng cơ nghiệp đó;",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.192Z",
      "updatedAt": "2024-09-23T06:02:21.192Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "05c6bf4a-7f10-444d-9c9b-96c20b878ce3",
      "number": 9,
      "content": "Các tôi tớ Ta sẽ cư ngụ ở đó.",
      "order": 3,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.197Z",
      "updatedAt": "2024-09-23T06:02:21.197Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "1d74698e-001c-47f7-9986-5743581702bf",
      "number": 10,
      "content": "Sa-rôn sẽ là đồng cỏ cho các bầy súc vật;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.202Z",
      "updatedAt": "2024-09-23T06:02:21.202Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "1b8a2de1-c3eb-4688-baf5-596b7385d321",
      "number": 10,
      "content": "Thung lũng A-cơ sẽ là chỗ cho các đàn bò nằm,",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.208Z",
      "updatedAt": "2024-09-23T06:02:21.208Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "2d757a48-ff83-4d05-8e29-1d977bd220a0",
      "number": 10,
      "content": "Vì dân Ta đã tìm kiếm Ta.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.213Z",
      "updatedAt": "2024-09-23T06:02:21.213Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "d7d5f0b0-3c19-4099-82ee-291dab666512",
      "number": 11,
      "content": "Nhưng hỡi các ngươi là những kẻ bỏ **CHÚA**,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.218Z",
      "updatedAt": "2024-09-23T06:02:21.218Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "aea2d483-e0af-4f1d-8bad-a0cc01fe2d70",
      "number": 11,
      "content": "Những kẻ quên núi thánh của Ta,",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.224Z",
      "updatedAt": "2024-09-23T06:02:21.224Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "0ac1772e-c84a-4a97-a103-e59416ac4d17",
      "number": 11,
      "content": "Những kẻ lập bàn thờ cho Thần May Mắn,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.229Z",
      "updatedAt": "2024-09-23T06:02:21.229Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "ee899eaf-b98b-4b6a-9099-27287de4692b",
      "number": 11,
      "content": "Dâng rượu pha cho Thần Định Mệnh,",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.238Z",
      "updatedAt": "2024-09-23T06:02:21.238Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "e617afe3-1f03-4a82-8b7a-7217de5c8c6c",
      "number": 12,
      "content": "Vì thế Ta sẽ định phần số các ngươi cho gươm giáo;",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.248Z",
      "updatedAt": "2024-09-23T06:02:21.248Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "50613043-24f1-4b87-b453-09d6c65d10e7",
      "number": 12,
      "content": "Tất cả các ngươi sẽ quỳ xuống trước kẻ giết mình,",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.252Z",
      "updatedAt": "2024-09-23T06:02:21.252Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "ce1c704a-fbef-42c3-9110-fa589697ea44",
      "number": 12,
      "content": "Vì khi Ta gọi, các ngươi chẳng thèm đáp lại;",
      "order": 2,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.260Z",
      "updatedAt": "2024-09-23T06:02:21.260Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "ff72aa47-38c6-4979-99e8-c7dcfca430b8",
      "number": 12,
      "content": "Khi Ta phán, các ngươi chẳng muốn lắng nghe;",
      "order": 3,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.269Z",
      "updatedAt": "2024-09-23T06:02:21.269Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "1ed3cfa9-abf5-4f4f-ae17-b7ab6311df43",
      "number": 12,
      "content": "Nhưng các ngươi cứ làm điều ác trước mắt Ta,",
      "order": 4,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.274Z",
      "updatedAt": "2024-09-23T06:02:21.274Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "6c73a015-1cc3-4d4e-8901-1a269a3f74cd",
      "number": 12,
      "content": "Và cứ chọn làm những điều Ta không thích.”",
      "order": 5,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.279Z",
      "updatedAt": "2024-09-23T06:02:21.279Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "2ad980b4-f077-424d-ba5d-463932def2d1",
      "number": 13,
      "content": "Vì thế **CHÚA** Hằng Hữu phán thế này:",
      "order": 0,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.285Z",
      "updatedAt": "2024-09-23T06:02:21.285Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "9499202a-2709-48c1-a295-3d515841aea2",
      "number": 13,
      "content": "“Này, các tôi tớ Ta sẽ ăn,",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.290Z",
      "updatedAt": "2024-09-23T06:02:21.290Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "6d05c393-f473-4017-82bc-b3c2464c8c7f",
      "number": 13,
      "content": "Nhưng các ngươi sẽ đói;",
      "order": 2,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.295Z",
      "updatedAt": "2024-09-23T06:02:21.295Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "b43b88c1-1d67-43fd-8a11-589ae71c396a",
      "number": 13,
      "content": "Này, các tôi tớ Ta sẽ uống,",
      "order": 3,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.300Z",
      "updatedAt": "2024-09-23T06:02:21.300Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "1e009ebc-7907-4560-aed8-28b3b01ddc92",
      "number": 13,
      "content": "Nhưng các ngươi sẽ khát;",
      "order": 4,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.305Z",
      "updatedAt": "2024-09-23T06:02:21.305Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "b044e81a-cf75-47d8-97ba-81792c7156d9",
      "number": 13,
      "content": "Này, các tôi tớ Ta sẽ vui mừng,",
      "order": 5,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.310Z",
      "updatedAt": "2024-09-23T06:02:21.310Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "558f0d90-05b3-4673-b3c8-3a210ffd287c",
      "number": 13,
      "content": "Nhưng các ngươi sẽ tủi hổ;",
      "order": 6,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.315Z",
      "updatedAt": "2024-09-23T06:02:21.315Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "cd120afc-327f-4d31-ba82-c30794a6474f",
      "number": 14,
      "content": "Này, các tôi tớ Ta sẽ ca hát vì tâm hồn vui vẻ,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.320Z",
      "updatedAt": "2024-09-23T06:02:21.320Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "d7d738c8-4506-4266-a711-f4c8552eeef7",
      "number": 14,
      "content": "Nhưng các ngươi sẽ khóc than vì sầu thảm trong lòng,",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.325Z",
      "updatedAt": "2024-09-23T06:02:21.325Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "2b9f0ae0-6639-4990-97a1-8382518347a5",
      "number": 14,
      "content": "Và các ngươi sẽ khóc lóc vì tâm linh đau đớn.",
      "order": 2,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.330Z",
      "updatedAt": "2024-09-23T06:02:21.330Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "b86a9cb4-db6b-455f-ae3a-f65fe89085a3",
      "number": 15,
      "content": "Các ngươi cũng sẽ được lưu danh, nhưng chỉ để làm lời nguyền rủa cho những kẻ Ta chọn,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.335Z",
      "updatedAt": "2024-09-23T06:02:21.335Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "aa5ba81a-86d8-4d93-ae4e-5c229b21eea3",
      "number": 15,
      "content": "Vì **CHÚA** Hằng Hữu sẽ tiêu diệt ngươi,",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.339Z",
      "updatedAt": "2024-09-23T06:02:21.339Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "ab118957-6176-4a65-beab-5a1cd4b49647",
      "number": 15,
      "content": "Nhưng các tôi tớ Ngài, Ngài sẽ gọi họ bằng một danh khác.",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.347Z",
      "updatedAt": "2024-09-23T06:02:21.347Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "d3e0310b-30ce-4b67-a6f8-0a9bf876da4e",
      "number": 16,
      "content": "Vì vậy, ở trong xứ, ai cầu phúc cho mình",
      "order": 0,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.355Z",
      "updatedAt": "2024-09-23T06:02:21.355Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "21f6fbf7-53c7-409b-a012-e346db267114",
      "number": 16,
      "content": "Sẽ nhân danh Đức Chúa Trời của sự thật mà cầu phúc;",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.359Z",
      "updatedAt": "2024-09-23T06:02:21.359Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "790b17e7-8c4c-45d7-a5d8-c9d62895ac47",
      "number": 16,
      "content": "Ở trong xứ, ai tuyên thệ",
      "order": 2,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.364Z",
      "updatedAt": "2024-09-23T06:02:21.364Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "7884ed66-f6a5-460f-9469-4c237eb2f5e1",
      "number": 16,
      "content": "Sẽ nhân danh Đức Chúa Trời của sự thật mà thề,",
      "order": 3,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.369Z",
      "updatedAt": "2024-09-23T06:02:21.369Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "2fa0192c-b610-48a9-bdaa-d8e8ea03b9bd",
      "number": 16,
      "content": "Vì những gian truân khi trước đã bị quên rồi,",
      "order": 4,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.374Z",
      "updatedAt": "2024-09-23T06:02:21.374Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "78749695-e61c-4880-9507-5ea319b334a8",
      "number": 16,
      "content": "Và vì chúng đã bị che khuất khỏi mắt Ta rồi.",
      "order": 5,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.379Z",
      "updatedAt": "2024-09-23T06:02:21.379Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "ce4d352b-1054-41e9-bc6e-f025fc86349a",
      "number": 17,
      "content": "Vì này, Ta sẽ dựng nên",
      "order": 0,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.386Z",
      "updatedAt": "2024-09-23T06:02:21.386Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "0dbde644-eecb-45aa-b8c9-51357cf89019",
      "number": 17,
      "content": "Trời mới và đất mới;",
      "order": 1,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.396Z",
      "updatedAt": "2024-09-23T06:02:21.396Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "7e1f07e2-507a-4de4-9ea9-c54fb803cf0a",
      "number": 17,
      "content": "Những gì trước kia sẽ không còn được nhớ đến nữa,",
      "order": 2,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.401Z",
      "updatedAt": "2024-09-23T06:02:21.401Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "dfa0d075-5ee3-48cd-aabb-d4da84b068fd",
      "number": 17,
      "content": "Và cũng không còn đến trong trí nữa.",
      "order": 3,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.406Z",
      "updatedAt": "2024-09-23T06:02:21.406Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "a5e1fea8-83da-4eb9-8bf0-da58552d308c",
      "number": 18,
      "content": "Nhưng hãy hân hoan và vui mừng mãi mãi",
      "order": 0,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.411Z",
      "updatedAt": "2024-09-23T06:02:21.411Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "58f050c9-7093-4b82-91cb-6a760da64ed3",
      "number": 18,
      "content": "Về những gì Ta tạo dựng;",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.416Z",
      "updatedAt": "2024-09-23T06:02:21.416Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "2ac6ae92-5657-49ec-853b-bb8b023805e6",
      "number": 18,
      "content": "Vì này, Ta dựng nên Giê-ru-sa-lem để làm một niềm vui;",
      "order": 2,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.421Z",
      "updatedAt": "2024-09-23T06:02:21.421Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "9df9f676-99b8-4637-8b12-6fd4226c963c",
      "number": 18,
      "content": "Và dân thành ấy sẽ là một niềm vui thỏa.",
      "order": 3,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.426Z",
      "updatedAt": "2024-09-23T06:02:21.426Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "685d3477-e287-42e0-bbc7-01243c3ad640",
      "number": 19,
      "content": "Ta sẽ vui vẻ vì Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.431Z",
      "updatedAt": "2024-09-23T06:02:21.431Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "4793332b-18f0-45dc-a6f8-52a6a5992c47",
      "number": 19,
      "content": "Ta sẽ vui mừng vì dân Ta;",
      "order": 1,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.436Z",
      "updatedAt": "2024-09-23T06:02:21.436Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "5abb1292-38f1-4866-95f0-c00f1381374c",
      "number": 19,
      "content": "Tiếng than khóc sẽ không còn nghe trong thành ấy;",
      "order": 2,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.442Z",
      "updatedAt": "2024-09-23T06:02:21.442Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "367614b4-5f4f-47dd-9aca-096c935a4267",
      "number": 19,
      "content": "Tiếng khóc than sẽ vĩnh viễn không còn.",
      "order": 3,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.447Z",
      "updatedAt": "2024-09-23T06:02:21.447Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "6893f14d-6e50-4f45-bc75-0bc14de6fc64",
      "number": 20,
      "content": "Trong thành ấy trẻ thơ sẽ không còn chết yểu;",
      "order": 0,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.452Z",
      "updatedAt": "2024-09-23T06:02:21.452Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "b5799525-af86-4265-b2ea-fceba6b37846",
      "number": 20,
      "content": "Không người già nào chẳng hưởng trọn tuổi thọ của mình;",
      "order": 1,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.457Z",
      "updatedAt": "2024-09-23T06:02:21.457Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "691a7736-22b0-4f6b-bc57-ce1c66021fdb",
      "number": 20,
      "content": "Vì ai trăm tuổi mà qua đời sẽ bị xem là chết trẻ;",
      "order": 2,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.462Z",
      "updatedAt": "2024-09-23T06:02:21.462Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "3d0e9e12-409b-4670-aed6-0f9252775722",
      "number": 20,
      "content": "Người chưa được bách niên mà thất lộc sẽ xem như bị rủa nguyền.",
      "order": 3,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.466Z",
      "updatedAt": "2024-09-23T06:02:21.466Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "75f390ab-1d20-4dfd-a549-90b1f47aed67",
      "number": 21,
      "content": "Chúng sẽ xây nhà và an cư trong các nhà đó;",
      "order": 0,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.471Z",
      "updatedAt": "2024-09-23T06:02:21.471Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "666fcc09-d89b-4793-a594-4c735d4f1436",
      "number": 21,
      "content": "Chúng sẽ trồng vườn nho và được ăn trái mình trồng.",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.476Z",
      "updatedAt": "2024-09-23T06:02:21.476Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "21d07fde-ac53-4a9a-81e5-b0288ea76eaa",
      "number": 22,
      "content": "Chúng sẽ không xây nhà để bị người khác vào chiếm ở;",
      "order": 0,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.481Z",
      "updatedAt": "2024-09-23T06:02:21.481Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "52c623bc-2c93-4632-9b97-24023546010b",
      "number": 22,
      "content": "Chúng sẽ không trồng cây để bị người khác đến chiếm ăn.",
      "order": 1,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.486Z",
      "updatedAt": "2024-09-23T06:02:21.486Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "bf001836-3be5-4c7a-9d6e-c1b53b627476",
      "number": 22,
      "content": "Vì cây cối sống bao nhiêu tuổi, dân Ta cũng sẽ thọ bấy nhiêu.",
      "order": 2,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.492Z",
      "updatedAt": "2024-09-23T06:02:21.492Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "a7043000-3546-4413-813e-22e8a0764489",
      "number": 22,
      "content": "Những kẻ Ta chọn sẽ hưởng lâu dài mọi kết quả do tay mình làm ra.",
      "order": 3,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.497Z",
      "updatedAt": "2024-09-23T06:02:21.497Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "b8c964f8-2089-41de-a2dc-6585a7f20d38",
      "number": 23,
      "content": "Chúng sẽ không làm lụng cách luống công vô ích;",
      "order": 0,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.504Z",
      "updatedAt": "2024-09-23T06:02:21.504Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "1e710eaf-f786-430c-bee7-5c6e58590492",
      "number": 23,
      "content": "Chúng sinh con ra chẳng phải để chuốc lấy âu sầu,",
      "order": 1,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.509Z",
      "updatedAt": "2024-09-23T06:02:21.509Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "fded2533-af20-4970-bcf0-c1f3a9f3a826",
      "number": 23,
      "content": "Vì con cháu chúng sẽ là những người được **CHÚA** ban phước,",
      "order": 2,
      "parNumber": 95,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.514Z",
      "updatedAt": "2024-09-23T06:02:21.514Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "81260368-d7d8-43a9-a359-1477e7bba49e",
      "number": 23,
      "content": "Và dòng dõi chúng cùng với chúng nữa.",
      "order": 3,
      "parNumber": 96,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.519Z",
      "updatedAt": "2024-09-23T06:02:21.519Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "a99ad00c-a123-46a1-b255-1198b8dac955",
      "number": 24,
      "content": "Rồi đây, trước khi chúng cầu xin, Ta đã trả lời;",
      "order": 0,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.524Z",
      "updatedAt": "2024-09-23T06:02:21.524Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "bc57d652-4adf-4556-8e14-57182d19be96",
      "number": 24,
      "content": "Khi chúng còn đang nói, Ta đã nghe rõ hết.",
      "order": 1,
      "parNumber": 98,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.530Z",
      "updatedAt": "2024-09-23T06:02:21.530Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "dc1deeca-e250-495d-86ce-899ced26f646",
      "number": 25,
      "content": "Chó sói và chiên con sẽ ăn cỏ với nhau,",
      "order": 0,
      "parNumber": 99,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.535Z",
      "updatedAt": "2024-09-23T06:02:21.535Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "7cdc453a-d6cf-4f97-a671-79d01a7b1c72",
      "number": 25,
      "content": "Sư tử sẽ ăn rơm rạ như bò,",
      "order": 1,
      "parNumber": 100,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.540Z",
      "updatedAt": "2024-09-23T06:02:21.540Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "1d49e5f3-d0ab-4691-b5ea-c8a81b3792c0",
      "number": 25,
      "content": "Còn bụi đất sẽ là thức ăn của loài rắn;",
      "order": 2,
      "parNumber": 101,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.545Z",
      "updatedAt": "2024-09-23T06:02:21.545Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "12829d92-cbc0-401b-aeef-6c0cc06da600",
      "number": 25,
      "content": "Chúng sẽ không gây đau thương hay hủy diệt",
      "order": 3,
      "parNumber": 102,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.551Z",
      "updatedAt": "2024-09-23T06:02:21.551Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    },
    {
      "id": "fec4b30a-3958-418d-88dd-20754594233e",
      "number": 25,
      "content": "Trong khắp núi thánh của Ta,”",
      "order": 4,
      "parNumber": 103,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:02:21.555Z",
      "updatedAt": "2024-09-23T06:02:21.555Z",
      "chapterId": "e474bd86-9471-4d1b-97b3-ed93ffa94d0f"
    }
  ],
