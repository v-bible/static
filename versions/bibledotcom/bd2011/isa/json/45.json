    {
      "id": "d6ee3aab-19a3-443c-9b0d-a88da5d6ecbf",
      "content": "Đức Chúa Trời Dùng Vua Si-ru",
      "order": 0,
      "createdAt": "2024-09-23T06:01:12.784Z",
      "updatedAt": "2024-09-23T06:01:12.784Z",
