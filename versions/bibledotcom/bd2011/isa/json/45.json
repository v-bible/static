    },
    {
      "id": "415e872e-874b-41dc-9c6f-e5b1595d5b95",
      "content": "Hình Tượng Không Cứu Ai",
      "order": 0,
      "createdAt": "2024-09-23T06:01:13.218Z",
      "updatedAt": "2024-09-23T06:01:13.218Z",
