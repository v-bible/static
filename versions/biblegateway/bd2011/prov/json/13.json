{
  "chapter": {
    "id": "92bf695b-f55f-4aa0-b1b5-021c713e9d38",
    "number": 13,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2013&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.689Z",
    "updatedAt": "2024-09-24T01:00:45.689Z",
    "bookId": "2a7cfb76-854f-4125-8376-cd1d8bb799d7"
  },
  "verses": [
    {
      "id": "e9916efd-a88b-4da0-a3f9-9d79595f9d8c",
      "number": 1,
      "content": "Con khôn ngoan nghe lời cha khuyên dạy,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.241Z",
      "updatedAt": "2024-09-24T02:14:58.241Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "9bcad5d9-3f54-4e12-a269-b6c36b4a4eb0",
      "number": 1,
      "content": "Còn đứa nhạo báng không nghe lời quở trách của ai.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.250Z",
      "updatedAt": "2024-09-24T02:14:58.250Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "206b493c-1a94-49d8-933b-6973e992528a",
      "number": 2,
      "content": "Người tốt sẽ hưởng bông trái lời mình nói,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.252Z",
      "updatedAt": "2024-09-24T02:14:58.252Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "67fb14d2-0d02-4878-a36b-9da131182293",
      "number": 2,
      "content": "Còn kẻ bội bạc phải chuốc lấy bạo tàn.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.254Z",
      "updatedAt": "2024-09-24T02:14:58.254Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "d242a01b-66d6-409a-b46e-0017ec5748eb",
      "number": 3,
      "content": "Ai giữ miệng mình giữ được mạng sống mình,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.256Z",
      "updatedAt": "2024-09-24T02:14:58.256Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "3c0feca8-1cd0-4095-862e-06f2f4823e92",
      "number": 3,
      "content": "Kẻ nói nhiều ắt bại hoại chẳng sai.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.259Z",
      "updatedAt": "2024-09-24T02:14:58.259Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "c6e0db20-2a14-48ca-9e38-269e5d0a1cfd",
      "number": 4,
      "content": "Kẻ biếng nhác đói rũ mà không có gì ăn đỡ bụng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.264Z",
      "updatedAt": "2024-09-24T02:14:58.264Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "4d54ecec-f66f-4a82-8e55-b11fb03c8b1a",
      "number": 4,
      "content": "Còn người siêng năng lúc nào thực phẩm cũng dư thừa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.266Z",
      "updatedAt": "2024-09-24T02:14:58.266Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "4f189fa3-6cc7-40dc-80ad-213a540fe512",
      "number": 5,
      "content": "Người ngay lành ghét những gì gian dối,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.268Z",
      "updatedAt": "2024-09-24T02:14:58.268Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "fe9b0943-3ecd-447a-9fc7-62e0c83ff623",
      "number": 5,
      "content": "Còn kẻ ác gian mang đến xấu hổ và ô nhơ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.270Z",
      "updatedAt": "2024-09-24T02:14:58.270Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "b973115b-e2d3-4f14-b5e8-31e8463870db",
      "number": 6,
      "content": "Ðức công chính bảo hộ người theo đường chân chính,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.273Z",
      "updatedAt": "2024-09-24T02:14:58.273Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "ead9e6db-b133-48f1-8ea7-d5f2f0707dc5",
      "number": 6,
      "content": "Còn sự gian tà lật đổ kẻ ác gian.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.276Z",
      "updatedAt": "2024-09-24T02:14:58.276Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "29235789-e8b6-4c66-907d-2c0d1d3146fb",
      "number": 7,
      "content": "Có kẻ làm ra vẻ ta đây giàu có mà chẳng có gì cả,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.281Z",
      "updatedAt": "2024-09-24T02:14:58.281Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "8597b3e1-99e3-418a-9d28-f0cc1b229a2e",
      "number": 7,
      "content": "Cũng có người làm ra vẻ nghèo nàn nhưng lại có của cải rất nhiều.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.286Z",
      "updatedAt": "2024-09-24T02:14:58.286Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "23bbb152-4485-45a5-8ff2-3c55bab9a03d",
      "number": 8,
      "content": "Của cải nhiều có thể là tiền để chờ chuộc mạng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.291Z",
      "updatedAt": "2024-09-24T02:14:58.291Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "de869f46-fc04-49c2-81d4-375232cc8590",
      "number": 8,
      "content": "Còn người nghèo nàn chẳng nghe ai hăm dọa tiếng nào.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.295Z",
      "updatedAt": "2024-09-24T02:14:58.295Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "126621fa-0fda-4116-9aba-db74f523f416",
      "number": 9,
      "content": "Ánh sáng người ngay lành luôn tỏa soi rực rỡ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.300Z",
      "updatedAt": "2024-09-24T02:14:58.300Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "f165a3a7-7ce3-424a-9404-dcc7366f8461",
      "number": 9,
      "content": "Nhưng đèn kẻ ác gian bị dập tắt mịt mờ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.308Z",
      "updatedAt": "2024-09-24T02:14:58.308Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "9a3b3629-45aa-49d6-ac9d-5b8ece2f298f",
      "number": 10,
      "content": "Sự kiêu ngạo chỉ gây nên tranh cãi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.314Z",
      "updatedAt": "2024-09-24T02:14:58.314Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "b832a1e6-7c49-451d-b674-b9225c9c9da3",
      "number": 10,
      "content": "Người khôn ngoan biết ghi nhận lời khuyên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.319Z",
      "updatedAt": "2024-09-24T02:14:58.319Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "2f4de0ac-4f42-4066-9489-48901fc83d41",
      "number": 11,
      "content": "Của cải nhờ lừa dối tinh vi để giàu mau sẽ chóng mất;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.323Z",
      "updatedAt": "2024-09-24T02:14:58.323Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "312fef97-5f3b-468c-854e-9f88137be7d4",
      "number": 11,
      "content": "Nhưng người chăm chỉ làm ăn sẽ giàu vững từ từ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.327Z",
      "updatedAt": "2024-09-24T02:14:58.327Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "fec88d0b-fb2f-4bec-99b3-393df4799f23",
      "number": 12,
      "content": "Trì hoãn niềm hy vọng làm cho lòng đau đớn,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.330Z",
      "updatedAt": "2024-09-24T02:14:58.330Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "4288c5da-7e4f-4b2a-8614-05ebe6fae010",
      "number": 12,
      "content": "Ao ước được thành là cây sự sống xanh tươi.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.333Z",
      "updatedAt": "2024-09-24T02:14:58.333Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "4dc0d683-dcae-4588-b6f3-33a4c785d9f5",
      "number": 13,
      "content": "Kẻ khinh dể lời hằng sống chuốc cho mình sự hủy diệt,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.336Z",
      "updatedAt": "2024-09-24T02:14:58.336Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "81b2cf16-bc25-4799-ab73-dc31a1d77f55",
      "number": 13,
      "content": "Người quý trọng điều răn chắc sẽ được thưởng ban.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.339Z",
      "updatedAt": "2024-09-24T02:14:58.339Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "107227ea-7c71-4824-a08a-d308709f0c74",
      "number": 14,
      "content": "Sự dạy dỗ của người khôn ngoan chính là nguồn sự sống,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.341Z",
      "updatedAt": "2024-09-24T02:14:58.341Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "823922ed-199e-4471-849e-5355e087c32c",
      "number": 14,
      "content": "Kẻ vâng theo sẽ không sa vào bẫy tử vong.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.344Z",
      "updatedAt": "2024-09-24T02:14:58.344Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "9d2ceebb-6ee2-4a3d-857b-e2444020eae2",
      "number": 15,
      "content": "Nhận định sáng suốt luôn mang về ân huệ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.346Z",
      "updatedAt": "2024-09-24T02:14:58.346Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "75c95de1-c405-4223-81d6-39b95131eadb",
      "number": 15,
      "content": "Còn đường kẻ bất trung gặp khốn đốn rất nhiều.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.349Z",
      "updatedAt": "2024-09-24T02:14:58.349Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "6ac260ca-5832-4b68-a2a3-5bb5133831c4",
      "number": 16,
      "content": "Người khôn ngoan nắm vững tin tức trước khi hành động,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.351Z",
      "updatedAt": "2024-09-24T02:14:58.351Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "9b038fd1-4d2c-4944-81c9-9341f62b0d24",
      "number": 16,
      "content": "Kẻ dại khờ hấp tấp phô bày sự dại dột mình ra.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.354Z",
      "updatedAt": "2024-09-24T02:14:58.354Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "cef5e41c-dc8a-4139-81b2-59fdc2c3dc70",
      "number": 17,
      "content": "Sứ giả gian hùng gây thêm điều rắc rối,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.357Z",
      "updatedAt": "2024-09-24T02:14:58.357Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "014db221-42da-4bea-9654-92d44c3cd68b",
      "number": 17,
      "content": "Sứ giả trung thành hóa giải những khó khăn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.359Z",
      "updatedAt": "2024-09-24T02:14:58.359Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "b1f1e096-5a98-433c-8335-7fed6c72481a",
      "number": 18,
      "content": "Khinh dể lời khuyên dạy chuốc lấy bần cùng và nhục nhã,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.362Z",
      "updatedAt": "2024-09-24T02:14:58.362Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "5656b004-c908-4406-b53d-75f3c4ebb596",
      "number": 18,
      "content": "Nhưng ai chịu nghe lời quở trách sẽ được tôn trọng vinh danh.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.364Z",
      "updatedAt": "2024-09-24T02:14:58.364Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "d8e8985a-7312-4ea1-b9c5-93b036a5edeb",
      "number": 19,
      "content": "Mơ ước được thành làm tâm hồn khoan khoái;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.366Z",
      "updatedAt": "2024-09-24T02:14:58.366Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "d86a0b0b-2404-4647-b103-2b60f002ab59",
      "number": 19,
      "content": "Lìa bỏ gian tà là điều kẻ điên dại gớm ghê.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.368Z",
      "updatedAt": "2024-09-24T02:14:58.368Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "851d4eb2-7acd-4f33-ad31-cf12243ef9aa",
      "number": 20,
      "content": "Ai giao thiệp với người khôn ngoan sẽ học được khôn ngoan,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.370Z",
      "updatedAt": "2024-09-24T02:14:58.370Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "e4c01887-670c-49de-b332-a2fd37eb31ab",
      "number": 20,
      "content": "Ai làm bạn với kẻ điên dại sẽ có ngày mang họa.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.372Z",
      "updatedAt": "2024-09-24T02:14:58.372Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "85b8ce36-d8e5-4237-a1d4-2d5f4dd7d4c8",
      "number": 21,
      "content": "Nỗi bất hạnh luôn đuổi theo kẻ tội lỗi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.374Z",
      "updatedAt": "2024-09-24T02:14:58.374Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "aefd1d14-c2b3-4e69-9d72-2a64cdd9f3ff",
      "number": 21,
      "content": "Sự phồn vinh là phần thưởng của người ngay lành.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.375Z",
      "updatedAt": "2024-09-24T02:14:58.375Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "45b9e0f1-20fb-45ba-9525-1c43f750eab5",
      "number": 22,
      "content": "Người tốt để gia tài lại cho con cháu,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.377Z",
      "updatedAt": "2024-09-24T02:14:58.377Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "a809a099-ef8b-4f90-a196-8fcce436b840",
      "number": 22,
      "content": "Nhưng tội nhân tích trữ của cải để lại cho người ngay lành.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.379Z",
      "updatedAt": "2024-09-24T02:14:58.379Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "92b28363-c926-47c2-bc1e-38464a5e4de5",
      "number": 23,
      "content": "Ruộng của người nghèo trúng mùa và mang lại nhiều hoa lợi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.381Z",
      "updatedAt": "2024-09-24T02:14:58.381Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "e8e3158a-581d-42b5-bf00-54894b626038",
      "number": 23,
      "content": "Nhưng sự bất công cướp sạch những hoa lợi đó đi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.383Z",
      "updatedAt": "2024-09-24T02:14:58.383Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "8f6d3760-fd5d-4cae-b49c-0273717fd509",
      "number": 24,
      "content": "Ai kiêng cữ roi vọt là không thương con cái,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.385Z",
      "updatedAt": "2024-09-24T02:14:58.385Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "7bc16c8e-909a-40d4-9abc-123fb5d55316",
      "number": 24,
      "content": "Người thương con chăm chỉ sửa trị con mình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.387Z",
      "updatedAt": "2024-09-24T02:14:58.387Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "eed958c9-85a2-4877-b6ce-4f9616eb0d69",
      "number": 25,
      "content": "Người ngay lành có đầy đủ thức ăn thức uống,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.389Z",
      "updatedAt": "2024-09-24T02:14:58.389Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    },
    {
      "id": "34a22080-4c45-4d7e-b17e-04b198f2dc08",
      "number": 25,
      "content": "Kẻ ác gian bụng cứ đói triền miên.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:58.391Z",
      "updatedAt": "2024-09-24T02:14:58.391Z",
      "chapterId": "92bf695b-f55f-4aa0-b1b5-021c713e9d38"
    }
  ],
