{
  "chapter": {
    "id": "3811ca76-4792-4e0f-998b-5e7c2d035445",
    "number": 4,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%204&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.543Z",
    "updatedAt": "2024-09-24T01:00:45.543Z",
    "bookId": "2a7cfb76-854f-4125-8376-cd1d8bb799d7"
  },
  "verses": [
    {
      "id": "2411ea2a-e4c2-465f-8751-609ed8dfc563",
      "number": 1,
      "content": "Các con ơi, hãy lắng nghe lời cha dạy bảo;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.086Z",
      "updatedAt": "2024-09-24T02:14:10.086Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "d05bee93-62a6-48e0-8023-52f96f77d702",
      "number": 1,
      "content": "Hãy chú tâm lãnh hội đến tinh tường.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.097Z",
      "updatedAt": "2024-09-24T02:14:10.097Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "f8315128-820c-4ddc-adc2-5c63dcd2203d",
      "number": 2,
      "content": "Vì cha cho các con những lời khuyên dạy tốt;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.100Z",
      "updatedAt": "2024-09-24T02:14:10.100Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "2087f1c2-6f13-4549-b542-7532818794a6",
      "number": 2,
      "content": "Mong các con đừng bỏ lời giáo huấn của cha.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.102Z",
      "updatedAt": "2024-09-24T02:14:10.102Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "2591fd5a-1578-4105-aecf-513421d91b66",
      "number": 3,
      "content": "Khi cha còn là một đứa con thơ dại của ông nội các con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.104Z",
      "updatedAt": "2024-09-24T02:14:10.104Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "2ccabbdb-1fd7-47b9-a0e0-617a483c1efd",
      "number": 3,
      "content": "Là con một yêu dấu của bà nội các con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.106Z",
      "updatedAt": "2024-09-24T02:14:10.106Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "a47881a3-d110-4f6d-a3b8-84bcda5cc0e8",
      "number": 4,
      "content": "Ông nội các con đã dạy cha rằng:",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.108Z",
      "updatedAt": "2024-09-24T02:14:10.108Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "2d76c972-8727-4ffc-b13c-4fd2a627a62a",
      "number": 4,
      "content": "Lòng con khá ghi tạc lời cha dạy bảo;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.110Z",
      "updatedAt": "2024-09-24T02:14:10.110Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "192e5335-2c5a-4967-a5e9-b6eb5e90f3a5",
      "number": 4,
      "content": "Giữ mệnh lệnh cha để con sống oai hùng:",
      "order": 2,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.112Z",
      "updatedAt": "2024-09-24T02:14:10.112Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "2a07929b-d0bd-4320-b7f7-d8717a5e997e",
      "number": 5,
      "content": "Hãy kiếm khôn ngoan, khá tìm thông sáng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.117Z",
      "updatedAt": "2024-09-24T02:14:10.117Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "70bbee69-2c9f-4d6e-b9ea-69fe6cc28263",
      "number": 5,
      "content": "Ðừng quên và chớ lìa bỏ lời cha.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.123Z",
      "updatedAt": "2024-09-24T02:14:10.123Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "dd466da0-9812-435d-bfc0-c955589a6b53",
      "number": 6,
      "content": "Ðừng lìa bỏ khôn ngoan, thì nàng sẽ bảo vệ con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.128Z",
      "updatedAt": "2024-09-24T02:14:10.128Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "2ac25b04-633e-40cb-bea6-c6e0e88fdcdb",
      "number": 6,
      "content": "Yêu mến nàng, thì nàng sẽ gìn giữ con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.133Z",
      "updatedAt": "2024-09-24T02:14:10.133Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "cee89bfc-cdac-4316-bd2e-45a3993f6307",
      "number": 7,
      "content": "Khôn ngoan là nhu cầu tối thượng, nên con phải đạt sự khôn ngoan;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.138Z",
      "updatedAt": "2024-09-24T02:14:10.138Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "80e5f8db-ffb8-4071-920e-c1750672e956",
      "number": 7,
      "content": "Trong mọi thứ con đạt được ở đời, hãy cố đạt sự thông sáng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.144Z",
      "updatedAt": "2024-09-24T02:14:10.144Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "4dff68f6-fd1a-491d-978c-3d46f060666c",
      "number": 8,
      "content": "Hãy coi trọng nàng, nàng sẽ làm con được tôn trọng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.152Z",
      "updatedAt": "2024-09-24T02:14:10.152Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "6937e984-fd8d-45cb-b305-3698b8042e04",
      "number": 8,
      "content": "Ôm giữ nàng, nàng sẽ mang vinh dự đến cho con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.161Z",
      "updatedAt": "2024-09-24T02:14:10.161Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "046a6834-c3ac-48e4-853b-6ec0868caa95",
      "number": 9,
      "content": "Nàng sẽ đặt trên đầu con một vòng hoa tuyệt mỹ;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.166Z",
      "updatedAt": "2024-09-24T02:14:10.166Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "6e5c484d-f23f-4140-8f02-089bca7b37af",
      "number": 9,
      "content": "Nàng sẽ dâng tặng con mão miện đắc thắng huy hoàng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.171Z",
      "updatedAt": "2024-09-24T02:14:10.171Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "efe48950-010c-409e-8c28-de819191581e",
      "number": 10,
      "content": "Con ơi, hãy lắng nghe và tiếp nhận lời cha,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.176Z",
      "updatedAt": "2024-09-24T02:14:10.176Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "a1f3dc51-21ba-497d-83a8-51325a7903e9",
      "number": 10,
      "content": "Thì những năm của đời con sẽ thêm nhiều lên.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.184Z",
      "updatedAt": "2024-09-24T02:14:10.184Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "0e8b65bd-ca3c-4671-bded-aeec6ea43682",
      "number": 11,
      "content": "Cha đã dạy dỗ con theo đường lối khôn ngoan;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.187Z",
      "updatedAt": "2024-09-24T02:14:10.187Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "37e0cf26-f8ae-4805-8d56-82834828d487",
      "number": 11,
      "content": "Cha đã hướng dẫn con theo những nẻo đường ngay thẳng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.190Z",
      "updatedAt": "2024-09-24T02:14:10.190Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "c64e9c8c-7a46-4b6d-8510-bc6b87827e89",
      "number": 12,
      "content": "Khi con đi, các bước con sẽ không bị cản trở;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.193Z",
      "updatedAt": "2024-09-24T02:14:10.193Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "484dc549-4aeb-42f9-80de-5293deaaeb56",
      "number": 12,
      "content": "Nếu con chạy, con sẽ không bị vấp ngã bao giờ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.196Z",
      "updatedAt": "2024-09-24T02:14:10.196Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "d7375a70-cac5-4bc0-b8e9-29d64db57172",
      "number": 13,
      "content": "Hãy giữ chặt lời giáo huấn, đừng buông nó ra.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.199Z",
      "updatedAt": "2024-09-24T02:14:10.199Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "a4f7044a-efa5-4c65-8074-e2ae34d999a5",
      "number": 13,
      "content": "Hãy canh giữ nàng, vì nàng là sự sống của con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.204Z",
      "updatedAt": "2024-09-24T02:14:10.204Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "a5124300-9f3c-453b-aa3e-a6f987303a11",
      "number": 14,
      "content": "Con đừng đi vào nẻo đường của phường gian ác,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.207Z",
      "updatedAt": "2024-09-24T02:14:10.207Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "f0ffdecd-f1ff-4e70-888a-9cd73503e10b",
      "number": 14,
      "content": "Bước chân con chớ đặt vào lối của bọn gian tà.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.210Z",
      "updatedAt": "2024-09-24T02:14:10.210Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "45cb4047-9563-4bde-90ad-a28ca0b3f2a6",
      "number": 15,
      "content": "Con hãy tránh xa nó và đừng đi vào đường đó;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.212Z",
      "updatedAt": "2024-09-24T02:14:10.212Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "a98a8324-86c5-4713-a00a-612e225d0a14",
      "number": 15,
      "content": "Con hãy quay khỏi nó và tiếp tục đường mình.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.216Z",
      "updatedAt": "2024-09-24T02:14:10.216Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "4ee8ae5d-e61c-4a24-b559-00460c2ef815",
      "number": 16,
      "content": "Vì kẻ ác sẽ không ngủ được nếu không làm gì ác đức;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.221Z",
      "updatedAt": "2024-09-24T02:14:10.221Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "d820e51a-dd2b-4c00-bced-efc8e45f1619",
      "number": 16,
      "content": "Chúng sẽ bị mất ngủ nếu chưa gây vấp ngã cho ai.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.227Z",
      "updatedAt": "2024-09-24T02:14:10.227Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "7c4ee503-14cb-4409-bb1c-493d8ff9c97f",
      "number": 17,
      "content": "Vì chúng cho làm chuyện gian tà thiết yếu như ăn cơm ăn bánh,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.232Z",
      "updatedAt": "2024-09-24T02:14:10.232Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "4fc0d5de-ab47-4107-91d5-3abf5cec8c29",
      "number": 17,
      "content": "Và cho việc bạo tàn là cần có như uống rượu khi ăn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.238Z",
      "updatedAt": "2024-09-24T02:14:10.238Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "9f0f038b-dbc5-426e-9449-067f9fe97b40",
      "number": 18,
      "content": "Ðường của người ngay lành giống như ánh bình minh rực rỡ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.243Z",
      "updatedAt": "2024-09-24T02:14:10.243Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "64b24d15-f98e-4a18-a539-f842d4c3cc2a",
      "number": 18,
      "content": "Càng lúc càng rạng ngời cho đến khi chiếu sáng khắp bầu trời;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.249Z",
      "updatedAt": "2024-09-24T02:14:10.249Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "b0720551-a06c-43e6-a797-cff25f728227",
      "number": 19,
      "content": "Còn đường kẻ gian ác giống như đêm khuya tăm tối;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.254Z",
      "updatedAt": "2024-09-24T02:14:10.254Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "c357ed4a-b1bc-4bc1-b421-b892ac4970b6",
      "number": 19,
      "content": "Chúng ngã nhào mà không biết ngã vì sao.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.259Z",
      "updatedAt": "2024-09-24T02:14:10.259Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "07e607c0-321d-433e-bd90-03101a98c8f5",
      "number": 20,
      "content": "Con ơi, hãy chú tâm để ý đến lời cha;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.265Z",
      "updatedAt": "2024-09-24T02:14:10.265Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "804e7909-dd33-467b-ac43-8e98b4933498",
      "number": 20,
      "content": "Hãy nghiêng tai nghe lời cha khuyên bảo.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.270Z",
      "updatedAt": "2024-09-24T02:14:10.270Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "57dcbe4c-14cf-4ca8-8e45-ae33e50791d0",
      "number": 21,
      "content": "Ðừng để chúng vuột khỏi tầm mắt con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.275Z",
      "updatedAt": "2024-09-24T02:14:10.275Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "f277e51e-8b57-4ad8-b2e7-c431aba6be85",
      "number": 21,
      "content": "Hãy giữ chúng trong lòng con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.281Z",
      "updatedAt": "2024-09-24T02:14:10.281Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "b1b1ffc7-6bec-4fd9-878a-26c106dc5dc1",
      "number": 22,
      "content": "Vì chúng là sự sống cho người nào tìm được chúng;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.285Z",
      "updatedAt": "2024-09-24T02:14:10.285Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "fd61ad0d-944a-4dca-a889-56bb5487ba76",
      "number": 22,
      "content": "Chúng là sức khỏe cho thân thể ai có chúng trong lòng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.289Z",
      "updatedAt": "2024-09-24T02:14:10.289Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "c0aec84a-a3dd-4f2f-8473-207542f3ee1d",
      "number": 23,
      "content": "Hãy cẩn thận giữ tấm lòng của con hơn hết,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.292Z",
      "updatedAt": "2024-09-24T02:14:10.292Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "5e7fca29-0cf4-48a0-a21f-2786e135f003",
      "number": 23,
      "content": "Vì các nguồn sự sống đều từ đó mà ra.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.295Z",
      "updatedAt": "2024-09-24T02:14:10.295Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "efe0d16e-e207-4016-b87c-b7badc337b50",
      "number": 24,
      "content": "Hãy bỏ khỏi miệng con những lời điêu ngoa gian dối,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.297Z",
      "updatedAt": "2024-09-24T02:14:10.297Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "26d54333-7291-4ab0-b985-bee77e9fc4a4",
      "number": 24,
      "content": "Hãy quăng khỏi môi con những lời dối trá lọc lừa.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.300Z",
      "updatedAt": "2024-09-24T02:14:10.300Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "6f54012e-067b-41b7-b22d-5da0716a7f92",
      "number": 25,
      "content": "Mắt con hãy nhìn thẳng về phía trước,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.302Z",
      "updatedAt": "2024-09-24T02:14:10.302Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "0b54e2ac-deb6-4606-8451-295f0edabb2b",
      "number": 25,
      "content": "Ánh mắt con khá hướng thẳng về phía trước mặt con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.305Z",
      "updatedAt": "2024-09-24T02:14:10.305Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "3e6e5a84-02be-4d0d-9ab2-331f03143b9f",
      "number": 26,
      "content": "Hãy giữ chân con trong đường ngay nẻo chánh,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.307Z",
      "updatedAt": "2024-09-24T02:14:10.307Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "567769b2-d348-4b50-bc5a-900465d730d1",
      "number": 26,
      "content": "Ðể mọi đường lối con được thiết lập vững vàng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.310Z",
      "updatedAt": "2024-09-24T02:14:10.310Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "af064b02-1128-43f0-914b-0cafb5e7cf9c",
      "number": 27,
      "content": "Ðừng quay qua bên phải hoặc quay qua bên trái;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.312Z",
      "updatedAt": "2024-09-24T02:14:10.312Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    },
    {
      "id": "e33ba9f9-8da7-4e96-99ce-15ea5efd4f61",
      "number": 27,
      "content": "Hãy quay bước chân con ra khỏi chỗ gian tà.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:14:10.317Z",
      "updatedAt": "2024-09-24T02:14:10.317Z",
      "chapterId": "3811ca76-4792-4e0f-998b-5e7c2d035445"
    }
  ],
