{
  "chapter": {
    "id": "6d69021c-f7c8-4468-a560-da04276493d4",
    "number": 16,
    "ref": "https://www.biblegateway.com/passage/?search=Ch%C3%A2m%20Ng%C3%B4n%2016&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.738Z",
    "updatedAt": "2024-09-24T01:00:45.738Z",
    "bookId": "2a7cfb76-854f-4125-8376-cd1d8bb799d7"
  },
  "verses": [
    {
      "id": "0058e7ea-1860-4c9b-80e1-d81d0706fb10",
      "number": 1,
      "content": "Sự toan tính trong lòng thuộc về loài người,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.280Z",
      "updatedAt": "2024-09-24T02:15:14.280Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "dbef2380-d911-4edf-acfc-0dc5577746a0",
      "number": 1,
      "content": "Nhưng sự đáp lời của lưỡi đến từ Chúa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.289Z",
      "updatedAt": "2024-09-24T02:15:14.289Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "63b9b39b-c562-4e7f-98c7-bc14bc36edbd",
      "number": 2,
      "content": "Mọi đường lối của người ta đều trong sạch theo mắt họ;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.291Z",
      "updatedAt": "2024-09-24T02:15:14.291Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "f07aa9f4-d456-4a98-ba50-414efb96c897",
      "number": 2,
      "content": "Nhưng Chúa cân nhắc tấm lòng.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.293Z",
      "updatedAt": "2024-09-24T02:15:14.293Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "f8395340-0165-4656-918e-3c9661a6ef15",
      "number": 3,
      "content": "Hãy phó thác các công việc của bạn cho Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.296Z",
      "updatedAt": "2024-09-24T02:15:14.296Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "c125567f-4762-479a-aa1f-ae6e93f067eb",
      "number": 3,
      "content": "Thì các chương trình của bạn sẽ thành công.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.301Z",
      "updatedAt": "2024-09-24T02:15:14.301Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "91682b70-fb09-4a4f-9274-d9aa23f5d776",
      "number": 4,
      "content": "Chúa có mục đích khi dựng nên mọi vật,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.307Z",
      "updatedAt": "2024-09-24T02:15:14.307Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "8d561191-654d-4b91-a309-2a7d9abde38f",
      "number": 4,
      "content": "Ngay cả kẻ gian ác cho ngày tai vạ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.312Z",
      "updatedAt": "2024-09-24T02:15:14.312Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "1a5e0245-d415-4e26-a908-da695db50257",
      "number": 5,
      "content": "Chúa gớm ghiếc kẻ có lòng kiêu ngạo;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.317Z",
      "updatedAt": "2024-09-24T02:15:14.317Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "5a692bcf-396f-4745-967e-4dcf061c6916",
      "number": 5,
      "content": "Chắc chắn nó sẽ không khỏi bị phạt.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.322Z",
      "updatedAt": "2024-09-24T02:15:14.322Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "b258a297-5779-4728-8837-e18dc60b2391",
      "number": 6,
      "content": "Nhờ ơn thương xót và đức chân thật mà lỗi lầm được chuộc,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.327Z",
      "updatedAt": "2024-09-24T02:15:14.327Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "41512636-b214-4c6f-bd01-52ff11ccadb6",
      "number": 6,
      "content": "Do sự kính sợ Chúa mà người ta lánh xa điều ác.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.332Z",
      "updatedAt": "2024-09-24T02:15:14.332Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "5414651a-d696-4425-ba7f-a479233e1498",
      "number": 7,
      "content": "Khi đường lối người nào đẹp lòng Chúa,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.337Z",
      "updatedAt": "2024-09-24T02:15:14.337Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "72e38dc8-cacd-4459-9b04-8e88625641e0",
      "number": 7,
      "content": "Thì Ngài khiến ngay cả kẻ thù cũng làm hòa với người ấy.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.342Z",
      "updatedAt": "2024-09-24T02:15:14.342Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "1e56e108-4d38-460d-b133-c66d9aac6965",
      "number": 8,
      "content": "Thà ít của mà ngay lành đạo đức,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.347Z",
      "updatedAt": "2024-09-24T02:15:14.347Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "18c74602-e531-4c85-8c90-7b85b55c71d7",
      "number": 8,
      "content": "Còn hơn lợi tức nhiều mà bất công bất chính cặp theo.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.352Z",
      "updatedAt": "2024-09-24T02:15:14.352Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "eae3e9c9-2185-4693-8253-80eb1ff0e501",
      "number": 9,
      "content": "Lòng người ta toan tính đường lối mình,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.357Z",
      "updatedAt": "2024-09-24T02:15:14.357Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "dc23e672-5991-4431-bbbb-9179af44e435",
      "number": 9,
      "content": "Nhưng Chúa định đoạt các bước của mỗi người.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.362Z",
      "updatedAt": "2024-09-24T02:15:14.362Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "8fd6e706-4005-41d1-83da-71c8c63359e3",
      "number": 10,
      "content": "Khi môi vua đã phán những lời như sấm ngôn bất biến,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.367Z",
      "updatedAt": "2024-09-24T02:15:14.367Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "f1e5f8e2-3e40-4c7c-83a5-b6d83c7ff606",
      "number": 10,
      "content": "Thì miệng vua không được truyền ra một phán quyết bất công.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.372Z",
      "updatedAt": "2024-09-24T02:15:14.372Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "3732f487-cc3b-4468-9445-f32ecefc72c6",
      "number": 11,
      "content": "Cán cân công bình và bàn cân chính xác đều thuộc về Chúa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.377Z",
      "updatedAt": "2024-09-24T02:15:14.377Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "493f3b0f-104b-4047-a5fc-65bd5b8ce3e9",
      "number": 11,
      "content": "Tất cả trái cân trong bao là công việc của Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.382Z",
      "updatedAt": "2024-09-24T02:15:14.382Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "9a8ba341-6b67-4b05-98f7-d1026087bd4a",
      "number": 12,
      "content": "Vua mà còn làm ác thì quả là điều gớm ghiếc,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.387Z",
      "updatedAt": "2024-09-24T02:15:14.387Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "90cba814-6787-4840-9e8c-93ca8efa275d",
      "number": 12,
      "content": "Vì ngôi vua được lập lên để thi hành sự công chính.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.392Z",
      "updatedAt": "2024-09-24T02:15:14.392Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "d2fbc9af-8ce6-4e79-87fc-e11e39a90468",
      "number": 13,
      "content": "Môi chân thật được các vua vui thích,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.396Z",
      "updatedAt": "2024-09-24T02:15:14.396Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "c405ef2d-e14c-4a82-9728-819d2b19e674",
      "number": 13,
      "content": "Ai nói lời chân chính thì được họ mến thương.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.401Z",
      "updatedAt": "2024-09-24T02:15:14.401Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "de86e24a-e621-4ec1-852c-25aadd5cdd15",
      "number": 14,
      "content": "Cơn thịnh nộ của vua khác nào sứ giả của tử thần,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.406Z",
      "updatedAt": "2024-09-24T02:15:14.406Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "9b65081e-b23d-476b-b10d-336b167ab1f6",
      "number": 14,
      "content": "Nhưng người khôn ngoan biết làm cho nguôi nó.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.411Z",
      "updatedAt": "2024-09-24T02:15:14.411Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "cbcb6530-23b0-45b5-9daf-9f2ff10b7360",
      "number": 15,
      "content": "Nét mặt vua vui đem đến hy vọng sống,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.415Z",
      "updatedAt": "2024-09-24T02:15:14.415Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "2ecc3264-ef94-4268-a197-bce06f43d5a9",
      "number": 15,
      "content": "Ân điển của vua khác nào mây mang lại trận mưa xuân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.420Z",
      "updatedAt": "2024-09-24T02:15:14.420Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "1114b86a-ffb6-475b-909b-1ff76969eed9",
      "number": 16,
      "content": "Ðược khôn ngoan sang hơn vàng biết mấy!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.429Z",
      "updatedAt": "2024-09-24T02:15:14.429Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "a64ead7e-1ae1-436f-90b9-a46b60a324e0",
      "number": 16,
      "content": "Ðược thông sáng đáng hơn bạc biết bao!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.435Z",
      "updatedAt": "2024-09-24T02:15:14.435Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "ebc24061-3a77-4af7-8c0e-f316ff15ef01",
      "number": 17,
      "content": "Con đường của người chính trực luôn tránh xa điều ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.440Z",
      "updatedAt": "2024-09-24T02:15:14.440Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "f4fc9f79-db04-48f6-bd0f-3edaef1cdf76",
      "number": 17,
      "content": "Ai canh giữ đường lối mình giữ được mạng sống mình.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.445Z",
      "updatedAt": "2024-09-24T02:15:14.445Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "4c354612-6535-40cf-9aac-8a9d3a13b8e3",
      "number": 18,
      "content": "Sự kiêu ngạo đi trước sự hủy diệt;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.450Z",
      "updatedAt": "2024-09-24T02:15:14.450Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "d9861d77-8502-43c0-80cb-f2e5a0dde0c2",
      "number": 18,
      "content": "Tính kiêu căng đi trước sự sụp đổ.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.455Z",
      "updatedAt": "2024-09-24T02:15:14.455Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "1c31b1f2-a283-4b7a-a5d1-362ab5d2a742",
      "number": 19,
      "content": "Thà có tinh thần khiêm tốn với người nghèo khó,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.459Z",
      "updatedAt": "2024-09-24T02:15:14.459Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "c1bacd5b-1d10-4d96-81f5-daa6dc78adf6",
      "number": 19,
      "content": "Còn hơn chia chiến lợi phẩm với kẻ kiêu ngạo.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.464Z",
      "updatedAt": "2024-09-24T02:15:14.464Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "277a7019-7957-437e-89c9-b89ceb034eac",
      "number": 20,
      "content": "Ai chú tâm làm theo lời Chúa sẽ tìm được phước;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.469Z",
      "updatedAt": "2024-09-24T02:15:14.469Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "24764f90-2588-42af-a058-56b3c18fdb37",
      "number": 20,
      "content": "Người nào tin cậy Chúa thật phước hạnh biết bao.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.474Z",
      "updatedAt": "2024-09-24T02:15:14.474Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "afb0a418-cb6e-4a4e-8a34-e6072d6c3c55",
      "number": 21,
      "content": "Người có lòng khôn ngoan được gọi là người thông sáng;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.478Z",
      "updatedAt": "2024-09-24T02:15:14.478Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "dc261c41-32f7-4735-a1f3-c835a872467b",
      "number": 21,
      "content": "Lời nói dịu dàng làm tăng sức thuyết phục người nghe.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.483Z",
      "updatedAt": "2024-09-24T02:15:14.483Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "156dcdda-a797-4e77-89ec-226911b744d1",
      "number": 22,
      "content": "Sự thông sáng là suối sự sống cho người có nó;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.488Z",
      "updatedAt": "2024-09-24T02:15:14.488Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "9dc172ae-cc8c-41a4-bedf-9b94d54ab93c",
      "number": 22,
      "content": "Còn lời chỉ bảo của kẻ dại chỉ là sự điên rồ.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.493Z",
      "updatedAt": "2024-09-24T02:15:14.493Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "84a95090-ee45-4a42-9f60-8d85381be98b",
      "number": 23,
      "content": "Lòng người khôn dạy miệng mình ăn nói;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.497Z",
      "updatedAt": "2024-09-24T02:15:14.497Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "8a483073-e2ad-4043-b3ed-d869f2527bb6",
      "number": 23,
      "content": "Và tạo thêm sức thuyết phục ở trên môi.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.502Z",
      "updatedAt": "2024-09-24T02:15:14.502Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "931062b0-f360-49ad-bcc7-980574310b9b",
      "number": 24,
      "content": "Lời hay ý đẹp giống như mật của tàng ong,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.507Z",
      "updatedAt": "2024-09-24T02:15:14.507Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "5c03ff22-2fe4-494a-94ac-78f9d7f5afbb",
      "number": 24,
      "content": "Làm ngọt ngào tâm hồn và khỏe mạnh xương cốt.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.512Z",
      "updatedAt": "2024-09-24T02:15:14.512Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "18f2e910-c3e5-4433-b08b-b060dff8467b",
      "number": 25,
      "content": "Có một con đường dường như chính đáng cho người ta,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.517Z",
      "updatedAt": "2024-09-24T02:15:14.517Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "cfe8ed58-164b-4cf6-b7a2-a766e7b354b5",
      "number": 25,
      "content": "Nhưng đến cuối đường hóa ra là tử lộ.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.521Z",
      "updatedAt": "2024-09-24T02:15:14.521Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "ad7d8149-b155-47ba-b2a4-1950f5dd4388",
      "number": 26,
      "content": "Cơn đói khát thúc người lao công kiếm việc,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.527Z",
      "updatedAt": "2024-09-24T02:15:14.527Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "7ce18225-77f7-4539-b17f-f11fc56c1268",
      "number": 26,
      "content": "Miệng muốn ăn giục người ấy tìm kế sinh nhai.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.532Z",
      "updatedAt": "2024-09-24T02:15:14.532Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "45a6cefa-55a8-4c4c-80ac-c26e4e6bd24a",
      "number": 27,
      "content": "Kẻ vô lại toan tính chuyện hại người,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.536Z",
      "updatedAt": "2024-09-24T02:15:14.536Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "2c22bcd1-6302-4ba6-8b09-fc2da6223df1",
      "number": 27,
      "content": "Môi miệng nó như lửa hừng thiêu hủy.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.541Z",
      "updatedAt": "2024-09-24T02:15:14.541Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "cd881a06-59bc-4f90-ae6c-10925dc480e3",
      "number": 28,
      "content": "Kẻ xuyên tạc gieo rắc sự xung đột;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.546Z",
      "updatedAt": "2024-09-24T02:15:14.546Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "8b6f0aa1-9e3e-4041-8818-bf85930421e2",
      "number": 28,
      "content": "Kẻ rỉ tai gây chia rẽ bạn bè.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.551Z",
      "updatedAt": "2024-09-24T02:15:14.551Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "157eb7da-8f80-4aca-9c4a-14425d05f2de",
      "number": 29,
      "content": "Kẻ hung bạo xúi bậy người lân cận mình,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.555Z",
      "updatedAt": "2024-09-24T02:15:14.555Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "9a989af4-654b-4b32-a538-d09faee323d2",
      "number": 29,
      "content": "Và dẫn người ấy vào con đường bất hảo.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.560Z",
      "updatedAt": "2024-09-24T02:15:14.560Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "9306bce0-304a-417c-b898-c624553951f2",
      "number": 30,
      "content": "Nó nháy mắt làm hiệu toan tính chuyện gian tà,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.565Z",
      "updatedAt": "2024-09-24T02:15:14.565Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "4f6cc296-bdaf-4d62-a4b8-5cc385fe3c82",
      "number": 30,
      "content": "Nó bặm môi ngầm bảo thực hiện điều gian ác.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.570Z",
      "updatedAt": "2024-09-24T02:15:14.570Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "afc966cf-8bb4-4776-8d78-aea9dd7008a8",
      "number": 31,
      "content": "Tóc bạc là mão miện hiển vinh;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.574Z",
      "updatedAt": "2024-09-24T02:15:14.574Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "e048914b-5c63-48fa-b4d3-d1bc659be5f0",
      "number": 31,
      "content": "Có được nó nhờ sống đời công chính.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.579Z",
      "updatedAt": "2024-09-24T02:15:14.579Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "6118a72c-a12f-4ad1-b447-6f92eaa5462f",
      "number": 32,
      "content": "Người chậm nóng giận giỏi hơn người dũng mãnh;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.584Z",
      "updatedAt": "2024-09-24T02:15:14.584Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "c6ae5cb1-d16a-4f8c-97a2-d5f289820960",
      "number": 32,
      "content": "Người kiềm chế tính khí mình giỏi hơn người chiếm được thành.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.589Z",
      "updatedAt": "2024-09-24T02:15:14.589Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "bd30265c-1b93-4d27-9e56-91c00e9f78f1",
      "number": 33,
      "content": "Người ta bắt thăm trong vạt áo,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.594Z",
      "updatedAt": "2024-09-24T02:15:14.594Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    },
    {
      "id": "c8b4094d-03b5-4ae7-9d87-17e2fb3bde71",
      "number": 33,
      "content": "Nhưng Chúa định đoạt mọi sự.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:15:14.599Z",
      "updatedAt": "2024-09-24T02:15:14.599Z",
      "chapterId": "6d69021c-f7c8-4468-a560-da04276493d4"
    }
  ],
