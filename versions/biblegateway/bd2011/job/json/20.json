    {
      "id": "ff2189e1-0712-4132-90db-b3c66e5e9221",
      "content": "Xô-pha Cho Rằng Kẻ Gian Ác Bị Phạt Là Ðích Ðáng",
      "order": 0,
      "createdAt": "2024-09-24T01:48:36.719Z",
      "updatedAt": "2024-09-24T01:48:36.719Z",
      "verseId": "3d2bfabb-7163-43e5-bf2f-f9b7307ccf0e",
      "chapterId": "c69427ba-ddd1-4d52-b757-9322532328b2"
    }
  ],
