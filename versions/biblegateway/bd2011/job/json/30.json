    {
      "id": "505baa7d-9663-4415-9d34-f7b5c3ebdc05",
      "content": "Gióp Lại Tiếp Tục Binh Vực Ông",
      "order": 0,
      "createdAt": "2024-09-24T01:49:45.843Z",
      "updatedAt": "2024-09-24T01:49:45.843Z",
