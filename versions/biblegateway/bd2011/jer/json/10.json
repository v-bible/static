      "chapterId": "9fd19fc9-e1ed-4988-8577-c50652de1553"
    },
    {
      "id": "8bd1d8af-ff80-4d72-9ad9-fce28b8192c9",
      "number": 25,
      "content": "Trên những dân không kêu cầu danh Ngài,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:27:02.916Z",
      "updatedAt": "2024-09-24T02:27:02.916Z",
      "chapterId": "9fd19fc9-e1ed-4988-8577-c50652de1553"
    },
    {
      "id": "41235bc7-356e-422a-892e-cba0e45f3738",
      "number": 25,
      "content": "Vì chúng đã ăn nuốt Gia-cốp,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:27:02.920Z",
      "updatedAt": "2024-09-24T02:27:02.920Z",
      "chapterId": "9fd19fc9-e1ed-4988-8577-c50652de1553"
    },
    {
      "id": "75059b36-65ae-4b33-8437-66942355bb22",
      "number": 25,
      "content": "Chúng đã nuốt mất ông ấy và phá tan tành chỗ ở của ông ấy.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:27:02.925Z",
      "updatedAt": "2024-09-24T02:27:02.925Z",
      "chapterId": "9fd19fc9-e1ed-4988-8577-c50652de1553"
    }
  ],
  "footnotes": [
    {
