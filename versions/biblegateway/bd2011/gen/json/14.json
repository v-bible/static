    },
    {
      "id": "7aad2305-e939-4deb-b578-db8b0ba9f0cf",
      "content": "Áp-ram và Mên-chi-xê-đéc",
      "order": 0,
      "createdAt": "2024-09-24T01:02:24.429Z",
      "updatedAt": "2024-09-24T01:02:24.429Z",
