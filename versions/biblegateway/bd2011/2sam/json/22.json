    {
      "id": "ff9bd27c-9278-47e1-8659-f768c58fe567",
      "content": "nt: chê-rúp (số ít của chê-ru-bim)",
      "position": 36,
      "order": 1,
      "createdAt": "2024-09-24T01:31:03.789Z",
      "updatedAt": "2024-09-24T01:31:03.789Z",
      "verseId": "927c6e9c-415e-450a-ade3-708466dfeb60",
      "chapterId": "31ffd416-5e40-487e-b22c-41f64e499e57"
