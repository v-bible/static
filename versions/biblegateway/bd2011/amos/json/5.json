    }
  ],
  "headings": [
    {
      "id": "5f606a60-9b5f-404e-8b22-350e053f2c61",
      "content": "Than Khóc vì Tội của I-sơ-ra-ên",
      "order": 0,
      "createdAt": "2024-09-24T02:40:09.123Z",
      "updatedAt": "2024-09-24T02:40:09.123Z",
      "verseId": "be41c220-ccb5-4b20-a81e-5e097206bedc",
      "chapterId": "aa386934-380e-4203-943d-cfbb5480dd84"
    },
    {
      "id": "3258cf7e-2d64-4564-82e8-5b3dd737fb3c",
      "content": "Ngày của Chúa Thật Ðen Tối",
      "order": 0,
      "createdAt": "2024-09-24T02:40:09.340Z",
      "updatedAt": "2024-09-24T02:40:09.340Z",
      "verseId": "8579478c-78ad-4c6b-b217-6439d35e4de1",
      "chapterId": "aa386934-380e-4203-943d-cfbb5480dd84"
    },
    {
      "id": "24d56aa7-0c67-4f1a-9a74-f2f60987ddea",
      "content": "Chúa Không Hài Lòng về Sự Thờ Phượng Chỉ Bằng Hình Thức",
      "order": 0,
      "createdAt": "2024-09-24T02:40:09.378Z",
      "updatedAt": "2024-09-24T02:40:09.378Z",
      "verseId": "da6d9744-b0ad-446f-9b19-0c289d2d130c",
      "chapterId": "aa386934-380e-4203-943d-cfbb5480dd84"
    }
  ],
