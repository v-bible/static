{
  "chapter": {
    "id": "f98949d2-3bf3-4aad-8c13-f6757414e8d2",
    "number": 9,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%209&version=BD2011",
    "createdAt": "2024-09-24T01:00:46.593Z",
    "updatedAt": "2024-09-24T01:00:46.593Z",
    "bookId": "1e0ea116-0110-44af-8fdc-87130c3208f3"
  },
  "verses": [
    {
      "id": "35e423a1-0faa-4051-af51-54923311fb48",
      "number": 1,
      "content": "Tuy nhiên sẽ không còn sầu thảm cho những kẻ bị hoạn nạn nữa.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.539Z",
      "updatedAt": "2024-09-24T02:20:18.539Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "293afc63-5922-4c80-82fc-c5294d37e22e",
      "number": 1,
      "content": "Trong quá khứ Ngài đã hạ xứ Xê-bu-lun và xứ Náp-ta-li xuống,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.549Z",
      "updatedAt": "2024-09-24T02:20:18.549Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "f326db25-e6b9-4108-ae00-cd8f1615e6e6",
      "number": 1,
      "content": "Nhưng trong tương lai Ngài sẽ làm rạng rỡ con đường nằm ven biển,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.551Z",
      "updatedAt": "2024-09-24T02:20:18.551Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "8697d732-2e9a-4b53-a23d-e4d4ed1eb59c",
      "number": 1,
      "content": "Tức miền bên kia Sông Giô-đanh,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.553Z",
      "updatedAt": "2024-09-24T02:20:18.553Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "c1a30d64-c219-44ff-bc25-a137e8eae17e",
      "number": 1,
      "content": "Thuộc vùng Ga-li-lê của các dân ngoại.",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.556Z",
      "updatedAt": "2024-09-24T02:20:18.556Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "ca1207fb-a17a-4d46-9604-eac6f73953c6",
      "number": 2,
      "content": "Dân đi trong bóng tối sẽ thấy ánh sáng lớn;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.558Z",
      "updatedAt": "2024-09-24T02:20:18.558Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "e5bb77bd-70d1-414b-aa01-99ffc6083481",
      "number": 2,
      "content": "Ðối với những kẻ sống trong xứ của bóng tử thần,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.561Z",
      "updatedAt": "2024-09-24T02:20:18.561Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "ef0a83a9-3885-46ad-a77a-5b2c757ea7c7",
      "number": 2,
      "content": "Một vầng chân quang đã bừng lên chiếu sáng trên họ.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.564Z",
      "updatedAt": "2024-09-24T02:20:18.564Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "4e592be6-f8b6-4e3d-8423-c0e46d9a80a5",
      "number": 3,
      "content": "Ngài đã làm cho đất nước này được tăng trưởng bội phần;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.566Z",
      "updatedAt": "2024-09-24T02:20:18.566Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "cf21c96b-3b99-44fd-a56e-c725e9882a79",
      "number": 3,
      "content": "Ngài đã cho niềm vui của họ gia tăng;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.569Z",
      "updatedAt": "2024-09-24T02:20:18.569Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "46474910-ff2a-4fe1-8fa7-52c1142cdc84",
      "number": 3,
      "content": "Họ vui mừng trước mặt Ngài, như vui trong mùa gặt, như vui khi chia nhau chiến lợi phẩm,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.574Z",
      "updatedAt": "2024-09-24T02:20:18.574Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "4cbd904c-ba6d-48f8-a799-a928424310a8",
      "number": 4,
      "content": "Vì cái ách trên cổ họ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.579Z",
      "updatedAt": "2024-09-24T02:20:18.579Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "2f7b8e0e-6400-4145-a898-c01ad8072daf",
      "number": 4,
      "content": "Ðòn gánh trên vai họ,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.584Z",
      "updatedAt": "2024-09-24T02:20:18.584Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "46080329-2031-4014-8540-17c42b98f49f",
      "number": 4,
      "content": "Và cây roi của kẻ áp bức họ",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.589Z",
      "updatedAt": "2024-09-24T02:20:18.589Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "d1ae298b-6577-4f24-b4d6-a9feca760cec",
      "number": 4,
      "content": "Ðã bị Ngài bẻ gãy như trong ngày Mi-đi-an bị đánh bại.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.594Z",
      "updatedAt": "2024-09-24T02:20:18.594Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "f9140f0b-7322-4767-b912-e5cb8c20923e",
      "number": 5,
      "content": "Vì mọi giày trận mang ra chiến trường,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.599Z",
      "updatedAt": "2024-09-24T02:20:18.599Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "355000a0-6996-4f6a-bef0-bee3c294870a",
      "number": 5,
      "content": "Và mọi quân phục đẫm máu quân thù,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.603Z",
      "updatedAt": "2024-09-24T02:20:18.603Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "28e995ca-1904-4c88-9020-9365a24584fb",
      "number": 5,
      "content": "Ðều sẽ bị đem ra thiêu hủy như nhiên liệu được dùng để đốt.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.608Z",
      "updatedAt": "2024-09-24T02:20:18.608Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "1a7e40e7-8bde-41f7-978d-4f4f6968728b",
      "number": 6,
      "content": "Vì sẽ có một Con Trẻ sinh cho chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.613Z",
      "updatedAt": "2024-09-24T02:20:18.613Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "a9f87058-c585-4886-9085-08e1b3777aff",
      "number": 6,
      "content": "Một Con Trai ban cho chúng ta;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.618Z",
      "updatedAt": "2024-09-24T02:20:18.618Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "efaa64b0-73ad-4f30-8eba-d277f18d8c14",
      "number": 6,
      "content": "Quyền trị vì sẽ đặt trên vai Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.623Z",
      "updatedAt": "2024-09-24T02:20:18.623Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "44527dbb-cea0-4e23-83d3-8c1f4fb5e873",
      "number": 6,
      "content": "Ngài sẽ được xưng là",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.628Z",
      "updatedAt": "2024-09-24T02:20:18.628Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "2d6ac414-c02b-4818-8dcd-59dab9dc4594",
      "number": 6,
      "content": "Ðấng Mưu Luận Lạ Lùng,",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.632Z",
      "updatedAt": "2024-09-24T02:20:18.632Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "0cce600e-db4f-4efc-a218-eab9d3631477",
      "number": 6,
      "content": "Ðức Chúa Trời Quyền Năng,",
      "order": 5,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.637Z",
      "updatedAt": "2024-09-24T02:20:18.637Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "b01c2e24-bf95-4c74-a1d3-945e522e75fe",
      "number": 6,
      "content": "Cha Ðời Ðời,",
      "order": 6,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.642Z",
      "updatedAt": "2024-09-24T02:20:18.642Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "824f6f4f-d2dd-407c-b247-d2eec62f42b2",
      "number": 6,
      "content": "Chúa Bình An.",
      "order": 7,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.647Z",
      "updatedAt": "2024-09-24T02:20:18.647Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "ea16662f-01bc-42f8-a820-73f8148fff36",
      "number": 7,
      "content": "Quyền trị vì của Ngài sẽ thêm mãi không ngừng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.652Z",
      "updatedAt": "2024-09-24T02:20:18.652Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "956b170e-bfbf-46d6-8c76-6fde143ae4f6",
      "number": 7,
      "content": "Và hòa bình sẽ kéo dài vô tận",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.657Z",
      "updatedAt": "2024-09-24T02:20:18.657Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "b8b8e6cc-3046-45a9-be9f-3206f74ef51e",
      "number": 7,
      "content": "trên ngai của Ða-vít và trong vương quốc của Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.662Z",
      "updatedAt": "2024-09-24T02:20:18.662Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "d82a2851-baea-4c8e-98be-2e361b45e1c5",
      "number": 7,
      "content": "Ngài sẽ thiết lập và duy trì quyền trị vì của Ngài bằng công lý và công chính,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.667Z",
      "updatedAt": "2024-09-24T02:20:18.667Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "21505cb2-8a08-4a07-966f-b88106aa6526",
      "number": 7,
      "content": "Từ nay cho đến đời đời.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.672Z",
      "updatedAt": "2024-09-24T02:20:18.672Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "124e9c4e-2b0f-4afd-bfe3-505347009ad1",
      "number": 7,
      "content": "Nguyện lòng nhiệt thành của Chúa các đạo quân sẽ thực hiện điều ấy.",
      "order": 5,
      "parNumber": 2,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.677Z",
      "updatedAt": "2024-09-24T02:20:18.677Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "3ffe3d51-b00a-4aee-bc12-e533661b7260",
      "number": 8,
      "content": "Chúa đã truyền một sứ điệp nghịch lại Gia-cốp;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.682Z",
      "updatedAt": "2024-09-24T02:20:18.682Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "4068f0b7-5857-4064-8b5d-9a5fe2d36bfc",
      "number": 8,
      "content": "Sứ điệp ấy bao gồm toàn thể I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.692Z",
      "updatedAt": "2024-09-24T02:20:18.692Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "b13cf474-5d29-40db-9914-8c9a26556690",
      "number": 9,
      "content": "Toàn dân sẽ biết đến,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.697Z",
      "updatedAt": "2024-09-24T02:20:18.697Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "2f1081a5-b27c-45f8-8032-c8860d06f7c1",
      "number": 9,
      "content": "Tức mọi người ở Ép-ra-im và dân chúng ở Sa-ma-ri.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.702Z",
      "updatedAt": "2024-09-24T02:20:18.702Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "cff1cf4c-487e-48a0-b243-703d05478a94",
      "number": 9,
      "content": "Với lòng đầy kiêu căng và ngạo mạn, họ nói rằng,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.707Z",
      "updatedAt": "2024-09-24T02:20:18.707Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "ad8e50e5-7a68-4424-8eca-46b44447b330",
      "number": 10,
      "content": "“Gạch đã đổ xuống, ta sẽ xây lại bằng đá khối;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.711Z",
      "updatedAt": "2024-09-24T02:20:18.711Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "49c39deb-2cb1-4f6c-be0a-36f7b75f95b2",
      "number": 10,
      "content": "Những cây sung đã bị đốn hạ, ta sẽ thay thế bằng những cây tùng.”",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.716Z",
      "updatedAt": "2024-09-24T02:20:18.716Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "d2ce12d5-260b-452e-a971-62c80019f083",
      "number": 11,
      "content": "Nhưng Chúa sẽ khiến những kẻ thù của Rê-xin chống lại họ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.721Z",
      "updatedAt": "2024-09-24T02:20:18.721Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "fb611e1c-733d-4887-b725-a479cd31daca",
      "number": 11,
      "content": "Ngài khích động quân thù của họ nổi lên.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.727Z",
      "updatedAt": "2024-09-24T02:20:18.727Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "e316a823-1ab5-4449-9c22-a74d726ffe67",
      "number": 12,
      "content": "Phía đông có A-ram, phía tây có Phi-li-tin;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.733Z",
      "updatedAt": "2024-09-24T02:20:18.733Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "346c5b7d-ee70-46a8-8b09-16c5399c3c90",
      "number": 12,
      "content": "Chúng hả mồm nuốt lấy I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.738Z",
      "updatedAt": "2024-09-24T02:20:18.738Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "f0ac1dd1-55a4-430b-aa34-61b4f4f99927",
      "number": 12,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.743Z",
      "updatedAt": "2024-09-24T02:20:18.743Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "50b951cd-c570-48b9-979e-89b48d1e099a",
      "number": 21,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 3,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.903Z",
      "updatedAt": "2024-09-24T02:20:18.903Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "edc691c8-b20f-4638-8099-feeb384a8897",
      "number": 17,
      "content": "Dù như vậy Ngài vẫn chưa nguôi giận;",
      "order": 4,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.812Z",
      "updatedAt": "2024-09-24T02:20:18.812Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "0db05a04-f24c-47e2-8a7b-6d2676f62bab",
      "number": 12,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.748Z",
      "updatedAt": "2024-09-24T02:20:18.748Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "b86aee29-80ab-4a19-be20-abe6fd44d664",
      "number": 21,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.908Z",
      "updatedAt": "2024-09-24T02:20:18.908Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "e758081b-f91e-44d2-829c-244dbf2d92d1",
      "number": 17,
      "content": "Tay Ngài còn tiếp tục đưa lên.",
      "order": 5,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.817Z",
      "updatedAt": "2024-09-24T02:20:18.817Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "5e78eeec-b3dd-4960-94a4-509b56a8e0d8",
      "number": 13,
      "content": "Thế mà con dân Ngài vẫn không quay về với Ðấng đánh phạt mình;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.753Z",
      "updatedAt": "2024-09-24T02:20:18.753Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "81f61dc2-d4ae-496e-a378-c9f9b488ec0b",
      "number": 13,
      "content": "Họ không tìm kiếm Chúa của các đạo quân.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.758Z",
      "updatedAt": "2024-09-24T02:20:18.758Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "aaeec3cd-eaf2-4931-a870-d8ab4472edeb",
      "number": 14,
      "content": "Vì vậy Chúa sẽ chặt đầu chặt đuôi của I-sơ-ra-ên;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.763Z",
      "updatedAt": "2024-09-24T02:20:18.763Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "dd11c72c-0f4d-4c3d-9d85-946ee4d20e0a",
      "number": 14,
      "content": "Trong một ngày, cả cành cây chà là và cây sậy đều bị chặt.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.768Z",
      "updatedAt": "2024-09-24T02:20:18.768Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "ed597015-818d-48b2-9179-ced2d15cc7c3",
      "number": 15,
      "content": "Các trưởng lão và các người quyền quý của họ là đầu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.773Z",
      "updatedAt": "2024-09-24T02:20:18.773Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "6ff16bef-8bfb-4820-8e8a-fd90e43ba172",
      "number": 15,
      "content": "Các tiên tri dạy dỗ những điều dối trá là đuôi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 16,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.778Z",
      "updatedAt": "2024-09-24T02:20:18.778Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "ee79eb51-8ce1-4ca5-805c-2530acb187a8",
      "number": 16,
      "content": "Những kẻ lãnh đạo dân này đã dẫn họ đi lạc hướng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 17,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.783Z",
      "updatedAt": "2024-09-24T02:20:18.783Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "a63e756c-e1a8-4155-8ab5-3a7e4a387e2f",
      "number": 16,
      "content": "Những kẻ dẫn dắt dân này đã đưa họ đến chỗ diệt vong.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 18,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.787Z",
      "updatedAt": "2024-09-24T02:20:18.787Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "1cd391d9-94a0-4316-baf5-6d446b248d2b",
      "number": 17,
      "content": "Vì thế Chúa chẳng thương tiếc các trai tráng của họ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 19,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.792Z",
      "updatedAt": "2024-09-24T02:20:18.792Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "a227b000-c44f-449e-9a9c-6d3f6f1a3d4e",
      "number": 17,
      "content": "Ngài cũng chẳng đoái hoài các cô nhi quả phụ của họ,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.798Z",
      "updatedAt": "2024-09-24T02:20:18.798Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "ccb559ae-0be9-4b29-854d-ff007f1146a4",
      "number": 17,
      "content": "Bởi ai nấy đều gian manh và tà ác;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 21,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.803Z",
      "updatedAt": "2024-09-24T02:20:18.803Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "1d28efae-2792-4f2d-98cd-f4525242a54f",
      "number": 17,
      "content": "Mọi miệng đều nói những lời rồ dại ngông cuồng.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.808Z",
      "updatedAt": "2024-09-24T02:20:18.808Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "c39000bb-b15a-4b48-acd0-ade697bbcdea",
      "number": 18,
      "content": "Sự gian ác bùng lên như lửa cháy;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.822Z",
      "updatedAt": "2024-09-24T02:20:18.822Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "e985d20f-ce4c-497c-9c70-82ca56b74d64",
      "number": 18,
      "content": "Nó đốt thiêu gai góc và bụi gai;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.827Z",
      "updatedAt": "2024-09-24T02:20:18.827Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "edfe2c18-14a7-41ca-8737-24a699b025a2",
      "number": 18,
      "content": "Nó thiêu sạch các bụi rậm trong rừng,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.833Z",
      "updatedAt": "2024-09-24T02:20:18.833Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "d440ae9f-1f8e-4047-b665-f62c62e0e328",
      "number": 18,
      "content": "Rồi bốc lên thành một luồng khói đen mù mịt.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.838Z",
      "updatedAt": "2024-09-24T02:20:18.838Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "5f920368-5d16-4bc3-9941-1fa419b502ea",
      "number": 19,
      "content": "Vì Chúa các đạo quân nổi cơn thịnh nộ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.843Z",
      "updatedAt": "2024-09-24T02:20:18.843Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "254911c4-f4cb-4772-b925-3ef8448ea2ad",
      "number": 19,
      "content": "Nên đất nước này bị lửa cháy ngập trời,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.848Z",
      "updatedAt": "2024-09-24T02:20:18.848Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "0289fb61-cce3-4a10-8a05-6250550b6383",
      "number": 19,
      "content": "Còn dân chúng trở thành mồi cho lửa hừng thiêu đốt,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.853Z",
      "updatedAt": "2024-09-24T02:20:18.853Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "da20cedd-410d-4246-8609-b8e22a126f2b",
      "number": 19,
      "content": "Ðến nỗi anh chị em cũng chẳng thương tiếc nhau.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.858Z",
      "updatedAt": "2024-09-24T02:20:18.858Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "74157cf4-36d5-4502-9d22-5db53da0a9df",
      "number": 20,
      "content": "Chúng chộp giật bên phải, nhưng bụng vẫn đói;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.868Z",
      "updatedAt": "2024-09-24T02:20:18.868Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "c6bc7507-e7ce-4560-bb30-0961bd75d423",
      "number": 20,
      "content": "Chúng ngoạm lấy bên trái, nhưng bụng vẫn không no;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.873Z",
      "updatedAt": "2024-09-24T02:20:18.873Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "b4f8d6fc-97b5-4940-bbbb-4454913450c0",
      "number": 20,
      "content": "Thậm chí chúng ăn thịt luôn cả người thân của mình!",
      "order": 2,
      "parNumber": 5,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.878Z",
      "updatedAt": "2024-09-24T02:20:18.878Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "f3813165-e5ab-4bbb-9705-b31f79639b05",
      "number": 21,
      "content": "Ma-na-se ăn nuốt Ép-ra-im;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.888Z",
      "updatedAt": "2024-09-24T02:20:18.888Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "42898092-9060-4fce-b2ab-42e7506421bb",
      "number": 21,
      "content": "Ép-ra-im ăn nuốt Ma-na-se;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.893Z",
      "updatedAt": "2024-09-24T02:20:18.893Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    },
    {
      "id": "eab0d557-adb0-44b5-baf4-b046d73a6bc2",
      "number": 21,
      "content": "Rồi cả hai cùng đánh Giu-đa.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:20:18.898Z",
      "updatedAt": "2024-09-24T02:20:18.898Z",
      "chapterId": "f98949d2-3bf3-4aad-8c13-f6757414e8d2"
    }
  ],
