{
  "chapter": {
    "id": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a",
    "number": 48,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2048&version=BD2011",
    "createdAt": "2024-09-24T01:00:47.299Z",
    "updatedAt": "2024-09-24T01:00:47.299Z",
    "bookId": "1e0ea116-0110-44af-8fdc-87130c3208f3"
  },
  "verses": [
    {
      "id": "b7efc08f-4c84-4764-9970-94a95d9e3798",
      "number": 1,
      "content": "Hãy nghe đây, hỡi nhà Gia-cốp;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.341Z",
      "updatedAt": "2024-09-24T02:24:25.341Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "113f3a65-b4f2-45c9-b76d-87e843cc36e8",
      "number": 1,
      "content": "Hỡi những kẻ được gọi bằng danh I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.352Z",
      "updatedAt": "2024-09-24T02:24:25.352Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "b3bed0e7-ebd6-45f5-94e3-882ee34cd6b9",
      "number": 1,
      "content": "Và những người ra từ nguồn cội của Giu-đa,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.355Z",
      "updatedAt": "2024-09-24T02:24:25.355Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "36cd0549-1024-4bb1-9a18-a0772774ce5a",
      "number": 1,
      "content": "Những người lấy danh Chúa mà thề,",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.357Z",
      "updatedAt": "2024-09-24T02:24:25.357Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "e206cee5-a98a-4102-a451-d86e78ab884f",
      "number": 1,
      "content": "Cầu khẩn Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 4,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.360Z",
      "updatedAt": "2024-09-24T02:24:25.360Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "1bebf6c4-286a-40dc-8689-51180076e066",
      "number": 1,
      "content": "Nhưng lại không thật lòng và ngay thẳng,",
      "order": 5,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.363Z",
      "updatedAt": "2024-09-24T02:24:25.363Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "da4aa8a5-e0a2-400a-b914-c91d98010b3a",
      "number": 2,
      "content": "Dù chúng cho mình là dân của thành thánh,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.366Z",
      "updatedAt": "2024-09-24T02:24:25.366Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "80e43379-f854-4cf2-b385-1b70006724f2",
      "number": 2,
      "content": "Và nương cậy vào Ðức Chúa Trời của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.368Z",
      "updatedAt": "2024-09-24T02:24:25.368Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "894c2c63-15ce-4d2d-8fa3-f91fcb7807c5",
      "number": 2,
      "content": "Tức Ðấng có danh là Chúa các đạo quân.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.371Z",
      "updatedAt": "2024-09-24T02:24:25.371Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "4b8a82a4-1c3e-4717-ac7f-57aa41c0b8c0",
      "number": 3,
      "content": "Những chuyện đã qua Ta đã báo trước tự ngàn xưa;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.373Z",
      "updatedAt": "2024-09-24T02:24:25.373Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "425bcedc-9f24-42be-9193-078a0372b439",
      "number": 3,
      "content": "Những điều ấy đã ra từ miệng Ta,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.375Z",
      "updatedAt": "2024-09-24T02:24:25.375Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "7538b400-fd2f-4953-9285-ed6d2921d96e",
      "number": 3,
      "content": "Và Ta làm cho người ta biết chúng,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.377Z",
      "updatedAt": "2024-09-24T02:24:25.377Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "83b732c7-a28f-4dad-bd8f-5763574dd965",
      "number": 3,
      "content": "Rồi thình lình Ta thực hiện những điều ấy,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.379Z",
      "updatedAt": "2024-09-24T02:24:25.379Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "58ae1422-15f9-4bae-b557-a31d511e7fb7",
      "number": 3,
      "content": "Và chúng đã xảy ra.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.382Z",
      "updatedAt": "2024-09-24T02:24:25.382Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "148e4b66-310e-48b0-8d17-625ca1d163fe",
      "number": 4,
      "content": "Vì Ta biết ngươi là kẻ cứng đầu,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.385Z",
      "updatedAt": "2024-09-24T02:24:25.385Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "33c4fe66-7029-4ccc-9031-f2fbc1e590c6",
      "number": 4,
      "content": "Gân cổ ngươi cứng như sắt,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.390Z",
      "updatedAt": "2024-09-24T02:24:25.390Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "436977a8-c4c7-4030-9492-727a1246c7f4",
      "number": 4,
      "content": "Trán của ngươi cứng như đồng,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.395Z",
      "updatedAt": "2024-09-24T02:24:25.395Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "dc156243-ca1c-4130-8645-81d4f0756313",
      "number": 5,
      "content": "Nên ngay từ ban đầu Ta đã nói cho ngươi những điều ấy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.401Z",
      "updatedAt": "2024-09-24T02:24:25.401Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "04f5a5ab-c90b-4957-901d-97d546f99baf",
      "number": 5,
      "content": "Trước khi chúng xảy ra, Ta đã nói về chúng cho ngươi hay,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.406Z",
      "updatedAt": "2024-09-24T02:24:25.406Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "e9bc36b2-c8c9-4f88-8fd7-202173e517a5",
      "number": 5,
      "content": "Ðể ngươi không thể nói,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.411Z",
      "updatedAt": "2024-09-24T02:24:25.411Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "e0d0e2aa-6615-411c-9f7b-7d20cc9d8ef3",
      "number": 7,
      "content": "Ðể ngươi không thể nói,",
      "order": 3,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.465Z",
      "updatedAt": "2024-09-24T02:24:25.465Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "cd2fafd5-62ec-4ed0-ab1d-a4522dbd468d",
      "number": 5,
      "content": "“Ấy là thần tượng của tôi đã làm những điều ấy;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.416Z",
      "updatedAt": "2024-09-24T02:24:25.416Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "ff73f921-4e39-4de3-9433-da82a4465662",
      "number": 5,
      "content": "Ðó là tượng chạm của tôi và tượng đúc của tôi đã ra lịnh cho chúng phải xảy ra.”",
      "order": 4,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.421Z",
      "updatedAt": "2024-09-24T02:24:25.421Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "9be9de78-6c89-4f84-acb8-88ee400e701e",
      "number": 6,
      "content": "Ngươi đã nghe rồi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.426Z",
      "updatedAt": "2024-09-24T02:24:25.426Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "1f0fa93f-0da1-45da-beab-969c38968f9e",
      "number": 6,
      "content": "Ngươi đã thấy tất cả rồi;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.430Z",
      "updatedAt": "2024-09-24T02:24:25.430Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "62e075f1-10a8-4887-ab9c-6eea5ffb1a8f",
      "number": 6,
      "content": "Sao ngươi không tuyên bố ra đi?",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.436Z",
      "updatedAt": "2024-09-24T02:24:25.436Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "9eb17c59-cf00-4264-936d-def9ae05a203",
      "number": 6,
      "content": "Từ nay Ta sẽ cho ngươi nghe những điều mới,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.440Z",
      "updatedAt": "2024-09-24T02:24:25.440Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "b4569deb-8354-4af3-ae9b-7862d8a5a655",
      "number": 6,
      "content": "Ngay cả những điều còn giữ kín mà ngươi chưa hề biết.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.445Z",
      "updatedAt": "2024-09-24T02:24:25.445Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "66ce5474-0a26-4ca6-8a1a-ed5637cab898",
      "number": 7,
      "content": "Ðến bây giờ những điều ấy mới được tạo nên,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.450Z",
      "updatedAt": "2024-09-24T02:24:25.450Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "0fe4cb72-ceac-425d-8e0c-1949b910c9dd",
      "number": 7,
      "content": "Chứ chẳng phải chúng đã có sẵn từ lâu rồi đâu;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.455Z",
      "updatedAt": "2024-09-24T02:24:25.455Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "9012dd17-f94b-4568-af4b-0cd629e02085",
      "number": 7,
      "content": "Trước ngày này ngươi chưa bao giờ nghe về chúng,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.461Z",
      "updatedAt": "2024-09-24T02:24:25.461Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "5dced632-2f37-408f-81f3-a33741194016",
      "number": 7,
      "content": "“Ồ tưởng gì chứ những điều đó tôi đã biết cả rồi.”",
      "order": 4,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.470Z",
      "updatedAt": "2024-09-24T02:24:25.470Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "278dd5c9-0093-4766-b01d-495353a0e730",
      "number": 8,
      "content": "Chắc chắn ngươi chưa nghe đến;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.475Z",
      "updatedAt": "2024-09-24T02:24:25.475Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "f90acc86-0950-467b-88fd-3e4351673ba8",
      "number": 8,
      "content": "Chắc chắn ngươi chưa biết đến;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.480Z",
      "updatedAt": "2024-09-24T02:24:25.480Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "f36f7602-1b6d-4677-bb48-e8e943950209",
      "number": 8,
      "content": "Chắc chắn từ lâu tai ngươi chưa mở ra,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.485Z",
      "updatedAt": "2024-09-24T02:24:25.485Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "ef1dbc18-493c-4e3a-ad03-6b50a9cc57aa",
      "number": 8,
      "content": "Vì Ta biết ngươi là kẻ phản bội,",
      "order": 3,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.490Z",
      "updatedAt": "2024-09-24T02:24:25.490Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "12f8b038-9095-4eb6-a6e2-6f429f6ff877",
      "number": 8,
      "content": "Ngươi mang tên là kẻ phản phúc từ lúc ngươi còn là một thai nhi.",
      "order": 4,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.497Z",
      "updatedAt": "2024-09-24T02:24:25.497Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "b4d346c9-192a-4ae0-aba1-ce48f624c8b1",
      "number": 9,
      "content": "Bấy lâu nay, vì danh Ta, Ta cam nén giận,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.502Z",
      "updatedAt": "2024-09-24T02:24:25.502Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "3fa7a596-f9ff-4c7a-9e1f-bb85b2b68446",
      "number": 9,
      "content": "Vì sự ca ngợi dành cho Ta, Ta dằn lòng chịu đựng ngươi,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.507Z",
      "updatedAt": "2024-09-24T02:24:25.507Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "6f528ee5-089d-4ddb-a29a-a05a5d1c330e",
      "number": 9,
      "content": "Mà không tiêu diệt ngươi.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.511Z",
      "updatedAt": "2024-09-24T02:24:25.511Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "6c58e513-dd4a-4df0-b704-469a3f425a26",
      "number": 10,
      "content": "Này, Ta đã tinh luyện ngươi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.516Z",
      "updatedAt": "2024-09-24T02:24:25.516Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "a0c8a3c7-b3c5-4659-915d-d7a7535ed8c5",
      "number": 10,
      "content": "Nhưng không như người ta luyện bạc;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.521Z",
      "updatedAt": "2024-09-24T02:24:25.521Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "f537419c-f8cf-4114-a799-593962af52ec",
      "number": 10,
      "content": "Ta đã rèn luyện ngươi trong lò lửa khổ đau.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.526Z",
      "updatedAt": "2024-09-24T02:24:25.526Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "72e8cdda-bc0a-44b5-8188-8b6d2168e6ac",
      "number": 11,
      "content": "Nhưng nay vì cớ Ta, vì chính Ta, Ta sẽ ra tay hành động;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.531Z",
      "updatedAt": "2024-09-24T02:24:25.531Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "99409373-e982-4802-ac7b-c36c2c4a20a6",
      "number": 11,
      "content": "Chứ lẽ nào Ta cứ để cho danh Ta bị sỉ nhục hay sao?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.536Z",
      "updatedAt": "2024-09-24T02:24:25.536Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "0db10779-7709-4a5c-985f-ea810070b579",
      "number": 11,
      "content": "Ta sẽ không nhường vinh hiển Ta cho một kẻ nào khác.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.541Z",
      "updatedAt": "2024-09-24T02:24:25.541Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "15ae12af-8fad-4469-bd60-f59ffac57cd0",
      "number": 12,
      "content": "Hỡi Gia-cốp, hãy lắng nghe Ta;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.546Z",
      "updatedAt": "2024-09-24T02:24:25.546Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "cd9e0cae-9429-4460-9c90-0d87757ff71c",
      "number": 12,
      "content": "Hỡi I-sơ-ra-ên, người Ta đã gọi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.551Z",
      "updatedAt": "2024-09-24T02:24:25.551Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "c4ac954f-5d5a-4ce0-a8a7-c3c6f31202d0",
      "number": 12,
      "content": "Ta là Ðấng ấy,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.555Z",
      "updatedAt": "2024-09-24T02:24:25.555Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "e52fdd5c-04a7-4a40-8131-dbaadf183bb6",
      "number": 12,
      "content": "Ta là Ðấng Ðầu Tiên, Ta cũng là Ðấng Sau Cùng.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.560Z",
      "updatedAt": "2024-09-24T02:24:25.560Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "0eaf2c5c-8d14-4e4d-8d97-6963f28c5a9e",
      "number": 13,
      "content": "Chính tay Ta đã đặt nền trái đất;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.565Z",
      "updatedAt": "2024-09-24T02:24:25.565Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "df1470ff-b6c1-4b57-8af8-116e7dd5c718",
      "number": 13,
      "content": "Tay phải Ta đã trải rộng bầu trời;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.570Z",
      "updatedAt": "2024-09-24T02:24:25.570Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "da67f8ca-5f96-4971-a0cf-45bec8bced30",
      "number": 13,
      "content": "Khi Ta gọi chúng, chúng cùng nhau đứng dậy.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.576Z",
      "updatedAt": "2024-09-24T02:24:25.576Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "b12f87de-4bcd-4d4f-b8c6-2205ec2d9d29",
      "number": 14,
      "content": "Hỡi tất cả các ngươi, hãy họp lại và lắng nghe!",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.581Z",
      "updatedAt": "2024-09-24T02:24:25.581Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "e565afdb-32a4-49bd-b542-f3234c4f70f3",
      "number": 14,
      "content": "Ai trong vòng chúng đã công bố những điều ấy?",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.586Z",
      "updatedAt": "2024-09-24T02:24:25.586Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "1dd31f87-a04a-41b3-9ff6-1703c55ad788",
      "number": 14,
      "content": "Chúa yêu thương nó;",
      "order": 2,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.590Z",
      "updatedAt": "2024-09-24T02:24:25.590Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "a73588f0-425c-4007-b8ba-159dc7a737a2",
      "number": 14,
      "content": "Ngài sẽ thực hiện điều đẹp ý Ngài đối với Ba-by-lôn;",
      "order": 3,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.595Z",
      "updatedAt": "2024-09-24T02:24:25.595Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "7f14fe16-4304-4ee3-8028-df62c6f8308f",
      "number": 14,
      "content": "Ngài sẽ đưa tay ra nghịch lại người Canh-đê.",
      "order": 4,
      "parNumber": 6,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.600Z",
      "updatedAt": "2024-09-24T02:24:25.600Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "0d2167b3-9125-4492-8c6b-5a3af0c5d42b",
      "number": 15,
      "content": "Ta, chính Ta, đã phán;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.605Z",
      "updatedAt": "2024-09-24T02:24:25.605Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "7ea6fbd8-cfde-4106-a632-93d28337c318",
      "number": 15,
      "content": "Phải, Ta đã gọi nó;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.610Z",
      "updatedAt": "2024-09-24T02:24:25.610Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "ef9aedd5-79f2-4c85-af2b-9215dede6ab6",
      "number": 15,
      "content": "Ta đã dẫn dắt nó,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.615Z",
      "updatedAt": "2024-09-24T02:24:25.615Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "051fe643-60ce-403f-a37d-b53985ac3d31",
      "number": 15,
      "content": "Nên đường lối nó sẽ thành công thịnh vượng.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.620Z",
      "updatedAt": "2024-09-24T02:24:25.620Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "57673a2a-f5ac-43b8-8de6-74ad81fa2e7c",
      "number": 16,
      "content": "Hãy đến gần Ta và nghe rõ điều này:",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.625Z",
      "updatedAt": "2024-09-24T02:24:25.625Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "efcccf4d-e3d2-466e-8774-b5925ef11c3b",
      "number": 16,
      "content": "Từ ban đầu, Ta không nói điều gì bí ẩn;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.630Z",
      "updatedAt": "2024-09-24T02:24:25.630Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "d0bb41c9-a22b-41f9-bd0f-63c97eb1340f",
      "number": 16,
      "content": "Lúc nó xảy ra, Ta đã có ở đó rồi.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.635Z",
      "updatedAt": "2024-09-24T02:24:25.635Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "a9e01adb-9b9b-4913-91d8-3422d186f7ea",
      "number": 16,
      "content": "Và bây giờ Chúa Hằng Hữu và Thần của Ngài đã sai tôi.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.639Z",
      "updatedAt": "2024-09-24T02:24:25.639Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "731ee16c-18ee-4f4e-a087-20e70f1a69d9",
      "number": 17,
      "content": "Chúa, Ðấng Cứu Chuộc ngươi, Ðấng Thánh của I-sơ-ra-ên, phán thế này:",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.644Z",
      "updatedAt": "2024-09-24T02:24:25.644Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "a233515c-68ab-4a49-9c9f-bf5c8391cb91",
      "number": 17,
      "content": "Ta là Chúa, Ðức Chúa Trời của ngươi,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.650Z",
      "updatedAt": "2024-09-24T02:24:25.650Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "1f13c600-f201-43eb-b651-0012dc901635",
      "number": 17,
      "content": "Ðấng dạy bảo ngươi để ngươi được phước,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.655Z",
      "updatedAt": "2024-09-24T02:24:25.655Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "2871b2c7-7319-43a7-b075-97a0a2a0e4dc",
      "number": 17,
      "content": "Ðấng dẫn dắt ngươi vào đường lối ngươi nên đi.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.661Z",
      "updatedAt": "2024-09-24T02:24:25.661Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "60d61975-fc4e-47a5-b8e5-52750f72362d",
      "number": 18,
      "content": "Ôi, ước chi ngươi chú ý đến các điều răn Ta,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.666Z",
      "updatedAt": "2024-09-24T02:24:25.666Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "1d0b9e06-006e-447c-ba3f-90aedcdec58d",
      "number": 18,
      "content": "Thì hòa bình thịnh vượng của ngươi sẽ còn mãi như dòng sông vô tận,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.671Z",
      "updatedAt": "2024-09-24T02:24:25.671Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "4e8d3753-8dd3-4b83-917a-e01ce9ddd5ca",
      "number": 18,
      "content": "Sự thành công của ngươi sẽ không ngừng như sóng ở đại dương,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.676Z",
      "updatedAt": "2024-09-24T02:24:25.676Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "6cff42ca-c5cd-41e5-80dd-e5653ee050d7",
      "number": 19,
      "content": "Con cháu của ngươi sẽ đông như cát nơi bờ biển,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.681Z",
      "updatedAt": "2024-09-24T02:24:25.681Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "d37cbb3f-0d16-4476-9bc5-cac58eabedc6",
      "number": 19,
      "content": "Dòng dõi của ngươi sẽ nhiều tựa cát của đại dương,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.686Z",
      "updatedAt": "2024-09-24T02:24:25.686Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "a1143d11-bb1a-44ff-83f6-837ff05a4230",
      "number": 19,
      "content": "Danh của ngươi sẽ không bao giờ bị xóa bỏ,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.691Z",
      "updatedAt": "2024-09-24T02:24:25.691Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "ed4d3c6b-e612-4c49-b78e-ed624408e5f4",
      "number": 19,
      "content": "Và chẳng bao giờ bị diệt mất trước mặt Ta.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.696Z",
      "updatedAt": "2024-09-24T02:24:25.696Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "bad759c0-8ebf-4b60-bd50-4e7836bdf5cf",
      "number": 20,
      "content": "Hãy rời khỏi Ba-by-lôn,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.701Z",
      "updatedAt": "2024-09-24T02:24:25.701Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "e1ff6743-d2c3-41bc-bbc8-bc58b245467d",
      "number": 20,
      "content": "Hãy trốn khỏi dân Canh-đê,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.706Z",
      "updatedAt": "2024-09-24T02:24:25.706Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "e1abf024-143f-4d5a-a08a-a50a03ee98e3",
      "number": 20,
      "content": "Hãy cất tiếng hát ca,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.711Z",
      "updatedAt": "2024-09-24T02:24:25.711Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "966c4aad-6d3f-4698-bcc4-7389fc2e55da",
      "number": 20,
      "content": "Hãy tuyên bố,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.716Z",
      "updatedAt": "2024-09-24T02:24:25.716Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "11cb3a3f-13ce-4709-8839-9852325126a2",
      "number": 20,
      "content": "Hãy công bố điều ấy ra,",
      "order": 4,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.720Z",
      "updatedAt": "2024-09-24T02:24:25.720Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "d4422f05-b0d4-442a-a6c9-c8568f950990",
      "number": 20,
      "content": "Hãy truyền tin ấy đến tận cùng trái đất,",
      "order": 5,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.725Z",
      "updatedAt": "2024-09-24T02:24:25.725Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "599b17e8-59ad-408f-b3a9-71b3302ad1ed",
      "number": 20,
      "content": "Bảo rằng, “Chúa cứu chuộc Gia-cốp đầy tớ Ngài.”",
      "order": 6,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.730Z",
      "updatedAt": "2024-09-24T02:24:25.730Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "edcdccf0-542e-427b-937a-415380e90fcb",
      "number": 21,
      "content": "Dù Ngài dẫn họ đi qua sa mạc,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.736Z",
      "updatedAt": "2024-09-24T02:24:25.736Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "213a2d28-f2ee-4e8a-9659-8244ad26f57e",
      "number": 21,
      "content": "Họ cũng không khát nước,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.743Z",
      "updatedAt": "2024-09-24T02:24:25.743Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "3a08a148-6c3b-4eab-a47f-46d2c844e154",
      "number": 21,
      "content": "Vì Ngài khiến nước từ vầng đá chảy ra cho họ uống.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.748Z",
      "updatedAt": "2024-09-24T02:24:25.748Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "0d976ca9-6c7e-4654-a97b-1b4bf72b86fe",
      "number": 21,
      "content": "Ngài chẻ vầng đá, và nước đã trào ra.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.753Z",
      "updatedAt": "2024-09-24T02:24:25.753Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "bb7255c6-649d-44c1-9668-e102fdd662a3",
      "number": 22,
      "content": "Tuy nhiên phường gian tà sẽ không có bình an,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.758Z",
      "updatedAt": "2024-09-24T02:24:25.758Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    },
    {
      "id": "561b825a-b2c3-4cd7-8795-555bbca8eac3",
      "number": 22,
      "content": "Chúa phán.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:25.762Z",
      "updatedAt": "2024-09-24T02:24:25.762Z",
      "chapterId": "2f94b17f-d35f-43f1-979e-0a4a65e0fd8a"
    }
  ],
