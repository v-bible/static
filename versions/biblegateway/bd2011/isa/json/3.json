    },
    {
      "id": "3bf3be1d-c313-4b49-9bfa-ff0739c2df97",
      "content": "Trong Bản Qumran có chữ \"xấu hổ\"",
      "position": 30,
      "order": 4,
      "createdAt": "2024-09-24T02:19:44.702Z",
      "updatedAt": "2024-09-24T02:19:44.702Z",
