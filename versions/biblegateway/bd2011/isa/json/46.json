{
  "chapter": {
    "id": "5b0816ba-5775-4b92-88af-16874584b9b8",
    "number": 46,
    "ref": "https://www.biblegateway.com/passage/?search=%C3%8A-sai%2046&version=BD2011",
    "createdAt": "2024-09-24T01:00:47.263Z",
    "updatedAt": "2024-09-24T01:00:47.263Z",
    "bookId": "1e0ea116-0110-44af-8fdc-87130c3208f3"
  },
  "verses": [
    {
      "id": "507bec97-64a6-4602-97e7-6831867df321",
      "number": 1,
      "content": "Thần Bên cúi xuống, Thần Nê-bô rạp mình;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.530Z",
      "updatedAt": "2024-09-24T02:24:14.530Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "9be6684f-564c-49ff-be75-71fe151ad282",
      "number": 1,
      "content": "Các thần tượng của chúng được chở đi bằng các xe do thú vật và trâu bò kéo;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.546Z",
      "updatedAt": "2024-09-24T02:24:14.546Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "5666c42b-12e3-4230-af91-ce4160cd7cfa",
      "number": 1,
      "content": "Các thần tượng các ngươi mang theo đã trở thành gánh nặng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.550Z",
      "updatedAt": "2024-09-24T02:24:14.550Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "50ec7170-ddc0-4c60-9be7-6271f2c97467",
      "number": 1,
      "content": "Một trọng tải nặng nề khiến các thú vật kéo xe mệt nhọc.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.555Z",
      "updatedAt": "2024-09-24T02:24:14.555Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "1380eb9c-bcc0-43d6-9db2-14efbf72423d",
      "number": 2,
      "content": "Các thần tượng ấy cúi rạp xuống;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.560Z",
      "updatedAt": "2024-09-24T02:24:14.560Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "301f28a4-1dd5-4196-adf3-c35370772a36",
      "number": 2,
      "content": "Chúng thảy đều gục xuống;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.565Z",
      "updatedAt": "2024-09-24T02:24:14.565Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "6914e5f4-8cc0-4aaa-8fe6-bd9308cce7be",
      "number": 2,
      "content": "Chúng chẳng cứu nổi các súc vật đang kéo nặng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.570Z",
      "updatedAt": "2024-09-24T02:24:14.570Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "cb9439a6-855b-43c2-afe6-1455ddc7b6ca",
      "number": 2,
      "content": "Vì chính bản thân chúng cũng bị bắt đem đi lưu đày.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.575Z",
      "updatedAt": "2024-09-24T02:24:14.575Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "62de656b-97e7-44ef-8874-2e31deab815a",
      "number": 3,
      "content": "Hỡi nhà Gia-cốp,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.580Z",
      "updatedAt": "2024-09-24T02:24:14.580Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "55dfa054-3ee2-4eb7-b9f6-12d00cf62d29",
      "number": 3,
      "content": "Hỡi những kẻ còn sót lại của nhà I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.585Z",
      "updatedAt": "2024-09-24T02:24:14.585Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "f0c355e4-d798-4c12-8bdf-814994a8dfa5",
      "number": 3,
      "content": "Hãy lắng nghe Ta:",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.589Z",
      "updatedAt": "2024-09-24T02:24:14.589Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "1d347107-969a-4b2b-aaa4-c012f3dc1d23",
      "number": 3,
      "content": "Ta đã bồng ẵm các ngươi từ lúc các ngươi mới chào đời;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.593Z",
      "updatedAt": "2024-09-24T02:24:14.593Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "2585fe31-e7ac-49ff-a1c6-062706549b44",
      "number": 3,
      "content": "Ta đã cưu mang các ngươi từ khi các ngươi còn trong lòng mẹ,",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.596Z",
      "updatedAt": "2024-09-24T02:24:14.596Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "e65646e7-4431-41eb-9821-801a4ad566d0",
      "number": 4,
      "content": "Cho đến khi các ngươi già nua cao tuổi, Ta vẫn là Ðấng ấy;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.599Z",
      "updatedAt": "2024-09-24T02:24:14.599Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "1c57f684-727f-454d-8d55-4721e2a859c3",
      "number": 4,
      "content": "Dầu cho các ngươi đã ra da mồi tóc bạc, Ta sẽ còn bồng ẵm các ngươi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.602Z",
      "updatedAt": "2024-09-24T02:24:14.602Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "ec579115-76a0-465d-a023-85e95a11bf70",
      "number": 4,
      "content": "Ta đã tạo ra, nên Ta sẽ bồng ẵm;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.605Z",
      "updatedAt": "2024-09-24T02:24:14.605Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "f1e8ab17-020a-4caf-8f3c-8fcf0919be45",
      "number": 4,
      "content": "Ta sẽ cưu mang và sẽ giải cứu các ngươi.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.608Z",
      "updatedAt": "2024-09-24T02:24:14.608Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "32456385-d44e-4e7f-9832-fd48f24036d6",
      "number": 5,
      "content": "Các ngươi sẽ ví sánh ai với Ta và cho ai được ngang hàng với Ta?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.610Z",
      "updatedAt": "2024-09-24T02:24:14.610Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "209de260-55a2-4f89-8ea4-cf0ce5e9861b",
      "number": 5,
      "content": "Các ngươi so sánh ai với Ta như thể kẻ đó và Ta có điều gì tương xứng?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.612Z",
      "updatedAt": "2024-09-24T02:24:14.612Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "ec8d077d-4d73-4401-b7d5-bb5fbe62e7cf",
      "number": 6,
      "content": "Chúng lấy vàng trong túi ra;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.615Z",
      "updatedAt": "2024-09-24T02:24:14.615Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "b618d9eb-719c-421c-8066-46d446fdcedd",
      "number": 6,
      "content": "Chúng lấy bạc đem cân trên cân;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.617Z",
      "updatedAt": "2024-09-24T02:24:14.617Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "11dee669-21b0-4e07-a58c-4ad6ef270082",
      "number": 6,
      "content": "Chúng mướn một người thợ bạc;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.619Z",
      "updatedAt": "2024-09-24T02:24:14.619Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "3fade338-5bbc-4f52-9319-f4896bb8432d",
      "number": 6,
      "content": "Chúng nhờ hắn lấy vàng bạc đó làm nên một tượng thần,",
      "order": 3,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.623Z",
      "updatedAt": "2024-09-24T02:24:14.623Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "82633725-b846-4ea8-be6b-dff659b203c0",
      "number": 6,
      "content": "Rồi chúng sụp xuống thờ lạy tượng thần đó.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.625Z",
      "updatedAt": "2024-09-24T02:24:14.625Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "9724969e-73c5-4bd6-8499-380a2e0c4c72",
      "number": 7,
      "content": "Chúng nhấc nó lên vai và cùng nhau khiêng nó đi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.627Z",
      "updatedAt": "2024-09-24T02:24:14.627Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "114e479f-3dac-488c-8e17-4b7c4f2ed111",
      "number": 7,
      "content": "Chúng đặt nó vào một chỗ, và nó cứ đứng yên tại đó;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.629Z",
      "updatedAt": "2024-09-24T02:24:14.629Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "412daaa4-38a6-4e18-894e-50b10dff0273",
      "number": 7,
      "content": "Nó không thể di chuyển khỏi chỗ đứng của nó.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.631Z",
      "updatedAt": "2024-09-24T02:24:14.631Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "b60f7742-41a3-4b09-9199-bae52d0aeca9",
      "number": 7,
      "content": "Nếu ai kêu cầu nó, nó chẳng thể trả lời;",
      "order": 3,
      "parNumber": 2,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.633Z",
      "updatedAt": "2024-09-24T02:24:14.633Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "baa4bff1-af2e-421d-bec0-6504ed5dc54d",
      "number": 7,
      "content": "Nó không thể cứu ai khỏi cơn hoạn nạn.",
      "order": 4,
      "parNumber": 2,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.635Z",
      "updatedAt": "2024-09-24T02:24:14.635Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "054044ac-2427-4e5c-9832-c8ccceb7da35",
      "number": 8,
      "content": "Hãy ghi nhớ điều ấy mà sống xứng đáng làm người,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.637Z",
      "updatedAt": "2024-09-24T02:24:14.637Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "91f71d74-eb05-4a69-ac4a-519e282cbe45",
      "number": 8,
      "content": "Hỡi các ngươi, những kẻ hay phạm tội, hãy ghi nhớ trong lòng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.639Z",
      "updatedAt": "2024-09-24T02:24:14.639Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "137a48ba-b29b-4d2e-b5df-650144726ae9",
      "number": 9,
      "content": "Hãy nhớ lại những chuyện xưa kia,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.641Z",
      "updatedAt": "2024-09-24T02:24:14.641Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "2f14e704-67e6-4f1c-886a-ad1ed6663fc3",
      "number": 9,
      "content": "Vì Ta là Ðức Chúa Trời, không có thần nào khác;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.643Z",
      "updatedAt": "2024-09-24T02:24:14.643Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "7485fb85-6018-42c8-9722-f60d45fc2f11",
      "number": 9,
      "content": "Ta là Ðức Chúa Trời, không có thần nào khác như Ta.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.645Z",
      "updatedAt": "2024-09-24T02:24:14.645Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "8568fe8f-ab1d-4a80-bf1a-5205183b93e5",
      "number": 10,
      "content": "Từ ban đầu Ta đã cho biết cuối cùng sẽ ra thể nào;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.647Z",
      "updatedAt": "2024-09-24T02:24:14.647Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "27f0c20c-0da1-4c68-bb13-569b1f461e1a",
      "number": 10,
      "content": "Từ ngàn xưa Ta đã báo trước việc sẽ xảy đến trong tương lai;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.649Z",
      "updatedAt": "2024-09-24T02:24:14.649Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "460450bc-29ab-4660-a705-2e9a7f973cab",
      "number": 10,
      "content": "Ta phán: Những gì Ta dự định Ta sẽ làm cho thành tựu;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.651Z",
      "updatedAt": "2024-09-24T02:24:14.651Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "ba050754-f3cd-4796-a3f0-576a9ce8e712",
      "number": 10,
      "content": "Mọi điều lòng Ta vui thích Ta sẽ thực hiện cho xong.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.653Z",
      "updatedAt": "2024-09-24T02:24:14.653Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "1c9dcea9-2975-4cc0-ab3b-0aa6b982ac39",
      "number": 11,
      "content": "Ta sẽ gọi một chim săn mồi từ phương đông bay đến,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.655Z",
      "updatedAt": "2024-09-24T02:24:14.655Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "356c5516-ddf0-4dd0-aee7-567f93d7d297",
      "number": 11,
      "content": "Một người thi hành ý chỉ Ta sẽ đến từ một xứ xa xăm;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.657Z",
      "updatedAt": "2024-09-24T02:24:14.657Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "bfe54f1f-0c12-4e95-93ef-845f3174bb6d",
      "number": 11,
      "content": "Thật vậy, điều Ta nói, Ta sẽ làm;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.659Z",
      "updatedAt": "2024-09-24T02:24:14.659Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "0bb079db-d673-4fd6-97f6-0e058aa4d2d1",
      "number": 11,
      "content": "Ðiều Ta định, Ta sẽ thực hiện.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.661Z",
      "updatedAt": "2024-09-24T02:24:14.661Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "e0900715-4600-49cf-bd24-5bf0930f43ec",
      "number": 12,
      "content": "Hãy lắng nghe Ta, hỡi những kẻ có lòng bướng bỉnh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.666Z",
      "updatedAt": "2024-09-24T02:24:14.666Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "2cea1e41-9dbc-4a0e-901a-d966bdba5e65",
      "number": 12,
      "content": "Những kẻ đang ở xa lẽ công chính:",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.672Z",
      "updatedAt": "2024-09-24T02:24:14.672Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "0986d301-08e6-4744-863a-3ddc7d36f3fa",
      "number": 13,
      "content": "Ngày Ta thực hiện lẽ công chính của Ta đã gần;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.677Z",
      "updatedAt": "2024-09-24T02:24:14.677Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "1c872a4b-ce4d-4fe9-839e-7c2724d5dff1",
      "number": 13,
      "content": "Chẳng còn bao lâu nữa đâu;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.682Z",
      "updatedAt": "2024-09-24T02:24:14.682Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "0ee80e2c-7c67-4f0a-a0ce-6093e6347d9e",
      "number": 13,
      "content": "Ơn cứu rỗi của Ta sẽ không đình trệ;",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.687Z",
      "updatedAt": "2024-09-24T02:24:14.687Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "2ea0bc0b-f619-41a8-94c1-b8992de66f61",
      "number": 13,
      "content": "Ta sẽ ban ơn cứu rỗi cho Si-ôn,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.691Z",
      "updatedAt": "2024-09-24T02:24:14.691Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    },
    {
      "id": "a0eb6a2a-8bd8-4afb-ab2a-e6e60b3169e5",
      "number": 13,
      "content": "Vì cớ I-sơ-ra-ên, vinh hiển Ta.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:24:14.696Z",
      "updatedAt": "2024-09-24T02:24:14.696Z",
      "chapterId": "5b0816ba-5775-4b92-88af-16874584b9b8"
    }
  ],
