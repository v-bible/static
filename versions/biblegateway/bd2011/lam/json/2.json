{
  "chapter": {
    "id": "d59b83bf-fa92-4a77-a916-c6f098a00a08",
    "number": 2,
    "ref": "https://www.biblegateway.com/passage/?search=Ai%20Ca%202&version=BD2011",
    "createdAt": "2024-09-24T01:00:48.697Z",
    "updatedAt": "2024-09-24T01:00:48.697Z",
    "bookId": "8f8552cf-33c5-4092-92d0-c47be22f31ba"
  },
  "verses": [
    {
      "id": "95b9bac8-8db9-445c-b7d0-6a582bfa3af4",
      "number": 1,
      "content": "Sao Chúa nỡ bao phủ Ái Nữ của Si-ôn bằng cơn giận?",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.219Z",
      "updatedAt": "2024-09-24T02:31:41.219Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "e12cb1b0-1321-4e96-93d7-d05436c9c1e2",
      "number": 1,
      "content": "Từ trời cao Ngài ném xuống đất vẻ đẹp của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.224Z",
      "updatedAt": "2024-09-24T02:31:41.224Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "42b08d41-953e-4cc1-beb8-8fe260cc527d",
      "number": 1,
      "content": "Và không nhớ đến bệ chân Ngài trong ngày Ngài nổi giận.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.227Z",
      "updatedAt": "2024-09-24T02:31:41.227Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "3f9451f2-cb89-4349-9e07-62e729a82e1f",
      "number": 2,
      "content": "Chúa tiêu diệt không chút xót thương mọi nơi cư ngụ của Gia-cốp;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.230Z",
      "updatedAt": "2024-09-24T02:31:41.230Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "fb776fed-2291-4ed0-97f4-3d4d764de097",
      "number": 2,
      "content": "Trong cơn giận Ngài phá tan các thành lũy Ái Nữ của Giu-đa;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.232Z",
      "updatedAt": "2024-09-24T02:31:41.232Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "c32f45fc-8c1f-4196-ac48-162f421f7f62",
      "number": 2,
      "content": "Ngài khiến chúng đổ xuống đất;",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.234Z",
      "updatedAt": "2024-09-24T02:31:41.234Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "91f4e3a0-3a6c-4823-bb5a-f8c0ae4935cb",
      "number": 2,
      "content": "Ðể làm nhục vương quốc và những người lãnh đạo của nó.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.236Z",
      "updatedAt": "2024-09-24T02:31:41.236Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "260ce6d3-0661-418a-b9d7-3413d36185c2",
      "number": 3,
      "content": "Trong cơn cực kỳ phẫn nộ Ngài chặt đứt tất cả các sừng của I-sơ-ra-ên;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.239Z",
      "updatedAt": "2024-09-24T02:31:41.239Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "421324a1-5333-4c0d-b463-5a085b28b6b3",
      "number": 3,
      "content": "Ngài rút lại cánh tay phải Ngài trước mặt quân thù của họ;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.240Z",
      "updatedAt": "2024-09-24T02:31:41.240Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "104a2978-9056-4d26-846b-9b26ada7d25c",
      "number": 3,
      "content": "Ngài đốt rụi trong xứ của Gia-cốp,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.243Z",
      "updatedAt": "2024-09-24T02:31:41.243Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "eaac2fff-6431-4738-ab33-0f2bac97dba9",
      "number": 3,
      "content": "Như ngọn lửa hừng thiêu rụi mọi thứ xung quanh.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.245Z",
      "updatedAt": "2024-09-24T02:31:41.245Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "0cb9021a-0122-46c0-b7dc-0e8aa1e66410",
      "number": 4,
      "content": "Ngài đã giương cung nhắm bắn như một xạ thủ của quân thù,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.247Z",
      "updatedAt": "2024-09-24T02:31:41.247Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "49de32d6-5d27-471e-a256-5a8ef02168aa",
      "number": 4,
      "content": "Tay phải Ngài sẵn sàng buông tên;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.249Z",
      "updatedAt": "2024-09-24T02:31:41.249Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "062de249-59cf-41e1-8f33-a609a27539b5",
      "number": 4,
      "content": "Như kẻ thù, Ngài giết sạch kẻ nào mắt Ngài muốn giết,",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.251Z",
      "updatedAt": "2024-09-24T02:31:41.251Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "1bb2dbc7-902b-4741-a170-db7819b61070",
      "number": 5,
      "content": "Chúa đã trở thành như kẻ thù;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.254Z",
      "updatedAt": "2024-09-24T02:31:41.254Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "99b474f5-c9de-411a-9fcb-4f06eb308699",
      "number": 5,
      "content": "Ngài nuốt chửng I-sơ-ra-ên;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.256Z",
      "updatedAt": "2024-09-24T02:31:41.256Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "0197a34e-d26c-44db-aeb8-0ae4841ba3de",
      "number": 5,
      "content": "Ngài nuốt trộng tất cả cung điện nó,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.258Z",
      "updatedAt": "2024-09-24T02:31:41.258Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "817a0219-a353-4dc9-8038-1f0ca6c691b6",
      "number": 5,
      "content": "Ngài biến các thành trì nó ra hoang phế,",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.260Z",
      "updatedAt": "2024-09-24T02:31:41.260Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "e0225a23-85e4-4739-b4a4-201e36448424",
      "number": 5,
      "content": "Ngài làm gia tăng nỗi sầu thảm và bi ai cho Ái Nữ của Giu-đa.",
      "order": 4,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.263Z",
      "updatedAt": "2024-09-24T02:31:41.263Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "5e616581-c588-40e0-a5c7-0e3a3844b6e5",
      "number": 6,
      "content": "Ngài phá tan Lều Tạm của Ngài như người ta phá bỏ một mảnh vườn;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.265Z",
      "updatedAt": "2024-09-24T02:31:41.265Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "4420f9ba-289e-4d1e-a80e-458351ebf583",
      "number": 6,
      "content": "Ngài phá hủy nơi nhóm lại của Ngài.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.267Z",
      "updatedAt": "2024-09-24T02:31:41.267Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "0713ba83-b280-4cfc-afd0-b574e6df6efb",
      "number": 6,
      "content": "Chúa làm cho những ngày đại lễ và những ngày Sa-bát ở Si-ôn không được nhớ đến nữa;",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.269Z",
      "updatedAt": "2024-09-24T02:31:41.269Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "3ce7c291-06e0-4e6e-96b5-8f889ade3177",
      "number": 6,
      "content": "Trong cơn cực kỳ phẫn nộ Ngài ghê tởm cả vua lẫn tư tế ở đó.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.271Z",
      "updatedAt": "2024-09-24T02:31:41.271Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "0b2d0cd3-60bb-4378-99e6-2ec11fb22b2d",
      "number": 7,
      "content": "Chúa loại bỏ bàn thờ của Ngài;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.274Z",
      "updatedAt": "2024-09-24T02:31:41.274Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "99492730-54ef-4041-a563-3adef4668d8a",
      "number": 7,
      "content": "Ngài ghê tởm nơi thánh của Ngài;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.276Z",
      "updatedAt": "2024-09-24T02:31:41.276Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "7d629836-3c7f-4df6-be9f-b0cceb91c8f4",
      "number": 7,
      "content": "Ngài trao vào tay quân thù các bức tường cung điện của nàng;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.279Z",
      "updatedAt": "2024-09-24T02:31:41.279Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "a1b4f73b-6979-4443-a346-4f165b645575",
      "number": 7,
      "content": "Chúng làm ồn trong nhà Chúa như tiếng ồn trong ngày đại lễ.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.282Z",
      "updatedAt": "2024-09-24T02:31:41.282Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "e6b3d41f-7438-4e5f-87c1-d01357f84c0d",
      "number": 8,
      "content": "Chúa quyết định phá đổ tường thành Ái Nữ của Si-ôn;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.284Z",
      "updatedAt": "2024-09-24T02:31:41.284Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "b7f878a0-bc37-486e-8b8a-17631a88a3a9",
      "number": 8,
      "content": "Ngài giăng dây ra đo và không rút tay Ngài lại khi hủy diệt;",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.286Z",
      "updatedAt": "2024-09-24T02:31:41.286Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "b26bc032-afe4-4e71-83cc-9c655ecc3a1f",
      "number": 8,
      "content": "Ngài khiến các chiến lũy và tường thành đều buồn thảm;",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.288Z",
      "updatedAt": "2024-09-24T02:31:41.288Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "1a12e526-b7bc-46d9-ac4a-dcd683456aa3",
      "number": 8,
      "content": "Chúng cùng nhau suy tàn.",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.291Z",
      "updatedAt": "2024-09-24T02:31:41.291Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "e4c67d94-aee5-4163-a639-f766a575db25",
      "number": 9,
      "content": "Các cổng thành của nàng lún sâu dưới đất;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.293Z",
      "updatedAt": "2024-09-24T02:31:41.293Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "d6176974-6541-4129-b76b-81561f03ede8",
      "number": 9,
      "content": "Ngài phá tan và bẻ gãy các thanh ngang;",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.295Z",
      "updatedAt": "2024-09-24T02:31:41.295Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "c130cd9a-1271-4255-a227-7f1d6d4c9725",
      "number": 9,
      "content": "Vua và các quan nàng bị lưu đày nơi xứ lạ,",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.298Z",
      "updatedAt": "2024-09-24T02:31:41.298Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "57fbc1e0-7986-4545-b703-212377824edd",
      "number": 9,
      "content": "Luật Pháp không còn nữa,",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.300Z",
      "updatedAt": "2024-09-24T02:31:41.300Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "4a8459bd-87ed-4d49-b61f-2ad5d09fba24",
      "number": 9,
      "content": "Các tiên tri nàng chẳng thấy khải tượng nào từ Chúa cả.",
      "order": 4,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.302Z",
      "updatedAt": "2024-09-24T02:31:41.302Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "60c9fe2d-4bd8-4062-a39f-722b6fc717bf",
      "number": 10,
      "content": "Các trưởng lão Ái Nữ của Si-ôn ngồi im lìm dưới đất,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.304Z",
      "updatedAt": "2024-09-24T02:31:41.304Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "56047e76-9204-438b-826a-c1f1f377db26",
      "number": 10,
      "content": "Họ hốt bụi rải trên đầu,",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.306Z",
      "updatedAt": "2024-09-24T02:31:41.306Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "21ccd710-fc7b-4f0f-b69d-34abf667f9b7",
      "number": 10,
      "content": "Họ mặc vải thô;",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.309Z",
      "updatedAt": "2024-09-24T02:31:41.309Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "84eb4bea-e5a9-4f84-b225-9b14256d4aac",
      "number": 10,
      "content": "Các trinh nữ của Giê-ru-sa-lem cúi gục đầu xuống đất.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.312Z",
      "updatedAt": "2024-09-24T02:31:41.312Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "c03dac76-69e6-4e19-a036-0c638b157efc",
      "number": 11,
      "content": "Mắt tôi hốc hác vì khóc khô dòng lệ;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.314Z",
      "updatedAt": "2024-09-24T02:31:41.314Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "259b7f5a-ba02-4eb5-b2b7-1318c5693fa5",
      "number": 11,
      "content": "Lòng dạ tôi tan nát từng cơn,",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.317Z",
      "updatedAt": "2024-09-24T02:31:41.317Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "8267b6a3-8261-44e8-9bda-f2f51afc7164",
      "number": 11,
      "content": "Mật tôi đổ ra trên đất vì sự hủy diệt Ái Nữ của dân tôi,",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.326Z",
      "updatedAt": "2024-09-24T02:31:41.326Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "54cdcd4c-6265-481f-aa37-eadfc30da3ee",
      "number": 11,
      "content": "Bởi vì các trẻ thơ và các ấu nhi ngất xỉu trên các đường phố.",
      "order": 3,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.330Z",
      "updatedAt": "2024-09-24T02:31:41.330Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "c20f48ee-2ce5-43bc-86ff-d6a4fc1ffe4f",
      "number": 12,
      "content": "Chúng khóc xin với mẹ chúng,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.336Z",
      "updatedAt": "2024-09-24T02:31:41.336Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "4554fdb2-610f-420b-8bc6-32c5ea015405",
      "number": 12,
      "content": "“Cơm bánh và thức uống đâu?”",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.338Z",
      "updatedAt": "2024-09-24T02:31:41.338Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "941acfca-42da-47e5-b397-271db0a88652",
      "number": 12,
      "content": "Chúng ngất xỉu như người bị tử thương trên các đường phố,",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.341Z",
      "updatedAt": "2024-09-24T02:31:41.341Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "14b275b2-e18d-42f0-9321-0b807b8d3452",
      "number": 12,
      "content": "Rồi chúng từ từ tắt thở trong vòng tay bên lòng mẹ.",
      "order": 3,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.344Z",
      "updatedAt": "2024-09-24T02:31:41.344Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "18e70761-a845-494d-9bf4-6bbf056c6c6b",
      "number": 13,
      "content": "Hỡi Ái Nữ của Giê-ru-sa-lem, ta có thể nói gì cho ngươi nữa?",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.346Z",
      "updatedAt": "2024-09-24T02:31:41.346Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "2056c62a-0a65-4455-9d2e-9166d4dd650f",
      "number": 13,
      "content": "Ta có thể lấy gì so sánh với ngươi?",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.350Z",
      "updatedAt": "2024-09-24T02:31:41.350Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "774a9601-b86f-4f18-bd99-24bdfefc721e",
      "number": 13,
      "content": "Hỡi nàng trinh nữ, Ái Nữ của Si-ôn, ta có thể so sánh ngươi với ai,",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.352Z",
      "updatedAt": "2024-09-24T02:31:41.352Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "1799cd65-4873-45df-b11a-696c916f27cd",
      "number": 13,
      "content": "Ðể ta có thể an ủi ngươi?",
      "order": 3,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.355Z",
      "updatedAt": "2024-09-24T02:31:41.355Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "4dff2725-00a7-4335-9334-0a478f0b9f86",
      "number": 13,
      "content": "Vì nỗi đau thương của ngươi lớn như biển cả,",
      "order": 4,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.357Z",
      "updatedAt": "2024-09-24T02:31:41.357Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "79fd1d91-1818-4b4f-975c-c29660d40764",
      "number": 13,
      "content": "Ai có thể chữa cho ngươi được lành?",
      "order": 5,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.360Z",
      "updatedAt": "2024-09-24T02:31:41.360Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "694c547f-618c-4a79-b2ea-cc6e3f1d73f0",
      "number": 14,
      "content": "Các tiên tri của ngươi đã thấy toàn những điều giả dối và tầm phào;",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.362Z",
      "updatedAt": "2024-09-24T02:31:41.362Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "d1c93d85-b9bc-4c64-a63b-63c9514b66d6",
      "number": 14,
      "content": "Chúng không vạch rõ các tội của ngươi ra,",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.365Z",
      "updatedAt": "2024-09-24T02:31:41.365Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "854014c3-e81d-4df2-8042-fbd9ff8ada2b",
      "number": 14,
      "content": "Hầu mong đảo ngược vận mạng của ngươi,",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.385Z",
      "updatedAt": "2024-09-24T02:31:41.385Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "d7dde5dd-e3f8-4c6b-b188-76a97d3ddbb5",
      "number": 14,
      "content": "Nhưng chúng chỉ thấy và bảo ngươi toàn những sứ điệp giả dối và ảo giác hư không.",
      "order": 3,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.390Z",
      "updatedAt": "2024-09-24T02:31:41.390Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "dde31120-eb3c-4b51-b7a2-dfe4aa92d52e",
      "number": 15,
      "content": "Tất cả những kẻ đi ngang qua thấy ngươi đều đập tay tiếc rẻ,",
      "order": 0,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.393Z",
      "updatedAt": "2024-09-24T02:31:41.393Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "af08aa28-29bd-4ab4-ad34-18c2ec418ff6",
      "number": 15,
      "content": "Chúng xầm xì chế nhạo và lắc đầu khinh bỉ Ái Nữ của Giê-ru-sa-lem,",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.395Z",
      "updatedAt": "2024-09-24T02:31:41.395Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "6426b23f-a7a8-411b-abc6-b383616b2fde",
      "number": 15,
      "content": "“Phải chăng đây vốn là thành mang danh tuyệt mỹ,",
      "order": 2,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.397Z",
      "updatedAt": "2024-09-24T02:31:41.397Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "b9392415-e908-4b29-99e0-0483bb055af8",
      "number": 15,
      "content": "Và niềm vui của cả thế gian?”",
      "order": 3,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.400Z",
      "updatedAt": "2024-09-24T02:31:41.400Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "a25fe86a-d044-4a6d-a965-3f86d4990a62",
      "number": 16,
      "content": "Tất cả kẻ thù của ngươi mở miệng chê bai ngươi;",
      "order": 0,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.402Z",
      "updatedAt": "2024-09-24T02:31:41.402Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "84cd35df-ab66-4d47-acb7-fbaf4523ecd3",
      "number": 16,
      "content": "Chúng xầm xì chế nhạo, nghiến răng, và đắc ý nói vang,",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.405Z",
      "updatedAt": "2024-09-24T02:31:41.405Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "2877eaab-3b11-4e7b-bc16-875e63c244de",
      "number": 16,
      "content": "“Phen này chúng ta nuốt trộng nó được rồi!",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.409Z",
      "updatedAt": "2024-09-24T02:31:41.409Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "efdbb5e8-b732-4ac9-8cc5-bdb36e95eeed",
      "number": 16,
      "content": "Chúng ta đã đợi ngày này lâu lắm rồi;",
      "order": 3,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.412Z",
      "updatedAt": "2024-09-24T02:31:41.412Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "4211a449-1474-463e-8e6e-dc16c24f4261",
      "number": 16,
      "content": "Cuối cùng chúng ta đã thấy được tận mắt!”",
      "order": 4,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.414Z",
      "updatedAt": "2024-09-24T02:31:41.414Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "c8d81561-c215-4345-8457-a64ef516b974",
      "number": 17,
      "content": "Chúa thực hiện những gì Ngài đã quyết định;",
      "order": 0,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.420Z",
      "updatedAt": "2024-09-24T02:31:41.420Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "2c0115eb-ce0e-46ba-9cdf-b3df55402469",
      "number": 17,
      "content": "Ngài làm ứng nghiệm lời Ngài đã cảnh cáo từ lâu.",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.423Z",
      "updatedAt": "2024-09-24T02:31:41.423Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "ba09c023-839e-4a49-bb5f-462a810dc317",
      "number": 17,
      "content": "Ngài lật đổ chẳng mảy may thương xót;",
      "order": 2,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.425Z",
      "updatedAt": "2024-09-24T02:31:41.425Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "6f3b4fc1-8a81-4102-aa08-0529f7e44453",
      "number": 17,
      "content": "Ngài làm cho kẻ thù của ngươi vui mừng vì ngươi,",
      "order": 3,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.428Z",
      "updatedAt": "2024-09-24T02:31:41.428Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "dabc8ce8-bb33-4175-9db1-311598f0b7a5",
      "number": 17,
      "content": "Và làm cho sừng của kẻ thù ngươi thêm cứng mạnh.",
      "order": 4,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.430Z",
      "updatedAt": "2024-09-24T02:31:41.430Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "0aee4f26-d2ac-4d5c-ad88-ddf8136d48dd",
      "number": 18,
      "content": "Hỡi tường thành Ái Nữ của Si-ôn, hãy dốc đổ lòng ngươi kêu cầu với Chúa;",
      "order": 0,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.432Z",
      "updatedAt": "2024-09-24T02:31:41.432Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "54c78979-7aec-48aa-a4c2-2431fe00f3a7",
      "number": 18,
      "content": "Hãy đổ lệ ngươi như suối suốt ngày đêm.",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.436Z",
      "updatedAt": "2024-09-24T02:31:41.436Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "dcb36583-ee8f-48c3-bd21-3942d6617603",
      "number": 18,
      "content": "Chớ để cho ngươi được nghỉ ngơi thơi thả.",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.439Z",
      "updatedAt": "2024-09-24T02:31:41.439Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "c476e171-2795-4b37-bfe8-3fd2c3b5c764",
      "number": 18,
      "content": "Ðừng để cho mắt ngươi ngưng đổ lệ phút nào.",
      "order": 3,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.442Z",
      "updatedAt": "2024-09-24T02:31:41.442Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "0dcf9125-8e3d-4603-9ef8-f254705b1dda",
      "number": 19,
      "content": "Hãy thức dậy giữa đêm khuya để kêu cầu thống thiết,",
      "order": 0,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.444Z",
      "updatedAt": "2024-09-24T02:31:41.444Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "2eff0f33-d3ef-49f2-a9c8-fa8c4f9b2fdb",
      "number": 19,
      "content": "Mỗi đầu canh hãy cất tiếng van nài;",
      "order": 1,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.447Z",
      "updatedAt": "2024-09-24T02:31:41.447Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "2b69c63a-d244-4828-8a9a-a2b4642798db",
      "number": 19,
      "content": "Hãy dốc đổ lòng ngươi trước mặt Chúa như nước tuôn lai láng,",
      "order": 2,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.449Z",
      "updatedAt": "2024-09-24T02:31:41.449Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "f9d7077f-882a-454e-a054-0946701a7a7a",
      "number": 19,
      "content": "Hãy đưa đôi tay ngươi lên hướng về Ngài cầu nguyện cho sinh mạng đàn con dại của ngươi,",
      "order": 3,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.452Z",
      "updatedAt": "2024-09-24T02:31:41.452Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "03c1821a-a7d9-48e0-a542-7f1638e4024e",
      "number": 19,
      "content": "Vì chúng đói khát đến ngất xỉu nơi mỗi đầu đường góc phố.",
      "order": 4,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.454Z",
      "updatedAt": "2024-09-24T02:31:41.454Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "047a471a-6991-471f-bac1-80accd68445b",
      "number": 20,
      "content": "Lạy Chúa, xin Ngài đoái xem và nhìn thấy!",
      "order": 0,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.456Z",
      "updatedAt": "2024-09-24T02:31:41.456Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "a5a80e7f-cf5d-4042-a771-75c01bfaf635",
      "number": 20,
      "content": "Ðể phạt ai mà Ngài làm những điều này?",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.458Z",
      "updatedAt": "2024-09-24T02:31:41.458Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "9bf60cf8-1544-4c89-b1f6-ec8589d16d36",
      "number": 20,
      "content": "Chẳng lẽ những người mẹ phải ăn thịt con mình để sống,",
      "order": 2,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.460Z",
      "updatedAt": "2024-09-24T02:31:41.460Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "b709f0f1-df39-47f2-b1d4-0bb58b8768de",
      "number": 20,
      "content": "Những đứa con mà họ mang nặng đẻ đau?",
      "order": 3,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.462Z",
      "updatedAt": "2024-09-24T02:31:41.462Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "3f52bd4a-68b5-4a3b-aef8-2e3bda00a114",
      "number": 20,
      "content": "Chẳng lẽ tư tế và tiên tri phải bị giết trong nơi thánh của Chúa sao?",
      "order": 4,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.464Z",
      "updatedAt": "2024-09-24T02:31:41.464Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "eef5971b-87b4-4978-8406-04b820744cd3",
      "number": 21,
      "content": "Xác trai trẻ và người già nằm la liệt trên đất khắp các đường phố;",
      "order": 0,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.466Z",
      "updatedAt": "2024-09-24T02:31:41.466Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "2a31af65-6b19-441a-9dac-792e987420a3",
      "number": 21,
      "content": "Các thiếu nữ và các thanh niên bị ngã chết vì gươm.",
      "order": 1,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.468Z",
      "updatedAt": "2024-09-24T02:31:41.468Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "4e42126c-b017-4b5d-976d-8016a2734627",
      "number": 21,
      "content": "Ngài giết họ trong ngày Ngài nổi giận,",
      "order": 2,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.470Z",
      "updatedAt": "2024-09-24T02:31:41.470Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "10cd7e59-5aaa-4c12-9350-2f2f96834ffe",
      "number": 21,
      "content": "Ngài tàn sát họ không một chút xót thương.",
      "order": 3,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.472Z",
      "updatedAt": "2024-09-24T02:31:41.472Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "cfa10ac2-2fd6-4598-a6ec-e0a1188a5aeb",
      "number": 22,
      "content": "Ngài mời quân thù của con từ khắp nơi kéo đến,",
      "order": 0,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.474Z",
      "updatedAt": "2024-09-24T02:31:41.474Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "ccb68726-00b6-4faa-add0-5db7e5af8cde",
      "number": 22,
      "content": "Như thể chúng được mời đến dự một ngày đại lễ tưng bừng,",
      "order": 1,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.476Z",
      "updatedAt": "2024-09-24T02:31:41.476Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "725613ea-7b01-4956-8d51-a6159c9fde3b",
      "number": 22,
      "content": "Ðể trong ngày thịnh nộ của Chúa không người nào trốn thoát hoặc sống sót,",
      "order": 2,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.478Z",
      "updatedAt": "2024-09-24T02:31:41.478Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    },
    {
      "id": "98fa2aa7-d022-4e31-9384-a11c5bd00daa",
      "number": 22,
      "content": "Quân thù giết cả những con thơ mà con bồng ẵm và dưỡng nuôi.",
      "order": 3,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-24T02:31:41.481Z",
      "updatedAt": "2024-09-24T02:31:41.481Z",
      "chapterId": "d59b83bf-fa92-4a77-a916-c6f098a00a08"
    }
  ],
