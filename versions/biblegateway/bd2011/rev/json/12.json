    },
    {
      "id": "5bdb1f70-f878-47a6-9953-20bc8832efc7",
      "content": "Trận Chiến giữa Thiên Sứ Mi-chên và Con Rồng",
      "order": 0,
      "createdAt": "2024-09-24T03:10:17.163Z",
      "updatedAt": "2024-09-24T03:10:17.163Z",
