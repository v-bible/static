    },
    {
      "id": "b02c5972-1c5c-449e-b0de-d3fae22dbd25",
      "content": "Giá Trị của Hình Tượng",
      "order": 0,
      "createdAt": "2024-09-24T02:42:49.155Z",
      "updatedAt": "2024-09-24T02:42:49.155Z",
