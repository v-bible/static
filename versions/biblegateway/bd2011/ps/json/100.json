{
  "chapter": {
    "id": "03768f8d-1ff2-4aa6-8121-2f50f84ce812",
    "number": 100,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20100&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.397Z",
    "updatedAt": "2024-09-24T01:00:44.397Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "b3a0b4a2-2507-44ee-b8db-a6c1a8d95751",
      "number": 1,
      "content": "Hỡi cả thế giới,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.787Z",
      "updatedAt": "2024-09-24T02:06:40.787Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "2ba5330a-7321-46d4-b562-685b881fc337",
      "number": 1,
      "content": "Hãy lớn tiếng reo mừng tung hô Chúa.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.800Z",
      "updatedAt": "2024-09-24T02:06:40.800Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "c7d6f05a-4a29-451f-9bce-dc15c29b58cf",
      "number": 2,
      "content": "Hãy thờ phượng Chúa cách vui mừng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.806Z",
      "updatedAt": "2024-09-24T02:06:40.806Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "97dabe6e-f470-41ed-af9e-6b7131465fef",
      "number": 2,
      "content": "Hãy đến trước thánh nhan Ngài với tiếng hoan ca.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.816Z",
      "updatedAt": "2024-09-24T02:06:40.816Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "61da42f8-2026-4189-a124-b4d947e3d15c",
      "number": 3,
      "content": "Khá biết rằng Chúa là Ðức Chúa Trời.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.821Z",
      "updatedAt": "2024-09-24T02:06:40.821Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "6a2dd5a7-221f-439d-a1b1-1ddc1b6b0018",
      "number": 3,
      "content": "Ấy là Ngài, Đấng dựng nên chúng ta;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.826Z",
      "updatedAt": "2024-09-24T02:06:40.826Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "681dbc85-1278-4b18-8495-87c355f6822b",
      "number": 3,
      "content": "Chúng ta thuộc về Ngài;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.830Z",
      "updatedAt": "2024-09-24T02:06:40.830Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "4e9f84ff-67f2-4961-9280-36b3a01657ad",
      "number": 3,
      "content": "Chúng ta là con dân Ngài và bầy chiên của đồng cỏ Ngài.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.835Z",
      "updatedAt": "2024-09-24T02:06:40.835Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "9981fece-eb38-4603-927c-d6036c27bc83",
      "number": 4,
      "content": "Hãy cảm tạ mà vào các cổng Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.840Z",
      "updatedAt": "2024-09-24T02:06:40.840Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "ec9cf4ed-aaf0-462a-92a1-88df5437a8cb",
      "number": 4,
      "content": "Khá ngợi ca mà vào các sân Ngài;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.845Z",
      "updatedAt": "2024-09-24T02:06:40.845Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "fb065014-dd76-4bd4-a9bc-586756be264e",
      "number": 4,
      "content": "Hãy tạ ơn Ngài, khá chúc tụng danh Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.850Z",
      "updatedAt": "2024-09-24T02:06:40.850Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "4842b6a1-3574-4efe-b854-2489da0cdb28",
      "number": 5,
      "content": "Vì Chúa thật tốt;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.855Z",
      "updatedAt": "2024-09-24T02:06:40.855Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "5d0b7236-627c-4864-8df1-4f018fb80ed9",
      "number": 5,
      "content": "Tình thương của Ngài hằng còn mãi mãi;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.860Z",
      "updatedAt": "2024-09-24T02:06:40.860Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    },
    {
      "id": "0adca9f3-1702-45ac-8906-3d511a7f4e6c",
      "number": 5,
      "content": "Ðức thành tín của Ngài còn đến muôn đời.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:40.864Z",
      "updatedAt": "2024-09-24T02:06:40.864Z",
      "chapterId": "03768f8d-1ff2-4aa6-8121-2f50f84ce812"
    }
  ],
