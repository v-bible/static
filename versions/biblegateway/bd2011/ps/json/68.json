{
  "chapter": {
    "id": "b12f1ad7-a15e-448c-903b-b52a561cab9f",
    "number": 68,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2068&version=BD2011",
    "createdAt": "2024-09-24T01:00:43.742Z",
    "updatedAt": "2024-09-24T01:00:43.742Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "f1288124-5f31-4596-9193-33e280d896ea",
      "number": 1,
      "content": "Nguyện Ðức Chúa Trời trỗi dậy;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.197Z",
      "updatedAt": "2024-09-24T02:02:16.197Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "35c7d930-563d-4d24-acad-45f1787f6310",
      "number": 1,
      "content": "Nguyện những kẻ chống nghịch Ngài tản lạc;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.209Z",
      "updatedAt": "2024-09-24T02:02:16.209Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "cc452bec-9fdf-4448-8dca-0a37bb2f0aca",
      "number": 1,
      "content": "Nguyện những kẻ ghét Ngài chạy trốn trước mặt Ngài.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.212Z",
      "updatedAt": "2024-09-24T02:02:16.212Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "d8c965fb-a3c0-4cd9-ad8b-e2108c8cc846",
      "number": 2,
      "content": "Như khói bị tan biến thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.214Z",
      "updatedAt": "2024-09-24T02:02:16.214Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "26ee4c38-8795-40fb-9986-74523a19a701",
      "number": 2,
      "content": "Nguyện chúng bị tan đi thể ấy;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.216Z",
      "updatedAt": "2024-09-24T02:02:16.216Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "7c109cf4-da3b-48a9-95e9-b86376d5c974",
      "number": 2,
      "content": "Như sáp bị tan chảy trước ngọn lửa thể nào,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.219Z",
      "updatedAt": "2024-09-24T02:02:16.219Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "f53b2011-08f8-4b50-ba92-91d2abd8866d",
      "number": 2,
      "content": "Nguyện những kẻ ác bị diệt mất trước mặt Ðức Chúa Trời thể ấy.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.222Z",
      "updatedAt": "2024-09-24T02:02:16.222Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "31af5198-70f2-4c79-bbbc-7037f0adf364",
      "number": 3,
      "content": "Nguyện những người ngay lành hân hoan,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.224Z",
      "updatedAt": "2024-09-24T02:02:16.224Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "98cb12e7-c326-44ae-85a9-44c05a243500",
      "number": 3,
      "content": "Nguyện họ vui vẻ trước mặt Ðức Chúa Trời;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.227Z",
      "updatedAt": "2024-09-24T02:02:16.227Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "7a147ee7-b518-4537-807b-164e9d923544",
      "number": 3,
      "content": "Thật vậy nguyện họ hớn hở và vui mừng.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.229Z",
      "updatedAt": "2024-09-24T02:02:16.229Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "9bafbca4-370b-4bd3-99d3-4dce8dac4d4d",
      "number": 4,
      "content": "Hãy ca ngợi Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.231Z",
      "updatedAt": "2024-09-24T02:02:16.231Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "733a96a9-993a-4eb8-9740-336fff8eab4b",
      "number": 4,
      "content": "Hãy hát ca chúc tụng danh Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.234Z",
      "updatedAt": "2024-09-24T02:02:16.234Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "ea77ff7f-b0ea-4038-aed0-74c3a5c5a485",
      "number": 4,
      "content": "Khá tôn ngợi Ðấng ngự trên các tầng mây;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.236Z",
      "updatedAt": "2024-09-24T02:02:16.236Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "fc27efde-a392-496e-a2c2-b136b7af837b",
      "number": 4,
      "content": "Danh Ngài là Chúa;",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.242Z",
      "updatedAt": "2024-09-24T02:02:16.242Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "63d698d3-4b7b-42a5-adae-e5cc89b71dab",
      "number": 4,
      "content": "Khá reo vui trước mặt Ngài.",
      "order": 4,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.244Z",
      "updatedAt": "2024-09-24T02:02:16.244Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "410eac6b-4c4d-410e-b556-28fc4b541a52",
      "number": 5,
      "content": "Cha kẻ mồ côi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.246Z",
      "updatedAt": "2024-09-24T02:02:16.246Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "70d9da6d-a28a-4360-a416-9b6201b7fc4a",
      "number": 5,
      "content": "Ðấng binh vực người góa bụa,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.249Z",
      "updatedAt": "2024-09-24T02:02:16.249Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "922f6ac1-0a05-40d3-927e-1d63492744a0",
      "number": 5,
      "content": "Chính là Ðức Chúa Trời,",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.251Z",
      "updatedAt": "2024-09-24T02:02:16.251Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "4ba3c34a-20cd-427c-9108-f0b3b0fd4a01",
      "number": 5,
      "content": "Ðấng ngự trong nơi thánh của Ngài.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.253Z",
      "updatedAt": "2024-09-24T02:02:16.253Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "f543f24a-f7bc-4eef-a588-257fbe9964e4",
      "number": 6,
      "content": "Ðức Chúa Trời là Ðấng làm cho kẻ cô đơn có mái ấm gia đình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.257Z",
      "updatedAt": "2024-09-24T02:02:16.257Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "5d8ff0ba-289f-4d14-87d8-c9daf02e7104",
      "number": 6,
      "content": "Ngài dẫn đưa những kẻ bị tù đày đến nơi sung mãn;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.259Z",
      "updatedAt": "2024-09-24T02:02:16.259Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "7dbdcad5-270c-486b-a6f7-860302b72fa9",
      "number": 6,
      "content": "Còn những kẻ bội nghịch phải ở trong xứ khô khan.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.261Z",
      "updatedAt": "2024-09-24T02:02:16.261Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "e85972fd-c9eb-403e-900a-50f348c5610e",
      "number": 7,
      "content": "Ðức Chúa Trời ôi, khi Ngài đi ra trước mặt con dân Ngài,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.263Z",
      "updatedAt": "2024-09-24T02:02:16.263Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "9a80cef9-bbcb-4abc-80b0-a6c73df6a0e3",
      "number": 7,
      "content": "Khi Ngài đi qua sa mạc, *(Sê-la)*",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.266Z",
      "updatedAt": "2024-09-24T02:02:16.266Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "35c4885f-7217-4e3a-ac75-85b833a42340",
      "number": 8,
      "content": "Ðất rúng động, các tầng trời ào ạt đổ cơn mưa,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.268Z",
      "updatedAt": "2024-09-24T02:02:16.268Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "f4900a97-2b34-4693-b00e-00ba9e73c245",
      "number": 8,
      "content": "Trước mặt Ðức Chúa Trời, Thần của Núi Si-nai,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.272Z",
      "updatedAt": "2024-09-24T02:02:16.272Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "46d56872-8400-4804-803d-2e095d8286ef",
      "number": 8,
      "content": "Trước mặt Ðức Chúa Trời, Thần của I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.278Z",
      "updatedAt": "2024-09-24T02:02:16.278Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "4c6a322a-98a8-45ca-9036-e38c487e4b36",
      "number": 9,
      "content": "Ðức Chúa Trời ôi, Ngài đã ban mưa xuống cách dồi dào,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.281Z",
      "updatedAt": "2024-09-24T02:02:16.281Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "1d469a01-ec6a-40df-aa44-787eeeafd1a8",
      "number": 9,
      "content": "Khi sản nghiệp của Ngài bị khô khan, Ngài đã bồi bổ nó lại.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.283Z",
      "updatedAt": "2024-09-24T02:02:16.283Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "d46699a7-9de8-4f98-93cc-b8a8ab5fa2b4",
      "number": 10,
      "content": "Con dân Ngài đã định cư trong sản nghiệp đó;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.286Z",
      "updatedAt": "2024-09-24T02:02:16.286Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "06bf0ba5-33f1-4ad7-8df5-449b48648b3f",
      "number": 10,
      "content": "Ðức Chúa Trời ôi, do lòng tốt của Ngài, Ngài đã chu cấp cho những người nghèo khó.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.288Z",
      "updatedAt": "2024-09-24T02:02:16.288Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "051e6b43-48b9-467c-955a-a7076f67ab26",
      "number": 11,
      "content": "Khi Chúa ban lệnh,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.291Z",
      "updatedAt": "2024-09-24T02:02:16.291Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "2f3d954e-1f6b-407c-85e2-a3f96d3779c3",
      "number": 11,
      "content": "Những kẻ loan truyền tin mừng ra đi đông vô số;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.293Z",
      "updatedAt": "2024-09-24T02:02:16.293Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "9bb39fe5-92f9-4f02-9d1e-c8c56dc8f290",
      "number": 11,
      "content": "Họ báo rằng,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.295Z",
      "updatedAt": "2024-09-24T02:02:16.295Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "1d4edf0c-fe42-4971-a8bf-d38bee1edd16",
      "number": 12,
      "content": "“Các vua của các đạo quân đã bỏ chạy. Tất cả đã chạy trốn.”",
      "order": 0,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.297Z",
      "updatedAt": "2024-09-24T02:02:16.297Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "aaa7817f-50e0-4472-93cc-2172dcf5221f",
      "number": 12,
      "content": "Những phụ nữ ở nhà đều sẽ được chia các chiến lợi phẩm,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.300Z",
      "updatedAt": "2024-09-24T02:02:16.300Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "74427c94-ff3b-4538-869d-f8fbc9c63604",
      "number": 13,
      "content": "Dù họ chỉ nằm nghỉ ở giữa các trại quân.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.302Z",
      "updatedAt": "2024-09-24T02:02:16.302Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "71fc42a9-e1eb-4dfa-a724-0c9449b51794",
      "number": 13,
      "content": "Kìa họ trông giống như những cánh bồ câu bọc bạc,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.308Z",
      "updatedAt": "2024-09-24T02:02:16.308Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "8dc226a4-7d54-4426-8420-cd125025ff9b",
      "number": 13,
      "content": "Với bộ lông như vàng ròng lóng lánh.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.312Z",
      "updatedAt": "2024-09-24T02:02:16.312Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "de67e0c6-623b-4358-a05e-52045b7091de",
      "number": 14,
      "content": "Khi Ðấng Toàn Năng đánh văng các vua trong xứ,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.315Z",
      "updatedAt": "2024-09-24T02:02:16.315Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "27711e71-bb69-4797-b548-7f309402c90b",
      "number": 14,
      "content": "Họ như tuyết rơi tơi tả ở Xanh-môn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.317Z",
      "updatedAt": "2024-09-24T02:02:16.317Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "59e63206-2c75-4087-bdc7-832bdcb160e3",
      "number": 15,
      "content": "Núi Ba-san là núi của Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.320Z",
      "updatedAt": "2024-09-24T02:02:16.320Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "2b6e8d22-8667-4dd9-aeeb-a5e82c7b9b55",
      "number": 15,
      "content": "Rặng Núi Ba-san quả có nhiều đỉnh cao chót vót.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.323Z",
      "updatedAt": "2024-09-24T02:02:16.323Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "bb18b461-d339-4b2e-96fb-b0eb4edef53b",
      "number": 16,
      "content": "Hỡi những rặng núi cao có nhiều đỉnh,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.325Z",
      "updatedAt": "2024-09-24T02:02:16.325Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "b267527b-b277-453c-b06f-49b7d973e6e6",
      "number": 16,
      "content": "Tại sao các ngươi ganh tị với ngọn núi Ðức Chúa Trời đã chọn để làm nơi Ngài ngự?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.329Z",
      "updatedAt": "2024-09-24T02:02:16.329Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "c1585760-de67-4a95-81a8-42963a26afbb",
      "number": 16,
      "content": "Thật vậy Chúa sẽ ngự tại đó đời đời.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.331Z",
      "updatedAt": "2024-09-24T02:02:16.331Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "1893ea16-3289-4d9e-ac95-92b174d4d1c8",
      "number": 17,
      "content": "Các xe chiến mã của Ðức Chúa Trời nhiều đến hằng vạn, thật là hằng ngàn và hằng vạn;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.334Z",
      "updatedAt": "2024-09-24T02:02:16.334Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "bba81162-84d5-48b2-b575-ce126e1d49a6",
      "number": 17,
      "content": "Chúa ngự giữa chúng như Ngài ngự trong nơi thánh của Ngài ở Si-nai.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.337Z",
      "updatedAt": "2024-09-24T02:02:16.337Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "cd1bfbbe-5271-46a4-8b5b-abb8b23961f6",
      "number": 18,
      "content": "Ngài đã lên nơi cao,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.340Z",
      "updatedAt": "2024-09-24T02:02:16.340Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "a66a43b6-154a-490d-a919-c006c52a37d8",
      "number": 18,
      "content": "Ngài dẫn theo đoàn tù binh bị bắt đem đi lưu đày;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.342Z",
      "updatedAt": "2024-09-24T02:02:16.342Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "af86c65c-7df5-4c48-949a-e5e6228e7450",
      "number": 18,
      "content": "Ngài đã nhận những lễ vật của loài người,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.344Z",
      "updatedAt": "2024-09-24T02:02:16.344Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "d7de8bb2-8880-454d-8f51-c54245028fbe",
      "number": 18,
      "content": "Ngay cả những lễ vật của những kẻ bội nghịch,",
      "order": 3,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.346Z",
      "updatedAt": "2024-09-24T02:02:16.346Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "bc3a9a4f-fe72-44b1-8f0a-0cbb26187c72",
      "number": 18,
      "content": "Ðể chứng tỏ rằng Chúa Ðức Chúa Trời cũng ở tại đó.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.349Z",
      "updatedAt": "2024-09-24T02:02:16.349Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "41d631e6-1adb-4170-bdc8-a634436cc063",
      "number": 19,
      "content": "Tôn ngợi Chúa, Ðấng hằng ngày cưu mang chúng ta;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.353Z",
      "updatedAt": "2024-09-24T02:02:16.353Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "336305a5-68cb-4052-a76c-58c329d6f844",
      "number": 19,
      "content": "Ðức Chúa Trời là Ðấng Giải Cứu của chúng ta. *(Sê-la)*",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.356Z",
      "updatedAt": "2024-09-24T02:02:16.356Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "7f15c226-b092-44f5-945c-1dc8f79d4922",
      "number": 20,
      "content": "Ðức Chúa Trời chúng ta là Ðức Chúa Trời giải cứu;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.359Z",
      "updatedAt": "2024-09-24T02:02:16.359Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "c7200e6f-1fcb-487f-8bcb-a6cb78d06390",
      "number": 20,
      "content": "Nhờ Chúa Hằng Hữu, chúng ta được thoát chết.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.361Z",
      "updatedAt": "2024-09-24T02:02:16.361Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "c59351ee-4498-4a68-b1e8-e0ae55f51317",
      "number": 21,
      "content": "Chắc chắn Ðức Chúa Trời sẽ đạp nát đầu kẻ thù của Ngài,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.366Z",
      "updatedAt": "2024-09-24T02:02:16.366Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "f6a39252-eaf1-40db-bffd-35274c392bdf",
      "number": 21,
      "content": "Tức đầu tóc của những kẻ cứ miệt mài trong tội ác.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.369Z",
      "updatedAt": "2024-09-24T02:02:16.369Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "c24cb30e-c3b8-4aec-92f4-4bac3a925475",
      "number": 22,
      "content": "Chúa phán, “Từ Ba-san, Ta sẽ đem chúng về;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.372Z",
      "updatedAt": "2024-09-24T02:02:16.372Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "ea5a6c91-d488-4dcb-8f43-5457d05b1d77",
      "number": 22,
      "content": "Từ các đại dương sâu thẳm, Ta sẽ mang chúng về,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.376Z",
      "updatedAt": "2024-09-24T02:02:16.376Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "b609c362-88c5-4faf-8c52-d811088e151a",
      "number": 23,
      "content": "Ðể các ngươi có thể giẫm chân trên máu kẻ thù của mình,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.379Z",
      "updatedAt": "2024-09-24T02:02:16.379Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "94de5e29-a45d-4276-9627-e71e63b43c68",
      "number": 23,
      "content": "Ngay cả lưỡi của chó các ngươi cũng sẽ được phần trên quân thù của các ngươi.”",
      "order": 1,
      "parNumber": 7,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.381Z",
      "updatedAt": "2024-09-24T02:02:16.381Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "a8dc185d-0237-4435-9898-d73a8a7b2fcf",
      "number": 24,
      "content": "Ðức Chúa Trời ôi, người ta đã chứng kiến lễ nghinh rước Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.383Z",
      "updatedAt": "2024-09-24T02:02:16.383Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "c3d2c186-a43d-47c3-96e1-3bc3885088d8",
      "number": 24,
      "content": "Tức lễ nghinh rước Ðức Chúa Trời của con, Vua của con, vào đền thánh.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.387Z",
      "updatedAt": "2024-09-24T02:02:16.387Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "5ab49323-9eed-487e-94b8-9e5de44bebb8",
      "number": 25,
      "content": "Ca đoàn đi trước ngợi ca, ban nhạc theo sau trỗi nhạc,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.390Z",
      "updatedAt": "2024-09-24T02:02:16.390Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "ef6eb0d9-ba53-4e06-b63c-56115b3c4388",
      "number": 25,
      "content": "Ở giữa là đoàn thiếu nữ đánh trống lục lạc nhịp nhàng.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.396Z",
      "updatedAt": "2024-09-24T02:02:16.396Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "69f3d94c-1b5f-44ba-aa39-cf5b10374645",
      "number": 26,
      "content": "Hãy chúc tụng Ðức Chúa Trời giữa những đoàn người đông đúc dự lễ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.399Z",
      "updatedAt": "2024-09-24T02:02:16.399Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "11b9fdf4-6ade-438e-9c50-e436e18637ac",
      "number": 26,
      "content": "Hãy tôn ngợi Chúa, nguồn cội của I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.402Z",
      "updatedAt": "2024-09-24T02:02:16.402Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "07312bc0-2121-4316-842a-338b715e6e8b",
      "number": 27,
      "content": "Kìa là Bên-gia-min, chi tộc trẻ nhất, đang dẫn đầu;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.411Z",
      "updatedAt": "2024-09-24T02:02:16.411Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "990ec12c-23d2-45ed-8fc0-1c4aa4b42ab0",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Giu-đa và phái đoàn của họ;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.417Z",
      "updatedAt": "2024-09-24T02:02:16.417Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "0a86e2ea-38a6-4063-ae1c-d1599d1643b6",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Xê-bu-lun và các nhà quyền quý của Náp-ta-li.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.423Z",
      "updatedAt": "2024-09-24T02:02:16.423Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "6a3388a3-be0b-4b16-9899-f85577e6d57a",
      "number": 28,
      "content": "Ðức Chúa Trời ôi, xin thể hiện quyền năng Ngài;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.428Z",
      "updatedAt": "2024-09-24T02:02:16.428Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "885ddf32-b356-45f5-bf83-475a38669706",
      "number": 28,
      "content": "Ðức Chúa Trời ôi, xin bày tỏ sức mạnh Ngài,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.440Z",
      "updatedAt": "2024-09-24T02:02:16.440Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "d4028aea-d62a-4d10-9723-0633d5a7592f",
      "number": 28,
      "content": "Như Ngài đã từng làm cho chúng con khi trước.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.446Z",
      "updatedAt": "2024-09-24T02:02:16.446Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "68d5577b-a510-4fa5-ac92-2ffa055992c1",
      "number": 29,
      "content": "Vì đền thờ Ngài tọa lạc tại Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.451Z",
      "updatedAt": "2024-09-24T02:02:16.451Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "398d1694-eb2b-4b7d-9854-774376b01d74",
      "number": 29,
      "content": "Nên các vua sẽ đem các lễ vật đến dâng lên Ngài tại đó.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.457Z",
      "updatedAt": "2024-09-24T02:02:16.457Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "0b6207e0-7632-49f5-ba19-3112994724f2",
      "number": 30,
      "content": "Cầu xin Ngài trách phạt bầy thú hoang trong đám lau sậy,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.464Z",
      "updatedAt": "2024-09-24T02:02:16.464Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "1ab1065f-2b9c-4f87-ab23-93888b69ec08",
      "number": 30,
      "content": "Cùng đám bò đực ở giữa đàn bò con của các dân.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.469Z",
      "updatedAt": "2024-09-24T02:02:16.469Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "58d0d8c7-ffcc-4b23-84b9-b1929f494438",
      "number": 30,
      "content": "Xin Ngài bắt chúng phải đem bạc nén đến quỳ dâng;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.475Z",
      "updatedAt": "2024-09-24T02:02:16.475Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "a6e1cdb3-bea2-4885-9cbf-513263f250f8",
      "number": 30,
      "content": "Còn những dân hiếu chiến, xin Ngài làm cho chúng bị tản lạc khắp nơi trên đất.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.480Z",
      "updatedAt": "2024-09-24T02:02:16.480Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "c17e1705-aa5c-4730-bd44-c46ed8fd43af",
      "number": 31,
      "content": "Những đoàn người quyền thế sẽ từ Ai-cập đến thờ phượng;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.486Z",
      "updatedAt": "2024-09-24T02:02:16.486Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "ee5dbbb6-50b1-4eb2-a044-0e211f677110",
      "number": 31,
      "content": "Người Ê-thi-ô-pi sẽ sớm đưa tay lên hướng về Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.491Z",
      "updatedAt": "2024-09-24T02:02:16.491Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "e140711b-4a35-46e0-96e0-7e71f52df080",
      "number": 32,
      "content": "Hỡi tất cả quốc gia trên thế giới, hãy ca tụng Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.501Z",
      "updatedAt": "2024-09-24T02:02:16.501Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "a52c8191-96ba-47a5-91c0-2e90e18d8c37",
      "number": 32,
      "content": "Hãy ca ngợi Chúa, *(Sê-la)*",
      "order": 1,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.510Z",
      "updatedAt": "2024-09-24T02:02:16.510Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "45ef4f76-d4cf-44a9-aaff-c7d32670d51b",
      "number": 33,
      "content": "Ðấng ngự trên nơi chí cao của các tầng trời,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.516Z",
      "updatedAt": "2024-09-24T02:02:16.516Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "007bf664-aa67-4d7f-84ce-804d4c58a5cf",
      "number": 33,
      "content": "Tức các tầng trời đã được dựng nên từ thời thái cổ;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.522Z",
      "updatedAt": "2024-09-24T02:02:16.522Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "4a393001-454a-4505-85cf-9a9498d540d4",
      "number": 33,
      "content": "Kìa, Ngài cất tiếng nói,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.529Z",
      "updatedAt": "2024-09-24T02:02:16.529Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "32e0c1a3-5311-4130-a2a6-e83a406458fb",
      "number": 33,
      "content": "Giọng Ngài thật oai hùng.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.534Z",
      "updatedAt": "2024-09-24T02:02:16.534Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "d20ff1e6-2448-47b9-9263-c34bbc795caf",
      "number": 34,
      "content": "Khá biết rằng mọi quyền lực đều thuộc về Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.543Z",
      "updatedAt": "2024-09-24T02:02:16.543Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "9d5b7ec1-89f1-4acd-bb54-cdb7f2d79145",
      "number": 34,
      "content": "Nguyện uy nghi Ngài tỏa rạng trên I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.549Z",
      "updatedAt": "2024-09-24T02:02:16.549Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "a99d3bc2-a073-4e81-bf0d-1bf596bcd259",
      "number": 34,
      "content": "Và quyền năng Ngài đầy dẫy khắp không trung.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.555Z",
      "updatedAt": "2024-09-24T02:02:16.555Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "ee3a8d49-7d91-41d1-8e77-8bc7ae333068",
      "number": 35,
      "content": "Ðức Chúa Trời ôi, trong đền thánh Ngài, Ngài đáng kinh hãi biết bao!",
      "order": 0,
      "parNumber": 10,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.561Z",
      "updatedAt": "2024-09-24T02:02:16.561Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "ddf950bd-8fdd-41cf-8b9d-0f9f2e3f4d02",
      "number": 35,
      "content": "Chính Ðức Chúa Trời của I-sơ-ra-ên đã ban sức mạnh và quyền lực cho con dân Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.566Z",
      "updatedAt": "2024-09-24T02:02:16.566Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    },
    {
      "id": "bc491c67-a770-44d5-a106-7f80698d69d9",
      "number": 35,
      "content": "Tôn ngợi Ðức Chúa Trời!",
      "order": 2,
      "parNumber": 10,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:02:16.572Z",
      "updatedAt": "2024-09-24T02:02:16.572Z",
      "chapterId": "b12f1ad7-a15e-448c-903b-b52a561cab9f"
    }
  ],
