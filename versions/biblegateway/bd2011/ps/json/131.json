{
  "chapter": {
    "id": "96db5055-5566-42e8-95f7-d71b29e675b5",
    "number": 131,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20131&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.038Z",
