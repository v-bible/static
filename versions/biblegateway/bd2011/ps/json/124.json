{
  "chapter": {
    "id": "48f580ef-12ba-40e8-995e-67b933c23b72",
    "number": 124,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20124&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.898Z",
