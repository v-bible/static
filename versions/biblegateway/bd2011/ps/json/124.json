  },
  "verses": [
    {
      "id": "a40a8851-20b1-46af-8ad0-b43e8c57086b",
      "number": 1,
      "content": "Nếu chẳng phải Chúa bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.720Z",
      "updatedAt": "2024-09-24T02:10:31.720Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "687cd06f-edf9-469e-aab2-cfdf79b793e5",
      "number": 2,
      "content": "Nếu chẳng phải Chúa bênh vực chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.740Z",
      "updatedAt": "2024-09-24T02:10:31.740Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "2dc6005c-7808-4f08-bea0-124ae9da979a",
      "number": 1,
      "content": "Hỡi I-sơ-ra-ên, hãy nói đi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.738Z",
      "updatedAt": "2024-09-24T02:10:31.738Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "394aaf50-2490-48e3-86a7-482cedb979ec",
      "number": 2,
      "content": "Thì khi người ta nổi lên chống lại chúng ta,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.742Z",
      "updatedAt": "2024-09-24T02:10:31.742Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "cc72aec1-37aa-4230-a9b0-5fed159b69f0",
      "number": 3,
      "content": "Khi cơn cuồng nộ của chúng bừng lên nghịch lại chúng ta,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.744Z",
      "updatedAt": "2024-09-24T02:10:31.744Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "56e6a0d2-4175-4fe1-a5ca-cbf48d292bd3",
      "number": 3,
      "content": "Chắc hẳn chúng đã nuốt chửng chúng ta rồi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.746Z",
      "updatedAt": "2024-09-24T02:10:31.746Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "e5802e86-708b-417b-a552-b7d37aa0f44b",
      "number": 4,
      "content": "Các dòng nước lũ chắc đã phủ ngập chúng ta;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.749Z",
      "updatedAt": "2024-09-24T02:10:31.749Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "c899e82e-390a-43cb-b263-9a2118d31057",
      "number": 4,
      "content": "Luồng nước mạnh chắc đã cuốn trôi linh hồn chúng ta;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.751Z",
      "updatedAt": "2024-09-24T02:10:31.751Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "f82125ad-d59f-467b-87e4-014ffee795fa",
      "number": 5,
      "content": "Các đợt sóng thần dữ dội chắc đã cuốn mất linh hồn chúng ta rồi.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.753Z",
      "updatedAt": "2024-09-24T02:10:31.753Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "988cadda-3448-4355-baec-8a72a36fbc8e",
      "number": 6,
      "content": "Chúc tụng Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.755Z",
      "updatedAt": "2024-09-24T02:10:31.755Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "0fe653d0-e65e-4dc5-8163-d2dcde9dc505",
      "number": 6,
      "content": "Ðấng không cho phép chúng ta làm mồi cho răng chúng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.757Z",
      "updatedAt": "2024-09-24T02:10:31.757Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "f0245740-6a6e-4f8a-9b09-7b14e135611f",
      "number": 7,
      "content": "Linh hồn chúng ta được giải thoát,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.759Z",
      "updatedAt": "2024-09-24T02:10:31.759Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "1b11f4d1-61c8-403d-ae22-dbcbb4ed6b17",
      "number": 7,
      "content": "Như con chim thoát khỏi bẫy của kẻ bẫy chim;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.761Z",
      "updatedAt": "2024-09-24T02:10:31.761Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "d0f88266-651b-4808-8448-600b86b3f47b",
      "number": 7,
      "content": "Cái bẫy bị bẻ gãy, và chúng ta thoát ra ngoài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.763Z",
      "updatedAt": "2024-09-24T02:10:31.763Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "919b8d67-71a4-4c48-90bc-df66aedb153b",
      "number": 8,
      "content": "Chúng ta được giúp đỡ nhờ danh Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.765Z",
      "updatedAt": "2024-09-24T02:10:31.765Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    },
    {
      "id": "f04da705-a9ae-492f-8d01-150be00d642a",
      "number": 8,
      "content": "Ðấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:31.768Z",
      "updatedAt": "2024-09-24T02:10:31.768Z",
      "chapterId": "48f580ef-12ba-40e8-995e-67b933c23b72"
    }
  ],
