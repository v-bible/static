{
  "chapter": {
    "id": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b",
    "number": 23,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2023&version=BD2011",
    "createdAt": "2024-09-24T01:00:42.833Z",
    "updatedAt": "2024-09-24T01:00:42.833Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "5b1181a5-bdf7-4c96-afb9-18454f352038",
      "number": 1,
      "content": "Chúa là Ðấng chăn giữ tôi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.199Z",
      "updatedAt": "2024-09-24T01:54:59.199Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "a4f3efa2-6294-442f-841e-4aa4b0256a9a",
      "number": 1,
      "content": "Tôi sẽ chẳng thiếu thốn gì.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.208Z",
      "updatedAt": "2024-09-24T01:54:59.208Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "d6b2eeae-643a-4867-badf-1687524e602e",
      "number": 2,
      "content": "Ngài cho tôi nằm nghỉ trong đồng cỏ xanh tươi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.212Z",
      "updatedAt": "2024-09-24T01:54:59.212Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "eeeb362d-0198-40c8-bf4f-dd5cdcd73a4f",
      "number": 2,
      "content": "Ngài dẫn tôi đến mé nước bình tịnh.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.215Z",
      "updatedAt": "2024-09-24T01:54:59.215Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "784e6505-fa2a-4d4e-a09c-876bde1596bd",
      "number": 3,
      "content": "Ngài phục hồi sức lực cho linh hồn tôi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.219Z",
      "updatedAt": "2024-09-24T01:54:59.219Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "251754af-885b-4ad4-9d96-3ba7774ba0b8",
      "number": 3,
      "content": "Ngài dẫn tôi vào các lối công chính, vì cớ danh Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.222Z",
      "updatedAt": "2024-09-24T01:54:59.222Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "19f9d0a9-5a98-4338-a463-986b7572c723",
      "number": 4,
      "content": "Thật vậy, dù khi con đi qua thung lũng rợp bóng tử thần,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.226Z",
      "updatedAt": "2024-09-24T01:54:59.226Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "dbdaba76-942d-422f-b630-a6e422a1983c",
      "number": 4,
      "content": "Con sẽ chẳng sợ tai họa nào,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.229Z",
      "updatedAt": "2024-09-24T01:54:59.229Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "31f70555-8fac-4da4-99a0-7170bace1aa5",
      "number": 4,
      "content": "Vì Ngài ở với con;",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.233Z",
      "updatedAt": "2024-09-24T01:54:59.233Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "fa363c04-c525-4c70-8b3f-335516581fa0",
      "number": 4,
      "content": "Cây trượng và cây gậy của Ngài an ủi con.",
      "order": 3,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.236Z",
      "updatedAt": "2024-09-24T01:54:59.236Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "4403edc8-4c8f-4e8c-9a29-a12c5ef0b66d",
      "number": 5,
      "content": "Ngài dọn bàn cho con trước mặt những kẻ thù của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.240Z",
      "updatedAt": "2024-09-24T01:54:59.240Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "f6ef0ccf-7d6e-4a9a-bba3-0a930e46e963",
      "number": 5,
      "content": "Ngài xức dầu cho đầu con;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.244Z",
      "updatedAt": "2024-09-24T01:54:59.244Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "790b9c69-5abc-4de0-a4e1-4a59f1a77b7c",
      "number": 5,
      "content": "Chén con đầy tràn.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.247Z",
      "updatedAt": "2024-09-24T01:54:59.247Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "27384d3b-d139-4e4d-b28b-6e5523025062",
      "number": 6,
      "content": "Quả thật, trọn đời tôi, phước hạnh và ơn thương xót sẽ theo tôi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.250Z",
      "updatedAt": "2024-09-24T01:54:59.250Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    },
    {
      "id": "19b26d57-00e9-4d3b-9477-045f91b43b4c",
      "number": 6,
      "content": "Tôi sẽ ở trong nhà của Chúa cho đến đời đời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:54:59.255Z",
      "updatedAt": "2024-09-24T01:54:59.255Z",
      "chapterId": "f07566d1-1bfd-4d0d-9be9-a0b7144ca96b"
    }
  ],
