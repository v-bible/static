{
  "chapter": {
    "id": "9e2d8af3-3505-46e3-985b-eb82ac856009",
    "number": 113,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20113&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.672Z",
