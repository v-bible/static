  },
  "verses": [
    {
      "id": "8043300b-cd13-428e-b6bd-738c94940d55",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.582Z",
      "updatedAt": "2024-09-24T02:08:40.582Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "0f882319-8897-44a1-a8a1-c64381b0c33b",
      "number": 9,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.630Z",
      "updatedAt": "2024-09-24T02:08:40.630Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "4e8d1f9d-94c2-4cb7-9784-afa6f9bfa5dc",
      "number": 1,
      "content": "Hỡi các tôi tớ Chúa, hãy ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.593Z",
      "updatedAt": "2024-09-24T02:08:40.593Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "165d00ea-8797-447b-835e-a1ffc373f530",
      "number": 1,
      "content": "Hãy ca ngợi danh Chúa.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.595Z",
      "updatedAt": "2024-09-24T02:08:40.595Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "91e5e542-19a5-46df-9ca0-5f42514d628b",
      "number": 2,
      "content": "Hãy chúc tụng danh Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.598Z",
      "updatedAt": "2024-09-24T02:08:40.598Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "59b864f8-1eac-41bf-9a6e-be9901b7c73d",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.600Z",
      "updatedAt": "2024-09-24T02:08:40.600Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "4733be0b-1159-45e1-a6b0-52a6bc894573",
      "number": 3,
      "content": "Từ bình minh cho đến hoàng hôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.601Z",
      "updatedAt": "2024-09-24T02:08:40.601Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "1b06bca8-0fdc-4ef5-8e88-5a3f2f9ab26d",
      "number": 3,
      "content": "Danh Chúa phải được tôn vinh ca ngợi.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.603Z",
      "updatedAt": "2024-09-24T02:08:40.603Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "c93fa0e4-a13d-47d7-b645-db0fa6ac4353",
      "number": 4,
      "content": "Chúa là Ðấng siêu việt hơn tất cả các dân các nước;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.606Z",
      "updatedAt": "2024-09-24T02:08:40.606Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "539e4089-11f7-4722-baa3-8c73a3561a4a",
      "number": 4,
      "content": "Vinh quang Ngài trỗi cao hơn các tầng trời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.608Z",
      "updatedAt": "2024-09-24T02:08:40.608Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "cb5654b2-6f3a-489f-939d-f40558bb0b86",
      "number": 5,
      "content": "Có ai giống như Chúa, Ðức Chúa Trời chúng ta,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.610Z",
      "updatedAt": "2024-09-24T02:08:40.610Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "523cf437-3d42-49fc-bdb3-907583bf0ad9",
      "number": 5,
      "content": "Ðấng ngự trên ngai cao cả,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.612Z",
      "updatedAt": "2024-09-24T02:08:40.612Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "12745bb8-b5c5-40b6-aaf2-fa1d6d30d4a4",
      "number": 6,
      "content": "Lại tự hạ mình xuống,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.614Z",
      "updatedAt": "2024-09-24T02:08:40.614Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "625bcedf-7c39-42b1-aa4c-017313cfe29c",
      "number": 6,
      "content": "Ðể đoái xem mọi vật trong bầu trời và trên mặt đất chăng?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.616Z",
      "updatedAt": "2024-09-24T02:08:40.616Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "ca5e55ec-312e-4089-8447-ebe371174922",
      "number": 7,
      "content": "Ngài nâng đỡ kẻ nghèo khổ ra khỏi chốn bụi tro,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.618Z",
      "updatedAt": "2024-09-24T02:08:40.618Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "186672e0-d6c3-4434-a4b0-32feb06af400",
      "number": 7,
      "content": "Và nhấc đưa kẻ khó nghèo ra khỏi gò rác rến.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.620Z",
      "updatedAt": "2024-09-24T02:08:40.620Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "fb706163-9e82-4a4f-965d-7fa2f4f6f280",
      "number": 8,
      "content": "Ngài đặt họ ngồi ngang hàng với các bậc quan quyền,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.621Z",
      "updatedAt": "2024-09-24T02:08:40.621Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "5e440af1-d9dc-491a-9117-81ac0c9af496",
      "number": 8,
      "content": "Tức với những người quyền thế của con dân Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.624Z",
      "updatedAt": "2024-09-24T02:08:40.624Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "22dbaa66-0589-4a4e-b0aa-5e5f7e467e8a",
      "number": 9,
      "content": "Ngài làm cho người đàn bà son sẻ sống quạnh quẽ trong nhà",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.625Z",
      "updatedAt": "2024-09-24T02:08:40.625Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    },
    {
      "id": "0b77855a-cfbc-43d6-8d56-dc1a419445f4",
      "number": 9,
      "content": "Thành người mẹ hiền vui vẻ của một đàn con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:40.628Z",
      "updatedAt": "2024-09-24T02:08:40.628Z",
      "chapterId": "9e2d8af3-3505-46e3-985b-eb82ac856009"
    }
  ],
