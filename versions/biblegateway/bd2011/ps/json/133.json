  },
  "verses": [
    {
      "id": "9f9cbcd0-8265-4f0a-9afb-3c99567d51fd",
      "number": 1,
      "content": "Kìa, tốt đẹp thay và phước hạnh thay khi anh chị em sống hòa thuận với nhau!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:35.931Z",
      "updatedAt": "2024-09-24T02:11:35.931Z",
      "chapterId": "0eb7c4b9-a7b7-4c4e-aa8d-5b915bdeed8b"
    },
    {
      "id": "6b330e34-5a2a-462a-88b4-139f85056f4b",
      "number": 2,
      "content": "Ðiều ấy chẳng khác gì dầu quý báu từ trên đầu chảy xuống,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:35.953Z",
      "updatedAt": "2024-09-24T02:11:35.953Z",
      "chapterId": "0eb7c4b9-a7b7-4c4e-aa8d-5b915bdeed8b"
    },
    {
      "id": "9b80b863-3868-477c-bed6-f961ee36c241",
      "number": 2,
      "content": "Chảy xuống râu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:35.959Z",
      "updatedAt": "2024-09-24T02:11:35.959Z",
      "chapterId": "0eb7c4b9-a7b7-4c4e-aa8d-5b915bdeed8b"
    },
    {
      "id": "085856d4-2bea-400d-ae51-c22d4c58c59c",
      "number": 2,
      "content": "Chảy xuống râu của A-rôn,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:35.964Z",
      "updatedAt": "2024-09-24T02:11:35.964Z",
      "chapterId": "0eb7c4b9-a7b7-4c4e-aa8d-5b915bdeed8b"
    },
    {
      "id": "f7c0929e-5032-4cd0-a85f-56c8708178b8",
      "number": 2,
      "content": "Rồi tiếp tục chảy xuống vạt áo ông.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:35.969Z",
      "updatedAt": "2024-09-24T02:11:35.969Z",
      "chapterId": "0eb7c4b9-a7b7-4c4e-aa8d-5b915bdeed8b"
    },
    {
      "id": "bf667144-05f4-4373-98f3-36752087e20d",
      "number": 3,
      "content": "Ðiều ấy giống như sương móc từ Núi Hẹt-môn sa xuống các rặng núi Si-ôn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:35.974Z",
      "updatedAt": "2024-09-24T02:11:35.974Z",
      "chapterId": "0eb7c4b9-a7b7-4c4e-aa8d-5b915bdeed8b"
    },
    {
      "id": "2f15e5e9-8041-4f16-b0ae-8581793b74af",
      "number": 3,
      "content": "Vì tại đó Chúa ban phước,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:35.979Z",
      "updatedAt": "2024-09-24T02:11:35.979Z",
      "chapterId": "0eb7c4b9-a7b7-4c4e-aa8d-5b915bdeed8b"
    },
    {
      "id": "020d75f8-bd3e-4871-a70f-969b892d04fe",
      "number": 3,
      "content": "Phước của sự sống đời đời.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:35.984Z",
      "updatedAt": "2024-09-24T02:11:35.984Z",
      "chapterId": "0eb7c4b9-a7b7-4c4e-aa8d-5b915bdeed8b"
    }
  ],
