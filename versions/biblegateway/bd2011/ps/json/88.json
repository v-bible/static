{
  "chapter": {
    "id": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535",
    "number": 88,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2088&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.156Z",
    "updatedAt": "2024-09-24T01:00:44.156Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "177f7085-0b29-4a28-845f-ea7f2ee5d392",
      "number": 1,
      "content": "Lạy Chúa, Ðức Chúa Trời, Ðấng Giải Cứu của con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.166Z",
      "updatedAt": "2024-09-24T02:05:02.166Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "2f0d7c3e-3a5f-4fbb-ae31-a5c9be78ec93",
      "number": 1,
      "content": "Ngày và đêm con kêu cầu trước thánh nhan Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.171Z",
      "updatedAt": "2024-09-24T02:05:02.171Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "c9f29eaa-de17-4e6e-8695-964220407029",
      "number": 2,
      "content": "Nguyện lời cầu nguyện của con thấu đến trước mặt Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.173Z",
      "updatedAt": "2024-09-24T02:05:02.173Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "b7142cca-514f-4100-a32b-5b1b9157037f",
      "number": 2,
      "content": "Xin nghiêng tai Ngài nghe tiếng kêu cầu của con,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.175Z",
      "updatedAt": "2024-09-24T02:05:02.175Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "b4f2b7cc-dc0e-410f-a8bb-31a7a0ba984d",
      "number": 3,
      "content": "Vì linh hồn con đang dẫy đầy hoạn nạn,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.177Z",
      "updatedAt": "2024-09-24T02:05:02.177Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "77a06744-a24e-42d6-9f04-2f7f3876a5c6",
      "number": 3,
      "content": "Và mạng sống con đang cận kề âm phủ.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.180Z",
      "updatedAt": "2024-09-24T02:05:02.180Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "f88b47b2-73ce-47f4-bd37-855ca385b19b",
      "number": 4,
      "content": "Con bị xem như kẻ sắp đi vào huyệt mả;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.182Z",
      "updatedAt": "2024-09-24T02:05:02.182Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "5313e2ac-e113-4684-8431-f3177de34ced",
      "number": 4,
      "content": "Contrở thành một kẻ chẳng còn chút sức lực nào;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.184Z",
      "updatedAt": "2024-09-24T02:05:02.184Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "c02cb85b-a5cb-4421-aad8-9eef7e1be03f",
      "number": 5,
      "content": "Con bị lãng quên như những người đã chết,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.186Z",
      "updatedAt": "2024-09-24T02:05:02.186Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "60fbdac4-52f8-48e9-80f4-8cdfd4f4316c",
      "number": 5,
      "content": "Như những kẻ đã nằm trong phần mộ từ lâu,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.188Z",
      "updatedAt": "2024-09-24T02:05:02.188Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "6d31d25e-ae9c-47b2-9207-9217d8e6e4c2",
      "number": 5,
      "content": "Những kẻ Ngài không còn nhớ đến nữa,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.191Z",
      "updatedAt": "2024-09-24T02:05:02.191Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "04fcdf20-1e83-41ec-bacd-09b39ca80134",
      "number": 5,
      "content": "Và như những người đã bị dứt bỏ khỏi tay Ngài.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.193Z",
      "updatedAt": "2024-09-24T02:05:02.193Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "e560b89c-3ea4-490e-b589-71a63460454f",
      "number": 6,
      "content": "Ngài đã bỏ con vào một huyệt sâu không đáy,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.195Z",
      "updatedAt": "2024-09-24T02:05:02.195Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "4f5ca782-d019-4dde-910b-b4d16df770f8",
      "number": 6,
      "content": "Vào chỗ cực kỳ tối tăm trong nơi tận cùng sâu thẳm.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 13,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.197Z",
      "updatedAt": "2024-09-24T02:05:02.197Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "cc88cac9-020e-4bce-bd9b-76efa2dd9f0c",
      "number": 7,
      "content": "Cơn thịnh nộ của Ngài đã giáng trên con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.199Z",
      "updatedAt": "2024-09-24T02:05:02.199Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "db356e36-1e0a-46b1-9a8f-95f18ae7662e",
      "number": 7,
      "content": "Ngài đã phủ trên con tất cả các đợt sóng của Ngài. *(Sê-la)*",
      "order": 1,
      "parNumber": 0,
      "parIndex": 15,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.202Z",
      "updatedAt": "2024-09-24T02:05:02.202Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "d0dfcfc1-b69a-4f6d-8921-0693c61f92c2",
      "number": 8,
      "content": "Ngài đã làm cho những người thân quen của con xa lánh con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.204Z",
      "updatedAt": "2024-09-24T02:05:02.204Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "347ef896-8b47-4125-82d9-ec3d532e1a8b",
      "number": 8,
      "content": "Ngài đã khiến con thành một đối tượng để họ ghê tởm;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.206Z",
      "updatedAt": "2024-09-24T02:05:02.206Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "ae48c307-3037-456c-85cd-4fb780d45c5d",
      "number": 8,
      "content": "Con đã bị cô lập và không còn lối thoát.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.208Z",
      "updatedAt": "2024-09-24T02:05:02.208Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "32fae4d6-8088-4446-8c8f-a99a10ebe2a4",
      "number": 9,
      "content": "Mắt con thẫn thờ vì sầu khổ;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.212Z",
      "updatedAt": "2024-09-24T02:05:02.212Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "494ba4fe-2d59-4a5d-8cf0-60c0207fc04b",
      "number": 9,
      "content": "Chúa ôi, hằng ngày con kêu cầu Ngài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.214Z",
      "updatedAt": "2024-09-24T02:05:02.214Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "bda89b67-cdc5-4ee8-9349-2cdf9f94cf7b",
      "number": 9,
      "content": "Con đưa đôi tay con lên và hướng về Ngài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.216Z",
      "updatedAt": "2024-09-24T02:05:02.216Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "df088d01-cfc5-4184-a273-0d4780e3d2a0",
      "number": 10,
      "content": "Chẳng lẽ Ngài sẽ thực hiện phép lạ cho những người đã chết sao?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.218Z",
      "updatedAt": "2024-09-24T02:05:02.218Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "74f040c7-b667-44e0-9b8f-98bec006ee83",
      "number": 10,
      "content": "Những linh hồn đã xuất ra rồi lẽ nào còn có thể trỗi dậy để ca ngợi Ngài sao?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.221Z",
      "updatedAt": "2024-09-24T02:05:02.221Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "6506cf61-187a-486a-8785-7b36fca529a2",
      "number": 10,
      "content": "*(Sê-la)*",
      "order": 2,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.225Z",
      "updatedAt": "2024-09-24T02:05:02.225Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "91cfbf05-4bd0-4b53-a581-fc8da4fabc85",
      "number": 11,
      "content": "Tình thương của Ngài có thể được công bố trong phần mộ sao?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.227Z",
      "updatedAt": "2024-09-24T02:05:02.227Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "0a28c3a6-b47d-4339-903d-c969b203ade6",
      "number": 11,
      "content": "Ðức thành tín của Ngài sẽ được giảng ra trong nơi người ta bị hủy diệt sao?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.229Z",
      "updatedAt": "2024-09-24T02:05:02.229Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "444151e9-6cbb-4890-b898-dc371966d0b3",
      "number": 12,
      "content": "Cõi u minh làm sao biết được những việc diệu kỳ của Ngài?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.232Z",
      "updatedAt": "2024-09-24T02:05:02.232Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "4e9189a0-e7c8-4fa1-ba2c-8cc5da305bc3",
      "number": 12,
      "content": "Ðức công chính của Ngài há sẽ được biết đến trong cõi của những kẻ đã bị lãng quên sao?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.234Z",
      "updatedAt": "2024-09-24T02:05:02.234Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "9703c5fa-71ec-4de6-ae30-11c776bf23e8",
      "number": 13,
      "content": "Còn con, Chúa ôi, con kêu cầu Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.236Z",
      "updatedAt": "2024-09-24T02:05:02.236Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "5ed976f5-12b2-4c88-9bfe-7d156c023903",
      "number": 13,
      "content": "Vừa sớm tinh sương con dâng lời cầu nguyện lên trước thánh nhan Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.238Z",
      "updatedAt": "2024-09-24T02:05:02.238Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "82f4d85b-3540-43ab-ac17-f3e2bae2169c",
      "number": 14,
      "content": "Chúa ôi, sao Ngài nỡ từ bỏ con?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.240Z",
      "updatedAt": "2024-09-24T02:05:02.240Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "bd6ceac3-a15b-4df1-a154-e49710386dfc",
      "number": 14,
      "content": "Sao Ngài nỡ ẩn mặt Ngài khỏi con?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.242Z",
      "updatedAt": "2024-09-24T02:05:02.242Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "c759face-011c-4029-b9ef-bd8e5404e18e",
      "number": 15,
      "content": "Con đã bị đau đớn và gần chết từ thời niên thiếu;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.244Z",
      "updatedAt": "2024-09-24T02:05:02.244Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "3a55cfa1-d50d-41cc-9c4a-59a6e10aa220",
      "number": 15,
      "content": "Con bị những trận đòn kinh hoàng của Ngài nên đâm ra tuyệt vọng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.246Z",
      "updatedAt": "2024-09-24T02:05:02.246Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "e89769cc-4887-4945-a91b-b7c04e8d9f7c",
      "number": 16,
      "content": "Cơn giận phừng phừng của Ngài đã giáng trên con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.248Z",
      "updatedAt": "2024-09-24T02:05:02.248Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "086a191e-4bb7-4eea-9600-abfa75c21174",
      "number": 16,
      "content": "Những cơn hãi hùng của Ngài đã tàn phá con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.250Z",
      "updatedAt": "2024-09-24T02:05:02.250Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "46487698-29ef-475c-8b43-c513695f8228",
      "number": 17,
      "content": "Những điều ấy bao phủ con như nước lụt tràn ngập cả ngày;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:05:02.254Z",
      "updatedAt": "2024-09-24T02:05:02.254Z",
      "chapterId": "d1dabce0-2dae-40c9-8eaf-fc8ef4ecb535"
    },
    {
      "id": "4c0ae891-b413-4666-b5ae-aaa255040112",
      "number": 17,
      "content": "Chúng cứ cùng nhau vây chặt lấy con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
