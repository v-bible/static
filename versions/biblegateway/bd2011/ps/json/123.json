{
  "chapter": {
    "id": "3b09d373-52a8-442a-8a41-d74db576127e",
    "number": 123,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20123&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.878Z",
    "updatedAt": "2024-09-24T01:00:44.878Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "e40ae3c5-04c8-4d9d-b2e9-c8ec5dc25766",
      "number": 1,
      "content": "Lạy Ðấng ngự trên các tầng trời,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:18.590Z",
      "updatedAt": "2024-09-24T02:10:18.590Z",
      "chapterId": "3b09d373-52a8-442a-8a41-d74db576127e"
    },
    {
      "id": "3d73ad4f-0a3d-4048-b277-71ec8bf22eae",
      "number": 1,
      "content": "Con ngước mắt lên hướng về Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:18.606Z",
      "updatedAt": "2024-09-24T02:10:18.606Z",
      "chapterId": "3b09d373-52a8-442a-8a41-d74db576127e"
    },
    {
      "id": "604b9dd4-4d84-43fb-bc64-1414038b576f",
      "number": 2,
      "content": "Kìa, như mắt các đầy tớ trông nơi tay ông chủ họ thể nào,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:18.611Z",
      "updatedAt": "2024-09-24T02:10:18.611Z",
      "chapterId": "3b09d373-52a8-442a-8a41-d74db576127e"
    },
    {
      "id": "b8bc6db0-6bb7-4392-baff-520d7b3625d1",
      "number": 2,
      "content": "Như mắt người tớ gái trông nơi tay bà chủ nàng thể nào,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:18.616Z",
      "updatedAt": "2024-09-24T02:10:18.616Z",
      "chapterId": "3b09d373-52a8-442a-8a41-d74db576127e"
    },
    {
      "id": "8829b8c6-08de-4e03-8a54-f960c17bb016",
      "number": 2,
      "content": "Mắt chúng con cũng vọng trông nơi Chúa, Ðức Chúa Trời chúng con, thể ấy,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:18.621Z",
      "updatedAt": "2024-09-24T02:10:18.621Z",
      "chapterId": "3b09d373-52a8-442a-8a41-d74db576127e"
    },
    {
      "id": "baae0464-a115-4a87-a6ca-4ad2aba8893f",
      "number": 2,
      "content": "Cho đến khi Ngài bày tỏ ơn thương xót đối với chúng con.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:18.626Z",
      "updatedAt": "2024-09-24T02:10:18.626Z",
      "chapterId": "3b09d373-52a8-442a-8a41-d74db576127e"
    },
    {
      "id": "982876ae-05ea-436d-af7c-2d2b481ab86f",
      "number": 3,
      "content": "Xin thương xót chúng con, Chúa ôi, xin thương xót chúng con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:18.631Z",
      "updatedAt": "2024-09-24T02:10:18.631Z",
      "chapterId": "3b09d373-52a8-442a-8a41-d74db576127e"
    },
    {
      "id": "7458ca4f-2ef2-4bfd-9683-c2ca1c1d7765",
      "number": 3,
      "content": "Vì chúng con bị khinh khi quá đỗi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:18.636Z",
      "updatedAt": "2024-09-24T02:10:18.636Z",
      "chapterId": "3b09d373-52a8-442a-8a41-d74db576127e"
    },
    {
      "id": "18f6ee27-9179-4d49-bf12-3630dc19a754",
      "number": 4,
      "content": "Linh hồn chúng con quá ngao ngán những lời nhạo báng của đám người kiêu ngạo,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:18.641Z",
      "updatedAt": "2024-09-24T02:10:18.641Z",
      "chapterId": "3b09d373-52a8-442a-8a41-d74db576127e"
    },
    {
      "id": "073307fb-5868-4bda-b6e2-717a8c409275",
      "number": 4,
      "content": "Cùng với giọng khinh người của những kẻ kiêu căng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:18.646Z",
      "updatedAt": "2024-09-24T02:10:18.646Z",
      "chapterId": "3b09d373-52a8-442a-8a41-d74db576127e"
    }
  ],
