    },
    {
      "id": "d95e0144-3e17-448a-9fba-301df5240277",
      "content": "ctd: quân khủng bố phá hoại ban đêm",
      "position": 47,
      "order": 1,
      "createdAt": "2024-09-24T02:05:30.811Z",
      "updatedAt": "2024-09-24T02:05:30.811Z",
      "verseId": "e5e3208e-843b-4733-9d01-5af4cf3624d9",
      "chapterId": "4f42a121-e9d6-4e7d-9637-eb1db28bedbb"
    },
    {
      "id": "a8b13109-5d35-4c5e-868c-ebdae2ef83c0",
      "content": "ctd: tên bay đạn réo ban ngày",
      "position": 22,
      "order": 2,
      "createdAt": "2024-09-24T02:05:30.815Z",
      "updatedAt": "2024-09-24T02:05:30.815Z",
      "verseId": "febc652a-7831-4810-b03c-295e2cc82648",
      "chapterId": "4f42a121-e9d6-4e7d-9637-eb1db28bedbb"
    },
