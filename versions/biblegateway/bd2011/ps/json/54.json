{
  "chapter": {
    "id": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2",
    "number": 54,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2054&version=BD2011",
    "createdAt": "2024-09-24T01:00:43.459Z",
    "updatedAt": "2024-09-24T01:00:43.459Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "18296149-59ea-4536-ad41-2138c9e797ac",
      "number": 1,
      "content": "Ðức Chúa Trời ôi, xin cứu con, bằng uy danh Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.032Z",
      "updatedAt": "2024-09-24T01:59:56.032Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "dd1b04fc-924d-4a67-9484-2ee86f9fdb9f",
      "number": 1,
      "content": "Xin minh oan cho con, bằng quyền năng Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.045Z",
      "updatedAt": "2024-09-24T01:59:56.045Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "7eab6d02-fc76-4c43-b413-3b484a00c311",
      "number": 2,
      "content": "Ðức Chúa Trời ôi, xin lắng nghe lời cầu nguyện của con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.047Z",
      "updatedAt": "2024-09-24T01:59:56.047Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "ca1a5c42-b1bd-47b6-a998-1b9fc6f501e6",
      "number": 2,
      "content": "Xin nghiêng tai nghe tiếng của miệng con.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.050Z",
      "updatedAt": "2024-09-24T01:59:56.050Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "1ac2a69b-8bf2-428c-806a-2267c27a8f89",
      "number": 3,
      "content": "Vì những kẻ xa lạ đã nổi lên chống lại con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.053Z",
      "updatedAt": "2024-09-24T01:59:56.053Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "d27b1d74-20bd-4a7f-8023-e82588fc778e",
      "number": 3,
      "content": "Những kẻ hung bạo đã tìm cách hại mạng sống con;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.059Z",
      "updatedAt": "2024-09-24T01:59:56.059Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "ed885479-562e-4f8b-8495-d05b0f2d65b1",
      "number": 3,
      "content": "Quả là chúng chẳng coi Ðức Chúa Trời ra gì. *(Sê-la)*",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.064Z",
      "updatedAt": "2024-09-24T01:59:56.064Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "36c38b4f-4d7e-49bf-ab59-473488ee2a9f",
      "number": 4,
      "content": "Này, Ðức Chúa Trời là Ðấng giúp đỡ tôi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.070Z",
      "updatedAt": "2024-09-24T01:59:56.070Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "da6b6081-4333-49b2-b2ad-62c702a24516",
      "number": 4,
      "content": "Chúa là Ðấng gìn giữ mạng sống tôi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.075Z",
      "updatedAt": "2024-09-24T01:59:56.075Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "3a7469cf-dea6-4faa-b1b2-51fab53bd23f",
      "number": 5,
      "content": "Ngài sẽ báo trả điều ác trên kẻ thù của tôi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.081Z",
      "updatedAt": "2024-09-24T01:59:56.081Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "41585e28-a443-448e-ac68-a97ccd21b1eb",
      "number": 5,
      "content": "Tiêu diệt chúng vì đức thành tín của Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.086Z",
      "updatedAt": "2024-09-24T01:59:56.086Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "61d2b1f0-70ce-40df-aace-8b50ce8654cc",
      "number": 6,
      "content": "Con sẽ đem của lễ lạc ý dâng lên Ngài với tất cả niềm vui;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.091Z",
      "updatedAt": "2024-09-24T01:59:56.091Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "d9e1429b-e1f5-424e-8636-904306af2d24",
      "number": 6,
      "content": "Con sẽ cảm tạ danh Ngài, Chúa ôi, vì đó là điều tốt đẹp,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.097Z",
      "updatedAt": "2024-09-24T01:59:56.097Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "94810a17-762d-49fe-802d-cfe986d79c39",
      "number": 7,
      "content": "Vì Ngài đã giải cứu con khỏi mọi hoạn nạn,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.102Z",
      "updatedAt": "2024-09-24T01:59:56.102Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    },
    {
      "id": "1ec9929d-576d-472d-b00f-3b7f00282b79",
      "number": 7,
      "content": "Và mắt con được nhìn thấy những kẻ thù của con bị bại.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:59:56.107Z",
      "updatedAt": "2024-09-24T01:59:56.107Z",
      "chapterId": "9b3971d9-8b94-4430-8405-8f8e6b5aa0c2"
    }
  ],
