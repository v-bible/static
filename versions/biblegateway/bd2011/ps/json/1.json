{
  "chapter": {
    "id": "26aabf07-301a-4392-92a1-ff72a58a589d",
    "number": 1,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%201&version=BD2011",
    "createdAt": "2024-09-24T01:00:42.374Z",
    "updatedAt": "2024-09-24T01:00:42.374Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "d784057d-8a3d-431b-85a9-f07cfd929efa",
      "number": 1,
      "content": "Phước cho người không đi theo kế kẻ gian ác,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.458Z",
      "updatedAt": "2024-09-24T01:51:16.458Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "b19c4881-9349-4100-b515-a2b9501170df",
      "number": 1,
      "content": "Không đứng chung đường với phường tội lỗi,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.485Z",
      "updatedAt": "2024-09-24T01:51:16.485Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "eaf6aaee-6183-4501-b2a2-8bda051d016b",
      "number": 1,
      "content": "Không ngồi cùng chỗ với đồ nhạo báng,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.489Z",
      "updatedAt": "2024-09-24T01:51:16.489Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "82226aab-e8cf-4581-b1e6-fbc6fc4887fe",
      "number": 2,
      "content": "Nhưng vui thích về luật pháp của Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.493Z",
      "updatedAt": "2024-09-24T01:51:16.493Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "a15144c0-437c-403a-a889-eb1332b26f95",
      "number": 2,
      "content": "Và suy gẫm luật pháp ấy ngày và đêm.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.498Z",
      "updatedAt": "2024-09-24T01:51:16.498Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "c499a2e4-b700-4ec0-92c5-94a7c2132979",
      "number": 3,
      "content": "Người ấy sẽ như cây trồng gần dòng nước,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.502Z",
      "updatedAt": "2024-09-24T01:51:16.502Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "287d6264-2efe-4bd0-902d-4db50f93291e",
      "number": 3,
      "content": "Cứ đến mùa được hoa quả đầy cành,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.506Z",
      "updatedAt": "2024-09-24T01:51:16.506Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "ead4f397-6f26-4d67-9511-dca4e93528bb",
      "number": 3,
      "content": "Lá vẫn xanh chứ chẳng hề héo úa;",
      "order": 2,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.510Z",
      "updatedAt": "2024-09-24T01:51:16.510Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "9b4ed99e-b3cb-406d-b4c6-3122421c2680",
      "number": 3,
      "content": "Mọi sự người ấy làm đều sẽ thành công.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.518Z",
      "updatedAt": "2024-09-24T01:51:16.518Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "7049216e-fa5a-4731-9bcb-506753e789e2",
      "number": 4,
      "content": "Những kẻ gian ác sẽ không được như vậy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.522Z",
      "updatedAt": "2024-09-24T01:51:16.522Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "9b960556-6aaa-4fe4-89c4-80692e1464a9",
      "number": 4,
      "content": "Nhưng chúng chẳng khác gì rác rến bị gió đùa đi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.526Z",
      "updatedAt": "2024-09-24T01:51:16.526Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "4ff3650f-c3a3-467b-b4cb-3dc4d869883e",
      "number": 5,
      "content": "Vì thế những kẻ gian ác sẽ chẳng đứng nổi trong ngày phán xét;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.531Z",
      "updatedAt": "2024-09-24T01:51:16.531Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "802f1b3d-3608-47c3-84e5-a0fc13434eea",
      "number": 5,
      "content": "Những kẻ tội lỗi sẽ không được vào hội người ngay lành;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.534Z",
      "updatedAt": "2024-09-24T01:51:16.534Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "72312ab6-db04-4479-a1e9-62c1f9a7a292",
      "number": 6,
      "content": "Vì Chúa biết rõ đường lối người ngay lành,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.538Z",
      "updatedAt": "2024-09-24T01:51:16.538Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    },
    {
      "id": "4ed88d6c-cdf4-465b-8b88-e877bf6c0864",
      "number": 6,
      "content": "Còn đường lối những kẻ gian ác sẽ dẫn đến diệt vong.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T01:51:16.542Z",
      "updatedAt": "2024-09-24T01:51:16.542Z",
      "chapterId": "26aabf07-301a-4392-92a1-ff72a58a589d"
    }
  ],
