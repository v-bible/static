{
  "chapter": {
    "id": "602b5da1-1401-4dec-abb8-bf97c1e3c843",
    "number": 128,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20128&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.976Z",
