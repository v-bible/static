  },
  "verses": [
    {
      "id": "9791df0e-9202-4b58-b8c6-12413f2128f3",
      "number": 1,
      "content": "Phước cho người kính sợ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:00.598Z",
      "updatedAt": "2024-09-24T02:11:00.598Z",
      "chapterId": "602b5da1-1401-4dec-abb8-bf97c1e3c843"
    },
    {
      "id": "871bdc5d-513e-49a0-875a-a7a6cfccf9ba",
      "number": 1,
      "content": "Người đi trong đường lối Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:00.614Z",
      "updatedAt": "2024-09-24T02:11:00.614Z",
      "chapterId": "602b5da1-1401-4dec-abb8-bf97c1e3c843"
    },
    {
      "id": "1675dd56-fb2e-4984-b79f-17ebdd4a1489",
      "number": 2,
      "content": "Bạn sẽ hưởng hoa lợi của tay mình;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:00.619Z",
      "updatedAt": "2024-09-24T02:11:00.619Z",
      "chapterId": "602b5da1-1401-4dec-abb8-bf97c1e3c843"
    },
    {
      "id": "4a6d7ed6-ecee-4345-b0aa-9f24dde8a13b",
      "number": 2,
      "content": "Bạn sẽ được phước và thịnh vượng.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:00.624Z",
      "updatedAt": "2024-09-24T02:11:00.624Z",
      "chapterId": "602b5da1-1401-4dec-abb8-bf97c1e3c843"
    },
    {
      "id": "7abe9b2d-2d07-4266-b539-189cd15b6613",
      "number": 3,
      "content": "Vợ bạn sẽ như cây nho sai trái trong nhà bạn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:00.629Z",
      "updatedAt": "2024-09-24T02:11:00.629Z",
      "chapterId": "602b5da1-1401-4dec-abb8-bf97c1e3c843"
    },
    {
      "id": "9efb1070-60cf-4d3d-b174-da27247dcecb",
      "number": 3,
      "content": "Con cái bạn sẽ như các chồi ô-liu quanh bàn bạn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:00.634Z",
      "updatedAt": "2024-09-24T02:11:00.634Z",
      "chapterId": "602b5da1-1401-4dec-abb8-bf97c1e3c843"
    },
    {
      "id": "7478acfa-84a7-4189-8f26-9f0f103e212e",
      "number": 4,
      "content": "Kìa, người nào kính sợ Chúa sẽ được phước như vậy.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:00.639Z",
      "updatedAt": "2024-09-24T02:11:00.639Z",
      "chapterId": "602b5da1-1401-4dec-abb8-bf97c1e3c843"
    },
    {
      "id": "1b25353b-bf67-435c-803e-e46e065a901c",
      "number": 5,
      "content": "Nguyện Chúa ban phước cho bạn từ Si-ôn.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:00.643Z",
      "updatedAt": "2024-09-24T02:11:00.643Z",
      "chapterId": "602b5da1-1401-4dec-abb8-bf97c1e3c843"
    },
    {
      "id": "3b9af619-aca7-4e18-9287-ec517c4a2c4c",
      "number": 5,
      "content": "Nguyện suốt đời bạn chứng kiến những phước hạnh của Giê-ru-sa-lem.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:00.649Z",
      "updatedAt": "2024-09-24T02:11:00.649Z",
      "chapterId": "602b5da1-1401-4dec-abb8-bf97c1e3c843"
    },
    {
      "id": "9ea397f6-bddb-4e38-a4a1-65222ddbb497",
      "number": 6,
      "content": "Nguyện bạn được nhìn thấy con cháu của con cái bạn.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:00.654Z",
      "updatedAt": "2024-09-24T02:11:00.654Z",
      "chapterId": "602b5da1-1401-4dec-abb8-bf97c1e3c843"
    },
    {
      "id": "dfa34ac0-b7dd-49ca-8957-d5ff829e175a",
      "number": 6,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:00.659Z",
      "updatedAt": "2024-09-24T02:11:00.659Z",
      "chapterId": "602b5da1-1401-4dec-abb8-bf97c1e3c843"
    }
  ],
