      "order": 0,
      "parNumber": 1,
      "parIndex": 20,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.884Z",
      "updatedAt": "2024-09-24T02:11:56.884Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "5177a39f-ce9d-4985-8be4-c5edcb311dd9",
      "number": 15,
      "content": "Nhưng Ngài lật đổ Pha-ra-ôn và đạo quân vua ấy xuống Hồng Hải,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 22,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.889Z",
      "updatedAt": "2024-09-24T02:11:56.889Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "9a648a68-ceff-423d-83fb-fdee5c24f56b",
      "number": 16,
      "content": "Ðấng dẫn dắt dân Ngài đi qua đồng hoang,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 24,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.898Z",
      "updatedAt": "2024-09-24T02:11:56.898Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "968d143a-ef26-4454-9bc1-ab6efb86629e",
      "number": 17,
      "content": "Ðấng đánh bại các vua lớn,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 26,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.908Z",
      "updatedAt": "2024-09-24T02:11:56.908Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "9c929976-ff9e-4527-a733-dd0921f48cbc",
      "number": 18,
      "content": "Và tiêu diệt những vua mạnh,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 28,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.918Z",
      "updatedAt": "2024-09-24T02:11:56.918Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "72b59b64-65e3-4b17-94be-f0cb0242dfa6",
      "number": 19,
      "content": "Ðó là Si-hôn vua dân A-mô-ri,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 30,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.928Z",
      "updatedAt": "2024-09-24T02:11:56.928Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "fd5154e6-7b97-4b76-abbb-91f0a6214ced",
      "number": 20,
      "content": "Và Óc vua Ba-san,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 32,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.938Z",
      "updatedAt": "2024-09-24T02:11:56.938Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "15f688b2-fc95-4fc2-a4b6-06deacbe12e1",
      "number": 21,
      "content": "Rồi ban xứ của họ để làm sản nghiệp,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 34,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.948Z",
      "updatedAt": "2024-09-24T02:11:56.948Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "389573b8-a532-4ddd-bba1-3a8cd6e119db",
      "number": 22,
      "content": "Tức làm sản nghiệp cho I-sơ-ra-ên tôi tớ Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 36,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.958Z",
      "updatedAt": "2024-09-24T02:11:56.958Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "1cac1555-96a2-4d32-ab6c-a21eef02a14c",
      "number": 23,
      "content": "Ðấng nhớ đến thân phận thấp hèn của chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.968Z",
      "updatedAt": "2024-09-24T02:11:56.968Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "e67e4e0d-a122-492f-b1fe-9421fb0ac4e1",
      "number": 24,
      "content": "Và giải cứu chúng ta khỏi những kẻ thù của chúng ta,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.978Z",
      "updatedAt": "2024-09-24T02:11:56.978Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "e397bb1b-fc08-44d9-b70a-740256a0238f",
      "number": 25,
      "content": "Ðấng ban thực phẩm cho mọi loài xác thịt,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.991Z",
      "updatedAt": "2024-09-24T02:11:56.991Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    },
    {
      "id": "ccc9fc43-34b1-4b94-a64d-99dcfc0f40fb",
      "number": 26,
      "content": "Hãy cảm tạ Ðức Chúa Trời của các tầng trời,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:57.004Z",
      "updatedAt": "2024-09-24T02:11:57.004Z",
      "chapterId": "40435f36-54d3-4059-a730-bc3e2ad6f932"
    }
  ],
