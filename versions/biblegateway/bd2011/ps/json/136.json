      "number": 11,
      "content": "Ðấng đem dân I-sơ-ra-ên ra khỏi xứ ấy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 14,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:56.868Z",
