{
  "chapter": {
    "id": "41cef2a4-24b8-42ad-b245-8600bf82b286",
    "number": 114,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20114&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.694Z",
    "updatedAt": "2024-09-24T01:00:44.694Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "341aa43d-3019-4735-ad34-013d74b28df6",
      "number": 1,
      "content": "Khi dân I-sơ-ra-ên ra khỏi Ai-cập,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.096Z",
      "updatedAt": "2024-09-24T02:08:48.096Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "9cab42cf-38d5-4d0c-bc16-04123ab9db7d",
      "number": 1,
      "content": "Nhà Gia-cốp ra khỏi một dân nói một ngôn ngữ lạ,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.108Z",
      "updatedAt": "2024-09-24T02:08:48.108Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "981f252b-6eba-43cb-a264-462f7d83c42f",
      "number": 2,
      "content": "Giu-đa trở thành nơi thánh của Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.110Z",
      "updatedAt": "2024-09-24T02:08:48.110Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "43c7124a-8d09-4354-9de6-e874c44d9988",
      "number": 2,
      "content": "I-sơ-ra-ên trở nên vương quốc của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.112Z",
      "updatedAt": "2024-09-24T02:08:48.112Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "c2cb5101-4bf2-4c4f-9993-dad1fe48ede0",
      "number": 3,
      "content": "Nước đại dương trông thấy thế liền hoảng kinh chạy trốn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.114Z",
      "updatedAt": "2024-09-24T02:08:48.114Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "6229196b-9bb9-42c9-a696-28a408faeec2",
      "number": 3,
      "content": "Nước Sông Giô-đanh đã hỗn loạn chảy ngược về nguồn;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.116Z",
      "updatedAt": "2024-09-24T02:08:48.116Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "4f8e1c85-0e3b-4823-b176-df12125b62b5",
      "number": 4,
      "content": "Các rặng núi nhảy lên như chiên đực;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.118Z",
      "updatedAt": "2024-09-24T02:08:48.118Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "414f5253-eb56-4684-9fdd-81a0bf69739e",
      "number": 4,
      "content": "Các dãy đồi nhảy cỡn như chiên con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.120Z",
      "updatedAt": "2024-09-24T02:08:48.120Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "3476c44a-0d30-4a3e-8b8d-5a7baea8242b",
      "number": 5,
      "content": "Ðiều gì đã khiến các ngươi làm như thế?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.122Z",
      "updatedAt": "2024-09-24T02:08:48.122Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "2fd81c8e-3127-4e14-b865-a8721cea8041",
      "number": 5,
      "content": "Hỡi nước đại dương, sao ngươi chạy trốn?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.124Z",
      "updatedAt": "2024-09-24T02:08:48.124Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "c5ade8d3-d5b6-436d-9ede-b78056622502",
      "number": 5,
      "content": "Hỡi nước Sông Giô-đanh, sao ngươi chảy ngược về nguồn?",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.125Z",
      "updatedAt": "2024-09-24T02:08:48.125Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "486e6af7-94d7-4481-b0e8-758c3be62af2",
      "number": 6,
      "content": "Hỡi các rặng núi, sao các ngươi nhảy lên như chiên đực?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.128Z",
      "updatedAt": "2024-09-24T02:08:48.128Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "e684c251-8f5b-4a3d-b328-e2afcf74f38d",
      "number": 6,
      "content": "Hỡi các dãy đồi, sao các ngươi nhảy cỡn như chiên con?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.130Z",
      "updatedAt": "2024-09-24T02:08:48.130Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "577473c6-ecab-4e49-929a-a3ac4b783274",
      "number": 7,
      "content": "Hỡi trái đất, khá run rẩy trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.132Z",
      "updatedAt": "2024-09-24T02:08:48.132Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "7f6a98d2-b802-4256-ba2d-8f043e59ef95",
      "number": 7,
      "content": "Tức trước thánh nhan Ðức Chúa Trời của Gia-cốp,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.134Z",
      "updatedAt": "2024-09-24T02:08:48.134Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "056c14ac-3ec3-431b-8777-cfdef93c4630",
      "number": 8,
      "content": "Ðấng khiến nước từ một vầng đá tuôn ra thành ao hồ,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.136Z",
      "updatedAt": "2024-09-24T02:08:48.136Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    },
    {
      "id": "12aa743f-1e62-4c15-aae1-5774ccbfd042",
      "number": 8,
      "content": "Từ tảng đá hoa cương, dòng nước ngọt lai láng trào ra.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:08:48.137Z",
      "updatedAt": "2024-09-24T02:08:48.137Z",
      "chapterId": "41cef2a4-24b8-42ad-b245-8600bf82b286"
    }
  ],
