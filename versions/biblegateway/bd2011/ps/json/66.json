{
  "chapter": {
    "id": "e7f1f6df-6e21-447d-a77d-c88efc962d7d",
    "number": 66,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2066&version=BD2011",
    "createdAt": "2024-09-24T01:00:43.704Z",
    "updatedAt": "2024-09-24T01:00:43.704Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "f7d7b1ad-2a38-4d6c-a2e2-e60a9f43c6ff",
      "number": 1,
      "content": "Hỡi muôn loài trên đất,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.521Z",
      "updatedAt": "2024-09-24T02:01:58.521Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "c087f709-8eb1-479e-bca7-b53e8664ef78",
      "number": 1,
      "content": "Hãy cất tiếng reo mừng dâng lên Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.539Z",
      "updatedAt": "2024-09-24T02:01:58.539Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "61bce417-d75e-4a89-97ef-7b246b269ee4",
      "number": 2,
      "content": "Hãy ca ngợi danh vinh hiển của Ngài;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.542Z",
      "updatedAt": "2024-09-24T02:01:58.542Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "8b63db90-9902-4712-ad24-5f378aa8e988",
      "number": 2,
      "content": "Hãy tôn vinh ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.544Z",
      "updatedAt": "2024-09-24T02:01:58.544Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "e523e7cf-dd16-4892-b7f1-1f0c4d29b042",
      "number": 3,
      "content": "Hãy thưa cùng Ðức Chúa Trời rằng,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.547Z",
      "updatedAt": "2024-09-24T02:01:58.547Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "cea93f93-6f70-4e5b-99b9-f21f98a7caf9",
      "number": 3,
      "content": "“Những công việc của Ngài thật đáng kinh hãi thay!",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.549Z",
      "updatedAt": "2024-09-24T02:01:58.549Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "95dc066e-f71e-4d29-a323-d3139add4012",
      "number": 3,
      "content": "Bởi quyền năng lớn lao của Ngài,",
      "order": 2,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.552Z",
      "updatedAt": "2024-09-24T02:01:58.552Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "c0bdb233-7f61-4ad4-ab43-f1c8c68d609d",
      "number": 3,
      "content": "Những kẻ thù của Ngài phải phủ phục trước mặt Ngài.",
      "order": 3,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.554Z",
      "updatedAt": "2024-09-24T02:01:58.554Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "7916b2ec-1a9e-4ff3-9c7b-c8eb6bf1bc1f",
      "number": 4,
      "content": "Mọi loài trên đất sẽ thờ phượng Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.558Z",
      "updatedAt": "2024-09-24T02:01:58.558Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "698c47c4-6bfd-4c62-aeb7-38d62a7ff806",
      "number": 4,
      "content": "Chúng sẽ hát ca tôn ngợi Ngài;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.560Z",
      "updatedAt": "2024-09-24T02:01:58.560Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "918298b2-ae3e-49c5-94b0-9b2fc79fb907",
      "number": 4,
      "content": "Chúng sẽ ca hát tôn vinh danh Ngài.” *(Sê-la)*",
      "order": 2,
      "parNumber": 0,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.563Z",
      "updatedAt": "2024-09-24T02:01:58.563Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "6a976833-03ac-4014-b7b5-aa637f1f4da4",
      "number": 5,
      "content": "Hãy đến và nhìn xem những công việc của Ðức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.565Z",
      "updatedAt": "2024-09-24T02:01:58.565Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "44f02dad-7352-4d80-96a7-6c823130940f",
      "number": 5,
      "content": "Những gì Ngài làm cho loài người thật đáng kinh hãi thay!",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.568Z",
      "updatedAt": "2024-09-24T02:01:58.568Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "ad02f452-4677-4a6d-8b86-be56504b5c93",
      "number": 6,
      "content": "Ngài biến đại dương thành đất khô;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.571Z",
      "updatedAt": "2024-09-24T02:01:58.571Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "bb3b7c72-ed5d-4198-a945-86809a251d4f",
      "number": 6,
      "content": "Ngài cho họ đi bộ vượt qua dòng nước mạnh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.574Z",
      "updatedAt": "2024-09-24T02:01:58.574Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "c7e06450-8b6c-4b76-bdc9-340fcc086a14",
      "number": 6,
      "content": "Hãy đến, chúng ta hãy vui mừng trong Ngài!",
      "order": 2,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.577Z",
      "updatedAt": "2024-09-24T02:01:58.577Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "2f73a9c8-ca36-4d35-8b4f-51a9ccdb7e3c",
      "number": 7,
      "content": "Ngài dùng quyền năng của Ngài trị vì đời đời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.580Z",
      "updatedAt": "2024-09-24T02:01:58.580Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "48361d06-5e5f-4129-90f6-ae9c66dedcfd",
      "number": 7,
      "content": "Mắt Ngài nhìn xem các nước,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.583Z",
      "updatedAt": "2024-09-24T02:01:58.583Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "899e9528-331d-49e3-a958-8c6bf75ef97c",
      "number": 7,
      "content": "Kẻo những kẻ phản loạn nổi lên gây rối chăng. *(Sê-la)*",
      "order": 2,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.586Z",
      "updatedAt": "2024-09-24T02:01:58.586Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "2a98e8fb-697d-4a97-b405-8c127fffeef5",
      "number": 8,
      "content": "Hỡi các dân, hãy ca ngợi Ðức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.590Z",
      "updatedAt": "2024-09-24T02:01:58.590Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "c9375344-9fe0-40dd-84a1-313b8d1b3698",
      "number": 8,
      "content": "Hãy làm cho âm thanh ca ngợi Ngài vang ra khắp chốn,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.593Z",
      "updatedAt": "2024-09-24T02:01:58.593Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "74141881-f3dc-4cd4-a895-a5f3370f5ab1",
      "number": 9,
      "content": "Vì Ngài giữ gìn mạng sống chúng ta;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.596Z",
      "updatedAt": "2024-09-24T02:01:58.596Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "27bf4290-4aa5-438d-b32e-540353236d3e",
      "number": 9,
      "content": "Ngài không để cho chân chúng ta trợt ngã.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.598Z",
      "updatedAt": "2024-09-24T02:01:58.598Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "06b91f8c-0b36-4590-ad77-cbefb96ccf50",
      "number": 10,
      "content": "Thật vậy Ðức Chúa Trời ôi, Ngài đã thử thách chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.601Z",
      "updatedAt": "2024-09-24T02:01:58.601Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "66cb403d-3523-4846-afa9-f3fe5d126009",
      "number": 10,
      "content": "Ngài đã rèn luyện chúng con như người ta luyện bạc.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.603Z",
      "updatedAt": "2024-09-24T02:01:58.603Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "cea8c45a-4b96-4297-a7fa-e98dcfc44959",
      "number": 11,
      "content": "Ngài đã để cho chúng con bị sa vào lưới;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.607Z",
      "updatedAt": "2024-09-24T02:01:58.607Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "261dbd08-d729-463e-a9c3-0179ab41ff0e",
      "number": 11,
      "content": "Ngài đã để gánh nặng bị áp bức đè nặng trên lưng chúng con.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.610Z",
      "updatedAt": "2024-09-24T02:01:58.610Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "0239ff4b-bd6e-4a37-ab53-312768908d5f",
      "number": 12,
      "content": "Ngài đã để người ta cỡi trên đầu trên cổ chúng con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.613Z",
      "updatedAt": "2024-09-24T02:01:58.613Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "2b15b19f-d496-4fd9-9aec-7ec29875519c",
      "number": 12,
      "content": "Chúng con phải đi qua lửa và qua nước;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.616Z",
      "updatedAt": "2024-09-24T02:01:58.616Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "936dd4dd-2af1-4c36-9dd6-e1febe18d5c9",
      "number": 12,
      "content": "Nhưng rồi Ngài đã đem chúng con ra khỏi,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.618Z",
      "updatedAt": "2024-09-24T02:01:58.618Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "1e52dd42-9e0e-4235-9a36-e2032a3ed7b6",
      "number": 12,
      "content": "Ðể đưa chúng con vào một nơi trù phú.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.621Z",
      "updatedAt": "2024-09-24T02:01:58.621Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "953652d7-22a7-4f2d-b961-4ce9c5279ca5",
      "number": 13,
      "content": "Con sẽ vào nhà Ngài với những của lễ thiêu;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.625Z",
      "updatedAt": "2024-09-24T02:01:58.625Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "9f2898ae-5636-4def-9ece-34162c15febe",
      "number": 13,
      "content": "Con sẽ trả xong cho Ngài những gì con đã hứa nguyện,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.629Z",
      "updatedAt": "2024-09-24T02:01:58.629Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "bfa49e6b-e79d-4eae-a04f-635c2902a4ef",
      "number": 14,
      "content": "Những gì môi con đã hứa,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.631Z",
      "updatedAt": "2024-09-24T02:01:58.631Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "92b3764e-54d6-4d5d-862b-8fa61e6a6b48",
      "number": 14,
      "content": "Và miệng con đã nói ra trong lúc bị khốn khổ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.634Z",
      "updatedAt": "2024-09-24T02:01:58.634Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "3c512b6c-7c24-46e6-935f-59c8ac80ab4e",
      "number": 15,
      "content": "Con sẽ dâng lên Ngài những con vật hiến tế béo tốt để làm của lễ thiêu, chung với mỡ của các chiên đực;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.637Z",
      "updatedAt": "2024-09-24T02:01:58.637Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "e1ef7c7a-b34c-4ba7-899b-8c65d93fc4df",
      "number": 15,
      "content": "Con sẽ dâng các bò đực và các dê đực. *(Sê-la)*",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.640Z",
      "updatedAt": "2024-09-24T02:01:58.640Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "c4724011-e8b2-4eb6-b218-ba553e4d63bc",
      "number": 16,
      "content": "Hỡi mọi người kính sợ Ðức Chúa Trời, xin đến nghe,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.642Z",
      "updatedAt": "2024-09-24T02:01:58.642Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "b1686f6a-7fd5-4ca6-9918-ef7b9453a0b3",
      "number": 16,
      "content": "Tôi sẽ thuật lại những gì Ngài đã làm cho tôi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.645Z",
      "updatedAt": "2024-09-24T02:01:58.645Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "97dab615-cb3f-495b-aab3-d1ce8cc719e5",
      "number": 17,
      "content": "Miệng tôi đã kêu cầu Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.648Z",
      "updatedAt": "2024-09-24T02:01:58.648Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "ea1e9047-60f0-4c7b-8bf4-03b28615853e",
      "number": 17,
      "content": "Và lưỡi tôi đã tôn vinh Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.650Z",
      "updatedAt": "2024-09-24T02:01:58.650Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "308fb4c1-b804-4c9d-b5c3-fc8c30a79c7b",
      "number": 18,
      "content": "Nếu lòng tôi chú về tội ác,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.653Z",
      "updatedAt": "2024-09-24T02:01:58.653Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "6a1895b1-fc17-4a02-bf6c-5d3514fdf978",
      "number": 18,
      "content": "Ắt Chúa chẳng nghe tôi;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.656Z",
      "updatedAt": "2024-09-24T02:01:58.656Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "211a4083-2947-41a1-9fb5-9170425da6bf",
      "number": 19,
      "content": "Nhưng Ðức Chúa Trời thật có nghe;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.659Z",
      "updatedAt": "2024-09-24T02:01:58.659Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "19e68544-be1c-40dc-9f14-dcf6df8b85ae",
      "number": 19,
      "content": "Ngài đã lắng nghe tiếng cầu nguyện của tôi.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.662Z",
      "updatedAt": "2024-09-24T02:01:58.662Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "b363e569-cae1-4efe-82f9-e4f2309c9310",
      "number": 20,
      "content": "Tôn ngợi Ðức Chúa Trời, Ðấng chẳng bỏ qua lời cầu nguyện của tôi,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.664Z",
      "updatedAt": "2024-09-24T02:01:58.664Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    },
    {
      "id": "d36acea6-d4f8-48a3-9bbe-8b998e991ebc",
      "number": 20,
      "content": "Và không rút lại tình thương của Ngài đối với tôi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:01:58.667Z",
      "updatedAt": "2024-09-24T02:01:58.667Z",
      "chapterId": "e7f1f6df-6e21-447d-a77d-c88efc962d7d"
    }
  ],
