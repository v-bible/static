{
  "chapter": {
    "id": "409d8e25-4548-4455-a571-ef792acfbec9",
    "number": 97,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%2097&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.337Z",
    "updatedAt": "2024-09-24T01:00:44.337Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "0fa58398-ac34-403e-8f7f-6509069c8660",
      "number": 1,
      "content": "Chúa trị vì!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.847Z",
      "updatedAt": "2024-09-24T02:06:16.847Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "88727b09-af61-4e86-8b65-91641b14c33c",
      "number": 1,
      "content": "Hỡi trái đất, hãy vui mừng;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.859Z",
      "updatedAt": "2024-09-24T02:06:16.859Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "9f53f618-f864-4abb-aa4a-b1488ae360f3",
      "number": 1,
      "content": "Hỡi muôn ngàn hải đảo, hãy hân hoan.",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.861Z",
      "updatedAt": "2024-09-24T02:06:16.861Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "a83ee1d6-919c-4233-8321-4cb1ba2608ee",
      "number": 2,
      "content": "Mây dày và bóng tối mù mịt bao quanh Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.864Z",
      "updatedAt": "2024-09-24T02:06:16.864Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "c98d2df5-77ab-4c89-a34a-829e3a0a0bff",
      "number": 2,
      "content": "Công chính và công lý là nền tảng của ngai Ngài.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.866Z",
      "updatedAt": "2024-09-24T02:06:16.866Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "2f127e83-9542-4b1c-b8f9-2d0a1b74a7ba",
      "number": 3,
      "content": "Lửa hừng đi trước mặt Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.869Z",
      "updatedAt": "2024-09-24T02:06:16.869Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "4b7ac1a7-e8cc-442b-b073-df1a040dd83a",
      "number": 3,
      "content": "Thiêu hủy mọi kẻ chống nghịch chung quanh.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.872Z",
      "updatedAt": "2024-09-24T02:06:16.872Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "cca6dbdc-1cef-4088-a7e9-7c38eb45a1fb",
      "number": 4,
      "content": "Các tia chớp của Ngài lóe lên rực sáng khắp thế gian;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.875Z",
      "updatedAt": "2024-09-24T02:06:16.875Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "9bc57f63-48f5-4c48-a190-efb3665d5295",
      "number": 4,
      "content": "Ðất thấy vậy run lên lẩy bẩy.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.878Z",
      "updatedAt": "2024-09-24T02:06:16.878Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "4cc17d9f-0593-46db-8b37-01b4cde605a6",
      "number": 5,
      "content": "Các núi đồi tan chảy như sáp trước mặt Chúa,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.880Z",
      "updatedAt": "2024-09-24T02:06:16.880Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "62506bd3-107e-4d96-964a-3dbdc3f990ee",
      "number": 5,
      "content": "Tức trước thánh nhan của Ðấng Chúa Tể toàn cầu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.883Z",
      "updatedAt": "2024-09-24T02:06:16.883Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "5bd6e2ae-5789-4571-890a-f6a14b8ce1a0",
      "number": 6,
      "content": "Các tầng trời rao truyền đức công chính Ngài;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.885Z",
      "updatedAt": "2024-09-24T02:06:16.885Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "11482d5f-2162-4c97-844e-965dc3aa799e",
      "number": 6,
      "content": "Tất cả các dân đều thấy vinh quang Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.888Z",
      "updatedAt": "2024-09-24T02:06:16.888Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "43511175-c330-4fe7-93ab-e10899b7627f",
      "number": 7,
      "content": "Hãy để mọi kẻ thờ lạy các hình tượng bị xấu hổ,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.890Z",
      "updatedAt": "2024-09-24T02:06:16.890Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "00a99466-8b81-4fbd-b1ea-177114099665",
      "number": 7,
      "content": "Tức những kẻ khoe mình về các hình tượng vô tri của chúng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.892Z",
      "updatedAt": "2024-09-24T02:06:16.892Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "32c01360-56a4-43e7-8e29-c743c0b920c6",
      "number": 7,
      "content": "Hỡi tất cả các thần, hãy sấp mình xuống thờ lạy Ngài.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.895Z",
      "updatedAt": "2024-09-24T02:06:16.895Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "89f35a39-cc6c-45a7-86fc-80187bbc4c70",
      "number": 8,
      "content": "Si-ôn nghe thế bèn vui mừng,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.898Z",
      "updatedAt": "2024-09-24T02:06:16.898Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "91eb723d-efbd-4379-ad30-d058c9f2f8c4",
      "number": 8,
      "content": "Các ái nữ của Giu-đa đều vui vẻ,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.900Z",
      "updatedAt": "2024-09-24T02:06:16.900Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "7a1cfbb4-e961-45fa-81c7-de22d7db737a",
      "number": 8,
      "content": "Vì những phán quyết của Ngài, Chúa ôi;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.903Z",
      "updatedAt": "2024-09-24T02:06:16.903Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "bef1a022-ee83-47fa-aeda-0bbe5fb08bb2",
      "number": 9,
      "content": "Vì Ngài, Chúa ôi, là Ðấng Tối Cao trên khắp đất;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.905Z",
      "updatedAt": "2024-09-24T02:06:16.905Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "b7b14b77-8f25-42d8-b8d0-a0eabe584e41",
      "number": 9,
      "content": "Ngài vượt trỗi bỏ xa tất cả các thần.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.908Z",
      "updatedAt": "2024-09-24T02:06:16.908Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "7cc7c904-5f93-45b8-8bac-86483ffdf8f9",
      "number": 10,
      "content": "Hỡi những người yêu mến Chúa, hãy ghét điều ác;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.910Z",
      "updatedAt": "2024-09-24T02:06:16.910Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "9854b728-9560-411e-9d46-8b28fe2d9618",
      "number": 10,
      "content": "Ngài bảo vệ sinh mạng các thánh đồ của Ngài khỏi tay kẻ ác.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.913Z",
      "updatedAt": "2024-09-24T02:06:16.913Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "183ece80-6953-4884-a568-b3dc8e1f04ac",
      "number": 11,
      "content": "Ánh sáng tỏa rạng trên người ngay lành,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.916Z",
      "updatedAt": "2024-09-24T02:06:16.916Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "0940cff3-56d9-4ab6-9fc1-2a3a04dc0cb6",
      "number": 11,
      "content": "Và niềm vui cho người có lòng ngay thẳng.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.919Z",
      "updatedAt": "2024-09-24T02:06:16.919Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "75aac30f-4d81-485c-916a-9c6093f07fca",
      "number": 12,
      "content": "Hỡi những người ngay lành, hãy vui mừng trong Chúa;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.922Z",
      "updatedAt": "2024-09-24T02:06:16.922Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    },
    {
      "id": "cd92b36a-c1b3-411e-a249-77c768d23f3f",
      "number": 12,
      "content": "Hãy dâng lòng biết ơn lên danh thánh của Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:06:16.925Z",
      "updatedAt": "2024-09-24T02:06:16.925Z",
      "chapterId": "409d8e25-4548-4455-a571-ef792acfbec9"
    }
  ],
