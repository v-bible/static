{
  "chapter": {
    "id": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d",
    "number": 135,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20135&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.116Z",
    "updatedAt": "2024-09-24T01:00:45.116Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "de89b74f-9abc-4453-b671-1126bab7f891",
      "number": 1,
      "content": "Hãy ca ngợi Chúa! Hãy ca ngợi danh Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.901Z",
      "updatedAt": "2024-09-24T02:11:49.901Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "0510e5cf-6377-4b2e-a9ae-9ddd46856788",
      "number": 1,
      "content": "Hỡi các tôi tớ Chúa, hãy ca ngợi Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.912Z",
      "updatedAt": "2024-09-24T02:11:49.912Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "2bc92228-2aba-4164-b3c8-1eda9b985abb",
      "number": 2,
      "content": "Hỡi các bạn, những người đứng trong nhà Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.915Z",
      "updatedAt": "2024-09-24T02:11:49.915Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "7c57206f-b644-4942-8a54-294e56cce99e",
      "number": 2,
      "content": "Những người đứng trong sân đền thờ Ðức Chúa Trời chúng ta,",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.917Z",
      "updatedAt": "2024-09-24T02:11:49.917Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "8c5bae54-71af-4d90-b361-1b1f7721bf5a",
      "number": 3,
      "content": "Hãy ca ngợi Chúa, vì Chúa thật tốt;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.919Z",
      "updatedAt": "2024-09-24T02:11:49.919Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "f971b297-b8c4-4ee2-a66d-b7e41102e17e",
      "number": 3,
      "content": "Hãy ca tụng danh Ngài, vì danh Ngài đáng kính yêu;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.921Z",
      "updatedAt": "2024-09-24T02:11:49.921Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "7034d7f6-784c-4a79-995b-d69a89a54b30",
      "number": 4,
      "content": "Vì Chúa đã chọn Gia-cốp cho Ngài,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.923Z",
      "updatedAt": "2024-09-24T02:11:49.923Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "3e4ec62f-a1a4-4f92-b8dd-74a5057fa86d",
      "number": 4,
      "content": "I-sơ-ra-ên làm tài sản riêng của Ngài.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.925Z",
      "updatedAt": "2024-09-24T02:11:49.925Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "82dcc464-7e7a-48bf-9509-711c558cd6e6",
      "number": 5,
      "content": "Vì tôi biết rằng Chúa thật vĩ đại;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.927Z",
      "updatedAt": "2024-09-24T02:11:49.927Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "3a667ef0-1cb7-4cab-acbe-752c77e4527d",
      "number": 5,
      "content": "Chúa của chúng ta vĩ đại hơn tất cả thần thánh của loài người.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.929Z",
      "updatedAt": "2024-09-24T02:11:49.929Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "4e114136-3d94-40b9-a900-71220d583255",
      "number": 6,
      "content": "Bất cứ điều gì Chúa muốn, Ngài làm điều ấy,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.937Z",
      "updatedAt": "2024-09-24T02:11:49.937Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "3d213729-0148-432a-bfca-39bdcd3af814",
      "number": 6,
      "content": "Bất kể trên trời hay dưới đất, trong lòng biển hay trong các vực sâu.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.939Z",
      "updatedAt": "2024-09-24T02:11:49.939Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "61fc6e74-42ed-41e3-9a7d-3e68da547c9d",
      "number": 7,
      "content": "Ngài khiến mây trồi lên từ những nơi tận cùng trái đất;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.941Z",
      "updatedAt": "2024-09-24T02:11:49.941Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "750320dc-eb53-4a3c-aa2f-a3f981c68c53",
      "number": 7,
      "content": "Ngài làm cho sấm sét nổ vang khi trời đổ cơn mưa;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.943Z",
      "updatedAt": "2024-09-24T02:11:49.943Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "148dc781-8a83-4a38-888c-d161e0f435c4",
      "number": 7,
      "content": "Ngài sai gió bay ra từ các kho lẫm của Ngài.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.948Z",
      "updatedAt": "2024-09-24T02:11:49.948Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "971eac8e-eec8-45f4-9a5b-fda56db6cdaf",
      "number": 8,
      "content": "Ngài đánh hạ các con đầu lòng của Ai-cập,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.951Z",
      "updatedAt": "2024-09-24T02:11:49.951Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "cf8b119d-51c9-4b98-bab2-860630c48937",
      "number": 8,
      "content": "Con của loài người và con của súc vật.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.953Z",
      "updatedAt": "2024-09-24T02:11:49.953Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "3c3345ad-d7a6-42e8-b601-35211e9f38da",
      "number": 9,
      "content": "Hỡi Ai-cập, Ngài sai các dấu kỳ và phép lạ đến giữa ngươi,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.955Z",
      "updatedAt": "2024-09-24T02:11:49.955Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "b0edb157-6db4-432d-89fc-38ace9ad2c68",
      "number": 9,
      "content": "Ðể trừng phạt Pha-ra-ôn và các tôi tớ của ông ta.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.958Z",
      "updatedAt": "2024-09-24T02:11:49.958Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "395a5f69-cf3d-4311-9fe9-19a745ec16e9",
      "number": 10,
      "content": "Ngài đánh bại nhiều dân tộc,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.961Z",
      "updatedAt": "2024-09-24T02:11:49.961Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "d6592eae-f3d5-4466-b440-d16ed27cfb2b",
      "number": 10,
      "content": "Và tiêu diệt các vua mạnh mẽ;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.963Z",
      "updatedAt": "2024-09-24T02:11:49.963Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "e7377275-4225-4603-b1a0-b4ff46154575",
      "number": 11,
      "content": "Si-hôn vua dân A-mô-ri, Óc vua Ba-san,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.966Z",
      "updatedAt": "2024-09-24T02:11:49.966Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "15fd944e-6cd4-4ed2-af70-cd5ac21d9443",
      "number": 11,
      "content": "Và tất cả các vương quốc ở Ca-na-an.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.970Z",
      "updatedAt": "2024-09-24T02:11:49.970Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "1bff9b83-22ef-42e8-a949-43d11e069ba0",
      "number": 12,
      "content": "Ngài ban xứ của họ để làm sản nghiệp,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.973Z",
      "updatedAt": "2024-09-24T02:11:49.973Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "351a0051-c728-4eff-8b0c-dfc54ad79bc7",
      "number": 12,
      "content": "Một sản nghiệp cho dân I-sơ-ra-ên của Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.978Z",
      "updatedAt": "2024-09-24T02:11:49.978Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "17a1933b-fcf8-4b2c-80af-3a0445c37a1f",
      "number": 13,
      "content": "Chúa ôi, danh Ngài sẽ trường tồn mãi mãi;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.982Z",
      "updatedAt": "2024-09-24T02:11:49.982Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "5513ee7a-db88-4fdd-8b76-0ae4546738e1",
      "number": 13,
      "content": "Chúa ôi, công ơn Ngài sẽ được ghi nhớ từ đời nọ đến đời kia,",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.987Z",
      "updatedAt": "2024-09-24T02:11:49.987Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "b9e68791-801f-499d-8dbb-f6ff26c8a1bf",
      "number": 14,
      "content": "Vì Chúa sẽ xét đoán dân Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.992Z",
      "updatedAt": "2024-09-24T02:11:49.992Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "36b90bf4-372a-4832-a0fe-89fad88bfc47",
      "number": 14,
      "content": "Ngài sẽ tỏ lòng thương xót đối với các tôi tớ Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:49.997Z",
      "updatedAt": "2024-09-24T02:11:49.997Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "db4f6290-d7e7-40ec-892c-e3d167f0498a",
      "number": 15,
      "content": "Các thần tượng của các dân chẳng qua là bạc và vàng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.002Z",
      "updatedAt": "2024-09-24T02:11:50.002Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "e4ce0f3f-3885-4064-a930-47adf4665ac2",
      "number": 15,
      "content": "Chúng chỉ là sản phẩm do tay người làm ra.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.007Z",
      "updatedAt": "2024-09-24T02:11:50.007Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "2eddf20b-c4d4-40ab-8f42-325505ca9b74",
      "number": 16,
      "content": "Chúng có miệng mà chẳng nói;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.012Z",
      "updatedAt": "2024-09-24T02:11:50.012Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "9dfb77f1-b4f6-4e03-bb5a-45d35f1dc05e",
      "number": 16,
      "content": "Chúng có mắt mà chẳng thấy;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.017Z",
      "updatedAt": "2024-09-24T02:11:50.017Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "110c8761-e59e-4f78-8fcc-45c31d5392b9",
      "number": 17,
      "content": "Chúng có tai mà chẳng nghe;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.022Z",
      "updatedAt": "2024-09-24T02:11:50.022Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "0e6c157e-af03-4e89-8ac9-f0ffff5c89f3",
      "number": 17,
      "content": "Không có chút hơi thở nào trong miệng chúng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.027Z",
      "updatedAt": "2024-09-24T02:11:50.027Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "53c96b6a-f48c-417d-8b43-6ac1bcfd1633",
      "number": 18,
      "content": "Nguyện những kẻ làm thần tượng sẽ trở nên như chúng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.032Z",
      "updatedAt": "2024-09-24T02:11:50.032Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "29e0441c-8578-4fec-aee6-11012ff379dd",
      "number": 18,
      "content": "Và những ai tin cậy vào thần tượng cũng giống như vậy.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.038Z",
      "updatedAt": "2024-09-24T02:11:50.038Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "e45ed436-64ce-4ab9-9c9a-93fe7dbec508",
      "number": 19,
      "content": "Hỡi nhà I-sơ-ra-ên, hãy chúc tụng Chúa!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.043Z",
      "updatedAt": "2024-09-24T02:11:50.043Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "dab8dfa4-87c1-4186-9b0b-3811be984141",
      "number": 19,
      "content": "Hỡi nhà A-rôn, hãy chúc tụng Chúa!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.047Z",
