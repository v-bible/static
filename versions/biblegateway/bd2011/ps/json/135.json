    },
    {
      "id": "d66e3e1e-823e-411c-9d3c-0fd5ee0037b4",
      "number": 20,
      "content": "Hỡi nhà Lê-vi, hãy chúc tụng Chúa!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.052Z",
      "updatedAt": "2024-09-24T02:11:50.052Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "d9d60755-9f5c-4295-b18c-1afef07bc9b7",
      "number": 20,
      "content": "Hỡi các bạn, những người kính sợ Chúa, hãy chúc tụng Chúa!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.057Z",
      "updatedAt": "2024-09-24T02:11:50.057Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "253bee22-9881-4218-b88f-05d712f81bfb",
      "number": 21,
      "content": "Hỡi những người sống ở Si-ôn, hãy chúc tụng Chúa, Ðấng ngự tại Giê-ru-sa-lem!",
      "order": 0,
      "parNumber": 5,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.062Z",
      "updatedAt": "2024-09-24T02:11:50.062Z",
      "chapterId": "0d8b5f0f-eb2c-4bf2-8913-bd3ed7ef332d"
    },
    {
      "id": "2fd57d4f-f028-4093-bb05-87309e90ec79",
      "number": 21,
      "content": "Hãy ca ngợi Chúa!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:11:50.067Z",
      "updatedAt": "2024-09-24T02:11:50.067Z",
