{
  "chapter": {
    "id": "a62cd349-8e99-4079-90a6-9424630badf6",
    "number": 121,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20121&version=BD2011",
    "createdAt": "2024-09-24T01:00:44.839Z",
    "updatedAt": "2024-09-24T01:00:44.839Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "9145d267-904b-4c2e-8d38-c98f9b2777d0",
      "number": 1,
      "content": "Tôi ngước mắt nhìn lên đồi núi;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.431Z",
      "updatedAt": "2024-09-24T02:10:03.431Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "3f7507c2-5aaa-438e-b27a-b260801d5604",
      "number": 1,
      "content": "Sự giúp đỡ tôi đến từ đâu?",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.436Z",
      "updatedAt": "2024-09-24T02:10:03.436Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "033a1491-0b87-4013-8ac5-452c5feda406",
      "number": 2,
      "content": "Sự giúp đỡ tôi đến từ Chúa,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.438Z",
      "updatedAt": "2024-09-24T02:10:03.438Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "ba396061-fff8-44be-9c73-9ac4857a8bb7",
      "number": 2,
      "content": "Ðấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 0,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.440Z",
      "updatedAt": "2024-09-24T02:10:03.440Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "4834c4bd-d660-4360-bd61-c87fc8296308",
      "number": 3,
      "content": "Ngài sẽ không để cho chân bạn trợt ngã;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.442Z",
      "updatedAt": "2024-09-24T02:10:03.442Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "c5cd4641-866e-4b27-8d4d-646924b53bc6",
      "number": 3,
      "content": "Ðấng gìn giữ bạn không hề buồn ngủ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.444Z",
      "updatedAt": "2024-09-24T02:10:03.444Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "cfd7109c-9eee-4dc6-ba3f-46c5d3ff2d36",
      "number": 4,
      "content": "Thật vậy Ðấng gìn giữ I-sơ-ra-ên không hề buồn ngủ và không bao giờ ngủ.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.446Z",
      "updatedAt": "2024-09-24T02:10:03.446Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "00af327a-38ee-4f46-884f-3889fdf3d795",
      "number": 5,
      "content": "Chúa là Ðấng gìn giữ bạn;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.448Z",
      "updatedAt": "2024-09-24T02:10:03.448Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "03e542d7-3bb6-415d-ae4c-b72633046934",
      "number": 5,
      "content": "Chúa là bóng mát bên phải bạn.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.450Z",
      "updatedAt": "2024-09-24T02:10:03.450Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "e4287215-97c0-4eaa-a1e5-53551e0b4fa5",
      "number": 6,
      "content": "Mặt trời sẽ không hại bạn ban ngày;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.454Z",
      "updatedAt": "2024-09-24T02:10:03.454Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "e29a67fa-a897-4b9b-b81c-6dc1fb39548c",
      "number": 6,
      "content": "Mặt trăng sẽ không hại bạn ban đêm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.459Z",
      "updatedAt": "2024-09-24T02:10:03.459Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "c356affd-9585-4da4-9202-d6997b1bfb55",
      "number": 7,
      "content": "Chúa sẽ gìn giữ bạn khỏi mọi tai họa;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.464Z",
      "updatedAt": "2024-09-24T02:10:03.464Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "27c7fdae-9db9-44c5-a5fe-117bf483a8cf",
      "number": 7,
      "content": "Ngài sẽ gìn giữ mạng sống bạn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.469Z",
      "updatedAt": "2024-09-24T02:10:03.469Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "97bfb23e-e3ca-48c5-9343-5bce170f248c",
      "number": 8,
      "content": "Chúa sẽ gìn giữ bạn khi ra khi vào,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.474Z",
      "updatedAt": "2024-09-24T02:10:03.474Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    },
    {
      "id": "57c33c54-2067-4d83-9627-a5724bd04525",
      "number": 8,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:10:03.479Z",
      "updatedAt": "2024-09-24T02:10:03.479Z",
      "chapterId": "a62cd349-8e99-4079-90a6-9424630badf6"
    }
  ],
