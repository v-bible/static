    },
    {
      "id": "1158e65b-a079-48cd-8b57-d15d40f5c022",
      "number": 19,
      "content": "Ngài tuyên bố lời Ngài cho Gia-cốp,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.692Z",
      "updatedAt": "2024-09-24T02:13:20.692Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "96680825-1de5-486b-9f64-6faba4b0ce1c",
      "number": 19,
      "content": "Luật lệ Ngài và mạng lịnh Ngài cho I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.697Z",
      "updatedAt": "2024-09-24T02:13:20.697Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "702fb5d0-3f24-4b48-a3a7-73e7893a07a2",
      "number": 20,
      "content": "Ngài chẳng làm như vậy cho một dân nào khác;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.701Z",
      "updatedAt": "2024-09-24T02:13:20.701Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "0048c78a-59c4-4613-8ec9-02ecebe8efeb",
      "number": 20,
      "content": "Họ chẳng biết gì về các mạng lịnh của Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.705Z",
      "updatedAt": "2024-09-24T02:13:20.705Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    }
