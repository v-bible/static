{
  "chapter": {
    "id": "daf12e9f-7650-46a3-a66a-bb0dce8614af",
    "number": 147,
    "ref": "https://www.biblegateway.com/passage/?search=Th%C3%A1nh%20Thi%20147&version=BD2011",
    "createdAt": "2024-09-24T01:00:45.376Z",
    "updatedAt": "2024-09-24T01:00:45.376Z",
    "bookId": "c717afb1-3e2a-4420-ab57-161df8cd461f"
  },
  "verses": [
    {
      "id": "f10ae2d4-7245-437f-8338-836b3839fc2e",
      "number": 1,
      "content": "Hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.519Z",
      "updatedAt": "2024-09-24T02:13:20.519Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "87389f88-2229-4823-84e3-8012708443ea",
      "number": 20,
      "content": "Hãy ca ngợi Chúa!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.709Z",
      "updatedAt": "2024-09-24T02:13:20.709Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "ff0a281f-d534-4844-8fb1-ae9431a1a6a7",
      "number": 1,
      "content": "Ca hát chúc tụng Ðức Chúa Trời chúng ta là điều tốt đẹp;",
      "order": 1,
      "parNumber": 0,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.535Z",
      "updatedAt": "2024-09-24T02:13:20.535Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "c23f79da-e57e-4712-9ab8-45c70acf27be",
      "number": 1,
      "content": "Vì tôn ngợi Ngài là điều vui thỏa và hợp lý dường nào!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.539Z",
      "updatedAt": "2024-09-24T02:13:20.539Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "f3107d07-d0d5-44e0-9c25-90942a3a7fb3",
      "number": 2,
      "content": "Chúa xây dựng lại Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.543Z",
      "updatedAt": "2024-09-24T02:13:20.543Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "9e5fa75f-0cab-4c03-9db6-f0aeeec8775f",
      "number": 2,
      "content": "Ngài gom nhóm những người bị tản lạc của I-sơ-ra-ên trở về.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.547Z",
      "updatedAt": "2024-09-24T02:13:20.547Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "a2191859-2b29-4e86-9980-1ce7cd21ccfd",
      "number": 3,
      "content": "Ngài rịt lành tâm hồn tan vỡ;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.551Z",
      "updatedAt": "2024-09-24T02:13:20.551Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "0aff6dec-5707-40db-bc62-b22105f706b1",
      "number": 3,
      "content": "Và băng bó các vết thương của họ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.555Z",
      "updatedAt": "2024-09-24T02:13:20.555Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "a8f012fd-fa8e-4170-8400-8506de74207d",
      "number": 4,
      "content": "Ngài định số lượng các vì sao;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.559Z",
      "updatedAt": "2024-09-24T02:13:20.559Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "ef0d9fba-88c8-47b6-affa-772f822abf7a",
      "number": 4,
      "content": "Ngài gọi chúng theo tên riêng của từng vì sao một.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.563Z",
      "updatedAt": "2024-09-24T02:13:20.563Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "9b9967e4-888a-4ef0-9478-3c982fef91b2",
      "number": 5,
      "content": "Chúa chúng ta vĩ đại biết bao!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.567Z",
      "updatedAt": "2024-09-24T02:13:20.567Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "dab90ade-21d9-4392-9409-efd94901b50a",
      "number": 5,
      "content": "Quyền năng Ngài thật là vô hạn.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.571Z",
      "updatedAt": "2024-09-24T02:13:20.571Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "9bd1c460-f23e-4e2d-88a7-e5a0c5904752",
      "number": 5,
      "content": "Kiến thức Ngài quả thật vô cùng.",
      "order": 2,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.575Z",
      "updatedAt": "2024-09-24T02:13:20.575Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "d12d3c65-1c55-4f3e-8da7-41c6ab46d855",
      "number": 6,
      "content": "Chúa nhấc đưa những người nghèo khó lên cao,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.579Z",
      "updatedAt": "2024-09-24T02:13:20.579Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "9d497c57-ee3e-4b5d-a3ae-a673121c2ab4",
      "number": 6,
      "content": "Nhưng Ngài đánh đổ những kẻ gian ác ngã nhào xuống đất.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.583Z",
      "updatedAt": "2024-09-24T02:13:20.583Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "20a1b045-8c68-4c71-8f2a-90caf1c8ad3b",
      "number": 7,
      "content": "Hãy dâng lên Chúa những lời cảm tạ;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.587Z",
      "updatedAt": "2024-09-24T02:13:20.587Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "e5562b3b-e736-4a88-a221-a2539bd07201",
      "number": 7,
      "content": "Hãy dùng nhạc khí ca ngợi Ðức Chúa Trời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.591Z",
      "updatedAt": "2024-09-24T02:13:20.591Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "b3d96ff2-3df2-4dd4-a5b9-b7240f6128a9",
      "number": 8,
      "content": "Ngài bao phủ các tầng trời bằng mây;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.599Z",
      "updatedAt": "2024-09-24T02:13:20.599Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "e49750dc-4e7c-48ec-bd92-2bc8f977b6dc",
      "number": 8,
      "content": "Ngài chuẩn bị các trận mưa cho đất;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.603Z",
      "updatedAt": "2024-09-24T02:13:20.603Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "8d9ba100-8a7d-4bba-9459-00f82dcc0362",
      "number": 8,
      "content": "Ngài khiến cỏ mọc trên các núi đồi.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.607Z",
      "updatedAt": "2024-09-24T02:13:20.607Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "d097b014-5162-48b4-82be-8097db33dc38",
      "number": 9,
      "content": "Ngài ban thực phẩm cho muôn loài cầm thú;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.611Z",
      "updatedAt": "2024-09-24T02:13:20.611Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "ee9dc62d-52b2-4743-8a54-84778cd2ef8e",
      "number": 9,
      "content": "Ngài ban thức ăn cho các quạ con đói bụng kêu gào.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.615Z",
      "updatedAt": "2024-09-24T02:13:20.615Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "4b0b3ce8-0fa7-4016-a598-bb0c2c321c95",
      "number": 10,
      "content": "Ngài chẳng hài lòng về sức lực của ngựa chiến;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.619Z",
      "updatedAt": "2024-09-24T02:13:20.619Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "b5f08916-9bfc-479e-a7e0-90aa4d055d32",
      "number": 10,
      "content": "Ngài không vui về đôi chân nhanh nhẹn của loài người;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.623Z",
      "updatedAt": "2024-09-24T02:13:20.623Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "bded2a83-b57b-4b64-84fa-9ea7ba268a4c",
      "number": 11,
      "content": "Nhưng Chúa rất vui lòng về những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.627Z",
      "updatedAt": "2024-09-24T02:13:20.627Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "05dc0c8a-4eb1-4f3b-83a4-8b3ae5a03683",
      "number": 11,
      "content": "Những người đặt hy vọng vào tình thương của Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.631Z",
      "updatedAt": "2024-09-24T02:13:20.631Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "c8872eb4-2203-4efd-9acc-02bea69a34c0",
      "number": 12,
      "content": "Hỡi Giê-ru-sa-lem, hãy ca ngợi Chúa!",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.635Z",
      "updatedAt": "2024-09-24T02:13:20.635Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "7394aef4-e4f0-4e90-bf72-c708bf497722",
      "number": 12,
      "content": "Hỡi Si-ôn, hãy tôn ngợi Ðức Chúa Trời!",
      "order": 1,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.639Z",
      "updatedAt": "2024-09-24T02:13:20.639Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "db9f409c-232c-4789-bc06-b81ae9785e5a",
      "number": 13,
      "content": "Vì Ngài làm vững mạnh các song sắt ở các cổng thành của ngươi;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 2,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.644Z",
      "updatedAt": "2024-09-24T02:13:20.644Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "a9d5d409-4582-4ac1-8c29-c425183b5136",
      "number": 13,
      "content": "Ngài ban phước cho con dân ngươi sống trong ngươi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 3,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.648Z",
      "updatedAt": "2024-09-24T02:13:20.648Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "e72f2a9f-b1b4-4983-9489-a259bc09fab2",
      "number": 14,
      "content": "Ngài ban bình an nơi các biên giới ngươi;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 4,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.652Z",
      "updatedAt": "2024-09-24T02:13:20.652Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "80a7946f-438b-493d-810e-21f45a65017a",
      "number": 14,
      "content": "Ngài cho ngươi no thỏa bằng gạo ngon hảo hạng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 5,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.656Z",
      "updatedAt": "2024-09-24T02:13:20.656Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "758745bb-4e32-4f02-a0e7-b5fc9a8ab337",
      "number": 15,
      "content": "Ngài truyền lịnh của Ngài cho trái đất;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 6,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.660Z",
      "updatedAt": "2024-09-24T02:13:20.660Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "a67a6415-af43-42ae-bf18-8c8a53552282",
      "number": 15,
      "content": "Lời Ngài phán ra được cấp tốc truyền đi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 7,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.664Z",
      "updatedAt": "2024-09-24T02:13:20.664Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "ccdbc6e2-b91a-45cf-a439-27cc02a90cad",
      "number": 16,
      "content": "Ngài ban tuyết xuống phủ trắng như lông chiên;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 8,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.668Z",
      "updatedAt": "2024-09-24T02:13:20.668Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "8cc879e7-dc92-4f2a-b218-992822c21109",
      "number": 16,
      "content": "Ngài rải sương giá ra như tro bụi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 9,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.672Z",
      "updatedAt": "2024-09-24T02:13:20.672Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "bdac5c9d-cb5d-4965-b917-aa413af82d77",
      "number": 17,
      "content": "Ngài quăng băng giá ra từng mảng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 10,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.676Z",
      "updatedAt": "2024-09-24T02:13:20.676Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "8ba5ce32-a3e0-46a1-a0fb-9719fd2d597c",
      "number": 17,
      "content": "Ai có thể chịu nổi cơn buốt giá của Ngài?",
      "order": 1,
      "parNumber": 4,
      "parIndex": 11,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.680Z",
      "updatedAt": "2024-09-24T02:13:20.680Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "0bd87eef-bf16-4b2b-a558-f0eb0f59573b",
      "number": 18,
      "content": "Ngài ban lịnh, chúng liền tan chảy;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 12,
      "isPoetry": true,
      "createdAt": "2024-09-24T02:13:20.684Z",
      "updatedAt": "2024-09-24T02:13:20.684Z",
      "chapterId": "daf12e9f-7650-46a3-a66a-bb0dce8614af"
    },
    {
      "id": "bbf81069-fc3d-47fe-98ad-5df4d69be19a",
      "number": 18,
      "content": "Ngài khiến gió thổi qua, nước chảy xuống thành dòng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 13,
