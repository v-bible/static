    },
    {
      "id": "a07aee7b-cd8d-43c3-bf56-261795a61a3d",
      "content": "Mt 5,21-22; 1 Ga 3,15",
      "position": 50,
      "order": 1,
      "createdAt": "2024-09-23T03:27:20.340Z",
