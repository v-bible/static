    },
    {
      "id": "991cdded-1b63-477b-845c-1244771bcc85",
      "content": "Ngành ông Kham",
      "order": 0,
      "createdAt": "2024-09-23T04:03:47.744Z",
      "updatedAt": "2024-09-23T04:03:47.744Z",
      "verseId": "5054b111-5038-4701-8530-1e9c21434815",
      "chapterId": "aa4d4e5f-ef82-4b0e-879c-a3fd56131a9c"
    },
    {
      "id": "a8c19408-052f-4712-a504-7ed5fe1ad74d",
      "content": "Ngành ông [Sêm]()",
      "order": 0,
