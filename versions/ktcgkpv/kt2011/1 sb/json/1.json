    },
    {
      "id": "bb7280e8-a39d-4ba4-92d3-8b0791c1190f",
      "content": "SÁCH SỬ BIÊN NIÊN Quyển 1",
      "order": 0,
      "createdAt": "2024-09-23T04:03:47.712Z",
      "updatedAt": "2024-09-23T04:03:47.712Z",
      "verseId": "5ef9cfc2-2162-4e51-a2bb-e9b494769a74",
      "chapterId": "aa4d4e5f-ef82-4b0e-879c-a3fd56131a9c"
    },
    {
      "id": "3987bbd3-c694-4b3d-b7d2-6364c6cd1cae",
      "content": "Ngành ông [Gia-phét]()",
      "order": 0,
      "createdAt": "2024-09-23T04:03:47.734Z",
      "updatedAt": "2024-09-23T04:03:47.734Z",
      "verseId": "9686bfa3-6e61-4c67-9026-60268568f117",
