      "updatedAt": "2024-09-23T04:09:28.669Z",
      "verseId": "f37d441b-a143-4bee-b24e-eaa0b80600db",
      "chapterId": "5c36dd06-5741-49e6-b380-1758fc9d8cef"
    },
    {
      "id": "142beaa6-4f68-4570-8f37-a16831d18b7b",
      "content": "Cn 11,4",
