    },
    {
      "id": "ce0b1f54-a389-4d78-9bcf-6a9edec9c385",
      "number": 33,
      "content": "[Kha-xo](), [Ra-ma](), [Ghít-ta-gim](),",
      "order": 0,
      "parNumber": 13,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:04:17.094Z",
      "updatedAt": "2024-09-23T04:04:17.094Z",
