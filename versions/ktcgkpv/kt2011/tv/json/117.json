  "verses": [
    {
      "id": "28740186-e9de-4737-bc4f-bcb20fb09e6c",
      "number": 1,
      "content": "Muôn nước hỡi, nào ca ngợi CHÚA,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:19:51.975Z",
      "updatedAt": "2024-09-23T04:19:51.975Z",
      "chapterId": "31c04882-981b-4524-9b87-467316ed137e"
    },
    {
      "id": "534078f4-8b3b-41de-888a-24a9943d4e9e",
      "number": 1,
      "content": "ngàn dân ơi, hãy chúc tụng Người !",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:19:51.984Z",
      "updatedAt": "2024-09-23T04:19:51.984Z",
      "chapterId": "31c04882-981b-4524-9b87-467316ed137e"
    },
    {
      "id": "94af829f-4be4-4d30-9919-63083125e7a4",
      "number": 2,
      "content": "Vì tình Chúa thương ta thật là mãnh liệt,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:19:51.986Z",
      "updatedAt": "2024-09-23T04:19:51.986Z",
      "chapterId": "31c04882-981b-4524-9b87-467316ed137e"
    },
    {
      "id": "374bb5ae-76fd-4246-b2d8-9d751c2380c6",
      "number": 2,
      "content": "lòng thành tín của Người bền vững muôn năm. Ha-lê-lui-a.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:19:51.990Z",
      "updatedAt": "2024-09-23T04:19:51.990Z",
      "chapterId": "31c04882-981b-4524-9b87-467316ed137e"
    }
  ],
