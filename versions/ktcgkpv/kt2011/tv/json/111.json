    {
      "id": "ed7ca377-ea8b-43f5-b19f-0030fe8aeb48",
      "number": 10,
      "content": "Sáng suốt thay kẻ thực hành như vậy.",
      "order": 3,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.596Z",
