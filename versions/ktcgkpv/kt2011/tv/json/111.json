    },
    {
      "id": "517ac42c-f0b6-478b-a567-07e575da1cb3",
      "number": 10,
      "content": "Mãi đến thiên thu còn vang tiếng ngợi khen Người.",
      "order": 5,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.607Z",
      "updatedAt": "2024-09-23T04:22:08.607Z",
