    },
    {
      "id": "2b4f6db2-ed93-4577-a80e-731c5617a0b5",
      "number": 1,
      "content": "A-lép",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.400Z",
      "updatedAt": "2024-09-23T04:22:08.400Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "c66f2524-5200-4546-bc5a-d75c3328c27e",
      "number": 1,
      "content": "Tôi xin hết lòng cảm tạ CHÚA,",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.402Z",
      "updatedAt": "2024-09-23T04:22:08.402Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "9b7dc6bb-6f0b-4b0b-8a4a-4b09ac5dc334",
      "number": 1,
      "content": "Bết",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.404Z",
      "updatedAt": "2024-09-23T04:22:08.404Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "59e71f7c-c649-452e-be4a-f0588da3647e",
      "number": 1,
      "content": "trong cộng đoàn chính nhân, giữa lòng đại hội.",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.406Z",
      "updatedAt": "2024-09-23T04:22:08.406Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "b0fa50ab-1543-45c1-8622-9944fde84d95",
      "number": 2,
      "content": "Ghi-men",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.408Z",
      "updatedAt": "2024-09-23T04:22:08.408Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "4206ddfd-1ad4-41b3-a00e-ab521fa0add2",
      "number": 2,
      "content": "Việc CHÚA làm quả thật lớn lao,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.411Z",
      "updatedAt": "2024-09-23T04:22:08.411Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "ce9e5cca-0cd6-413b-9fd8-4d81c202c4cb",
      "number": 2,
      "content": "Đa-lét",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.412Z",
      "updatedAt": "2024-09-23T04:22:08.412Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "dbcd2299-d378-4697-b6c7-665c0ced4d96",
      "number": 2,
      "content": "người mộ mến ra công tìm hiểu.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.416Z",
      "updatedAt": "2024-09-23T04:22:08.416Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "e59ba589-d280-410c-be19-fa926bd871bd",
      "number": 3,
      "content": "Hê",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.420Z",
      "updatedAt": "2024-09-23T04:22:08.420Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "ebd2ec45-4c9a-40b6-93fc-61ccf0111c45",
      "number": 3,
      "content": "Sự nghiệp Chúa oai phong hiển hách,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.425Z",
      "updatedAt": "2024-09-23T04:22:08.425Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "72f853af-c829-4206-b9dd-ec5320747c5d",
      "number": 3,
      "content": "Vau",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.430Z",
      "updatedAt": "2024-09-23T04:22:08.430Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "a8c0e321-ace5-4991-b070-c5775451221a",
      "number": 3,
      "content": "đức công chính của Người tồn tại thiên thu.",
      "order": 3,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.434Z",
      "updatedAt": "2024-09-23T04:22:08.434Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "d2e1526e-d34f-4a06-b361-20aae99a2bd1",
      "number": 4,
      "content": "Da-in",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.444Z",
      "updatedAt": "2024-09-23T04:22:08.444Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "9a019d62-94b4-4a50-96c4-6a5118dd5c9f",
      "number": 4,
      "content": "Chúa đã truyền tưởng niệm những kỳ công của Người.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.449Z",
      "updatedAt": "2024-09-23T04:22:08.449Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "6e6839a9-ddd9-4dcc-be34-0233eafd7bba",
      "number": 4,
      "content": "Khết",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.453Z",
      "updatedAt": "2024-09-23T04:22:08.453Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "51fc9d50-1be1-4b4a-ba83-c7185c75fbc1",
      "number": 4,
      "content": "CHÚA là Đấng từ bi nhân hậu.",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.458Z",
      "updatedAt": "2024-09-23T04:22:08.458Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "2c9956fd-559e-4e05-b691-f984b74f6b96",
      "number": 5,
      "content": "Tết",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.464Z",
      "updatedAt": "2024-09-23T04:22:08.464Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "33e9b662-ac30-42bb-a999-0c0d235d9cde",
      "number": 5,
      "content": "Ai kính sợ Người, Người ban phát của ăn ;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.468Z",
      "updatedAt": "2024-09-23T04:22:08.468Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "6a2ced98-9197-4987-8d22-c3789d89abda",
      "number": 5,
      "content": "Giốt",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.477Z",
      "updatedAt": "2024-09-23T04:22:08.477Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "9b71a64d-ed84-43ce-b061-4b57058c5e79",
      "number": 5,
      "content": "giao ước đã lập ra, muôn đời Người nhớ mãi.",
      "order": 3,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.482Z",
      "updatedAt": "2024-09-23T04:22:08.482Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "356b77d8-75de-4de6-8482-9e8bb6f04317",
      "number": 6,
      "content": "Cáp",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.487Z",
      "updatedAt": "2024-09-23T04:22:08.487Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "901302b3-8403-4735-a71e-fc319d9a0ed6",
      "number": 6,
      "content": "Sức mạnh việc Người làm, Người cho dân thấy rõ,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.492Z",
      "updatedAt": "2024-09-23T04:22:08.492Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "8e1e3723-ace6-42a4-ba7f-b0d50b3e0e7c",
      "number": 6,
      "content": "La-mét",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.496Z",
      "updatedAt": "2024-09-23T04:22:08.496Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "7b205b79-1f19-4234-8f2e-31aa6aa8c4c4",
      "number": 6,
      "content": "khi tặng ban cho họ gia sản của chư dân.",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.501Z",
      "updatedAt": "2024-09-23T04:22:08.501Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "e2d79cd7-3083-4f30-aeb3-076ecb445ec4",
      "number": 7,
      "content": "Mêm",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.506Z",
      "updatedAt": "2024-09-23T04:22:08.506Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "90113607-fdef-4d4e-a865-173170b12f8e",
      "number": 7,
      "content": "Những công trình tay Chúa thực hiện",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.511Z",
      "updatedAt": "2024-09-23T04:22:08.511Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "440d6a32-c58b-45bd-88f7-b63a42eb6251",
      "number": 7,
      "content": "quả là chân thật và công minh.",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.516Z",
      "updatedAt": "2024-09-23T04:22:08.516Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "9b2772e1-9b3c-4f66-94be-10a71bab3842",
      "number": 7,
      "content": "[Nun]()",
      "order": 3,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.520Z",
      "updatedAt": "2024-09-23T04:22:08.520Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "57fb74be-fe83-4885-8d40-911448d12978",
      "number": 7,
      "content": "Huấn lệnh Người ban đều chắc chắn,",
      "order": 4,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.525Z",
      "updatedAt": "2024-09-23T04:22:08.525Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "788e5f15-4b20-409b-9aa8-d4ad0d937642",
      "number": 8,
      "content": "Xa-méc",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.529Z",
      "updatedAt": "2024-09-23T04:22:08.529Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "32c20449-cf50-48ff-b424-2ce2732690ba",
      "number": 8,
      "content": "bền vững đến muôn đời muôn thuở,",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.534Z",
      "updatedAt": "2024-09-23T04:22:08.534Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "76604603-335e-47b1-8903-836542b396e2",
      "number": 8,
      "content": "A-in",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.538Z",
      "updatedAt": "2024-09-23T04:22:08.538Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "1646561a-9ba2-492f-83e5-64ba4fc0fee7",
      "number": 8,
      "content": "căn cứ vào sự thật lẽ ngay.",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.543Z",
      "updatedAt": "2024-09-23T04:22:08.543Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "07a3b9e3-ae87-40b9-9ec2-7d5acb312f1f",
      "number": 9,
      "content": "Pê",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.548Z",
      "updatedAt": "2024-09-23T04:22:08.548Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "c94f22a7-06f5-4919-8a37-fdd2c18155e0",
      "number": 9,
      "content": "Người đem lại cho dân ơn giải thoát,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.552Z",
      "updatedAt": "2024-09-23T04:22:08.552Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "8141ab5f-976e-4fe4-88b6-83c65fac2044",
      "number": 9,
      "content": "Xa-đê",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.556Z",
      "updatedAt": "2024-09-23T04:22:08.556Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "d1ce31e3-3ef4-4696-983b-e6997beb562a",
      "number": 9,
      "content": "thiết lập giao ước đến muôn đời.",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.561Z",
      "updatedAt": "2024-09-23T04:22:08.561Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "8cc1e705-804d-4641-838d-80547549d676",
      "number": 9,
      "content": "Cốp",
      "order": 4,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.565Z",
      "updatedAt": "2024-09-23T04:22:08.565Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "bca33163-5304-400f-9f7a-2573691653cf",
      "number": 9,
      "content": "Tôn danh Người thánh thiêng khả uý.",
      "order": 5,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.570Z",
      "updatedAt": "2024-09-23T04:22:08.570Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "1d348b3a-3f50-4f10-8e65-df08537ee47d",
      "number": 10,
      "content": "Rết",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:22:08.574Z",
      "updatedAt": "2024-09-23T04:22:08.574Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "97ec7cef-3a33-4c25-924f-e9e3c3812aa9",
      "number": 10,
      "content": "Kính sợ CHÚA là đầu mối khôn ngoan.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:22:08.578Z",
      "updatedAt": "2024-09-23T04:22:08.578Z",
      "chapterId": "ce9b9214-8d3b-417a-afb4-7a5fe3a646a4"
    },
    {
      "id": "633dcde7-0edf-4f7e-aa36-8522304d17cc",
      "number": 10,
      "content": "Sin",
      "order": 2,
      "parNumber": 42,
