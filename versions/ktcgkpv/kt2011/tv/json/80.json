  "verses": [
    {
      "id": "e8299d34-a9d1-4f2a-98aa-0ec05cdd1b7e",
      "number": 1,
      "content": "*Phần ca trưởng. Theo điệu : Bông huệ chứng ước. Của ông [A-xáp](). Thánh vịnh.*",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:18:36.176Z",
      "updatedAt": "2024-09-23T04:18:36.176Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "ade0c93a-b64a-4695-9442-ccabf14c2486",
      "number": 2,
      "content": "Lạy Mục Tử nhà [Ít-ra-en](),",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.190Z",
      "updatedAt": "2024-09-23T04:18:36.190Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "88ceb24c-b255-4510-857b-f650ea787a13",
      "number": 2,
      "content": "Ngài là Đấng chăn giữ nhà [Giu-se]()",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.192Z",
      "updatedAt": "2024-09-23T04:18:36.192Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "403fe484-6756-46a2-a063-a9c9a97c869f",
      "number": 2,
      "content": "như chăn giữ chiên cừu, xin hãy lắng tai nghe !",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.194Z",
      "updatedAt": "2024-09-23T04:18:36.194Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "381c62b9-4b8f-4444-be5a-5a28f367115d",
      "number": 2,
      "content": "Ngài là Đấng ngự trên các Kê-ru-bim,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.196Z",
      "updatedAt": "2024-09-23T04:18:36.196Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "924fa50f-64d7-4fec-920e-6762cc53ce5e",
      "number": 3,
      "content": "xin giãi sáng hiển linh cho dòng dõi [Ép-ra-im](),",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.198Z",
      "updatedAt": "2024-09-23T04:18:36.198Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "d78637c8-88b4-4278-a59b-8728e7f04b88",
      "number": 3,
      "content": "[Ben-gia-min]() và [Mơ-na-se]() được thấy.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.200Z",
      "updatedAt": "2024-09-23T04:18:36.200Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "93aacf01-ed64-4111-9e9d-408158be5e0c",
      "number": 3,
      "content": "[Xin]() khơi dậy uy dũng của Ngài,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.204Z",
      "updatedAt": "2024-09-23T04:18:36.204Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "1b0cf064-ee29-4bab-8fe9-6b66bf37dd81",
      "number": 3,
      "content": "đến cùng chúng con và thương cứu độ.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.206Z",
      "updatedAt": "2024-09-23T04:18:36.206Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "73a0218d-4b73-40cd-850b-a91b212710bc",
      "number": 4,
      "content": "Lạy Thiên Chúa, xin phục hồi chúng con,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.208Z",
      "updatedAt": "2024-09-23T04:18:36.208Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "3e979e41-06c3-4ce3-9175-9462130b7971",
      "number": 8,
      "content": "xin toả ánh tôn nhan rạng ngời",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.270Z",
      "updatedAt": "2024-09-23T04:18:36.270Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "40277551-919f-45ed-89c2-c605e7faa012",
      "number": 4,
      "content": "xin toả ánh tôn nhan rạng ngời",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.214Z",
      "updatedAt": "2024-09-23T04:18:36.214Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "674f852a-7280-4ca9-9c2e-39a6b2010013",
      "number": 20,
      "content": "xin toả ánh tôn nhan rạng ngời",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.355Z",
      "updatedAt": "2024-09-23T04:18:36.355Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "2f1fe421-daf7-4137-9d83-f7db33ca5306",
      "number": 8,
      "content": "để chúng con được ơn cứu độ.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.281Z",
      "updatedAt": "2024-09-23T04:18:36.281Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "84922e29-91cd-439f-9cd3-810e03f75ea9",
      "number": 4,
      "content": "để chúng con được ơn cứu độ.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.217Z",
      "updatedAt": "2024-09-23T04:18:36.217Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "1b82fe17-0d58-4e08-a1ae-caad7d6487ea",
      "number": 20,
      "content": "để chúng con được ơn cứu độ.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.357Z",
      "updatedAt": "2024-09-23T04:18:36.357Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "9e4e3f3e-2c6d-4d24-a4c7-80b19748eb08",
      "number": 20,
      "content": "Lạy CHÚA là Chúa Tể càn khôn,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.351Z",
      "updatedAt": "2024-09-23T04:18:36.351Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "b7480d8b-64a6-4cb5-8a6e-06e43f7defd2",
      "number": 5,
      "content": "Lạy CHÚA là Chúa Tể càn khôn,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.220Z",
      "updatedAt": "2024-09-23T04:18:36.220Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "d8079b2f-ddb7-4559-93ac-22bca1782960",
      "number": 5,
      "content": "đến khi nao Chúa còn nóng giận,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.223Z",
      "updatedAt": "2024-09-23T04:18:36.223Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "9bb9114d-f232-4cb5-841f-10df4722cfc4",
      "number": 5,
      "content": "chẳng màng chi lời dân Chúa nguyện cầu ?",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.227Z",
      "updatedAt": "2024-09-23T04:18:36.227Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "d177b9aa-b4de-495a-8cf2-ca02caea2fa0",
      "number": 6,
      "content": "Cơm Ngài cho ăn chỉ là châu luỵ,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.231Z",
      "updatedAt": "2024-09-23T04:18:36.231Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "5796ac93-4cda-4268-b7a6-2fcb34b43fbd",
      "number": 6,
      "content": "nước Ngài cho uống là suối lệ chứa chan.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.235Z",
      "updatedAt": "2024-09-23T04:18:36.235Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "adcd3557-017a-4919-9257-4e368640ee7e",
      "number": 7,
      "content": "Ngài đã khiến chúng con thành cớ",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.244Z",
      "updatedAt": "2024-09-23T04:18:36.244Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "13b7319e-55fb-4513-9cf2-e0c8421ec032",
      "number": 7,
      "content": "cho lân bang cãi cọ tranh giành,",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.249Z",
      "updatedAt": "2024-09-23T04:18:36.249Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "abc78fa1-4eb6-43c1-bae3-5d570aa291b6",
      "number": 7,
      "content": "cho thù địch nhạo cười chế giễu.",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.258Z",
      "updatedAt": "2024-09-23T04:18:36.258Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "c6c9b1bd-4ec6-4a13-bace-040b206f0228",
      "number": 8,
      "content": "Lạy Chúa Tể càn khôn, xin phục hồi chúng con,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.266Z",
      "updatedAt": "2024-09-23T04:18:36.266Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "327becac-9143-4ac7-a0be-ac1896b6778a",
      "number": 9,
      "content": "[Gốc]() nho này, Chúa bứng từ [Ai-cập](),",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.285Z",
      "updatedAt": "2024-09-23T04:18:36.285Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "9fbb6d9a-0f3b-4e0e-8874-3f30eaa70983",
      "number": 9,
      "content": "đuổi chư dân, lấy chỗ mà trồng,",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.294Z",
      "updatedAt": "2024-09-23T04:18:36.294Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "0483a920-3b12-46eb-9779-27422423429a",
      "number": 10,
      "content": "Chúa khẩn hoang bốn bề quang đãng,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.297Z",
      "updatedAt": "2024-09-23T04:18:36.297Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "e17e675c-6881-40e9-a260-a273eb392b08",
      "number": 10,
      "content": "cho bén rễ sâu và lan rộng khắp nơi.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.299Z",
      "updatedAt": "2024-09-23T04:18:36.299Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "c884582b-1048-47ff-9a37-d1ba647b5a2d",
      "number": 11,
      "content": "Bóng um tùm phủ xanh đầu núi,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.302Z",
      "updatedAt": "2024-09-23T04:18:36.302Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "69175242-34e1-4665-bac1-ef62a7acfb71",
      "number": 11,
      "content": "cành sum sê rợp bá hương thần,",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.304Z",
      "updatedAt": "2024-09-23T04:18:36.304Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "b5eb748b-a86b-44cb-b49b-f3dd9c30bc23",
      "number": 12,
      "content": "nhánh vươn dài tới phía đại dương,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.308Z",
      "updatedAt": "2024-09-23T04:18:36.308Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "0adda59a-2d39-495f-ab5b-867ba6c9d9c8",
      "number": 12,
      "content": "chồi mọc xa đến tận miền Sông Cả.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.312Z",
      "updatedAt": "2024-09-23T04:18:36.312Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "e468e878-2463-490b-bbd7-947d21d4c363",
      "number": 13,
      "content": "Tường rào nó, vậy sao Ngài phá đổ ?",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.317Z",
      "updatedAt": "2024-09-23T04:18:36.317Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "89e07bdb-ce5e-400e-8831-18d69a66238c",
      "number": 13,
      "content": "Khách qua đường mặc sức hái mà ăn !",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.321Z",
      "updatedAt": "2024-09-23T04:18:36.321Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "5b316bdd-24d7-4e40-9bd1-065c83acd499",
      "number": 14,
      "content": "Heo rừng vào phá phách,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.323Z",
      "updatedAt": "2024-09-23T04:18:36.323Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "031065e7-9744-4099-b71b-94954a443fe7",
      "number": 14,
      "content": "dã thú gặm tan hoang.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.325Z",
      "updatedAt": "2024-09-23T04:18:36.325Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "70a72d25-84f9-46ae-8365-f3487055d56c",
      "number": 15,
      "content": "Lạy Chúa Tể càn khôn, xin trở lại,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.327Z",
      "updatedAt": "2024-09-23T04:18:36.327Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "2c11a88a-b1a8-434a-8fa7-31e263be6260",
      "number": 15,
      "content": "tự cõi trời, xin ngó xuống mà xem,",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.329Z",
      "updatedAt": "2024-09-23T04:18:36.329Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "da8583e7-e6e6-40ef-8c26-a6b83269d6d0",
      "number": 15,
      "content": "xin Ngài thăm nom vườn nho cũ,",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.332Z",
      "updatedAt": "2024-09-23T04:18:36.332Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "b59f6209-da3d-4a25-a3e8-9c991d9602f6",
      "number": 16,
      "content": "bảo vệ cây tay hữu Chúa đã trồng,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.334Z",
      "updatedAt": "2024-09-23T04:18:36.334Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "50140c6f-1760-4037-af45-0497d2dadcac",
      "number": 16,
      "content": "và chồi non được Ngài ban sức mạnh.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.336Z",
      "updatedAt": "2024-09-23T04:18:36.336Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "c36331b7-c42d-4f5e-87ed-1de388b89308",
      "number": 17,
      "content": "Những người đã hoả thiêu chặt phá,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.338Z",
      "updatedAt": "2024-09-23T04:18:36.338Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "21fb4327-78c7-47c9-8c54-83a8a5b5018b",
      "number": 17,
      "content": "Ngài nghiêm sắc mặt là chúng phải tiêu tan.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.340Z",
      "updatedAt": "2024-09-23T04:18:36.340Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "639e7b41-7c7e-4ef0-a584-2e56f53746f3",
      "number": 18,
      "content": "[Xin]() giơ tay bênh vực Đấng đang ngồi bên hữu",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.342Z",
      "updatedAt": "2024-09-23T04:18:36.342Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "a56079a4-3023-4b7e-9d29-3334111e2468",
      "number": 18,
      "content": "là con người được Chúa ban sức mạnh.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.344Z",
      "updatedAt": "2024-09-23T04:18:36.344Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "f6aaf339-640b-4544-bd63-415d2227666e",
      "number": 19,
      "content": "Chúng con nguyền chẳng xa Chúa nữa đâu,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.346Z",
      "updatedAt": "2024-09-23T04:18:36.346Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "4c174885-4f72-44d5-866c-5aa8f62bdd99",
      "number": 19,
      "content": "cúi xin Ngài ban cho được sống,",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.347Z",
      "updatedAt": "2024-09-23T04:18:36.347Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "5ed377b3-8ae7-4a3c-8ded-1a4117faf704",
      "number": 19,
      "content": "để chúng con xưng tụng danh Ngài.",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.349Z",
      "updatedAt": "2024-09-23T04:18:36.349Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "d7996f9e-342a-47cb-b1b9-ebd642eea2db",
      "number": 20,
      "content": "xin phục hồi chúng con,",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:18:36.353Z",
      "updatedAt": "2024-09-23T04:18:36.353Z",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    }
  ],
  "footnotes": [
    {
      "id": "dddb53e9-84e9-4ae5-8162-54abbeabc067",
      "content": "Trong *ai ca này của tập thể*, độc giả có thể thấy : dân Ít-ra-en đã gặp phải những tai hoạ lớn (cc. 13-14.17a). Những tai hoạ lớn đó có thể là hai biến cố : Sa-ma-ri bị thất thủ năm 721 tCN và thành Giê-ru-sa-lem bị chiếm đóng năm 587 tCN (hoặc năm 586 tCN). Trong cơn quẫn bách đó, dân Chúa nhắc lại lòng ưu ái của Thiên Chúa trong quá khứ và xin Người ra tay cứu giúp, nhằm phục hồi Ít-ra-en (chiếm lại những vùng đất thuộc vương quốc miền Bắc thời xưa và lấy lại ranh giới cũ dưới triều vua Đa-vít). Cấu trúc của Tv 80 : **1**) Cc. 2-4 : cc. 2-3 và c.4 ; **2**) Cc. 5-8 : cc. 5-7 và c.8 ; **3**) Cc. 9-14 : cc. 9-12 và cc. 13-14 ; **4**) Cc. 15-20 : cc. 15-17 và cc. 18-19. Cấu trúc này dựa vào điệp khúc trong cc. 4.8.20 và c.15.",
      "position": 57,
      "order": 0,
      "createdAt": "2024-09-23T04:18:36.188Z",
      "updatedAt": "2024-09-23T04:18:36.188Z",
      "headingId": "993302ba-703f-49f9-841e-795a96c54efc",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "0c2fcbc5-5a01-4d4b-9e39-0e23b994bc56",
      "content": "*Ép-ra-im* và *Mơ-na-se* (c.3) là hai chi tộc thuộc *nhà Giu-se* (c.2) nằm trong vương quốc miền Bắc ; còn chi tộc *Ben-gia-min* (c.3) thì ở trong vương quốc miền Nam, nhưng lại có hai thành Giê-ri-khô và Ghin-gan thuộc vương quốc miền Bắc.",
      "position": 41,
      "order": 1,
      "createdAt": "2024-09-23T04:18:36.202Z",
      "updatedAt": "2024-09-23T04:18:36.202Z",
      "verseId": "d78637c8-88b4-4278-a59b-8728e7f04b88",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "d3433f35-32e5-4aad-a70e-6433ba387bf4",
      "content": "Câu này có lẽ ám chỉ tình trạng của những chi tộc miền Bắc bị đạo binh Át-sua áp bức.",
      "position": 39,
      "order": 2,
      "createdAt": "2024-09-23T04:18:36.240Z",
      "updatedAt": "2024-09-23T04:18:36.240Z",
      "verseId": "5796ac93-4cda-4268-b7a6-2fcb34b43fbd",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "de7db930-4cc6-46ca-bd62-02e20e1d2508",
      "content": "Các nước lân cận giành nhau chiếm đất đai, lãnh thổ của Ít-ra-en.",
      "position": 31,
      "order": 3,
      "createdAt": "2024-09-23T04:18:36.253Z",
      "updatedAt": "2024-09-23T04:18:36.253Z",
      "verseId": "13b7319e-55fb-4513-9cf2-e0c8421ec032",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "e1044691-c2b5-41df-9d04-7c399cbe05ce",
      "content": "*Cây nho* chỉ Ít-ra-en. Chủ đề *cây nho* : x. Is 5,1-7 ; 27,2-5 ; Gr 2,21 ; 5,10 ; 6,9 ; 12,10 ; Ed 15,1-8 ; 17,6-10 ; 19,10-14 ; Hs 10,1 ; Tv 80,9-17.",
      "position": 15,
      "order": 4,
      "createdAt": "2024-09-23T04:18:36.288Z",
      "updatedAt": "2024-09-23T04:18:36.288Z",
      "verseId": "327becac-9143-4ac7-a0be-ac1896b6778a",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    },
    {
      "id": "1b4df3b8-3401-4b8c-9178-171ba7569017",
      "content": "C.12 nói đến ranh giới lý tưởng của Ít-ra-en (*Sông Cả* chỉ Êu-phơ-rát ở phía đông, *đại dương* là Địa Trung Hải ở phía tây) : x. Đnl 11,24 ; 1 V 5,1 ; Tv 72,8 ; 89,26.",
      "position": 32,
      "order": 5,
      "createdAt": "2024-09-23T04:18:36.315Z",
      "updatedAt": "2024-09-23T04:18:36.315Z",
      "verseId": "0adda59a-2d39-495f-ab5b-867ba6c9d9c8",
      "chapterId": "19238505-a563-4e18-aecd-28fe972e66f2"
    }
  ],
