  },
  "verses": [
    {
      "id": "dae46d2f-badb-4fa6-af8d-abf924c8469b",
      "number": 1,
      "content": "Năm thứ mười một, ngày đầu tháng, có lời [ĐỨC CHÚA]() phán với tôi rằng :",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:49:30.362Z",
      "updatedAt": "2024-09-23T04:49:30.362Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "ca195ed0-3614-4ae0-9307-374aee91a784",
      "number": 2,
      "content": "Hỡi con người, vì [Tia]() đã nói",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.385Z",
      "updatedAt": "2024-09-23T04:49:30.385Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "9a52e65d-f142-4290-9787-917c46da9aa6",
      "number": 2,
      "content": "chống lại [Giê-ru-sa-lem]() :",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.389Z",
      "updatedAt": "2024-09-23T04:49:30.389Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "49d4431c-11f6-416c-9edc-7c39dca3900d",
      "number": 2,
      "content": "“A ha !",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.393Z",
      "updatedAt": "2024-09-23T04:49:30.393Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "acb7fe20-e37e-4a92-aa99-6e0021fd2990",
      "number": 2,
      "content": "Cửa của các dân đã tan nát ;",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.402Z",
      "updatedAt": "2024-09-23T04:49:30.402Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "32fbe4c4-ec59-4646-a64d-7366b1efa89a",
      "number": 2,
      "content": "đến lượt ta phú túc,",
      "order": 4,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.406Z",
      "updatedAt": "2024-09-23T04:49:30.406Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "a4aa8eab-5264-496e-ad64-8133c7a90301",
      "number": 2,
      "content": "còn nó phải tan hoang”,",
      "order": 5,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.410Z",
      "updatedAt": "2024-09-23T04:49:30.410Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "b502d0ac-df63-49eb-8780-2c200cc9d949",
      "number": 3,
      "content": "nên Chúa Thượng",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.414Z",
      "updatedAt": "2024-09-23T04:49:30.414Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "0e1bcd3a-bb0d-4bd8-9a66-e564909a2378",
      "number": 3,
      "content": "là [ĐỨC CHÚA]() phán thế này :",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.418Z",
      "updatedAt": "2024-09-23T04:49:30.418Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "a6858495-9327-4f5c-aa8a-3e63a8c1404c",
      "number": 7,
      "content": "là [ĐỨC CHÚA]() phán thế này :",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.513Z",
      "updatedAt": "2024-09-23T04:49:30.513Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "5c6615bb-86ff-447e-9612-89a7031a5b64",
      "number": 3,
      "content": "Ta sẽ trừng phạt ngươi, hỡi [Tia](),",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.422Z",
      "updatedAt": "2024-09-23T04:49:30.422Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "8671d405-70e5-4abf-8cfe-ca3451b73057",
      "number": 3,
      "content": "sẽ làm cho nhiều dân tộc",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.430Z",
      "updatedAt": "2024-09-23T04:49:30.430Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "1ccf23c8-6a3f-4c35-b7bb-f94dcac5af2f",
      "number": 3,
      "content": "nổi lên chống lại ngươi",
      "order": 4,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.434Z",
      "updatedAt": "2024-09-23T04:49:30.434Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "3708015e-71ac-48c1-bb6b-7bde8adeb9ac",
      "number": 3,
      "content": "như biển khơi dậy sóng.",
      "order": 5,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.439Z",
      "updatedAt": "2024-09-23T04:49:30.439Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "cecbdab8-e1ba-4551-9bd7-a67553efe46e",
      "number": 4,
      "content": "Chúng sẽ triệt hạ",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.443Z",
      "updatedAt": "2024-09-23T04:49:30.443Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "de73760a-e36d-479d-992d-3a1a28035322",
      "number": 4,
      "content": "tường luỹ thành [Tia](),",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.447Z",
      "updatedAt": "2024-09-23T04:49:30.447Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "e1a2fcf3-86b4-477b-a94d-8d7bcec239cd",
      "number": 4,
      "content": "sẽ phá huỷ các tháp ;",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.451Z",
      "updatedAt": "2024-09-23T04:49:30.451Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "7dce5e92-c37b-400c-a11f-9ba008fbb2a5",
      "number": 4,
      "content": "còn Ta sẽ quét bụi khỏi thành",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.455Z",
      "updatedAt": "2024-09-23T04:49:30.455Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "2a63b740-a339-43f2-acbc-d016cb980889",
      "number": 4,
      "content": "và biến nó ra núi đá trọc.",
      "order": 4,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.459Z",
      "updatedAt": "2024-09-23T04:49:30.459Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "8b99f3c9-c92a-461b-9c4a-d3040f385245",
      "number": 5,
      "content": "Nó sẽ thành chỗ phơi lưới,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.463Z",
      "updatedAt": "2024-09-23T04:49:30.463Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "0ad6bf54-93d6-48e9-beb4-53b01bf602db",
      "number": 5,
      "content": "giữa biển khơi,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.467Z",
      "updatedAt": "2024-09-23T04:49:30.467Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "9c5f6859-2a21-4a66-ac9f-c67ca47a1364",
      "number": 5,
      "content": "bởi vì chính Ta đã phán,",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.471Z",
      "updatedAt": "2024-09-23T04:49:30.471Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "cccb392c-2a3f-42a2-859d-1f7e0a001132",
      "number": 5,
      "content": "– sấm ngôn của Chúa Thượng",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.475Z",
      "updatedAt": "2024-09-23T04:49:30.475Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "d7ad32dc-5f59-4b91-b770-220e5b4f1273",
      "number": 14,
      "content": "– sấm ngôn của Chúa Thượng",
      "order": 7,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.783Z",
      "updatedAt": "2024-09-23T04:49:30.783Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "2c9da500-135c-4d17-ac1d-a95a1b38f725",
      "number": 5,
      "content": "là [ĐỨC CHÚA]().",
      "order": 4,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.479Z",
      "updatedAt": "2024-09-23T04:49:30.479Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "d3816d2f-beb8-45af-b321-16ca068d52e6",
      "number": 14,
      "content": "là [ĐỨC CHÚA]().",
      "order": 8,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.788Z",
      "updatedAt": "2024-09-23T04:49:30.788Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "7b7a13f9-d6bd-457f-ad42-d7efb36ac35a",
      "number": 5,
      "content": "Nó sẽ là mồi ngon cho các dân tộc.",
      "order": 5,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.482Z",
      "updatedAt": "2024-09-23T04:49:30.482Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "b1b475c0-b499-4e40-9b39-e18fca3cb452",
      "number": 6,
      "content": "Các con gái của nó",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.486Z",
      "updatedAt": "2024-09-23T04:49:30.486Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "2e4de69b-6915-46a3-8ec8-fa1711eeb00d",
      "number": 6,
      "content": "đang ở ngoài đồng",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.494Z",
      "updatedAt": "2024-09-23T04:49:30.494Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "14b516ed-1834-427c-8cf6-d0ff68678e16",
      "number": 6,
      "content": "sẽ bị gươm tàn sát ;",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.497Z",
      "updatedAt": "2024-09-23T04:49:30.497Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "3b341926-b07d-442c-9ef6-edba2ad96315",
      "number": 6,
      "content": "bấy giờ, chúng sẽ nhận biết",
      "order": 3,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.501Z",
      "updatedAt": "2024-09-23T04:49:30.501Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "7c0d7883-03dd-41de-bd9a-8629a4d7e564",
      "number": 6,
      "content": "chính Ta là [ĐỨC CHÚA]().",
      "order": 4,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.505Z",
      "updatedAt": "2024-09-23T04:49:30.505Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "d0d183c2-92f2-46e8-802d-7c9144a0284c",
      "number": 7,
      "content": "Quả vậy, Chúa Thượng",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.509Z",
      "updatedAt": "2024-09-23T04:49:30.509Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "7fbd1d3b-e724-4762-bc90-26b943d01ff7",
      "number": 7,
      "content": "Đây, từ phương bắc,",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.518Z",
      "updatedAt": "2024-09-23T04:49:30.518Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "e64ab99c-1034-4d17-a9fc-5654e99f32f9",
      "number": 7,
      "content": "Ta sắp dẫn đến [Tia]()",
      "order": 3,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.521Z",
      "updatedAt": "2024-09-23T04:49:30.521Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "07e54637-d974-45b7-89b2-cf800a0a68b6",
      "number": 7,
      "content": "[Na-bu-cô-đô-nô-xo](),",
      "order": 4,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.525Z",
      "updatedAt": "2024-09-23T04:49:30.525Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "1068177e-f7c1-4139-bdc8-b18988a038e0",
      "number": 7,
      "content": "vua [Ba-by-lon](), vua các vua,",
      "order": 5,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.533Z",
      "updatedAt": "2024-09-23T04:49:30.533Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "b58666a4-fb61-4dfe-829c-6f03c96e2b20",
      "number": 7,
      "content": "cùng với ngựa,",
      "order": 6,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.537Z",
      "updatedAt": "2024-09-23T04:49:30.537Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "ec081c21-8fb8-4dad-8bb2-ac9f1207902b",
      "number": 7,
      "content": "chiến xa và kỵ mã,",
      "order": 7,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.542Z",
      "updatedAt": "2024-09-23T04:49:30.542Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "759caffb-d86d-4862-ac04-09a78bdde4e7",
      "number": 7,
      "content": "một đoàn lũ",
      "order": 8,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.547Z",
      "updatedAt": "2024-09-23T04:49:30.547Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "e584b165-b561-4857-9602-da4a94d2c935",
      "number": 7,
      "content": "và một đám dân đông đảo.",
      "order": 9,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.552Z",
      "updatedAt": "2024-09-23T04:49:30.552Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "02f3d9d8-6cd5-4636-af87-4e622482dfc5",
      "number": 8,
      "content": "Các con gái của ngươi",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.560Z",
      "updatedAt": "2024-09-23T04:49:30.560Z",
      "chapterId": "8bd80990-e698-4c25-9086-b30584d17e00"
    },
    {
      "id": "e6023861-0f4a-4c1d-a69d-95e51331997c",
      "number": 8,
      "content": "đang ở ngoài đồng,",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T04:49:30.564Z",
