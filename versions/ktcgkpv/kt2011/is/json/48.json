      "chapterId": "14ae7636-c914-440b-92e4-f27403219b0e"
    },
    {
      "id": "6a3f6bb2-796c-4e7f-bb98-f2118c4b1de2",
      "content": "Vận mệnh [Ít-ra-en]()",
      "order": 0,
      "createdAt": "2024-09-23T04:41:29.034Z",
      "updatedAt": "2024-09-23T04:41:29.034Z",
      "verseId": "3841ec01-0717-451f-8f1c-8103550ceb7a",
      "chapterId": "14ae7636-c914-440b-92e4-f27403219b0e"
    },
    {
      "id": "e37ff9fd-1d4b-40e7-903d-2d41ebc39efb",
      "content": "Thời lưu đày chấm dứt",
      "order": 0,
      "createdAt": "2024-09-23T04:41:29.132Z",
      "updatedAt": "2024-09-23T04:41:29.132Z",
      "verseId": "6744b844-3c17-43bf-9875-278770b560b2",
