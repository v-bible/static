    },
    {
      "id": "9d40c13a-15e7-4523-a427-5d1dfcddbc69",
      "content": "St 19,4-11",
      "position": 5,
      "order": 2,
      "createdAt": "2024-09-23T04:44:33.364Z",
      "updatedAt": "2024-09-23T04:44:33.364Z",
      "verseId": "a502bd49-3b22-4eeb-b9ea-8cf7f3fa98f6",
      "chapterId": "b7a8b5d8-b2eb-4c8d-934b-c586de244866"
