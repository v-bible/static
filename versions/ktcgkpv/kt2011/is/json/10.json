      "chapterId": "d209129e-b003-4e45-ae83-61a19c78bce9"
    },
    {
      "id": "d449fa7f-4147-4207-a506-8d9e0ec93b86",
      "content": "Tin tưởng vào Thiên Chúa",
      "order": 0,
      "createdAt": "2024-09-23T04:41:12.418Z",
      "updatedAt": "2024-09-23T04:41:12.418Z",
      "verseId": "e7633f66-8ee7-4eed-8818-034cc48721dc",
      "chapterId": "d209129e-b003-4e45-ae83-61a19c78bce9"
    },
    {
      "id": "83766a02-06e4-425d-bbff-11a34870edbc",
      "content": "Cuộc xâm lăng",
      "order": 0,
      "createdAt": "2024-09-23T04:41:12.506Z",
      "updatedAt": "2024-09-23T04:41:12.506Z",
