      "number": 10,
      "content": "con cái ông [Ba-ni]() : sáu trăm bốn mươi hai ;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 8,
      "isPoetry": false,
      "createdAt": "2024-09-23T04:06:03.051Z",
      "updatedAt": "2024-09-23T04:06:03.051Z",
