    },
    {
      "id": "cedc69cd-bdad-4052-9762-5c4c472fa321",
      "content": "Cn 11,1",
      "position": 0,
      "order": 5,
      "createdAt": "2024-09-23T04:28:32.201Z",
      "updatedAt": "2024-09-23T04:28:32.201Z",
      "verseId": "39858b09-b6e1-46e9-b8b3-9daab337e839",
      "chapterId": "d6305a3f-c393-4168-8d91-ec75cb4f80e4"
