    },
    {
      "id": "0d49e0bf-3b7a-4a8f-970f-d4b2cd22bbfc",
      "content": "Pê-nu-ên nghĩa là mặt của Đức Chúa Trời",
      "position": 30,
      "order": 1,
      "createdAt": "2024-09-07T11:57:24.695Z",
      "updatedAt": "2024-09-07T11:57:24.695Z",
