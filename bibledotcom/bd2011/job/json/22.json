    {
      "id": "344d20e7-dad3-4cec-81ef-2e2de61fc0bf",
      "content": "Ê-li-pha Cho Rằng Sự Gian Ác của Gióp Rất Lớn",
      "order": 0,
      "createdAt": "2024-09-07T13:48:47.688Z",
      "updatedAt": "2024-09-07T13:48:47.688Z",
      "verseId": "8d0e2546-a6a7-4713-bee5-46c4680f38ae",
      "chapterId": "67d45c96-839e-4f91-bad0-78bb06c43b2a"
    }
  ],
