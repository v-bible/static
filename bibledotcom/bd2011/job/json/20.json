    {
      "id": "8354de2f-3b34-40e8-b491-5da94d2b6a21",
      "content": "Xô-pha Cho Rằng Kẻ Gian Ác Bị Phạt Là Đích Đáng",
      "order": 0,
      "createdAt": "2024-09-07T13:47:49.109Z",
      "updatedAt": "2024-09-07T13:47:49.109Z",
