{
  "chapter": {
    "id": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740",
    "number": 39,
    "ref": "https://www.bible.com/bible/19/JOB.39.BD2011",
    "createdAt": "2024-09-23T05:08:40.195Z",
    "updatedAt": "2024-09-23T05:08:40.195Z",
    "bookId": "897377b4-2f5e-4199-9bc0-08481d6cc53c"
  },
  "verses": [
    {
      "id": "143c63ca-3d44-4663-b996-8759bcba5706",
      "number": 1,
      "content": "Ngươi có biết khi nào các dê rừng sinh đẻ?",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.727Z",
      "updatedAt": "2024-09-23T05:42:31.727Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "3024c698-0a8c-4dea-85bd-cf322fac16cd",
      "number": 1,
      "content": "Ngươi có nhớ lúc nào các nai cái sinh con?",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.737Z",
      "updatedAt": "2024-09-23T05:42:31.737Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "a44412d6-8da3-4623-95b7-5c6b2e70b056",
      "number": 2,
      "content": "Ngươi có đếm nó mang thai mấy tháng?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.739Z",
      "updatedAt": "2024-09-23T05:42:31.739Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "cdec7e41-50f3-40ea-9f36-50d857eefb21",
      "number": 2,
      "content": "Ngươi có hay khi nào nó đẻ con chăng?",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.741Z",
      "updatedAt": "2024-09-23T05:42:31.741Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "9d1ec677-a281-4128-a19a-9f5ee68793e2",
      "number": 3,
      "content": "Chúng quỵ xuống và sinh ra con nhỏ;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.743Z",
      "updatedAt": "2024-09-23T05:42:31.743Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "3ee55723-5c83-4c24-9e83-e866c9be976c",
      "number": 3,
      "content": "Sinh xong rồi chúng quên ngay nỗi đau đớn lúc sinh con.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.745Z",
      "updatedAt": "2024-09-23T05:42:31.745Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "9b3c2a17-0443-425a-8da0-6978112d6988",
      "number": 4,
      "content": "Các con chúng lớn lên trong đồng hoang khỏe mạnh;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.746Z",
      "updatedAt": "2024-09-23T05:42:31.746Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "7acaf069-b6b7-4ac9-988d-0b5d88cb4d9d",
      "number": 4,
      "content": "Các con chúng ra đi và không trở về với mẹ chúng nữa.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.748Z",
      "updatedAt": "2024-09-23T05:42:31.748Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "a6d3b6c4-3cc7-497d-9e67-461889504514",
      "number": 5,
      "content": "Ai ban cho lừa rừng được tự do chạy nhảy?",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.750Z",
      "updatedAt": "2024-09-23T05:42:31.750Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "0b85a757-3f2f-4d46-9e1d-8c99b7a2b676",
      "number": 5,
      "content": "Ai mở dây cho lừa hoang để nhanh chân chạy nhảy thế kia?",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.752Z",
      "updatedAt": "2024-09-23T05:42:31.752Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "58f8cad0-bdcc-481a-adbd-6e5900228480",
      "number": 6,
      "content": "Ta ban cho nó đồng hoang làm nhà để ở,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.754Z",
      "updatedAt": "2024-09-23T05:42:31.754Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "87132292-eef5-4dd4-b3d5-e918cff2fd93",
      "number": 6,
      "content": "Và ban vùng đất mặn khô cằn làm chỗ trú ngày đêm.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.756Z",
      "updatedAt": "2024-09-23T05:42:31.756Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "25d523f1-7354-48d9-99b3-94cefb6a7b5f",
      "number": 7,
      "content": "Nó khinh bỉ tiếng ồn ào nơi thành thị;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.758Z",
      "updatedAt": "2024-09-23T05:42:31.758Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "902c8992-6192-4489-a01f-0b7aa07ebe11",
      "number": 7,
      "content": "Nó không muốn nghe tiếng quát tháo của người đánh xe.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.760Z",
      "updatedAt": "2024-09-23T05:42:31.760Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "cdc415dc-922b-49df-b108-1ec1f9425cc5",
      "number": 8,
      "content": "Nó dong ruổi khắp núi rừng nơi nó chọn làm đồng cỏ;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.762Z",
      "updatedAt": "2024-09-23T05:42:31.762Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "1bf24320-1203-4b64-9e31-4274abaa9e56",
      "number": 8,
      "content": "Đó là nơi nó tìm từng ngọn cỏ xanh non.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.764Z",
      "updatedAt": "2024-09-23T05:42:31.764Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "f68fccbb-3fbb-46df-a2fe-e1966994c1e8",
      "number": 9,
      "content": "Con bò mộng há muốn làm tôi cho ngươi sao?",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.766Z",
      "updatedAt": "2024-09-23T05:42:31.766Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "fd13d830-6ea8-43b8-9c2a-a007bb99a514",
      "number": 9,
      "content": "Há nó muốn ngủ suốt đêm bên cạnh máng cỏ sao?",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.768Z",
      "updatedAt": "2024-09-23T05:42:31.768Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "7547971b-c20b-40e6-9f99-44a4d46cf90e",
      "number": 10,
      "content": "Ngươi có thể lấy dây cột con bò mộng vào ách để kéo cày sao?",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.770Z",
      "updatedAt": "2024-09-23T05:42:31.770Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "079b343f-abd6-40e6-9d0c-44c809436a26",
      "number": 10,
      "content": "Nó há muốn cày ruộng cho ngươi trong thung lũng sao?",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.772Z",
      "updatedAt": "2024-09-23T05:42:31.772Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "92fa45b7-1eca-4835-941a-4af15c08b214",
      "number": 11,
      "content": "Ngươi sẽ cậy vào nó vì sức lực của nó mạnh mẽ sao?",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.774Z",
      "updatedAt": "2024-09-23T05:42:31.774Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "3a5869c2-a179-416d-b2a5-20287eee56f6",
      "number": 11,
      "content": "Ngươi sẽ giao công việc nặng nhọc của đồng áng cho nó sao?",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.775Z",
      "updatedAt": "2024-09-23T05:42:31.775Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "0f06aefa-428b-4d32-babd-5adfb8b52c75",
      "number": 12,
      "content": "Ngươi tin rằng nó có thể mang lúa về,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.777Z",
      "updatedAt": "2024-09-23T05:42:31.777Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "4fd517dc-c253-407f-9e36-d8694b64e5a4",
      "number": 12,
      "content": "Và đem lúa vào sân đập lúa của ngươi sao?",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.779Z",
      "updatedAt": "2024-09-23T05:42:31.779Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "c25c4e0d-582a-4837-a3c2-ba78622a3c9c",
      "number": 13,
      "content": "Chim đà điểu vỗ cánh điên cuồng;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.781Z",
      "updatedAt": "2024-09-23T05:42:31.781Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "772236b6-79c0-4691-8a3d-33023ba28085",
      "number": 13,
      "content": "Nhưng cánh và lông ấy không thể sánh bằng cánh hạc.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.783Z",
      "updatedAt": "2024-09-23T05:42:31.783Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "9db4ce3d-95c3-4902-b072-7ea95aadd204",
      "number": 14,
      "content": "Vì nó đẻ trứng rồi trao cho đất;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.785Z",
      "updatedAt": "2024-09-23T05:42:31.785Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "0402791e-c8da-4f69-94b9-5243d08c2c69",
      "number": 14,
      "content": "Để hơi ấm của đất ấp các trứng nó nở ra.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.786Z",
      "updatedAt": "2024-09-23T05:42:31.786Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "339ab5a8-654b-4eb5-a9ad-35123256a484",
      "number": 15,
      "content": "Nó quên rằng một bàn chân có thể đạp bể các trứng ấy,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.788Z",
      "updatedAt": "2024-09-23T05:42:31.788Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "cf0b4289-d2bd-44df-a755-cc707936bbf6",
      "number": 15,
      "content": "Hoặc thú hoang có thể giẫm nát các trứng ấy đi.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.790Z",
      "updatedAt": "2024-09-23T05:42:31.790Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "d0276136-51fc-46b9-9a67-1600655996e0",
      "number": 16,
      "content": "Nó đối xử với các con nó thô bạo như thể chúng không là con nó;",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.792Z",
      "updatedAt": "2024-09-23T05:42:31.792Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "f30f0cf7-7f6b-4348-8021-63320f68d472",
      "number": 16,
      "content": "Nó chẳng màng dù công sức của nó luống công.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.794Z",
      "updatedAt": "2024-09-23T05:42:31.794Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "213cb638-3def-4c80-82bf-08d34b112d8e",
      "number": 17,
      "content": "Vì Đức Chúa Trời đã không ban khôn ngoan cho nó;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.796Z",
      "updatedAt": "2024-09-23T05:42:31.796Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "7f8a57ec-92bd-4100-9f5a-91043e4d8f70",
      "number": 17,
      "content": "Ngài cũng không phú thông sáng cho nó.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.801Z",
      "updatedAt": "2024-09-23T05:42:31.801Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "541f6a84-7a31-4754-8c4b-b7a20f462911",
      "number": 18,
      "content": "Thế nhưng khi nó đập cánh để bắt đầu vụt chạy,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.807Z",
      "updatedAt": "2024-09-23T05:42:31.807Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "f20d3c16-8574-4c3c-9438-d1c2b28949d4",
      "number": 18,
      "content": "Thì cả ngựa và người cỡi ngựa đều bị nó nhạo cười.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.810Z",
      "updatedAt": "2024-09-23T05:42:31.810Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "17180eb4-7b64-4099-bcd3-5365f13323f0",
      "number": 19,
      "content": "Có phải ngươi ban cho ngựa sức lực dẻo dai,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.813Z",
      "updatedAt": "2024-09-23T05:42:31.813Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "1f082000-4d6a-46db-9ae0-bde91669d25f",
      "number": 19,
      "content": "Hay ngươi khoác trên cổ ngựa cái bờm lông dài óng ả?",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.816Z",
      "updatedAt": "2024-09-23T05:42:31.816Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "94874c6b-2fd5-4015-868d-d1744f3d668d",
      "number": 20,
      "content": "Ngươi có thể khiến nó phóng tới như con cào cào búng nhảy?",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.818Z",
      "updatedAt": "2024-09-23T05:42:31.818Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "7ecce33f-c0da-4d2e-a462-05782d36c85a",
      "number": 20,
      "content": "Tiếng hí dương oai của nó làm người ta hồn phi phách tán.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.821Z",
      "updatedAt": "2024-09-23T05:42:31.821Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "4e159c2c-6d4c-4697-a97d-a8228a6daa51",
      "number": 21,
      "content": "Nó giậm chân hất đất dưới thung lũng và lấy làm hứng chí vì sức lực của nó;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.824Z",
      "updatedAt": "2024-09-23T05:42:31.824Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "c1d3b14e-67b3-47d8-8aa5-4a4efbbc2e66",
      "number": 21,
      "content": "Nó lao mình vào chiến trận và bất chấp gươm đao.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.826Z",
      "updatedAt": "2024-09-23T05:42:31.826Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "c3c445c4-bd9b-495d-b5a8-bd4f6a160448",
      "number": 22,
      "content": "Nó cười ngạo nghễ trước sự sợ hãi, và nó chẳng sợ gì;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.829Z",
      "updatedAt": "2024-09-23T05:42:31.829Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "4573f4a2-aad8-48c3-bbfe-7fe24b6df507",
      "number": 22,
      "content": "Trước rừng gươm nhọn nó không nao núng bỏ chạy.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.832Z",
      "updatedAt": "2024-09-23T05:42:31.832Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "564d8afb-78c8-491d-a836-3158399dd79b",
      "number": 23,
      "content": "Trên lưng nó tên trong ống đụng nhau kêu lách cách;",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.835Z",
      "updatedAt": "2024-09-23T05:42:31.835Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "13e7f451-b854-4091-8767-aa01b2becf65",
      "number": 23,
      "content": "Giáo và lao lóng lánh nó chẳng coi ra gì.",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.837Z",
      "updatedAt": "2024-09-23T05:42:31.837Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "e52b57a9-2289-400e-a077-060cad5a3972",
      "number": 24,
      "content": "Nó hăng tiết, hí lên, và lao vào chiến trận;",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.840Z",
      "updatedAt": "2024-09-23T05:42:31.840Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "4b9d36a8-d35c-4e0e-8bdc-937fd218685d",
      "number": 24,
      "content": "Nó không thể đứng yên khi nghe kèn xung trận thổi vang.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.843Z",
      "updatedAt": "2024-09-23T05:42:31.843Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "91c16fdc-0216-4c96-92f2-c68bee64c31b",
      "number": 25,
      "content": "Khi kèn trận thổi, nó nói, ‘A ha!’",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.846Z",
      "updatedAt": "2024-09-23T05:42:31.846Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "2c3d109e-a517-4db5-9431-97e2d46ac70e",
      "number": 25,
      "content": "Nó có thể đánh mùi chiến trận từ xa;",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.848Z",
      "updatedAt": "2024-09-23T05:42:31.848Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "c14711ae-382c-45ae-b9c0-4ac2f6471063",
      "number": 25,
      "content": "Nó có thể phân biệt tiếng hô xung trận của các vị chỉ huy và tiếng thét xáp chiến của đoàn quân.",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.851Z",
      "updatedAt": "2024-09-23T05:42:31.851Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "1f2db3d4-a86f-4cdb-bab0-22ab0b0db549",
      "number": 26,
      "content": "Có phải nhờ sự khôn ngoan của ngươi mà ó diều bay được?",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.854Z",
      "updatedAt": "2024-09-23T05:42:31.854Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "05f3740b-4836-4d7a-bba1-30bde7e80ba4",
      "number": 26,
      "content": "Hay nhờ ngươi mà nó có thể dang cánh bay tận trời nam?",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.856Z",
      "updatedAt": "2024-09-23T05:42:31.856Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "69aa6c71-d78a-4aba-88d8-7665fce67c83",
      "number": 27,
      "content": "Có phải vâng lịnh ngươi mà phượng hoàng cất cánh,",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.859Z",
      "updatedAt": "2024-09-23T05:42:31.859Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "158e542c-70da-43f0-aae4-eac375412ecb",
      "number": 27,
      "content": "Bay vút trời xanh và làm tổ trên cao?",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.862Z",
      "updatedAt": "2024-09-23T05:42:31.862Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "ddc19149-c71a-4d46-b405-a8ae753c0330",
      "number": 28,
      "content": "Nó chọn kẽ nứt của gành đá trên cao làm nhà để ở;",
      "order": 0,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.865Z",
      "updatedAt": "2024-09-23T05:42:31.865Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "c0d9549a-d63b-43d5-a81e-bbb82dade71a",
      "number": 28,
      "content": "Trong hốc đá trên non nó chọn làm chỗ ở kiên cố cho mình.",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.868Z",
      "updatedAt": "2024-09-23T05:42:31.868Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "af97e3de-81e4-4436-b4e6-beb2e40e5af8",
      "number": 29,
      "content": "Từ trên đó nó phóng tầm mắt ra xa để kiếm mồi;",
      "order": 0,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.871Z",
      "updatedAt": "2024-09-23T05:42:31.871Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "44de5cb6-ccf3-4789-ab1c-89c2c65a7caf",
      "number": 29,
      "content": "Mắt nó có thể nhìn thấy con mồi ở tận đằng xa.",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.874Z",
      "updatedAt": "2024-09-23T05:42:31.874Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "0e9ea845-16fa-47a8-a04f-49921cede1dd",
      "number": 30,
      "content": "Các con nhỏ của nó nuốt lấy máu của con mồi;",
      "order": 0,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.876Z",
      "updatedAt": "2024-09-23T05:42:31.876Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    },
    {
      "id": "08c15fbb-ad53-46db-a904-0c133bc76089",
      "number": 30,
      "content": "Hễ xác chết ở đâu nó sẽ bâu lại đó.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:31.879Z",
      "updatedAt": "2024-09-23T05:42:31.879Z",
      "chapterId": "ca91a8bd-439d-4b47-bd2d-c3c9113d0740"
    }
  ],
