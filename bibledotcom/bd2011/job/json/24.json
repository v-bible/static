    {
      "id": "72a6e44c-72db-46c6-94a8-bb8db3ddd578",
      "content": "Gióp Tin Rằng Thế Gian Còn Nhiều Bạo Ngược",
      "order": 0,
      "createdAt": "2024-09-23T05:41:46.455Z",
      "updatedAt": "2024-09-23T05:41:46.455Z",
