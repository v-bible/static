    },
    {
      "id": "39c8e05f-66d9-41d8-bcc9-d73a19d9ea8e",
      "content": "Lời Cầu Nguyện của Gióp Khi Ông Ngã Lòng",
      "order": 0,
      "createdAt": "2024-09-23T05:40:52.716Z",
      "updatedAt": "2024-09-23T05:40:52.716Z",
