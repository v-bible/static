{
  "chapter": {
    "id": "319199a4-334e-4d0d-adfc-d66b1a111ab8",
    "number": 8,
    "ref": "https://www.bible.com/bible/19/JOB.8.BD2011",
    "createdAt": "2024-09-07T11:51:10.322Z",
    "updatedAt": "2024-09-07T11:51:10.322Z",
    "bookId": "668a42b3-264b-4e0e-8b01-464c98299499"
  },
  "verses": [
    {
      "id": "c106230c-292b-4cfd-a4e4-9b61d1327e36",
      "number": 1,
      "content": "Bấy giờ Binh-đát người Su-a đáp lời và nói:",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:44:21.647Z",
      "updatedAt": "2024-09-07T13:44:21.647Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "f4f73443-f783-4c75-8c99-51ea603a7cda",
      "number": 2,
      "content": "Anh cứ nói những điều ấy cho đến bao giờ mới thôi?",
      "order": 0,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.656Z",
      "updatedAt": "2024-09-07T13:44:21.656Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "bceb845d-4587-4e51-b121-3324a4101055",
      "number": 2,
      "content": "Anh cứ nói ào ào như cuồng phong thổi tới.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.656Z",
      "updatedAt": "2024-09-07T13:44:21.656Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "91d9f47b-7e38-4985-9741-7e75c9477fa2",
      "number": 3,
      "content": "Đức Chúa Trời có bao giờ bẻ cong công lý chăng?",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.659Z",
      "updatedAt": "2024-09-07T13:44:21.659Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "9fe71d59-2cb6-4749-baa4-417250b15455",
      "number": 3,
      "content": "Đấng Toàn Năng há uốn cong lẽ chính trực hay sao?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.659Z",
      "updatedAt": "2024-09-07T13:44:21.659Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "d826172a-36fc-4f9f-95c7-7fb6e679b7df",
      "number": 4,
      "content": "Nếu các con anh đã phạm tội đối với Ngài,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.662Z",
      "updatedAt": "2024-09-07T13:44:21.662Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "6da62f40-75a9-469d-a55a-6c7b895a72dc",
      "number": 4,
      "content": "Thì Ngài bắt chúng phải gánh lấy hậu quả tội lỗi của chúng;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.662Z",
      "updatedAt": "2024-09-07T13:44:21.662Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "6e4e323c-37c6-460f-a7f9-30e1bf343fc9",
      "number": 5,
      "content": "Nếu anh chuyên tâm tìm kiếm Đức Chúa Trời,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.664Z",
      "updatedAt": "2024-09-07T13:44:21.664Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "38efd275-1444-4eda-a23b-71c1d411a8f6",
      "number": 5,
      "content": "Và hết lòng khẩn nguyện với Đấng Toàn Năng,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.664Z",
      "updatedAt": "2024-09-07T13:44:21.664Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "4eca339e-1659-416f-a27e-116673dcc767",
      "number": 6,
      "content": "Nếu anh quả thật là người trong sạch và ngay thẳng,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.666Z",
      "updatedAt": "2024-09-07T13:44:21.666Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "d6e37c29-591d-4617-bf85-78525bf194c1",
      "number": 6,
      "content": "Thì bây giờ Ngài sẽ đứng dậy để binh vực anh,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.666Z",
      "updatedAt": "2024-09-07T13:44:21.666Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "e4c60a7b-4898-4463-98da-02187a554c02",
      "number": 6,
      "content": "Và sẽ tái lập anh vào cương vị của một người công chính;",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.666Z",
      "updatedAt": "2024-09-07T13:44:21.666Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "76e91f83-a022-444d-89ff-dc86c8fdaa5c",
      "number": 7,
      "content": "Dù lúc bắt đầu hơi có phần khiêm tốn,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.669Z",
      "updatedAt": "2024-09-07T13:44:21.669Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "09dab59d-d7bf-4264-aa32-b01bec826f0c",
      "number": 7,
      "content": "Nhưng đến cuối cùng sẽ vĩ đại khôn lường.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.669Z",
      "updatedAt": "2024-09-07T13:44:21.669Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "258151ab-6592-4bb5-94c5-f5080337bfcc",
      "number": 8,
      "content": "Tôi xin anh hãy tìm hiểu kinh nghiệm của các bậc tiền nhân,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.671Z",
      "updatedAt": "2024-09-07T13:44:21.671Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "94fea6f4-24fc-427d-ad94-deeb1d1c5563",
      "number": 8,
      "content": "Và suy xét những gì tổ tiên của họ đã tìm thấy.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.671Z",
      "updatedAt": "2024-09-07T13:44:21.671Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "0871a182-8297-415b-9cdf-b972f2c5211b",
      "number": 9,
      "content": "Vì chúng ta mới có mặt ngày hôm qua và chẳng biết gì,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.673Z",
      "updatedAt": "2024-09-07T13:44:21.673Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "3604cc2c-5d70-401f-a387-8a2ccc2984fb",
      "number": 9,
      "content": "Vì những ngày của chúng ta trên đất qua mau như cái bóng.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.673Z",
      "updatedAt": "2024-09-07T13:44:21.673Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "da0716ca-b5b9-488c-b7a2-d1085d4343b2",
      "number": 10,
      "content": "Há chẳng phải các vị tiền bối sẽ dạy anh và bảo anh,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.675Z",
      "updatedAt": "2024-09-07T13:44:21.675Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "6d7bf5b4-e6f6-43de-82a7-c5bd9d25e29a",
      "number": 10,
      "content": "Và sẽ nói cho anh biết tri thức nằm lòng của họ hay sao?",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.675Z",
      "updatedAt": "2024-09-07T13:44:21.675Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "9a650e23-8a2d-429a-b53d-d40f2f19658f",
      "number": 11,
      "content": "Cây lau há có thể mọc lên nơi không phải bùn lầy?",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.678Z",
      "updatedAt": "2024-09-07T13:44:21.678Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "8ce459a3-f0a0-4f88-9868-c685f1ac4e49",
      "number": 11,
      "content": "Cây sậy há có thể mọc lên nơi không có nước?",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.678Z",
      "updatedAt": "2024-09-07T13:44:21.678Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "fa8269f6-56a4-4562-8588-225bc21a9be6",
      "number": 12,
      "content": "Khi đang còn xanh tươi chưa kịp cắt về,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.680Z",
      "updatedAt": "2024-09-07T13:44:21.680Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "ab7cae13-311b-4b93-8375-e8f453644364",
      "number": 12,
      "content": "Chúng đã bị khô héo trước các loài cỏ khác.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.680Z",
      "updatedAt": "2024-09-07T13:44:21.680Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "14fbdbfa-0d77-4ab1-98b3-0c6f634173f7",
      "number": 13,
      "content": "Đó là số phận của mọi kẻ quên ơn lìa bỏ Đức Chúa Trời;",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.682Z",
      "updatedAt": "2024-09-07T13:44:21.682Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "1f1f24a0-ef1f-47d1-9fd0-d872d5daa1e8",
      "number": 13,
      "content": "Hy vọng của những kẻ không tin kính Chúa sẽ tiêu vong.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.682Z",
      "updatedAt": "2024-09-07T13:44:21.682Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "e01096b4-27a3-48f4-a4ad-e1627b771121",
      "number": 14,
      "content": "Nơi nó nương dựa bị hoàn toàn sụp đổ;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.684Z",
      "updatedAt": "2024-09-07T13:44:21.684Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "fcc93000-f958-409e-b422-12eb8126f9d1",
      "number": 14,
      "content": "Chỗ nó tin cậy chỉ là màng nhện mong manh.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.684Z",
      "updatedAt": "2024-09-07T13:44:21.684Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "230f289e-4704-459a-927c-a63f760c31e7",
      "number": 15,
      "content": "Nó dựa vào căn nhà, nhưng nhà không đứng vững;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.686Z",
      "updatedAt": "2024-09-07T13:44:21.686Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "6f8f0524-5d72-4ad9-8239-f62f2b3ea4bb",
      "number": 15,
      "content": "Nó bám vào căn nhà, nhưng nhà chẳng còn được bao lâu.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.686Z",
      "updatedAt": "2024-09-07T13:44:21.686Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "aed26cf6-42a2-42bf-9344-35a0ff2badac",
      "number": 16,
      "content": "Nó như cây lớn mạnh xanh tươi dưới ánh mặt trời,",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.689Z",
      "updatedAt": "2024-09-07T13:44:21.689Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "ff0d6f17-0877-42ce-8f3e-a07405a62320",
      "number": 16,
      "content": "Các nhánh nó vươn ra tỏa rộng khắp vườn;",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.689Z",
      "updatedAt": "2024-09-07T13:44:21.689Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "a885dde0-f49d-487c-a1a3-6eca197a7cca",
      "number": 17,
      "content": "Các rễ nó quấn chằng chịt chung quanh đống đá;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.691Z",
      "updatedAt": "2024-09-07T13:44:21.691Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "76ec3c18-0ba6-4e1c-a10d-d931dc13c42a",
      "number": 17,
      "content": "Các rễ con nó len lỏi vào giữa những sỏi đá và bám vào.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.691Z",
      "updatedAt": "2024-09-07T13:44:21.691Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "3ba9202a-53ee-4e45-9571-59a9a058f178",
      "number": 18,
      "content": "Nhưng khi nó vừa bị bật gốc khỏi nơi ấy,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.693Z",
      "updatedAt": "2024-09-07T13:44:21.693Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "40fc42ae-9d4c-4367-9a68-12ac574731f4",
      "number": 18,
      "content": "Nơi ấy liền phủ nhận nó rằng, “Ta chưa hề thấy ngươi.”",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.693Z",
      "updatedAt": "2024-09-07T13:44:21.693Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "0fa95fc5-5301-4e3d-b2e6-011fd5f212f7",
      "number": 19,
      "content": "Này, đây là niềm vui của đường lối nó:",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.695Z",
      "updatedAt": "2024-09-07T13:44:21.695Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "a83c0a8e-b7a7-4fcd-b0d3-71a90edc5b06",
      "number": 19,
      "content": "Từ dưới mặt đất sẽ có các cây khác mọc lên.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.695Z",
      "updatedAt": "2024-09-07T13:44:21.695Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "ee0f0b96-1f0a-4e35-9d54-881259e0afbb",
      "number": 20,
      "content": "Này, Đức Chúa Trời sẽ không loại bỏ người vẹn toàn đạo đức;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.697Z",
      "updatedAt": "2024-09-07T13:44:21.697Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "2bd2ec30-305c-4580-a8b0-7342d49279dc",
      "number": 20,
      "content": "Ngài sẽ không phù hộ tay kẻ gian ác bao giờ.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.697Z",
      "updatedAt": "2024-09-07T13:44:21.697Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "347ba066-ce35-4d7c-951b-0a7c2bf47ae0",
      "number": 21,
      "content": "Ngài sẽ làm cho miệng anh đầy tiếng cười vui vẻ,",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.699Z",
      "updatedAt": "2024-09-07T13:44:21.699Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "62d95118-405b-4131-b9dc-a1591f979b8d",
      "number": 21,
      "content": "Ngài sẽ ban cho môi anh vang những tiếng reo mừng.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.699Z",
      "updatedAt": "2024-09-07T13:44:21.699Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "fb814783-a93c-46ab-9733-50f9212ed48b",
      "number": 22,
      "content": "Những kẻ ghét anh sẽ mặc lấy xấu hổ;",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.701Z",
      "updatedAt": "2024-09-07T13:44:21.701Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    },
    {
      "id": "569f232c-ee61-42fc-9837-542dff805026",
      "number": 22,
      "content": "Và chỗ ở của bọn gian ác sẽ không còn nữa.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:44:21.701Z",
      "updatedAt": "2024-09-07T13:44:21.701Z",
      "chapterId": "319199a4-334e-4d0d-adfc-d66b1a111ab8"
    }
  ],
