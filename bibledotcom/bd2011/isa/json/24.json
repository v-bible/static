      "chapterId": "95315df6-3313-41ae-bf9c-62bb86040f00"
    },
    {
      "id": "01e8f097-f6a5-4c17-b60b-768c9537d1c5",
      "content": "CHÚA",
      "order": 2,
      "createdAt": "2024-09-23T05:59:52.305Z",
      "updatedAt": "2024-09-23T05:59:52.305Z",
      "verseId": "bc53d7f0-8bcc-46c5-9552-8e8fa9093853",
      "chapterId": "95315df6-3313-41ae-bf9c-62bb86040f00"
    },
    {
      "id": "bd45d9b6-f973-41d2-96ec-1c6b380462dd",
      "content": "Phán Xét Thế Gian",
      "order": 3,
