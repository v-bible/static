{
  "chapter": {
    "id": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14",
    "number": 48,
    "ref": "https://www.bible.com/bible/19/ISA.48.BD2011",
    "createdAt": "2024-09-07T11:51:12.046Z",
    "updatedAt": "2024-09-07T11:51:12.046Z",
    "bookId": "dd94a7a2-46d4-489f-a1b9-a328e1eafa8e"
  },
  "verses": [
    {
      "id": "14611d66-3c17-434d-a5c5-caba8f6b3113",
      "number": 1,
      "content": "Hãy nghe đây, hỡi nhà Gia-cốp;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.693Z",
      "updatedAt": "2024-09-07T14:48:55.693Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "8383ebdd-0b2f-4b04-b9a2-7812c2f7f48a",
      "number": 1,
      "content": "Hỡi những kẻ được gọi bằng danh I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.693Z",
      "updatedAt": "2024-09-07T14:48:55.693Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "a1c75833-c3a9-41c8-845d-4d2b9423dc7b",
      "number": 1,
      "content": "Và những người ra từ nguồn cội của Giu-đa,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.693Z",
      "updatedAt": "2024-09-07T14:48:55.693Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "cc831691-9d87-42d8-8bc3-072ddc4ba111",
      "number": 1,
      "content": "Những người lấy danh CHÚA mà thề,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.693Z",
      "updatedAt": "2024-09-07T14:48:55.693Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "fcbd108b-3f00-4d9a-93ca-09456aa97ee2",
      "number": 1,
      "content": "Cầu khẩn Đức Chúa Trời của I-sơ-ra-ên,",
      "order": 4,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.693Z",
      "updatedAt": "2024-09-07T14:48:55.693Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "304821a6-f61b-4b71-a471-b5191f30db17",
      "number": 1,
      "content": "Nhưng lại không thật lòng và ngay thẳng,",
      "order": 5,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.693Z",
      "updatedAt": "2024-09-07T14:48:55.693Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "dd5ac57b-4594-48dd-9ecd-78d023c768f0",
      "number": 2,
      "content": "Dù chúng cho mình là dân của thành thánh,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.703Z",
      "updatedAt": "2024-09-07T14:48:55.703Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "ce5a064e-9fc0-479b-9581-eefb52434bf3",
      "number": 2,
      "content": "Và nương cậy vào Đức Chúa Trời của I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.703Z",
      "updatedAt": "2024-09-07T14:48:55.703Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "c79cf724-7fa6-42ab-80b6-0b55bdabdc6e",
      "number": 2,
      "content": "Tức Đấng có danh là CHÚA các đạo quân.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.703Z",
      "updatedAt": "2024-09-07T14:48:55.703Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "40526779-3c75-42c2-a2f9-761d3e21ea29",
      "number": 3,
      "content": "Những chuyện đã qua Ta đã báo trước tự ngàn xưa;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.706Z",
      "updatedAt": "2024-09-07T14:48:55.706Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "2bdacdca-bb51-47fe-98d7-8a31ed57f3d8",
      "number": 3,
      "content": "Những điều ấy đã ra từ miệng Ta, và Ta làm cho người ta biết chúng,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.706Z",
      "updatedAt": "2024-09-07T14:48:55.706Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "6c1ff7a4-8140-4f0b-a171-8e901b04e6c6",
      "number": 3,
      "content": "Rồi thình lình Ta thực hiện những điều ấy, và chúng đã xảy ra.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.706Z",
      "updatedAt": "2024-09-07T14:48:55.706Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "9a69536e-e510-4589-a7f7-d0d2d2668e00",
      "number": 4,
      "content": "Vì Ta biết ngươi là kẻ cứng đầu,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.708Z",
      "updatedAt": "2024-09-07T14:48:55.708Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "98e20b19-5d52-4481-a8d1-77847c47bf28",
      "number": 4,
      "content": "Gân cổ ngươi cứng như sắt,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.708Z",
      "updatedAt": "2024-09-07T14:48:55.708Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "d43603e0-cc30-4ee9-bfa9-348ff01a4fec",
      "number": 4,
      "content": "Trán của ngươi cứng như đồng,",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.708Z",
      "updatedAt": "2024-09-07T14:48:55.708Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "0d4faf70-6209-498d-ae47-980bac40fa06",
      "number": 5,
      "content": "Nên ngay từ ban đầu Ta đã nói cho ngươi những điều ấy;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.711Z",
      "updatedAt": "2024-09-07T14:48:55.711Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "2be31bb2-6276-437c-a8e4-ab0d4b0e7ecc",
      "number": 5,
      "content": "Trước khi chúng xảy ra, Ta đã nói về chúng cho ngươi hay,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.711Z",
      "updatedAt": "2024-09-07T14:48:55.711Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "5d67aea1-70ca-43ff-b5ae-cbcb7a4f3ade",
      "number": 5,
      "content": "Để ngươi không thể nói, “Ấy là thần tượng của tôi đã làm những điều ấy;",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.711Z",
      "updatedAt": "2024-09-07T14:48:55.711Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "4a7c1420-d1a0-4a63-a394-ec235c0d7ff0",
      "number": 5,
      "content": "Đó là tượng chạm của tôi và tượng đúc của tôi đã ra lịnh cho chúng phải xảy ra.”",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.711Z",
      "updatedAt": "2024-09-07T14:48:55.711Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "33d801f2-abbf-4fa0-a4c0-baa1aab22246",
      "number": 6,
      "content": "Ngươi đã nghe rồi; ngươi đã thấy tất cả rồi;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.714Z",
      "updatedAt": "2024-09-07T14:48:55.714Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "c6a0e5b9-15c7-4a5e-aa20-ddbef04b39af",
      "number": 6,
      "content": "Sao ngươi không tuyên bố ra đi?",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.714Z",
      "updatedAt": "2024-09-07T14:48:55.714Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "c490266a-12a4-4581-9c74-6999c47ea59e",
      "number": 6,
      "content": "Từ nay Ta sẽ cho ngươi nghe những điều mới,",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.714Z",
      "updatedAt": "2024-09-07T14:48:55.714Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "c6062a13-0b35-4d35-a991-7ef27a14fbde",
      "number": 6,
      "content": "Ngay cả những điều còn giữ kín mà ngươi chưa hề biết.",
      "order": 3,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.714Z",
      "updatedAt": "2024-09-07T14:48:55.714Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "0823ef03-729d-4d50-b3f6-f4b380a43138",
      "number": 7,
      "content": "Đến bây giờ những điều ấy mới được tạo nên, Chứ chẳng phải chúng đã có sẵn từ lâu rồi đâu;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.717Z",
      "updatedAt": "2024-09-07T14:48:55.717Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "a0005221-8737-4273-9a7d-32c5d6621c5a",
      "number": 7,
      "content": "Trước ngày này ngươi chưa bao giờ nghe về chúng,",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.717Z",
      "updatedAt": "2024-09-07T14:48:55.717Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "83020142-a8e9-4fd6-98e6-7196481a250e",
      "number": 7,
      "content": "Để ngươi không thể nói, “Ồ tưởng gì chứ những điều đó tôi đã biết cả rồi.”",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.717Z",
      "updatedAt": "2024-09-07T14:48:55.717Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "b711651e-b5ff-4ce9-928b-a1a6ab1882ae",
      "number": 8,
      "content": "Chắc chắn ngươi chưa nghe đến; Chắc chắn ngươi chưa biết đến;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.720Z",
      "updatedAt": "2024-09-07T14:48:55.720Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "efd29fd9-2ce5-48c4-ae73-243253cc47cc",
      "number": 8,
      "content": "Chắc chắn từ lâu tai ngươi chưa mở ra,",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.720Z",
      "updatedAt": "2024-09-07T14:48:55.720Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "d9864796-4b9a-4a10-8394-0accabe75d53",
      "number": 8,
      "content": "Vì Ta biết ngươi là kẻ phản bội,",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.720Z",
      "updatedAt": "2024-09-07T14:48:55.720Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "4ce36c12-b9eb-4459-9db9-20ab66042b1b",
      "number": 8,
      "content": "Ngươi mang tên là kẻ phản phúc từ lúc ngươi còn là một thai nhi.",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.720Z",
      "updatedAt": "2024-09-07T14:48:55.720Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "77735735-af67-4feb-aacb-ef75ef9fd818",
      "number": 9,
      "content": "Bấy lâu nay, vì danh Ta, Ta cam nén giận,",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.723Z",
      "updatedAt": "2024-09-07T14:48:55.723Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "b29d6ad6-5e44-456e-b1e1-f1742ff22b11",
      "number": 9,
      "content": "Vì sự ca ngợi dành cho Ta, Ta dằn lòng chịu đựng ngươi,",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.723Z",
      "updatedAt": "2024-09-07T14:48:55.723Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "c0e71255-1084-4a87-894b-dee2498e09ef",
      "number": 9,
      "content": "Mà không tiêu diệt ngươi.",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.723Z",
      "updatedAt": "2024-09-07T14:48:55.723Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "102a34ba-46f2-4972-9c90-b5c9386ab146",
      "number": 10,
      "content": "Này, Ta đã tinh luyện ngươi, nhưng không như người ta luyện bạc;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.728Z",
      "updatedAt": "2024-09-07T14:48:55.728Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "fb1768ac-899e-4b1b-99c2-ac0819466c4e",
      "number": 10,
      "content": "Ta đã rèn luyện ngươi trong lò lửa khổ đau.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.728Z",
      "updatedAt": "2024-09-07T14:48:55.728Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "d06edf63-27ff-4c8b-be1c-66e533521a34",
      "number": 11,
      "content": "Nhưng nay vì cớ Ta, vì chính Ta, Ta sẽ ra tay hành động;",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.732Z",
      "updatedAt": "2024-09-07T14:48:55.732Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "860f39db-7ffd-48fb-83a0-e1e2cb61f28b",
      "number": 11,
      "content": "Chứ lẽ nào Ta cứ để cho danh Ta bị sỉ nhục hay sao?",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.732Z",
      "updatedAt": "2024-09-07T14:48:55.732Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "8338c1f8-d13b-45d1-b63f-a96355b7649d",
      "number": 11,
      "content": "Ta sẽ không nhường vinh hiển Ta cho một kẻ nào khác.",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.732Z",
      "updatedAt": "2024-09-07T14:48:55.732Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "d68c8e77-5cbb-4418-8daf-5fec4b5d8487",
      "number": 12,
      "content": "Hỡi Gia-cốp, hãy lắng nghe Ta;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.735Z",
      "updatedAt": "2024-09-07T14:48:55.735Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "b43a4cf4-cbd1-4bc7-b94d-a12658715885",
      "number": 12,
      "content": "Hỡi I-sơ-ra-ên, người Ta đã gọi;",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.735Z",
      "updatedAt": "2024-09-07T14:48:55.735Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "42d2c011-462b-48c8-a8dd-9c661ff453c2",
      "number": 12,
      "content": "Ta là Đấng ấy, Ta là Đấng Đầu Tiên,",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.735Z",
      "updatedAt": "2024-09-07T14:48:55.735Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "de659ecd-750b-41c5-b4c3-f658609363ee",
      "number": 12,
      "content": "Ta cũng là Đấng Cuối Cùng.",
      "order": 3,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.735Z",
      "updatedAt": "2024-09-07T14:48:55.735Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "8d9f063e-aede-4ddb-a107-5471fd991170",
      "number": 13,
      "content": "Chính tay Ta đã đặt nền trái đất;",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.738Z",
      "updatedAt": "2024-09-07T14:48:55.738Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "ac1383d2-546e-4ef1-aafd-78ad88de7062",
      "number": 13,
      "content": "Tay phải Ta đã trải rộng bầu trời;",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.738Z",
      "updatedAt": "2024-09-07T14:48:55.738Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "665bfb93-3771-4d4f-9be4-c39b71b3ffd3",
      "number": 13,
      "content": "Khi Ta gọi chúng,",
      "order": 2,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.738Z",
      "updatedAt": "2024-09-07T14:48:55.738Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "0dc45936-8b54-4dd6-9bbe-0aa93dba0229",
      "number": 13,
      "content": "Chúng cùng nhau đứng dậy.",
      "order": 3,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.738Z",
      "updatedAt": "2024-09-07T14:48:55.738Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "f7438585-16f3-493d-8db3-e6b2b15a6bff",
      "number": 14,
      "content": "Hỡi tất cả các ngươi, hãy họp lại và lắng nghe!",
      "order": 0,
      "parNumber": 47,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.741Z",
      "updatedAt": "2024-09-07T14:48:55.741Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "e6fc04c1-4961-406e-8766-d3848f4132cf",
      "number": 14,
      "content": "Ai trong vòng chúng đã công bố những điều ấy?",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.741Z",
      "updatedAt": "2024-09-07T14:48:55.741Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "c00907cf-be63-4a57-b8f1-8f3cdc3a16fb",
      "number": 14,
      "content": "CHÚA yêu thương nó;",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.741Z",
      "updatedAt": "2024-09-07T14:48:55.741Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "ecfea480-e55b-4232-a8ef-495010621488",
      "number": 14,
      "content": "Ngài sẽ thực hiện điều đẹp ý Ngài đối với Ba-by-lôn;",
      "order": 3,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.741Z",
      "updatedAt": "2024-09-07T14:48:55.741Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "4f447977-43f1-40a8-b724-23296f4efa6a",
      "number": 14,
      "content": "Ngài sẽ đưa tay ra nghịch lại người Canh-đê.",
      "order": 4,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.741Z",
      "updatedAt": "2024-09-07T14:48:55.741Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "ba914681-8b10-4684-bfe7-2d84006dadca",
      "number": 15,
      "content": "Ta, chính Ta, đã phán; phải, Ta đã gọi nó;",
      "order": 0,
      "parNumber": 52,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.746Z",
      "updatedAt": "2024-09-07T14:48:55.746Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "1c547d00-63aa-44b3-aedc-45be5cc1f0fa",
      "number": 15,
      "content": "Ta đã dẫn dắt nó, nên đường lối nó sẽ thành công thịnh vượng.",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.746Z",
      "updatedAt": "2024-09-07T14:48:55.746Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "17db9733-c903-42bd-afec-53ec65d8b765",
      "number": 16,
      "content": "Hãy đến gần Ta và nghe rõ điều này:",
      "order": 0,
      "parNumber": 54,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.749Z",
      "updatedAt": "2024-09-07T14:48:55.749Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "b0ede0e8-bd83-4bb9-a5b2-20bd9d9f08e6",
      "number": 16,
      "content": "Từ ban đầu, Ta không nói điều gì bí ẩn;",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.749Z",
      "updatedAt": "2024-09-07T14:48:55.749Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "9de84898-7ccd-4f5e-b1eb-6f3cbb726f31",
      "number": 16,
      "content": "Lúc nó xảy ra, Ta đã có ở đó rồi.",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.749Z",
      "updatedAt": "2024-09-07T14:48:55.749Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "5e96cab6-6088-4dbc-bbb5-78c9def481a1",
      "number": 16,
      "content": "Và bây giờ CHÚA Hằng Hữu và Thần của Ngài đã sai tôi.",
      "order": 3,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.749Z",
      "updatedAt": "2024-09-07T14:48:55.749Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "37b3be1f-6815-4d78-9e88-1e17b9dd3c85",
      "number": 17,
      "content": "CHÚA, Đấng Cứu Chuộc ngươi,",
      "order": 0,
      "parNumber": 58,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.753Z",
      "updatedAt": "2024-09-07T14:48:55.753Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "299fc181-e137-400c-b6c0-552715c507bd",
      "number": 17,
      "content": "Đấng Thánh của I-sơ-ra-ên, phán thế này:",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.753Z",
      "updatedAt": "2024-09-07T14:48:55.753Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "aa4d325c-1f30-4a7b-9889-dd2c8c53efff",
      "number": 17,
      "content": "Ta là CHÚA, Đức Chúa Trời của ngươi,",
      "order": 2,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.753Z",
      "updatedAt": "2024-09-07T14:48:55.753Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "036c1a94-fdc0-4345-8996-0abe923b6a5a",
      "number": 17,
      "content": "Đấng dạy bảo ngươi để ngươi được phước,",
      "order": 3,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.753Z",
      "updatedAt": "2024-09-07T14:48:55.753Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "1733819f-7420-40bb-95a0-57182059be8a",
      "number": 17,
      "content": "Đấng dẫn dắt ngươi vào đường lối ngươi nên đi.",
      "order": 4,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.753Z",
      "updatedAt": "2024-09-07T14:48:55.753Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "b2cbc142-7ce5-4e67-92fe-4d87edaeea69",
      "number": 18,
      "content": "Ôi, ước chi ngươi chú ý đến các điều răn Ta,",
      "order": 0,
      "parNumber": 63,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.756Z",
      "updatedAt": "2024-09-07T14:48:55.756Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "ee8bca8b-680c-4d16-b365-e2124e38a788",
      "number": 18,
      "content": "Thì hòa bình thịnh vượng của ngươi sẽ còn mãi như dòng sông vô tận,",
      "order": 1,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.756Z",
      "updatedAt": "2024-09-07T14:48:55.756Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "cf75e66c-db80-44e9-b1a1-217857f99525",
      "number": 18,
      "content": "Sự thành công của ngươi sẽ không ngừng như sóng ở đại dương,",
      "order": 2,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.756Z",
      "updatedAt": "2024-09-07T14:48:55.756Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "65b2d934-eba1-4d03-abb7-2631fc71a744",
      "number": 19,
      "content": "Con cháu ngươi sẽ đông như cát nơi bờ biển,",
      "order": 0,
      "parNumber": 66,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.760Z",
      "updatedAt": "2024-09-07T14:48:55.760Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "4bada187-7d1a-4f37-b551-431b131ce68a",
      "number": 19,
      "content": "Dòng dõi ngươi sẽ nhiều tựa cát của đại dương,",
      "order": 1,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.760Z",
      "updatedAt": "2024-09-07T14:48:55.760Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "d2054885-f723-42ed-bad1-daa54167c856",
      "number": 19,
      "content": "Danh ngươi sẽ không bao giờ bị xóa bỏ,",
      "order": 2,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.760Z",
      "updatedAt": "2024-09-07T14:48:55.760Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "7a6f5c54-58b0-4c41-aa0b-d8654722db2c",
      "number": 19,
      "content": "Và chẳng bao giờ bị diệt mất trước mặt Ta.",
      "order": 3,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.760Z",
      "updatedAt": "2024-09-07T14:48:55.760Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "2fa9859c-dea2-411e-b10d-c679ace93d21",
      "number": 20,
      "content": "Hãy rời khỏi Ba-by-lôn, hãy trốn khỏi dân Canh-đê,",
      "order": 0,
      "parNumber": 70,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.765Z",
      "updatedAt": "2024-09-07T14:48:55.765Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "0577583d-f94f-4de6-8a77-7a57ec9435a7",
      "number": 20,
      "content": "Hãy cất tiếng hát ca, hãy tuyên bố,",
      "order": 1,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.765Z",
      "updatedAt": "2024-09-07T14:48:55.765Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "ab2ef913-3936-42ac-a950-6111f3605dac",
      "number": 20,
      "content": "Hãy công bố điều ấy ra, hãy truyền tin ấy đến tận cùng trái đất,",
      "order": 2,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.765Z",
      "updatedAt": "2024-09-07T14:48:55.765Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "48a14bbe-cc80-41af-ac1e-5f77da9c96b5",
      "number": 20,
      "content": "Bảo rằng, “CHÚA cứu chuộc Gia-cốp đầy tớ Ngài.”",
      "order": 3,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.765Z",
      "updatedAt": "2024-09-07T14:48:55.765Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "7798876e-0ff2-4b71-ba88-4a44ec904269",
      "number": 21,
      "content": "Dù Ngài dẫn họ đi qua sa mạc, họ cũng không khát nước,",
      "order": 0,
      "parNumber": 74,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.770Z",
      "updatedAt": "2024-09-07T14:48:55.770Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "dbeaaf98-64ee-4f3f-bb20-997b47803ee2",
      "number": 21,
      "content": "Vì Ngài khiến nước từ vầng đá chảy ra cho họ uống.",
      "order": 1,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.770Z",
      "updatedAt": "2024-09-07T14:48:55.770Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "eab2e42f-f66b-4b00-ad0e-65d96f0a562b",
      "number": 21,
      "content": "Ngài chẻ vầng đá, và nước đã trào ra.",
      "order": 2,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.770Z",
      "updatedAt": "2024-09-07T14:48:55.770Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    },
    {
      "id": "2919a6a6-1126-45a4-bcd8-259c480ea1b7",
      "number": 22,
      "content": "Tuy nhiên phường gian tà sẽ không có bình an, CHÚA phán.",
      "order": 0,
      "parNumber": 77,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:48:55.772Z",
      "updatedAt": "2024-09-07T14:48:55.772Z",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    }
  ],
  "footnotes": [],
  "headings": [
    {
      "id": "8d3ae267-b862-4ba4-b00b-292d8b0e8bb0",
      "content": "CHÚA Sẽ Giải Cứu I-sơ-ra-ên",
      "order": 0,
      "createdAt": "2024-09-07T14:49:12.106Z",
      "updatedAt": "2024-09-07T14:49:12.106Z",
      "verseId": "14611d66-3c17-434d-a5c5-caba8f6b3113",
      "chapterId": "b23ecaa9-3cec-4b6c-87f8-a67607f5fd14"
    }
  ],
  "references": []
}