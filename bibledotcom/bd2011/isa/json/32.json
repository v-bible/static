    },
    {
      "id": "813ae314-28d3-4e7b-bd48-da8e14056b30",
      "content": "Tự Mãn Đưa Đến Nguy Hại",
      "order": 0,
      "createdAt": "2024-09-07T14:47:02.109Z",
      "updatedAt": "2024-09-07T14:47:02.109Z",
      "verseId": "5ba97e72-7b29-41b1-889d-80fb2e5c84bf",
      "chapterId": "b59e41d0-ae5b-4304-9b17-cdbbbba907df"
    },
    {
      "id": "24897dbe-2c54-47fa-87a1-83edfaa25973",
      "content": "Nền Hòa Bình Dưới Sự Trị Vì của CHÚA",
      "order": 0,
      "createdAt": "2024-09-07T14:47:02.109Z",
      "updatedAt": "2024-09-07T14:47:02.109Z",
      "verseId": "0bc2df18-dd50-4923-ab1d-731a09e8eac1",
      "chapterId": "b59e41d0-ae5b-4304-9b17-cdbbbba907df"
    }
  ],
