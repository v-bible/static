      "chapterId": "00509e0a-2a8d-4ef3-8e6e-7ba8cf134627"
    },
    {
      "id": "90cdb0bd-7506-4740-853d-d21ec3372cf4",
      "content": "CHÚA",
      "order": 2,
      "createdAt": "2024-09-23T05:58:52.658Z",
      "updatedAt": "2024-09-23T05:58:52.658Z",
      "verseId": "2662aa29-b5da-4038-a182-4e7b981b5f13",
      "chapterId": "00509e0a-2a8d-4ef3-8e6e-7ba8cf134627"
