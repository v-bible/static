{
  "chapter": {
    "id": "a00af68d-9ae9-48b0-921e-2651275576f7",
    "number": 35,
    "ref": "https://www.bible.com/bible/19/ISA.35.BD2011",
    "createdAt": "2024-09-23T05:08:41.023Z",
    "updatedAt": "2024-09-23T05:08:41.023Z",
    "bookId": "655961d7-6793-43d1-be06-cff911774d08"
  },
  "verses": [
    {
      "id": "084fe0aa-e741-4814-adff-c34ecde566ee",
      "number": 1,
      "content": "Miền hoang dã và vùng đất khô cằn sẽ vui mừng;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.162Z",
      "updatedAt": "2024-09-23T06:00:34.162Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "a8e3d59c-7a22-4740-b3ce-d49a885379a9",
      "number": 1,
      "content": "Sa mạc sẽ mừng rỡ và trổ hoa như hoa hồng nở rộ.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.173Z",
      "updatedAt": "2024-09-23T06:00:34.173Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "e0728bf6-eca3-417e-9097-ef6cd7994111",
      "number": 2,
      "content": "Nó sẽ trổ hoa đầy dẫy khắp nơi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.175Z",
      "updatedAt": "2024-09-23T06:00:34.175Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "ac8b7f93-3d31-4097-9ea6-00d34b111a2b",
      "number": 2,
      "content": "Nó sẽ hân hoan vui mừng ca hát.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.180Z",
      "updatedAt": "2024-09-23T06:00:34.180Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "9292fe18-e799-4da4-b792-42002c1aeaef",
      "number": 2,
      "content": "Vinh hiển của Li-băng,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.187Z",
      "updatedAt": "2024-09-23T06:00:34.187Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "c8b6aa00-5b96-4b86-8de1-7ca1d909980a",
      "number": 2,
      "content": "Hùng vĩ của Cạt-mên, và rực rỡ của Sa-rôn đều được ban cho nó.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.192Z",
      "updatedAt": "2024-09-23T06:00:34.192Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "9b1d05ac-a4d9-44d7-8061-4c6777bd225e",
      "number": 2,
      "content": "Người ta sẽ thấy vinh hiển của **CHÚA**,",
      "order": 4,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.198Z",
      "updatedAt": "2024-09-23T06:00:34.198Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "995578d5-9853-4560-9b30-bad0db50cbe4",
      "number": 2,
      "content": "Tức sự huy hoàng của Đức Chúa Trời chúng ta.",
      "order": 5,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.202Z",
      "updatedAt": "2024-09-23T06:00:34.202Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "7403cfa4-c905-4b51-8796-85739f6c7632",
      "number": 3,
      "content": "Hãy làm cho những cánh tay yếu đuối được mạnh mẽ,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.207Z",
      "updatedAt": "2024-09-23T06:00:34.207Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "a9dae252-5640-4cf8-bcd9-83f5ad96198f",
      "number": 3,
      "content": "Những đầu gối run rẩy được vững vàng.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.212Z",
      "updatedAt": "2024-09-23T06:00:34.212Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "bf79fe3b-b6bb-402a-9aeb-fb6ca42ee46a",
      "number": 4,
      "content": "Hãy nói với những người có lòng sợ hãi,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.216Z",
      "updatedAt": "2024-09-23T06:00:34.216Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "c76c8ca8-d8d8-4cd0-a4da-e1013ab0b4d1",
      "number": 4,
      "content": "“Hãy mạnh mẽ lên, đừng sợ!",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.221Z",
      "updatedAt": "2024-09-23T06:00:34.221Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "9e558152-5f87-4026-8a70-eda270e135d6",
      "number": 4,
      "content": "Này, Đức Chúa Trời của anh chị em",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.225Z",
      "updatedAt": "2024-09-23T06:00:34.225Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "3a515090-c101-4470-ad9a-470c23248737",
      "number": 4,
      "content": "Sẽ đến để báo thù",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.230Z",
      "updatedAt": "2024-09-23T06:00:34.230Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "987dd538-16c9-41b1-80ff-630ca176b862",
      "number": 4,
      "content": "Bằng sự báo trả của Đức Chúa Trời;",
      "order": 4,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.236Z",
      "updatedAt": "2024-09-23T06:00:34.236Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "fcd14405-c786-478d-9815-154b9f1baafd",
      "number": 4,
      "content": "Ngài sẽ đến và cứu anh chị em.”",
      "order": 5,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.240Z",
      "updatedAt": "2024-09-23T06:00:34.240Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "70c5f4c5-7903-4ed2-83de-9bbab64715f4",
      "number": 5,
      "content": "Bấy giờ mắt người mù sẽ mở ra,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.245Z",
      "updatedAt": "2024-09-23T06:00:34.245Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "52466f7c-7826-4343-8478-95b8e4e10bb8",
      "number": 5,
      "content": "Tai người điếc sẽ nghe rõ,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.250Z",
      "updatedAt": "2024-09-23T06:00:34.250Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "d68d4426-d52c-448a-b850-cca2d40c9e99",
      "number": 6,
      "content": "Bấy giờ người què sẽ nhảy như nai rừng,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.254Z",
      "updatedAt": "2024-09-23T06:00:34.254Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "9c6f93c4-0c71-4d0b-97d1-a47fc6ab6519",
      "number": 6,
      "content": "Lưỡi người câm sẽ ca hát.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.259Z",
      "updatedAt": "2024-09-23T06:00:34.259Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "dd57b07e-f379-42c8-b47b-3e54b9f79678",
      "number": 6,
      "content": "Vì nước đã phun lên trong đồng hoang khô hạn;",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.264Z",
      "updatedAt": "2024-09-23T06:00:34.264Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "04f02080-268d-488a-9952-83b0a0762fd1",
      "number": 6,
      "content": "Sa mạc bây giờ đầy những suối nước trào tuôn.",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.269Z",
      "updatedAt": "2024-09-23T06:00:34.269Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "9254f2b9-ba70-4b83-a1e6-361398e282e4",
      "number": 7,
      "content": "Miền đất khô cằn biến thành ao hồ ngập nước;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.274Z",
      "updatedAt": "2024-09-23T06:00:34.274Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "e1e2c318-fa13-4c58-ac88-ed28b1ab83bb",
      "number": 7,
      "content": "Vùng đất khô khan đầy các suối nước tuôn dòng;",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.278Z",
      "updatedAt": "2024-09-23T06:00:34.278Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "bbeb6783-3762-4ae0-b721-c9283c99077c",
      "number": 7,
      "content": "Nơi ngày trước những chó rừng nằm ngủ,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.283Z",
      "updatedAt": "2024-09-23T06:00:34.283Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "32530279-df95-40be-9672-b61d3e1366bf",
      "number": 7,
      "content": "Giờ cỏ xanh, lau, sậy mọc um tùm.",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.287Z",
      "updatedAt": "2024-09-23T06:00:34.287Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "75c9dfe6-4aad-43fd-9f86-41959c982a6e",
      "number": 8,
      "content": "Rồi ở đó người ta sẽ xây một đại lộ làm con đường di chuyển,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.292Z",
      "updatedAt": "2024-09-23T06:00:34.292Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "0f95d911-1155-438d-b19a-768560230196",
      "number": 8,
      "content": "Và đặt tên là Đường Thánh;",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.297Z",
      "updatedAt": "2024-09-23T06:00:34.297Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "a201dab0-3493-4c55-a7fd-f3b415301251",
      "number": 8,
      "content": "Những kẻ ô uế sẽ không được đi trên đường ấy,",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.302Z",
      "updatedAt": "2024-09-23T06:00:34.302Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "0fa3929a-58c1-41d2-bcf3-f56c2b91f1df",
      "number": 8,
      "content": "Đường ấy chỉ dành cho những người thuộc về Chúa mà thôi.",
      "order": 3,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.307Z",
      "updatedAt": "2024-09-23T06:00:34.307Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "420bd26f-c800-431b-ad90-fd72afc85443",
      "number": 8,
      "content": "Những lữ khách điên rồ sẽ không được lang thang trên đường ấy.",
      "order": 4,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.316Z",
      "updatedAt": "2024-09-23T06:00:34.316Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "a7106ef0-f965-40ec-beb0-17f154509f4d",
      "number": 9,
      "content": "Sư tử sẽ không đón đường ai trên đường ấy;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.321Z",
      "updatedAt": "2024-09-23T06:00:34.321Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "14bf05d0-5b46-4fd3-a528-d9b8f7655717",
      "number": 9,
      "content": "Các thú dữ sẽ không đi trên đường ấy;",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.326Z",
      "updatedAt": "2024-09-23T06:00:34.326Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "749015bd-022d-4b6c-aef5-8e878c76b01d",
      "number": 9,
      "content": "Chẳng ai thấy chúng lảng vảng ở đó;",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.330Z",
      "updatedAt": "2024-09-23T06:00:34.330Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "88ca7dc2-2f5d-4360-9df9-d975bec9a96f",
      "number": 9,
      "content": "Nhưng chỉ những người được cứu chuộc đi trên đường ấy.",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.335Z",
      "updatedAt": "2024-09-23T06:00:34.335Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "80f5076b-988a-4fb5-96cd-70e777dfd134",
      "number": 10,
      "content": "Những người được **CHÚA** cứu chuộc sẽ trở về;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.339Z",
      "updatedAt": "2024-09-23T06:00:34.339Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "145fcaa9-e4b2-48a3-9b66-efdd8e4fbd27",
      "number": 10,
      "content": "Họ sẽ tiến về Si-ôn với tiếng hát vui mừng;",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.344Z",
      "updatedAt": "2024-09-23T06:00:34.344Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "2d8132d9-3649-4d15-ba87-c7a5caa5d67b",
      "number": 10,
      "content": "Niềm vui đời đời sẽ rạng ngời trên gương mặt họ,",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.349Z",
      "updatedAt": "2024-09-23T06:00:34.349Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "4c781fbc-44b1-4961-89fa-ed7c3344b74c",
      "number": 10,
      "content": "Họ sẽ vui mừng và hớn hở,",
      "order": 3,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:00:34.354Z",
      "updatedAt": "2024-09-23T06:00:34.354Z",
      "chapterId": "a00af68d-9ae9-48b0-921e-2651275576f7"
    },
    {
      "id": "b24e81e6-9a6f-4a4a-9d92-29261568876f",
      "number": 10,
      "content": "Sầu thảm và than thở sẽ trốn mất.",
      "order": 4,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
