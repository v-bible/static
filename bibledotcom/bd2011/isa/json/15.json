{
  "chapter": {
    "id": "00419860-d1c8-4fde-a55c-4d9aa8f64772",
    "number": 15,
    "ref": "https://www.bible.com/bible/19/ISA.15.BD2011",
    "createdAt": "2024-09-07T11:51:11.943Z",
    "updatedAt": "2024-09-07T11:51:11.943Z",
    "bookId": "dd94a7a2-46d4-489f-a1b9-a328e1eafa8e"
  },
  "verses": [
    {
      "id": "e0302692-ba2c-4735-8eea-476bb779affc",
      "number": 1,
      "content": "Sứ điệp về Mô-áp:",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T14:43:15.855Z",
      "updatedAt": "2024-09-07T14:43:15.855Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "91cfc40a-6b0a-4f21-b3f4-069485ad0309",
      "number": 1,
      "content": "Ôi, chỉ trong một đêm mà Thành A-rơ của Mô-áp",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.855Z",
      "updatedAt": "2024-09-07T14:43:15.855Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "0d411ac6-5c3f-437f-9b7e-c20276b84762",
      "number": 1,
      "content": "Đã bị tàn phá tan hoang;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.855Z",
      "updatedAt": "2024-09-07T14:43:15.855Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "851ebaba-1407-4ef1-978a-cde8f53a0ba3",
      "number": 1,
      "content": "Ôi, chỉ trong một đêm mà Thành Ki-rơ của Mô-áp",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.855Z",
      "updatedAt": "2024-09-07T14:43:15.855Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "6d467f6f-5781-4b3a-ae68-8e8ac869896f",
      "number": 1,
      "content": "Chỉ còn là đống đổ nát hoang tàn.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.855Z",
      "updatedAt": "2024-09-07T14:43:15.855Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "0561b7aa-791d-4549-ab33-9597abab6f92",
      "number": 2,
      "content": "Dân Đi-bôn kéo nhau lên đền thờ thần của họ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.864Z",
      "updatedAt": "2024-09-07T14:43:15.864Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "d77891b4-e5e6-4411-b6c5-fbbb8ccbfb7e",
      "number": 2,
      "content": "Họ cùng nhau đi lên các miếu thờ thần của họ trên các nơi cao để than khóc.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.864Z",
      "updatedAt": "2024-09-07T14:43:15.864Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "277ed39d-b09c-4fdc-908a-5652bf09234d",
      "number": 2,
      "content": "Tại Nê-bô và tại Mê-đê-ba Mô-áp,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.864Z",
      "updatedAt": "2024-09-07T14:43:15.864Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "f82edd20-b08c-41f1-a7f2-0c42b14d259c",
      "number": 2,
      "content": "Họ rên rỉ thảm sầu;",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.864Z",
      "updatedAt": "2024-09-07T14:43:15.864Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "4e509904-c193-4c7e-849c-476c92196594",
      "number": 2,
      "content": "Ai nấy đều phơi đầu trọc mình ra;",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.864Z",
      "updatedAt": "2024-09-07T14:43:15.864Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "96085d10-9055-4838-9d80-21955c4a1b0c",
      "number": 2,
      "content": "Người nào cũng cạo râu mình nhẵn nhụi.",
      "order": 5,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.864Z",
      "updatedAt": "2024-09-07T14:43:15.864Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "c3d1671b-ca05-409c-8bfc-809dc566bf8b",
      "number": 3,
      "content": "Ngoài đường phố người ta mặc vải thô tang chế;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.867Z",
      "updatedAt": "2024-09-07T14:43:15.867Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "f90c4291-5272-4a02-9d5e-8c354bea409c",
      "number": 3,
      "content": "Trên các sân thượng và tại những nơi hội họp công cộng ai nấy đều khóc lóc bi thương;",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.867Z",
      "updatedAt": "2024-09-07T14:43:15.867Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "3a3001bd-bfed-4ce5-bbef-a0462e89912d",
      "number": 3,
      "content": "Lòng dạ nát tan, dầm dề nước mắt.",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.867Z",
      "updatedAt": "2024-09-07T14:43:15.867Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "f8a0a39e-32c0-40ac-9761-67f04c812c39",
      "number": 4,
      "content": "Dân ở Hếch-bôn và ở Ê-lê-a-lê khóc gào thảm thiết;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.870Z",
      "updatedAt": "2024-09-07T14:43:15.870Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "99d61aa7-3222-4ff9-8ff6-878877f17d27",
      "number": 4,
      "content": "Tiếng than khóc của họ vang vọng đến tận Gia-ha;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.870Z",
      "updatedAt": "2024-09-07T14:43:15.870Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "6e795c90-884b-4aab-b1ed-3e7959b402cd",
      "number": 4,
      "content": "Thế nên quân đội của Mô-áp đều nản lòng,",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.870Z",
      "updatedAt": "2024-09-07T14:43:15.870Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "f405a243-73e4-4703-993f-4d17a5a53f52",
      "number": 4,
      "content": "Họ kinh hồn táng đởm và run lên lẩy bẩy.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.870Z",
      "updatedAt": "2024-09-07T14:43:15.870Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "b8d806fe-daa4-4592-8bcd-6721b953f265",
      "number": 5,
      "content": "Lòng Ta đau xót vì dân Mô-áp;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.872Z",
      "updatedAt": "2024-09-07T14:43:15.872Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "6260454d-62e9-4ba4-81e7-1e997bc9dfa2",
      "number": 5,
      "content": "Những người tỵ nạn của họ chạy trốn đến tận Xô-a",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.872Z",
      "updatedAt": "2024-09-07T14:43:15.872Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "f9cd43d5-5d8c-4b41-bcf8-058408369924",
      "number": 5,
      "content": "Và tận Ê-lát Sê-li-si-a.",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.872Z",
      "updatedAt": "2024-09-07T14:43:15.872Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "d5299c5e-0992-47f5-bd88-2b9842654495",
      "number": 5,
      "content": "Kìa, họ trèo qua đèo Lu-hít,",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.872Z",
      "updatedAt": "2024-09-07T14:43:15.872Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "d27c3521-1e38-4b41-8b33-d7509f80f2af",
      "number": 5,
      "content": "Vừa đi vừa khóc thút thít;",
      "order": 4,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.872Z",
      "updatedAt": "2024-09-07T14:43:15.872Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "5a618d55-70f6-4237-a5d7-bcded1ebeb65",
      "number": 5,
      "content": "Kìa, trên đường đến Hô-rô-na-im,",
      "order": 5,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.872Z",
      "updatedAt": "2024-09-07T14:43:15.872Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "4da6c209-d91d-4733-aeb0-d70f432e5260",
      "number": 5,
      "content": "Họ vừa đi vừa khóc sụt sùi vì bị diệt vong.",
      "order": 6,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.872Z",
      "updatedAt": "2024-09-07T14:43:15.872Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "6e519ebe-3093-48a3-a637-0f2cb1b53ea6",
      "number": 6,
      "content": "Kìa, các nguồn nước ở Nim-rim",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.874Z",
      "updatedAt": "2024-09-07T14:43:15.874Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "b9a033e5-d0f9-492e-8b53-c75690878052",
      "number": 6,
      "content": "Đã cạn khô;",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.874Z",
      "updatedAt": "2024-09-07T14:43:15.874Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "e402d3df-3e38-46f5-ad9f-06c09742fd3c",
      "number": 6,
      "content": "Kìa, các đồng cỏ úa vàng, cây cối héo tàn,",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.874Z",
      "updatedAt": "2024-09-07T14:43:15.874Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "e3fc7c0e-8a64-4fec-887f-b37921618eba",
      "number": 6,
      "content": "Thực vật chẳng còn chi nữa.",
      "order": 3,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.874Z",
      "updatedAt": "2024-09-07T14:43:15.874Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "d46fdd4b-173b-4470-8ff5-9efdb17e15f1",
      "number": 7,
      "content": "Vì thế của cải họ dành dụm được",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.877Z",
      "updatedAt": "2024-09-07T14:43:15.877Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "ec2f0a1c-e164-4e00-b3fc-98edf8d6e6f7",
      "number": 7,
      "content": "Và những gì họ tích lũy được,",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.877Z",
      "updatedAt": "2024-09-07T14:43:15.877Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "6b4bca69-ecb5-4cea-b184-22320226f335",
      "number": 7,
      "content": "Họ đều mang",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.877Z",
      "updatedAt": "2024-09-07T14:43:15.877Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "2f855c54-8068-4701-8831-6df859c9ef36",
      "number": 7,
      "content": "Qua bên kia Thác Thùy Dương.",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.877Z",
      "updatedAt": "2024-09-07T14:43:15.877Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "0612575b-8dda-4099-a807-7b7984054ec5",
      "number": 8,
      "content": "Kìa, tiếng khóc than",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.879Z",
      "updatedAt": "2024-09-07T14:43:15.879Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "5ab8fb2f-323c-4512-8443-208d1d9653f8",
      "number": 8,
      "content": "Vang khắp biên thùy Mô-áp;",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.879Z",
      "updatedAt": "2024-09-07T14:43:15.879Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "116d5376-2c32-4237-bbec-ec85a5a2218a",
      "number": 8,
      "content": "Tiếng thổn thức kêu gào vào tận Ê-la-im;",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.879Z",
      "updatedAt": "2024-09-07T14:43:15.879Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "06fe5c27-8c6f-454d-be37-9a95a168bf6b",
      "number": 8,
      "content": "Và tiếng rên rỉ nỉ non vọng thấu Bê-e Ê-lim.",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.879Z",
      "updatedAt": "2024-09-07T14:43:15.879Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "90ef1868-04d5-4b07-ac57-5bd5b2485881",
      "number": 9,
      "content": "Kìa, những dòng nước ở Đi-bôn đều đầy máu người;",
      "order": 0,
      "parNumber": 38,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.882Z",
      "updatedAt": "2024-09-07T14:43:15.882Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "167e56f2-8bbd-4ca3-9534-0f1171904f88",
      "number": 9,
      "content": "Dầu vậy Ta sẽ gia thêm tai họa cho Đi-bôn;",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:43:15.882Z",
      "updatedAt": "2024-09-07T14:43:15.882Z",
      "chapterId": "00419860-d1c8-4fde-a55c-4d9aa8f64772"
    },
    {
      "id": "1a5dddf6-d28f-41f5-8545-5fa7eab1c4f2",
      "number": 9,
      "content": "Đó là sư tử sẽ đuổi theo những người Mô-áp vừa thoát nạn",
      "order": 2,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
