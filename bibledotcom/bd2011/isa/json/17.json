{
  "chapter": {
    "id": "17bae6d7-9bba-40c1-a965-0128db0beb64",
    "number": 17,
    "ref": "https://www.bible.com/bible/19/ISA.17.BD2011",
    "createdAt": "2024-09-07T11:51:11.949Z",
    "updatedAt": "2024-09-07T11:51:11.949Z",
    "bookId": "dd94a7a2-46d4-489f-a1b9-a328e1eafa8e"
  },
  "verses": [
    {
      "id": "832b6ae1-0a05-4c14-84b4-df0a800772e6",
      "number": 1,
      "content": "Sứ điệp về Đa-mách:",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T14:44:02.223Z",
      "updatedAt": "2024-09-07T14:44:02.223Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "48706dfb-667c-45b7-9fd9-f42933bfe27b",
      "number": 1,
      "content": "Kìa, Đa-mách sẽ không còn là một thành phố nữa,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.223Z",
      "updatedAt": "2024-09-07T14:44:02.223Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "61bc4aa0-8e76-4bbf-81d5-cd80ad09caf4",
      "number": 1,
      "content": "Nhưng chỉ là một đống đổ nát hoang tàn.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.223Z",
      "updatedAt": "2024-09-07T14:44:02.223Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "8cb8d312-d0e1-4c95-90d3-1a45a20f1897",
      "number": 2,
      "content": "Các khu phố của nó sẽ trở nên hoang vắng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.233Z",
      "updatedAt": "2024-09-07T14:44:02.233Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "9b7616f2-288a-4d5d-a314-fee65f365124",
      "number": 2,
      "content": "Chỉ còn làm nơi cho các đàn chiên nằm nghỉ;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.233Z",
      "updatedAt": "2024-09-07T14:44:02.233Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "54a5a5dc-8217-4745-8641-663be18e0abb",
      "number": 2,
      "content": "Chẳng ai sẽ làm cho chúng kinh động.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.233Z",
      "updatedAt": "2024-09-07T14:44:02.233Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "ff5afd1f-6efb-4912-a9bc-b26c6cfa707a",
      "number": 3,
      "content": "Đồn lũy của nó đóng ở Ép-ra-im sẽ không còn nữa;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.236Z",
      "updatedAt": "2024-09-07T14:44:02.236Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "eaf49a4a-8f65-43bf-83bd-7740d5283b4e",
      "number": 3,
      "content": "Vương quyền của Đa-mách đã bị cất bỏ rồi;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.236Z",
      "updatedAt": "2024-09-07T14:44:02.236Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "3fc9e841-34a7-47ac-b19a-61193b118408",
      "number": 3,
      "content": "Những người A-ram còn sống sót",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.236Z",
      "updatedAt": "2024-09-07T14:44:02.236Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "0d8f8f3f-2472-40ef-aca1-134a53f7efda",
      "number": 3,
      "content": "Sẽ giống như vinh hiển của con cái I-sơ-ra-ên,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.236Z",
      "updatedAt": "2024-09-07T14:44:02.236Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "b3ddbf46-94c3-4e54-bc28-6b6e511e6754",
      "number": 3,
      "content": "CHÚA các đạo quân phán vậy.",
      "order": 4,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.236Z",
      "updatedAt": "2024-09-07T14:44:02.236Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "137f958f-e2dd-4c53-a90a-a372e8251b83",
      "number": 4,
      "content": "Trong ngày ấy, vinh hiển của Gia-cốp sẽ suy giảm;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.239Z",
      "updatedAt": "2024-09-07T14:44:02.239Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "842e6754-3450-4e22-b004-c2c5b667b8ae",
      "number": 4,
      "content": "Thân thể mập mạnh của nó sẽ trở nên gầy yếu.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.239Z",
      "updatedAt": "2024-09-07T14:44:02.239Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "0399b861-951a-483b-b38e-10a0eb6a8a0c",
      "number": 5,
      "content": "Nó sẽ như người thợ gặt nắm lấy những ngọn lúa,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.241Z",
      "updatedAt": "2024-09-07T14:44:02.241Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "e21c5992-afd4-42a9-9c37-6b07c2c6b490",
      "number": 5,
      "content": "Rồi đưa tay ra cắt",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.241Z",
      "updatedAt": "2024-09-07T14:44:02.241Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "54c7973b-543d-4050-9d24-a5c05fe16b79",
      "number": 5,
      "content": "Và chỉ có được một ít gié lúa thôi;",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.241Z",
      "updatedAt": "2024-09-07T14:44:02.241Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "6fbc8fc1-723f-4424-8525-35a05476efde",
      "number": 5,
      "content": "Giống như người ta đi mót lúa trong thung lũng Rê-pha-im.",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.241Z",
      "updatedAt": "2024-09-07T14:44:02.241Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "cff3146f-c5a7-44a6-a3b4-32552939982d",
      "number": 6,
      "content": "Thật vậy chỉ một ít gié lúa còn sót lại;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.243Z",
      "updatedAt": "2024-09-07T14:44:02.243Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "ca4b9ca0-7724-404c-8b0a-52d7858785a8",
      "number": 6,
      "content": "Giống như cây ô-liu kia bị người ta rung,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.243Z",
      "updatedAt": "2024-09-07T14:44:02.243Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "9d0b03f9-735b-4691-b470-76898a3033f1",
      "number": 6,
      "content": "Chỉ còn hai ba trái",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.243Z",
      "updatedAt": "2024-09-07T14:44:02.243Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "65f37ed0-b48b-4565-b0a0-bad9f6e3e4f7",
      "number": 6,
      "content": "Sót lại trên ngọn cây,",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.243Z",
      "updatedAt": "2024-09-07T14:44:02.243Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "bb58e112-3c60-45ae-81b7-1e6335fbd56e",
      "number": 6,
      "content": "Hoặc bốn năm trái",
      "order": 4,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.243Z",
      "updatedAt": "2024-09-07T14:44:02.243Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "9b3ab7d1-e95d-4243-95e1-fb4b6c3cee01",
      "number": 6,
      "content": "Còn dính lại trên các cành ra trái.",
      "order": 5,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.243Z",
      "updatedAt": "2024-09-07T14:44:02.243Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "3a047cd7-d00f-4c3e-a8b7-68dd40749eb9",
      "number": 6,
      "content": "CHÚA Đức Chúa Trời của I-sơ-ra-ên phán vậy.",
      "order": 6,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.243Z",
      "updatedAt": "2024-09-07T14:44:02.243Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "241a59c8-eaac-430f-bcaf-1052b133721b",
      "number": 7,
      "content": "Trong ngày ấy người ta sẽ để lòng hướng về Đấng Tạo Hóa mình; mắt họ sẽ ngưỡng vọng Đấng Thánh của I-sơ-ra-ên.",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T14:44:02.246Z",
      "updatedAt": "2024-09-07T14:44:02.246Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "1d8d5554-99fd-4b06-90d8-361546a6ad99",
      "number": 8,
      "content": "Họ sẽ không cúng tế nơi bàn thờ các thần tượng, tức nơi các tác phẩm do tay họ làm ra; họ sẽ chẳng trông mong gì nơi các sản phẩm mà ngón tay họ đã tạo, bất kể là trụ thờ Nữ Thần A-sê-ra, hay các bàn thờ để tế hương cho các thần tượng.",
      "order": 0,
      "parNumber": 25,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-07T14:44:02.248Z",
      "updatedAt": "2024-09-07T14:44:02.248Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "7127b4e3-77fa-4a24-8080-ae584511bd25",
      "number": 9,
      "content": "Trong ngày ấy các thành kiên cố của họ sẽ giống như những nơi hoang vắng của dân Hi-vi và dân A-mô-ri thời xưa; các dân ấy đã bỏ xứ ra đi vì cớ dân I-sơ-ra-ên; thật là cảnh hoang tàn đổ nát.",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T14:44:02.250Z",
      "updatedAt": "2024-09-07T14:44:02.250Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "314b12b5-ad2d-4865-a7ad-afb68a7826fe",
      "number": 10,
      "content": "Vì ngươi đã quên Đức Chúa Trời, Đấng Giải Cứu của ngươi,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.252Z",
      "updatedAt": "2024-09-07T14:44:02.252Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "e467f64a-d426-4dc7-a8e3-a8e6ccbe115e",
      "number": 10,
      "content": "Vì ngươi không nhớ đến Vầng Đá vững chãi làm nơi ẩn náu của ngươi,",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.252Z",
      "updatedAt": "2024-09-07T14:44:02.252Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "b9e97e93-207d-4235-a245-4b9c7b1aef90",
      "number": 10,
      "content": "Cho nên, dù ngươi trồng những cây giống tốt",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.252Z",
      "updatedAt": "2024-09-07T14:44:02.252Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "5c9376fc-61d1-4f6b-ac69-cbb6db219381",
      "number": 10,
      "content": "Và ương những cây lấy giống từ ngoại quốc mang về;",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.252Z",
      "updatedAt": "2024-09-07T14:44:02.252Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "a60068f5-633c-478e-90bd-b5e0ad7f7f68",
      "number": 11,
      "content": "Dù trong ngày ngươi trồng, ngươi mong sao cho chúng chóng lớn;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.255Z",
      "updatedAt": "2024-09-07T14:44:02.255Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "2fb3799f-087a-457d-99ce-5a13679a7911",
      "number": 11,
      "content": "Ngay buổi sáng ngươi trồng, ngươi đã muốn thấy chúng sớm trổ hoa,",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.255Z",
      "updatedAt": "2024-09-07T14:44:02.255Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "88531134-f1a2-42d8-bc1e-08745f8a009a",
      "number": 11,
      "content": "Nhưng đến ngày gặt hái, ngươi sẽ chẳng thu hoạch được gì,",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.255Z",
      "updatedAt": "2024-09-07T14:44:02.255Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "e373fc87-f0dc-49ac-b70a-a662dd01af4b",
      "number": 11,
      "content": "Mà chỉ thấy toàn thảm sầu và nỗi đau tuyệt vọng.",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.255Z",
      "updatedAt": "2024-09-07T14:44:02.255Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "a70714d0-6a4a-4ba8-8436-18bdabcddeff",
      "number": 12,
      "content": "Khốn cho muôn dân đang thét gào;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.257Z",
      "updatedAt": "2024-09-07T14:44:02.257Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "a6d5e177-0f5f-4fba-9ec5-f6b6a16e8171",
      "number": 12,
      "content": "Họ thét to tựa biển cả gầm vang;",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.257Z",
      "updatedAt": "2024-09-07T14:44:02.257Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "29d10f3d-8e4d-49bb-8fd4-f8e4380b4e9c",
      "number": 12,
      "content": "Khốn cho các nước đang gào la;",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.257Z",
      "updatedAt": "2024-09-07T14:44:02.257Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "af3b3d07-8fe3-479f-9943-32d7da539b5a",
      "number": 12,
      "content": "Họ gào to tựa sóng cuồng ập xuống.",
      "order": 3,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.257Z",
      "updatedAt": "2024-09-07T14:44:02.257Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "db793617-91b8-4abb-af36-42361b36734e",
      "number": 13,
      "content": "Nhiều nước gào la như sóng cao cuồn cuộn đập vào bờ;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.259Z",
      "updatedAt": "2024-09-07T14:44:02.259Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "fa7da9a0-8e87-4255-97ab-b7537eaa57db",
      "number": 13,
      "content": "Nhưng khi CHÚA quở trách, chúng liền cao bay xa chạy;",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.259Z",
      "updatedAt": "2024-09-07T14:44:02.259Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "cd94aa1d-f558-4354-b119-e0364b880100",
      "number": 13,
      "content": "Chúng bị săn đuổi như rác rến trên núi gặp cơn lốc,",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.259Z",
      "updatedAt": "2024-09-07T14:44:02.259Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "81a0dac7-f8cf-4042-a4ba-7dc19f26d69c",
      "number": 13,
      "content": "Như bụi bay khi gặp trận cuồng phong.",
      "order": 3,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.259Z",
      "updatedAt": "2024-09-07T14:44:02.259Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "653e0aee-dcd9-4f0f-8bf3-fd8ed81faf23",
      "number": 14,
      "content": "Chiều tà đến, ôi thật là kinh hoàng!",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.262Z",
      "updatedAt": "2024-09-07T14:44:02.262Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "64934adf-3def-4a9c-a8e7-6c98c4637175",
      "number": 14,
      "content": "Trước khi trời sáng, chúng chẳng còn nữa.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.262Z",
      "updatedAt": "2024-09-07T14:44:02.262Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "612c922b-91b5-4c11-a97e-28c57edb1b9b",
      "number": 14,
      "content": "Đây là số phận của những kẻ bóc lột chúng ta,",
      "order": 2,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.262Z",
      "updatedAt": "2024-09-07T14:44:02.262Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    },
    {
      "id": "74ff2455-6fc3-4fd1-9763-faf3dd50ff42",
      "number": 14,
      "content": "Là định mạng dành cho quân cướp đoạt chúng ta.",
      "order": 3,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:44:02.262Z",
      "updatedAt": "2024-09-07T14:44:02.262Z",
      "chapterId": "17bae6d7-9bba-40c1-a965-0128db0beb64"
    }
  ],
