{
  "chapter": {
    "id": "2bc223d9-5b3e-4674-8377-fe7b0055f690",
    "number": 65,
    "ref": "https://www.bible.com/bible/19/ISA.65.BD2011",
    "createdAt": "2024-09-07T11:51:12.096Z",
    "updatedAt": "2024-09-07T11:51:12.096Z",
    "bookId": "dd94a7a2-46d4-489f-a1b9-a328e1eafa8e"
  },
  "verses": [
    {
      "id": "b257c3b3-ba99-4b84-8cf2-3325fae60ea1",
      "number": 1,
      "content": "“Ta đã cho những kẻ chẳng thèm hỏi đến Ta được cầu xin Ta,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.436Z",
      "updatedAt": "2024-09-07T14:54:06.436Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "25ef7ca3-b814-4426-a294-9f09a8cb0f50",
      "number": 1,
      "content": "Ta đã cho những kẻ chẳng tìm kiếm Ta được tìm Ta.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.436Z",
      "updatedAt": "2024-09-07T14:54:06.436Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "9cd1a821-e7ab-432e-ac52-620b1d86cf01",
      "number": 1,
      "content": "Ta nói với một dân chẳng thèm kêu cầu danh Ta rằng,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.436Z",
      "updatedAt": "2024-09-07T14:54:06.436Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "4fab9179-4638-4a53-b19f-9252d2889c29",
      "number": 1,
      "content": "‘Ta ở đây! Ta ở đây!’",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.436Z",
      "updatedAt": "2024-09-07T14:54:06.436Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "dba99b4e-f1ae-4629-93f9-e250a88ff682",
      "number": 2,
      "content": "Suốt ngày Ta dang hai tay",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.445Z",
      "updatedAt": "2024-09-07T14:54:06.445Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "0b0915f6-82fd-448d-be84-8ef8c9a14ee9",
      "number": 2,
      "content": "Mời gọi một dân phản nghịch,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.445Z",
      "updatedAt": "2024-09-07T14:54:06.445Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "6b1ba105-9185-4885-a918-fdbba74835d6",
      "number": 2,
      "content": "Một dân cứ bước theo đường lối gian tà,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.445Z",
      "updatedAt": "2024-09-07T14:54:06.445Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "b1444716-8af3-4ca3-8c82-202e8af80344",
      "number": 2,
      "content": "Theo ý riêng mình,",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.445Z",
      "updatedAt": "2024-09-07T14:54:06.445Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "4ec5f985-53e2-40fb-aba3-587b1641c7ee",
      "number": 3,
      "content": "Một dân không ngừng chọc giận",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.448Z",
      "updatedAt": "2024-09-07T14:54:06.448Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "9eac4705-b596-48e3-8c23-c4e79901e24a",
      "number": 3,
      "content": "Ngay vào mặt Ta,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.448Z",
      "updatedAt": "2024-09-07T14:54:06.448Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "8b78cb7a-1cdd-4fd4-9d16-44f0204b68a6",
      "number": 3,
      "content": "Chúng dâng con vật hiến tế cho tà thần trong các vườn,",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.448Z",
      "updatedAt": "2024-09-07T14:54:06.448Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "d61f13d3-bd7b-4b2f-9ae2-fba8784711ea",
      "number": 3,
      "content": "Và dâng hương cho các thần tượng trên những bàn thờ bằng gạch;",
      "order": 3,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.448Z",
      "updatedAt": "2024-09-07T14:54:06.448Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "2a331269-cbd6-491c-82ce-d622fd8b9bf5",
      "number": 4,
      "content": "Chúng ngồi trầm tư mặc tưởng giữa các mồ mả,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.450Z",
      "updatedAt": "2024-09-07T14:54:06.450Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "eff24217-a843-42fc-935e-53b3d5c747ae",
      "number": 4,
      "content": "Và thức thâu đêm tĩnh tọa nơi mộ địa âm u.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.450Z",
      "updatedAt": "2024-09-07T14:54:06.450Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "4bd4ce5d-5163-4a4d-8814-4f56a5f7176a",
      "number": 4,
      "content": "Chúng ăn thịt heo ô uế;",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.450Z",
      "updatedAt": "2024-09-07T14:54:06.450Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "bead79ea-6875-46a0-99d0-1ecd62e5c60c",
      "number": 4,
      "content": "Bát đĩa chúng chứa đầy các thức ăn gớm ghiếc.",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.450Z",
      "updatedAt": "2024-09-07T14:54:06.450Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "4084d607-13d1-425d-a986-617457aced3c",
      "number": 5,
      "content": "Chúng là những kẻ đã nói,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.453Z",
      "updatedAt": "2024-09-07T14:54:06.453Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "456e6f16-8eed-4046-8875-e7230bc2f054",
      "number": 5,
      "content": "‘Hãy đứng xa ra, chớ lại gần ta, vì ta thánh khiết hơn ngươi!’",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.453Z",
      "updatedAt": "2024-09-07T14:54:06.453Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "5ad8a3c2-5827-46b2-8307-ec059e96e32d",
      "number": 5,
      "content": "Những điều ấy làm cho khói giận của Ta bốc ra nơi lỗ mũi,",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.453Z",
      "updatedAt": "2024-09-07T14:54:06.453Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "4206d71a-94d5-4e3d-b795-43003f7b749f",
      "number": 5,
      "content": "Như lửa hừng cháy hừng hực suốt ngày.",
      "order": 3,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.453Z",
      "updatedAt": "2024-09-07T14:54:06.453Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "c11323c8-c3a1-4011-94fd-9f91879a1ebf",
      "number": 6,
      "content": "Này, những việc ấy đã được ghi lại trước mặt Ta;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.455Z",
      "updatedAt": "2024-09-07T14:54:06.455Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "038acd63-a934-485a-a60c-1d863b2489d5",
      "number": 6,
      "content": "Ta sẽ không im lặng, nhưng sẽ báo trả,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.455Z",
      "updatedAt": "2024-09-07T14:54:06.455Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "3d3465d2-5007-40fc-aaed-174b588565f1",
      "number": 6,
      "content": "Ta sẽ báo trả ngay vào lòng chúng;",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.455Z",
      "updatedAt": "2024-09-07T14:54:06.455Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "a1c6b8b1-80e8-4bbb-add0-a3549af03f9f",
      "number": 7,
      "content": "Ta sẽ báo trả tội của các ngươi và tội của tổ tiên các ngươi nữa,”",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.458Z",
      "updatedAt": "2024-09-07T14:54:06.458Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "b0b80ee6-c39c-42f6-9a6d-01354ad27d3c",
      "number": 7,
      "content": "CHÚA phán.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.458Z",
      "updatedAt": "2024-09-07T14:54:06.458Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "5b46849f-9517-4d87-8682-ffb4a06989d7",
      "number": 7,
      "content": "“Chúng đã dâng hương cho các tà thần trên các núi;",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.458Z",
      "updatedAt": "2024-09-07T14:54:06.458Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "d57a295c-5d5a-491a-a79c-ee720565990b",
      "number": 7,
      "content": "Chúng đã xúc phạm đến Ta trên các ngọn đồi,",
      "order": 3,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.458Z",
      "updatedAt": "2024-09-07T14:54:06.458Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "34b3c49f-dd75-4c3e-82a3-e73f1908f29a",
      "number": 7,
      "content": "Nên Ta sẽ lường theo những việc chúng làm khi trước,",
      "order": 4,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.458Z",
      "updatedAt": "2024-09-07T14:54:06.458Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "23c6cb41-22ab-4edd-a0b6-d94eb8d774ea",
      "number": 7,
      "content": "Mà báo trả đích đáng vào lòng chúng.”",
      "order": 5,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.458Z",
      "updatedAt": "2024-09-07T14:54:06.458Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "c597e73a-f3f7-404a-8e50-d13855540032",
      "number": 8,
      "content": "CHÚA phán thế này:",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.460Z",
      "updatedAt": "2024-09-07T14:54:06.460Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "b2d93d75-2b85-4a96-b333-248d8722632f",
      "number": 8,
      "content": "“Như rượu mới còn tìm thấy trong chùm nho héo,",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.460Z",
      "updatedAt": "2024-09-07T14:54:06.460Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "cdd92ec4-0f8d-4bee-ba80-5c0bad4dd7cd",
      "number": 8,
      "content": "Người ta bảo rằng, ‘Đừng bỏ nó đi,",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.460Z",
      "updatedAt": "2024-09-07T14:54:06.460Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "35a68969-921f-47ab-bbd7-61a6c4853de9",
      "number": 8,
      "content": "Vì trong đó còn tìm thấy chút phước lành,’",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.460Z",
      "updatedAt": "2024-09-07T14:54:06.460Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "0dd264e2-19cf-42f7-9f1c-d5b0d8641498",
      "number": 8,
      "content": "Thì vì cớ các tôi tớ Ta, Ta sẽ làm như vậy;",
      "order": 4,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.460Z",
      "updatedAt": "2024-09-07T14:54:06.460Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "68b87999-b0e4-4c7b-a105-9b2a5ce62033",
      "number": 8,
      "content": "Ta sẽ không tiêu diệt hết chúng đâu.",
      "order": 5,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.460Z",
      "updatedAt": "2024-09-07T14:54:06.460Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "404e0146-ed69-42c3-a115-88634a9ef13a",
      "number": 9,
      "content": "Từ Gia-cốp Ta sẽ cho phát sinh một dòng dõi;",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.463Z",
      "updatedAt": "2024-09-07T14:54:06.463Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "16f6c256-26a5-47cd-99ab-64a87c5072ec",
      "number": 9,
      "content": "Từ Giu-đa sẽ ra một người thừa kế các núi thánh của Ta;",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.463Z",
      "updatedAt": "2024-09-07T14:54:06.463Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "a80fb4f2-0239-4c3f-bb62-def7a65804d9",
      "number": 9,
      "content": "Người Ta chọn sẽ thừa hưởng cơ nghiệp đó;",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.463Z",
      "updatedAt": "2024-09-07T14:54:06.463Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "ee312e25-3350-4b44-a4f3-90b85f265c86",
      "number": 9,
      "content": "Các tôi tớ Ta sẽ cư ngụ ở đó.",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.463Z",
      "updatedAt": "2024-09-07T14:54:06.463Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "dc11241e-777b-4273-be06-c3c5aac87aef",
      "number": 10,
      "content": "Sa-rôn sẽ là đồng cỏ cho các bầy súc vật;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.465Z",
      "updatedAt": "2024-09-07T14:54:06.465Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "66e917b8-449d-4652-ab44-d80e8aef8254",
      "number": 10,
      "content": "Thung lũng A-cơ sẽ là chỗ cho các đàn bò nằm,",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.465Z",
      "updatedAt": "2024-09-07T14:54:06.465Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "e59ba28e-f0c0-4089-846a-1f788c119314",
      "number": 10,
      "content": "Vì dân Ta đã tìm kiếm Ta.",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.465Z",
      "updatedAt": "2024-09-07T14:54:06.465Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "3bf3fd59-a50c-4e63-84dc-b6c9cc818207",
      "number": 11,
      "content": "Nhưng hỡi các ngươi là những kẻ bỏ CHÚA,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.467Z",
      "updatedAt": "2024-09-07T14:54:06.467Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "c2b22ff0-ea7c-45c0-b4f2-afc81388554e",
      "number": 11,
      "content": "Những kẻ quên núi thánh của Ta,",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.467Z",
      "updatedAt": "2024-09-07T14:54:06.467Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "66efeaa1-606a-4cf8-be31-0be605bd9120",
      "number": 11,
      "content": "Những kẻ lập bàn thờ cho Thần May Mắn,",
      "order": 2,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.467Z",
      "updatedAt": "2024-09-07T14:54:06.467Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "e3701652-3fa6-4da5-8e74-5768bf353eb7",
      "number": 11,
      "content": "Dâng rượu pha cho Thần Định Mệnh,",
      "order": 3,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.467Z",
      "updatedAt": "2024-09-07T14:54:06.467Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "f9de8433-b361-4ad2-b66a-c109ec6c2e58",
      "number": 12,
      "content": "Vì thế Ta sẽ định phần số các ngươi cho gươm giáo;",
      "order": 0,
      "parNumber": 47,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.470Z",
      "updatedAt": "2024-09-07T14:54:06.470Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "b431306b-f841-4803-8e38-5a90fc169fe5",
      "number": 12,
      "content": "Tất cả các ngươi sẽ quỳ xuống trước kẻ giết mình,",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.470Z",
      "updatedAt": "2024-09-07T14:54:06.470Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "fdab57b3-927d-4b49-a297-f724a9ec492f",
      "number": 12,
      "content": "Vì khi Ta gọi, các ngươi chẳng thèm đáp lại;",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.470Z",
      "updatedAt": "2024-09-07T14:54:06.470Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "04f9c25b-0e31-4c02-8b02-a477accbc494",
      "number": 12,
      "content": "Khi Ta phán, các ngươi chẳng muốn lắng nghe;",
      "order": 3,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.470Z",
      "updatedAt": "2024-09-07T14:54:06.470Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "e5d44121-b2f8-4d3e-a389-a7b57fc0496b",
      "number": 12,
      "content": "Nhưng các ngươi cứ làm điều ác trước mắt Ta,",
      "order": 4,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.470Z",
      "updatedAt": "2024-09-07T14:54:06.470Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "e4b2a8d4-7a1c-47b0-9d71-ad86d63aa3b3",
      "number": 12,
      "content": "Và cứ chọn làm những điều Ta không thích.”",
      "order": 5,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.470Z",
      "updatedAt": "2024-09-07T14:54:06.470Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "b9bc5025-0d5a-4a0d-9d82-7507e7bd1a9e",
      "number": 13,
      "content": "Vì thế CHÚA Hằng Hữu phán thế này:",
      "order": 0,
      "parNumber": 53,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.472Z",
      "updatedAt": "2024-09-07T14:54:06.472Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "b239e0c5-b9b9-4f85-b26f-1ffc74e0dccd",
      "number": 13,
      "content": "“Này, các tôi tớ Ta sẽ ăn,",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.472Z",
      "updatedAt": "2024-09-07T14:54:06.472Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "a4a77fef-3e67-4fd4-a3e7-e784b9ef35d6",
      "number": 13,
      "content": "Nhưng các ngươi sẽ đói;",
      "order": 2,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.472Z",
      "updatedAt": "2024-09-07T14:54:06.472Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "32c69aa3-f0ef-4161-884c-539cc5594816",
      "number": 13,
      "content": "Này, các tôi tớ Ta sẽ uống,",
      "order": 3,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.472Z",
      "updatedAt": "2024-09-07T14:54:06.472Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "a87bba92-d54a-42ac-b9ae-11b9a5331b99",
      "number": 13,
      "content": "Nhưng các ngươi sẽ khát;",
      "order": 4,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.472Z",
      "updatedAt": "2024-09-07T14:54:06.472Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "5acf9fa8-ea06-42d6-b715-af86e7214bbb",
      "number": 13,
      "content": "Này, các tôi tớ Ta sẽ vui mừng,",
      "order": 5,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.472Z",
      "updatedAt": "2024-09-07T14:54:06.472Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "43cf11c6-1219-4275-b881-e5e16dd2e350",
      "number": 13,
      "content": "Nhưng các ngươi sẽ tủi hổ;",
      "order": 6,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.472Z",
      "updatedAt": "2024-09-07T14:54:06.472Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "99774a63-6d86-460b-91a3-13132f9bb628",
      "number": 14,
      "content": "Này, các tôi tớ Ta sẽ ca hát vì tâm hồn vui vẻ,",
      "order": 0,
      "parNumber": 60,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.475Z",
      "updatedAt": "2024-09-07T14:54:06.475Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "ca4af200-46cb-4da8-921a-eac7e1f95125",
      "number": 14,
      "content": "Nhưng các ngươi sẽ khóc than vì sầu thảm trong lòng,",
      "order": 1,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.475Z",
      "updatedAt": "2024-09-07T14:54:06.475Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "2bc4f264-4a67-47e3-901b-25f830d742aa",
      "number": 14,
      "content": "Và các ngươi sẽ khóc lóc vì tâm linh đau đớn.",
      "order": 2,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.475Z",
      "updatedAt": "2024-09-07T14:54:06.475Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "c1eb2beb-e5e4-45a7-ab84-372232e78365",
      "number": 15,
      "content": "Các ngươi cũng sẽ được lưu danh, nhưng chỉ để làm lời nguyền rủa cho những kẻ Ta chọn,",
      "order": 0,
      "parNumber": 63,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.477Z",
      "updatedAt": "2024-09-07T14:54:06.477Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "0016f7a5-b3ca-4b58-90f2-7e643be6903e",
      "number": 15,
      "content": "Vì CHÚA Hằng Hữu sẽ tiêu diệt ngươi,",
      "order": 1,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.477Z",
      "updatedAt": "2024-09-07T14:54:06.477Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "f0d7f5e9-2cb2-400a-801e-0437a0ddc323",
      "number": 15,
      "content": "Nhưng các tôi tớ Ngài, Ngài sẽ gọi họ bằng một danh khác.",
      "order": 2,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.477Z",
      "updatedAt": "2024-09-07T14:54:06.477Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "c2402bfb-59bf-4e6f-aea7-137cc0677cab",
      "number": 16,
      "content": "Vì vậy, ở trong xứ, ai cầu phúc cho mình",
      "order": 0,
      "parNumber": 66,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.479Z",
      "updatedAt": "2024-09-07T14:54:06.479Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "0b76e456-5835-4dfb-ae89-3dbf879c532e",
      "number": 16,
      "content": "Sẽ nhân danh Đức Chúa Trời của sự thật mà cầu phúc;",
      "order": 1,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.479Z",
      "updatedAt": "2024-09-07T14:54:06.479Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "d0a3a55f-f6f6-4b5d-b675-0b8d5249c2e8",
      "number": 16,
      "content": "Ở trong xứ, ai tuyên thệ",
      "order": 2,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.479Z",
      "updatedAt": "2024-09-07T14:54:06.479Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "4c2c8109-8848-4592-9ca5-bcebda3aa219",
      "number": 16,
      "content": "Sẽ nhân danh Đức Chúa Trời của sự thật mà thề,",
      "order": 3,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.479Z",
      "updatedAt": "2024-09-07T14:54:06.479Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "a4cf9c14-e3b5-4fef-b760-c246293b7e22",
      "number": 16,
      "content": "Vì những gian truân khi trước đã bị quên rồi,",
      "order": 4,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.479Z",
      "updatedAt": "2024-09-07T14:54:06.479Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "161dd1ba-3d96-490c-a358-1afe4de568a4",
      "number": 16,
      "content": "Và vì chúng đã bị che khuất khỏi mắt Ta rồi.",
      "order": 5,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.479Z",
      "updatedAt": "2024-09-07T14:54:06.479Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "5f29dfe3-b677-4bed-9d45-08eea51abf03",
      "number": 17,
      "content": "Vì này, Ta sẽ dựng nên",
      "order": 0,
      "parNumber": 73,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.482Z",
      "updatedAt": "2024-09-07T14:54:06.482Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "c937ec8e-ad7e-4c87-86b3-60586d9ddc9c",
      "number": 17,
      "content": "Trời mới và đất mới;",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.482Z",
      "updatedAt": "2024-09-07T14:54:06.482Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "531a7f12-438d-4283-99ca-1f144babddef",
      "number": 17,
      "content": "Những gì trước kia sẽ không còn được nhớ đến nữa,",
      "order": 2,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.482Z",
      "updatedAt": "2024-09-07T14:54:06.482Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "f6f4ec89-d37e-461b-9a76-c342853a680a",
      "number": 17,
      "content": "Và cũng không còn đến trong trí nữa.",
      "order": 3,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.482Z",
      "updatedAt": "2024-09-07T14:54:06.482Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "47eeb1da-58a0-43f8-a40b-1c0583b70a11",
      "number": 18,
      "content": "Nhưng hãy hân hoan và vui mừng mãi mãi",
      "order": 0,
      "parNumber": 77,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.484Z",
      "updatedAt": "2024-09-07T14:54:06.484Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "a90140fe-d9bc-4d62-8a12-f36bb2914fb1",
      "number": 18,
      "content": "Về những gì Ta tạo dựng;",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.484Z",
      "updatedAt": "2024-09-07T14:54:06.484Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "9fc81993-673c-4f38-96d1-8948916a3c37",
      "number": 18,
      "content": "Vì này, Ta dựng nên Giê-ru-sa-lem để làm một niềm vui;",
      "order": 2,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.484Z",
      "updatedAt": "2024-09-07T14:54:06.484Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "40a168d8-3b82-435f-9746-346d280c4529",
      "number": 18,
      "content": "Và dân thành ấy sẽ là một niềm vui thỏa.",
      "order": 3,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.484Z",
      "updatedAt": "2024-09-07T14:54:06.484Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "4dfabbcf-4d35-4976-ba50-39177a8efb3c",
      "number": 19,
      "content": "Ta sẽ vui vẻ vì Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 81,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.487Z",
      "updatedAt": "2024-09-07T14:54:06.487Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "f798cc62-aaf3-4907-aa7a-ed20e8c4404b",
      "number": 19,
      "content": "Ta sẽ vui mừng vì dân Ta;",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.487Z",
      "updatedAt": "2024-09-07T14:54:06.487Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "42de1928-6d6a-4022-be3c-1770ee4a638d",
      "number": 19,
      "content": "Tiếng than khóc sẽ không còn nghe trong thành ấy;",
      "order": 2,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.487Z",
      "updatedAt": "2024-09-07T14:54:06.487Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "acac11da-aaf7-4d9c-ad0f-63f29c05b4d4",
      "number": 19,
      "content": "Tiếng khóc than sẽ vĩnh viễn không còn.",
      "order": 3,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.487Z",
      "updatedAt": "2024-09-07T14:54:06.487Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "8329273a-b943-4356-870a-79d41fe1a6c8",
      "number": 20,
      "content": "Trong thành ấy trẻ thơ sẽ không còn chết yểu;",
      "order": 0,
      "parNumber": 85,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.489Z",
      "updatedAt": "2024-09-07T14:54:06.489Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "90c0eb5e-7397-442c-8cfa-5776ccc7cf5d",
      "number": 20,
      "content": "Không người già nào chẳng hưởng trọn tuổi thọ của mình;",
      "order": 1,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.489Z",
      "updatedAt": "2024-09-07T14:54:06.489Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "828da434-d9f2-43b7-b537-92e2433c7636",
      "number": 20,
      "content": "Vì ai trăm tuổi mà qua đời sẽ bị xem là chết trẻ;",
      "order": 2,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.489Z",
      "updatedAt": "2024-09-07T14:54:06.489Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "4c2c3d8e-1b6c-4c67-a5d6-4950cc195520",
      "number": 20,
      "content": "Người chưa được bách niên mà thất lộc sẽ xem như bị rủa nguyền.",
      "order": 3,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.489Z",
      "updatedAt": "2024-09-07T14:54:06.489Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "af1bf5f8-7012-4c20-9a7a-c2506831a6e6",
      "number": 21,
      "content": "Chúng sẽ xây nhà và an cư trong các nhà đó;",
      "order": 0,
      "parNumber": 89,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.491Z",
      "updatedAt": "2024-09-07T14:54:06.491Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "3c01356e-807f-4a8a-aebb-b8fae06f526f",
      "number": 21,
      "content": "Chúng sẽ trồng vườn nho và được ăn trái mình trồng.",
      "order": 1,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.491Z",
      "updatedAt": "2024-09-07T14:54:06.491Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "9d72551a-d326-4973-b195-8f8a69f5aaa4",
      "number": 22,
      "content": "Chúng sẽ không xây nhà để bị người khác vào chiếm ở;",
      "order": 0,
      "parNumber": 91,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.494Z",
      "updatedAt": "2024-09-07T14:54:06.494Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "babc25bf-ced0-47c3-9d96-42f6b9365daa",
      "number": 22,
      "content": "Chúng sẽ không trồng cây để bị người khác đến chiếm ăn.",
      "order": 1,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.494Z",
      "updatedAt": "2024-09-07T14:54:06.494Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "1b4bd2f9-6400-4193-886f-512fb6b9ed26",
      "number": 22,
      "content": "Vì cây cối sống bao nhiêu tuổi, dân Ta cũng sẽ thọ bấy nhiêu.",
      "order": 2,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.494Z",
      "updatedAt": "2024-09-07T14:54:06.494Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "7d2ee074-aeb4-47e9-a75b-672c7b00dcb8",
      "number": 22,
      "content": "Những kẻ Ta chọn sẽ hưởng lâu dài mọi kết quả do tay mình làm ra.",
      "order": 3,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.494Z",
      "updatedAt": "2024-09-07T14:54:06.494Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "8f5322d7-f16a-4580-be8d-a64bf8092a64",
      "number": 23,
      "content": "Chúng sẽ không làm lụng cách luống công vô ích;",
      "order": 0,
      "parNumber": 95,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.500Z",
      "updatedAt": "2024-09-07T14:54:06.500Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "8c51c37d-3457-4329-8a45-7ccb81daef1a",
      "number": 23,
      "content": "Chúng sinh con ra chẳng phải để chuốc lấy âu sầu,",
      "order": 1,
      "parNumber": 96,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.500Z",
      "updatedAt": "2024-09-07T14:54:06.500Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "caddd06d-a919-4031-9b1d-0379cd6995f9",
      "number": 23,
      "content": "Vì con cháu chúng sẽ là những người được CHÚA ban phước,",
      "order": 2,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.500Z",
      "updatedAt": "2024-09-07T14:54:06.500Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "36969e80-bd8c-4f49-8148-d210b6e993d4",
      "number": 23,
      "content": "Và dòng dõi chúng cùng với chúng nữa.",
      "order": 3,
      "parNumber": 98,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.500Z",
      "updatedAt": "2024-09-07T14:54:06.500Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "65f00784-b6e6-4713-96de-1d4f5fe21dcf",
      "number": 24,
      "content": "Rồi đây, trước khi chúng cầu xin, Ta đã trả lời;",
      "order": 0,
      "parNumber": 99,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.505Z",
      "updatedAt": "2024-09-07T14:54:06.505Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "505e28bc-f34a-43d9-af6e-2acf6071cac8",
      "number": 24,
      "content": "Khi chúng còn đang nói, Ta đã nghe rõ hết.",
      "order": 1,
      "parNumber": 100,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.505Z",
      "updatedAt": "2024-09-07T14:54:06.505Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "2bad76f9-bdb9-4ab3-ad15-61f51184df34",
      "number": 25,
      "content": "Chó sói và chiên con sẽ ăn cỏ với nhau,",
      "order": 0,
      "parNumber": 101,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.510Z",
      "updatedAt": "2024-09-07T14:54:06.510Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "ab8f50e3-28ae-4ad2-9853-ac0e8fb6be6f",
      "number": 25,
      "content": "Sư tử sẽ ăn rơm rạ như bò,",
      "order": 1,
      "parNumber": 102,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.510Z",
      "updatedAt": "2024-09-07T14:54:06.510Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "e1568fed-a49f-4e9f-9570-899dcf9605c5",
      "number": 25,
      "content": "Còn bụi đất sẽ là thức ăn của loài rắn;",
      "order": 2,
      "parNumber": 103,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.510Z",
      "updatedAt": "2024-09-07T14:54:06.510Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "b5f3f47b-2a4c-4828-9daa-879db825b859",
      "number": 25,
      "content": "Chúng sẽ không gây đau thương hay hủy diệt",
      "order": 3,
      "parNumber": 104,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.510Z",
      "updatedAt": "2024-09-07T14:54:06.510Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "5306af73-3b9f-4557-8390-78c9dfc67fce",
      "number": 25,
      "content": "Trong khắp núi thánh của Ta,”",
      "order": 4,
      "parNumber": 105,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.510Z",
      "updatedAt": "2024-09-07T14:54:06.510Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    },
    {
      "id": "0e812d5a-fc22-4f0e-b1c4-509dc35becd6",
      "number": 25,
      "content": "CHÚA phán.",
      "order": 5,
      "parNumber": 106,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:54:06.510Z",
      "updatedAt": "2024-09-07T14:54:06.510Z",
      "chapterId": "2bc223d9-5b3e-4674-8377-fe7b0055f690"
    }
  ],
