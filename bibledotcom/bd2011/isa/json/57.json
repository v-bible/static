{
  "chapter": {
    "id": "318c6baa-1da2-41f6-b009-0df6255a7968",
    "number": 57,
    "ref": "https://www.bible.com/bible/19/ISA.57.BD2011",
    "createdAt": "2024-09-07T11:51:12.072Z",
    "updatedAt": "2024-09-07T11:51:12.072Z",
    "bookId": "dd94a7a2-46d4-489f-a1b9-a328e1eafa8e"
  },
  "verses": [
    {
      "id": "e3d483d9-aa7c-461f-97a7-f136b73e8e9c",
      "number": 1,
      "content": "Người công chính qua đời",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.416Z",
      "updatedAt": "2024-09-07T14:51:24.416Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "34cee56e-9b28-4bba-b599-041cc16b0dab",
      "number": 1,
      "content": "Mà chẳng ai quan tâm đến;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.416Z",
      "updatedAt": "2024-09-07T14:51:24.416Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "085a8b34-cea8-4bc6-9222-bd05850e967c",
      "number": 1,
      "content": "Người đạo đức bị cất đi",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.416Z",
      "updatedAt": "2024-09-07T14:51:24.416Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "126afbc1-f312-4385-b7c0-247418c95605",
      "number": 1,
      "content": "Mà không ai nghĩ rằng,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.416Z",
      "updatedAt": "2024-09-07T14:51:24.416Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "0a86bb4c-9577-4311-9de8-acf7e733bab6",
      "number": 1,
      "content": "Người công chính được cất đi là để tránh khỏi tai họa.",
      "order": 4,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.416Z",
      "updatedAt": "2024-09-07T14:51:24.416Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "50e60230-278c-4259-ae8f-54c955c44fb8",
      "number": 2,
      "content": "Người ấy sẽ đi vào cõi bình an;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.419Z",
      "updatedAt": "2024-09-07T14:51:24.419Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "95b372fc-a758-4235-b055-db509614ae25",
      "number": 2,
      "content": "Ai bước đi trong sự chính trực",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.419Z",
      "updatedAt": "2024-09-07T14:51:24.419Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "5e14e4eb-8bda-4bfe-b26a-1722340109c0",
      "number": 2,
      "content": "Sẽ được nghỉ ngơi trên giường mình.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.419Z",
      "updatedAt": "2024-09-07T14:51:24.419Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "cc89bbfb-a051-4335-845d-65a78ee50c46",
      "number": 3,
      "content": "“Còn các ngươi, hãy đến đây,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.422Z",
      "updatedAt": "2024-09-07T14:51:24.422Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "36985ed9-9fe3-47b4-8820-f293e422f52a",
      "number": 3,
      "content": "Hỡi con cái của mụ phù thủy,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.422Z",
      "updatedAt": "2024-09-07T14:51:24.422Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "7bd730cb-285d-446c-ba0b-03e192c84f87",
      "number": 3,
      "content": "Hỡi dòng dõi của kẻ ngoại tình và phường đĩ điếm.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.422Z",
      "updatedAt": "2024-09-07T14:51:24.422Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "d64ff178-55df-4a15-b6b1-1117f518ad36",
      "number": 4,
      "content": "Các ngươi cười nhạo ai?",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.424Z",
      "updatedAt": "2024-09-07T14:51:24.424Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "368818e9-27f8-4aed-a9e0-a58d0c422d8b",
      "number": 4,
      "content": "Các ngươi mở rộng miệng",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.424Z",
      "updatedAt": "2024-09-07T14:51:24.424Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "86a45c86-3703-4284-8e9f-491f2890d25d",
      "number": 4,
      "content": "Và le lưỡi chống lại ai?",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.424Z",
      "updatedAt": "2024-09-07T14:51:24.424Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "678b5f94-3bf4-4f52-90f0-853b2f438287",
      "number": 4,
      "content": "Há chẳng phải các ngươi là con cái của tội lỗi,",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.424Z",
      "updatedAt": "2024-09-07T14:51:24.424Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "9396dd51-7629-4436-8440-2b94146594c0",
      "number": 4,
      "content": "Và dòng dõi của dối trá sao?",
      "order": 4,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.424Z",
      "updatedAt": "2024-09-07T14:51:24.424Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "7bfbec61-29d1-4190-863b-b1da84c5d52e",
      "number": 5,
      "content": "Há chẳng phải các ngươi đã hành dâm với các thần tượng",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.427Z",
      "updatedAt": "2024-09-07T14:51:24.427Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "84da99bd-e470-433c-bf5f-b5a1f68b086c",
      "number": 5,
      "content": "Dưới mọi cây cổ thụ,",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.427Z",
      "updatedAt": "2024-09-07T14:51:24.427Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "21dfa186-e755-41cc-83fe-119a25eb3a2c",
      "number": 5,
      "content": "Các ngươi giết các con thơ mình trong các thung lũng,",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.427Z",
      "updatedAt": "2024-09-07T14:51:24.427Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "d70038d5-2fc6-4a65-904e-047e0164596e",
      "number": 5,
      "content": "Trong các hốc đá để tế thần sao?",
      "order": 3,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.427Z",
      "updatedAt": "2024-09-07T14:51:24.427Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "fa51c408-4266-4239-bbf0-9368b535a9b2",
      "number": 6,
      "content": "Nằm giữa các viên đá trơn tru trong khe suối sẽ là phần của ngươi,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.430Z",
      "updatedAt": "2024-09-07T14:51:24.430Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "b6a83563-8fba-4189-b409-be5a1e9c57d8",
      "number": 6,
      "content": "Chúng, chính chúng, là phần của ngươi;",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.430Z",
      "updatedAt": "2024-09-07T14:51:24.430Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "550e7062-77d1-466f-a7db-2fa6d3287a89",
      "number": 6,
      "content": "Ngay cả những vật vô tri như thế mà ngươi cũng dâng lễ quán cho chúng,",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.430Z",
      "updatedAt": "2024-09-07T14:51:24.430Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "cb0a4821-9519-429e-a816-297116eb7e93",
      "number": 6,
      "content": "Ngươi lấy lúa gạo làm của lễ chay dâng cho chúng,",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.430Z",
      "updatedAt": "2024-09-07T14:51:24.430Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "957aa510-6995-4dab-b0ad-78cf522a0396",
      "number": 6,
      "content": "Ta há có thể cứ thản nhiên trước những hành vi như thế hay sao?",
      "order": 4,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.430Z",
      "updatedAt": "2024-09-07T14:51:24.430Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "0165a1a3-c329-4506-8680-0f2bd5f69cc4",
      "number": 7,
      "content": "Ngươi đã kê giường cho mình trên đỉnh núi cao chót vót;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.432Z",
      "updatedAt": "2024-09-07T14:51:24.432Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "55efe8fd-4455-4dfb-8ca8-1cf2b2c02348",
      "number": 7,
      "content": "Ngươi đã lên tận nơi cao như thế",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.432Z",
      "updatedAt": "2024-09-07T14:51:24.432Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "7448bb4e-3057-4648-a3c6-69e7f5183fa8",
      "number": 7,
      "content": "Để dâng con vật hiến tế cho các thần tượng.",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.432Z",
      "updatedAt": "2024-09-07T14:51:24.432Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "97acb4f5-bb7f-4e4d-86ec-05de0f50e18a",
      "number": 8,
      "content": "Sau các cánh cửa và sau các cột cửa,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.435Z",
      "updatedAt": "2024-09-07T14:51:24.435Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "09c001a3-9071-4238-8054-7cb8fbc06d1e",
      "number": 8,
      "content": "Ngươi đã lưu dấu kỷ niệm mình,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.435Z",
      "updatedAt": "2024-09-07T14:51:24.435Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "d8d60dac-a4fd-4185-8ab3-2b36506a485a",
      "number": 8,
      "content": "Tại đó ngươi đã phản bội Ta.",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.435Z",
      "updatedAt": "2024-09-07T14:51:24.435Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "0b23c177-808d-4d14-ba5f-c9fa26ab2e89",
      "number": 8,
      "content": "Ngươi đã cởi bỏ y phục mình, để lên giường với các thần tượng;",
      "order": 3,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.435Z",
      "updatedAt": "2024-09-07T14:51:24.435Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "89c15156-2f4c-497c-87a6-f4040dda855a",
      "number": 8,
      "content": "Ngươi đã nới rộng giường của ngươi;",
      "order": 4,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.435Z",
      "updatedAt": "2024-09-07T14:51:24.435Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "def83d91-771c-4c7f-bacd-a33b82fb607a",
      "number": 8,
      "content": "Ngươi đã kết ước với chúng;",
      "order": 5,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.435Z",
      "updatedAt": "2024-09-07T14:51:24.435Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "7e8a3578-2bb8-4f1e-bf84-2b2105be7a3d",
      "number": 8,
      "content": "Ngươi đã yêu mến giường của chúng,",
      "order": 6,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.435Z",
      "updatedAt": "2024-09-07T14:51:24.435Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "6ff522aa-d30e-47c5-8172-4a8aa80c7de3",
      "number": 8,
      "content": "Và ngươi đã ngắm nhìn chúng trần trụi lõa lồ.",
      "order": 7,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.435Z",
      "updatedAt": "2024-09-07T14:51:24.435Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "b9053bb6-a52d-49d5-9f66-71bb53e18421",
      "number": 9,
      "content": "Ngươi đã đem dầu và rất nhiều dầu thơm",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.437Z",
      "updatedAt": "2024-09-07T14:51:24.437Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "d4dc691d-894a-4317-8d80-520df9c89628",
      "number": 9,
      "content": "Đến dâng cho Mô-lếch;",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.437Z",
      "updatedAt": "2024-09-07T14:51:24.437Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "ea38b9a5-e50b-442d-87a5-f73f945f24df",
      "number": 9,
      "content": "Ngươi đã sai các sứ giả đến tận phương xa,",
      "order": 2,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.437Z",
      "updatedAt": "2024-09-07T14:51:24.437Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "eea887f5-efb7-4026-9aff-491600f960ac",
      "number": 9,
      "content": "Và đã phái chúng xuống tận âm phủ.",
      "order": 3,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.437Z",
      "updatedAt": "2024-09-07T14:51:24.437Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "128c96d9-a2a9-4a9c-b2b7-8d7a25627dea",
      "number": 10,
      "content": "Ngươi đã mệt mỏi vì hành trình dài đằng đẵng,",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.440Z",
      "updatedAt": "2024-09-07T14:51:24.440Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "ebcc5c83-386b-409f-bb7c-c7d4aaafdf74",
      "number": 10,
      "content": "Nhưng ngươi chẳng than rằng, ‘Thật uổng công phí sức!’",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.440Z",
      "updatedAt": "2024-09-07T14:51:24.440Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "b026b7a3-44a7-4523-9648-9c5b6cb053a9",
      "number": 10,
      "content": "Ngươi cho rằng tay mình thừa sức làm điều mình muốn,",
      "order": 2,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.440Z",
      "updatedAt": "2024-09-07T14:51:24.440Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "85cd3091-d056-4409-acb2-704bf0939845",
      "number": 10,
      "content": "Nên ngươi chẳng ngại mà cứ làm.",
      "order": 3,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.440Z",
      "updatedAt": "2024-09-07T14:51:24.440Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "c1ad4cbb-db0d-48d1-a12f-3a05ce751560",
      "number": 11,
      "content": "Ngươi nào có kính nể ai hoặc kính sợ ai,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.442Z",
      "updatedAt": "2024-09-07T14:51:24.442Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "3029386d-cbbc-4163-bcb6-88f7955b8f35",
      "number": 11,
      "content": "Có phải vì thế mà ngươi đã nói dối,",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.442Z",
      "updatedAt": "2024-09-07T14:51:24.442Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "38c11d4f-08c4-4484-b0a6-8539b610bacb",
      "number": 11,
      "content": "Chẳng nhớ Ta,",
      "order": 2,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.442Z",
      "updatedAt": "2024-09-07T14:51:24.442Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "067a3350-571f-4c7e-ab56-e349216b9249",
      "number": 11,
      "content": "Và không quan tâm gì đến Ta chăng?",
      "order": 3,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.442Z",
      "updatedAt": "2024-09-07T14:51:24.442Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "df500794-a1f2-4355-8d40-69c68e783427",
      "number": 11,
      "content": "Có phải vì lâu lắm rồi Ta đã cố nhịn,",
      "order": 4,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.442Z",
      "updatedAt": "2024-09-07T14:51:24.442Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "2193e8ae-f4f4-47d9-ab3e-40d2a58f516f",
      "number": 11,
      "content": "Nên ngươi không kính sợ Ta chăng?",
      "order": 5,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.442Z",
      "updatedAt": "2024-09-07T14:51:24.442Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "4ad6ba41-92f9-4c28-8449-72dccda6dddd",
      "number": 12,
      "content": "Ta sẽ công bố những gì công chính của ngươi ra, cùng các việc làm của ngươi nữa,",
      "order": 0,
      "parNumber": 51,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.445Z",
      "updatedAt": "2024-09-07T14:51:24.445Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "863c27c2-6cde-4a95-a791-ac93d0a5dbb9",
      "number": 12,
      "content": "Nhưng chúng sẽ chẳng giúp được gì cho ngươi đâu.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.445Z",
      "updatedAt": "2024-09-07T14:51:24.445Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "3bb67441-4573-4f0b-b414-eff54740c85f",
      "number": 13,
      "content": "Khi ngươi kêu cầu, hãy để các thần tượng ngươi thờ cúng giải cứu ngươi.",
      "order": 0,
      "parNumber": 53,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.447Z",
      "updatedAt": "2024-09-07T14:51:24.447Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "7d944ce0-5106-482c-b4cf-5ef6f63150a2",
      "number": 13,
      "content": "Kìa chỉ một cơn gió thổi qua,",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.447Z",
      "updatedAt": "2024-09-07T14:51:24.447Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "d4fe978c-5251-4c63-948e-51e84e523703",
      "number": 13,
      "content": "Một làn hơi nhẹ lướt qua, cũng đủ sức cuốn tất cả chúng bay mất.",
      "order": 2,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.447Z",
      "updatedAt": "2024-09-07T14:51:24.447Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "5d01fb36-0a11-44a0-a4f9-a94359af41fd",
      "number": 13,
      "content": "Nhưng ai để lòng tin cậy Ta sẽ được đất làm cơ nghiệp,",
      "order": 3,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.447Z",
      "updatedAt": "2024-09-07T14:51:24.447Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "41356d78-1372-4b6e-80bc-cbc9bef53169",
      "number": 13,
      "content": "Và sẽ thừa hưởng núi thánh của Ta.”",
      "order": 4,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.447Z",
      "updatedAt": "2024-09-07T14:51:24.447Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "964503aa-bf15-49b7-9d29-1f323b0225a9",
      "number": 14,
      "content": "Có lời phán rằng,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.450Z",
      "updatedAt": "2024-09-07T14:51:24.450Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "f43ec009-0890-4b0a-8694-2282c861a1c8",
      "number": 14,
      "content": "“Hãy đắp đường! Hãy đắp đường! Hãy sửa soạn đường.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.450Z",
      "updatedAt": "2024-09-07T14:51:24.450Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "21b66514-2be4-4be7-b3f9-14608021f449",
      "number": 14,
      "content": "Hãy dẹp bỏ mọi chướng ngại trên đường của dân Ta,”",
      "order": 2,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.450Z",
      "updatedAt": "2024-09-07T14:51:24.450Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "77c7270f-bc0b-4c48-a86e-679fe4980299",
      "number": 15,
      "content": "Vì Đấng Chí Cao và Chí Tôn,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.452Z",
      "updatedAt": "2024-09-07T14:51:24.452Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "8e0f9f90-d317-40af-af1d-0a1d32043f3d",
      "number": 15,
      "content": "Đấng ngự trong cõi vĩnh hằng, Đấng mang danh là Đấng Thánh, phán thế này:",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.452Z",
      "updatedAt": "2024-09-07T14:51:24.452Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "72106560-fee2-44b3-b365-2942e883c3b6",
      "number": 15,
      "content": "“Dù Ta ngự trong cõi chí cao và chí thánh,",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.452Z",
      "updatedAt": "2024-09-07T14:51:24.452Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "dfaea16d-4737-4ba0-9d7f-67e6d0226135",
      "number": 15,
      "content": "Nhưng Ta vẫn ở với người có lòng tan vỡ và khiêm cung,",
      "order": 3,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.452Z",
      "updatedAt": "2024-09-07T14:51:24.452Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "71bc7885-eb98-4cc8-a044-361aa32e1e1c",
      "number": 15,
      "content": "Để làm hồi sinh tinh thần của người khiêm tốn,",
      "order": 4,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.452Z",
      "updatedAt": "2024-09-07T14:51:24.452Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "445d5691-0686-4530-b850-370bf53cd804",
      "number": 15,
      "content": "Và làm hồi sinh tấm lòng của người tan vỡ,",
      "order": 5,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.452Z",
      "updatedAt": "2024-09-07T14:51:24.452Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "4d4ae8cf-df0a-4362-9725-0b2a544506e0",
      "number": 16,
      "content": "Vì Ta sẽ không buộc tội đến đời đời,",
      "order": 0,
      "parNumber": 68,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.454Z",
      "updatedAt": "2024-09-07T14:51:24.454Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "0bc7a0d3-23f7-4177-8703-211eb626ffcc",
      "number": 16,
      "content": "Ta sẽ không giận buồn mãi mãi,",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.454Z",
      "updatedAt": "2024-09-07T14:51:24.454Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "a646834a-7c78-472b-9d37-650e899f3073",
      "number": 16,
      "content": "Kẻo linh khí và các linh hồn Ta đã dựng nên",
      "order": 2,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.454Z",
      "updatedAt": "2024-09-07T14:51:24.454Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "848483ae-a988-432d-b5b9-33f94b59d2f8",
      "number": 16,
      "content": "Phải tàn tạ trước mặt Ta chăng.",
      "order": 3,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.454Z",
      "updatedAt": "2024-09-07T14:51:24.454Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "cc32328a-c81e-4a53-a137-06041931ce20",
      "number": 17,
      "content": "Vì tội tham lam ích kỷ của nó, Ta đã giận và đánh phạt nó;",
      "order": 0,
      "parNumber": 72,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.457Z",
      "updatedAt": "2024-09-07T14:51:24.457Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "321d86c7-5ae3-44f6-abf6-7a4aa60573f6",
      "number": 17,
      "content": "Ta đã ẩn mặt với nó và giận nó;",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.457Z",
      "updatedAt": "2024-09-07T14:51:24.457Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "34685e37-9c2f-4438-b2d5-f237ffd43b53",
      "number": 17,
      "content": "Thế mà nó vẫn trở lại con đường lòng nó muốn đi theo.",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.457Z",
      "updatedAt": "2024-09-07T14:51:24.457Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "e4b253d8-c70b-4783-8788-c47258afa6bd",
      "number": 18,
      "content": "Ta đã thấy rõ các đường lối nó, dầu vậy Ta sẽ chữa lành nó;",
      "order": 0,
      "parNumber": 75,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.459Z",
      "updatedAt": "2024-09-07T14:51:24.459Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "535b1e95-d964-4b14-92e6-ab4ec105e5a6",
      "number": 18,
      "content": "Ta sẽ dẫn dắt nó và phục hồi niềm an ủi cho nó và những kẻ than khóc của nó.",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.459Z",
      "updatedAt": "2024-09-07T14:51:24.459Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "b8eb233f-c150-404b-87ca-717eddfc407e",
      "number": 19,
      "content": "Ta sẽ làm cho môi miệng chúng thốt lên những lời ca ngợi.",
      "order": 0,
      "parNumber": 77,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.461Z",
      "updatedAt": "2024-09-07T14:51:24.461Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "3f464529-79dd-42db-a11c-128e78feabc1",
      "number": 19,
      "content": "Hòa bình thịnh vượng! Hòa bình thịnh vượng đến với những kẻ ở xa và những người ở gần,” CHÚA phán,",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.461Z",
      "updatedAt": "2024-09-07T14:51:24.461Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "6b216277-d868-460c-941f-37b7c5866bc1",
      "number": 19,
      "content": "“Rồi Ta sẽ chữa lành nó.”",
      "order": 2,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.461Z",
      "updatedAt": "2024-09-07T14:51:24.461Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "3ee73763-0702-4e9b-a5a2-f7ec3e20b9b5",
      "number": 20,
      "content": "Nhưng phường gian ác giống như biển động;",
      "order": 0,
      "parNumber": 80,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.464Z",
      "updatedAt": "2024-09-07T14:51:24.464Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "87637e32-b42e-44ce-9405-9072fa57574f",
      "number": 20,
      "content": "Vì chúng không thể lặng yên;",
      "order": 1,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.464Z",
      "updatedAt": "2024-09-07T14:51:24.464Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "68d1fb5f-325e-4815-b3fc-f69bc03c425d",
      "number": 20,
      "content": "Chúng giống như sóng biển, cứ cuộn lên, đẩy ra bùn, và làm vẩn đục.",
      "order": 2,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:51:24.464Z",
      "updatedAt": "2024-09-07T14:51:24.464Z",
      "chapterId": "318c6baa-1da2-41f6-b009-0df6255a7968"
    },
    {
      "id": "0f4a43e1-c113-4b14-9c4b-d6f9c40559b5",
      "number": 21,
      "content": "Đức Chúa Trời của tôi phán, “Phường gian ác sẽ không có hòa bình thịnh vượng.”",
      "order": 0,
      "parNumber": 83,
      "parIndex": 1,
