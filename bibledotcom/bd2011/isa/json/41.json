    },
    {
      "id": "5fa705b6-d072-4341-9d21-506bbe1b70d1",
      "content": "Sự Hư Không của Thần Tượng",
      "order": 0,
      "createdAt": "2024-09-07T14:47:58.417Z",
      "updatedAt": "2024-09-07T14:47:58.417Z",
