    {
      "id": "cec45eaa-d468-4857-8600-bcc4f176c151",
      "content": "Sự Kiêng Ăn Đẹp Lòng CHÚA",
      "order": 0,
      "createdAt": "2024-09-07T14:51:54.510Z",
      "updatedAt": "2024-09-07T14:51:54.510Z",
      "verseId": "71d95b9d-fab4-4557-b1f1-9ab9c89289c0",
      "chapterId": "b75d4658-0b56-485e-b3d3-12ab13e7033f"
    }
  ],
