{
  "chapter": {
    "id": "8fc902cb-ef92-445b-98b2-76efa3ca4397",
    "number": 6,
    "ref": "https://www.bible.com/bible/19/JER.6.BD2011",
    "createdAt": "2024-09-07T11:51:12.119Z",
    "updatedAt": "2024-09-07T11:51:12.119Z",
    "bookId": "7077d0f5-3ae3-430b-90b5-7bd6139c2f9e"
  },
  "verses": [
    {
      "id": "2213f9d9-86cf-4b32-8733-123b467ff725",
      "number": 1,
      "content": "“Hỡi các con cháu của Bên-gia-min,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.933Z",
      "updatedAt": "2024-09-07T14:56:04.933Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "a4928dda-cb40-4cc5-9aff-e45d5ebd4894",
      "number": 1,
      "content": "Hãy trốn khỏi Giê-ru-sa-lem để bảo tồn tính mạng!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.933Z",
      "updatedAt": "2024-09-07T14:56:04.933Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "453a2e0a-e92f-4a02-aa66-332835ff4822",
      "number": 1,
      "content": "Hãy thổi kèn báo động vang lên ở Tê-cô-a;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.933Z",
      "updatedAt": "2024-09-07T14:56:04.933Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "704c49e8-8b69-4662-9c2e-e35cf942e201",
      "number": 1,
      "content": "Hãy giương cao cờ báo giặc tới ở Bết Hạc-kê-rem,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.933Z",
      "updatedAt": "2024-09-07T14:56:04.933Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "f420bf45-763b-4e0d-9e3d-4bf5ae71ba02",
      "number": 1,
      "content": "Vì hiểm họa đang lộ dần từ phương bắc;",
      "order": 4,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.933Z",
      "updatedAt": "2024-09-07T14:56:04.933Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "233f50b4-84ee-41c2-8027-ca2850441119",
      "number": 1,
      "content": "Thật là một cuộc tàn sát lớn lao khủng khiếp!",
      "order": 5,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.933Z",
      "updatedAt": "2024-09-07T14:56:04.933Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "6fcaeb17-6810-4a76-bb2b-060b95d38823",
      "number": 2,
      "content": "Hỡi Ái Nữ của Si-ôn xinh đẹp yêu kiều của Ta,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.942Z",
      "updatedAt": "2024-09-07T14:56:04.942Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "3c0b45e0-4214-485b-9b4b-3366f7d8e304",
      "number": 2,
      "content": "Ta buộc lòng phải để cho con bị tiêu diệt!",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.942Z",
      "updatedAt": "2024-09-07T14:56:04.942Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "48160433-9395-4d52-9144-87f6ceae9806",
      "number": 3,
      "content": "Bọn chăn chiên sẽ dẫn các đàn chiên của chúng quanh nàng;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.945Z",
      "updatedAt": "2024-09-07T14:56:04.945Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "b6319084-c305-4ec5-be95-362dad528dd5",
      "number": 3,
      "content": "Chúng sẽ cắm lều dựng trại quanh nàng;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.945Z",
      "updatedAt": "2024-09-07T14:56:04.945Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "e03bd1c4-6a6e-4699-87ea-cb87b782b8e8",
      "number": 3,
      "content": "Chúng sẽ cho đàn súc vật của chúng ăn cỏ ở những vùng chúng chiếm lấy.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.945Z",
      "updatedAt": "2024-09-07T14:56:04.945Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "9ebfb2d7-3bdd-4de6-8582-3672fd48cfb2",
      "number": 4,
      "content": "‘Hãy chuẩn bị tấn công nàng.",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.947Z",
      "updatedAt": "2024-09-07T14:56:04.947Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "27e53c07-9939-4a42-8092-3f5799220c70",
      "number": 4,
      "content": "Hãy đứng lên! Chúng ta hãy công hãm lúc giữa trưa.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.947Z",
      "updatedAt": "2024-09-07T14:56:04.947Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "f0e01e5b-2ab8-40ae-95c2-ebfa1d37ba1d",
      "number": 4,
      "content": "Khốn thay cho chúng ta! Trời đã về chiều mất rồi.",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.947Z",
      "updatedAt": "2024-09-07T14:56:04.947Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "8fbbe6ab-2c70-432b-97af-66838b308ca4",
      "number": 4,
      "content": "Bóng hoàng hôn đã ngã dài rồi!",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.947Z",
      "updatedAt": "2024-09-07T14:56:04.947Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "ade16b4f-250a-424f-9fab-13a5ec9e8b51",
      "number": 5,
      "content": "Hãy đứng dậy! Chúng ta hãy tấn công nàng vào ban đêm",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.950Z",
      "updatedAt": "2024-09-07T14:56:04.950Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "ba194c8b-5520-4375-9cd1-fcdd2b1ae228",
      "number": 5,
      "content": "Và tiêu diệt các lâu đài của nàng,’”",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.950Z",
      "updatedAt": "2024-09-07T14:56:04.950Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "e5f74729-fd8d-4543-a2b6-6073339df7f6",
      "number": 6,
      "content": "Vì CHÚA các đạo quân phán,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.952Z",
      "updatedAt": "2024-09-07T14:56:04.952Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "1132e80e-0f7a-43f8-a691-c4faa87c873e",
      "number": 6,
      "content": "“Hãy chặt cây xuống;",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.952Z",
      "updatedAt": "2024-09-07T14:56:04.952Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "34daf591-93bc-4f78-b1a8-3b1cd77fdd5a",
      "number": 6,
      "content": "Hãy đắp chiến hào công phá Giê-ru-sa-lem.",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.952Z",
      "updatedAt": "2024-09-07T14:56:04.952Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "9613d7ca-7829-4b8c-990f-3e7aa559d78b",
      "number": 6,
      "content": "Đây là thành phải bị đánh phạt,",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.952Z",
      "updatedAt": "2024-09-07T14:56:04.952Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "26a3ee10-f34a-4b2a-9d3c-5c0fc65600b4",
      "number": 6,
      "content": "Vì trong thành chỉ toàn là áp bức hiếp người.",
      "order": 4,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.952Z",
      "updatedAt": "2024-09-07T14:56:04.952Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "3e8eceb7-b9d4-4cb9-bd1f-69991f5524f7",
      "number": 7,
      "content": "Như dòng suối trào tuôn nước mát,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.955Z",
      "updatedAt": "2024-09-07T14:56:04.955Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "44e4a307-661e-45c7-a90a-72c4e3705fac",
      "number": 7,
      "content": "Nàng tuôn trào tội ác không ngừng.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.955Z",
      "updatedAt": "2024-09-07T14:56:04.955Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "e87bbbb7-512a-47e9-a55b-c2ae430f3f17",
      "number": 7,
      "content": "Tin tức về bạo ngược và cướp bóc luôn nghe nói ở giữa nàng;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.955Z",
      "updatedAt": "2024-09-07T14:56:04.955Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "178ced10-e533-4449-a76f-fc045f146599",
      "number": 7,
      "content": "Sầu khổ và đau thương xảy ra không ngớt trước mắt Ta.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.955Z",
      "updatedAt": "2024-09-07T14:56:04.955Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "a23b40f0-8d03-4463-af99-1a83b057d453",
      "number": 8,
      "content": "Hỡi Giê-ru-sa-lem, hãy nghe lời khuyên răn cảnh cáo,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.957Z",
      "updatedAt": "2024-09-07T14:56:04.957Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "03b75485-ad16-4681-a2c7-99773a993d52",
      "number": 8,
      "content": "Nếu không, Ta sẽ bỏ ngươi,",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.957Z",
      "updatedAt": "2024-09-07T14:56:04.957Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "2ce1e1be-3a3f-4b96-af56-a6b2cda11e94",
      "number": 8,
      "content": "Và biến ngươi thành một nơi hoang vu vắng vẻ,",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.957Z",
      "updatedAt": "2024-09-07T14:56:04.957Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "194f483a-aa10-4ff2-84d9-42e8573c4ba7",
      "number": 8,
      "content": "Một đất không người ở.”",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.957Z",
      "updatedAt": "2024-09-07T14:56:04.957Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "edd2cf8c-affc-4de0-999b-58c1128bc60b",
      "number": 9,
      "content": "CHÚA các đạo quân phán,",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.960Z",
      "updatedAt": "2024-09-07T14:56:04.960Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "aa1e1820-37da-40a7-ba5a-a1f55b9a79b1",
      "number": 9,
      "content": "“Hãy như người đi mót nho,",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.960Z",
      "updatedAt": "2024-09-07T14:56:04.960Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "d127756a-b4b9-49cd-8e0f-db4e282cafbe",
      "number": 9,
      "content": "Lặt cho kỹ những gì còn sót lại của I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.960Z",
      "updatedAt": "2024-09-07T14:56:04.960Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "beeb6106-16de-40de-96cf-3356acc5157d",
      "number": 9,
      "content": "Hãy như người đi hái nho,",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.960Z",
      "updatedAt": "2024-09-07T14:56:04.960Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "921ad7d7-6b1a-4d68-81c3-37f68abd8aaa",
      "number": 9,
      "content": "Tuốt bàn tay qua các cành nhỏ của cây nho.”",
      "order": 4,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.960Z",
      "updatedAt": "2024-09-07T14:56:04.960Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "3606f10c-2ea7-4dc8-81be-aaf762dedf4a",
      "number": 10,
      "content": "Tôi sẽ nói với ai, để báo cho biết lời cảnh cáo này,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.965Z",
      "updatedAt": "2024-09-07T14:56:04.965Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "7a1a0da3-54f7-4ad3-99e9-96de301586b6",
      "number": 10,
      "content": "Mà họ chịu nghe đây?",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.965Z",
      "updatedAt": "2024-09-07T14:56:04.965Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "43b74755-461a-454c-8886-3abff3148c14",
      "number": 10,
      "content": "Này, tai họ chưa được cắt bì,",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.965Z",
      "updatedAt": "2024-09-07T14:56:04.965Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "54c3a763-91ac-4ab0-802a-df07b0fe2bf5",
      "number": 10,
      "content": "Nên họ không thể nghe;",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.965Z",
      "updatedAt": "2024-09-07T14:56:04.965Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "20925189-5a08-4939-b1f5-9c71f656ae98",
      "number": 10,
      "content": "Đối với họ lời của CHÚA là những gì đáng hổ thẹn,",
      "order": 4,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.965Z",
      "updatedAt": "2024-09-07T14:56:04.965Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "0987c3cd-aea9-43aa-91f7-d040c598781c",
      "number": 10,
      "content": "Họ chẳng có hứng thú gì đối với lời Ngài.",
      "order": 5,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.965Z",
      "updatedAt": "2024-09-07T14:56:04.965Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "663b80d1-5a10-44c3-aeb2-5e6e2634c2ac",
      "number": 11,
      "content": "Nhưng trong tôi lại đầy tràn cơn thịnh nộ của CHÚA;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.970Z",
      "updatedAt": "2024-09-07T14:56:04.970Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "c815cfee-2c33-444c-92e7-13b06ea10cd6",
      "number": 11,
      "content": "Tôi thật mệt mỏi vì kiềm giữ nó trong lòng.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.970Z",
      "updatedAt": "2024-09-07T14:56:04.970Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "e3709736-6bc8-4d06-89ab-749030aec4b2",
      "number": 11,
      "content": "“Hãy trút xuống trên trẻ con ngoài đường phố,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.970Z",
      "updatedAt": "2024-09-07T14:56:04.970Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "1a4f1305-f913-4717-be03-030138551df2",
      "number": 11,
      "content": "Trên nam nữ thanh niên đang tụ tập với nhau,",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.970Z",
      "updatedAt": "2024-09-07T14:56:04.970Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "0443181d-34d0-4b7c-bf05-4c8bacde1a04",
      "number": 11,
      "content": "Vì cả chồng lẫn vợ sẽ bị bắt đem đi,",
      "order": 4,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.970Z",
      "updatedAt": "2024-09-07T14:56:04.970Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "818f48d2-f29a-4161-93c2-30f66000e5ad",
      "number": 11,
      "content": "Ngay cả người cao niên và người đại thọ cũng không thoát khỏi.",
      "order": 5,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.970Z",
      "updatedAt": "2024-09-07T14:56:04.970Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "8a4476c6-09b9-43c2-b700-d452fba0b54f",
      "number": 12,
      "content": "Nhà của chúng sẽ bị trao cho kẻ khác,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.974Z",
      "updatedAt": "2024-09-07T14:56:04.974Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "27644bdd-feed-4064-995d-108d1e77281b",
      "number": 12,
      "content": "Các ruộng vườn và vợ chúng cũng bị trao luôn;",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.974Z",
      "updatedAt": "2024-09-07T14:56:04.974Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "c4cbed20-b2b8-4ca8-8bc2-5e31514352f1",
      "number": 12,
      "content": "Vì Ta đã đưa tay Ta ra",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.974Z",
      "updatedAt": "2024-09-07T14:56:04.974Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "2613ce5e-8d04-40ea-8316-9788c8c60596",
      "number": 12,
      "content": "Để chống lại dân trong xứ,”",
      "order": 3,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.974Z",
      "updatedAt": "2024-09-07T14:56:04.974Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "f9e803ba-92a6-485f-8931-882858d7590e",
      "number": 12,
      "content": "CHÚA phán,",
      "order": 4,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.974Z",
      "updatedAt": "2024-09-07T14:56:04.974Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "98435823-dae3-4dcc-a5af-8f820699e77d",
      "number": 13,
      "content": "“Vì từ kẻ nhỏ nhất cho đến người lớn nhất trong chúng",
      "order": 0,
      "parNumber": 53,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.979Z",
      "updatedAt": "2024-09-07T14:56:04.979Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "b0bdcdfd-16da-4940-a0b7-de0fe5532855",
      "number": 13,
      "content": "Đều gian tham bất chính,",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.979Z",
      "updatedAt": "2024-09-07T14:56:04.979Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "85b14869-771c-4e98-bc6c-731594dfd0e2",
      "number": 13,
      "content": "Từ các tiên tri cho đến các tư tế",
      "order": 2,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.979Z",
      "updatedAt": "2024-09-07T14:56:04.979Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "9dadc0ed-3fbe-42c4-ab5f-a6817240e10c",
      "number": 13,
      "content": "Đều là phường gian dối như nhau.",
      "order": 3,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.979Z",
      "updatedAt": "2024-09-07T14:56:04.979Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "32136516-5ae4-4fd8-b231-1a52c58a5489",
      "number": 14,
      "content": "Chúng băng bó vết thương của dân Ta cách sơ sài",
      "order": 0,
      "parNumber": 57,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.983Z",
      "updatedAt": "2024-09-07T14:56:04.983Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "f01f2e87-58bc-4970-9b8b-52389458589a",
      "number": 14,
      "content": "Rồi nói, ‘Bình an! Bình an!’",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.983Z",
      "updatedAt": "2024-09-07T14:56:04.983Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "973303fd-2d42-45c1-af36-a528a26ffb8d",
      "number": 14,
      "content": "Mà kỳ thật chẳng có bình an chi cả.",
      "order": 2,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.983Z",
      "updatedAt": "2024-09-07T14:56:04.983Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "1081377d-b7df-43e8-8167-b3bae05fd8a9",
      "number": 15,
      "content": "Việc chúng làm thật đáng tởm biết bao; lẽ ra chúng phải lấy làm xấu hổ mới đúng;",
      "order": 0,
      "parNumber": 60,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.987Z",
      "updatedAt": "2024-09-07T14:56:04.987Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "c0207aa4-5725-4d4a-9450-050d873c294d",
      "number": 15,
      "content": "Nhưng mặt chúng vẫn trơ ra,",
      "order": 1,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.987Z",
      "updatedAt": "2024-09-07T14:56:04.987Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "d0eedefd-c960-4c77-b023-1eb6184944dd",
      "number": 15,
      "content": "Bởi chúng chẳng biết xấu hổ là gì.",
      "order": 2,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.987Z",
      "updatedAt": "2024-09-07T14:56:04.987Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "b155cd56-6507-43ed-9766-8a1fc9e53460",
      "number": 15,
      "content": "Vì thế chúng sẽ ngã gục giữa bao người ngã gục,",
      "order": 3,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.987Z",
      "updatedAt": "2024-09-07T14:56:04.987Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "415b3d5f-532a-48e1-8a44-5643bd652051",
      "number": 15,
      "content": "Trong lúc Ta sửa phạt chúng. chúng sẽ bị ném xuống,”",
      "order": 4,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.987Z",
      "updatedAt": "2024-09-07T14:56:04.987Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "17497407-0ecf-40bb-9cec-1f7891bf3635",
      "number": 15,
      "content": "CHÚA phán.",
      "order": 5,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.987Z",
      "updatedAt": "2024-09-07T14:56:04.987Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "fdcee09b-1613-4064-bd77-0c7ab4820686",
      "number": 16,
      "content": "CHÚA phán thế này,",
      "order": 0,
      "parNumber": 66,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.995Z",
      "updatedAt": "2024-09-07T14:56:04.995Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "9d0f8bf3-af8c-4d38-9dc8-9a959b810772",
      "number": 16,
      "content": "“Hãy đứng nơi các con đường giao nhau,",
      "order": 1,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.995Z",
      "updatedAt": "2024-09-07T14:56:04.995Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "7e833d98-009e-48e0-b020-bc2e8bff0610",
      "number": 16,
      "content": "Hãy hỏi xem các đường lối cũ như thế nào,",
      "order": 2,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.995Z",
      "updatedAt": "2024-09-07T14:56:04.995Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "7df1c814-a25e-4b55-beca-b5b25bf3e8e8",
      "number": 16,
      "content": "Hãy tìm cho được con đường ngay lành thiện hảo, rồi bước vào đường ấy mà đi.",
      "order": 3,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.995Z",
      "updatedAt": "2024-09-07T14:56:04.995Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "75b6885a-1ee8-4432-8eb7-7e8dd2666f5d",
      "number": 16,
      "content": "Bấy giờ các ngươi sẽ tìm được bình an thư thái cho linh hồn mình.",
      "order": 4,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.995Z",
      "updatedAt": "2024-09-07T14:56:04.995Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "05791566-bd43-43db-9559-65be879440c3",
      "number": 16,
      "content": "Nhưng chúng nói, ‘Chúng tôi không muốn đi con đường ấy.’",
      "order": 5,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:04.995Z",
      "updatedAt": "2024-09-07T14:56:04.995Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "74ce6090-e2bd-4d2b-94c7-5a574814b1c1",
      "number": 17,
      "content": "Ta cũng đã đặt những người canh gác để cảnh báo các ngươi,",
      "order": 0,
      "parNumber": 72,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.002Z",
      "updatedAt": "2024-09-07T14:56:05.002Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "f9017892-a77e-43de-9cc8-d2c4d0dc0d0e",
      "number": 17,
      "content": "‘Hãy để ý mà nghe khi kèn báo động.’",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.002Z",
      "updatedAt": "2024-09-07T14:56:05.002Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "e40d9bae-9854-41f2-b35e-8d7af7fe1824",
      "number": 17,
      "content": "Nhưng chúng bảo, ‘Chúng tôi không muốn để ý nghe đâu.’",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.002Z",
      "updatedAt": "2024-09-07T14:56:05.002Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "adf7a356-03a9-4f36-b80f-62d5b6f687b8",
      "number": 18,
      "content": "Vì thế hỡi các dân, hãy nghe,",
      "order": 0,
      "parNumber": 75,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.006Z",
      "updatedAt": "2024-09-07T14:56:05.006Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "446e5f97-bde7-409c-b1e8-a2a43e551259",
      "number": 18,
      "content": "Hỡi các nước, hãy biết rõ những gì sẽ xảy ra cho chúng.",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.006Z",
      "updatedAt": "2024-09-07T14:56:05.006Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "206bbcfb-aaae-4ce7-be59-93a1336059b7",
      "number": 19,
      "content": "Hỡi trái đất, hãy nghe! Này, Ta sẽ đem tai họa đến trên dân này,",
      "order": 0,
      "parNumber": 77,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.011Z",
      "updatedAt": "2024-09-07T14:56:05.011Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "77d26114-b2dd-4400-a6cc-6f51052f4f27",
      "number": 19,
      "content": "Đó là hậu quả do quyết tâm của chúng,",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.011Z",
      "updatedAt": "2024-09-07T14:56:05.011Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "9c60e8da-f5d9-41cf-b79a-c0a1a7f683e4",
      "number": 19,
      "content": "Vì chúng đã không nghe theo lời Ta,",
      "order": 2,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.011Z",
      "updatedAt": "2024-09-07T14:56:05.011Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "d44a7e3f-1828-41dd-8d5f-4cd5c4c42dc4",
      "number": 19,
      "content": "Chúng đã khước từ luật pháp Ta.",
      "order": 3,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.011Z",
      "updatedAt": "2024-09-07T14:56:05.011Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "7f887627-c6dc-4a04-94ed-c217a0ee25a6",
      "number": 20,
      "content": "Có ích chi nhũ hương từ Sê-ba đem đến?",
      "order": 0,
      "parNumber": 81,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.016Z",
      "updatedAt": "2024-09-07T14:56:05.016Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "66047888-72e1-4516-8d10-e9db749ba4d4",
      "number": 20,
      "content": "Có ích chi gỗ thơm từ viễn xứ mang về?",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.016Z",
      "updatedAt": "2024-09-07T14:56:05.016Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "a125bf02-3fe1-4d75-971b-0817dea7fb51",
      "number": 20,
      "content": "Của lễ thiêu các ngươi dâng, Ta không đoái nhậm,",
      "order": 2,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.016Z",
      "updatedAt": "2024-09-07T14:56:05.016Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "3af8f21a-a1b4-40b3-bcbe-12019a1866c2",
      "number": 20,
      "content": "Những sinh tế của các ngươi, Ta cũng chẳng vui lòng.”",
      "order": 3,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.016Z",
      "updatedAt": "2024-09-07T14:56:05.016Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "f4ad70df-58f0-4c72-b367-a7fb40b6fcd3",
      "number": 21,
      "content": "Vì vậy CHÚA phán thế này,",
      "order": 0,
      "parNumber": 85,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.020Z",
      "updatedAt": "2024-09-07T14:56:05.020Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "89ac748f-89a7-4711-ab77-7d913a1d5c0c",
      "number": 21,
      "content": "“Này, Ta đặt trước dân này các chướng ngại vật",
      "order": 1,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.020Z",
      "updatedAt": "2024-09-07T14:56:05.020Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "38fdf9da-953d-4af6-80b4-4ac9b2a9d3d0",
      "number": 21,
      "content": "Để chúng sẽ vấp ngã;",
      "order": 2,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.020Z",
      "updatedAt": "2024-09-07T14:56:05.020Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "69d036bc-c9b7-4fde-ac89-8e9d8360e883",
      "number": 21,
      "content": "Để cha mẹ và con cái,",
      "order": 3,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.020Z",
      "updatedAt": "2024-09-07T14:56:05.020Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "a8229f36-3430-4890-996f-f6f85000757e",
      "number": 21,
      "content": "Láng giềng và bạn hữu đều cùng nhau chết mất.”",
      "order": 4,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.020Z",
      "updatedAt": "2024-09-07T14:56:05.020Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "801fc71c-8f34-4c7e-9770-515fd7f97a8d",
      "number": 22,
      "content": "CHÚA phán thế này,",
      "order": 0,
      "parNumber": 90,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.026Z",
      "updatedAt": "2024-09-07T14:56:05.026Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "f6ffe3fe-c25b-45c4-8b2f-da18c9f1ceff",
      "number": 22,
      "content": "“Này, một dân từ phương bắc sẽ đến,",
      "order": 1,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.026Z",
      "updatedAt": "2024-09-07T14:56:05.026Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "8566b795-ecce-49a3-8ddb-8a645f8c4ed0",
      "number": 22,
      "content": "Một cường quốc từ một xứ xa xăm đang huy động binh hùng.",
      "order": 2,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.026Z",
      "updatedAt": "2024-09-07T14:56:05.026Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "59709002-2638-42f8-a722-ac789355adeb",
      "number": 23,
      "content": "Chúng nắm lấy cung tên và lao giáo trong tay;",
      "order": 0,
      "parNumber": 93,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.031Z",
      "updatedAt": "2024-09-07T14:56:05.031Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "49f4e7ef-5df5-4ac1-9998-8bc8536c8fee",
      "number": 23,
      "content": "Chúng bạo tàn và không xót thương ai cả;",
      "order": 1,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.031Z",
      "updatedAt": "2024-09-07T14:56:05.031Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "dd6ed761-fcaa-47c6-9cc4-78545ac589d1",
      "number": 23,
      "content": "Tiếng đoàn quân chúng đi vang động như sóng biển thét gào;",
      "order": 2,
      "parNumber": 95,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.031Z",
      "updatedAt": "2024-09-07T14:56:05.031Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "9f214001-72f5-4ae7-b5e4-97d8e09cbba1",
      "number": 23,
      "content": "Chúng cỡi trên các chiến mã",
      "order": 3,
      "parNumber": 96,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.031Z",
      "updatedAt": "2024-09-07T14:56:05.031Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "337316f6-3313-4c0e-9ee5-f7192472a324",
      "number": 23,
      "content": "Theo đội ngũ xông vào tấn công ngươi,",
      "order": 4,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.031Z",
      "updatedAt": "2024-09-07T14:56:05.031Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "de85a9e9-a17a-49a1-8745-7d809036ff39",
      "number": 23,
      "content": "Hỡi Ái Nữ của Si-ôn!”",
      "order": 5,
      "parNumber": 98,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.031Z",
      "updatedAt": "2024-09-07T14:56:05.031Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "4a371d83-d27f-455b-bd21-f6636108b7a6",
      "number": 24,
      "content": "Nghe tin đó",
      "order": 0,
      "parNumber": 99,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.036Z",
      "updatedAt": "2024-09-07T14:56:05.036Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "feef2a3b-e66c-439f-9917-b194a85972ce",
      "number": 24,
      "content": "Tay chân chúng tôi bủn rủn;",
      "order": 1,
      "parNumber": 100,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.036Z",
      "updatedAt": "2024-09-07T14:56:05.036Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "23b84039-4780-4b3a-85d7-26810555ad2a",
      "number": 24,
      "content": "Nỗi kinh hoàng đã bắt lấy chúng tôi,",
      "order": 2,
      "parNumber": 101,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.036Z",
      "updatedAt": "2024-09-07T14:56:05.036Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "3a6eb78f-3c39-42e2-bf74-e21a3f88e415",
      "number": 24,
      "content": "Chúng tôi đau đớn như sản phụ lúc lâm bồn.",
      "order": 3,
      "parNumber": 102,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.036Z",
      "updatedAt": "2024-09-07T14:56:05.036Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "4a18af2e-6299-43ac-88ab-74068331652e",
      "number": 25,
      "content": "Chớ ra ruộng",
      "order": 0,
      "parNumber": 103,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.042Z",
      "updatedAt": "2024-09-07T14:56:05.042Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "6a501259-9125-49cf-a63e-567a120f2ba0",
      "number": 25,
      "content": "Hay đi đâu ngoài đường,",
      "order": 1,
      "parNumber": 104,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.042Z",
      "updatedAt": "2024-09-07T14:56:05.042Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "aef00769-985a-4cd7-a588-3490103a65e0",
      "number": 25,
      "content": "Vì quân thù sẽ dùng gươm giết chết,",
      "order": 2,
      "parNumber": 105,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.042Z",
      "updatedAt": "2024-09-07T14:56:05.042Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "674f89b8-2863-4bae-91e8-cad4b3156e4f",
      "number": 25,
      "content": "Gieo kinh hoàng khiếp sợ khắp nơi.",
      "order": 3,
      "parNumber": 106,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.042Z",
      "updatedAt": "2024-09-07T14:56:05.042Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "d5278613-2026-4845-a564-d41f203d50fa",
      "number": 26,
      "content": "Hỡi ái nữ của dân tôi,",
      "order": 0,
      "parNumber": 107,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.047Z",
      "updatedAt": "2024-09-07T14:56:05.047Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "0daaaf1d-f6c7-4f52-9959-66ec821bfd71",
      "number": 26,
      "content": "Hãy mặc lấy vải tang và lăn mình trong tro bụi;",
      "order": 1,
      "parNumber": 108,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.047Z",
      "updatedAt": "2024-09-07T14:56:05.047Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "3d643e27-b7d2-4fd4-aa3b-f62fd0a2fa00",
      "number": 26,
      "content": "Hãy khóc than như vừa mất con một của mình;",
      "order": 2,
      "parNumber": 109,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.047Z",
      "updatedAt": "2024-09-07T14:56:05.047Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "980ae5d1-2d48-4c42-9de9-2529101fd438",
      "number": 26,
      "content": "Hãy khóc lóc cách đắng cay,",
      "order": 3,
      "parNumber": 110,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.047Z",
      "updatedAt": "2024-09-07T14:56:05.047Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "1ee8edf4-7bc2-4af0-bb55-abcf19710b45",
      "number": 26,
      "content": "Vì quân tàn sát chúng ta",
      "order": 4,
      "parNumber": 111,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.047Z",
      "updatedAt": "2024-09-07T14:56:05.047Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "f9d0f68f-542a-4454-bad3-399dba7a96b0",
      "number": 26,
      "content": "Sẽ ập đến thình lình.",
      "order": 5,
      "parNumber": 112,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.047Z",
      "updatedAt": "2024-09-07T14:56:05.047Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "6b2cc25f-520f-414b-941f-ccebba6b447d",
      "number": 27,
      "content": "“Ta đã lập ngươi làm người thử luyện và người kiểm phẩm chất đạo đức giữa dân Ta,",
      "order": 0,
      "parNumber": 113,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.052Z",
      "updatedAt": "2024-09-07T14:56:05.052Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "ee9a820b-529b-41c3-acf6-58cdc3131d41",
      "number": 27,
      "content": "Để ngươi biết rõ và thẩm định đường lối của chúng.",
      "order": 1,
      "parNumber": 114,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.052Z",
      "updatedAt": "2024-09-07T14:56:05.052Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "3edb882e-fbdf-4283-b606-51c6947873ac",
      "number": 28,
      "content": "Tất cả chúng đều là phường ương ngạnh và phản loạn;",
      "order": 0,
      "parNumber": 115,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.057Z",
      "updatedAt": "2024-09-07T14:56:05.057Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "4702b748-0ec5-4d4f-bc5e-04dcd3b7997b",
      "number": 28,
      "content": "Đi đến đâu chúng vu khống đến đó;",
      "order": 1,
      "parNumber": 116,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.057Z",
      "updatedAt": "2024-09-07T14:56:05.057Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "488d168f-82f3-4136-9b64-1b5f09ba708e",
      "number": 28,
      "content": "Mặt chúng trơ như đồng và cứng như sắt;",
      "order": 2,
      "parNumber": 117,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.057Z",
      "updatedAt": "2024-09-07T14:56:05.057Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "9fa4110c-2cde-4c48-bfcf-fa58c87cc39c",
      "number": 28,
      "content": "Tất cả chúng đều băng hoại.",
      "order": 3,
      "parNumber": 118,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.057Z",
      "updatedAt": "2024-09-07T14:56:05.057Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "315ea3d1-52aa-4293-9d01-1bbc20a1aee0",
      "number": 29,
      "content": "Các ống bể thổi ào ào,",
      "order": 0,
      "parNumber": 119,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.062Z",
      "updatedAt": "2024-09-07T14:56:05.062Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "058e1b6f-1be2-4f8b-943b-75eefd6498b0",
      "number": 29,
      "content": "Chì bị chảy tan trong lửa hực;",
      "order": 1,
      "parNumber": 120,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.062Z",
      "updatedAt": "2024-09-07T14:56:05.062Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "6db82302-1317-4f4b-9352-7b49cc5d7d8a",
      "number": 29,
      "content": "Nhưng dân Ta, dù được luyện đến đâu cũng vô ích,",
      "order": 2,
      "parNumber": 121,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.062Z",
      "updatedAt": "2024-09-07T14:56:05.062Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "6f667105-145c-4ea8-bce8-5fc052fa4c78",
      "number": 29,
      "content": "Vì cặn bã vẫn không chịu tách rời khỏi tinh chất.",
      "order": 3,
      "parNumber": 122,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.062Z",
      "updatedAt": "2024-09-07T14:56:05.062Z",
      "chapterId": "8fc902cb-ef92-445b-98b2-76efa3ca4397"
    },
    {
      "id": "5f0577fd-f8ca-4337-afdc-308645abbc3b",
      "number": 30,
      "content": "Vì thế người ta đã gọi chúng là ‘Bạc bị phế thải,’",
      "order": 0,
      "parNumber": 123,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:56:05.067Z",
      "updatedAt": "2024-09-07T14:56:05.067Z",
