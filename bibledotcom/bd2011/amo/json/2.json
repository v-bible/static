  },
  "verses": [
    {
      "id": "0808decf-0227-4d3c-b4fa-a03421e6d8bd",
      "number": 1,
      "content": "**CHÚA** phán thế này,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T06:10:05.270Z",
      "updatedAt": "2024-09-23T06:10:05.270Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "d446d9be-648a-48cb-afdd-c0d841427be3",
      "number": 4,
      "content": "**CHÚA** phán thế này,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T06:10:05.340Z",
      "updatedAt": "2024-09-23T06:10:05.340Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "6776a5f2-64d8-4742-b693-a6fbe0a9a266",
      "number": 6,
      "content": "**CHÚA** phán thế này,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T06:10:05.390Z",
      "updatedAt": "2024-09-23T06:10:05.390Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "9af81455-2a5c-4685-bee9-1ce4b3e43ba6",
      "number": 1,
      "content": "“Vì tội của Mô-áp nặng gấp ba gấp bốn lần,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.286Z",
      "updatedAt": "2024-09-23T06:10:05.286Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "d9876a18-761f-4da0-bd9e-f820a2c35ff6",
      "number": 6,
      "content": "Nên Ta sẽ chẳng quay mặt đi mà không phạt nó.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.404Z",
      "updatedAt": "2024-09-23T06:10:05.404Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "beb71544-58d3-4303-8cd7-e3d6bf9b786a",
      "number": 1,
      "content": "Nên Ta sẽ chẳng quay mặt đi mà không phạt nó.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.291Z",
      "updatedAt": "2024-09-23T06:10:05.291Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "e52f5b42-57f7-4ed3-b485-04ce668cfe97",
      "number": 4,
      "content": "Nên Ta sẽ chẳng quay mặt đi mà không phạt nó.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.354Z",
      "updatedAt": "2024-09-23T06:10:05.354Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "21f136bf-29f1-4b3c-9253-5ce8a1a3c3f7",
      "number": 1,
      "content": "Vì nó đã đốt hài cốt vua Ê-đôm",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.296Z",
      "updatedAt": "2024-09-23T06:10:05.296Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "26cef776-79a5-4bda-89a1-606a5433668e",
      "number": 1,
      "content": "Ra tro bụi,",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.301Z",
      "updatedAt": "2024-09-23T06:10:05.301Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "135b7c0c-ac2b-44bc-b3af-c0de8a527242",
      "number": 2,
      "content": "Nên Ta sẽ sai một ngọn lửa đến trên Mô-áp;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.305Z",
      "updatedAt": "2024-09-23T06:10:05.305Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "0d09a4a0-1c4c-470f-bbd5-61092c5487d8",
      "number": 2,
      "content": "Ngọn lửa ấy sẽ thiêu rụi các lâu đài của Kê-ri-ốt.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.310Z",
      "updatedAt": "2024-09-23T06:10:05.310Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "93a1d8b5-769c-4de7-bb02-d6497628e97a",
      "number": 2,
      "content": "Mô-áp sẽ bị diệt vong trong cảnh hỗn loạn,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.315Z",
      "updatedAt": "2024-09-23T06:10:05.315Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "a7582c3d-cfa5-406b-bbd5-e86853b35cec",
      "number": 2,
      "content": "Giữa những tiếng gào thét giao tranh và những tiếng kèn xung trận.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.320Z",
      "updatedAt": "2024-09-23T06:10:05.320Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "ad7156c3-f421-4506-bf98-531900246950",
      "number": 3,
      "content": "Ta sẽ tiêu diệt kẻ cầm quyền phán xét khỏi nó,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.325Z",
      "updatedAt": "2024-09-23T06:10:05.325Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "bde904e2-98de-4ca2-bb09-77d5d1051c95",
      "number": 3,
      "content": "Và diệt trừ tất cả các kẻ có quyền hành chung với kẻ ấy,”",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.330Z",
      "updatedAt": "2024-09-23T06:10:05.330Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "21314f80-aa10-4526-98cf-d529a45addbe",
      "number": 3,
      "content": "**CHÚA** phán.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.335Z",
      "updatedAt": "2024-09-23T06:10:05.335Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "fa665bd1-a1a6-45dd-91d6-8d10b6cb9ba6",
      "number": 16,
      "content": "**CHÚA** phán.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.582Z",
      "updatedAt": "2024-09-23T06:10:05.582Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "4b47d22a-f114-4ef9-88d2-c2471643310d",
      "number": 11,
      "content": "**CHÚA** phán.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.513Z",
      "updatedAt": "2024-09-23T06:10:05.513Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "e4ffcef6-7870-438d-a3dd-2468e6aa037c",
      "number": 4,
      "content": "“Vì tội của Giu-đa nặng gấp ba gấp bốn lần,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.349Z",
      "updatedAt": "2024-09-23T06:10:05.349Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "2bf256d4-ff83-48f8-9c80-4dde43fc43cb",
      "number": 4,
      "content": "Vì chúng đã loại bỏ Luật Pháp của **CHÚA**",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.360Z",
      "updatedAt": "2024-09-23T06:10:05.360Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "2ac99d39-e3ea-4229-a6ea-e054bb0ca853",
      "number": 4,
      "content": "Và không vâng giữ các luật lệ Ngài,",
      "order": 4,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.365Z",
      "updatedAt": "2024-09-23T06:10:05.365Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "ccd2b2d1-1a1a-4afd-af86-3cfb351d433d",
      "number": 4,
      "content": "Chúng đã để các thần tượng giả dối lừa gạt chúng đi sai lạc,",
      "order": 5,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.369Z",
      "updatedAt": "2024-09-23T06:10:05.369Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "f8926d43-d1e1-4acf-8904-825b01791917",
      "number": 4,
      "content": "Đó là các thần tượng giả dối mà tổ tiên chúng đã tin thờ,",
      "order": 6,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.375Z",
      "updatedAt": "2024-09-23T06:10:05.375Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "2eced7cc-cc08-484f-98a0-4d4d57a9a691",
      "number": 5,
      "content": "Nên Ta sẽ sai một ngọn lửa đến trên Giu-đa;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.380Z",
      "updatedAt": "2024-09-23T06:10:05.380Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "5d128f93-c8cb-4231-8ec4-059dd5bfe499",
      "number": 5,
      "content": "Ngọn lửa ấy sẽ thiêu rụi các lâu đài của Giê-ru-sa-lem.”",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.385Z",
      "updatedAt": "2024-09-23T06:10:05.385Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "2b52c3e8-ce75-4c4e-8e9f-eb41d52cb1e7",
      "number": 6,
      "content": "“Vì tội của I-sơ-ra-ên nặng gấp ba gấp bốn lần,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.400Z",
      "updatedAt": "2024-09-23T06:10:05.400Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "6b0d469c-b47a-497b-a996-113b6ce691f4",
      "number": 6,
      "content": "Vì chúng đã bán đứng người công chính để lấy bạc,",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.409Z",
      "updatedAt": "2024-09-23T06:10:05.409Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "92a0b847-380f-4efd-901c-9100cb6dee8c",
      "number": 6,
      "content": "Chúng đã bán người nghèo để lấy một đôi giày,",
      "order": 4,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.413Z",
      "updatedAt": "2024-09-23T06:10:05.413Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "47fe8553-bb3d-4091-b740-48e9c11aee99",
      "number": 7,
      "content": "Chúng đã đạp đầu người nghèo vào bụi đất,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.418Z",
      "updatedAt": "2024-09-23T06:10:05.418Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "a8523c52-6a62-47c7-bf80-6d0d55263b65",
      "number": 7,
      "content": "Chúng đã cướp công lý của người bị áp bức,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.423Z",
      "updatedAt": "2024-09-23T06:10:05.423Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "5a92d3fe-6059-44cd-ae38-7ad441b63ad7",
      "number": 7,
      "content": "Tệ hơn nữa, hai cha con ăn nằm với một người phụ nữ,",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.428Z",
      "updatedAt": "2024-09-23T06:10:05.428Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "c5badd42-fe28-4c26-a079-060e1ff6a3a6",
      "number": 7,
      "content": "Khiến cho danh thánh của Ta bị ô uế giữa các dân.",
      "order": 3,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.433Z",
      "updatedAt": "2024-09-23T06:10:05.433Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "a34ab4b2-a25a-4e5a-8eac-fe9a657c2f19",
      "number": 8,
      "content": "Chúng mặc những y phục của người ta cầm thế",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.438Z",
      "updatedAt": "2024-09-23T06:10:05.438Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "e2777329-e90a-499e-a2f7-b09754528fb5",
      "number": 8,
      "content": "Mà phủ phục trước bàn thờ các thần tượng của chúng;",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.443Z",
      "updatedAt": "2024-09-23T06:10:05.443Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "7816d214-c2e6-432f-9830-850695c7021c",
      "number": 8,
      "content": "Trong đền thờ các thần tượng của chúng,",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.448Z",
      "updatedAt": "2024-09-23T06:10:05.448Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "c4d24d75-5665-4009-9a52-b9f3dcbe95a2",
      "number": 8,
      "content": "Chúng dâng rượu tế mua bằng tiền lừa gạt người ta.",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.453Z",
      "updatedAt": "2024-09-23T06:10:05.453Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "ee36c3e1-4f04-4586-8f68-74fd49f81c3c",
      "number": 9,
      "content": "Dù vậy Ta đã phù hộ chúng bằng cách tiêu diệt dân A-mô-ri trước mặt chúng,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.458Z",
      "updatedAt": "2024-09-23T06:10:05.458Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "97074167-ee7e-448f-8600-9ef1e7857902",
      "number": 9,
      "content": "Đó là dân cao lớn như cây bá hương",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.463Z",
      "updatedAt": "2024-09-23T06:10:05.463Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "9ca9015d-5360-4fbd-8626-a79eda28ff8b",
      "number": 9,
      "content": "Và mạnh mẽ như cây sồi;",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.468Z",
      "updatedAt": "2024-09-23T06:10:05.468Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "64b753d0-7fe3-4236-930f-c2ba3f50d971",
      "number": 9,
      "content": "Trên mặt đất, Ta đã diệt trừ mọi trái;",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.472Z",
      "updatedAt": "2024-09-23T06:10:05.472Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "a1d66dd5-2125-4f9f-b972-6af98ae3b79f",
      "number": 9,
      "content": "Dưới mặt đất, Ta đã không cho một rễ nào được sống còn.",
      "order": 4,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.477Z",
      "updatedAt": "2024-09-23T06:10:05.477Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "ceda4e08-31ef-49be-a206-67122b5e19c5",
      "number": 10,
      "content": "Ta đã đem các ngươi lên khỏi đất Ai-cập,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.482Z",
      "updatedAt": "2024-09-23T06:10:05.482Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "5ec29a2d-25bd-4e32-b7e3-7622b31bde6b",
      "number": 10,
      "content": "Ta đã dẫn dắt các ngươi bốn mươi năm trong đồng hoang",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.487Z",
      "updatedAt": "2024-09-23T06:10:05.487Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "de1d674f-e4f7-438d-bcf4-32f8c6dcbcc7",
      "number": 10,
      "content": "Và ban cho các ngươi xứ của dân A-mô-ri.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.492Z",
      "updatedAt": "2024-09-23T06:10:05.492Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "581b7dae-9475-4823-a4c0-a21db7e92eb9",
      "number": 11,
      "content": "Ta đã nuôi dưỡng một số con trai các ngươi thành các tiên tri,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.497Z",
      "updatedAt": "2024-09-23T06:10:05.497Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "31bc1299-0f75-4ad4-a3ce-80106af0be27",
      "number": 11,
      "content": "Một số người trẻ của các ngươi thành những người Na-xi-rê;",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.502Z",
      "updatedAt": "2024-09-23T06:10:05.502Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "b8281d1c-5f24-4c67-b715-99ae79c80bd2",
      "number": 11,
      "content": "Hỡi dân I-sơ-ra-ên, có đúng như thế không?”",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.507Z",
      "updatedAt": "2024-09-23T06:10:05.507Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "f70fd22b-7bef-4426-89fe-704755e399b4",
      "number": 12,
      "content": "“Thế nhưng các ngươi đã bắt những người Na-xi-rê uống rượu,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.518Z",
      "updatedAt": "2024-09-23T06:10:05.518Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "1c45454d-999a-46f5-b53d-fe6fc3569e8d",
      "number": 12,
      "content": "Và đã ra lệnh cho các tiên tri rằng",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.523Z",
      "updatedAt": "2024-09-23T06:10:05.523Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "3a418aa8-d7a7-416d-8c31-6750bd3a168c",
      "number": 12,
      "content": "‘Các ông không được nói tiên tri.’",
      "order": 2,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.528Z",
      "updatedAt": "2024-09-23T06:10:05.528Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "e72741a7-6197-4829-ae13-aa72aa5a5664",
      "number": 13,
      "content": "Này, Ta sẽ ép các ngươi",
      "order": 0,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.533Z",
      "updatedAt": "2024-09-23T06:10:05.533Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "caac2e04-ebd1-4bc9-a11c-21b909bb5818",
      "number": 13,
      "content": "Như chiếc xe chở đầy những bó lúa cán xuống mặt đường.",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.538Z",
      "updatedAt": "2024-09-23T06:10:05.538Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "ade3f42f-cb34-42db-bee4-b181201fbb05",
      "number": 14,
      "content": "Do đó người nhanh lẹ sẽ không thoát chết,",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.543Z",
      "updatedAt": "2024-09-23T06:10:05.543Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "ff4c4ff8-bdd1-466f-bd0d-2de41175d546",
      "number": 14,
      "content": "Người mạnh mẽ sẽ không duy trì được sức lực,",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.548Z",
      "updatedAt": "2024-09-23T06:10:05.548Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "f819a332-8c12-4960-896d-36365f7fb23a",
      "number": 14,
      "content": "Kẻ dũng mãnh sẽ không cứu được mạng sống mình,",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.553Z",
      "updatedAt": "2024-09-23T06:10:05.553Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "e325662d-e769-4751-95a7-12c6b8ffef0a",
      "number": 15,
      "content": "Quân xạ tiễn sẽ không trụ được tại chỗ,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.558Z",
      "updatedAt": "2024-09-23T06:10:05.558Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "c425d156-16f4-415f-8a06-42560a1b4eb8",
      "number": 15,
      "content": "Các chiến sĩ chạy nhanh nhất cũng không chạy thoát,",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.563Z",
      "updatedAt": "2024-09-23T06:10:05.563Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "9c069494-66b4-4cd0-a196-3a56b68229d4",
      "number": 15,
      "content": "Ngay cả những kỵ binh cỡi các chiến mã cũng không chạy thoát kịp để giữ mạng mình,",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.568Z",
      "updatedAt": "2024-09-23T06:10:05.568Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "b620f6d9-03da-4045-944a-0e98755018e5",
      "number": 16,
      "content": "Thậm chí kẻ can đảm nhất giữa vòng các dũng sĩ",
      "order": 0,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.572Z",
      "updatedAt": "2024-09-23T06:10:05.572Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    },
    {
      "id": "6275fe27-1602-4b9c-a238-8d5ee73d1630",
      "number": 16,
      "content": "Sẽ chạy trốn trần truồng trong ngày ấy,”",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:05.577Z",
      "updatedAt": "2024-09-23T06:10:05.577Z",
      "chapterId": "d7edffa2-c39f-4d81-9cea-787fa0da6d15"
    }
  ],
