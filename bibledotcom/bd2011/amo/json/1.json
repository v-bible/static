    "bookId": "1bc16804-38a8-4a17-8657-5ce6c9b92ce6"
  },
  "verses": [
    {
      "id": "c2483499-2d44-4977-ad32-75d0bab2b858",
      "number": 1,
      "content": "Các sứ điệp của A-mốt, một trong những người chăn bầy ở Tê-cô-a, mà ông đã thấy liên quan đến I-sơ-ra-ên trong thời của U-xi-a vua Giu-đa và trong thời của Giê-rô-bô-am con trai Giô-ách vua I-sơ-ra-ên, hai năm trước khi động đất.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T06:10:02.167Z",
      "updatedAt": "2024-09-23T06:10:02.167Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "6f245ce4-c81a-4bf4-8803-df3e18800245",
      "number": 2,
      "content": "Ông nói,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T06:10:02.183Z",
      "updatedAt": "2024-09-23T06:10:02.183Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "d625499b-63e3-49bd-a9bd-424ed797acf2",
      "number": 2,
      "content": "“**CHÚA** gầm thét từ Si-ôn",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.201Z",
      "updatedAt": "2024-09-23T06:10:02.201Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "e65cf796-3eaa-4c11-9276-103561f6e11c",
      "number": 2,
      "content": "Và cất tiếng từ Giê-ru-sa-lem;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.205Z",
      "updatedAt": "2024-09-23T06:10:02.205Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "786873e3-1219-4895-be20-740ae2bca85e",
      "number": 2,
      "content": "Những người chăn than thở vì các đồng cỏ héo khô,",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.210Z",
      "updatedAt": "2024-09-23T06:10:02.210Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "832b0bfa-1269-4bca-83b9-f31c7041f452",
      "number": 2,
      "content": "Ngay cả trên đỉnh Núi Cạt-mên cũng bị khô cằn.”",
      "order": 4,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.215Z",
      "updatedAt": "2024-09-23T06:10:02.215Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "bbcbae1b-8d99-408a-a55b-090727a37f43",
      "number": 11,
      "content": "**CHÚA** phán thế này,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T06:10:02.384Z",
      "updatedAt": "2024-09-23T06:10:02.384Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "3f3794ab-712f-4772-b0bb-291baca58e6d",
      "number": 3,
      "content": "**CHÚA** phán thế này,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T06:10:02.219Z",
      "updatedAt": "2024-09-23T06:10:02.219Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "f398e725-fa41-4a74-8609-20bc733eb2f9",
      "number": 6,
      "content": "**CHÚA** phán thế này,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T06:10:02.285Z",
      "updatedAt": "2024-09-23T06:10:02.285Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "0ac74627-9364-464d-b83e-339b6e40d093",
      "number": 9,
      "content": "**CHÚA** phán thế này,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T06:10:02.348Z",
      "updatedAt": "2024-09-23T06:10:02.348Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "c16f9d09-a0a8-44e1-b185-48898b311fcb",
      "number": 13,
      "content": "**CHÚA** phán thế này,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-23T06:10:02.432Z",
      "updatedAt": "2024-09-23T06:10:02.432Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "ee965dee-8bd8-4a02-8b7b-c8289e88409a",
      "number": 3,
      "content": "“Vì tội của Đa-mách nặng gấp ba gấp bốn lần,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.229Z",
      "updatedAt": "2024-09-23T06:10:02.229Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "37311b55-dceb-4382-a651-2af040ed476c",
      "number": 13,
      "content": "Nên Ta sẽ chẳng quay mặt đi mà không phạt nó.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.445Z",
      "updatedAt": "2024-09-23T06:10:02.445Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "d1db4861-737f-41d8-99d9-20e01a93830c",
      "number": 11,
      "content": "Nên Ta sẽ chẳng quay mặt đi mà không phạt nó.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.397Z",
      "updatedAt": "2024-09-23T06:10:02.397Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "ff5b6129-d32c-4777-ac6f-342f1965adb0",
      "number": 6,
      "content": "Nên Ta sẽ chẳng quay mặt đi mà không phạt nó.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.298Z",
      "updatedAt": "2024-09-23T06:10:02.298Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "0364702e-0f08-4f0a-991f-7da75c61d5cf",
      "number": 3,
      "content": "Nên Ta sẽ chẳng quay mặt đi mà không phạt nó.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.233Z",
      "updatedAt": "2024-09-23T06:10:02.233Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "6ebf2744-9418-437b-a84b-fcc378d1d6e4",
      "number": 9,
      "content": "Nên Ta sẽ chẳng quay mặt đi mà không phạt nó.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.361Z",
      "updatedAt": "2024-09-23T06:10:02.361Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "3f6a02a0-1d38-4349-a2bf-c270b89670f4",
      "number": 3,
      "content": "Vì chúng đã đập nát Ghi-lê-át",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.241Z",
      "updatedAt": "2024-09-23T06:10:02.241Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "5160ae7d-2203-4713-b376-7eb3654481e2",
      "number": 3,
      "content": "Bằng dụng cụ bằng sắt,",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.248Z",
      "updatedAt": "2024-09-23T06:10:02.248Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "54ad4be8-c9a3-46ff-a493-0f3df541e41e",
      "number": 4,
      "content": "Nên Ta sẽ sai một ngọn lửa đến nhà Ha-xa-ên;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.253Z",
      "updatedAt": "2024-09-23T06:10:02.253Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "39bb4181-d350-4adb-945b-44740dcd8c8d",
      "number": 4,
      "content": "Ngọn lửa ấy sẽ thiêu rụi các lâu đài của Bên Ha-đát.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.258Z",
      "updatedAt": "2024-09-23T06:10:02.258Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "ac6e9333-6472-45b3-9f0c-4ea0a9fbd125",
      "number": 5,
      "content": "Ta sẽ bẻ gãy các thanh cài cổng Thành Đa-mách;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.262Z",
      "updatedAt": "2024-09-23T06:10:02.262Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "d3764b8f-7dc3-401d-b2d3-3d03833953bc",
      "number": 5,
      "content": "Ta sẽ tiêu diệt dân cư ở Thung Lũng A-ven,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.267Z",
      "updatedAt": "2024-09-23T06:10:02.267Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "517b65be-17ff-4c8d-8f8c-6fa527a3099b",
      "number": 5,
      "content": "Và diệt trừ kẻ cầm vương trượng của Bết Ê-đen.",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.272Z",
      "updatedAt": "2024-09-23T06:10:02.272Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "f6aa0b10-8a83-47f6-94f9-2b82d4036d6d",
      "number": 5,
      "content": "Dân chúng của A-ram sẽ bị đem lưu đày tại Ki-rơ,”",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.276Z",
      "updatedAt": "2024-09-23T06:10:02.276Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "fd237e6d-ff21-4f93-81cd-782f8431a86f",
      "number": 15,
      "content": "**CHÚA** phán.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.489Z",
      "updatedAt": "2024-09-23T06:10:02.489Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "30471377-d3d7-4717-b7dc-3b15a085052c",
      "number": 5,
      "content": "**CHÚA** phán.",
      "order": 4,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.281Z",
      "updatedAt": "2024-09-23T06:10:02.281Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "ef3a9383-5c39-43c0-9de7-252fd9402149",
      "number": 6,
      "content": "“Vì tội của Ga-xa nặng gấp ba gấp bốn lần,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.294Z",
      "updatedAt": "2024-09-23T06:10:02.294Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "f72c90c5-cb4b-4f50-a456-b02a045b9d26",
      "number": 6,
      "content": "Vì nó đã bắt dân cả vùng đem lưu đày",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.303Z",
      "updatedAt": "2024-09-23T06:10:02.303Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "072b5c4b-3018-420b-8819-4c926eaa0326",
      "number": 6,
      "content": "Và trao nộp chúng cho Ê-đôm,",
      "order": 4,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.308Z",
      "updatedAt": "2024-09-23T06:10:02.308Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "bbe4da7e-1d4d-4095-b8c8-1ce9d6c772d5",
      "number": 7,
      "content": "Nên Ta sẽ sai một ngọn lửa đến thiêu hủy tường Thành Ga-xa;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.313Z",
      "updatedAt": "2024-09-23T06:10:02.313Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "ee391e54-4c83-445f-acb6-722aff31cb5e",
      "number": 7,
      "content": "Ngọn lửa ấy sẽ thiêu rụi các lâu đài của nó.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.318Z",
      "updatedAt": "2024-09-23T06:10:02.318Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "e8591297-00b8-4e95-be3a-7204daf70f7c",
      "number": 8,
      "content": "Ta sẽ tiêu diệt dân cư của Ách-đốt,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.323Z",
      "updatedAt": "2024-09-23T06:10:02.323Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "dcb55dd0-8ff5-440f-a97e-2a4c6ce10d41",
      "number": 8,
      "content": "Và diệt trừ kẻ cầm vương trượng của Ách-kê-lôn.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.329Z",
      "updatedAt": "2024-09-23T06:10:02.329Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "1c8a937e-4345-43a9-af68-6b6af5d976d2",
      "number": 8,
      "content": "Ta sẽ trở tay chống lại Éc-rôn,",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.333Z",
      "updatedAt": "2024-09-23T06:10:02.333Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "b967f367-db92-4184-8b2d-2b3c17284426",
      "number": 8,
      "content": "Và những kẻ còn sót lại của Phi-li-tin sẽ bị tiêu diệt,”",
      "order": 3,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.338Z",
      "updatedAt": "2024-09-23T06:10:02.338Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "91fc800f-faba-46f1-a6ab-b170c680c496",
      "number": 8,
      "content": "**CHÚA** Hằng Hữu phán.",
      "order": 4,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.343Z",
      "updatedAt": "2024-09-23T06:10:02.343Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "92dd7dd0-1706-4412-ace9-6e3211bb982c",
      "number": 9,
      "content": "“Vì tội của Ty-rơ nặng gấp ba gấp bốn lần,",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.356Z",
      "updatedAt": "2024-09-23T06:10:02.356Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "70aa0e10-3d47-4c22-8012-7af35f038b59",
      "number": 9,
      "content": "Vì nó đã bắt dân cả vùng đem lưu đày và trao nộp họ cho Ê-đôm,",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.365Z",
      "updatedAt": "2024-09-23T06:10:02.365Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "ca2eab3b-0eca-465b-9fec-d1458ff822df",
      "number": 9,
      "content": "Nó đã không nhớ đến giao ước kết nghĩa huynh đệ giữa hai dân tộc,",
      "order": 4,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.370Z",
      "updatedAt": "2024-09-23T06:10:02.370Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "74e5ba76-e3fb-4c4e-98cc-e8ae3463a3f6",
      "number": 10,
      "content": "Nên Ta sẽ sai một ngọn lửa đến thiêu hủy tường Thành Ty-rơ,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.375Z",
      "updatedAt": "2024-09-23T06:10:02.375Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "0bf89c9c-cebc-4e08-8728-574c1cf82269",
      "number": 10,
      "content": "Ngọn lửa ấy sẽ thiêu rụi các lâu đài của nó.”",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.379Z",
      "updatedAt": "2024-09-23T06:10:02.379Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "64d72a7b-3f3c-4156-bf9d-662ef95e3960",
      "number": 11,
      "content": "“Vì tội của Ê-đôm nặng gấp ba gấp bốn lần,",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.393Z",
      "updatedAt": "2024-09-23T06:10:02.393Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "63ff5380-a2cc-4378-8272-657deaa7dbcb",
      "number": 11,
      "content": "Vì nó đã dùng gươm đuổi giết em nó,",
      "order": 3,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.402Z",
      "updatedAt": "2024-09-23T06:10:02.402Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "ab87d78a-b491-43a9-9744-6fc1fe88c4c3",
      "number": 11,
      "content": "Nó đã không giữ lại trong lòng mảy may lòng thương xót,",
      "order": 4,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.407Z",
      "updatedAt": "2024-09-23T06:10:02.407Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "95e4a920-2d2a-48ea-9d19-c2a17437077d",
      "number": 11,
      "content": "Thay vào đó nó để cho lòng hận thù dày vò nó mãi,",
      "order": 5,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.412Z",
      "updatedAt": "2024-09-23T06:10:02.412Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "a9520d81-c2e5-45a1-8865-a562c55895d2",
      "number": 11,
      "content": "Và nó nuôi cơn thịnh nộ kéo dài đến muôn đời,",
      "order": 6,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.417Z",
      "updatedAt": "2024-09-23T06:10:02.417Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "0943b091-d9a6-4bbf-b438-9c8ca293c905",
      "number": 12,
      "content": "Nên Ta sẽ sai một ngọn lửa đến trên Tê-man;",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.422Z",
      "updatedAt": "2024-09-23T06:10:02.422Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "58896eba-7b40-4027-922b-a2062ceeb275",
      "number": 12,
      "content": "Ngọn lửa ấy sẽ thiêu rụi các lâu đài của Bô-ra.”",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.427Z",
      "updatedAt": "2024-09-23T06:10:02.427Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "b420d521-7335-4211-ade1-64f29ca7f72b",
      "number": 13,
      "content": "“Vì tội của Am-môn nặng gấp ba gấp bốn lần,",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.441Z",
      "updatedAt": "2024-09-23T06:10:02.441Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "5b656811-f219-4c86-b6d1-57da724b3ffd",
      "number": 13,
      "content": "Vì nó đã mổ bụng đàn bà có thai tại Ghi-lê-át",
      "order": 3,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.450Z",
      "updatedAt": "2024-09-23T06:10:02.450Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "e4a26aac-d3ef-499f-96d7-ebea0e422b17",
      "number": 13,
      "content": "Để bành trướng lãnh thổ của nó,",
      "order": 4,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.454Z",
      "updatedAt": "2024-09-23T06:10:02.454Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "6c98fd16-36e7-404f-b30b-ce8bd589c17e",
      "number": 14,
      "content": "Nên Ta sẽ nhen lên một ngọn lửa trên tường Thành Ráp-ba;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.459Z",
      "updatedAt": "2024-09-23T06:10:02.459Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "2d952dad-b857-4d24-ab34-336c52b82017",
      "number": 14,
      "content": "Ngọn lửa ấy sẽ thiêu rụi các lâu đài của nó,",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.464Z",
      "updatedAt": "2024-09-23T06:10:02.464Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "20646368-cc54-4df0-800e-03e138825883",
      "number": 14,
      "content": "Giữa lúc tiếng gào thét trong ngày chiến trận,",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.469Z",
      "updatedAt": "2024-09-23T06:10:02.469Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "31df4732-86d2-4cff-a58c-0c0d6fc4a3a0",
      "number": 14,
      "content": "Cùng với cuồng phong trong ngày giông bão.",
      "order": 3,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.474Z",
      "updatedAt": "2024-09-23T06:10:02.474Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "6f9437c6-99a5-4056-96e0-cc296ed89f6e",
      "number": 15,
      "content": "Vua của nó sẽ bị bắt đem đi lưu đày,",
      "order": 0,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.479Z",
      "updatedAt": "2024-09-23T06:10:02.479Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    },
    {
      "id": "a201cf71-c23d-4d26-9976-71e8a7c73f5c",
      "number": 15,
      "content": "Hắn và các quan tướng của hắn sẽ bị bắt đem đi cùng một lúc,”",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:10:02.484Z",
      "updatedAt": "2024-09-23T06:10:02.484Z",
      "chapterId": "99339dc5-e5a1-4015-bb96-38cdda031b75"
    }
  ],
