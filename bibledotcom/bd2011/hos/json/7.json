{
  "chapter": {
    "id": "4b189dc9-3ef7-4750-a822-bc7b669e89ff",
    "number": 7,
    "ref": "https://www.bible.com/bible/19/HOS.7.BD2011",
    "createdAt": "2024-09-23T05:08:41.530Z",
    "updatedAt": "2024-09-23T05:08:41.530Z",
    "bookId": "cc5501c8-9a45-4a4c-be0a-5c92fd635f3f"
  },
  "verses": [
    {
      "id": "2427b572-526f-4644-80d3-49a9564ee4c6",
      "number": 1,
      "content": "Khi Ta muốn chữa lành cho I-sơ-ra-ên,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.794Z",
      "updatedAt": "2024-09-23T06:09:25.794Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "af2a270e-5179-4174-acf8-5ce17de6916e",
      "number": 1,
      "content": "Tội lỗi của Ép-ra-im",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.811Z",
      "updatedAt": "2024-09-23T06:09:25.811Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "1f129329-0852-4174-9c31-a22fb3783213",
      "number": 1,
      "content": "Và gian ác của Sa-ma-ri lại lộ ra;",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.813Z",
      "updatedAt": "2024-09-23T06:09:25.813Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "26fad82c-5bc6-428f-acb0-68c39b708ce1",
      "number": 1,
      "content": "Vì chúng đã làm những việc gian tà;",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.817Z",
      "updatedAt": "2024-09-23T06:09:25.817Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "5a78ab37-b46f-4eaa-8be8-0b489979ce71",
      "number": 1,
      "content": "Bên trong phường ăn trộm lấy cắp;",
      "order": 4,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.821Z",
      "updatedAt": "2024-09-23T06:09:25.821Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "b473c3f6-1d5b-4526-8ab0-17f427753894",
      "number": 1,
      "content": "Bên ngoài quân ăn cướp tấn công.",
      "order": 5,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.826Z",
      "updatedAt": "2024-09-23T06:09:25.826Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "46cacd11-1491-4245-a3bb-c7621dc816d4",
      "number": 2,
      "content": "Nhưng chúng không nghĩ rằng",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.831Z",
      "updatedAt": "2024-09-23T06:09:25.831Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "c2fe7c15-72a4-4795-950d-f46a034199a5",
      "number": 2,
      "content": "Ta nhớ hết mọi việc ác của chúng.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.836Z",
      "updatedAt": "2024-09-23T06:09:25.836Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "c55f6210-d07b-4f93-9421-75a51a65e7b7",
      "number": 2,
      "content": "Những hành vi tội lỗi của chúng dẫy đầy chung quanh chúng,",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.841Z",
      "updatedAt": "2024-09-23T06:09:25.841Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "68df1d19-4156-45a1-b056-4727913d2e89",
      "number": 2,
      "content": "Những hành vi ấy hằng ở trước mặt Ta.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.846Z",
      "updatedAt": "2024-09-23T06:09:25.846Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "afe6d304-a3a1-4886-9b54-869b49ce9ed3",
      "number": 3,
      "content": "Để giúp vui cho vua, chúng đã bày những trò gian ác,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.851Z",
      "updatedAt": "2024-09-23T06:09:25.851Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "c36f5bd3-1f8c-4e8b-b9d4-56d47f6629bb",
      "number": 3,
      "content": "Để làm hài lòng những kẻ có quyền, chúng đã tạo ra các việc dối gian.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.857Z",
      "updatedAt": "2024-09-23T06:09:25.857Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "ec059af9-158b-4b15-b443-7a83fd4fe65f",
      "number": 4,
      "content": "Tất cả chúng đều là những kẻ ngoại tình;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.862Z",
      "updatedAt": "2024-09-23T06:09:25.862Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "3793367e-ea42-47e0-9b2c-b5d1e4386002",
      "number": 4,
      "content": "Chúng giống như một lò lửa cứ hực cháy,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.867Z",
      "updatedAt": "2024-09-23T06:09:25.867Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "ce443afb-0bc4-4520-80da-790afcdaf44d",
      "number": 4,
      "content": "Mặc dù người nướng bánh đã ngưng nuôi ngọn lửa",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.873Z",
      "updatedAt": "2024-09-23T06:09:25.873Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "11fb0b62-3794-4d57-8ec0-ba3660834fb0",
      "number": 4,
      "content": "Từ lúc nhồi bột",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.883Z",
      "updatedAt": "2024-09-23T06:09:25.883Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "d938fed3-b1a4-466f-9957-ea9063956870",
      "number": 4,
      "content": "Cho đến khi bột dậy men.",
      "order": 4,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.889Z",
      "updatedAt": "2024-09-23T06:09:25.889Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "6ff9b5c5-a50e-41f6-9726-40be902f51c8",
      "number": 5,
      "content": "Trong ngày lễ tôn trọng vua,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.894Z",
      "updatedAt": "2024-09-23T06:09:25.894Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "7e78a90a-0662-4582-a01f-a2235239df3f",
      "number": 5,
      "content": "Các quan tướng đều say mèm đến ngã bịnh;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.899Z",
      "updatedAt": "2024-09-23T06:09:25.899Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "791c11e4-ee4f-4500-baa6-bf8b071516cb",
      "number": 5,
      "content": "Còn vua chẳng còn biết giữ thể thống gì, nên đã cười đùa với những kẻ chế nhạo mình.",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.904Z",
      "updatedAt": "2024-09-23T06:09:25.904Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "c8a56737-4d66-4b2f-9866-21f153973a5b",
      "number": 6,
      "content": "Vì chúng đã chuẩn bị lòng chúng như một lò lửa;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.914Z",
      "updatedAt": "2024-09-23T06:09:25.914Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "488a9f1b-cb5a-4bd3-a420-36752e93e840",
      "number": 6,
      "content": "Suốt đêm chúng toan tính những ý đồ tội lỗi xấu xa;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.919Z",
      "updatedAt": "2024-09-23T06:09:25.919Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "76bc67e6-06d2-4ac8-be51-004cd4fbc300",
      "number": 6,
      "content": "Sáng ngày chúng bộc lộ những điều ấy ra như một ngọn lửa bùng lên.",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.924Z",
      "updatedAt": "2024-09-23T06:09:25.924Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "e796abb8-abd1-4229-a5cd-a413880f3038",
      "number": 7,
      "content": "Tất cả chúng đều nóng như một lò lửa hực;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.929Z",
      "updatedAt": "2024-09-23T06:09:25.929Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "759c5cbf-d1f4-4cd8-b403-90d89fba7f2f",
      "number": 7,
      "content": "Chúng thiêu đốt cả những kẻ cầm quyền của chúng;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.934Z",
      "updatedAt": "2024-09-23T06:09:25.934Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "9867e502-23e2-45cb-8b03-58ed14625e2d",
      "number": 7,
      "content": "Tất cả các vua chúng đã sa ngã;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.940Z",
      "updatedAt": "2024-09-23T06:09:25.940Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "137d1b89-5a09-4083-8bb8-2384c7aa6e32",
      "number": 7,
      "content": "Không ai trong chúng kêu cầu Ta.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.945Z",
      "updatedAt": "2024-09-23T06:09:25.945Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "d27c85e1-a671-44f5-be16-6c47b90294da",
      "number": 8,
      "content": "Ép-ra-im tin thờ chung chạ với các dân ngoại;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.949Z",
      "updatedAt": "2024-09-23T06:09:25.949Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "bf41af7f-888d-484c-8502-9cea994912ee",
      "number": 8,
      "content": "Ép-ra-im trở thành cái bánh nướng không được lật qua mặt kia, nên bị cháy đen một mặt.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.954Z",
      "updatedAt": "2024-09-23T06:09:25.954Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "34bdccc0-5df4-45b4-8eaf-b2b39a957762",
      "number": 9,
      "content": "Sự thờ lạy những thần tượng của các dân ngoại đã làm tiêu hao sức lực của nó,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.959Z",
      "updatedAt": "2024-09-23T06:09:25.959Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "ba2144bf-0212-4965-a39c-59aa8aa83e86",
      "number": 9,
      "content": "Nhưng nó không biết.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.964Z",
      "updatedAt": "2024-09-23T06:09:25.964Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "1362e683-1f91-4099-964a-d30ba72bf7a4",
      "number": 9,
      "content": "Phải, nó giống như một người tóc đã hoa râm, sức khỏe đang giảm dần,",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.969Z",
      "updatedAt": "2024-09-23T06:09:25.969Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "354d5d8b-a7ad-4afe-ac40-354caf1bc872",
      "number": 9,
      "content": "Mà không biết.",
      "order": 3,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.975Z",
      "updatedAt": "2024-09-23T06:09:25.975Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "0f3fc204-e6c8-4bdb-8582-29d8cc0135ab",
      "number": 10,
      "content": "Sự kiêu ngạo của I-sơ-ra-ên đã làm chứng nghịch lại nó;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.980Z",
      "updatedAt": "2024-09-23T06:09:25.980Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "2db77a59-b5fb-4172-95c1-bd0acc1c308c",
      "number": 10,
      "content": "Thế mà trong mọi sự đó, nó vẫn không quay về với **CHÚA**, Đức Chúa Trời của nó,",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.985Z",
      "updatedAt": "2024-09-23T06:09:25.985Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "ed0537e4-913e-4d89-b2b7-4c86431214a0",
      "number": 10,
      "content": "Hoặc để hết lòng tìm kiếm Ngài!",
      "order": 2,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.991Z",
      "updatedAt": "2024-09-23T06:09:25.991Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "43a199d5-fc65-4ad9-82af-21ee7b485750",
      "number": 11,
      "content": "Ép-ra-im giống như một con bồ câu",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:25.996Z",
      "updatedAt": "2024-09-23T06:09:25.996Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "50769e2e-b135-4239-917f-b5b72e14f4c1",
      "number": 11,
      "content": "Khờ dại, tâm trí chẳng biết nghĩ suy;",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.013Z",
      "updatedAt": "2024-09-23T06:09:26.013Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "39b825d0-f6c9-47a5-97fc-d472a39313ae",
      "number": 11,
      "content": "Khi thì cầu cạnh Ai-cập, lúc thì nhờ vả A-sy-ri.",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.018Z",
      "updatedAt": "2024-09-23T06:09:26.018Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "91453bbd-66ca-4d5c-ac21-71214ffab300",
      "number": 12,
      "content": "Khi chúng ra đi, Ta sẽ bủa lưới ra giăng trên chúng;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.024Z",
      "updatedAt": "2024-09-23T06:09:26.024Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "67cb5a6b-3973-4a4b-be3b-69730ae7865b",
      "number": 12,
      "content": "Ta sẽ bắt trọn chúng như bắt gọn các chim trời;",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.029Z",
      "updatedAt": "2024-09-23T06:09:26.029Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "bcde9472-9102-490d-8830-0b8c29bfcd69",
      "number": 12,
      "content": "Khi chúng hội họp nhau để toan tính sự nhờ cậy loài người, Ta sẽ sửa trị chúng.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.034Z",
      "updatedAt": "2024-09-23T06:09:26.034Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "a2e50a26-4a0e-4678-b8a6-42d6ff70fbd2",
      "number": 13,
      "content": "Khốn cho chúng, vì chúng đã lìa bỏ Ta!",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.039Z",
      "updatedAt": "2024-09-23T06:09:26.039Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "47feb4c8-0b08-4c70-8312-ca820d08b356",
      "number": 13,
      "content": "Thảm khốc thay cho chúng, vì chúng đã nổi loạn chống lại Ta!",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.044Z",
      "updatedAt": "2024-09-23T06:09:26.044Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "3978a58e-bf2e-47f0-a015-82b3290ecdd9",
      "number": 13,
      "content": "Ta muốn cứu chuộc chúng biết bao,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.049Z",
      "updatedAt": "2024-09-23T06:09:26.049Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "a35e93d0-8a27-4017-90b3-883ee87f21bb",
      "number": 13,
      "content": "Nhưng chúng cứ nói những lời dối trá xúc phạm Ta.",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.053Z",
      "updatedAt": "2024-09-23T06:09:26.053Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "694b3600-2d24-4ff3-ba7f-798f80302701",
      "number": 14,
      "content": "Chúng chẳng để lòng kêu cầu Ta,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.059Z",
      "updatedAt": "2024-09-23T06:09:26.059Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "490a2a76-b619-4625-b936-c9d085d744de",
      "number": 14,
      "content": "Nhưng cứ nằm trên giường mà trăn trở thở than.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.064Z",
      "updatedAt": "2024-09-23T06:09:26.064Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "ded152a8-68b3-4db5-a60f-72fc0d500859",
      "number": 14,
      "content": "Chúng quay qua các thần tượng",
      "order": 2,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.068Z",
      "updatedAt": "2024-09-23T06:09:26.068Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "7edb949b-e7a0-4c07-92cf-0f73fc161134",
      "number": 14,
      "content": "Và cầu xin cho ruộng lúa trúng mùa và vườn nho sản xuất rượu.",
      "order": 3,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.073Z",
      "updatedAt": "2024-09-23T06:09:26.073Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "eeb8db2c-f144-455a-9b73-039ca1131d3a",
      "number": 15,
      "content": "Dù Ta đã huấn luyện chúng và ban sức lực cho tay chúng,",
      "order": 0,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.078Z",
      "updatedAt": "2024-09-23T06:09:26.078Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "052966ef-4674-4547-9ba8-3ab30e69cbb6",
      "number": 15,
      "content": "Chúng vẫn lập mưu tính kế chống lại Ta.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.083Z",
      "updatedAt": "2024-09-23T06:09:26.083Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "826d49a9-e7d7-4644-bb1b-a2996bbc173f",
      "number": 16,
      "content": "Chúng chẳng quay về với Đấng Tối Cao,",
      "order": 0,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.087Z",
      "updatedAt": "2024-09-23T06:09:26.087Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "01914b04-3823-4b87-ba74-18a49a0b8f0a",
      "number": 16,
      "content": "Chúng như cây cung hỏng, không thể nào bắn trúng mục tiêu;",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.092Z",
      "updatedAt": "2024-09-23T06:09:26.092Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "cd820a86-a369-4de1-912b-40f008af0cdc",
      "number": 16,
      "content": "Những kẻ lãnh đạo của chúng sẽ ngã chết bằng gươm,",
      "order": 2,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.097Z",
      "updatedAt": "2024-09-23T06:09:26.097Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "dc34686e-cd65-4cd5-8db9-97e85a5eea5a",
      "number": 16,
      "content": "Bởi miệng chúng đã buông những lời hỗn láo xúc phạm đến Ta;",
      "order": 3,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.101Z",
      "updatedAt": "2024-09-23T06:09:26.101Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    },
    {
      "id": "02d4d045-d088-4631-bc7f-c60ee7ff57cd",
      "number": 16,
      "content": "Vì vậy chúng sẽ làm đầu đề cho người ta giễu cợt ở Ai-cập.",
      "order": 4,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T06:09:26.106Z",
      "updatedAt": "2024-09-23T06:09:26.106Z",
      "chapterId": "4b189dc9-3ef7-4750-a822-bc7b669e89ff"
    }
  ],
