{
  "chapter": {
    "id": "3e242f4b-1c60-4c30-8b0f-495d05c12c14",
    "number": 7,
    "ref": "https://www.bible.com/bible/19/HOS.7.BD2011",
    "createdAt": "2024-09-07T11:51:12.923Z",
    "updatedAt": "2024-09-07T11:51:12.923Z",
    "bookId": "29f7e7cf-50a1-479e-94a2-0a42acf6401f"
  },
  "verses": [
    {
      "id": "f6eb3df2-faed-42ec-8d23-4f3a1aa575ab",
      "number": 1,
      "content": "Khi Ta muốn chữa lành cho I-sơ-ra-ên,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.348Z",
      "updatedAt": "2024-09-07T15:19:19.348Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "b45dceaf-0df9-45e7-9ad6-efd0505f2503",
      "number": 1,
      "content": "Tội lỗi của Ép-ra-im",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.348Z",
      "updatedAt": "2024-09-07T15:19:19.348Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "6d62b20b-c60a-482d-a0f1-cef10a357573",
      "number": 1,
      "content": "Và gian ác của Sa-ma-ri lại lộ ra;",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.348Z",
      "updatedAt": "2024-09-07T15:19:19.348Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "3cf52d4b-6d4b-40d4-9ecb-efacbddd97f2",
      "number": 1,
      "content": "Vì chúng đã làm những việc gian tà;",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.348Z",
      "updatedAt": "2024-09-07T15:19:19.348Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "6cd5bcfe-cb58-4c4b-bf97-e06b129c5e92",
      "number": 1,
      "content": "Bên trong phường ăn trộm lấy cắp;",
      "order": 4,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.348Z",
      "updatedAt": "2024-09-07T15:19:19.348Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "fcfb36cd-f03c-47ad-b058-6017b687284b",
      "number": 1,
      "content": "Bên ngoài quân ăn cướp tấn công.",
      "order": 5,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.348Z",
      "updatedAt": "2024-09-07T15:19:19.348Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "4c07df2d-3e9e-4ef4-8c99-26df7e597632",
      "number": 2,
      "content": "Nhưng chúng không nghĩ rằng",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.358Z",
      "updatedAt": "2024-09-07T15:19:19.358Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "7471a7b3-bee0-4c1e-a02d-ed5f5cab4c21",
      "number": 2,
      "content": "Ta nhớ hết mọi việc ác của chúng.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.358Z",
      "updatedAt": "2024-09-07T15:19:19.358Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "5a065b09-c59f-41ab-9168-3350aa57a0b9",
      "number": 2,
      "content": "Những hành vi tội lỗi của chúng dẫy đầy chung quanh chúng,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.358Z",
      "updatedAt": "2024-09-07T15:19:19.358Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "087ea891-9735-45f6-afeb-6b18dda575e9",
      "number": 2,
      "content": "Những hành vi ấy hằng ở trước mặt Ta.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.358Z",
      "updatedAt": "2024-09-07T15:19:19.358Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "9c733cdb-cdbe-44f0-a94f-786be972c8de",
      "number": 3,
      "content": "Để giúp vui cho vua, chúng đã bày những trò gian ác,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.362Z",
      "updatedAt": "2024-09-07T15:19:19.362Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "518cdf39-abed-4d27-8948-5b4a4efde45e",
      "number": 3,
      "content": "Để làm hài lòng những kẻ có quyền, chúng đã tạo ra các việc dối gian.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.362Z",
      "updatedAt": "2024-09-07T15:19:19.362Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "db678da0-f05c-4b36-94d7-28757ab6b7a5",
      "number": 4,
      "content": "Tất cả chúng đều là những kẻ ngoại tình;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.366Z",
      "updatedAt": "2024-09-07T15:19:19.366Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "501f5a5f-e373-4e9d-80a5-aca26751f2d6",
      "number": 4,
      "content": "Chúng giống như một lò lửa cứ hực cháy,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.366Z",
      "updatedAt": "2024-09-07T15:19:19.366Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "98bd299d-99ac-4627-8a8d-a29133b691a2",
      "number": 4,
      "content": "Mặc dù người nướng bánh đã ngưng nuôi ngọn lửa",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.366Z",
      "updatedAt": "2024-09-07T15:19:19.366Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "5ae9080c-387c-4c04-8598-797d75c162a5",
      "number": 4,
      "content": "Từ lúc nhồi bột",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.366Z",
      "updatedAt": "2024-09-07T15:19:19.366Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "72220711-cda7-42fa-964d-00ef59b3353e",
      "number": 4,
      "content": "Cho đến khi bột dậy men.",
      "order": 4,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.366Z",
      "updatedAt": "2024-09-07T15:19:19.366Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "533f904a-60a5-40dc-b8ec-c412439553d4",
      "number": 5,
      "content": "Trong ngày lễ tôn trọng vua,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.371Z",
      "updatedAt": "2024-09-07T15:19:19.371Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "ebcbed94-37a9-41ca-971f-45b83ad5110f",
      "number": 5,
      "content": "Các quan tướng đều say mèm đến ngã bịnh;",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.371Z",
      "updatedAt": "2024-09-07T15:19:19.371Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "988c7848-07ab-4524-925c-ccc3840b55af",
      "number": 5,
      "content": "Còn vua chẳng còn biết giữ thể thống gì, nên đã cười đùa với những kẻ chế nhạo mình.",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.371Z",
      "updatedAt": "2024-09-07T15:19:19.371Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "8cb4fb4b-4e2d-4ebc-bed8-ce9e638c6a0b",
      "number": 6,
      "content": "Vì chúng đã chuẩn bị lòng chúng như một lò lửa;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.375Z",
      "updatedAt": "2024-09-07T15:19:19.375Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "f869afda-f577-42b2-8111-12024dd8ac98",
      "number": 6,
      "content": "Suốt đêm chúng toan tính những ý đồ tội lỗi xấu xa;",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.375Z",
      "updatedAt": "2024-09-07T15:19:19.375Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "16aa2cdf-96e1-4cff-a295-3b72c054f8b5",
      "number": 6,
      "content": "Sáng ngày chúng bộc lộ những điều ấy ra như một ngọn lửa bùng lên.",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.375Z",
      "updatedAt": "2024-09-07T15:19:19.375Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "465d5fec-6bea-4548-b6fc-eacd0ff909a0",
      "number": 7,
      "content": "Tất cả chúng đều nóng như một lò lửa hực;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.379Z",
      "updatedAt": "2024-09-07T15:19:19.379Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "84161dc0-8ffa-4b0a-aabf-180c65b40b5f",
      "number": 7,
      "content": "Chúng thiêu đốt cả những kẻ cầm quyền của chúng;",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.379Z",
      "updatedAt": "2024-09-07T15:19:19.379Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "96608fa1-829a-41a3-be4b-52bd402f49b4",
      "number": 7,
      "content": "Tất cả các vua chúng đã sa ngã;",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.379Z",
      "updatedAt": "2024-09-07T15:19:19.379Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "698f71b0-37f0-4022-a328-bf6f54cdb7b4",
      "number": 7,
      "content": "Không ai trong chúng kêu cầu Ta.",
      "order": 3,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.379Z",
      "updatedAt": "2024-09-07T15:19:19.379Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "79c9a67a-4d51-446a-a81c-d6e636a6af0c",
      "number": 8,
      "content": "Ép-ra-im tin thờ chung chạ với các dân ngoại;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.383Z",
      "updatedAt": "2024-09-07T15:19:19.383Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "b6bd6f70-a0c4-4ec9-ba28-7ee1da520f45",
      "number": 8,
      "content": "Ép-ra-im trở thành cái bánh nướng không được lật qua mặt kia, nên bị cháy đen một mặt.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.383Z",
      "updatedAt": "2024-09-07T15:19:19.383Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "f5367479-c063-4366-8324-b41d07d071c9",
      "number": 9,
      "content": "Sự thờ lạy những thần tượng của các dân ngoại đã làm tiêu hao sức lực của nó,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.387Z",
      "updatedAt": "2024-09-07T15:19:19.387Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "4ed33c66-f773-46f5-86c7-76d890062f9f",
      "number": 9,
      "content": "Nhưng nó không biết.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.387Z",
      "updatedAt": "2024-09-07T15:19:19.387Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "9c573b11-7478-4ef8-a1ba-d42204b369ff",
      "number": 9,
      "content": "Phải, nó giống như một người tóc đã hoa râm, sức khỏe đang giảm dần,",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.387Z",
      "updatedAt": "2024-09-07T15:19:19.387Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "aca9a668-c0ed-4b13-aabb-2bea0311f1f9",
      "number": 9,
      "content": "Mà không biết.",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.387Z",
      "updatedAt": "2024-09-07T15:19:19.387Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "e696a4d0-cc78-4301-b0d6-b351e3ca3eb5",
      "number": 10,
      "content": "Sự kiêu ngạo của I-sơ-ra-ên đã làm chứng nghịch lại nó;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.391Z",
      "updatedAt": "2024-09-07T15:19:19.391Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "bb7cae12-465c-4a60-a6d0-4216ee323f29",
      "number": 10,
      "content": "Thế mà trong mọi sự đó, nó vẫn không quay về với CHÚA, Đức Chúa Trời của nó,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.391Z",
      "updatedAt": "2024-09-07T15:19:19.391Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "80e32d4d-664c-4cb9-9dab-90fb05eec6ac",
      "number": 10,
      "content": "Hoặc để hết lòng tìm kiếm Ngài!",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.391Z",
      "updatedAt": "2024-09-07T15:19:19.391Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "7e748ac4-9b6a-4cac-8ec0-83b3e0284e02",
      "number": 11,
      "content": "Ép-ra-im giống như một con bồ câu",
      "order": 0,
      "parNumber": 38,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.395Z",
      "updatedAt": "2024-09-07T15:19:19.395Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "7a387b22-2099-48c9-8941-a8c05a7a3b93",
      "number": 11,
      "content": "Khờ dại, tâm trí chẳng biết nghĩ suy;",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.395Z",
      "updatedAt": "2024-09-07T15:19:19.395Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "249422ac-3609-4ac5-b266-dafeb74bac22",
      "number": 11,
      "content": "Khi thì cầu cạnh Ai-cập, lúc thì nhờ vả A-sy-ri.",
      "order": 2,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.395Z",
      "updatedAt": "2024-09-07T15:19:19.395Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "0398810f-66d5-4311-9b74-435e8282ccd6",
      "number": 12,
      "content": "Khi chúng ra đi, Ta sẽ bủa lưới ra giăng trên chúng;",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.399Z",
      "updatedAt": "2024-09-07T15:19:19.399Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "476189cf-fc22-4f77-a2ae-8593868aeb55",
      "number": 12,
      "content": "Ta sẽ bắt trọn chúng như bắt gọn các chim trời;",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.399Z",
      "updatedAt": "2024-09-07T15:19:19.399Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "15bd8732-699b-4860-b29c-b989a106884d",
      "number": 12,
      "content": "Khi chúng hội họp nhau để toan tính sự nhờ cậy loài người, Ta sẽ sửa trị chúng.",
      "order": 2,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.399Z",
      "updatedAt": "2024-09-07T15:19:19.399Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "1631236f-32b9-492d-8bcc-262b6f45fb3c",
      "number": 13,
      "content": "Khốn cho chúng, vì chúng đã lìa bỏ Ta!",
      "order": 0,
      "parNumber": 44,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.406Z",
      "updatedAt": "2024-09-07T15:19:19.406Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "ea287a24-d7bc-4d4d-ae26-f09fa62a653b",
      "number": 13,
      "content": "Thảm khốc thay cho chúng, vì chúng đã nổi loạn chống lại Ta!",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.406Z",
      "updatedAt": "2024-09-07T15:19:19.406Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "8d02fec4-cf9c-4b47-abac-7ab8ee0ef04c",
      "number": 13,
      "content": "Ta muốn cứu chuộc chúng biết bao,",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.406Z",
      "updatedAt": "2024-09-07T15:19:19.406Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "40221786-195b-4aeb-80f5-8407c6e4b4f4",
      "number": 13,
      "content": "Nhưng chúng cứ nói những lời dối trá xúc phạm Ta.",
      "order": 3,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.406Z",
      "updatedAt": "2024-09-07T15:19:19.406Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "8a0e2f9c-4bfb-47bf-a7df-be3c57cd66e4",
      "number": 14,
      "content": "Chúng chẳng để lòng kêu cầu Ta,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.411Z",
      "updatedAt": "2024-09-07T15:19:19.411Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "24bbc71c-4483-4896-aeea-16bb0d0b06ec",
      "number": 14,
      "content": "Nhưng cứ nằm trên giường mà trăn trở thở than.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.411Z",
      "updatedAt": "2024-09-07T15:19:19.411Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "ddf55e7c-1025-4190-93b2-0d9f606e726d",
      "number": 14,
      "content": "Chúng quay qua các thần tượng",
      "order": 2,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.411Z",
      "updatedAt": "2024-09-07T15:19:19.411Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "24bcb64c-f099-4507-acc1-740d95d4716c",
      "number": 14,
      "content": "Và cầu xin cho ruộng lúa trúng mùa và vườn nho sản xuất rượu.",
      "order": 3,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.411Z",
      "updatedAt": "2024-09-07T15:19:19.411Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "dc207422-3cfb-43af-a95d-d98864a15296",
      "number": 15,
      "content": "Dù Ta đã huấn luyện chúng và ban sức lực cho tay chúng,",
      "order": 0,
      "parNumber": 52,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.416Z",
      "updatedAt": "2024-09-07T15:19:19.416Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "19649911-91a0-4589-b763-205625ed02a0",
      "number": 15,
      "content": "Chúng vẫn lập mưu tính kế chống lại Ta.",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.416Z",
      "updatedAt": "2024-09-07T15:19:19.416Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "647cb2cc-2560-47df-a157-fcd403554093",
      "number": 16,
      "content": "Chúng chẳng quay về với Đấng Tối Cao,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.422Z",
      "updatedAt": "2024-09-07T15:19:19.422Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "ad5236e0-529a-4f13-ae24-eefde6838eaa",
      "number": 16,
      "content": "Chúng như cây cung hỏng, không thể nào bắn trúng mục tiêu;",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.422Z",
      "updatedAt": "2024-09-07T15:19:19.422Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "66caaa79-3db1-4b35-b1cf-0b0109192393",
      "number": 16,
      "content": "Những kẻ lãnh đạo của chúng sẽ ngã chết bằng gươm,",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.422Z",
      "updatedAt": "2024-09-07T15:19:19.422Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "21b721fe-791e-4b36-b2af-3aa3c74940f6",
      "number": 16,
      "content": "Bởi miệng chúng đã buông những lời hỗn láo xúc phạm đến Ta;",
      "order": 3,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.422Z",
      "updatedAt": "2024-09-07T15:19:19.422Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    },
    {
      "id": "51167c9b-7595-4eff-92d7-5fa5ad63bede",
      "number": 16,
      "content": "Vì vậy chúng sẽ làm đầu đề cho người ta giễu cợt ở Ai-cập.",
      "order": 4,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:19:19.422Z",
      "updatedAt": "2024-09-07T15:19:19.422Z",
      "chapterId": "3e242f4b-1c60-4c30-8b0f-495d05c12c14"
    }
  ],
