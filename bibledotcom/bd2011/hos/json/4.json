      "chapterId": "20f5bc4b-0406-49f1-ab9f-58e5a3166609"
    },
    {
      "id": "7333f313-46b0-4102-be3e-c21f7e40b2d8",
      "content": "Cảnh Cáo I-sơ-ra-ên và Giu-đa",
      "order": 0,
      "createdAt": "2024-09-07T15:18:20.762Z",
