      "chapterId": "20f5bc4b-0406-49f1-ab9f-58e5a3166609"
    },
    {
      "id": "f244d028-0f56-4613-b214-a11d93f84b5b",
      "content": "Các Tư Tế Bị Kết Tội",
      "order": 0,
      "createdAt": "2024-09-07T15:18:20.762Z",
      "updatedAt": "2024-09-07T15:18:20.762Z",
      "verseId": "9aaf72c9-1c35-430c-b019-cb20ec77238e",
      "chapterId": "20f5bc4b-0406-49f1-ab9f-58e5a3166609"
    },
    {
      "id": "cb0f06b6-a74d-4c1c-85e8-bf9f55a0d26d",
      "content": "Thờ Hình Tượng và Nếp Sống Sa Đọa",
      "order": 0,
      "createdAt": "2024-09-07T15:18:20.762Z",
      "updatedAt": "2024-09-07T15:18:20.762Z",
