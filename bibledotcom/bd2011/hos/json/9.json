      "order": 0,
      "createdAt": "2024-09-07T15:19:48.817Z",
      "updatedAt": "2024-09-07T15:19:48.817Z",
      "verseId": "eeb19ee2-1df4-449f-a312-715e2ca19283",
      "chapterId": "63bbc205-07ea-4874-81fd-c3a9e3532ab0"
    },
    {
      "id": "413d37e3-57a6-4b1d-b6de-2418c6ba5f75",
      "content": "Tiên Tri của CHÚA Bị Khủng Bố",
      "order": 0,
      "createdAt": "2024-09-07T15:19:48.817Z",
      "updatedAt": "2024-09-07T15:19:48.817Z",
      "verseId": "0b455405-adb5-4486-835e-3868f3e63877",
      "chapterId": "63bbc205-07ea-4874-81fd-c3a9e3532ab0"
    },
    {
      "id": "03197072-8d8b-413f-b455-195fad29909c",
      "content": "Hình Phạt Xưa và Nay",
      "order": 0,
      "createdAt": "2024-09-07T15:19:48.817Z",
