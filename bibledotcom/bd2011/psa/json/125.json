{
  "chapter": {
    "id": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11",
    "number": 125,
    "ref": "https://www.bible.com/bible/19/PSA.125.BD2011",
    "createdAt": "2024-09-07T11:51:11.533Z",
    "updatedAt": "2024-09-07T11:51:11.533Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "512c24c1-a271-47b3-8a54-8a813446ca6f",
      "number": 1,
      "content": "Phàm ai tin cậy CHÚA sẽ như Núi Si-ôn,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.678Z",
      "updatedAt": "2024-09-07T14:21:50.678Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "7deddd4c-daae-4be2-9164-720081673a9c",
      "number": 1,
      "content": "Chẳng hề rúng động nhưng cứ bền vững muôn đời.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.678Z",
      "updatedAt": "2024-09-07T14:21:50.678Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "3fc856f7-1b6e-442e-b9fb-aa30568da04b",
      "number": 2,
      "content": "Như các rặng núi bao quanh Giê-ru-sa-lem thể nào,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.688Z",
      "updatedAt": "2024-09-07T14:21:50.688Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "4a36ce7e-bfa0-4824-90cc-fb136991202b",
      "number": 2,
      "content": "CHÚA bao quanh dân Ngài thể ấy,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.688Z",
      "updatedAt": "2024-09-07T14:21:50.688Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "0602ccf7-32e1-407c-88ed-1de931f700de",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.688Z",
      "updatedAt": "2024-09-07T14:21:50.688Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "afd06c19-77f3-43cd-9f1f-5562a1a6630e",
      "number": 3,
      "content": "Vì gậy cầm quyền của kẻ ác không được phép hành sử",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.691Z",
      "updatedAt": "2024-09-07T14:21:50.691Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "ea104adc-555f-4310-b23e-62bd2f5394f7",
      "number": 3,
      "content": "Trên địa phận của người ngay lành,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.691Z",
      "updatedAt": "2024-09-07T14:21:50.691Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "91091e2f-4934-4f8e-b891-2e0c403628f5",
      "number": 3,
      "content": "Nên tay người ngay lành",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.691Z",
      "updatedAt": "2024-09-07T14:21:50.691Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "1a047d64-82c3-4524-90c1-b6a65941e9c1",
      "number": 3,
      "content": "Không bị buộc phải tham dự vào việc ác.",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.691Z",
      "updatedAt": "2024-09-07T14:21:50.691Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "eaf46446-5441-4bfa-b6bc-49b9aeccc2d6",
      "number": 4,
      "content": "CHÚA ôi, xin ban phước cho những người tốt,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.693Z",
      "updatedAt": "2024-09-07T14:21:50.693Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "18d03c02-43b2-49e4-8945-41d6931db726",
      "number": 4,
      "content": "Và cho những người có lòng ngay thẳng thật thà.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.693Z",
      "updatedAt": "2024-09-07T14:21:50.693Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "069b9353-83fa-4f15-9cdb-f29e2d6566cc",
      "number": 5,
      "content": "Nhưng đối với những kẻ bỏ Ngài để đi theo con đường lươn lẹo,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.696Z",
      "updatedAt": "2024-09-07T14:21:50.696Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "939dd30b-ef65-4e02-845a-6bc8540adca0",
      "number": 5,
      "content": "Xin CHÚA trục xuất chúng chung với những kẻ làm điều ác.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.696Z",
      "updatedAt": "2024-09-07T14:21:50.696Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    },
    {
      "id": "808bebe9-98c6-4b6f-922b-7ef39915d7fd",
      "number": 5,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:50.696Z",
      "updatedAt": "2024-09-07T14:21:50.696Z",
      "chapterId": "d3ea8aac-c9c6-47e6-ae1e-92f0673b0e11"
    }
  ],
