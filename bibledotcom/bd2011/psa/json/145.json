    },
    {
      "id": "3de21145-efc8-4762-a4fe-39d3ff757e16",
      "number": 20,
      "content": "CHÚA gìn giữ mọi người yêu kính Ngài,",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:36.147Z",
      "updatedAt": "2024-09-07T14:28:36.147Z",
      "chapterId": "06177318-6af1-4ae9-8e30-ccfe12c86dd8"
    },
    {
      "id": "4fbf2903-5ebf-42c8-84ff-dbd702ebbc9c",
      "number": 20,
      "content": "Nhưng Ngài tiêu diệt mọi kẻ gian ác.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:36.147Z",
      "updatedAt": "2024-09-07T14:28:36.147Z",
      "chapterId": "06177318-6af1-4ae9-8e30-ccfe12c86dd8"
    },
    {
      "id": "0d685ee4-5a13-47a3-a82f-8e498281a323",
      "number": 21,
      "content": "Miệng tôi sẽ nói lên những lời ca ngợi CHÚA.",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:36.152Z",
      "updatedAt": "2024-09-07T14:28:36.152Z",
      "chapterId": "06177318-6af1-4ae9-8e30-ccfe12c86dd8"
    },
    {
      "id": "f2690e96-1f11-46aa-a685-477e492771db",
      "number": 21,
      "content": "Nguyện mọi loài xác thịt sẽ chúc tụng danh thánh Ngài đời đời vô cùng.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:36.152Z",
      "updatedAt": "2024-09-07T14:28:36.152Z",
      "chapterId": "06177318-6af1-4ae9-8e30-ccfe12c86dd8"
    }
  ],
