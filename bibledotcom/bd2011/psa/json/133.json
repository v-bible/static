{
  "chapter": {
    "id": "3d6d295a-4844-4668-98e2-f900710325c8",
    "number": 133,
    "ref": "https://www.bible.com/bible/19/PSA.133.BD2011",
    "createdAt": "2024-09-07T11:51:11.554Z",
