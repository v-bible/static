  },
  "verses": [
    {
      "id": "be05caa5-9de7-4a33-b005-7dc8feb72e74",
      "number": 1,
      "content": "Kìa, tốt đẹp thay và phước hạnh thay",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:50.776Z",
      "updatedAt": "2024-09-07T14:23:50.776Z",
      "chapterId": "3d6d295a-4844-4668-98e2-f900710325c8"
    },
    {
      "id": "ce5869a0-a2f2-4e16-90cb-ac87c4c269c4",
      "number": 1,
      "content": "Khi anh chị em sống hòa thuận với nhau!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:50.776Z",
      "updatedAt": "2024-09-07T14:23:50.776Z",
      "chapterId": "3d6d295a-4844-4668-98e2-f900710325c8"
    },
    {
      "id": "e55fcb2e-c1cb-465a-aa02-f414812d66b1",
      "number": 2,
      "content": "Điều ấy chẳng khác gì dầu quý báu từ trên đầu chảy xuống,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:50.787Z",
      "updatedAt": "2024-09-07T14:23:50.787Z",
      "chapterId": "3d6d295a-4844-4668-98e2-f900710325c8"
    },
    {
      "id": "ac8b6ca2-4724-44a1-b622-295ed818ce15",
      "number": 2,
      "content": "Chảy xuống râu,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:50.787Z",
      "updatedAt": "2024-09-07T14:23:50.787Z",
      "chapterId": "3d6d295a-4844-4668-98e2-f900710325c8"
    },
    {
      "id": "569c9f5a-7684-41fd-b668-82891fb0b456",
      "number": 2,
      "content": "Chảy xuống râu của A-rôn,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:50.787Z",
      "updatedAt": "2024-09-07T14:23:50.787Z",
      "chapterId": "3d6d295a-4844-4668-98e2-f900710325c8"
    },
    {
      "id": "9bbb2d4a-8f1a-48a9-a822-32e5848b2fa4",
      "number": 2,
      "content": "Rồi tiếp tục chảy xuống vạt áo ông.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:50.787Z",
      "updatedAt": "2024-09-07T14:23:50.787Z",
      "chapterId": "3d6d295a-4844-4668-98e2-f900710325c8"
    },
    {
      "id": "35c3ab21-073d-4112-8c38-105bb976a4ea",
      "number": 3,
      "content": "Điều ấy giống như sương móc từ Núi Hẹt-môn",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:50.790Z",
      "updatedAt": "2024-09-07T14:23:50.790Z",
      "chapterId": "3d6d295a-4844-4668-98e2-f900710325c8"
    },
    {
      "id": "9d238199-4e8d-4c62-abc3-ad332813d794",
      "number": 3,
      "content": "Sa xuống các rặng núi Si-ôn,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:50.790Z",
      "updatedAt": "2024-09-07T14:23:50.790Z",
      "chapterId": "3d6d295a-4844-4668-98e2-f900710325c8"
    },
    {
      "id": "d9cf4cd3-37a0-43cc-89fa-8c72ea5679ab",
      "number": 3,
      "content": "Vì tại đó CHÚA ban phước,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:50.790Z",
      "updatedAt": "2024-09-07T14:23:50.790Z",
      "chapterId": "3d6d295a-4844-4668-98e2-f900710325c8"
    },
    {
      "id": "6762f6a4-1a03-472c-830e-89c76601c646",
      "number": 3,
      "content": "Phước của sự sống đời đời.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:50.790Z",
      "updatedAt": "2024-09-07T14:23:50.790Z",
      "chapterId": "3d6d295a-4844-4668-98e2-f900710325c8"
    }
  ],
