{
  "chapter": {
    "id": "56e335fe-1b3f-4f38-ac82-a9c86a9869af",
    "number": 63,
    "ref": "https://www.bible.com/bible/19/PSA.63.BD2011",
    "createdAt": "2024-09-07T11:51:11.109Z",
    "updatedAt": "2024-09-07T11:51:11.109Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "4ec53ee3-1685-4e36-aaa3-648e0b4b6277",
      "number": 1,
      "content": "Đức Chúa Trời ôi, Ngài là Đức Chúa Trời của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.247Z",
      "updatedAt": "2024-09-07T14:07:02.247Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "2e415684-5df5-48d6-8c0d-494489898339",
      "number": 1,
      "content": "Khi hừng đông chưa ló dạng, con đã tìm cầu Ngài;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.247Z",
      "updatedAt": "2024-09-07T14:07:02.247Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "f9e80cee-960d-4872-986b-83a319266914",
      "number": 1,
      "content": "Linh hồn con khao khát Ngài, thân thể con mong mỏi Ngài,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.247Z",
      "updatedAt": "2024-09-07T14:07:02.247Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "fbf6ea72-544b-4f3a-bfe3-22038c514a4a",
      "number": 1,
      "content": "Trong một nơi khô khan cằn cỗi và chẳng có nước.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.247Z",
      "updatedAt": "2024-09-07T14:07:02.247Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "59654d5b-8d4b-43b0-89cf-c5d8f49ea529",
      "number": 2,
      "content": "Con mong ước được thấy quyền năng và vinh hiển Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.257Z",
      "updatedAt": "2024-09-07T14:07:02.257Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "fbb1cc86-1ea5-4956-8342-519a2b1839af",
      "number": 2,
      "content": "Như con đã từng thấy trong nơi thánh.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.257Z",
      "updatedAt": "2024-09-07T14:07:02.257Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "51f891cb-14eb-4616-bb0c-a37720990ecd",
      "number": 3,
      "content": "Vì tình thương của Ngài tốt hơn mạng sống,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.260Z",
      "updatedAt": "2024-09-07T14:07:02.260Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "26bbd6e9-da23-4e17-960d-a24870619123",
      "number": 3,
      "content": "Nên môi con sẽ ca ngợi Ngài.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.260Z",
      "updatedAt": "2024-09-07T14:07:02.260Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "04c9bed1-cc26-4732-a2ab-4042e993e13b",
      "number": 4,
      "content": "Hễ con còn sống bao lâu, con sẽ ca ngợi Ngài bấy lâu;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.262Z",
      "updatedAt": "2024-09-07T14:07:02.262Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "edf3be8c-f4f2-4d7e-9163-a0e1e51134a4",
      "number": 4,
      "content": "Con sẽ đưa tay lên và chúc tụng danh Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.262Z",
      "updatedAt": "2024-09-07T14:07:02.262Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "1e3de528-fd84-46ea-b5c2-52e0611f7b67",
      "number": 5,
      "content": "Linh hồn con sẽ vui thỏa như được hưởng các thức ăn béo bổ,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.265Z",
      "updatedAt": "2024-09-07T14:07:02.265Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "eec8e763-663f-4224-9397-18c258e6151c",
      "number": 5,
      "content": "Miệng con sẽ ca ngợi Ngài với môi vui vẻ.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.265Z",
      "updatedAt": "2024-09-07T14:07:02.265Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "df0aa167-af06-4237-a9c1-3f0dfa8b2036",
      "number": 6,
      "content": "Khi nằm trên giường con nhớ đến Ngài,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.268Z",
      "updatedAt": "2024-09-07T14:07:02.268Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "59c0d7f8-346d-4ff4-a73b-0d174e252df3",
      "number": 6,
      "content": "Con nghĩ về Ngài trọn các canh đêm,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.268Z",
      "updatedAt": "2024-09-07T14:07:02.268Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "291173f7-f537-4b20-a457-9a78c89d2fc2",
      "number": 7,
      "content": "Vì Ngài là nguồn cứu giúp của con,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.270Z",
      "updatedAt": "2024-09-07T14:07:02.270Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "04dbe581-4960-4e97-8cb0-69aad64d59f9",
      "number": 7,
      "content": "Nên dưới cánh Ngài con hát mừng vui vẻ.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.270Z",
      "updatedAt": "2024-09-07T14:07:02.270Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "9359222b-c962-4b4c-bc7e-02ef672ad52d",
      "number": 8,
      "content": "Linh hồn con bám chặt vào Ngài;",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.273Z",
      "updatedAt": "2024-09-07T14:07:02.273Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "15ec350e-0bcf-4d65-9a6d-1ceb813a3d97",
      "number": 8,
      "content": "Tay phải Ngài giữ vững con.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.273Z",
      "updatedAt": "2024-09-07T14:07:02.273Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "37b365fc-bf14-42f2-af0b-ac9e0ae21220",
      "number": 9,
      "content": "Nhưng những kẻ tìm cách tiêu diệt mạng sống con",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.275Z",
      "updatedAt": "2024-09-07T14:07:02.275Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "2ed0aba5-6f23-42f9-a4b6-2c0ce166bd5d",
      "number": 9,
      "content": "Sẽ bị rơi vào đáy sâu của lòng đất.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.275Z",
      "updatedAt": "2024-09-07T14:07:02.275Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "5ee245a7-db4a-4f65-9bbc-2d212adc1765",
      "number": 10,
      "content": "Chúng sẽ bị phó cho lưỡi gươm;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.277Z",
      "updatedAt": "2024-09-07T14:07:02.277Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "f7da69c4-36f9-4b30-b96e-c210dbc7d43d",
      "number": 10,
      "content": "Chúng sẽ thành mồi cho muông sói.",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.277Z",
      "updatedAt": "2024-09-07T14:07:02.277Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "001c31c7-e099-4604-b0cb-e97e0b2966ed",
      "number": 11,
      "content": "Nhưng vua sẽ vui mừng trong Đức Chúa Trời;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.279Z",
      "updatedAt": "2024-09-07T14:07:02.279Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "326590ec-918d-4120-ac04-4eecd00d606d",
      "number": 11,
      "content": "Những ai nhân danh Ngài mà thề nguyện sẽ được vinh hiển;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.279Z",
      "updatedAt": "2024-09-07T14:07:02.279Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    },
    {
      "id": "1c2b6674-0493-4138-bec5-abed5cfb46a7",
      "number": 11,
      "content": "Nhưng miệng của những kẻ nói dối phải ngậm lại.",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:07:02.279Z",
      "updatedAt": "2024-09-07T14:07:02.279Z",
      "chapterId": "56e335fe-1b3f-4f38-ac82-a9c86a9869af"
    }
  ],
