{
  "chapter": {
    "id": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45",
    "number": 106,
    "ref": "https://www.bible.com/bible/19/PSA.106.BD2011",
    "createdAt": "2024-09-07T11:51:11.464Z",
    "updatedAt": "2024-09-07T11:51:11.464Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "47403c48-9f77-4682-abfe-35a964fc86bb",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.232Z",
      "updatedAt": "2024-09-07T14:17:24.232Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "d29aba74-8d1f-4ba6-870c-3280a5d9a324",
      "number": 1,
      "content": "Hãy cảm tạ CHÚA, vì Ngài thật tốt,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.232Z",
      "updatedAt": "2024-09-07T14:17:24.232Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "fde628c9-2ca3-4705-ad9b-6c12aed31366",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.232Z",
      "updatedAt": "2024-09-07T14:17:24.232Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "8d1fecdd-8dd5-43b0-a393-2f9d16b4886b",
      "number": 2,
      "content": "Ai có thể thuật lại những việc quyền năng của CHÚA,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.241Z",
      "updatedAt": "2024-09-07T14:17:24.241Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "bbda5f21-7621-4142-bb85-f2a8f50cba5c",
      "number": 2,
      "content": "Hoặc rao ra tất cả lời ca ngợi về Ngài?",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.241Z",
      "updatedAt": "2024-09-07T14:17:24.241Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "9f3ab4bc-c5f1-4344-974d-e7706de7da5c",
      "number": 3,
      "content": "Phước hạnh thay cho những người duy trì công lý,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.244Z",
      "updatedAt": "2024-09-07T14:17:24.244Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "aef000f1-0150-4ec6-9305-31784c9b2297",
      "number": 3,
      "content": "Tức những người làm điều công chính ở mọi lúc mọi nơi!",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.244Z",
      "updatedAt": "2024-09-07T14:17:24.244Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "2850ebc5-f6b2-449a-8eb8-0caa0e47b145",
      "number": 4,
      "content": "CHÚA ôi, xin nhớ đến con khi Ngài ban ơn cho dân Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.247Z",
      "updatedAt": "2024-09-07T14:17:24.247Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "d17dca15-692f-478f-93c5-a35e97e21507",
      "number": 4,
      "content": "Xin đoái đến con khi Ngài ban ơn cứu rỗi của Ngài,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.247Z",
      "updatedAt": "2024-09-07T14:17:24.247Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "bcbf1462-953d-4f4d-ab72-edda58e5b827",
      "number": 5,
      "content": "Để con được thấy sự thịnh vượng của người Ngài chọn,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.249Z",
      "updatedAt": "2024-09-07T14:17:24.249Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "6bd79fa7-0be2-4287-bdf9-66fb51a7e83d",
      "number": 5,
      "content": "Được hân hoan trong niềm vui của người dân trong nước Ngài,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.249Z",
      "updatedAt": "2024-09-07T14:17:24.249Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "a5091cfd-b3f9-40e9-aadb-20a69c6df8a2",
      "number": 5,
      "content": "Và được vinh hiển với những người hưởng cơ nghiệp Ngài.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.249Z",
      "updatedAt": "2024-09-07T14:17:24.249Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "511d950b-9fdb-4b87-87f7-a7d976b9de71",
      "number": 6,
      "content": "Giống như tổ tiên chúng con, chúng con đã phạm tội;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.251Z",
      "updatedAt": "2024-09-07T14:17:24.251Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "479f2634-e508-4828-b25d-56063b8b408d",
      "number": 6,
      "content": "Chúng con đã làm những điều xấu xa tội lỗi; chúng con đã hành động cách gian tà.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.251Z",
      "updatedAt": "2024-09-07T14:17:24.251Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "750c6b3d-716c-4596-b0ec-7f758355a2c8",
      "number": 7,
      "content": "Khi ở Ai-cập tổ tiên chúng con",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.253Z",
      "updatedAt": "2024-09-07T14:17:24.253Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "dae582fd-568d-4418-8022-b26bc559e0c7",
      "number": 7,
      "content": "Đã không nghĩ đến những việc diệu kỳ của Ngài;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.253Z",
      "updatedAt": "2024-09-07T14:17:24.253Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "066ee85e-d341-448d-8e30-777596b791f4",
      "number": 7,
      "content": "Họ đã chẳng nhớ đến lòng thương xót lớn lao của Ngài;",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.253Z",
      "updatedAt": "2024-09-07T14:17:24.253Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "6059a65d-5064-4007-89b6-ae4d24d165c0",
      "number": 7,
      "content": "Họ đã chọc giận Ngài bên bờ biển, tức bên bờ Hồng Hải.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.253Z",
      "updatedAt": "2024-09-07T14:17:24.253Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "9894e146-d651-4dd4-839d-1a33159a9992",
      "number": 8,
      "content": "Dầu vậy vì cớ danh Ngài, Ngài đã cứu họ,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.255Z",
      "updatedAt": "2024-09-07T14:17:24.255Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "cb0c8110-90c4-44a5-8900-0cd8cd48c56a",
      "number": 8,
      "content": "Để quyền năng Ngài được biết đến khắp nơi.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.255Z",
      "updatedAt": "2024-09-07T14:17:24.255Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "f68d0300-4df3-4941-b8e3-f97d2c055ad2",
      "number": 9,
      "content": "Ngài quở trách Hồng Hải, và nó liền khô cạn;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.257Z",
      "updatedAt": "2024-09-07T14:17:24.257Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "463f4edc-1d26-43e9-8c27-a48197c0c3c5",
      "number": 9,
      "content": "Ngài dẫn họ qua biển sâu như qua một đồng hoang.",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.257Z",
      "updatedAt": "2024-09-07T14:17:24.257Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "b64cbda0-26b5-4ca6-9d50-df26998dd6bd",
      "number": 10,
      "content": "Ngài giải cứu họ khỏi tay những kẻ ghét họ,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.259Z",
      "updatedAt": "2024-09-07T14:17:24.259Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "9ce759e3-04a0-4a7e-b65e-31b17474be17",
      "number": 10,
      "content": "Và cứu chuộc họ khỏi tay những kẻ thù của họ.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.259Z",
      "updatedAt": "2024-09-07T14:17:24.259Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "a8ea0178-27d5-42f5-883a-951575dc863a",
      "number": 11,
      "content": "Nước biển đã bao phủ những kẻ thù của họ,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.261Z",
      "updatedAt": "2024-09-07T14:17:24.261Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "e4df3771-f0b3-4407-801a-0d1a5f8306f5",
      "number": 11,
      "content": "Không chừa lại ai, dù chỉ một người.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.261Z",
      "updatedAt": "2024-09-07T14:17:24.261Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "29c4694a-3310-4017-aecf-2beba9fa3e1d",
      "number": 12,
      "content": "Bấy giờ họ mới chịu tin lời Ngài;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.263Z",
      "updatedAt": "2024-09-07T14:17:24.263Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "04044646-ea31-4098-b809-da78dc4c4888",
      "number": 12,
      "content": "Họ mới cất tiếng chúc tụng Ngài.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.263Z",
      "updatedAt": "2024-09-07T14:17:24.263Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "301f47b6-18b7-4f1c-a692-7663777aa21c",
      "number": 13,
      "content": "Nhưng chẳng bao lâu họ lại quên các công việc Ngài;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.266Z",
      "updatedAt": "2024-09-07T14:17:24.266Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "fd3db41b-0509-431f-a5ea-297c5765b7a1",
      "number": 13,
      "content": "Họ chẳng màng đến việc chờ đợi để cầu hỏi ý Ngài.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.266Z",
      "updatedAt": "2024-09-07T14:17:24.266Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "a884c233-bb46-4540-8b54-19964995a051",
      "number": 14,
      "content": "Họ chiều theo dục vọng mình quá đỗi trong đồng hoang;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.268Z",
      "updatedAt": "2024-09-07T14:17:24.268Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "eebad2a0-ee24-41a8-b888-06f0062c10a6",
      "number": 14,
      "content": "Và dám thách đố đến cả Đức Chúa Trời trong sa mạc.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.268Z",
      "updatedAt": "2024-09-07T14:17:24.268Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "3964db5c-92d4-47da-b5d8-4e24d07904c7",
      "number": 15,
      "content": "Ngài cho họ những gì họ đòi hỏi,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.273Z",
      "updatedAt": "2024-09-07T14:17:24.273Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "c3347117-80f1-49f4-8bb0-d2f8446ec363",
      "number": 15,
      "content": "Nhưng cũng sai đến một ôn dịch chết người.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.273Z",
      "updatedAt": "2024-09-07T14:17:24.273Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "d9b45cfd-9596-423d-8047-2b55c2b193fd",
      "number": 16,
      "content": "Trong doanh trại họ ganh tị với Môi-se,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.278Z",
      "updatedAt": "2024-09-07T14:17:24.278Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "ef6e655b-d1a9-42c0-8f55-dcfe27c71b40",
      "number": 16,
      "content": "Và với A-rôn người thánh của CHÚA.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.278Z",
      "updatedAt": "2024-09-07T14:17:24.278Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "6d983512-4002-4b02-ae1d-e0d15168a831",
      "number": 17,
      "content": "Đất mở miệng ra và nuốt chửng Đa-than,",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.283Z",
      "updatedAt": "2024-09-07T14:17:24.283Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "7eb770d7-619d-4fd3-bd46-9d4c435ab8fa",
      "number": 17,
      "content": "Và ngoạm lấy đồng bọn của A-bi-ram.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.283Z",
      "updatedAt": "2024-09-07T14:17:24.283Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "6466a92b-43c1-4598-b520-b7c91886a4cb",
      "number": 18,
      "content": "Một ngọn lửa đã bùng lên giữa đám người phản loạn;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.288Z",
      "updatedAt": "2024-09-07T14:17:24.288Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "ec430dae-1bac-40a0-a478-7ca07f1fb06a",
      "number": 18,
      "content": "Ngọn lửa hừng đã thiêu rụi những kẻ gian tà.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.288Z",
      "updatedAt": "2024-09-07T14:17:24.288Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "5f8b3bb7-a98f-4c7e-ba00-c77d198d30b0",
      "number": 19,
      "content": "Họ đã đúc một tượng bò con ở Hô-rếp,",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.293Z",
      "updatedAt": "2024-09-07T14:17:24.293Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "f7aeb3c5-1dc4-4570-a33d-3b1e18983afa",
      "number": 19,
      "content": "Rồi quỳ mọp xuống thờ lạy cái tượng đúc bằng vàng.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.293Z",
      "updatedAt": "2024-09-07T14:17:24.293Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "5eee178f-d070-409d-b86b-c6da7be25ffe",
      "number": 20,
      "content": "Họ đã đánh đổi vinh hiển mình cho hình tượng một con bò,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.299Z",
      "updatedAt": "2024-09-07T14:17:24.299Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "ea6c09ba-ff23-49e7-90c3-42a481708b1c",
      "number": 20,
      "content": "Một con vật ăn cỏ.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.299Z",
      "updatedAt": "2024-09-07T14:17:24.299Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "db5a484c-5f8d-4346-9ef8-08f07e3b0d5c",
      "number": 21,
      "content": "Họ quên rằng Đức Chúa Trời là Đấng Giải Cứu họ,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.304Z",
      "updatedAt": "2024-09-07T14:17:24.304Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "f888a4f4-14f2-4671-9a7e-ffe73c3da857",
      "number": 21,
      "content": "Đấng làm những việc lớn lao ở Ai-cập,",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.304Z",
      "updatedAt": "2024-09-07T14:17:24.304Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "b008717c-5fc3-4649-9453-a4f8e26af0ad",
      "number": 22,
      "content": "Những việc lạ lùng trong xứ của Cham,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.309Z",
      "updatedAt": "2024-09-07T14:17:24.309Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "5147e001-0cf7-4f73-8ffd-adee701c919f",
      "number": 22,
      "content": "Những việc kinh hoàng bên bờ Hồng Hải.",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.309Z",
      "updatedAt": "2024-09-07T14:17:24.309Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "7f3d3d7b-966b-4099-acf8-528e691541c5",
      "number": 23,
      "content": "Vì thế Ngài phán rằng Ngài sẽ tiêu diệt họ;",
      "order": 0,
      "parNumber": 49,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.314Z",
      "updatedAt": "2024-09-07T14:17:24.314Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "7a422a9a-06b4-4900-a02b-fd84c82f9b55",
      "number": 23,
      "content": "May nhờ có Môi-se, người Ngài đã chọn,",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.314Z",
      "updatedAt": "2024-09-07T14:17:24.314Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "d96b48bc-46f1-4a01-8d11-a36422529abd",
      "number": 23,
      "content": "Đứng mũi chịu sào,",
      "order": 2,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.314Z",
      "updatedAt": "2024-09-07T14:17:24.314Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "59d5bf2a-1286-4158-84ec-502dd386520a",
      "number": 23,
      "content": "Để van nài Ngài nguôi ngoai cơn giận mà không diệt họ đi.",
      "order": 3,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.314Z",
      "updatedAt": "2024-09-07T14:17:24.314Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "23e9651f-0a15-4848-8a04-4a54fd696d09",
      "number": 24,
      "content": "Kế đến họ khinh chê đất hứa;",
      "order": 0,
      "parNumber": 53,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.320Z",
      "updatedAt": "2024-09-07T14:17:24.320Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "282c0afd-ba9b-4321-872e-9f151565d3fe",
      "number": 24,
      "content": "Họ không tin vào lời hứa của Ngài.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.320Z",
      "updatedAt": "2024-09-07T14:17:24.320Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "cc84bbe3-0bd6-46dd-840e-2c586a09be56",
      "number": 25,
      "content": "Trong lều họ, họ lầm bầm oán trách;",
      "order": 0,
      "parNumber": 55,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.325Z",
      "updatedAt": "2024-09-07T14:17:24.325Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "fe0f2382-f79f-47a9-8855-e29538562f5f",
      "number": 25,
      "content": "Chẳng muốn nghe tiếng CHÚA phán truyền.",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.325Z",
      "updatedAt": "2024-09-07T14:17:24.325Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "033a0738-5f17-4b51-9ed4-5de3551061aa",
      "number": 26,
      "content": "Vì thế Ngài đã đưa tay lên và thề rằng,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.330Z",
      "updatedAt": "2024-09-07T14:17:24.330Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "687369d1-c126-4a45-ae4e-3f65ab96a243",
      "number": 26,
      "content": "Ngài sẽ cho họ bỏ thây trong sa mạc,",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.330Z",
      "updatedAt": "2024-09-07T14:17:24.330Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "ceed289f-37af-43c0-8a8d-97ed497016ff",
      "number": 27,
      "content": "Rải con cháu họ ra trong các quốc gia,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.335Z",
      "updatedAt": "2024-09-07T14:17:24.335Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "2c8c8f22-270b-42eb-a7e2-3d9c625cf06c",
      "number": 27,
      "content": "Và làm cho họ tản lạc khắp nơi trong các nước.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.335Z",
      "updatedAt": "2024-09-07T14:17:24.335Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "2e52d123-413d-46e9-b09a-f56a0b39116f",
      "number": 28,
      "content": "Đã thế họ còn thờ lạy Ba-anh Pê-ô;",
      "order": 0,
      "parNumber": 61,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.340Z",
      "updatedAt": "2024-09-07T14:17:24.340Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "fa4afb9f-e8ae-4569-957b-c4c9be06b059",
      "number": 28,
      "content": "Và ăn đồ cúng cho người chết.",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.340Z",
      "updatedAt": "2024-09-07T14:17:24.340Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "731c6cbe-dc3c-4ab1-baf7-9017c6cb0b83",
      "number": 29,
      "content": "Họ đã dùng các hành động mình để chọc giận Ngài;",
      "order": 0,
      "parNumber": 63,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.345Z",
      "updatedAt": "2024-09-07T14:17:24.345Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "bacfa04c-ccfe-4dfc-9d3f-868820a83600",
      "number": 29,
      "content": "Nên ôn dịch đã bộc phát giữa họ.",
      "order": 1,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.345Z",
      "updatedAt": "2024-09-07T14:17:24.345Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "7536f246-9513-4a21-b6cb-fbc02ff1a63e",
      "number": 30,
      "content": "May nhờ có Phi-nê-a đứng dậy và cầu thay,",
      "order": 0,
      "parNumber": 65,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.350Z",
      "updatedAt": "2024-09-07T14:17:24.350Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "a7b58cee-8bc4-4af5-8269-6d359b355521",
      "number": 30,
      "content": "Nên bệnh dịch mới ngừng ngay tại chỗ.",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.350Z",
      "updatedAt": "2024-09-07T14:17:24.350Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "0be74e3b-8934-4849-9b3b-ac8aa9e58667",
      "number": 31,
      "content": "Điều ấy được xem là công đức của ông ta,",
      "order": 0,
      "parNumber": 67,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.355Z",
      "updatedAt": "2024-09-07T14:17:24.355Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "f0e5dee3-2028-48e8-8084-b8c2f9abd5af",
      "number": 31,
      "content": "Và được ghi nhớ mãi mãi trải qua mọi thế hệ.",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.355Z",
      "updatedAt": "2024-09-07T14:17:24.355Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "23923b61-c4e6-4b8d-87dd-9ba08fb9ec59",
      "number": 32,
      "content": "Họ đã chọc giận Ngài trong vụ nước ở Mê-ri-ba,",
      "order": 0,
      "parNumber": 69,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.360Z",
      "updatedAt": "2024-09-07T14:17:24.360Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "e7c7d09f-3415-49d2-8e3f-490492429a1d",
      "number": 32,
      "content": "Cũng tại vì họ mà Môi-se đã mang lụy vào thân.",
      "order": 1,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.360Z",
      "updatedAt": "2024-09-07T14:17:24.360Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "47238a8b-9a4e-42df-9351-816d3fa3eee2",
      "number": 33,
      "content": "Bởi họ đã làm cho tâm thần ông bực dọc,",
      "order": 0,
      "parNumber": 71,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.365Z",
      "updatedAt": "2024-09-07T14:17:24.365Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "3bd279f7-6293-408b-8aa9-1f56ace4ad95",
      "number": 33,
      "content": "Nên môi ông đã nói những lời thiếu đắn đo suy nghĩ.",
      "order": 1,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.365Z",
      "updatedAt": "2024-09-07T14:17:24.365Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "0f832956-6bef-4c5c-933d-7d59709dba84",
      "number": 34,
      "content": "Họ cũng chẳng tiêu diệt các dân trong xứ,",
      "order": 0,
      "parNumber": 73,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.370Z",
      "updatedAt": "2024-09-07T14:17:24.370Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "87ebee2c-acbb-47f3-bd2a-61b6e4c0853f",
      "number": 34,
      "content": "Để làm đúng theo mạng lịnh CHÚA đã truyền,",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.370Z",
      "updatedAt": "2024-09-07T14:17:24.370Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "21f6445e-4036-48fc-a8a8-fbd07876ad5f",
      "number": 35,
      "content": "Nhưng họ đã sống chung lộn với các dân tộc ấy,",
      "order": 0,
      "parNumber": 75,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.375Z",
      "updatedAt": "2024-09-07T14:17:24.375Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "d6fda828-4eec-4134-ba9c-5595bde3d994",
      "number": 35,
      "content": "Và học theo các thói tục của chúng.",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.375Z",
      "updatedAt": "2024-09-07T14:17:24.375Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "d8dfebe0-328f-4542-a34d-08d82f10b41a",
      "number": 36,
      "content": "Đã thế họ còn thờ lạy các thần tượng của chúng;",
      "order": 0,
      "parNumber": 77,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.380Z",
      "updatedAt": "2024-09-07T14:17:24.380Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "c62ddf06-fdd1-4a00-82ff-1bf047803b24",
      "number": 36,
      "content": "Các thần tượng ấy đã trở thành cái bẫy cho họ.",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.380Z",
      "updatedAt": "2024-09-07T14:17:24.380Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "03b3b740-9159-4df2-974b-b871dd3a40ec",
      "number": 37,
      "content": "Thậm chí họ còn sát tế các con trai và các con gái mình,",
      "order": 0,
      "parNumber": 79,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.386Z",
      "updatedAt": "2024-09-07T14:17:24.386Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "4bb94731-3013-4e45-9f1c-42bf866a533a",
      "number": 37,
      "content": "Để cúng tế cho các tà linh ma quỷ.",
      "order": 1,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.386Z",
      "updatedAt": "2024-09-07T14:17:24.386Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "000bf929-13e5-44ed-8f60-71f790c42f3f",
      "number": 38,
      "content": "Họ đã làm đổ máu vô tội ra,",
      "order": 0,
      "parNumber": 81,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.391Z",
      "updatedAt": "2024-09-07T14:17:24.391Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "d020c2d6-85c3-41de-9f5e-77ad945872fc",
      "number": 38,
      "content": "Tức máu của các con trai và các con gái họ,",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.391Z",
      "updatedAt": "2024-09-07T14:17:24.391Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "18a58bb0-03fd-4b5f-a6e1-88e628f17611",
      "number": 38,
      "content": "Những trẻ thơ đã bị làm vật tế thần cho các thần tượng xứ Ca-na-an;",
      "order": 2,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.391Z",
      "updatedAt": "2024-09-07T14:17:24.391Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "d9c8a682-3daa-4b8d-8cdc-0520c1f33dbd",
      "number": 38,
      "content": "Và đất đã bị ô nhiễm vì máu các trẻ thơ ấy.",
      "order": 3,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.391Z",
      "updatedAt": "2024-09-07T14:17:24.391Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "2a625d72-7a03-4039-8091-9d607329a872",
      "number": 39,
      "content": "Như vậy họ đã làm cho mình ra ô uế vì các hành động mình;",
      "order": 0,
      "parNumber": 85,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.396Z",
      "updatedAt": "2024-09-07T14:17:24.396Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "76c81d29-6712-4e39-bdec-1db68839b9e5",
      "number": 39,
      "content": "Và biến mình thành đĩ điếm qua các hành vi mình.",
      "order": 1,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.396Z",
      "updatedAt": "2024-09-07T14:17:24.396Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "25d08d28-00f8-4c1e-89e3-6a6d3c9b2394",
      "number": 40,
      "content": "Vì thế cơn giận của CHÚA đã nổi lên nghịch lại dân Ngài;",
      "order": 0,
      "parNumber": 87,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.401Z",
      "updatedAt": "2024-09-07T14:17:24.401Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "728cfe98-7cf3-4b82-a996-eb21f6f61ca7",
      "number": 40,
      "content": "Và Ngài ghê tởm cơ nghiệp Ngài.",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.401Z",
      "updatedAt": "2024-09-07T14:17:24.401Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "dc8120a5-cd2c-403d-87b1-873bd928b7fc",
      "number": 41,
      "content": "Ngài phó họ vào tay các dân ngoại,",
      "order": 0,
      "parNumber": 89,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.406Z",
      "updatedAt": "2024-09-07T14:17:24.406Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "e2ddf1e3-dd03-4e27-ac38-c64250d28afa",
      "number": 41,
      "content": "Để cho những kẻ ghét họ cai trị họ.",
      "order": 1,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.406Z",
      "updatedAt": "2024-09-07T14:17:24.406Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "8bb01c31-6107-4f53-9121-9bd2bada6e07",
      "number": 42,
      "content": "Những kẻ thù của họ hà hiếp họ;",
      "order": 0,
      "parNumber": 91,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.411Z",
      "updatedAt": "2024-09-07T14:17:24.411Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "9e3eb1bf-68c8-45dd-b897-a4c33d22af20",
      "number": 42,
      "content": "Họ bị khuất phục dưới tay chúng.",
      "order": 1,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.411Z",
      "updatedAt": "2024-09-07T14:17:24.411Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "fdae6127-d039-432f-a128-f66024092c98",
      "number": 43,
      "content": "Dù nhiều lần Ngài đã giải cứu họ,",
      "order": 0,
      "parNumber": 93,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.415Z",
      "updatedAt": "2024-09-07T14:17:24.415Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "72c90fcd-28d8-4c8c-adf4-74445bb7ca17",
      "number": 43,
      "content": "Nhưng họ vẫn bàn với nhau để tìm cách phản bội Ngài,",
      "order": 1,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.415Z",
      "updatedAt": "2024-09-07T14:17:24.415Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "1c95c9fd-aede-41f4-8347-9994e30627b4",
      "number": 43,
      "content": "Và cứ thế họ càng ngày càng lún sâu trong tội lỗi.",
      "order": 2,
      "parNumber": 95,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.415Z",
      "updatedAt": "2024-09-07T14:17:24.415Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "d38c28ef-630f-48d1-9317-b9033f712da7",
      "number": 44,
      "content": "Dầu vậy Ngài đã đoái xem cảnh khốn cùng của họ,",
      "order": 0,
      "parNumber": 96,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.419Z",
      "updatedAt": "2024-09-07T14:17:24.419Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "9bd4d603-a5bb-4cf9-b96b-3bfabcc7ae82",
      "number": 44,
      "content": "Khi Ngài nghe tiếng họ kêu cầu.",
      "order": 1,
      "parNumber": 97,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.419Z",
      "updatedAt": "2024-09-07T14:17:24.419Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "f2b0eb88-8f61-49cb-ba3e-248ae45dae34",
      "number": 45,
      "content": "Ngài nhớ lại giao ước Ngài vì cớ họ,",
      "order": 0,
      "parNumber": 98,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.423Z",
      "updatedAt": "2024-09-07T14:17:24.423Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "b973b384-8127-4474-8a5e-c643f5ca84d5",
      "number": 45,
      "content": "Và dủ lòng thương theo ơn thương xót vô lượng của Ngài.",
      "order": 1,
      "parNumber": 99,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.423Z",
      "updatedAt": "2024-09-07T14:17:24.423Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "6d72a754-8a07-48ec-be8a-5324e762e789",
      "number": 46,
      "content": "Ngài cũng làm cho họ được người ta thương hại,",
      "order": 0,
      "parNumber": 100,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.427Z",
      "updatedAt": "2024-09-07T14:17:24.427Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "d953cd4a-e9d6-4038-b0c8-2bfa3227f4bc",
      "number": 46,
      "content": "Tức mọi kẻ bắt họ đem lưu đày.",
      "order": 1,
      "parNumber": 101,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.427Z",
      "updatedAt": "2024-09-07T14:17:24.427Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "7f0d2e5f-96d6-44e6-b84d-36da7885a3ed",
      "number": 47,
      "content": "Lạy CHÚA, Đức Chúa Trời của chúng con, xin giải cứu chúng con,",
      "order": 0,
      "parNumber": 102,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.431Z",
      "updatedAt": "2024-09-07T14:17:24.431Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "f2622641-1b59-4ceb-bdfc-a7099a77d13a",
      "number": 47,
      "content": "Xin đem chúng con từ giữa các dân ngoại trở về,",
      "order": 1,
      "parNumber": 103,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.431Z",
      "updatedAt": "2024-09-07T14:17:24.431Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "498bf749-0b43-4731-95da-cbbd6792ba96",
      "number": 47,
      "content": "Để chúng con cảm tạ danh thánh Ngài,",
      "order": 2,
      "parNumber": 104,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.431Z",
      "updatedAt": "2024-09-07T14:17:24.431Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "96664547-9201-4c2a-b88c-5c26db38cec4",
      "number": 47,
      "content": "Và tôn vinh ca ngợi Ngài.",
      "order": 3,
      "parNumber": 105,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.431Z",
      "updatedAt": "2024-09-07T14:17:24.431Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "fb35e190-5cf9-4001-8848-059fdcd2a687",
      "number": 48,
      "content": "Chúc tụng CHÚA, Đức Chúa Trời của I-sơ-ra-ên,",
      "order": 0,
      "parNumber": 106,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.435Z",
      "updatedAt": "2024-09-07T14:17:24.435Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "563ecc0e-9fb7-4c32-8444-d4f078737ed8",
      "number": 48,
      "content": "Từ đời đời cho đến đời đời.",
      "order": 1,
      "parNumber": 107,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.435Z",
      "updatedAt": "2024-09-07T14:17:24.435Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "8dc7e052-0b02-455c-bde0-a631dbcb6530",
      "number": 48,
      "content": "Xin mọi người hãy nói, “A-men!”",
      "order": 2,
      "parNumber": 108,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.435Z",
      "updatedAt": "2024-09-07T14:17:24.435Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    },
    {
      "id": "5740dc41-e00e-4fa2-94a7-51c782386c3b",
      "number": 48,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 3,
      "parNumber": 109,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:24.435Z",
      "updatedAt": "2024-09-07T14:17:24.435Z",
      "chapterId": "8a49ebd3-3555-4278-80f2-ca51b3a8ae45"
    }
  ],
