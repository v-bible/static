{
  "chapter": {
    "id": "e0104e37-1f03-4b18-b541-93d072d81329",
    "number": 70,
    "ref": "https://www.bible.com/bible/19/PSA.70.BD2011",
    "createdAt": "2024-09-07T11:51:11.166Z",
    "updatedAt": "2024-09-07T11:51:11.166Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "ded9fbc1-1718-44fc-987f-f2ab2f8ee5d6",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin mau mau giải cứu con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.724Z",
      "updatedAt": "2024-09-07T14:10:10.724Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "fae1af52-3f37-427e-b228-01a5cf20ff36",
      "number": 1,
      "content": "CHÚA ôi, xin nhanh chóng đến giúp đỡ con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.724Z",
      "updatedAt": "2024-09-07T14:10:10.724Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "c011d548-112c-43d0-97af-5376fb5e2603",
      "number": 2,
      "content": "Nguyện những kẻ tìm cách tiêu diệt con",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.736Z",
      "updatedAt": "2024-09-07T14:10:10.736Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "eca40d83-47c0-4467-aaea-8c9a047f344e",
      "number": 2,
      "content": "Bị hổ thẹn và bối rối;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.736Z",
      "updatedAt": "2024-09-07T14:10:10.736Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "c05d483f-2a4c-4752-8928-9ccf5e256da7",
      "number": 2,
      "content": "Nguyện những kẻ muốn làm tổn thương con",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.736Z",
      "updatedAt": "2024-09-07T14:10:10.736Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "3923a23b-98b1-4c9a-9d37-fa5afd5327a7",
      "number": 2,
      "content": "Phải bỏ đi trong sỉ nhục.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.736Z",
      "updatedAt": "2024-09-07T14:10:10.736Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "e922ef09-2c8e-4736-9d82-367e67ade7d6",
      "number": 3,
      "content": "Nguyện những kẻ nhạo cười con rằng, “Ha ha! Ha ha!”",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.739Z",
      "updatedAt": "2024-09-07T14:10:10.739Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "bfe94d85-3d3e-47b1-abd2-1610e4068538",
      "number": 3,
      "content": "Phải rút lui vì nhục nhã.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.739Z",
      "updatedAt": "2024-09-07T14:10:10.739Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "b919da5e-ca7e-46a5-9838-7e5bd0d2aa57",
      "number": 4,
      "content": "Nguyện những ai tìm kiếm Ngài",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.742Z",
      "updatedAt": "2024-09-07T14:10:10.742Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "be30dd9d-8eea-4ed6-b594-689b1eab893e",
      "number": 4,
      "content": "Vui vẻ và mừng rỡ trong Ngài;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.742Z",
      "updatedAt": "2024-09-07T14:10:10.742Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "a9c42ee4-9543-4d54-a75e-2e560d717af5",
      "number": 4,
      "content": "Nguyện những ai yêu mến ơn cứu rỗi của Ngài",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.742Z",
      "updatedAt": "2024-09-07T14:10:10.742Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "51c87c0c-a607-4033-af1e-b12312cfee7b",
      "number": 4,
      "content": "Cứ nói mãi, “Đức Chúa Trời vĩ đại thay!”",
      "order": 3,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.742Z",
      "updatedAt": "2024-09-07T14:10:10.742Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "a5bfaf74-3bc3-497f-b486-c1dddc024c16",
      "number": 5,
      "content": "Còn con, con đang gặp cảnh khốn khổ và cùng đường;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.744Z",
      "updatedAt": "2024-09-07T14:10:10.744Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "3604ef1a-8af7-4174-a153-6f822ba3f64f",
      "number": 5,
      "content": "Đức Chúa Trời ôi, xin mau mau đến với con.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.744Z",
      "updatedAt": "2024-09-07T14:10:10.744Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "b100ab19-a826-490d-b2a9-8d6279102049",
      "number": 5,
      "content": "Ngài là Đấng Giúp Đỡ và Đấng Giải Cứu của con;",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.744Z",
      "updatedAt": "2024-09-07T14:10:10.744Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    },
    {
      "id": "1ba43c59-51ea-49f6-bedd-9d80008f17f0",
      "number": 5,
      "content": "CHÚA ôi, xin đừng trì hoãn.",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:10.744Z",
      "updatedAt": "2024-09-07T14:10:10.744Z",
      "chapterId": "e0104e37-1f03-4b18-b541-93d072d81329"
    }
  ],
