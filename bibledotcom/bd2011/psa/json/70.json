{
  "chapter": {
    "id": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e",
    "number": 70,
    "ref": "https://www.bible.com/bible/19/PSA.70.BD2011",
    "createdAt": "2024-09-23T05:08:40.453Z",
    "updatedAt": "2024-09-23T05:08:40.453Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "f073fb95-c877-4a2e-8f1a-5b0d1517842a",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin mau mau giải cứu con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.779Z",
      "updatedAt": "2024-09-23T05:49:49.779Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "22b6d11e-f7ac-4491-80d8-fac7cc9d2a91",
      "number": 1,
      "content": "**CHÚA** ôi, xin nhanh chóng đến giúp đỡ con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.800Z",
      "updatedAt": "2024-09-23T05:49:49.800Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "52cfd751-5ab3-479f-813f-5ecc3528e191",
      "number": 2,
      "content": "Nguyện những kẻ tìm cách tiêu diệt con",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.804Z",
      "updatedAt": "2024-09-23T05:49:49.804Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "36b80950-0140-4c72-84b2-45796afc43d9",
      "number": 2,
      "content": "Bị hổ thẹn và bối rối;",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.809Z",
      "updatedAt": "2024-09-23T05:49:49.809Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "8f127657-9101-4c63-8b84-32447a8d057d",
      "number": 2,
      "content": "Nguyện những kẻ muốn làm tổn thương con",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.813Z",
      "updatedAt": "2024-09-23T05:49:49.813Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "34617318-c497-4102-9f6b-f7089c75c143",
      "number": 2,
      "content": "Phải bỏ đi trong sỉ nhục.",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.819Z",
      "updatedAt": "2024-09-23T05:49:49.819Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "db8be785-f587-4e60-89d3-d8ab0d1f8515",
      "number": 3,
      "content": "Nguyện những kẻ nhạo cười con rằng, “Ha ha! Ha ha!”",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.823Z",
      "updatedAt": "2024-09-23T05:49:49.823Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "fbfea516-1781-438c-b454-cfd2e50a55ca",
      "number": 3,
      "content": "Phải rút lui vì nhục nhã.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.828Z",
      "updatedAt": "2024-09-23T05:49:49.828Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "6f3d1d3e-7046-4172-a86c-8f2f1a6e352e",
      "number": 4,
      "content": "Nguyện những ai tìm kiếm Ngài",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.832Z",
      "updatedAt": "2024-09-23T05:49:49.832Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "688d67f3-d152-4d93-a204-debe3c211536",
      "number": 4,
      "content": "Vui vẻ và mừng rỡ trong Ngài;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.837Z",
      "updatedAt": "2024-09-23T05:49:49.837Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "00adf72d-27bf-41e0-b786-9a11ef6dfe82",
      "number": 4,
      "content": "Nguyện những ai yêu mến ơn cứu rỗi của Ngài",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.842Z",
      "updatedAt": "2024-09-23T05:49:49.842Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "7b991d6b-386b-48ca-9b74-9ae199956c59",
      "number": 4,
      "content": "Cứ nói mãi, “Đức Chúa Trời vĩ đại thay!”",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.847Z",
      "updatedAt": "2024-09-23T05:49:49.847Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "b8577fed-0dc6-4882-b94a-10e4f83b9062",
      "number": 5,
      "content": "Còn con, con đang gặp cảnh khốn khổ và cùng đường;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.852Z",
      "updatedAt": "2024-09-23T05:49:49.852Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "bfb4a396-41d4-4b95-86cf-d61471129464",
      "number": 5,
      "content": "Đức Chúa Trời ôi, xin mau mau đến với con.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.857Z",
      "updatedAt": "2024-09-23T05:49:49.857Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "41175d74-cf4e-4f9a-a68e-9d4d309a4a61",
      "number": 5,
      "content": "Ngài là Đấng Giúp Đỡ và Đấng Giải Cứu của con;",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.862Z",
      "updatedAt": "2024-09-23T05:49:49.862Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    },
    {
      "id": "661d5ad4-b9c9-4af7-a6e1-305c6d684de4",
      "number": 5,
      "content": "**CHÚA** ôi, xin đừng trì hoãn.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:49.867Z",
      "updatedAt": "2024-09-23T05:49:49.867Z",
      "chapterId": "7fa1ecc2-de3b-4fe9-b9b5-a4d6f3a8af8e"
    }
  ],
