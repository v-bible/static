{
  "chapter": {
    "id": "df0b49de-5b93-4c15-aaad-823f58f18000",
    "number": 146,
    "ref": "https://www.bible.com/bible/19/PSA.146.BD2011",
    "createdAt": "2024-09-07T11:51:11.588Z",
    "updatedAt": "2024-09-07T11:51:11.588Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "13a86bea-9b6b-43a4-9974-9b3a8b1318cf",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.903Z",
      "updatedAt": "2024-09-07T14:28:58.903Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "690a08ae-b165-4131-9f67-eaf0e9fed939",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi CHÚA!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.903Z",
      "updatedAt": "2024-09-07T14:28:58.903Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "f5b54fdc-9acc-4a3c-a68e-8409411f7ef6",
      "number": 2,
      "content": "Tôi sẽ ca ngợi CHÚA trọn đời mình;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.913Z",
      "updatedAt": "2024-09-07T14:28:58.913Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "ff9e4543-98e1-4d6c-bf34-0e3ff5d60731",
      "number": 2,
      "content": "Hễ tôi còn sống bao lâu, tôi sẽ ca hát tôn ngợi Đức Chúa Trời của tôi bấy lâu.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.913Z",
      "updatedAt": "2024-09-07T14:28:58.913Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "5ea1892c-7a0d-46df-8b30-3ff8032744dc",
      "number": 3,
      "content": "Đừng để lòng tin cậy vào những người quyền thế;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.915Z",
      "updatedAt": "2024-09-07T14:28:58.915Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "0c82cfe3-50d6-4627-a146-76b94ed37e37",
      "number": 3,
      "content": "Đừng tin cậy loài người là những kẻ không đủ khả năng cứu giúp.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.915Z",
      "updatedAt": "2024-09-07T14:28:58.915Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "bd46fd20-bc05-4f4f-a586-2bc5d61b85ff",
      "number": 4,
      "content": "Hơi thở tắt đi, thân xác nó liền trở về cát bụi;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.918Z",
      "updatedAt": "2024-09-07T14:28:58.918Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "2d97c57a-5ac8-4457-9cec-0c37fd4ce458",
      "number": 4,
      "content": "Trong chính ngày đó các toan tính của nó đều tiêu tan.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.918Z",
      "updatedAt": "2024-09-07T14:28:58.918Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "b54a339f-9171-4ae7-babc-c7cae725cc48",
      "number": 5,
      "content": "Phước cho người được Đức Chúa Trời của Gia-cốp giúp đỡ mình,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.920Z",
      "updatedAt": "2024-09-07T14:28:58.920Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "d891ce40-f34c-468d-8ffa-e668621c8f49",
      "number": 5,
      "content": "Người đặt hy vọng nơi CHÚA, Đức Chúa Trời mình.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.920Z",
      "updatedAt": "2024-09-07T14:28:58.920Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "68e0c721-c08f-4817-b481-5ebd074515d8",
      "number": 6,
      "content": "Ngài là Đấng dựng nên trời và đất,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.923Z",
      "updatedAt": "2024-09-07T14:28:58.923Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "dc99f2b8-bbef-4da4-be84-ce8bef7f5052",
      "number": 6,
      "content": "Biển và mọi vật trong đó.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.923Z",
      "updatedAt": "2024-09-07T14:28:58.923Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "caff9ced-938f-4581-9b6f-322225b2360a",
      "number": 6,
      "content": "Ngài giữ sự thành tín đến đời đời.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.923Z",
      "updatedAt": "2024-09-07T14:28:58.923Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "fdae63d2-8935-4e90-a4a5-e0b260dc9447",
      "number": 7,
      "content": "Ngài thi hành công lý cho người bị áp bức;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.926Z",
      "updatedAt": "2024-09-07T14:28:58.926Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "e3a225e0-ee11-4273-a84a-236231a6ccb6",
      "number": 7,
      "content": "Ngài ban bánh cho người đói.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.926Z",
      "updatedAt": "2024-09-07T14:28:58.926Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "124f6562-4433-414f-81e5-1b870a758ddf",
      "number": 7,
      "content": "CHÚA giải thoát người bị giam cầm.",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.926Z",
      "updatedAt": "2024-09-07T14:28:58.926Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "3c6c7e49-9933-472b-bd1e-10994b9d1dad",
      "number": 8,
      "content": "CHÚA mở mắt người bị mù;",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.929Z",
      "updatedAt": "2024-09-07T14:28:58.929Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "4a4bc4ac-7d5d-448c-af38-43166976bdb3",
      "number": 8,
      "content": "CHÚA đỡ dậy những ai bị bắt phải sấp mình;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.929Z",
      "updatedAt": "2024-09-07T14:28:58.929Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "4687b3ae-3160-4cb3-a086-550e8f54dd1b",
      "number": 8,
      "content": "CHÚA yêu mến người ngay lành;",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.929Z",
      "updatedAt": "2024-09-07T14:28:58.929Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "43b8c384-2800-42a9-a9a8-b34ad62046e5",
      "number": 9,
      "content": "CHÚA bảo vệ những kiều dân;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.931Z",
      "updatedAt": "2024-09-07T14:28:58.931Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "70b06629-50ca-4170-80c8-7998da111be5",
      "number": 9,
      "content": "Ngài nâng đỡ cô nhi và quả phụ;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.931Z",
      "updatedAt": "2024-09-07T14:28:58.931Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "f9f8986d-06c3-417a-8763-1708af1840f8",
      "number": 9,
      "content": "Nhưng Ngài phá hỏng đường lối của những kẻ ác.",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.931Z",
      "updatedAt": "2024-09-07T14:28:58.931Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "2273d873-b368-431f-886c-571d2ad02531",
      "number": 10,
      "content": "CHÚA sẽ trị vì đời đời;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.933Z",
      "updatedAt": "2024-09-07T14:28:58.933Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "1d1bb326-6859-43f6-a79c-e4cc049da73a",
      "number": 10,
      "content": "Hỡi Si-ôn, Đức Chúa Trời của ngươi sẽ trị vì đời đời.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.933Z",
      "updatedAt": "2024-09-07T14:28:58.933Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    },
    {
      "id": "bf298add-2f42-4d38-8fa9-1473fe016209",
      "number": 10,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:28:58.933Z",
      "updatedAt": "2024-09-07T14:28:58.933Z",
      "chapterId": "df0b49de-5b93-4c15-aaad-823f58f18000"
    }
  ],
