{
  "chapter": {
    "id": "77f63f33-7eeb-4722-8269-88c6bc3c313f",
    "number": 100,
    "ref": "https://www.bible.com/bible/19/PSA.100.BD2011",
    "createdAt": "2024-09-07T11:51:11.416Z",
    "updatedAt": "2024-09-07T11:51:11.416Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "4a19b964-fc9c-4657-89fc-02329703dac3",
      "number": 1,
      "content": "Hỡi cả địa cầu, hãy lớn tiếng reo mừng tung hô CHÚA.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.463Z",
      "updatedAt": "2024-09-07T14:16:38.463Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    },
    {
      "id": "09f679eb-a12f-4082-9ba3-a1e204f4a2bc",
      "number": 2,
      "content": "Hãy thờ phượng  CHÚA cách vui mừng;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.472Z",
      "updatedAt": "2024-09-07T14:16:38.472Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    },
    {
      "id": "edea50d8-eb1f-4886-bfd1-839052b5c15a",
      "number": 2,
      "content": "Hãy đến trước thánh nhan Ngài với tiếng hoan ca.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.472Z",
      "updatedAt": "2024-09-07T14:16:38.472Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    },
    {
      "id": "ee606dad-f60e-409d-b1fc-6cef10060eea",
      "number": 3,
      "content": "Khá biết rằng CHÚA là Đức Chúa Trời.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.475Z",
      "updatedAt": "2024-09-07T14:16:38.475Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    },
    {
      "id": "29aa05fa-f7ff-401b-bb6b-5ea87d0585a0",
      "number": 3,
      "content": "Chính Ngài đã dựng nên chúng ta; chúng ta thuộc về Ngài;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.475Z",
      "updatedAt": "2024-09-07T14:16:38.475Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    },
    {
      "id": "b8753a23-5430-45c0-a14b-a65f1d1baf15",
      "number": 3,
      "content": "Chúng ta là con dân Ngài và bầy chiên của đồng cỏ Ngài.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.475Z",
      "updatedAt": "2024-09-07T14:16:38.475Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    },
    {
      "id": "5616a2fa-4a63-43fc-bcc0-24c4feef8204",
      "number": 4,
      "content": "Hãy cảm tạ mà vào các cổng Ngài;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.478Z",
      "updatedAt": "2024-09-07T14:16:38.478Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    },
    {
      "id": "b2953b09-e50d-4146-94dc-5ee4278d2e7d",
      "number": 4,
      "content": "Khá ngợi ca mà vào các sân Ngài;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.478Z",
      "updatedAt": "2024-09-07T14:16:38.478Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    },
    {
      "id": "7515030e-ad8b-4cf8-a865-230d6e3ff2b3",
      "number": 4,
      "content": "Hãy tạ ơn Ngài, khá chúc tụng danh Ngài.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.478Z",
      "updatedAt": "2024-09-07T14:16:38.478Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    },
    {
      "id": "94f4b40b-56e7-44c3-a54c-0f47370cba5a",
      "number": 5,
      "content": "Vì CHÚA thật tốt;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.480Z",
      "updatedAt": "2024-09-07T14:16:38.480Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    },
    {
      "id": "fb3650ff-1e19-40be-9113-70181803bde7",
      "number": 5,
      "content": "Tình thương của Ngài hằng còn mãi mãi;",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.480Z",
      "updatedAt": "2024-09-07T14:16:38.480Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    },
    {
      "id": "fbfd81bb-4b07-4ea7-a1bd-883eacd0804c",
      "number": 5,
      "content": "Đức thành tín của Ngài còn đến muôn đời.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:38.480Z",
      "updatedAt": "2024-09-07T14:16:38.480Z",
      "chapterId": "77f63f33-7eeb-4722-8269-88c6bc3c313f"
    }
  ],
