{
  "chapter": {
    "id": "e71bdd6e-0154-4ba2-a681-e730276a7250",
    "number": 148,
    "ref": "https://www.bible.com/bible/19/PSA.148.BD2011",
    "createdAt": "2024-09-07T11:51:11.595Z",
    "updatedAt": "2024-09-07T11:51:11.595Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "907a93b4-6e71-45de-b08e-1c6e9fde6084",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.494Z",
      "updatedAt": "2024-09-07T14:29:41.494Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "81f218ee-c882-4303-ad06-aef31e426270",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA trên các tầng trời!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.494Z",
      "updatedAt": "2024-09-07T14:29:41.494Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "7912666d-46d2-4bff-85f6-e07d361e2428",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong cõi trời cao!",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.494Z",
      "updatedAt": "2024-09-07T14:29:41.494Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "f7688ab0-78a7-4767-ba47-71c154f2ac15",
      "number": 2,
      "content": "Hãy ca ngợi Ngài, hỡi tất cả các thiên sứ của Ngài!",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.504Z",
      "updatedAt": "2024-09-07T14:29:41.504Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "c5b6a4ba-5f0c-4f69-aa65-e6f1525866e6",
      "number": 2,
      "content": "Hãy ca ngợi Ngài, hỡi tất cả các thiên binh của Ngài!",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.504Z",
      "updatedAt": "2024-09-07T14:29:41.504Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "c5a2b807-2e7d-415d-8ee8-26b5928bcae2",
      "number": 3,
      "content": "Hãy ca ngợi Ngài, hỡi mặt trời và mặt trăng!",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.506Z",
      "updatedAt": "2024-09-07T14:29:41.506Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "53fb9027-f682-4b8a-b34c-67607231f7bb",
      "number": 3,
      "content": "Hãy ca ngợi Ngài, hỡi tất cả các ngươi, những vì sao sáng!",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.506Z",
      "updatedAt": "2024-09-07T14:29:41.506Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "a00a8db9-10b5-41ad-a668-615f43bf884c",
      "number": 4,
      "content": "Hãy ca ngợi Ngài, hỡi các ngươi, các tầng trời cao thăm thẳm!",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.508Z",
      "updatedAt": "2024-09-07T14:29:41.508Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "1d71c361-5e39-4a7c-b271-7813bc37e795",
      "number": 4,
      "content": "Và hỡi các ngươi, các nguồn nước trên các tầng trời!",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.508Z",
      "updatedAt": "2024-09-07T14:29:41.508Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "49fe0a4f-4044-4ffe-ad58-38d4d360b219",
      "number": 5,
      "content": "Tất cả hãy ca ngợi danh CHÚA,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.511Z",
      "updatedAt": "2024-09-07T14:29:41.511Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "cbbcd658-2c36-4742-ad53-738dcf4aa724",
      "number": 5,
      "content": "Vì Ngài ra lịnh, mọi vật được dựng nên.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.511Z",
      "updatedAt": "2024-09-07T14:29:41.511Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "80c26b62-6f8d-4f61-bbd1-d506cca4a90c",
      "number": 6,
      "content": "Ngài thiết lập chúng, để chúng trường tồn mãi mãi;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.514Z",
      "updatedAt": "2024-09-07T14:29:41.514Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "e1ebf55b-f1b7-494c-8cf6-75eadd8fa5c6",
      "number": 6,
      "content": "Ngài ban một định luật, và chúng không bao giờ bị đào thải.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.514Z",
      "updatedAt": "2024-09-07T14:29:41.514Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "ac137a7c-2797-46d5-848a-33f0360d91d1",
      "number": 7,
      "content": "Hãy ca ngợi CHÚA ở dưới đất,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.516Z",
      "updatedAt": "2024-09-07T14:29:41.516Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "f7cf7937-5f3c-4587-9bed-070b7c416bf7",
      "number": 7,
      "content": "Hỡi các thủy quái và các vực sâu,",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.516Z",
      "updatedAt": "2024-09-07T14:29:41.516Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "d03b0e78-a24d-4bff-b032-e93b8a9b9fb4",
      "number": 8,
      "content": "Hỡi lửa và mưa đá, hỡi tuyết và mây,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.519Z",
      "updatedAt": "2024-09-07T14:29:41.519Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "258e9c2d-9c50-40f0-baee-e6053b1345ef",
      "number": 8,
      "content": "Hỡi gió bão luôn vâng lời Ngài,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.519Z",
      "updatedAt": "2024-09-07T14:29:41.519Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "91481f86-6ffc-4d8f-8e17-e40b2fbc3b57",
      "number": 9,
      "content": "Hỡi các núi non và mọi đồi nổng,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.521Z",
      "updatedAt": "2024-09-07T14:29:41.521Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "220af9ff-08d8-4a5f-aa33-2bdb33c5e47f",
      "number": 9,
      "content": "Hỡi những cây ra trái và những cây bá hương,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.521Z",
      "updatedAt": "2024-09-07T14:29:41.521Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "70151ade-10cf-4135-853b-7a161deea199",
      "number": 10,
      "content": "Hỡi những thú rừng và mọi loài súc vật,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.523Z",
      "updatedAt": "2024-09-07T14:29:41.523Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "46482860-6a44-4a14-98cc-a1496684d6d2",
      "number": 10,
      "content": "Hỡi những côn trùng và những điểu thú,",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.523Z",
      "updatedAt": "2024-09-07T14:29:41.523Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "09f738a4-509c-4595-a1d5-a9bf9fb5743d",
      "number": 11,
      "content": "Hỡi các vua và mọi dân trên đất,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.525Z",
      "updatedAt": "2024-09-07T14:29:41.525Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "851b2c71-66a0-40eb-a84f-de48ede0d4c7",
      "number": 11,
      "content": "Hỡi các lãnh chúa và các lãnh tụ trên thế gian,",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.525Z",
      "updatedAt": "2024-09-07T14:29:41.525Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "51f46739-5888-4673-a5f9-3c8a44de916a",
      "number": 12,
      "content": "Hỡi tất cả thanh thiếu niên nam nữ,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.527Z",
      "updatedAt": "2024-09-07T14:29:41.527Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "8f1b34b5-b094-490a-b22a-39bc1443b540",
      "number": 12,
      "content": "Hỡi quý cụ lão thành và hỡi những trẻ còn thơ.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.527Z",
      "updatedAt": "2024-09-07T14:29:41.527Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "ad0a9834-fcdd-4089-8be4-47e530a1b4d6",
      "number": 13,
      "content": "Tất cả hãy ca ngợi danh CHÚA,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.533Z",
      "updatedAt": "2024-09-07T14:29:41.533Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "82dd3217-b13f-4d66-b218-29e63af241a7",
      "number": 13,
      "content": "Vì chỉ có danh Ngài mới đáng được tôn cao;",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.533Z",
      "updatedAt": "2024-09-07T14:29:41.533Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "a9b72736-da78-4e7d-842f-dae62ac5091a",
      "number": 13,
      "content": "Vinh quang Ngài vượt trỗi trên cả đất và trời.",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.533Z",
      "updatedAt": "2024-09-07T14:29:41.533Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "b9672345-2467-4aa5-bef5-81ea6c07488c",
      "number": 14,
      "content": "Ngài nhấc cao sừng của dân Ngài lên,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.538Z",
      "updatedAt": "2024-09-07T14:29:41.538Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "f4e34806-7c0d-40fc-9a8b-47e46407be8a",
      "number": 14,
      "content": "Nên hãy ca ngợi Ngài, hỡi tất cả thánh dân Ngài,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.538Z",
      "updatedAt": "2024-09-07T14:29:41.538Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "da7148b8-cae1-44a8-9f2d-69f6d6a8d846",
      "number": 14,
      "content": "Con cháu của I-sơ-ra-ên, một dân gần gũi bên Ngài.",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.538Z",
      "updatedAt": "2024-09-07T14:29:41.538Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    },
    {
      "id": "541e8878-4e46-4316-915c-462baffbbe93",
      "number": 14,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 3,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:41.538Z",
      "updatedAt": "2024-09-07T14:29:41.538Z",
      "chapterId": "e71bdd6e-0154-4ba2-a681-e730276a7250"
    }
  ],
