{
  "chapter": {
    "id": "705357de-b0a6-443d-9b37-1ef14475e02d",
    "number": 33,
    "ref": "https://www.bible.com/bible/19/PSA.33.BD2011",
    "createdAt": "2024-09-07T11:51:10.887Z",
    "updatedAt": "2024-09-07T11:51:10.887Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "7f487aca-ec57-4db0-851d-b291a5ef892a",
      "number": 1,
      "content": "Hỡi những người ngay lành, hãy vui vẻ ca ngợi CHÚA;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.830Z",
      "updatedAt": "2024-09-07T14:02:03.830Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "b5005a98-3ead-4518-ab2c-8d37cc646e53",
      "number": 1,
      "content": "Sự ca ngợi Ngài thật thích hợp với người ngay thật.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.830Z",
      "updatedAt": "2024-09-07T14:02:03.830Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "f75dfd51-4621-4a27-989e-dc68e571796f",
      "number": 2,
      "content": "Hãy dùng hạc cầm dâng lời cảm tạ CHÚA;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.839Z",
      "updatedAt": "2024-09-07T14:02:03.839Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "45f1b064-25e0-402c-b9ac-84838c8775b8",
      "number": 2,
      "content": "Hãy dùng thập huyền cầm ca hát tôn vinh Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.839Z",
      "updatedAt": "2024-09-07T14:02:03.839Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "1f3ee1ee-5822-42e8-a394-7e1db15a32bb",
      "number": 3,
      "content": "Hãy hát cho Ngài một bài ca mới;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.842Z",
      "updatedAt": "2024-09-07T14:02:03.842Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "d1624f0c-a356-449e-baab-5d4417be2596",
      "number": 3,
      "content": "Khá đàn thật hay, hòa nhịp với tiếng reo mừng,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.842Z",
      "updatedAt": "2024-09-07T14:02:03.842Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "b6e1eaef-54e9-4794-86b4-678514c81369",
      "number": 4,
      "content": "Vì lời CHÚA rất chân thật;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.845Z",
      "updatedAt": "2024-09-07T14:02:03.845Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "023ca84b-cde1-4bb2-8caf-0461c0c2af63",
      "number": 4,
      "content": "Mọi việc Ngài làm đều thành tín.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.845Z",
      "updatedAt": "2024-09-07T14:02:03.845Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "92c7ef2b-82a6-4b7c-b220-adb9b000f9d7",
      "number": 5,
      "content": "Ngài yêu mến công chính và công lý;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.847Z",
      "updatedAt": "2024-09-07T14:02:03.847Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "ea363405-f8eb-4555-8265-75d47dd24867",
      "number": 5,
      "content": "Cả thế gian đầy dẫy tình thương của CHÚA.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.847Z",
      "updatedAt": "2024-09-07T14:02:03.847Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "02d470be-9076-48d1-8633-10ba828722e0",
      "number": 6,
      "content": "Nhờ lời CHÚA, các tầng trời được dựng nên;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.850Z",
      "updatedAt": "2024-09-07T14:02:03.850Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "2d2fed32-af11-4aac-8a24-f99f428e423e",
      "number": 6,
      "content": "Bởi hơi thở của miệng Ngài, các thiên thể liền hiện hữu.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.850Z",
      "updatedAt": "2024-09-07T14:02:03.850Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "441dc3d9-4445-465e-b5a2-f9331fdb4174",
      "number": 7,
      "content": "Ngài gom nước biển lại thành một khối;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.852Z",
      "updatedAt": "2024-09-07T14:02:03.852Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "c78d663e-a8e8-47b8-bbbf-f7431e8c62fa",
      "number": 7,
      "content": "Ngài chứa nước trong các vực sâu.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.852Z",
      "updatedAt": "2024-09-07T14:02:03.852Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "e5763ab1-4480-4319-bf01-4d71238a1b77",
      "number": 8,
      "content": "Hỡi cả thế gian, hãy kính sợ CHÚA;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.854Z",
      "updatedAt": "2024-09-07T14:02:03.854Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "c6629196-8ca3-478f-a5ad-36dda3ed5d93",
      "number": 8,
      "content": "Hỡi các dân tộc trên thế gian, hãy khiếp sợ Ngài,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.854Z",
      "updatedAt": "2024-09-07T14:02:03.854Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "44609e1a-8e24-46e3-b0c6-7d01f49266ad",
      "number": 9,
      "content": "Vì khi Ngài phán, muôn vật liền hiện hữu;",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.856Z",
      "updatedAt": "2024-09-07T14:02:03.856Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "3eb69fb7-445d-4b0d-a35e-f3211aab2b2c",
      "number": 9,
      "content": "Ngài ra lịnh, tất cả được đứng vững vàng.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.856Z",
      "updatedAt": "2024-09-07T14:02:03.856Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "dd790e21-4283-485a-98d4-104d5aac5f84",
      "number": 10,
      "content": "CHÚA làm bại mưu các nước;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.858Z",
      "updatedAt": "2024-09-07T14:02:03.858Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "a3e5e2ba-8670-4604-a0f2-a470f1f3508a",
      "number": 10,
      "content": "Ngài khiến kế hoạch của các dân ra hư không.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.858Z",
      "updatedAt": "2024-09-07T14:02:03.858Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "e28fdb97-752b-420c-9ab3-af8673413580",
      "number": 11,
      "content": "Còn ý định của CHÚA cứ được vững lập đến đời đời;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.861Z",
      "updatedAt": "2024-09-07T14:02:03.861Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "daffa44d-c922-47f0-a05d-65fe5557e476",
      "number": 11,
      "content": "Các ý chỉ của lòng Ngài còn từ đời nọ đến đời kia.",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.861Z",
      "updatedAt": "2024-09-07T14:02:03.861Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "b86900a3-32d0-48eb-aa7f-9aa73a1a51c5",
      "number": 12,
      "content": "Phước cho nước nào có CHÚA là Thần của mình,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.866Z",
      "updatedAt": "2024-09-07T14:02:03.866Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "373dfb5c-b309-4e57-8e41-5a17470238a1",
      "number": 12,
      "content": "Phước cho dân nào được Ngài chọn làm cơ nghiệp Ngài.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.866Z",
      "updatedAt": "2024-09-07T14:02:03.866Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "e6f36a52-9579-4695-bbcf-de8d49401386",
      "number": 13,
      "content": "CHÚA từ trời nhìn xuống;",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.868Z",
      "updatedAt": "2024-09-07T14:02:03.868Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "8fd65094-e9c1-4d8c-a047-2360ee5328d5",
      "number": 13,
      "content": "Ngài thấy rõ mọi con cái loài người.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.868Z",
      "updatedAt": "2024-09-07T14:02:03.868Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "bc622af0-d300-4321-9234-e5756308799d",
      "number": 14,
      "content": "Từ nơi Ngài ngự Ngài quan sát",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.870Z",
      "updatedAt": "2024-09-07T14:02:03.870Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "ebba98fc-ef6d-4e30-bf67-1b7936dd58a6",
      "number": 14,
      "content": "Mọi người trên đất.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.870Z",
      "updatedAt": "2024-09-07T14:02:03.870Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "695e62fd-3485-408e-bb88-265c7ad0ac4e",
      "number": 15,
      "content": "Ngài dựng nên lòng dạ mọi người,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.872Z",
      "updatedAt": "2024-09-07T14:02:03.872Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "df52ae07-b2cf-4df2-a8bc-d9e5361e19a0",
      "number": 15,
      "content": "Nên Ngài hiểu rõ mọi việc họ làm.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.872Z",
      "updatedAt": "2024-09-07T14:02:03.872Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "c7dcd34d-c72c-451e-bdf2-99036456e9c1",
      "number": 16,
      "content": "Chẳng có vua nào được cứu nhờ quân đội mạnh;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.874Z",
      "updatedAt": "2024-09-07T14:02:03.874Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "a2a06657-e76c-4361-b7d0-483d8ab1a69d",
      "number": 16,
      "content": "Chẳng có chiến sĩ nào được cứu nhờ sức mạnh mình.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.874Z",
      "updatedAt": "2024-09-07T14:02:03.874Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "f45233ec-ff9b-41b5-b61d-9c3f9c69a126",
      "number": 17,
      "content": "Nhờ vào ngựa để chiến thắng là hy vọng hão huyền;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.877Z",
      "updatedAt": "2024-09-07T14:02:03.877Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "f1f22b34-30ee-48c5-b410-03c561068436",
      "number": 17,
      "content": "Dù ngựa có sức lực nó cũng chẳng cứu được ai.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.877Z",
      "updatedAt": "2024-09-07T14:02:03.877Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "9f9b859d-a7a6-4230-8706-4015dea78773",
      "number": 18,
      "content": "Kìa, mắt CHÚA nhìn xem những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.879Z",
      "updatedAt": "2024-09-07T14:02:03.879Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "9c5f6f25-18e7-4f39-937c-44b50c5f97c1",
      "number": 18,
      "content": "Những người đặt hy vọng vào lòng thương xót của Ngài,",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.879Z",
      "updatedAt": "2024-09-07T14:02:03.879Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "65c2c050-ffec-499b-9c36-c81e39f08e0d",
      "number": 19,
      "content": "Để giải cứu linh hồn họ khỏi chết,",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.881Z",
      "updatedAt": "2024-09-07T14:02:03.881Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "1ced5320-4369-4338-bc5e-f3e08b134f04",
      "number": 19,
      "content": "Và bảo tồn mạng sống họ khi nạn đói xảy ra.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.881Z",
      "updatedAt": "2024-09-07T14:02:03.881Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "f244d642-df0c-4d91-868d-7f321e9d3513",
      "number": 20,
      "content": "Linh hồn chúng ta trông cậy CHÚA;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.883Z",
      "updatedAt": "2024-09-07T14:02:03.883Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "21c59b10-bba6-40f1-b4d1-c2bf9c9d2a62",
      "number": 20,
      "content": "Ngài là nguồn cứu giúp và thuẫn khiên của chúng ta.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.883Z",
      "updatedAt": "2024-09-07T14:02:03.883Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "0c4e91e7-a450-4efc-848a-87cdd8d09f40",
      "number": 21,
      "content": "Lòng chúng ta vui mừng trong Ngài,",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.885Z",
      "updatedAt": "2024-09-07T14:02:03.885Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "d8048aba-d660-44c1-9955-12062ab96243",
      "number": 21,
      "content": "Bởi vì chúng ta tin cậy vào danh thánh Ngài.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.885Z",
      "updatedAt": "2024-09-07T14:02:03.885Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "5e4dfda1-de85-4c0e-aef1-9a9f43599da8",
      "number": 22,
      "content": "CHÚA ôi, nguyện tình thương của Ngài ngự trên chúng con,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.887Z",
      "updatedAt": "2024-09-07T14:02:03.887Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    },
    {
      "id": "51c78403-9841-4ce7-9ccc-5821f0b991b5",
      "number": 22,
      "content": "Y như những gì chúng con hy vọng nơi Ngài.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:02:03.887Z",
      "updatedAt": "2024-09-07T14:02:03.887Z",
      "chapterId": "705357de-b0a6-443d-9b37-1ef14475e02d"
    }
  ],
