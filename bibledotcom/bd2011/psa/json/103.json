{
  "chapter": {
    "id": "438a5028-cb79-4397-88ae-b85637da12ff",
    "number": 103,
    "ref": "https://www.bible.com/bible/19/PSA.103.BD2011",
    "createdAt": "2024-09-07T11:51:11.440Z",
    "updatedAt": "2024-09-07T11:51:11.440Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "67ac6217-2c98-4074-a798-df12ef73d1e2",
      "number": 1,
      "content": "Hỡi linh hồn ta, hãy ca ngợi CHÚA;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.047Z",
      "updatedAt": "2024-09-07T14:17:00.047Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "8f45768f-1197-4a15-b2c1-a0389b6cde43",
      "number": 1,
      "content": "Hỡi tất cả những gì ở trong ta,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.047Z",
      "updatedAt": "2024-09-07T14:17:00.047Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "a23b2715-b195-4ecb-8c05-3cd9bb1b2923",
      "number": 1,
      "content": "Hãy ca tụng danh thánh của Ngài.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.047Z",
      "updatedAt": "2024-09-07T14:17:00.047Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "c0e7026f-4dce-428c-b9c5-52f45a21106b",
      "number": 2,
      "content": "Hỡi linh hồn ta, hãy ca ngợi CHÚA,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.057Z",
      "updatedAt": "2024-09-07T14:17:00.057Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "e01c36d5-ac16-4795-9c48-d43d063d51a5",
      "number": 2,
      "content": "Chớ quên mọi phước hạnh của Ngài,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.057Z",
      "updatedAt": "2024-09-07T14:17:00.057Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "6ece1745-02f3-4c2a-a73a-de5d78e19f45",
      "number": 3,
      "content": "Đấng tha thứ mọi tội lỗi ngươi;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.059Z",
      "updatedAt": "2024-09-07T14:17:00.059Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "4c223f2e-141b-439e-b212-8bd48656beec",
      "number": 3,
      "content": "Đấng chữa lành mọi bịnh tật ngươi;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.059Z",
      "updatedAt": "2024-09-07T14:17:00.059Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "8ebba73d-602c-44e5-8c08-ce34e4e382fd",
      "number": 4,
      "content": "Đấng cứu chuộc mạng sống ngươi thoát khỏi mộ phần;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.062Z",
      "updatedAt": "2024-09-07T14:17:00.062Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "a8a885f4-28e7-45a6-8d70-2195ffed4030",
      "number": 4,
      "content": "Đấng đội trên đầu ngươi tình thương và lòng thương xót;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.062Z",
      "updatedAt": "2024-09-07T14:17:00.062Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "fae072e8-8157-40ca-81e5-1db9c7feb97b",
      "number": 5,
      "content": "Đấng cho ngươi no thỏa với thức ăn ngon,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.064Z",
      "updatedAt": "2024-09-07T14:17:00.064Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "625e6aab-c409-445b-bda6-2480d8f3f870",
      "number": 5,
      "content": "Nhờ đó sức lực ngươi trẻ lại như sức của phượng hoàng.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.064Z",
      "updatedAt": "2024-09-07T14:17:00.064Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "9b1aa838-f4aa-4894-b830-45aace45d31c",
      "number": 6,
      "content": "CHÚA thi hành sự công chính",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.067Z",
      "updatedAt": "2024-09-07T14:17:00.067Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "5ee1d188-140a-44da-9af2-80d1744ce134",
      "number": 6,
      "content": "Và công lý cho mọi người bị áp bức.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.067Z",
      "updatedAt": "2024-09-07T14:17:00.067Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "2a3a9b3e-7978-47ef-9662-897d45a99778",
      "number": 7,
      "content": "Ngài bày tỏ cho Môi-se các đường lối Ngài,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.069Z",
      "updatedAt": "2024-09-07T14:17:00.069Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "450ab32f-1e7d-43a6-84bd-8662adf98604",
      "number": 7,
      "content": "Cho dân I-sơ-ra-ên các công việc Ngài.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.069Z",
      "updatedAt": "2024-09-07T14:17:00.069Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "8b5b0d61-86a0-4280-8cc0-fe1be3c911fc",
      "number": 8,
      "content": "CHÚA hay thương xót và khoan dung độ lượng,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.071Z",
      "updatedAt": "2024-09-07T14:17:00.071Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "7d48adbf-15a1-4d37-9440-18984be7ec98",
      "number": 8,
      "content": "Chậm nóng giận và đầy dẫy tình thương.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.071Z",
      "updatedAt": "2024-09-07T14:17:00.071Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "9276a62c-2fee-4cb2-bd77-ec881111d82b",
      "number": 9,
      "content": "Ngài không bắt tội luôn luôn;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.073Z",
      "updatedAt": "2024-09-07T14:17:00.073Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "e3572056-6d88-4a61-bbf2-5644029ad885",
      "number": 9,
      "content": "Ngài cũng chẳng giữ lòng giận đến đời đời.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.073Z",
      "updatedAt": "2024-09-07T14:17:00.073Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "909da006-3d38-42e3-a6e2-1795b86cc4a4",
      "number": 10,
      "content": "Ngài không đối xử với chúng ta theo các tội của chúng ta;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.075Z",
      "updatedAt": "2024-09-07T14:17:00.075Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "28b537ec-3fdb-4b2a-8580-a00130993039",
      "number": 10,
      "content": "Ngài cũng không báo trả chúng ta theo các lỗi của chúng ta.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.075Z",
      "updatedAt": "2024-09-07T14:17:00.075Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "f9c10ace-ea72-46d0-8600-3b70bc32373f",
      "number": 11,
      "content": "Vì các tầng trời cao hơn đất bao nhiêu,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.078Z",
      "updatedAt": "2024-09-07T14:17:00.078Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "bdf1c4d7-0fec-4ecd-8d8b-5042d2c1fabb",
      "number": 11,
      "content": "Tình thương của Ngài đối với những người kính sợ Ngài cũng cao lớn bấy nhiêu.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.078Z",
      "updatedAt": "2024-09-07T14:17:00.078Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "18903150-66fa-458e-b33f-058d6165ee0b",
      "number": 12,
      "content": "Phương đông xa cách phương tây bao nhiêu,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.079Z",
      "updatedAt": "2024-09-07T14:17:00.079Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "e3313dc5-32eb-471c-bbf0-e14d69d7cfa3",
      "number": 12,
      "content": "Ngài đem những vi phạm của chúng ta xa khỏi chúng ta cũng bấy nhiêu.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.079Z",
      "updatedAt": "2024-09-07T14:17:00.079Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "f2ae9b9c-796b-43b9-877c-b853576de59b",
      "number": 13,
      "content": "Như người cha thương xót các con mình thể nào,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.081Z",
      "updatedAt": "2024-09-07T14:17:00.081Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "a3308f37-d0b8-4c56-bf45-45bc15234459",
      "number": 13,
      "content": "CHÚA thương xót những người kính sợ Ngài cũng thể ấy.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.081Z",
      "updatedAt": "2024-09-07T14:17:00.081Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "28e054a5-803d-4384-9d3f-85ddfa738b84",
      "number": 14,
      "content": "Vì Ngài biết rõ chúng ta được dựng nên bằng những gì;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.083Z",
      "updatedAt": "2024-09-07T14:17:00.083Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "cb70dbfc-4475-4ac4-9740-bbbb4eeeb902",
      "number": 14,
      "content": "Ngài nhớ lại rằng chúng ta là bụi đất.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.083Z",
      "updatedAt": "2024-09-07T14:17:00.083Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "ffa046d9-0fea-4960-8bd3-249d74e5d2c1",
      "number": 15,
      "content": "Về loài người, đời người như ngọn cỏ;",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.086Z",
      "updatedAt": "2024-09-07T14:17:00.086Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "5a4ef96c-d981-4ac0-b2e0-fdba165c023d",
      "number": 15,
      "content": "Nó thắm tươi như hoa dại ngoài đồng.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.086Z",
      "updatedAt": "2024-09-07T14:17:00.086Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "8275d92c-3e71-4711-816d-1c3eafee0658",
      "number": 16,
      "content": "Một cơn gió thổi qua, kìa, nó chẳng còn;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.088Z",
      "updatedAt": "2024-09-07T14:17:00.088Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "64b153b7-d7f1-42d5-89e2-114e015771b5",
      "number": 16,
      "content": "Chỗ nó mọc lên cũng không còn biết nó nữa.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.088Z",
      "updatedAt": "2024-09-07T14:17:00.088Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "908c0b31-4cde-4041-9d2f-54e73cc871ae",
      "number": 17,
      "content": "Nhưng tình thương của CHÚA thì còn mãi mãi cho những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.090Z",
      "updatedAt": "2024-09-07T14:17:00.090Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "c8f171f0-5b33-414f-a8b3-76dd107e5185",
      "number": 17,
      "content": "Và đức công chính Ngài dành cho con cháu chắt chít họ,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.090Z",
      "updatedAt": "2024-09-07T14:17:00.090Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "5d09aa79-4d34-4c92-9fb4-fc25de967ad5",
      "number": 18,
      "content": "Tức cho những người giữ giao ước Ngài,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.092Z",
      "updatedAt": "2024-09-07T14:17:00.092Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "6de4ade0-dca4-44ec-98c0-4129ce28d6ea",
      "number": 18,
      "content": "Và cho những người nhớ đến các điều răn Ngài mà làm theo.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.092Z",
      "updatedAt": "2024-09-07T14:17:00.092Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "bd243693-a875-4a87-bced-87f798d3f8da",
      "number": 19,
      "content": "CHÚA thiết lập ngai Ngài trên các tầng trời;",
      "order": 0,
      "parNumber": 38,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.094Z",
      "updatedAt": "2024-09-07T14:17:00.094Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "45b30779-90d3-4c59-8556-f2c0591424d0",
      "number": 19,
      "content": "Vương quyền Ngài bao trùm cả vũ trụ.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.094Z",
      "updatedAt": "2024-09-07T14:17:00.094Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "6a7e52de-76f5-46a7-b96f-91a0718d070c",
      "number": 20,
      "content": "Hãy ca ngợi CHÚA, hỡi các thiên sứ của Ngài,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.096Z",
      "updatedAt": "2024-09-07T14:17:00.096Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "6d9baf28-9ae0-4b7f-b53e-8725ed881298",
      "number": 20,
      "content": "Tức những đấng dũng mãnh siêu phàm, những đấng thi hành mệnh lệnh Ngài,",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.096Z",
      "updatedAt": "2024-09-07T14:17:00.096Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "4ae1031a-02e9-47c9-9f00-91894e405c56",
      "number": 20,
      "content": "Những đấng vâng theo lời Ngài!",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.096Z",
      "updatedAt": "2024-09-07T14:17:00.096Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "77fcb47c-da26-4679-8f90-aadd60185376",
      "number": 21,
      "content": "Hãy ca ngợi CHÚA, hỡi tất cả các đạo quân của Ngài,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.099Z",
      "updatedAt": "2024-09-07T14:17:00.099Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "7e5c39f3-3c70-451f-9d79-b82bb441ec73",
      "number": 21,
      "content": "Tức các đấng phục vụ Ngài, làm theo thánh ý Ngài.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.099Z",
      "updatedAt": "2024-09-07T14:17:00.099Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "058eca84-bfdf-4b6c-be09-5c275364ef67",
      "number": 22,
      "content": "Hãy ca ngợi CHÚA, hỡi tất cả các tạo vật của Ngài,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.101Z",
      "updatedAt": "2024-09-07T14:17:00.101Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "4b92abd2-e7ce-4b20-9f08-15c104fed2be",
      "number": 22,
      "content": "Ở mọi nơi dưới quyền trị vì của Ngài!",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.101Z",
      "updatedAt": "2024-09-07T14:17:00.101Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    },
    {
      "id": "49020929-a6a3-489f-a16f-80a9ece43672",
      "number": 22,
      "content": "Hãy ca ngợi CHÚA, hỡi linh hồn ta!",
      "order": 2,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:00.101Z",
      "updatedAt": "2024-09-07T14:17:00.101Z",
      "chapterId": "438a5028-cb79-4397-88ae-b85637da12ff"
    }
  ],
