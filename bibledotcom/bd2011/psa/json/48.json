    {
      "id": "a512e0fc-d08b-402e-a082-3f927c7c638c",
      "content": "nt: các ái nữ (nhân cách hóa)",
      "position": 16,
      "order": 1,
      "createdAt": "2024-09-07T14:04:31.348Z",
      "updatedAt": "2024-09-07T14:04:31.348Z",
