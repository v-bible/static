{
  "chapter": {
    "id": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb",
    "number": 126,
    "ref": "https://www.bible.com/bible/19/PSA.126.BD2011",
    "createdAt": "2024-09-07T11:51:11.535Z",
    "updatedAt": "2024-09-07T11:51:11.535Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "919ea797-637f-4372-a746-210c51499ffd",
      "number": 1,
      "content": "Khi CHÚA đem những người của Si-ôn bị lưu đày trở về,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.170Z",
      "updatedAt": "2024-09-07T14:21:58.170Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "fc809b57-885b-41c4-9a4c-e83d4464b84b",
      "number": 1,
      "content": "Chúng tôi như người nằm mơ.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.170Z",
      "updatedAt": "2024-09-07T14:21:58.170Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "7f5370cd-068d-497d-9a3c-b2328269cd6f",
      "number": 2,
      "content": "Miệng chúng tôi vui cười không ngớt;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.180Z",
      "updatedAt": "2024-09-07T14:21:58.180Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "390f5f99-5cc5-408d-9134-357b15869130",
      "number": 2,
      "content": "Lưỡi chúng tôi lớn tiếng reo mừng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.180Z",
      "updatedAt": "2024-09-07T14:21:58.180Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "6eda19ef-ee0f-4d40-99e7-a6ae95024032",
      "number": 2,
      "content": "Bấy giờ người ta bàn tán với nhau giữa các dân rằng,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.180Z",
      "updatedAt": "2024-09-07T14:21:58.180Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "f403cb09-5bad-4a74-8f10-bebcae9fb969",
      "number": 2,
      "content": "“CHÚA đã làm những việc lớn lao cho họ.”",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.180Z",
      "updatedAt": "2024-09-07T14:21:58.180Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "0f0de4ff-3d07-4a82-a870-6183fe7dbd7d",
      "number": 3,
      "content": "CHÚA đã làm những việc lớn lao cho chúng tôi;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.182Z",
      "updatedAt": "2024-09-07T14:21:58.182Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "e4e28ddf-8380-4679-8194-bc065ff2c8ff",
      "number": 3,
      "content": "Chúng tôi rất đỗi vui mừng!",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.182Z",
      "updatedAt": "2024-09-07T14:21:58.182Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "47b16bc3-0d36-437f-ad00-01d2e8e986fa",
      "number": 4,
      "content": "Lạy CHÚA, xin tiếp tục cho những người bị lưu đày trở về,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.185Z",
      "updatedAt": "2024-09-07T14:21:58.185Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "cc26e826-74a1-4bf1-9fd8-e39d8139542d",
      "number": 4,
      "content": "Như nước mưa đổ vào những dòng suối cạn ở Miền Nam.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.185Z",
      "updatedAt": "2024-09-07T14:21:58.185Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "bac167a8-2e70-4512-ae7b-4dc69a410031",
      "number": 5,
      "content": "Những người gieo giống trong nước mắt,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.187Z",
      "updatedAt": "2024-09-07T14:21:58.187Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "81d395c2-49ac-431a-9e15-dd39159b2e98",
      "number": 5,
      "content": "Ắt sẽ gặt hái trong tiếng reo mừng.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.187Z",
      "updatedAt": "2024-09-07T14:21:58.187Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "c6a7932c-773b-4e8e-8595-4d8627612b90",
      "number": 6,
      "content": "Người nào vừa đi vừa khóc",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.190Z",
      "updatedAt": "2024-09-07T14:21:58.190Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "a0c1f661-1731-4e2b-92a7-a80f683634b8",
      "number": 6,
      "content": "Đem giống ra rải,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.190Z",
      "updatedAt": "2024-09-07T14:21:58.190Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "b45dbc62-a913-4a75-b030-bc6e37c50996",
      "number": 6,
      "content": "Chắc chắn sẽ trở về với tiếng reo mừng,",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.190Z",
      "updatedAt": "2024-09-07T14:21:58.190Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    },
    {
      "id": "41491747-091d-478e-90e9-68f3387a25ea",
      "number": 6,
      "content": "Mang đầy những bó lúa chín vàng.",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:58.190Z",
      "updatedAt": "2024-09-07T14:21:58.190Z",
      "chapterId": "8c9a36b2-6c9c-4b3e-8dd3-c676c54d1cfb"
    }
  ],
