{
  "chapter": {
    "id": "36d69712-1d7d-4719-af26-9b86d118c88b",
    "number": 87,
    "ref": "https://www.bible.com/bible/19/PSA.87.BD2011",
    "createdAt": "2024-09-23T05:08:40.514Z",
    "updatedAt": "2024-09-23T05:08:40.514Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "9d09f70a-2b2d-49c6-8180-8238a3c1f20a",
      "number": 1,
      "content": "**Chúa** lập nền cho thành của Ngài trên núi thánh.",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.221Z",
      "updatedAt": "2024-09-23T05:51:34.221Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "12e9e669-f230-4c99-b3f6-d05bcd57e6cd",
      "number": 2,
      "content": "**CHÚA** yêu mến các cổng thành của Si-ôn",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.238Z",
      "updatedAt": "2024-09-23T05:51:34.238Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "703aca9e-d900-4185-9860-ce2896faae16",
      "number": 2,
      "content": "Hơn mọi nơi cư trú của Gia-cốp.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.243Z",
      "updatedAt": "2024-09-23T05:51:34.243Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "3f2406f2-caa8-4233-8422-db7464f055a2",
      "number": 3,
      "content": "Hỡi thành của Đức Chúa Trời,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.248Z",
      "updatedAt": "2024-09-23T05:51:34.248Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "05b3a9e1-886c-4f8d-82ee-c5d3adf1820b",
      "number": 3,
      "content": "Thiên hạ đã nói nhiều điều vinh hiển về ngươi. (Sê-la)",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.252Z",
      "updatedAt": "2024-09-23T05:51:34.252Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "736a6272-8ae1-4942-bcf4-384814faa8a5",
      "number": 4,
      "content": "Khi tôi nhắc đến Ai-cập và Ba-by-lôn cho những ai biết tôi,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.257Z",
      "updatedAt": "2024-09-23T05:51:34.257Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "f9ccbc0e-ddb0-4296-8dd5-8af7128d2895",
      "number": 4,
      "content": "Hoặc đề cập đến Phi-li-tin, Ty-rơ, với Ê-thi-ô-pi,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.267Z",
      "updatedAt": "2024-09-23T05:51:34.267Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "138807c3-7ed7-4b2c-881d-25835ca2148c",
      "number": 4,
      "content": "Thì người ta bảo, “Người này đã sinh trưởng tại Si-ôn đó!”",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.272Z",
      "updatedAt": "2024-09-23T05:51:34.272Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "f22f4aa9-a1d2-4c03-a6ee-8b9e48b55794",
      "number": 5,
      "content": "Thật vậy Si-ôn sẽ được trầm trồ khen ngợi rằng,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.283Z",
      "updatedAt": "2024-09-23T05:51:34.283Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "3373b68d-b8f7-477f-a94d-919e6a2688f0",
      "number": 5,
      "content": "“Người này và người nọ đã sinh trưởng trong thành ấy!”",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.288Z",
      "updatedAt": "2024-09-23T05:51:34.288Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "2824f09f-843e-488b-ac0d-25ced13bbbb6",
      "number": 5,
      "content": "Chính Đấng Tối Cao sẽ làm cho thành ấy được vững bền.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.293Z",
      "updatedAt": "2024-09-23T05:51:34.293Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "b3253cec-79f5-4dd1-8b3e-6d9332e68fbd",
      "number": 6,
      "content": "**CHÚA** sẽ ghi vào sổ hộ tịch của các dân rằng,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.299Z",
      "updatedAt": "2024-09-23T05:51:34.299Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "447d13a0-74cd-4525-8223-b5df733952b4",
      "number": 6,
      "content": "“Người này đã sinh ra tại Si-ôn.” (Sê-la)",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.304Z",
      "updatedAt": "2024-09-23T05:51:34.304Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "82e6e400-2d09-4ee3-a0e1-1357636a623d",
      "number": 7,
      "content": "Rồi những ca sĩ và nhạc sĩ sẽ nói,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.313Z",
      "updatedAt": "2024-09-23T05:51:34.313Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    },
    {
      "id": "e7cac1f5-8949-44cd-9de3-8ef071c3501a",
      "number": 7,
      "content": "“Tất cả nguồn cảm hứng của tôi đều phát xuất từ Si-ôn.”",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:51:34.318Z",
      "updatedAt": "2024-09-23T05:51:34.318Z",
      "chapterId": "36d69712-1d7d-4719-af26-9b86d118c88b"
    }
  ],
