{
  "chapter": {
    "id": "3894f282-87cb-4de3-8c08-dfacb11fd859",
    "number": 87,
    "ref": "https://www.bible.com/bible/19/PSA.87.BD2011",
    "createdAt": "2024-09-07T11:51:11.305Z",
    "updatedAt": "2024-09-07T11:51:11.305Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "5467c595-2f08-4cd9-a23c-110c0d4dbd6c",
      "number": 1,
      "content": "Chúa lập nền cho thành của Ngài trên núi thánh.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:32.996Z",
      "updatedAt": "2024-09-07T14:13:32.996Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "be45a8d2-02bb-4a40-8a54-aa866672600b",
      "number": 2,
      "content": "CHÚA yêu mến các cổng thành của Si-ôn",
      "order": 0,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.005Z",
      "updatedAt": "2024-09-07T14:13:33.005Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "b504e1e3-131a-495b-9bb5-f4e58e6e54fb",
      "number": 2,
      "content": "Hơn mọi nơi cư trú của Gia-cốp.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.005Z",
      "updatedAt": "2024-09-07T14:13:33.005Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "06f231c0-e455-4e14-a2c9-c06365381436",
      "number": 3,
      "content": "Hỡi thành của Đức Chúa Trời,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.008Z",
      "updatedAt": "2024-09-07T14:13:33.008Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "b5b26b92-f03c-4f0c-8e79-fe0031d3d877",
      "number": 3,
      "content": "Thiên hạ đã nói nhiều điều vinh hiển về ngươi. (Sê-la)",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.008Z",
      "updatedAt": "2024-09-07T14:13:33.008Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "75513818-51f5-4437-b7b1-54c4d6095d8c",
      "number": 4,
      "content": "Khi tôi nhắc đến Ai-cập và Ba-by-lôn cho những ai biết tôi,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.010Z",
      "updatedAt": "2024-09-07T14:13:33.010Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "859e9e16-7cb3-4f75-96a5-aa39b43bb76e",
      "number": 4,
      "content": "Hoặc đề cập đến Phi-li-tin, Ty-rơ, với Ê-thi-ô-pi,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.010Z",
      "updatedAt": "2024-09-07T14:13:33.010Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "d1c8d745-3819-4035-8252-22ec09978720",
      "number": 4,
      "content": "Thì người ta bảo, “Người này đã sinh trưởng tại Si-ôn đó!”",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.010Z",
      "updatedAt": "2024-09-07T14:13:33.010Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "8b2b9908-0e88-4eeb-9133-c1affec85083",
      "number": 5,
      "content": "Thật vậy Si-ôn sẽ được trầm trồ khen ngợi rằng,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.013Z",
      "updatedAt": "2024-09-07T14:13:33.013Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "acb9bcd4-8c2a-42d0-b091-8679a9122d40",
      "number": 5,
      "content": "“Người này và người nọ đã sinh trưởng trong thành ấy!”",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.013Z",
      "updatedAt": "2024-09-07T14:13:33.013Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "2299ab5c-b88c-4941-a350-3981cedd8b65",
      "number": 5,
      "content": "Chính Đấng Tối Cao sẽ làm cho thành ấy được vững bền.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.013Z",
      "updatedAt": "2024-09-07T14:13:33.013Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "2d319730-0452-4bda-a343-2222f2661f2f",
      "number": 6,
      "content": "CHÚA sẽ ghi vào sổ hộ tịch của các dân rằng,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.015Z",
      "updatedAt": "2024-09-07T14:13:33.015Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "b0833522-6c08-400f-a20d-fb2528714bc4",
      "number": 6,
      "content": "“Người này đã sinh ra tại Si-ôn.”  (Sê-la)",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.015Z",
      "updatedAt": "2024-09-07T14:13:33.015Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "5328ad42-a3aa-4e69-afaf-992af340f265",
      "number": 7,
      "content": "Rồi những ca sĩ và nhạc sĩ sẽ nói,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.017Z",
      "updatedAt": "2024-09-07T14:13:33.017Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    },
    {
      "id": "907a3857-4edb-4507-aaec-a8db8204db99",
      "number": 7,
      "content": "“Tất cả nguồn cảm hứng của tôi đều phát xuất từ Si-ôn.”",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:13:33.017Z",
      "updatedAt": "2024-09-07T14:13:33.017Z",
      "chapterId": "3894f282-87cb-4de3-8c08-dfacb11fd859"
    }
  ],
