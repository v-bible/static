{
  "chapter": {
    "id": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0",
    "number": 90,
    "ref": "https://www.bible.com/bible/19/PSA.90.BD2011",
    "createdAt": "2024-09-07T11:51:11.329Z",
    "updatedAt": "2024-09-07T11:51:11.329Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "971da9c9-26f0-4531-a2b3-92ceb1c5b52f",
      "number": 1,
      "content": "Lạy Chúa, Ngài là nơi cư trú của chúng con",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:23.997Z",
      "updatedAt": "2024-09-07T14:15:23.997Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "7b4865a6-b4b9-4e71-ac02-4943b845ad01",
      "number": 1,
      "content": "Trải qua mọi thế hệ.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:23.997Z",
      "updatedAt": "2024-09-07T14:15:23.997Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "5c89ba31-42d1-45fe-ae5e-e087e263ec7b",
      "number": 2,
      "content": "Trước khi núi non thành hình,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.007Z",
      "updatedAt": "2024-09-07T14:15:24.007Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "6e92f2ba-9ef7-4c08-b319-8624e617308b",
      "number": 2,
      "content": "Trước khi Ngài tạo thành trái đất và thế gian,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.007Z",
      "updatedAt": "2024-09-07T14:15:24.007Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "55a07bce-9102-40d2-a13e-377cf0bec929",
      "number": 2,
      "content": "Từ vô cực quá khứ đến vô cực tương lai Ngài là Đức Chúa Trời.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.007Z",
      "updatedAt": "2024-09-07T14:15:24.007Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "ded62359-66d2-46e8-a303-b25fc1156879",
      "number": 3,
      "content": "Ngài làm cho loài người trở về cát bụi và phán rằng,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.010Z",
      "updatedAt": "2024-09-07T14:15:24.010Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "efd3ebb4-d541-4e7d-98ea-bbf00f52f5ab",
      "number": 3,
      "content": "“Hỡi con cái loài người, hãy trở về.”",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.010Z",
      "updatedAt": "2024-09-07T14:15:24.010Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "3f698a4b-a942-4c35-aa3a-36c252b90c79",
      "number": 4,
      "content": "Vì một ngàn năm trước mắt Ngài",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.012Z",
      "updatedAt": "2024-09-07T14:15:24.012Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "6b804947-83cc-48f7-b0d6-303ff5d1f16b",
      "number": 4,
      "content": "Tựa như ngày hôm qua, đã qua rồi,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.012Z",
      "updatedAt": "2024-09-07T14:15:24.012Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "acddca41-332c-4e20-9139-782de91e628d",
      "number": 4,
      "content": "Hoặc như một canh đêm.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.012Z",
      "updatedAt": "2024-09-07T14:15:24.012Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "2cc4d8b6-64ae-496a-a4a4-66d7d61b5faf",
      "number": 5,
      "content": "Ngài làm cho đời người trôi nhanh như dòng nước lũ, thoáng qua như một giấc ngủ.",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.015Z",
      "updatedAt": "2024-09-07T14:15:24.015Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "a0269a4a-6842-4df1-a501-601ab76fe65f",
      "number": 5,
      "content": "Đời người như cỏ mọc ban mai,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.015Z",
      "updatedAt": "2024-09-07T14:15:24.015Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "16577513-8bd3-46ab-abfa-3417adda55ea",
      "number": 6,
      "content": "Sáng sớm chúng mọc lên và xanh tươi,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.018Z",
      "updatedAt": "2024-09-07T14:15:24.018Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "ce116e88-6863-4bb2-a855-75b7b61bebbc",
      "number": 6,
      "content": "Chiều lại chúng héo khô và tàn tạ.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.018Z",
      "updatedAt": "2024-09-07T14:15:24.018Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "569d84ea-4b41-48ba-a4ca-6b76433d600e",
      "number": 7,
      "content": "Thật vậy đời sống chúng con chóng tàn vì cơn giận của Ngài.",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.021Z",
      "updatedAt": "2024-09-07T14:15:24.021Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "340ab0df-727c-4b48-9ed2-585b100590fc",
      "number": 7,
      "content": "Chúng con kinh hãi vì cơn thịnh nộ của Ngài.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.021Z",
      "updatedAt": "2024-09-07T14:15:24.021Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "4a89b9ef-4f33-42d1-b030-e0478452cf33",
      "number": 8,
      "content": "Ngài đã để những tội lỗi chúng con trước mặt Ngài,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.023Z",
      "updatedAt": "2024-09-07T14:15:24.023Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "7322947f-3f0c-4327-b1b1-db8de3b5d024",
      "number": 8,
      "content": "Các tội bí mật chúng con trong ánh sáng của thánh nhan Ngài.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.023Z",
      "updatedAt": "2024-09-07T14:15:24.023Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "9db0d41c-d096-48bb-b3bd-72cbcb55c46e",
      "number": 9,
      "content": "Những ngày chúng con qua đi dưới cơn giận của Ngài;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.026Z",
      "updatedAt": "2024-09-07T14:15:24.026Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "381cf6a5-4275-4cee-9673-f2197b613089",
      "number": 9,
      "content": "Những năm chúng con kết thúc như một tiếng thở dài.",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.026Z",
      "updatedAt": "2024-09-07T14:15:24.026Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "f6824204-92c7-4de7-9665-11eecee40c68",
      "number": 10,
      "content": "Tuổi tác chúng con đến được bảy mươi,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.028Z",
      "updatedAt": "2024-09-07T14:15:24.028Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "40363b0a-8e5a-4971-b7db-d733efaf38b4",
      "number": 10,
      "content": "Còn nếu mạnh khỏe thì đến tám mươi,",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.028Z",
      "updatedAt": "2024-09-07T14:15:24.028Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "a6542c0b-e786-40f1-b700-93f1aef67355",
      "number": 10,
      "content": "Nhưng niềm kiêu hãnh của chúng chẳng qua là lao khổ và buồn thảm;",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.028Z",
      "updatedAt": "2024-09-07T14:15:24.028Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "0c8de53a-2fa2-4347-8b58-5ee220c8b42e",
      "number": 10,
      "content": "Vì đời sống chóng qua, rồi chúng con bay mất đi.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.028Z",
      "updatedAt": "2024-09-07T14:15:24.028Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "5c853638-5c27-4e6f-bbb9-5b2018e66761",
      "number": 11,
      "content": "Ai biết được sức mạnh của cơn giận Ngài?",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.030Z",
      "updatedAt": "2024-09-07T14:15:24.030Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "21b82506-a2fe-45f5-ac18-70c17c77a43f",
      "number": 11,
      "content": "Cơn thịnh nộ Ngài lớn bao nhiêu, sự khiếp sợ Ngài càng lớn bấy nhiêu.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.030Z",
      "updatedAt": "2024-09-07T14:15:24.030Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "3aad930c-c3ed-44f5-bc40-3ce740b80b12",
      "number": 12,
      "content": "Xin dạy chúng con biết đếm số ngày chúng con,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.036Z",
      "updatedAt": "2024-09-07T14:15:24.036Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "27a68f18-266c-4ac9-8923-503dba380cd6",
      "number": 12,
      "content": "Để chúng con được lòng khôn ngoan.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.036Z",
      "updatedAt": "2024-09-07T14:15:24.036Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "878b7ac8-f1eb-45d7-afb9-b5383ae4335c",
      "number": 13,
      "content": "Xin trở lại, CHÚA ôi; Ngài còn giận chúng con đến bao lâu nữa?",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.039Z",
      "updatedAt": "2024-09-07T14:15:24.039Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "67b6c207-4375-4f96-b636-934001d93165",
      "number": 13,
      "content": "Xin dủ lòng thương xót các tôi tớ Ngài.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.039Z",
      "updatedAt": "2024-09-07T14:15:24.039Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "66b23001-d413-4083-9cf7-05f468551ccf",
      "number": 14,
      "content": "Mỗi buổi sáng xin cho chúng con được no nê bằng ơn thương xót của Ngài,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.042Z",
      "updatedAt": "2024-09-07T14:15:24.042Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "0c961f88-e981-4442-89f0-b228a937a66a",
      "number": 14,
      "content": "Để chúng con được hân hoan và vui thỏa trọn những ngày của mình.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.042Z",
      "updatedAt": "2024-09-07T14:15:24.042Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "4d37fe38-4e68-4ea3-8fca-eb4451f74f2b",
      "number": 15,
      "content": "Xin ban cho chúng con niềm vui để bù lại những ngày Ngài phó chúng con cho hoạn nạn,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.048Z",
      "updatedAt": "2024-09-07T14:15:24.048Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "3d3d3488-22ce-4af1-9f3d-3c48791144e9",
      "number": 15,
      "content": "Và những năm chúng con thấy nỗi bất hạnh.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.048Z",
      "updatedAt": "2024-09-07T14:15:24.048Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "c58d3012-bfa8-450e-b905-60f5cfeba408",
      "number": 16,
      "content": "Nguyện công việc của Ngài được tỏ ra cho các tôi tớ Ngài.",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.051Z",
      "updatedAt": "2024-09-07T14:15:24.051Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "0ce83ac8-fa30-4aff-a03c-a7f0c9cfdddc",
      "number": 16,
      "content": "Nguyện vinh quang của Ngài tỏa rạng trên con cháu họ.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.051Z",
      "updatedAt": "2024-09-07T14:15:24.051Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "52b87f5b-ea56-4d88-870a-2180d200f307",
      "number": 17,
      "content": "Nguyện phước hạnh của Chúa, Đức Chúa Trời của chúng con, tuôn đổ trên chúng con.",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.054Z",
      "updatedAt": "2024-09-07T14:15:24.054Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "4cc14043-e722-4ccc-a189-c6d11952516b",
      "number": 17,
      "content": "Cầu xin Ngài lập vững công việc của tay chúng con;",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:24.054Z",
      "updatedAt": "2024-09-07T14:15:24.054Z",
      "chapterId": "99610cc9-cc1d-4a61-b88f-b0fb5b9d00c0"
    },
    {
      "id": "325df649-7b64-4940-a05d-91f0c4d74815",
      "number": 17,
      "content": "Thật vậy xin lập vững công việc của tay chúng con.",
      "order": 2,
      "parNumber": 41,
