{
  "chapter": {
    "id": "aa0c5d8f-7728-496e-baa7-b43af7229422",
    "number": 77,
    "ref": "https://www.bible.com/bible/19/PSA.77.BD2011",
    "createdAt": "2024-09-07T11:51:11.223Z",
    "updatedAt": "2024-09-07T11:51:11.223Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "783a3b5f-cc72-4735-bd62-df2c143a2be6",
      "number": 1,
      "content": "Tôi cất tiếng kêu cầu Đức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.721Z",
      "updatedAt": "2024-09-07T14:11:00.721Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "8602f158-70b6-441d-9d70-cda15561a033",
      "number": 1,
      "content": "Tiếng tôi thấu đến Đức Chúa Trời, và Ngài đã nghe tôi.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.721Z",
      "updatedAt": "2024-09-07T14:11:00.721Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "008da23d-84d5-401e-bd6a-63743f8804e5",
      "number": 2,
      "content": "Trong ngày hoạn nạn, tôi tìm cầu Chúa;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.730Z",
      "updatedAt": "2024-09-07T14:11:00.730Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "1595a687-ec82-4988-a892-d51b87cd145f",
      "number": 2,
      "content": "Tay tôi cứ đưa lên, chẳng mỏi mệt giữa đêm dài;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.730Z",
      "updatedAt": "2024-09-07T14:11:00.730Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "2ed2aabc-35df-46c3-a180-ccdd41d53222",
      "number": 2,
      "content": "Linh hồn tôi chẳng màng đến những lời người ta an ủi.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.730Z",
      "updatedAt": "2024-09-07T14:11:00.730Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "264a0085-ec13-45fd-b4b6-086dca770cbf",
      "number": 3,
      "content": "Tôi nhớ đến Đức Chúa Trời rồi bối rối băn khoăn;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.733Z",
      "updatedAt": "2024-09-07T14:11:00.733Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "03b8851e-001e-4f46-a2e5-51af0d7c29b5",
      "number": 3,
      "content": "Tôi thở dài và tâm linh tôi dần dần kiệt quệ. (Sê-la)",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.733Z",
      "updatedAt": "2024-09-07T14:11:00.733Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "637d5cf7-fc44-4f09-8fa9-a577e01c9c10",
      "number": 4,
      "content": "Ngài giữ mí mắt con mở ra;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.735Z",
      "updatedAt": "2024-09-07T14:11:00.735Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "e0926db2-7f90-4b51-867d-a768ff251b44",
      "number": 4,
      "content": "Con bị hoạn nạn đến nỗi chẳng thể nói thành lời.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.735Z",
      "updatedAt": "2024-09-07T14:11:00.735Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "014279e9-3db5-4bc2-a74b-c62b2df0c56d",
      "number": 5,
      "content": "Con nghĩ đến những ngày xa xưa,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.737Z",
      "updatedAt": "2024-09-07T14:11:00.737Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "f67f75d7-e92d-42e2-b1b0-d97dfa8af735",
      "number": 5,
      "content": "Những năm rất lâu về trước.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.737Z",
      "updatedAt": "2024-09-07T14:11:00.737Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "a043622f-8d61-49e2-8b40-e0adee2e1a9c",
      "number": 6,
      "content": "Con nhớ lại những bài thánh ca con đã từng hát giữa đêm khuya;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.739Z",
      "updatedAt": "2024-09-07T14:11:00.739Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "72f91d65-df31-4a3d-b390-e6fef47ebab6",
      "number": 6,
      "content": "Con suy gẫm trong lòng,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.739Z",
      "updatedAt": "2024-09-07T14:11:00.739Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "9e393990-c5bc-4b57-927d-48c36c8d635a",
      "number": 6,
      "content": "Và tâm linh con cứ ray rứt khôn nguôi:",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.739Z",
      "updatedAt": "2024-09-07T14:11:00.739Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "247eb311-1ccc-4812-be5b-e863eaf59481",
      "number": 7,
      "content": "Chẳng lẽ Chúa sẽ bỏ mình vĩnh viễn hay sao?",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.742Z",
      "updatedAt": "2024-09-07T14:11:00.742Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "93f8aae2-1816-425a-9ea9-f577f1d31109",
      "number": 7,
      "content": "Chẳng lẽ Ngài sẽ không bao giờ ban ơn trở lại sao?",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.742Z",
      "updatedAt": "2024-09-07T14:11:00.742Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "d32b5241-f3e8-4688-ba81-5f1d7212e9b2",
      "number": 8,
      "content": "Chẳng lẽ tình thương của Ngài đã ngừng mãi mãi rồi sao?",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.747Z",
      "updatedAt": "2024-09-07T14:11:00.747Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "525ed93a-c459-464b-a7fa-b97ece334ae9",
      "number": 8,
      "content": "Chẳng lẽ lời hứa của Ngài không còn được thực hiện nữa sao?",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.747Z",
      "updatedAt": "2024-09-07T14:11:00.747Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "1dad7412-02d7-4d6c-b8a2-e71e1efcbbec",
      "number": 9,
      "content": "Chẳng lẽ Đức Chúa Trời đã quên ban ơn rồi sao?",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.752Z",
      "updatedAt": "2024-09-07T14:11:00.752Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "b9b44f5f-4a67-456b-9757-5da5a090497c",
      "number": 9,
      "content": "Chẳng lẽ Ngài đã giận và thu hồi ơn thương xót của Ngài lại rồi sao? (Sê-la)",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.752Z",
      "updatedAt": "2024-09-07T14:11:00.752Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "96028426-9b30-49e1-8702-9cdf07ed53cc",
      "number": 10,
      "content": "Rồi tôi tự nhủ, “Điều đau đớn cho tôi là",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.757Z",
      "updatedAt": "2024-09-07T14:11:00.757Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "02ecdb52-3603-419a-83b2-3881904adef8",
      "number": 10,
      "content": "Cánh tay phải của Đấng Tối Cao không còn phù hộ tôi nữa.”",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.757Z",
      "updatedAt": "2024-09-07T14:11:00.757Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "51074cf0-ca3e-4794-92c8-d3be20929a77",
      "number": 11,
      "content": "Con sẽ nhớ lại những việc của CHÚA;",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.762Z",
      "updatedAt": "2024-09-07T14:11:00.762Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "41a02263-c420-4e60-817a-923850b5b3e9",
      "number": 11,
      "content": "Thật vậy, con sẽ nhớ lại những việc lạ lùng của Ngài trước kia.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.762Z",
      "updatedAt": "2024-09-07T14:11:00.762Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "fe396091-9926-4b3c-8a27-a8dcc73a6302",
      "number": 12,
      "content": "Con sẽ suy gẫm về những việc diệu kỳ của Ngài,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.767Z",
      "updatedAt": "2024-09-07T14:11:00.767Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "8cb2b5c9-5366-4414-9a09-64336b0ec59e",
      "number": 12,
      "content": "Và ngẫm nghĩ về những việc lớn lao của Ngài.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.767Z",
      "updatedAt": "2024-09-07T14:11:00.767Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "a179d0a3-c70b-401e-989d-f3fe09d42e3c",
      "number": 13,
      "content": "Đức Chúa Trời ôi, đường lối Ngài thật là thánh thiện;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.772Z",
      "updatedAt": "2024-09-07T14:11:00.772Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "10039884-6eda-4327-bcb3-efe89cb6d9d5",
      "number": 13,
      "content": "Có thần nào vĩ đại như Đức Chúa Trời của chúng con chăng?",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.772Z",
      "updatedAt": "2024-09-07T14:11:00.772Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "358a9f4f-fb92-46df-9675-d24bf16ffd62",
      "number": 14,
      "content": "Ngài là Đức Chúa Trời, Đấng đã làm những việc lạ lùng;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.777Z",
      "updatedAt": "2024-09-07T14:11:00.777Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "fb88d041-d1d8-4ecb-90b8-f1e4e120577f",
      "number": 14,
      "content": "Ngài bày tỏ quyền năng Ngài giữa muôn dân.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.777Z",
      "updatedAt": "2024-09-07T14:11:00.777Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "e1bf0b86-5a99-4f34-b6b5-3cc08c253b11",
      "number": 15,
      "content": "Ngài đã dùng cánh tay Ngài cứu chuộc con dân Ngài,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.782Z",
      "updatedAt": "2024-09-07T14:11:00.782Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "e2b4d708-f3d5-4af4-9b39-f7e78a2c948b",
      "number": 15,
      "content": "Tức các con cháu của Gia-cốp và Giô-sép. (Sê-la)",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.782Z",
      "updatedAt": "2024-09-07T14:11:00.782Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "a21ba304-af35-45d0-abc2-856c0a98111f",
      "number": 16,
      "content": "Đức Chúa Trời ôi, khi các dòng nước thấy Ngài,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.787Z",
      "updatedAt": "2024-09-07T14:11:00.787Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "ad5bc979-1839-45a5-a989-34fe02f876f3",
      "number": 16,
      "content": "Khi các dòng nước thấy Ngài, chúng đều kinh hãi;",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.787Z",
      "updatedAt": "2024-09-07T14:11:00.787Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "e64f20b3-d6e0-4cbe-b87c-67814eb0b2e5",
      "number": 16,
      "content": "Thật vậy, các vực sâu run lên cầm cập;",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.787Z",
      "updatedAt": "2024-09-07T14:11:00.787Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "71d8504d-6f2b-4703-8b87-01396cc431e3",
      "number": 17,
      "content": "Các tầng mây lật đật đổ mưa;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.793Z",
      "updatedAt": "2024-09-07T14:11:00.793Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "cb8d4080-6c99-4563-bd96-2d42ca615f66",
      "number": 17,
      "content": "Bầu trời phát ra sấm sét ầm ầm;",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.793Z",
      "updatedAt": "2024-09-07T14:11:00.793Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "112eca30-02d8-4ce4-a7ec-e84f7afec8d6",
      "number": 17,
      "content": "Chớp lóe lên sáng rực khắp không trung.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.793Z",
      "updatedAt": "2024-09-07T14:11:00.793Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "e0e0a695-ef05-4a5c-8e8c-e467639ee36b",
      "number": 18,
      "content": "Sấm nổ vang rền quyện trong gió hú;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.798Z",
      "updatedAt": "2024-09-07T14:11:00.798Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "424d7c41-0a03-48cb-8712-91c205eb59ca",
      "number": 18,
      "content": "Sét nhoáng lên rực sáng khắp thế gian;",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.798Z",
      "updatedAt": "2024-09-07T14:11:00.798Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "1901aa3e-bdc5-4c08-ae90-cbcd24c989e2",
      "number": 18,
      "content": "Trái đất run lập cập và lắc lư chao đảo.",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.798Z",
      "updatedAt": "2024-09-07T14:11:00.798Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "c2156d4d-16f0-46e5-8518-13eb3868ab9d",
      "number": 19,
      "content": "Con đường của Ngài chạy ngang qua biển cả;",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.803Z",
      "updatedAt": "2024-09-07T14:11:00.803Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "e32dca2d-56b3-40a8-bff1-de0bd7d703b7",
      "number": 19,
      "content": "Các lối của Ngài xuyên thẳng giữa các luồng nước của đại dương,",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.803Z",
      "updatedAt": "2024-09-07T14:11:00.803Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "a71aba03-964e-436e-9788-9fb7c5828ef7",
      "number": 19,
      "content": "Dù các dấu chân Ngài chẳng ai biết được.",
      "order": 2,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.803Z",
      "updatedAt": "2024-09-07T14:11:00.803Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "b0357119-45a0-43bc-9b4c-b4dc34b298fa",
      "number": 20,
      "content": "Ngài dẫn dắt con dân Ngài như dẫn dắt một đàn chiên",
      "order": 0,
      "parNumber": 48,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.809Z",
      "updatedAt": "2024-09-07T14:11:00.809Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    },
    {
      "id": "042fa609-0e5d-40d7-bf79-a1aed6600591",
      "number": 20,
      "content": "Bằng tay của Môi-se và A-rôn.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:11:00.809Z",
      "updatedAt": "2024-09-07T14:11:00.809Z",
      "chapterId": "aa0c5d8f-7728-496e-baa7-b43af7229422"
    }
  ],
