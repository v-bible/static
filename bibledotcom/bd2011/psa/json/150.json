  },
  "verses": [
    {
      "id": "18c518ce-e3c0-4e46-a73d-0204eeb0fe46",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.478Z",
      "updatedAt": "2024-09-07T14:30:50.478Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "3c21604d-534e-42d1-9f6b-0ccb8eb63a00",
      "number": 1,
      "content": "Hãy ca ngợi Đức Chúa Trời trong nơi thánh Ngài;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.478Z",
      "updatedAt": "2024-09-07T14:30:50.478Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "56facbd6-857f-45ef-a1f8-6c463d262365",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong bầu trời lớn lao vô tận của Ngài.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.478Z",
      "updatedAt": "2024-09-07T14:30:50.478Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "ef12bc39-2d2f-4c8c-b21e-258f19dbd066",
      "number": 2,
      "content": "Hãy ca ngợi Ngài về những việc quyền năng Ngài;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.487Z",
      "updatedAt": "2024-09-07T14:30:50.487Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "90105f92-fbf4-4b92-8b95-e58bb98eac89",
      "number": 2,
      "content": "Hãy ca ngợi Ngài theo sự lớn lao vĩ đại của Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.487Z",
      "updatedAt": "2024-09-07T14:30:50.487Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "d82ed7db-d556-4113-a540-4f2b31b8f002",
      "number": 3,
      "content": "Hãy dùng tiếng kèn ca ngợi Ngài;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.490Z",
      "updatedAt": "2024-09-07T14:30:50.490Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "4bc14c18-d962-4b57-ae18-8de691212597",
      "number": 3,
      "content": "Hãy dùng đàn lia và hạc cầm ca ngợi Ngài;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.490Z",
      "updatedAt": "2024-09-07T14:30:50.490Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "abb08b88-b893-421d-81fa-1340910660fa",
      "number": 4,
      "content": "Hãy dùng trống lục lạc và điệu vũ ca ngợi Ngài;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.494Z",
      "updatedAt": "2024-09-07T14:30:50.494Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "69b820b3-2edf-44c5-ad83-eb97009395de",
      "number": 4,
      "content": "Hãy dùng các nhạc khí bằng dây và ống sáo ca ngợi Ngài;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.494Z",
      "updatedAt": "2024-09-07T14:30:50.494Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "eb6eae76-b8a0-4f9b-bf4d-bb7062a8ac94",
      "number": 5,
      "content": "Hãy dùng các phèng la vang tiếng ca ngợi Ngài;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.497Z",
      "updatedAt": "2024-09-07T14:30:50.497Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "35b3e22e-e0cb-4a24-8ec1-680a4e98feb0",
      "number": 5,
      "content": "Hãy dùng các khánh chiêng vang rền ca ngợi Ngài;",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.497Z",
      "updatedAt": "2024-09-07T14:30:50.497Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "18d0c843-a63f-484a-a59a-2915576642ce",
      "number": 6,
      "content": "Phàm vật chi thở, hãy ca ngợi CHÚA.",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.499Z",
      "updatedAt": "2024-09-07T14:30:50.499Z",
      "chapterId": "32d985f0-9975-494c-8193-2b2f8956e991"
    },
    {
      "id": "13acdf1f-4f5f-4564-bfca-9180c1409f8c",
      "number": 6,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:50.499Z",
