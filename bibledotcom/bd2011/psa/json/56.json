{
  "chapter": {
    "id": "f849cfec-a998-4a3f-8981-a0093473cd10",
    "number": 56,
    "ref": "https://www.bible.com/bible/19/PSA.56.BD2011",
    "createdAt": "2024-09-23T05:08:40.408Z",
    "updatedAt": "2024-09-23T05:08:40.408Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "981c8c5f-fe31-4cf3-8552-229b20534050",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin thương xót con, vì người ta chà đạp con;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.192Z",
      "updatedAt": "2024-09-23T05:48:30.192Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "802c5809-611e-4aef-bf36-c2a6c15be1fb",
      "number": 1,
      "content": "Hằng ngày họ cứ gây sự và hà hiếp con.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.231Z",
      "updatedAt": "2024-09-23T05:48:30.231Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "88297be9-e694-435e-bb9f-309248df606e",
      "number": 2,
      "content": "Quân thù con chà đạp con suốt ngày;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.236Z",
      "updatedAt": "2024-09-23T05:48:30.236Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "870aea07-3fd3-4f40-9522-61fe7b08d06a",
      "number": 2,
      "content": "Nhiều kẻ tấn công con thay, ôi Đấng Chí Cao.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.241Z",
      "updatedAt": "2024-09-23T05:48:30.241Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "33793eb8-8e21-4aba-b941-569e5d2bdaca",
      "number": 3,
      "content": "Mỗi khi con lo sợ,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.246Z",
      "updatedAt": "2024-09-23T05:48:30.246Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "8af7c870-3756-4fef-b08c-9d926b43e04e",
      "number": 3,
      "content": "Con đặt trọn lòng tin cậy nơi Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.252Z",
      "updatedAt": "2024-09-23T05:48:30.252Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "d94f3912-2c00-4df2-9905-fa99d0abc366",
      "number": 4,
      "content": "Trong Đức Chúa Trời, tôi ca ngợi lời Ngài;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.257Z",
      "updatedAt": "2024-09-23T05:48:30.257Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "8fec51dd-2959-4376-a6ae-8081c23791fc",
      "number": 10,
      "content": "Trong Đức Chúa Trời, tôi ca ngợi lời Ngài;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.339Z",
      "updatedAt": "2024-09-23T05:48:30.339Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "d4621c7b-9dcc-43b9-9e6d-5a3aca4ca604",
      "number": 11,
      "content": "Nơi Đức Chúa Trời, tôi đặt trọn niềm tin; tôi sẽ không sợ hãi;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.350Z",
      "updatedAt": "2024-09-23T05:48:30.350Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "45dfe279-495d-4a42-8b95-31ccae4bdbb0",
      "number": 4,
      "content": "Nơi Đức Chúa Trời, tôi đặt trọn niềm tin; tôi sẽ không sợ hãi;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.262Z",
      "updatedAt": "2024-09-23T05:48:30.262Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "6ee917d3-556a-48b1-8d92-e4a85dadee1a",
      "number": 11,
      "content": "Loài người làm chi tôi được?",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.354Z",
      "updatedAt": "2024-09-23T05:48:30.354Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "c10c6acd-92e0-474f-89e4-daa3fdd43e44",
      "number": 4,
      "content": "Loài người làm chi tôi được?",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.268Z",
      "updatedAt": "2024-09-23T05:48:30.268Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "e34abe18-a312-4a1b-876e-abbc93c05cbc",
      "number": 5,
      "content": "Suốt ngày họ cứ bóp méo lời con;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.272Z",
      "updatedAt": "2024-09-23T05:48:30.272Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "21b67531-baf2-43bd-b4d8-639e1e4a811b",
      "number": 5,
      "content": "Họ luôn luôn nghĩ cách hại con.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.277Z",
      "updatedAt": "2024-09-23T05:48:30.277Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "6a485993-34c4-495c-84fb-db9b3bc38db0",
      "number": 6,
      "content": "Họ tạo sự bài bác;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.282Z",
      "updatedAt": "2024-09-23T05:48:30.282Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "8ee1765b-4331-464d-bb39-46b182071a77",
      "number": 6,
      "content": "Họ ngấm ngầm phá hoại;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.287Z",
      "updatedAt": "2024-09-23T05:48:30.287Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "c812e017-b94f-4eb9-b38c-ae074a1beeb9",
      "number": 6,
      "content": "Họ rình rập các bước con hầu chờ dịp lấy mạng con.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.292Z",
      "updatedAt": "2024-09-23T05:48:30.292Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "987d1137-2893-4cac-a70a-aa6cda4d9efc",
      "number": 7,
      "content": "Họ có thể nhờ gian ác mình mà tránh khỏi bị phạt sao?",
      "order": 0,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.296Z",
      "updatedAt": "2024-09-23T05:48:30.296Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "491dab22-0a80-4514-8d46-9a2f16013bfe",
      "number": 7,
      "content": "Đức Chúa Trời ôi, xin nổi thịnh nộ lên và trừng phạt họ.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.302Z",
      "updatedAt": "2024-09-23T05:48:30.302Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "893487a0-ec9b-4d59-9278-010dc4a92ea6",
      "number": 8,
      "content": "Ngài đã dõi theo những bước lang thang trốn tránh của con;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.307Z",
      "updatedAt": "2024-09-23T05:48:30.307Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "6f019381-9368-4743-a71c-e23869e3e85d",
      "number": 8,
      "content": "Xin để nước mắt con trong ve của Ngài;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.313Z",
      "updatedAt": "2024-09-23T05:48:30.313Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "ddcd56de-dd54-4b1a-bdbe-acf38e80122e",
      "number": 8,
      "content": "Nước mắt con chẳng đã được ghi vào sách của Ngài sao?",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.318Z",
      "updatedAt": "2024-09-23T05:48:30.318Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "a6d61ba5-2973-4066-88b3-188354c9e7b1",
      "number": 9,
      "content": "Những kẻ thù của con sẽ bỏ đi",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.324Z",
      "updatedAt": "2024-09-23T05:48:30.324Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "c08c81b2-1def-49d6-a75d-0cfd8818cdf5",
      "number": 9,
      "content": "Trong ngày con kêu cầu;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.329Z",
      "updatedAt": "2024-09-23T05:48:30.329Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "2dc06916-9425-41ea-953d-996d9eb9bce8",
      "number": 9,
      "content": "Con biết rõ điều đó, vì Đức Chúa Trời binh vực con.",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.334Z",
      "updatedAt": "2024-09-23T05:48:30.334Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "fbcaec41-176e-438a-86d3-ea53d325c9e6",
      "number": 10,
      "content": "Trong **CHÚA**, tôi ca ngợi lời Ngài;",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.344Z",
      "updatedAt": "2024-09-23T05:48:30.344Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "aafa5570-a450-4b2a-bff5-0c66162c74e1",
      "number": 12,
      "content": "Đức Chúa Trời ôi, con sẽ làm những gì con đã thệ nguyện;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.359Z",
      "updatedAt": "2024-09-23T05:48:30.359Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "247075e9-d3c2-4b5e-b5e6-2af864e3fe79",
      "number": 12,
      "content": "Con sẽ dâng của lễ tạ ơn lên Ngài.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.364Z",
      "updatedAt": "2024-09-23T05:48:30.364Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "f8fe860f-d24f-44f6-abe8-f6c7af8256dc",
      "number": 13,
      "content": "Vì chẳng phải Ngài đã giải cứu linh hồn con khỏi chết,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.369Z",
      "updatedAt": "2024-09-23T05:48:30.369Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "1ba75d2e-7271-43c5-8ee2-c66987825775",
      "number": 13,
      "content": "Khiến chân con không bị vấp ngã,",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.374Z",
      "updatedAt": "2024-09-23T05:48:30.374Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "505cff69-6525-4ff8-8e78-31b54c5987f0",
      "number": 13,
      "content": "Để con có thể bước đi trước mặt Đức Chúa Trời",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.380Z",
      "updatedAt": "2024-09-23T05:48:30.380Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    },
    {
      "id": "6cc5bf87-af00-4ba6-bab9-71a6c1eba986",
      "number": 13,
      "content": "Trong ánh sáng của sự sống hay sao?",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:48:30.385Z",
      "updatedAt": "2024-09-23T05:48:30.385Z",
      "chapterId": "f849cfec-a998-4a3f-8981-a0093473cd10"
    }
  ],
