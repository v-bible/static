{
  "chapter": {
    "id": "d7758ea8-59d6-4521-8cf3-226f4d6741dc",
    "number": 56,
    "ref": "https://www.bible.com/bible/19/PSA.56.BD2011",
    "createdAt": "2024-09-07T11:51:11.062Z",
    "updatedAt": "2024-09-07T11:51:11.062Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "4c40de4a-e466-4826-a8b1-6b255e206b59",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin thương xót con, vì người ta chà đạp con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.733Z",
      "updatedAt": "2024-09-07T14:05:35.733Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "81b78aed-4302-4efa-8128-620af1de4a6c",
      "number": 1,
      "content": "Hằng ngày họ cứ gây sự và hà hiếp con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.733Z",
      "updatedAt": "2024-09-07T14:05:35.733Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "a13fb6f9-a070-4914-9e75-7ba6b4384c56",
      "number": 2,
      "content": "Quân thù con chà đạp con suốt ngày;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.743Z",
      "updatedAt": "2024-09-07T14:05:35.743Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "2a96d0cf-2631-40fc-a05d-fedda69ca31b",
      "number": 2,
      "content": "Nhiều kẻ tấn công con thay, ôi Đấng Chí Cao.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.743Z",
      "updatedAt": "2024-09-07T14:05:35.743Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "908d1d2c-ad33-422d-bb6a-a0db2b6a4691",
      "number": 3,
      "content": "Mỗi khi con lo sợ,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.746Z",
      "updatedAt": "2024-09-07T14:05:35.746Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "7837a53a-e270-4712-aea1-27c3da9792f9",
      "number": 3,
      "content": "Con đặt trọn lòng tin cậy nơi Ngài.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.746Z",
      "updatedAt": "2024-09-07T14:05:35.746Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "6dee4648-bd21-426c-8f40-9ce553557c53",
      "number": 4,
      "content": "Trong Đức Chúa Trời, tôi ca ngợi lời Ngài;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.748Z",
      "updatedAt": "2024-09-07T14:05:35.748Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "797088f0-fd99-4f2f-83c8-9b1cef68942f",
      "number": 4,
      "content": "Nơi Đức Chúa Trời, tôi đặt trọn niềm tin; tôi sẽ không sợ hãi;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.748Z",
      "updatedAt": "2024-09-07T14:05:35.748Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "9ce52565-e160-48ad-a292-7a927297976a",
      "number": 4,
      "content": "Loài người làm chi tôi được?",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.748Z",
      "updatedAt": "2024-09-07T14:05:35.748Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "611dc100-1147-4ebc-8f15-48f08b99a15f",
      "number": 5,
      "content": "Suốt ngày họ cứ bóp méo lời con;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.750Z",
      "updatedAt": "2024-09-07T14:05:35.750Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "6ebc9635-5099-4495-a92f-eaf94b0a4c98",
      "number": 5,
      "content": "Họ luôn luôn nghĩ cách hại con.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.750Z",
      "updatedAt": "2024-09-07T14:05:35.750Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "f4ad003e-9b1d-430e-8c79-effad7adf8d2",
      "number": 6,
      "content": "Họ tạo sự bài bác;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.753Z",
      "updatedAt": "2024-09-07T14:05:35.753Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "4accfa5b-15c1-498a-8bb1-fbc219ccf87f",
      "number": 6,
      "content": "Họ ngấm ngầm phá hoại;",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.753Z",
      "updatedAt": "2024-09-07T14:05:35.753Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "7f5146f9-f8ab-41b7-9a31-efd80ef6d441",
      "number": 6,
      "content": "Họ rình rập các bước con hầu chờ dịp lấy mạng con.",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.753Z",
      "updatedAt": "2024-09-07T14:05:35.753Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "0eaddebd-c022-4b27-aada-bffc490a41e8",
      "number": 7,
      "content": "Họ có thể nhờ gian ác mình mà tránh khỏi bị phạt sao?",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.755Z",
      "updatedAt": "2024-09-07T14:05:35.755Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "62f43fb7-6239-4427-a22a-cbced6424f56",
      "number": 7,
      "content": "Đức Chúa Trời ôi, xin nổi thịnh nộ lên và trừng phạt họ.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.755Z",
      "updatedAt": "2024-09-07T14:05:35.755Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "50ccf516-0671-4fdf-b2fb-28b954938b37",
      "number": 8,
      "content": "Ngài đã dõi theo những bước lang thang trốn tránh của con;",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.757Z",
      "updatedAt": "2024-09-07T14:05:35.757Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "01e80ea6-c702-4a7b-ac3b-226007255f19",
      "number": 8,
      "content": "Xin để nước mắt con trong ve của Ngài;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.757Z",
      "updatedAt": "2024-09-07T14:05:35.757Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "3bb80553-56d5-428e-8b1e-a777db8aa6c0",
      "number": 8,
      "content": "Nước mắt con chẳng đã được ghi vào sách của Ngài sao?",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.757Z",
      "updatedAt": "2024-09-07T14:05:35.757Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "f27ecd4a-aac9-41c8-bbfd-06e250387d62",
      "number": 9,
      "content": "Những kẻ thù của con sẽ bỏ đi",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.760Z",
      "updatedAt": "2024-09-07T14:05:35.760Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "37b81e5c-4182-4c0c-bca7-ba909fbd8cde",
      "number": 9,
      "content": "Trong ngày con kêu cầu;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.760Z",
      "updatedAt": "2024-09-07T14:05:35.760Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "6ea52ba0-70aa-4d18-be44-652bd72afe9e",
      "number": 9,
      "content": "Con biết rõ điều đó, vì Đức Chúa Trời binh vực con.",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.760Z",
      "updatedAt": "2024-09-07T14:05:35.760Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "e39c2e90-739a-43b4-8a0d-df3bf9c43b7d",
      "number": 10,
      "content": "Trong Đức Chúa Trời, tôi ca ngợi lời Ngài;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.762Z",
      "updatedAt": "2024-09-07T14:05:35.762Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "3cfd5e32-3a16-4866-ae4c-417d23459518",
      "number": 10,
      "content": "Trong CHÚA, tôi ca ngợi lời Ngài;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.762Z",
      "updatedAt": "2024-09-07T14:05:35.762Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "39199e70-33a9-4afa-9084-8aa76775ecf4",
      "number": 11,
      "content": "Nơi Đức Chúa Trời, tôi đặt trọn niềm tin; tôi sẽ không sợ hãi;",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.764Z",
      "updatedAt": "2024-09-07T14:05:35.764Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "d1d8c916-a8b0-48a0-a9fd-ffb1c0ffc639",
      "number": 11,
      "content": "Loài người làm chi tôi được?",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.764Z",
      "updatedAt": "2024-09-07T14:05:35.764Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "d796e0b8-d47d-4445-9d2f-4a3a4902364f",
      "number": 12,
      "content": "Đức Chúa Trời ôi, con sẽ làm những gì con đã thệ nguyện;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.766Z",
      "updatedAt": "2024-09-07T14:05:35.766Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "e57a664a-e69c-476a-8df6-313c61bfb307",
      "number": 12,
      "content": "Con sẽ dâng của lễ tạ ơn lên Ngài.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.766Z",
      "updatedAt": "2024-09-07T14:05:35.766Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "712a0d8c-6372-478d-b1f9-27e6a0a212a0",
      "number": 13,
      "content": "Vì chẳng phải Ngài đã giải cứu linh hồn con khỏi chết,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.768Z",
      "updatedAt": "2024-09-07T14:05:35.768Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "af948868-5ff9-459c-8bc5-22eadc0c7658",
      "number": 13,
      "content": "Khiến chân con không bị vấp ngã,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.768Z",
      "updatedAt": "2024-09-07T14:05:35.768Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "9be5b9fc-4ba7-45ba-a119-3c7adc647837",
      "number": 13,
      "content": "Để con có thể bước đi trước mặt Đức Chúa Trời",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.768Z",
      "updatedAt": "2024-09-07T14:05:35.768Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    },
    {
      "id": "830ef034-b5cd-4867-8eb7-424c5f239f5d",
      "number": 13,
      "content": "Trong ánh sáng của sự sống hay sao?",
      "order": 3,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:35.768Z",
      "updatedAt": "2024-09-07T14:05:35.768Z",
      "chapterId": "d7758ea8-59d6-4521-8cf3-226f4d6741dc"
    }
  ],
