{
  "chapter": {
    "id": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b",
    "number": 127,
    "ref": "https://www.bible.com/bible/19/PSA.127.BD2011",
    "createdAt": "2024-09-07T11:51:11.538Z",
    "updatedAt": "2024-09-07T11:51:11.538Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "22588f04-7535-4c46-b340-c271c8cf37bc",
      "number": 1,
      "content": "Nếu CHÚA không xây dựng nhà,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.319Z",
      "updatedAt": "2024-09-07T14:22:37.319Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "2d2a4c41-f74c-412c-a8df-92dfd834b3e0",
      "number": 1,
      "content": "Những thợ xây làm việc uổng công;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.319Z",
      "updatedAt": "2024-09-07T14:22:37.319Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "332ec6b3-8f18-4395-8192-ecb15821b58e",
      "number": 1,
      "content": "Nếu CHÚA không canh giữ thành,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.319Z",
      "updatedAt": "2024-09-07T14:22:37.319Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "c8b26055-c49b-41f8-82b7-010f65e9333f",
      "number": 1,
      "content": "Những lính canh thức canh uổng công.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.319Z",
      "updatedAt": "2024-09-07T14:22:37.319Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "a7f84dbf-541b-4046-99d1-331fedd356a0",
      "number": 2,
      "content": "Uổng công thay cho các bạn là những người dậy sớm,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.329Z",
      "updatedAt": "2024-09-07T14:22:37.329Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "46611f66-7724-433f-a68a-4a6358d27a6b",
      "number": 2,
      "content": "Thức khuya, và ăn uống kham khổ,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.329Z",
      "updatedAt": "2024-09-07T14:22:37.329Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "bd1b473a-c1e1-48da-b2f6-af9f36fbd2aa",
      "number": 2,
      "content": "Vì Chúa sẽ cho người Ngài thương no đủ,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.329Z",
      "updatedAt": "2024-09-07T14:22:37.329Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "11b6d1a3-20ef-4a80-b6cf-563c80211302",
      "number": 2,
      "content": "Ngay cả khi họ đang an giấc ngủ say.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.329Z",
      "updatedAt": "2024-09-07T14:22:37.329Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "493ef52f-c200-42f8-b365-6aa20450f961",
      "number": 3,
      "content": "Kìa, con cái là cơ nghiệp của CHÚA ban cho;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.331Z",
      "updatedAt": "2024-09-07T14:22:37.331Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "8254fa3b-bdb6-46b7-9f75-86950dd39110",
      "number": 3,
      "content": "Bông trái của tử cung là phần thưởng của Ngài trao tặng.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.331Z",
      "updatedAt": "2024-09-07T14:22:37.331Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "d13fdb9e-a0d2-4fb3-9196-09e927592add",
      "number": 4,
      "content": "Con cái sinh ra trong tuổi thanh xuân",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.334Z",
      "updatedAt": "2024-09-07T14:22:37.334Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "be4dfa39-6f6b-485b-9ca1-1f82956d0e2d",
      "number": 4,
      "content": "Khác nào các mũi tên trong tay dũng sĩ.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.334Z",
      "updatedAt": "2024-09-07T14:22:37.334Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "4d0978ff-9ebe-4900-972e-1ace9b989fe1",
      "number": 5,
      "content": "Phước cho người",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.335Z",
      "updatedAt": "2024-09-07T14:22:37.335Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "7d55b67d-ba83-4e37-993a-47ef15db5d63",
      "number": 5,
      "content": "Có đầy chúng trong ống tên mình;",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.335Z",
      "updatedAt": "2024-09-07T14:22:37.335Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "bc21120a-b529-499f-a79a-2d312f69225c",
      "number": 5,
      "content": "Họ sẽ không bẽ mặt",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.335Z",
      "updatedAt": "2024-09-07T14:22:37.335Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    },
    {
      "id": "92ace336-f79c-4ac7-86c7-cb2e4a3fa18c",
      "number": 5,
      "content": "Khi đối đáp với kẻ thù tại cổng thành.",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:22:37.335Z",
      "updatedAt": "2024-09-07T14:22:37.335Z",
      "chapterId": "3ffd4588-8b58-4243-a0f6-07b3fb9c517b"
    }
  ],
