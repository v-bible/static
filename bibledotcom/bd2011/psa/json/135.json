    },
    {
      "id": "a8e59a3a-160c-44ec-b472-afdac0da4327",
      "number": 19,
      "content": "Hỡi nhà I-sơ-ra-ên, hãy chúc tụng CHÚA!",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.610Z",
      "updatedAt": "2024-09-07T14:24:03.610Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "58de0ccb-5981-47c9-a80a-01a5e9887062",
      "number": 19,
      "content": "Hỡi nhà A-rôn, hãy chúc tụng CHÚA!",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.610Z",
      "updatedAt": "2024-09-07T14:24:03.610Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "c03fa244-47fd-4e6a-a826-495e0d955280",
      "number": 20,
      "content": "Hỡi nhà Lê-vi, hãy chúc tụng CHÚA!",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.612Z",
      "updatedAt": "2024-09-07T14:24:03.612Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "49f363af-218c-4111-9996-4c65900e45a8",
      "number": 20,
      "content": "Hỡi các bạn, những người kính sợ CHÚA, hãy chúc tụng CHÚA!",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.612Z",
      "updatedAt": "2024-09-07T14:24:03.612Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "246cbc04-e7cd-478d-a990-eaaf7a0314d4",
      "number": 21,
      "content": "Hỡi những người sống ở Si-ôn, hãy chúc tụng CHÚA,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.614Z",
      "updatedAt": "2024-09-07T14:24:03.614Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "8818fa60-a219-40e5-b4d3-451efbe84894",
      "number": 21,
      "content": "Đấng ngự tại Giê-ru-sa-lem!",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.614Z",
      "updatedAt": "2024-09-07T14:24:03.614Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "6694a310-337f-434d-864e-dd62904cae79",
      "number": 21,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 2,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.614Z",
      "updatedAt": "2024-09-07T14:24:03.614Z",
