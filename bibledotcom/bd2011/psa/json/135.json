{
  "chapter": {
    "id": "cf0c09bc-0f58-402e-b07d-e737267e89c1",
    "number": 135,
    "ref": "https://www.bible.com/bible/19/PSA.135.BD2011",
    "createdAt": "2024-09-07T11:51:11.559Z",
    "updatedAt": "2024-09-07T11:51:11.559Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "9212ab6a-2791-4f09-ac6d-ae34000be902",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.556Z",
      "updatedAt": "2024-09-07T14:24:03.556Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "8a4eec98-616d-49aa-a032-71ba1af7f6f8",
      "number": 1,
      "content": "Hãy ca ngợi danh CHÚA!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.556Z",
      "updatedAt": "2024-09-07T14:24:03.556Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "a01ecece-80c4-48dd-9429-e2059cabe1ad",
      "number": 1,
      "content": "Hỡi các tôi tớ CHÚA, hãy ca ngợi Ngài.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.556Z",
      "updatedAt": "2024-09-07T14:24:03.556Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "0c7f3610-8b19-44d7-a9a0-0e1f6d0a8b09",
      "number": 2,
      "content": "Hỡi các bạn, những người đứng trong nhà CHÚA,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.565Z",
      "updatedAt": "2024-09-07T14:24:03.565Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "0bedd82e-b19c-4dae-8af0-449097dc69b7",
      "number": 2,
      "content": "Những người đứng trong sân đền thờ Đức Chúa Trời chúng ta,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.565Z",
      "updatedAt": "2024-09-07T14:24:03.565Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "1efffe90-e46e-4084-b233-20f07367072f",
      "number": 3,
      "content": "Hãy ca ngợi CHÚA, vì CHÚA thật tốt;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.568Z",
      "updatedAt": "2024-09-07T14:24:03.568Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "1d7a11ed-f8a1-4b3a-beb3-1e6ed84e0c05",
      "number": 3,
      "content": "Hãy ca tụng danh Ngài, vì danh Ngài đáng kính yêu;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.568Z",
      "updatedAt": "2024-09-07T14:24:03.568Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "a65b9f12-4250-43af-b74c-e3d208989a65",
      "number": 4,
      "content": "Vì CHÚA đã chọn Gia-cốp cho Ngài,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.571Z",
      "updatedAt": "2024-09-07T14:24:03.571Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "d5145f6d-f8f1-4ac2-9699-2de2c16e4532",
      "number": 4,
      "content": "I-sơ-ra-ên làm tài sản riêng của Ngài.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.571Z",
      "updatedAt": "2024-09-07T14:24:03.571Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "a31a4c3e-02dc-404f-baef-ada142483234",
      "number": 5,
      "content": "Vì tôi biết rằng CHÚA thật vĩ đại;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.575Z",
      "updatedAt": "2024-09-07T14:24:03.575Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "d3959cfb-aa83-4d10-8545-582decc71173",
      "number": 5,
      "content": "Chúa của chúng ta vĩ đại hơn tất cả thần thánh của loài người.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.575Z",
      "updatedAt": "2024-09-07T14:24:03.575Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "8747f80e-c63d-4801-831d-69a3dfb18bf6",
      "number": 6,
      "content": "Bất cứ điều gì CHÚA muốn, Ngài làm điều ấy,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.577Z",
      "updatedAt": "2024-09-07T14:24:03.577Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "f85462e6-a3bf-4e58-9e4e-94614a34fab7",
      "number": 6,
      "content": "Bất kể trên trời hay dưới đất,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.577Z",
      "updatedAt": "2024-09-07T14:24:03.577Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "a5dc6090-9f5e-4bb8-a113-7111d8073f40",
      "number": 6,
      "content": "Trong lòng biển hay trong các vực sâu.",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.577Z",
      "updatedAt": "2024-09-07T14:24:03.577Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "46fd2c78-b1e0-4536-b1aa-157705aeee0b",
      "number": 7,
      "content": "Ngài khiến mây trồi lên từ những nơi tận cùng trái đất;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.580Z",
      "updatedAt": "2024-09-07T14:24:03.580Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "262bc0f7-8e6a-4c3a-88d8-344a1c98543a",
      "number": 7,
      "content": "Ngài làm cho sấm sét nổ vang khi trời đổ cơn mưa;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.580Z",
      "updatedAt": "2024-09-07T14:24:03.580Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "fd3c4942-c2ec-429b-9a01-b6edeb2aae78",
      "number": 7,
      "content": "Ngài sai gió bay ra từ các kho lẫm của Ngài.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.580Z",
      "updatedAt": "2024-09-07T14:24:03.580Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "38165aa9-759b-4be9-968b-030643a1cc00",
      "number": 8,
      "content": "Ngài đánh hạ các con đầu lòng của Ai-cập,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.583Z",
      "updatedAt": "2024-09-07T14:24:03.583Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "c17dd88c-ac6f-4e1b-8125-a3c3abb5d2e4",
      "number": 8,
      "content": "Con của loài người và con của súc vật.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.583Z",
      "updatedAt": "2024-09-07T14:24:03.583Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "b03e3b6a-53d6-4fc9-bb67-fdf0fc07a353",
      "number": 9,
      "content": "Hỡi Ai-cập, Ngài sai các dấu kỳ và phép lạ đến giữa ngươi,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.586Z",
      "updatedAt": "2024-09-07T14:24:03.586Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "4a5c108f-6ece-4282-9390-d9cb2e9926d3",
      "number": 9,
      "content": "Để trừng phạt Pha-ra-ôn và các tôi tớ của ông ta.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.586Z",
      "updatedAt": "2024-09-07T14:24:03.586Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "54d65206-b026-47fe-94a0-e140407cc037",
      "number": 10,
      "content": "Ngài đánh bại nhiều dân tộc,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.589Z",
      "updatedAt": "2024-09-07T14:24:03.589Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "662a9771-4d86-4d5f-8462-b6920c5a115a",
      "number": 10,
      "content": "Và tiêu diệt các vua mạnh mẽ;",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.589Z",
      "updatedAt": "2024-09-07T14:24:03.589Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "066a729b-73d7-40e5-b22f-e7e9773cd9c0",
      "number": 11,
      "content": "Si-hôn vua dân A-mô-ri,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.591Z",
      "updatedAt": "2024-09-07T14:24:03.591Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "32458bdc-1b31-4bf0-a82c-75eeed540910",
      "number": 11,
      "content": "Óc vua Ba-san,",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.591Z",
      "updatedAt": "2024-09-07T14:24:03.591Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "8edc1fdf-cab4-4d4a-8d9d-0750bf9f49ac",
      "number": 11,
      "content": "Và tất cả các vương quốc ở Ca-na-an.",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.591Z",
      "updatedAt": "2024-09-07T14:24:03.591Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "60ba3d17-8ce9-4c12-947b-aa71f0f43910",
      "number": 12,
      "content": "Ngài ban xứ của họ để làm sản nghiệp,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.594Z",
      "updatedAt": "2024-09-07T14:24:03.594Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "7da4b240-ec04-463f-ad89-77ddbb49324a",
      "number": 12,
      "content": "Một sản nghiệp cho dân I-sơ-ra-ên của Ngài.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.594Z",
      "updatedAt": "2024-09-07T14:24:03.594Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "3c2c9d7d-7e84-49a0-a563-4dd3eec78d4d",
      "number": 13,
      "content": "CHÚA ôi, danh Ngài sẽ trường tồn mãi mãi;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.596Z",
      "updatedAt": "2024-09-07T14:24:03.596Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "fffe535a-d11a-4c78-bbca-1c5546f881f8",
      "number": 13,
      "content": "CHÚA ôi, công ơn Ngài sẽ được ghi nhớ từ đời nọ đến đời kia,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.596Z",
      "updatedAt": "2024-09-07T14:24:03.596Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "04e2d081-5d11-407b-a810-0365f084744e",
      "number": 14,
      "content": "Vì CHÚA sẽ xét đoán dân Ngài;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.598Z",
      "updatedAt": "2024-09-07T14:24:03.598Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "e0ff02ac-2e39-45d6-a9c2-01c47c8860a1",
      "number": 14,
      "content": "Ngài sẽ tỏ lòng thương xót đối với các tôi tớ Ngài.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.598Z",
      "updatedAt": "2024-09-07T14:24:03.598Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "9074400f-caef-4e2e-9791-f0a366aa7cb4",
      "number": 15,
      "content": "Các thần tượng của các dân chẳng qua là bạc và vàng,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.601Z",
      "updatedAt": "2024-09-07T14:24:03.601Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "c84a3b7e-d352-4338-a1f7-9622c36a3c9c",
      "number": 15,
      "content": "Là sản phẩm do tay người làm ra.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.601Z",
      "updatedAt": "2024-09-07T14:24:03.601Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "46b7b33b-f009-4d2a-9fcb-cd8e4aa533b0",
      "number": 16,
      "content": "Chúng có miệng mà chẳng nói;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.603Z",
      "updatedAt": "2024-09-07T14:24:03.603Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "6276a13d-7f18-47ef-8279-44f5aaf6792d",
      "number": 16,
      "content": "Chúng có mắt mà chẳng thấy;",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.603Z",
      "updatedAt": "2024-09-07T14:24:03.603Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "70ea3122-67d0-4cfa-ae36-24fad5bdf139",
      "number": 17,
      "content": "Chúng có tai mà chẳng nghe;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.605Z",
      "updatedAt": "2024-09-07T14:24:03.605Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "fb66588b-077d-4117-af1c-b485ae492d69",
      "number": 17,
      "content": "Không có chút hơi thở nào trong miệng chúng.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.605Z",
      "updatedAt": "2024-09-07T14:24:03.605Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "404f9222-c885-4c62-8438-8dfcc07d7ea2",
      "number": 18,
      "content": "Nguyện những kẻ làm thần tượng sẽ trở nên như chúng,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.607Z",
      "updatedAt": "2024-09-07T14:24:03.607Z",
      "chapterId": "cf0c09bc-0f58-402e-b07d-e737267e89c1"
    },
    {
      "id": "e6f06558-fb0b-41c2-8cc5-29838e700e14",
      "number": 18,
      "content": "Và những ai tin cậy vào thần tượng cũng giống như vậy.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:24:03.607Z",
      "updatedAt": "2024-09-07T14:24:03.607Z",
