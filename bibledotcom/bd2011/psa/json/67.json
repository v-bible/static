{
  "chapter": {
    "id": "173427aa-f5ee-4597-a97b-5745638a34b1",
    "number": 67,
    "ref": "https://www.bible.com/bible/19/PSA.67.BD2011",
    "createdAt": "2024-09-23T05:08:40.444Z",
    "updatedAt": "2024-09-23T05:08:40.444Z",
    "bookId": "1840e6a0-f360-4f40-9af5-0d79349280d4"
  },
  "verses": [
    {
      "id": "4723fade-ba0b-4ff9-932d-07e595dbe7de",
      "number": 1,
      "content": "Cầu xin Đức Chúa Trời thương xót chúng con và ban phước cho chúng con,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.167Z",
      "updatedAt": "2024-09-23T05:49:33.167Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "6ba56df9-3bd4-460f-a4c5-cda611448fe6",
      "number": 1,
      "content": "Cầu xin Ngài chiếu sáng mặt Ngài trên chúng con, (Sê-la)",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.195Z",
      "updatedAt": "2024-09-23T05:49:33.195Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "b2ff2d4f-a21c-4e18-ae0f-87a468ef3442",
      "number": 2,
      "content": "Để đường lối Ngài được biết đến trên đất,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.200Z",
      "updatedAt": "2024-09-23T05:49:33.200Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "24f8ba55-c324-4ad2-a63f-67f80fa2800b",
      "number": 2,
      "content": "Và ơn cứu rỗi Ngài được phổ cập giữa các dân.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.205Z",
      "updatedAt": "2024-09-23T05:49:33.205Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "0ded4019-5978-4977-addc-2a81d8897d31",
      "number": 5,
      "content": "Đức Chúa Trời ôi, nguyện muôn dân ca ngợi Ngài;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.235Z",
      "updatedAt": "2024-09-23T05:49:33.235Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "70e53592-e6e9-48e1-af41-a284ba0a7e87",
      "number": 3,
      "content": "Đức Chúa Trời ôi, nguyện muôn dân ca ngợi Ngài;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.210Z",
      "updatedAt": "2024-09-23T05:49:33.210Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "4818f44f-b2e9-4f63-a398-cb841c343f37",
      "number": 3,
      "content": "Nguyện tất cả các dân ca ngợi Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.214Z",
      "updatedAt": "2024-09-23T05:49:33.214Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "d8d4050c-3199-48e9-bb1b-8b7243c3eb3e",
      "number": 5,
      "content": "Nguyện tất cả các dân ca ngợi Ngài.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.240Z",
      "updatedAt": "2024-09-23T05:49:33.240Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "e0d2bc38-54c9-40e9-8891-ae6583c1a656",
      "number": 4,
      "content": "Nguyện các dân các nước vui mừng và hân hoan ca hát,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.220Z",
      "updatedAt": "2024-09-23T05:49:33.220Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "0574cb4d-bb74-4855-b215-24a1d298a923",
      "number": 4,
      "content": "Vì Ngài xét đoán muôn dân cách công minh,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.225Z",
      "updatedAt": "2024-09-23T05:49:33.225Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "028ec4e7-23a8-45fc-b07e-4dd387f3f281",
      "number": 4,
      "content": "Và lãnh đạo các quốc gia trên đất. (Sê-la)",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.230Z",
      "updatedAt": "2024-09-23T05:49:33.230Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "f71b6d61-00de-4968-8132-da45e6af12cc",
      "number": 6,
      "content": "Đất sẽ sinh sản hoa màu;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.244Z",
      "updatedAt": "2024-09-23T05:49:33.244Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "89789af4-7131-43f1-af58-143dd3a78294",
      "number": 6,
      "content": "Đức Chúa Trời, chính Đức Chúa Trời của chúng ta, sẽ ban phước cho chúng ta.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.249Z",
      "updatedAt": "2024-09-23T05:49:33.249Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "17bb3f1e-cd4c-46b9-9284-4720d9115635",
      "number": 7,
      "content": "Nguyện Đức Chúa Trời tiếp tục ban phước cho chúng ta,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.254Z",
      "updatedAt": "2024-09-23T05:49:33.254Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    },
    {
      "id": "054f6a41-5512-404f-9845-af63fbf71f6b",
      "number": 7,
      "content": "Để mọi người ở tận cùng trái đất kính sợ Ngài.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:49:33.258Z",
      "updatedAt": "2024-09-23T05:49:33.258Z",
      "chapterId": "173427aa-f5ee-4597-a97b-5745638a34b1"
    }
  ],
