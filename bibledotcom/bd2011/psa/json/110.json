{
  "chapter": {
    "id": "a0f5130e-2efb-46f9-8e34-60735c9bc674",
    "number": 110,
    "ref": "https://www.bible.com/bible/19/PSA.110.BD2011",
    "createdAt": "2024-09-07T11:51:11.487Z",
    "updatedAt": "2024-09-07T11:51:11.487Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "3159eefc-760c-43de-907d-7dd9d6d6b82e",
      "number": 1,
      "content": "CHÚA phán với Chúa của tôi,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.569Z",
      "updatedAt": "2024-09-07T14:18:03.569Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "865276d0-a8f0-4611-82b4-6f5c5d733033",
      "number": 1,
      "content": "“Con hãy ngồi bên phải Ta,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.569Z",
      "updatedAt": "2024-09-07T14:18:03.569Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "9db9aef6-235b-4b6b-8757-23d5c7563371",
      "number": 1,
      "content": "Cho đến khi Ta đặt những kẻ thù của Con làm bệ chân Con.”",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.569Z",
      "updatedAt": "2024-09-07T14:18:03.569Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "500d9726-1042-4f61-b32e-477f7250aa6e",
      "number": 2,
      "content": "“Từ Si-ôn CHÚA sẽ đưa vương trượng uy quyền của Con ra:",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.578Z",
      "updatedAt": "2024-09-07T14:18:03.578Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "3de732ca-53c3-49bb-95a5-7620213f73ca",
      "number": 2,
      "content": "Hãy trị vì những kẻ thù của Con.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.578Z",
      "updatedAt": "2024-09-07T14:18:03.578Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "21204eda-215f-492e-bb2f-85d11601feeb",
      "number": 3,
      "content": "Trong ngày Con biểu dương quyền lực,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.581Z",
      "updatedAt": "2024-09-07T14:18:03.581Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "0b4cbb94-9c66-43e6-9b90-e18fc4240c85",
      "number": 3,
      "content": "Dân tộc Con sẽ hăng hái xung phong;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.581Z",
      "updatedAt": "2024-09-07T14:18:03.581Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "e65cf7f1-6237-4f89-9f88-5d2a907736ed",
      "number": 3,
      "content": "Họ sẽ tập họp thành đội ngũ chỉnh tề khi hừng đông ló dạng;",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.581Z",
      "updatedAt": "2024-09-07T14:18:03.581Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "bd64870f-f143-453c-aa35-63cca7369595",
      "number": 3,
      "content": "Những trai trẻ sẽ quy tụ quanh Con",
      "order": 3,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.581Z",
      "updatedAt": "2024-09-07T14:18:03.581Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "ee4a36c7-8691-401c-8fc2-00593ff3a37e",
      "number": 3,
      "content": "Như sương phủ ban mai.”",
      "order": 4,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.581Z",
      "updatedAt": "2024-09-07T14:18:03.581Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "15fe4207-336e-46f3-8ed1-cca70f986d5e",
      "number": 4,
      "content": "CHÚA đã thề",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.584Z",
      "updatedAt": "2024-09-07T14:18:03.584Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "80e7af5e-d8a3-4ccf-bee4-7c8eb30cce31",
      "number": 4,
      "content": "Và sẽ không đổi ý của Ngài rằng,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.584Z",
      "updatedAt": "2024-09-07T14:18:03.584Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "514db5b6-d1e2-409e-b7cc-546229db1a99",
      "number": 4,
      "content": "“Con là tư tế đời đời",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.584Z",
      "updatedAt": "2024-09-07T14:18:03.584Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "657da6bc-87db-4615-999e-d3a8f378228a",
      "number": 4,
      "content": "Theo Dòng Mên-chi-xê-đéc.”",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.584Z",
      "updatedAt": "2024-09-07T14:18:03.584Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "6ef64e3f-ef4c-45cf-a747-db869379e767",
      "number": 5,
      "content": "Chúa ở bên phải Con.",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.586Z",
      "updatedAt": "2024-09-07T14:18:03.586Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "ede93fe4-539e-41d9-918b-2f6097fd647d",
      "number": 5,
      "content": "Ngài sẽ đập tan các vua trong ngày Ngài nổi giận.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.586Z",
      "updatedAt": "2024-09-07T14:18:03.586Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "3562c1f9-b46a-41de-be06-e8cbc1b5d8f9",
      "number": 6,
      "content": "Ngài sẽ đoán phạt các dân;",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.588Z",
      "updatedAt": "2024-09-07T14:18:03.588Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "ea9faa6b-6d78-4bb3-8e01-3a8a8398659b",
      "number": 6,
      "content": "Ngài sẽ làm cho thây những kẻ ác chất cao thành đống;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.588Z",
      "updatedAt": "2024-09-07T14:18:03.588Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "8534c84c-ad27-4c25-97a9-99c9ae55424f",
      "number": 6,
      "content": "Ngài sẽ đập tan các lãnh tụ của thế gian.",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.588Z",
      "updatedAt": "2024-09-07T14:18:03.588Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "590187b2-93be-454d-ba6f-daf365e91475",
      "number": 7,
      "content": "Ngài sẽ uống nước suối bên đường,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.591Z",
      "updatedAt": "2024-09-07T14:18:03.591Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    },
    {
      "id": "f9aa97cd-5f09-416d-af9e-6efee7f6801f",
      "number": 7,
      "content": "Rồi Ngài sẽ ngẩng đầu lên cách hiên ngang đắc thắng.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:03.591Z",
      "updatedAt": "2024-09-07T14:18:03.591Z",
      "chapterId": "a0f5130e-2efb-46f9-8e34-60735c9bc674"
    }
  ],
