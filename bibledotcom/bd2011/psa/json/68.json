{
  "chapter": {
    "id": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea",
    "number": 68,
    "ref": "https://www.bible.com/bible/19/PSA.68.BD2011",
    "createdAt": "2024-09-07T11:51:11.150Z",
    "updatedAt": "2024-09-07T11:51:11.150Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "725b7d7f-d2cb-48ba-b9a8-85ccaa41717f",
      "number": 1,
      "content": "Nguyện Đức Chúa Trời trỗi dậy; nguyện những kẻ chống nghịch Ngài tản lạc;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.049Z",
      "updatedAt": "2024-09-07T14:09:34.049Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "b26db78f-72c0-496e-9f76-b047eb58bbaf",
      "number": 1,
      "content": "Nguyện những kẻ ghét Ngài chạy trốn trước mặt Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.049Z",
      "updatedAt": "2024-09-07T14:09:34.049Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "7b16dadf-7d35-4064-96e2-6c13365ea9cd",
      "number": 2,
      "content": "Như khói bị tan biến thể nào, nguyện chúng bị tan đi thể ấy;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.051Z",
      "updatedAt": "2024-09-07T14:09:34.051Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "9e17cf93-2afd-4cb5-8211-ea7414e2be87",
      "number": 2,
      "content": "Như sáp bị tan chảy trước ngọn lửa thể nào,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.051Z",
      "updatedAt": "2024-09-07T14:09:34.051Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "ad8ae8aa-29ef-418c-8887-42b143dd32f6",
      "number": 2,
      "content": "Nguyện những kẻ ác bị diệt mất trước mặt Đức Chúa Trời thể ấy.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.051Z",
      "updatedAt": "2024-09-07T14:09:34.051Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "c95772f2-d35d-46d3-862c-32360154b00a",
      "number": 3,
      "content": "Nguyện những người ngay lành hân hoan,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.054Z",
      "updatedAt": "2024-09-07T14:09:34.054Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "25418b1b-e451-49c9-afe7-b1cb4c953222",
      "number": 3,
      "content": "Nguyện họ vui vẻ trước mặt Đức Chúa Trời;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.054Z",
      "updatedAt": "2024-09-07T14:09:34.054Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "c2b003f1-f79f-4529-b8ef-b46802b4d253",
      "number": 3,
      "content": "Thật vậy nguyện họ hớn hở và vui mừng.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.054Z",
      "updatedAt": "2024-09-07T14:09:34.054Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "348a22a5-eef6-449e-8856-0d18d40601b1",
      "number": 4,
      "content": "Hãy ca ngợi Đức Chúa Trời; hãy hát ca chúc tụng danh Ngài;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.056Z",
      "updatedAt": "2024-09-07T14:09:34.056Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "93f5b93e-e4e6-4443-9ed9-90bae8f4edce",
      "number": 4,
      "content": "Khá tôn ngợi Đấng ngự trên các tầng mây;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.056Z",
      "updatedAt": "2024-09-07T14:09:34.056Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "fd9b1abe-84e2-4202-ad6e-22f2c10ccf95",
      "number": 4,
      "content": "Danh Ngài là CHÚA;",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.056Z",
      "updatedAt": "2024-09-07T14:09:34.056Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "291b6d63-c83c-4220-b7a7-30d111b23ca1",
      "number": 4,
      "content": "Khá reo vui trước mặt Ngài.",
      "order": 3,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.056Z",
      "updatedAt": "2024-09-07T14:09:34.056Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "7fe9d3f6-c7e6-4749-83b3-31bc17f10429",
      "number": 5,
      "content": "Cha kẻ mồ côi, Đấng binh vực người góa bụa,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.058Z",
      "updatedAt": "2024-09-07T14:09:34.058Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "237f72da-1dea-4b67-a9b8-a61d8bd1562a",
      "number": 5,
      "content": "Chính là Đức Chúa Trời, Đấng ngự trong nơi thánh của Ngài.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.058Z",
      "updatedAt": "2024-09-07T14:09:34.058Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "29f961c6-84ba-4a58-abb1-95d1e919fef5",
      "number": 6,
      "content": "Đức Chúa Trời là Đấng làm cho kẻ cô đơn có mái ấm gia đình;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.061Z",
      "updatedAt": "2024-09-07T14:09:34.061Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "551ffab7-0f50-4392-bffc-60b84213e8d9",
      "number": 6,
      "content": "Ngài dẫn đưa những kẻ bị tù đày đến nơi sung mãn;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.061Z",
      "updatedAt": "2024-09-07T14:09:34.061Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "61cfe13c-3d3e-4c08-bd11-0a0b6d47c00a",
      "number": 6,
      "content": "Còn những kẻ bội nghịch phải ở trong xứ khô khan.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.061Z",
      "updatedAt": "2024-09-07T14:09:34.061Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "ea38eb63-a6f2-4d70-babb-f3c4b6dfdca3",
      "number": 7,
      "content": "Đức Chúa Trời ôi, khi Ngài đi ra trước mặt con dân Ngài,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.063Z",
      "updatedAt": "2024-09-07T14:09:34.063Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "46832457-3e86-419e-ad84-71b73243eb20",
      "number": 7,
      "content": "Khi Ngài đi qua sa mạc, (Sê-la)",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.063Z",
      "updatedAt": "2024-09-07T14:09:34.063Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "b8531236-d4e0-4392-aa95-0deb8a827c11",
      "number": 8,
      "content": "Đất rúng động, các tầng trời ào ạt đổ cơn mưa,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.065Z",
      "updatedAt": "2024-09-07T14:09:34.065Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "a31d01a2-4803-4d13-8b89-681daa12de4b",
      "number": 8,
      "content": "Trước mặt Đức Chúa Trời, Thần của Núi Si-nai,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.065Z",
      "updatedAt": "2024-09-07T14:09:34.065Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "f91a05c8-6dcd-41ed-b8f1-6c7f05f65d0f",
      "number": 8,
      "content": "Trước mặt Đức Chúa Trời, Thần của I-sơ-ra-ên.",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.065Z",
      "updatedAt": "2024-09-07T14:09:34.065Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "fab8ba00-91ae-45a6-a7ff-51c0f587b376",
      "number": 9,
      "content": "Đức Chúa Trời ôi, Ngài đã ban mưa xuống cách dồi dào,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.068Z",
      "updatedAt": "2024-09-07T14:09:34.068Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "a16c0ce8-798f-4b3c-8522-9f8893173a29",
      "number": 9,
      "content": "Khi sản nghiệp của Ngài bị khô khan, Ngài đã bồi bổ nó lại.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.068Z",
      "updatedAt": "2024-09-07T14:09:34.068Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "9db65201-ed55-4d59-8b9e-94b4292639ad",
      "number": 10,
      "content": "Con dân Ngài đã định cư trong sản nghiệp đó;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.070Z",
      "updatedAt": "2024-09-07T14:09:34.070Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "67fdc2b2-93cf-4ef7-8b5b-e71a57478edb",
      "number": 10,
      "content": "Đức Chúa Trời ôi, do lòng tốt của Ngài, Ngài đã chu cấp cho những người nghèo khó.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.070Z",
      "updatedAt": "2024-09-07T14:09:34.070Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "0890add1-1f73-490b-ae06-17f7ed0a4b02",
      "number": 11,
      "content": "Khi Chúa ban lệnh,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.072Z",
      "updatedAt": "2024-09-07T14:09:34.072Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "3d97a7b0-735a-48c6-a0e1-576f22abb358",
      "number": 11,
      "content": "Những phụ nữ ra đi báo tin mừng đông vô số:",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.072Z",
      "updatedAt": "2024-09-07T14:09:34.072Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "3bade4b0-3690-4f18-94bc-c09c40ba2093",
      "number": 12,
      "content": "“Các vua của các đạo quân bỏ chạy; họ chạy trốn.”",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.074Z",
      "updatedAt": "2024-09-07T14:09:34.074Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "914ccaaf-0da3-40d6-a9b2-d24611f54e9b",
      "number": 12,
      "content": "Những phụ nữ còn ở nhà chia chiến lợi phẩm–",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.074Z",
      "updatedAt": "2024-09-07T14:09:34.074Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "b912ad6d-e42b-409f-abe4-270c55493b92",
      "number": 13,
      "content": "Dù các ông nằm nghỉ giữa các ràn chiên–",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.076Z",
      "updatedAt": "2024-09-07T14:09:34.076Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "7985ca59-906f-4176-8979-49a9279369ed",
      "number": 13,
      "content": "Các chị em như những cánh bồ câu long lanh dát bạc,",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.076Z",
      "updatedAt": "2024-09-07T14:09:34.076Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "1494599b-af16-4fbc-8bd9-ab1bf896b410",
      "number": 13,
      "content": "Với bộ lông lóng lánh ánh vàng.",
      "order": 2,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.076Z",
      "updatedAt": "2024-09-07T14:09:34.076Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "3b7ba88a-f620-4d4a-93d5-9286d466d3ad",
      "number": 14,
      "content": "Khi Đấng Toàn Năng đánh văng các vua trong xứ,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.079Z",
      "updatedAt": "2024-09-07T14:09:34.079Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "952c1e48-c05c-4e2e-8491-a7ee30358d1b",
      "number": 14,
      "content": "Khiến họ như tuyết rơi tơi tả ở Xanh-môn.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.079Z",
      "updatedAt": "2024-09-07T14:09:34.079Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "587138ce-13c3-483f-a2a6-4b729d1bc480",
      "number": 15,
      "content": "Núi Ba-san là núi của Đức Chúa Trời;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.081Z",
      "updatedAt": "2024-09-07T14:09:34.081Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "819a639e-6351-46d9-b24a-bbfa25298f2b",
      "number": 15,
      "content": "Rặng Núi Ba-san quả có nhiều đỉnh cao chót vót.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.081Z",
      "updatedAt": "2024-09-07T14:09:34.081Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "97e7722c-786f-4cdd-8e6c-4c30a30e22b1",
      "number": 16,
      "content": "Hỡi những rặng núi cao có nhiều đỉnh,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.086Z",
      "updatedAt": "2024-09-07T14:09:34.086Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "ad3f7fe8-d785-4a69-a58b-e517a6ac9410",
      "number": 16,
      "content": "Tại sao các ngươi ganh tị với ngọn núi Đức Chúa Trời đã chọn để làm nơi Ngài ngự?",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.086Z",
      "updatedAt": "2024-09-07T14:09:34.086Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "9cce0391-0cae-4589-86e0-bf1549b6b360",
      "number": 16,
      "content": "Thật vậy CHÚA sẽ ngự tại đó đời đời.",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.086Z",
      "updatedAt": "2024-09-07T14:09:34.086Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "6388e781-5524-479a-8ad4-849de94425f5",
      "number": 17,
      "content": "Các xe chiến mã của Đức Chúa Trời nhiều đến hằng vạn,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.092Z",
      "updatedAt": "2024-09-07T14:09:34.092Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "739c0960-143c-402a-a546-39126dded000",
      "number": 17,
      "content": "Thật là hằng ngàn và hằng vạn;",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.092Z",
      "updatedAt": "2024-09-07T14:09:34.092Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "9f586868-b268-40d1-8376-96cfc59729f7",
      "number": 17,
      "content": "Chúa ngự giữa chúng như Ngài ngự trong nơi thánh của Ngài ở Si-nai.",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.092Z",
      "updatedAt": "2024-09-07T14:09:34.092Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "b8d1a12b-50bc-4248-8122-ace9b5c22fe0",
      "number": 18,
      "content": "Ngài đã lên nơi cao,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.098Z",
      "updatedAt": "2024-09-07T14:09:34.098Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "a2c541b1-8380-469d-890e-e6300041c1b3",
      "number": 18,
      "content": "Ngài dẫn theo đoàn tù binh bị bắt đem đi lưu đày;",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.098Z",
      "updatedAt": "2024-09-07T14:09:34.098Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "86f8558f-39a5-498b-8a1c-bfd40fc2dbd9",
      "number": 18,
      "content": "Ngài đã nhận những lễ vật của loài người,",
      "order": 2,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.098Z",
      "updatedAt": "2024-09-07T14:09:34.098Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "2ee2de99-29be-48b9-92dd-244fe83c7a4e",
      "number": 18,
      "content": "Ngay cả những lễ vật của những kẻ bội nghịch, để chứng tỏ rằng CHÚA Đức Chúa Trời cũng ở tại đó.",
      "order": 3,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.098Z",
      "updatedAt": "2024-09-07T14:09:34.098Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "e58db945-3a3a-475a-af6f-cf8febc3836d",
      "number": 19,
      "content": "Tôn ngợi Chúa,",
      "order": 0,
      "parNumber": 49,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.103Z",
      "updatedAt": "2024-09-07T14:09:34.103Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "9b2128e6-8b40-42db-aa4a-8c14da7b0906",
      "number": 19,
      "content": "Đấng hằng ngày cưu mang chúng ta;",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.103Z",
      "updatedAt": "2024-09-07T14:09:34.103Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "1f1be413-1bac-43e7-852d-6a8b03d52b9f",
      "number": 19,
      "content": "Đức Chúa Trời là Đấng Giải Cứu của chúng ta. (Sê-la)",
      "order": 2,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.103Z",
      "updatedAt": "2024-09-07T14:09:34.103Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "ad7671fa-01e9-47f6-9824-38b74da5f260",
      "number": 20,
      "content": "Đức Chúa Trời chúng ta là Đức Chúa Trời giải cứu;",
      "order": 0,
      "parNumber": 53,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.109Z",
      "updatedAt": "2024-09-07T14:09:34.109Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "6dff1b3a-1105-4e6e-8f0e-f903c7415466",
      "number": 20,
      "content": "Nhờ CHÚA Hằng Hữu, chúng ta được thoát chết.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.109Z",
      "updatedAt": "2024-09-07T14:09:34.109Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "649454bb-d10c-4691-928b-7a58cf4e77cb",
      "number": 21,
      "content": "Chắc chắn Đức Chúa Trời sẽ đạp nát đầu kẻ thù của Ngài,",
      "order": 0,
      "parNumber": 55,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.114Z",
      "updatedAt": "2024-09-07T14:09:34.114Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "0078badb-b9ab-472b-805a-b4e2228401be",
      "number": 21,
      "content": "Tức đầu tóc của những kẻ cứ miệt mài trong tội ác.",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.114Z",
      "updatedAt": "2024-09-07T14:09:34.114Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "1bc4b792-ec2e-41e0-9b73-32d45424007a",
      "number": 22,
      "content": "Chúa phán,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.119Z",
      "updatedAt": "2024-09-07T14:09:34.119Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "06f76e33-fa5f-4d5a-8ff2-37ffebaf1d76",
      "number": 22,
      "content": "“Từ Ba-san, Ta sẽ đem chúng về;",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.119Z",
      "updatedAt": "2024-09-07T14:09:34.119Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "10961fc1-cf04-4e87-ab45-73837a24a4e3",
      "number": 22,
      "content": "Từ các đại dương sâu thẳm, Ta sẽ mang chúng về,",
      "order": 2,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.119Z",
      "updatedAt": "2024-09-07T14:09:34.119Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "5a185b2b-76fe-4015-99db-f322b86b9cee",
      "number": 23,
      "content": "Để các ngươi có thể giẫm chân trên máu kẻ thù của mình,",
      "order": 0,
      "parNumber": 60,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.125Z",
      "updatedAt": "2024-09-07T14:09:34.125Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "4d971ce1-2a3c-46d7-8adb-107a78fef2ac",
      "number": 23,
      "content": "Ngay cả lưỡi của chó các ngươi cũng sẽ được phần trên quân thù của các ngươi.”",
      "order": 1,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.125Z",
      "updatedAt": "2024-09-07T14:09:34.125Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "3af82148-24d8-46e5-bd3f-782ef20029d5",
      "number": 24,
      "content": "Đức Chúa Trời ôi, người ta đã chứng kiến lễ nghinh rước Ngài,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.131Z",
      "updatedAt": "2024-09-07T14:09:34.131Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "36ed7c03-d03e-45c1-a260-212d12b3764c",
      "number": 24,
      "content": "Tức lễ nghinh rước Đức Chúa Trời của con, Vua của con, vào đền thánh.",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.131Z",
      "updatedAt": "2024-09-07T14:09:34.131Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "37fdce92-e5ae-4071-bc00-9893017bae90",
      "number": 25,
      "content": "Ca đoàn đi trước ngợi ca, ban nhạc theo sau trỗi nhạc,",
      "order": 0,
      "parNumber": 64,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.136Z",
      "updatedAt": "2024-09-07T14:09:34.136Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "615f64c3-bcaa-4eca-9a8b-f86bf73e1a0e",
      "number": 25,
      "content": "Ở giữa là đoàn thiếu nữ đánh trống lục lạc nhịp nhàng.",
      "order": 1,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.136Z",
      "updatedAt": "2024-09-07T14:09:34.136Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "6d287c60-09a5-4927-80a4-2e89d857ab89",
      "number": 26,
      "content": "Hãy chúc tụng Đức Chúa Trời giữa những đoàn người đông đúc dự lễ,",
      "order": 0,
      "parNumber": 66,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.141Z",
      "updatedAt": "2024-09-07T14:09:34.141Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "9c6d4379-c99c-478e-b592-90d36714759e",
      "number": 26,
      "content": "Hãy tôn ngợi CHÚA, hỡi dòng dõi của I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.141Z",
      "updatedAt": "2024-09-07T14:09:34.141Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "b19594f2-ceab-4194-bd50-276b966519ac",
      "number": 27,
      "content": "Kìa là Bên-gia-min, chi tộc trẻ nhất, đang dẫn đầu;",
      "order": 0,
      "parNumber": 68,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.152Z",
      "updatedAt": "2024-09-07T14:09:34.152Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "7b5aa738-8c65-48bc-b49f-54d6bb499d2e",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Giu-đa và phái đoàn của họ;",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.152Z",
      "updatedAt": "2024-09-07T14:09:34.152Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "2ff960cd-9886-42ed-82b7-d8be1d9625c0",
      "number": 27,
      "content": "Kìa là các nhà quyền quý của Xê-bu-lun và các nhà quyền quý của Náp-ta-li.",
      "order": 2,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.152Z",
      "updatedAt": "2024-09-07T14:09:34.152Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "c6a169f7-c2e0-4896-97cb-5d2a0a82d5af",
      "number": 28,
      "content": "Đức Chúa Trời ôi, xin thể hiện quyền năng Ngài;",
      "order": 0,
      "parNumber": 71,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.158Z",
      "updatedAt": "2024-09-07T14:09:34.158Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "b4df1de6-0454-4a40-b6d6-96e123002f13",
      "number": 28,
      "content": "Đức Chúa Trời ôi, xin bày tỏ sức mạnh Ngài, như Ngài đã từng làm cho chúng con khi trước.",
      "order": 1,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.158Z",
      "updatedAt": "2024-09-07T14:09:34.158Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "2b5ea94e-9dc5-40a6-a70b-c2fd315deaa6",
      "number": 29,
      "content": "Vì đền thờ Ngài tọa lạc tại Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 73,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.162Z",
      "updatedAt": "2024-09-07T14:09:34.162Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "dcc1f3cd-42a4-43ee-9b10-ce9555e88090",
      "number": 29,
      "content": "Nên các vua sẽ đem các lễ vật đến dâng lên Ngài tại đó.",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.162Z",
      "updatedAt": "2024-09-07T14:09:34.162Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "1edf5105-70ad-4a45-a655-ab53dbe198d4",
      "number": 30,
      "content": "Cầu xin Ngài trách phạt bầy thú hoang trong đám lau sậy,",
      "order": 0,
      "parNumber": 75,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.167Z",
      "updatedAt": "2024-09-07T14:09:34.167Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "53893833-bf10-456a-b5fc-d872eeab903b",
      "number": 30,
      "content": "Cùng đám bò đực ở giữa đàn bò con của các dân.",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.167Z",
      "updatedAt": "2024-09-07T14:09:34.167Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "f67e3884-18aa-4962-b590-071d60c26368",
      "number": 30,
      "content": "Xin Ngài bắt chúng phải đem bạc nén đến quỳ dâng;",
      "order": 2,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.167Z",
      "updatedAt": "2024-09-07T14:09:34.167Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "95ae859e-ff77-403a-9d27-fae9edca3ec6",
      "number": 30,
      "content": "Còn những dân hiếu chiến, xin Ngài làm cho chúng tản lạc khắp nơi.",
      "order": 3,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.167Z",
      "updatedAt": "2024-09-07T14:09:34.167Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "d7723ed1-debb-4066-916f-11295a33bd3f",
      "number": 31,
      "content": "Những đoàn người quyền thế sẽ từ Ai-cập đến thờ phượng;",
      "order": 0,
      "parNumber": 79,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.170Z",
      "updatedAt": "2024-09-07T14:09:34.170Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "4da78f61-4c08-47df-81ee-64a04b0d7c03",
      "number": 31,
      "content": "Người Ê-thi-ô-pi sẽ sớm đưa tay lên hướng về Đức Chúa Trời.",
      "order": 1,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.170Z",
      "updatedAt": "2024-09-07T14:09:34.170Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "558b71ae-2a5d-424e-a82f-738fd04df542",
      "number": 32,
      "content": "Hỡi tất cả quốc gia trên thế giới, hãy ca tụng Đức Chúa Trời;",
      "order": 0,
      "parNumber": 81,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.174Z",
      "updatedAt": "2024-09-07T14:09:34.174Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "92a742d6-b89f-4d20-b221-9d22d5a66a17",
      "number": 32,
      "content": "Hãy ca ngợi CHÚA, (Sê-la)",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.174Z",
      "updatedAt": "2024-09-07T14:09:34.174Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "dcf10088-7f03-448e-bcf7-40333939f978",
      "number": 33,
      "content": "Đấng ngự trên nơi chí cao của các tầng trời, tức các tầng trời đã được dựng nên từ thời thái cổ;",
      "order": 0,
      "parNumber": 84,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.178Z",
      "updatedAt": "2024-09-07T14:09:34.178Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "d0a98e87-6ff5-4c85-bc7f-abc9d902da84",
      "number": 33,
      "content": "Kìa, Ngài cất tiếng nói, giọng Ngài thật oai hùng.",
      "order": 1,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.178Z",
      "updatedAt": "2024-09-07T14:09:34.178Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "4f4afdd7-64d0-4e0f-ab06-b8633b238aec",
      "number": 34,
      "content": "Khá biết rằng mọi quyền lực đều thuộc về Đức Chúa Trời;",
      "order": 0,
      "parNumber": 86,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.181Z",
      "updatedAt": "2024-09-07T14:09:34.181Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "90683935-adbb-473c-b0fb-9fb10bb2641b",
      "number": 34,
      "content": "Nguyện uy nghi Ngài tỏa rạng trên I-sơ-ra-ên,",
      "order": 1,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.181Z",
      "updatedAt": "2024-09-07T14:09:34.181Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "0d0826c8-8406-447c-be84-15927c7c9020",
      "number": 34,
      "content": "Và quyền năng Ngài đầy dẫy khắp không trung.",
      "order": 2,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.181Z",
      "updatedAt": "2024-09-07T14:09:34.181Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "27f50ed8-1f55-4e39-9229-721217ee1f6c",
      "number": 35,
      "content": "Đức Chúa Trời ôi, trong đền thánh Ngài, Ngài đáng kinh hãi biết bao!",
      "order": 0,
      "parNumber": 89,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.185Z",
      "updatedAt": "2024-09-07T14:09:34.185Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "82454c2a-0bcb-47f3-8b42-02c6c4486423",
      "number": 35,
      "content": "Chính Đức Chúa Trời của I-sơ-ra-ên đã ban sức mạnh và quyền lực cho con dân Ngài.",
      "order": 1,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.185Z",
      "updatedAt": "2024-09-07T14:09:34.185Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    },
    {
      "id": "295afc39-bad8-4b89-b6b2-8ba4de1d058a",
      "number": 35,
      "content": "Tôn ngợi Đức Chúa Trời!",
      "order": 2,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:34.185Z",
      "updatedAt": "2024-09-07T14:09:34.185Z",
      "chapterId": "67b16821-ee6f-4f14-befb-4e2f2af2b8ea"
    }
  ],
