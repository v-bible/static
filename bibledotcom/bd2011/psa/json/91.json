{
  "chapter": {
    "id": "7bd7ee80-9d48-4eed-9400-87c83847e2c8",
    "number": 91,
    "ref": "https://www.bible.com/bible/19/PSA.91.BD2011",
    "createdAt": "2024-09-07T11:51:11.337Z",
    "updatedAt": "2024-09-07T11:51:11.337Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "8318235b-4d9a-46d7-8da2-86a0e3f165de",
      "number": 1,
      "content": "Người nào ở nơi kín đáo của Đấng Tối Cao",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.475Z",
      "updatedAt": "2024-09-07T14:15:34.475Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "ee864f4f-3541-40c3-b82a-7a25c29d567c",
      "number": 1,
      "content": "Sẽ được hằng ở dưới bóng của Đấng Toàn Năng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.475Z",
      "updatedAt": "2024-09-07T14:15:34.475Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "704a152a-224e-4c6d-b7fd-34aa4efef314",
      "number": 2,
      "content": "Tôi thưa với CHÚA rằng, “Ngài là nơi ẩn náu của con và là thành trì của con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.485Z",
      "updatedAt": "2024-09-07T14:15:34.485Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "569697ae-9bf2-4306-94ba-8b78e0e22777",
      "number": 2,
      "content": "Ngài là Đức Chúa Trời của con; con tin cậy nơi Ngài.”",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.485Z",
      "updatedAt": "2024-09-07T14:15:34.485Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "afddfff1-c2ee-4b68-8482-17e50b19adc7",
      "number": 3,
      "content": "Thật vậy Ngài sẽ giải cứu bạn khỏi cạm bẫy nguy hiểm",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.489Z",
      "updatedAt": "2024-09-07T14:15:34.489Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "22b96430-5546-427e-9161-a83b653284a1",
      "number": 3,
      "content": "Và khỏi ôn dịch chết người.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.489Z",
      "updatedAt": "2024-09-07T14:15:34.489Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "d6abbaf0-1ef2-4d5c-ab0c-4ef6dd6d45d7",
      "number": 4,
      "content": "Ngài sẽ lấy lông Ngài bao phủ bạn;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.493Z",
      "updatedAt": "2024-09-07T14:15:34.493Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "e8d36c84-7847-43c0-aaec-79a654cb3a7f",
      "number": 4,
      "content": "Dưới cánh Ngài bạn sẽ tìm được nơi nương náu an toàn.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.493Z",
      "updatedAt": "2024-09-07T14:15:34.493Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "fe4bbede-e28f-4eaa-b687-b99d4c5e902c",
      "number": 4,
      "content": "Đức thành tín Ngài là thuẫn khiên và mộc che của bạn.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.493Z",
      "updatedAt": "2024-09-07T14:15:34.493Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "e1b95c3b-24dc-47e5-8760-1e9439c76287",
      "number": 5,
      "content": "Bạn sẽ chẳng sợ dù sự khủng bố xảy đến ban đêm,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.496Z",
      "updatedAt": "2024-09-07T14:15:34.496Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "7c6e793a-7039-4730-ae39-606c29c3e221",
      "number": 5,
      "content": "Hoặc tên bay ban ngày,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.496Z",
      "updatedAt": "2024-09-07T14:15:34.496Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "dd0c199c-00f3-4f54-8fec-aa77d23b1847",
      "number": 6,
      "content": "Hoặc ôn dịch lan truyền trong đêm khuya,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.499Z",
      "updatedAt": "2024-09-07T14:15:34.499Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "80e7ede6-8919-40bd-8fbc-2c215011038d",
      "number": 6,
      "content": "Hoặc sự hủy diệt tàn phá lúc giữa trưa.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.499Z",
      "updatedAt": "2024-09-07T14:15:34.499Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "fb5d5183-dd34-4b34-a523-8ffc497168d0",
      "number": 7,
      "content": "Hằng ngàn người có thể ngã xuống bên cạnh bạn,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.502Z",
      "updatedAt": "2024-09-07T14:15:34.502Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "32861f4d-bee4-4c35-9e05-ccd701bba527",
      "number": 7,
      "content": "Hằng vạn người gục ngã bên phải bạn,",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.502Z",
      "updatedAt": "2024-09-07T14:15:34.502Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "329568e4-7af8-44a2-9e62-6c37b8351422",
      "number": 7,
      "content": "Nhưng nguy hiểm sẽ chẳng đến gần bạn.",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.502Z",
      "updatedAt": "2024-09-07T14:15:34.502Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "6e585e6e-04cd-4678-a0a8-8e4dd43b245c",
      "number": 8,
      "content": "Bạn sẽ nhìn thấy tận mắt mình;",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.507Z",
      "updatedAt": "2024-09-07T14:15:34.507Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "86c592b7-b5e6-4a61-967f-2b5069f886bb",
      "number": 8,
      "content": "Bạn sẽ thấy kẻ ác bị báo trả.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.507Z",
      "updatedAt": "2024-09-07T14:15:34.507Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "3a5e1398-455e-4786-a941-f50f6fd320b8",
      "number": 9,
      "content": "Vì bạn đã lấy CHÚA làm nơi nương náu mình,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.510Z",
      "updatedAt": "2024-09-07T14:15:34.510Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "de8bb1af-d710-44cf-b695-bbec9d98fef4",
      "number": 9,
      "content": "Lấy Đấng Tối Cao làm nơi trú ngụ mình,",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.510Z",
      "updatedAt": "2024-09-07T14:15:34.510Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "239812f4-42bc-46ed-8596-25b71fb03897",
      "number": 10,
      "content": "Nên sẽ chẳng có tai họa gì xảy đến với bạn,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.512Z",
      "updatedAt": "2024-09-07T14:15:34.512Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "c1b21c45-1155-4a79-9fe5-fda6413b22ea",
      "number": 10,
      "content": "Cũng chẳng có ôn dịch nào tới gần nhà bạn;",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.512Z",
      "updatedAt": "2024-09-07T14:15:34.512Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "8f58f5b5-1adc-4c5d-8e13-8419b19ed2f4",
      "number": 11,
      "content": "Vì Ngài sẽ ra lịnh cho các thiên sứ Ngài gìn giữ bạn,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.515Z",
      "updatedAt": "2024-09-07T14:15:34.515Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "4f3dbcb4-0fe4-40c0-a668-3d62a10b4253",
      "number": 11,
      "content": "Để bảo vệ bạn trong mọi đường lối của bạn.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.515Z",
      "updatedAt": "2024-09-07T14:15:34.515Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "f61ddc33-07ba-4868-808a-e49d9dd705b8",
      "number": 12,
      "content": "Các thiên sứ sẽ nâng bạn trên đôi tay họ,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.519Z",
      "updatedAt": "2024-09-07T14:15:34.519Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "be753254-7672-47c3-9696-2cdc40baaecd",
      "number": 12,
      "content": "Kẻo chân bạn vấp phải đá chăng.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.519Z",
      "updatedAt": "2024-09-07T14:15:34.519Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "608495bf-c5fc-4e6f-ab68-a01d0104cf8e",
      "number": 13,
      "content": "Bạn sẽ giẫm lên sư tử và rắn hổ mang;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.525Z",
      "updatedAt": "2024-09-07T14:15:34.525Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "e464de9e-6f1c-4b07-a50a-2d3f5461f933",
      "number": 13,
      "content": "Bạn sẽ chà đạp dưới chân sư tử tơ và rắn độc.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.525Z",
      "updatedAt": "2024-09-07T14:15:34.525Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "eab327af-53cd-4c74-bbda-bca0eda6a10b",
      "number": 14,
      "content": "Vì người ấy yêu kính Ta, nên Ta sẽ giải cứu người ấy;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.533Z",
      "updatedAt": "2024-09-07T14:15:34.533Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "d442fe7f-ba64-4cd6-b8f9-201a9dc56aa6",
      "number": 14,
      "content": "Ta sẽ đặt người ấy lên nơi cao, vì người ấy biết danh Ta.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.533Z",
      "updatedAt": "2024-09-07T14:15:34.533Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "019f47b7-7d96-43a4-9b76-3ad3993ef9b8",
      "number": 15,
      "content": "Khi người ấy kêu cầu Ta, Ta sẽ đáp lời người ấy;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.539Z",
      "updatedAt": "2024-09-07T14:15:34.539Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "50c7d831-a05f-4f0e-9e43-994c281019d8",
      "number": 15,
      "content": "Trong lúc nguy nan, Ta sẽ ở cùng người ấy;",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.539Z",
      "updatedAt": "2024-09-07T14:15:34.539Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "c416b708-ead6-4f3d-98a6-90549543579c",
      "number": 15,
      "content": "Ta sẽ giải cứu người ấy và làm cho người ấy được tôn trọng.",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.539Z",
      "updatedAt": "2024-09-07T14:15:34.539Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "45dc564d-0d63-43e7-a3c8-91f850e7c408",
      "number": 16,
      "content": "Ta sẽ cho người ấy được thỏa lòng sống lâu,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.544Z",
      "updatedAt": "2024-09-07T14:15:34.544Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    },
    {
      "id": "3723e2d3-35ca-49b7-a51b-9de1c8949e1e",
      "number": 16,
      "content": "Và chỉ cho người ấy ơn cứu rỗi của Ta.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:34.544Z",
      "updatedAt": "2024-09-07T14:15:34.544Z",
      "chapterId": "7bd7ee80-9d48-4eed-9400-87c83847e2c8"
    }
  ],
