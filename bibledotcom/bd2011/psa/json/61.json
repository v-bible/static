{
  "chapter": {
    "id": "24129e4b-385e-499b-8348-5797b24b3886",
    "number": 61,
    "ref": "https://www.bible.com/bible/19/PSA.61.BD2011",
    "createdAt": "2024-09-07T11:51:11.091Z",
    "updatedAt": "2024-09-07T11:51:11.091Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "7374d5ee-ef4f-4ab5-9dcc-55a6f8e77bf5",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin nghe tiếng kêu cầu của con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.716Z",
      "updatedAt": "2024-09-07T14:06:21.716Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "0de6c848-ab72-4eb7-bff6-8dbda732d4ef",
      "number": 1,
      "content": "Xin lắng nghe lời cầu nguyện của con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.716Z",
      "updatedAt": "2024-09-07T14:06:21.716Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "66c8faa7-75b6-4a38-a317-72a00f5a8e4c",
      "number": 2,
      "content": "Từ nơi xa xôi ở tận cùng trái đất con kêu cầu Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.725Z",
      "updatedAt": "2024-09-07T14:06:21.725Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "40f16b51-28f3-4f69-9429-5ef157858ec0",
      "number": 2,
      "content": "Lòng con bị choáng ngợp vì biết bao nỗi lo sợ,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.725Z",
      "updatedAt": "2024-09-07T14:06:21.725Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "3c868076-1961-4090-8642-7a0d8bc2f463",
      "number": 2,
      "content": "Xin dẫn đưa con đến một vầng đá",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.725Z",
      "updatedAt": "2024-09-07T14:06:21.725Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "5f68565d-4a25-4833-9ee2-9c8597c012d7",
      "number": 2,
      "content": "Cao lớn hơn con;",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.725Z",
      "updatedAt": "2024-09-07T14:06:21.725Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "a8630e5e-a321-455e-9029-ed514aa35774",
      "number": 3,
      "content": "Vì Ngài là nơi ẩn náu của con,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.728Z",
      "updatedAt": "2024-09-07T14:06:21.728Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "faa81e7a-6b44-415c-a9f3-a023a44dfd16",
      "number": 3,
      "content": "Một pháo tháp vững chắc để con đối phó với quân thù.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.728Z",
      "updatedAt": "2024-09-07T14:06:21.728Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "e15f1fc2-7fc1-4063-a1e3-91455bc13618",
      "number": 4,
      "content": "Xin cho con ở trong nhà Ngài mãi mãi;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.730Z",
      "updatedAt": "2024-09-07T14:06:21.730Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "d22abc34-4ea9-469e-8c35-697a9aa8ac9a",
      "number": 4,
      "content": "Xin cho con nương náu dưới sự che chở của bóng cánh Ngài. (Sê-la)",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.730Z",
      "updatedAt": "2024-09-07T14:06:21.730Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "7db73f1a-b04f-48d1-afc8-be09ad8c79da",
      "number": 5,
      "content": "Đức Chúa Trời ôi, vì Ngài đã nghe lời hứa nguyện của con;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.732Z",
      "updatedAt": "2024-09-07T14:06:21.732Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "bab11d08-5474-4b0a-86fe-42071c6a6d62",
      "number": 5,
      "content": "Ngài ban cho con cơ nghiệp dành cho những người kính sợ danh Ngài.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.732Z",
      "updatedAt": "2024-09-07T14:06:21.732Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "c972801e-e3ac-47b2-b9fa-8c1e83921bfa",
      "number": 6,
      "content": "Cầu xin Ngài gia tăng tuổi thọ cho vua;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.735Z",
      "updatedAt": "2024-09-07T14:06:21.735Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "72f04c34-f75a-4c00-bb81-26579bcf47c0",
      "number": 6,
      "content": "Xin cho những năm của đời người kéo dài qua nhiều thế hệ.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.735Z",
      "updatedAt": "2024-09-07T14:06:21.735Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "3f9e4c20-a521-4716-813e-56879aefef4e",
      "number": 7,
      "content": "Nguyện người trị vì trước mặt Đức Chúa Trời mãi mãi;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.738Z",
      "updatedAt": "2024-09-07T14:06:21.738Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "e54e0f31-deae-48e8-81f7-8c9d64f8fd1f",
      "number": 7,
      "content": "Cầu xin Ngài ban tình thương và đức thành tín bảo vệ người.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.738Z",
      "updatedAt": "2024-09-07T14:06:21.738Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "35d95f05-aac4-44b5-9bb0-bae4f4dfc3fb",
      "number": 8,
      "content": "Như vậy con sẽ ca ngợi danh Ngài mãi mãi;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.741Z",
      "updatedAt": "2024-09-07T14:06:21.741Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    },
    {
      "id": "fc86a3a9-6d89-453a-8955-6385aa4d0292",
      "number": 8,
      "content": "Con sẽ giữ trọn những gì con đã hứa nguyện mỗi ngày.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:06:21.741Z",
      "updatedAt": "2024-09-07T14:06:21.741Z",
      "chapterId": "24129e4b-385e-499b-8348-5797b24b3886"
    }
  ],
