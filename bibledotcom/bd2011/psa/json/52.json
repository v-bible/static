{
  "chapter": {
    "id": "d444af54-b396-4a24-bf6f-9ae1523f053b",
    "number": 52,
    "ref": "https://www.bible.com/bible/19/PSA.52.BD2011",
    "createdAt": "2024-09-07T11:51:11.039Z",
    "updatedAt": "2024-09-07T11:51:11.039Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "ed833c9c-1817-4405-b7e5-23a3fa1088bf",
      "number": 1,
      "content": "Hỡi kẻ có quyền lực, sao ngươi lại phô trương tính độc ác?",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.210Z",
      "updatedAt": "2024-09-07T14:05:05.210Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "42d341b8-b1c8-4d5f-b523-fcae71f17b03",
      "number": 1,
      "content": "Đức nhân từ của Đức Chúa Trời hằng còn mãi mãi.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.210Z",
      "updatedAt": "2024-09-07T14:05:05.210Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "01a6f0dd-638d-41da-8a4c-ee7001a3ea20",
      "number": 2,
      "content": "Hỡi kẻ gian dối, lưỡi ngươi bàn tính việc hại người;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.221Z",
      "updatedAt": "2024-09-07T14:05:05.221Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "dbdaf312-ac8f-4435-a749-e7d19903c7a9",
      "number": 2,
      "content": "Lưỡi ngươi quả là một lưỡi dao cạo sắc bén.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.221Z",
      "updatedAt": "2024-09-07T14:05:05.221Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "b3dfe878-b7ed-4fa6-9fee-e2b6e2b1e974",
      "number": 3,
      "content": "Ngươi yêu mến gian ác hơn thánh thiện,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.224Z",
      "updatedAt": "2024-09-07T14:05:05.224Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "f28c4a73-c4be-4143-8bbe-951ebe9e622c",
      "number": 3,
      "content": "Thích nói dối hơn nói thật. (Sê-la)",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.224Z",
      "updatedAt": "2024-09-07T14:05:05.224Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "21ebf048-fdda-4967-8e39-967ca0cfbea3",
      "number": 4,
      "content": "Ngươi quả yêu mến mọi lời gây nên hủy diệt,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.228Z",
      "updatedAt": "2024-09-07T14:05:05.228Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "09668c12-4c6f-43ce-9f23-c7fd22c24975",
      "number": 4,
      "content": "Hỡi lưỡi dối trá!",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.228Z",
      "updatedAt": "2024-09-07T14:05:05.228Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "3f44cad6-f955-448b-9ff7-8a05a09d9787",
      "number": 5,
      "content": "Chắc chắn Đức Chúa Trời sẽ tiêu diệt ngươi vĩnh viễn;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.231Z",
      "updatedAt": "2024-09-07T14:05:05.231Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "5d27dd81-8565-4f0c-885e-4b0f10ad6442",
      "number": 5,
      "content": "Ngài sẽ dứt bỏ ngươi; Ngài sẽ nhổ ngươi ra khỏi nhà ngươi;",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.231Z",
      "updatedAt": "2024-09-07T14:05:05.231Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "837164d3-1a8a-4b4f-9dee-3453df5ce044",
      "number": 5,
      "content": "Ngài sẽ bứng ngươi ra khỏi đất của người sống. (Sê-la)",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.231Z",
      "updatedAt": "2024-09-07T14:05:05.231Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "f3c68e59-07bb-4a7d-9441-33cdb4b45700",
      "number": 6,
      "content": "Bấy giờ người ngay lành sẽ thấy và phát sợ;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.234Z",
      "updatedAt": "2024-09-07T14:05:05.234Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "3a522535-fa8b-4084-8c84-78a330056a87",
      "number": 6,
      "content": "Người ấy sẽ cười chê nó mà rằng,",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.234Z",
      "updatedAt": "2024-09-07T14:05:05.234Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "b72c0340-6adc-4860-b870-e082a651bade",
      "number": 7,
      "content": "“Kìa là kẻ không nhờ cậy Đức Chúa Trời",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.239Z",
      "updatedAt": "2024-09-07T14:05:05.239Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "b79ffc67-d450-433f-942d-26e0ef8135cf",
      "number": 7,
      "content": "Làm nơi nương náu mình,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.239Z",
      "updatedAt": "2024-09-07T14:05:05.239Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "af2ad1c7-b827-47c2-b718-a436f8ecf61b",
      "number": 7,
      "content": "Nhưng cậy vào tiền tài của cải mình,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.239Z",
      "updatedAt": "2024-09-07T14:05:05.239Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "1c4d8bae-b9df-4f05-9c66-5c8b30e30c20",
      "number": 7,
      "content": "Và ỷ vào khả năng tiêu diệt đối thủ của mình.”",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.239Z",
      "updatedAt": "2024-09-07T14:05:05.239Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "9d366d18-8358-4ff6-a956-8113f8a71675",
      "number": 8,
      "content": "Còn tôi, tôi như cây ô-liu xanh tươi",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.241Z",
      "updatedAt": "2024-09-07T14:05:05.241Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "6d19f9f2-1344-4666-8d29-297cb8933657",
      "number": 8,
      "content": "Trong nhà Đức Chúa Trời;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.241Z",
      "updatedAt": "2024-09-07T14:05:05.241Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "dc286265-02e0-4960-851f-816b20c5b24e",
      "number": 8,
      "content": "Tôi tin cậy lòng thương xót của Đức Chúa Trời",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.241Z",
      "updatedAt": "2024-09-07T14:05:05.241Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "564f61f8-fcee-4799-a35f-6be3b6e137d4",
      "number": 8,
      "content": "Đời đời mãi mãi.",
      "order": 3,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.241Z",
      "updatedAt": "2024-09-07T14:05:05.241Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "63aa328d-1749-42fe-b6da-aa8619b6c370",
      "number": 9,
      "content": "Con sẽ cảm tạ Ngài cho đến đời đời",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.243Z",
      "updatedAt": "2024-09-07T14:05:05.243Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "c63519f8-f3eb-47fc-8128-5de152f1a8fb",
      "number": 9,
      "content": "Về những gì Ngài đã làm;",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.243Z",
      "updatedAt": "2024-09-07T14:05:05.243Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "3419a35b-9eab-4686-a221-dcd8a2812eaf",
      "number": 9,
      "content": "Con sẽ đặt hy vọng vào danh Ngài, vì đó là điều tốt đẹp",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.243Z",
      "updatedAt": "2024-09-07T14:05:05.243Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    },
    {
      "id": "b8b8bd2c-6522-42df-9d69-31662cb1f124",
      "number": 9,
      "content": "Trước mặt những người thánh của Ngài.",
      "order": 3,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:05.243Z",
      "updatedAt": "2024-09-07T14:05:05.243Z",
      "chapterId": "d444af54-b396-4a24-bf6f-9ae1523f053b"
    }
  ],
