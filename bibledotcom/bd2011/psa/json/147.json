{
  "chapter": {
    "id": "9a1c924f-8373-4648-a71b-f979faeaebe3",
    "number": 147,
    "ref": "https://www.bible.com/bible/19/PSA.147.BD2011",
    "createdAt": "2024-09-07T11:51:11.591Z",
    "updatedAt": "2024-09-07T11:51:11.591Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "3e11acc1-2f4e-4d9b-95b3-c81ef330b4d3",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.582Z",
      "updatedAt": "2024-09-07T14:29:26.582Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "b6236deb-516e-4f84-ae7c-d33a870a6a60",
      "number": 1,
      "content": "Ca hát chúc tụng Đức Chúa Trời chúng ta là điều tốt đẹp;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.582Z",
      "updatedAt": "2024-09-07T14:29:26.582Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "d6934ead-c24e-44a2-9388-373ddbf36061",
      "number": 1,
      "content": "Vì tôn ngợi Ngài là điều vui thỏa và hợp lý dường nào!",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.582Z",
      "updatedAt": "2024-09-07T14:29:26.582Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "a393ae43-7b16-4219-87bd-aa5ca50438c2",
      "number": 2,
      "content": "CHÚA xây dựng lại Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.592Z",
      "updatedAt": "2024-09-07T14:29:26.592Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "23da519b-152e-4e1b-b8b0-a3c220439cd9",
      "number": 2,
      "content": "Ngài gom nhóm những người bị tản lạc của I-sơ-ra-ên trở về.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.592Z",
      "updatedAt": "2024-09-07T14:29:26.592Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "d26ca9ea-e767-457c-ad5a-192c2367d3a9",
      "number": 3,
      "content": "Ngài rịt lành những tâm hồn tan vỡ;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.595Z",
      "updatedAt": "2024-09-07T14:29:26.595Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "8d54f940-8069-473a-8f86-b004cddd3b66",
      "number": 3,
      "content": "Và băng bó các vết thương của họ.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.595Z",
      "updatedAt": "2024-09-07T14:29:26.595Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "b51035cb-56a2-4cf5-9956-e45c018f5aeb",
      "number": 4,
      "content": "Ngài định số lượng các vì sao;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.597Z",
      "updatedAt": "2024-09-07T14:29:26.597Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "3d9d3185-7ff8-426a-92df-b1f54d784067",
      "number": 4,
      "content": "Ngài gọi chúng theo tên riêng của từng vì sao một.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.597Z",
      "updatedAt": "2024-09-07T14:29:26.597Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "e5113a22-bbcc-4f81-bdec-3c9c0d2e3f20",
      "number": 5,
      "content": "CHÚA chúng ta vĩ đại biết bao!",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.600Z",
      "updatedAt": "2024-09-07T14:29:26.600Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "bdfccd78-a8dc-4293-9718-5ad89d7d0654",
      "number": 5,
      "content": "Quyền năng Ngài thật vô hạn. Kiến thức Ngài quả vô cùng.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.600Z",
      "updatedAt": "2024-09-07T14:29:26.600Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "e79a19b2-bab3-4f01-9b63-d349a6a1b1ab",
      "number": 6,
      "content": "CHÚA nhấc đưa những người nghèo khó lên cao,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.602Z",
      "updatedAt": "2024-09-07T14:29:26.602Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "fad9fe7b-331b-4741-b0bc-c4c260d623dc",
      "number": 6,
      "content": "Nhưng Ngài đánh đổ phường gian ác ngã nhào xuống đất.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.602Z",
      "updatedAt": "2024-09-07T14:29:26.602Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "99d1660f-926e-4ea8-bd56-f799e9ef09ef",
      "number": 7,
      "content": "Hãy dâng lên CHÚA những lời cảm tạ;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.605Z",
      "updatedAt": "2024-09-07T14:29:26.605Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "3da2ce3f-44fe-48b2-8c81-e9d037249fd4",
      "number": 7,
      "content": "Hãy dùng nhạc khí ca ngợi Đức Chúa Trời.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.605Z",
      "updatedAt": "2024-09-07T14:29:26.605Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "9f018e2b-545e-4574-9fc5-dabbe2aa1e06",
      "number": 8,
      "content": "Ngài bao phủ các tầng trời bằng mây;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.607Z",
      "updatedAt": "2024-09-07T14:29:26.607Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "51276e7c-1878-450a-a684-ab5692927806",
      "number": 8,
      "content": "Ngài chuẩn bị các trận mưa cho đất;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.607Z",
      "updatedAt": "2024-09-07T14:29:26.607Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "b122e9ff-0d83-464a-8935-035c35e44935",
      "number": 8,
      "content": "Ngài khiến cỏ mọc trên các núi đồi.",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.607Z",
      "updatedAt": "2024-09-07T14:29:26.607Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "f1ec521d-5c3d-4958-b167-dee5a8487852",
      "number": 9,
      "content": "Ngài ban thực phẩm cho muôn loài cầm thú;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.610Z",
      "updatedAt": "2024-09-07T14:29:26.610Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "dac8513e-9b86-4870-a3ad-742007fe419d",
      "number": 9,
      "content": "Ngài ban thức ăn cho các quạ con đói bụng kêu gào.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.610Z",
      "updatedAt": "2024-09-07T14:29:26.610Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "fe21215a-2faa-4bc1-ad65-cdaf3a81ab07",
      "number": 10,
      "content": "Ngài chẳng hài lòng về sức lực của ngựa chiến;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.612Z",
      "updatedAt": "2024-09-07T14:29:26.612Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "2a78640e-bfdc-4b17-9aef-78148a6a7420",
      "number": 10,
      "content": "Ngài không vui về đôi chân nhanh nhẹn của loài người;",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.612Z",
      "updatedAt": "2024-09-07T14:29:26.612Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "33a865a9-dcc4-4d38-a343-5932179cfdf6",
      "number": 11,
      "content": "Nhưng CHÚA rất vui lòng về những người kính sợ Ngài,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.614Z",
      "updatedAt": "2024-09-07T14:29:26.614Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "8bbab3a8-b216-4b71-82cf-e7659a70cbc8",
      "number": 11,
      "content": "Những người đặt hy vọng vào tình thương của Ngài.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.614Z",
      "updatedAt": "2024-09-07T14:29:26.614Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "0d51c3cd-2927-47db-afe4-32752a28ac65",
      "number": 12,
      "content": "Hỡi Giê-ru-sa-lem, hãy ca ngợi CHÚA!",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.616Z",
      "updatedAt": "2024-09-07T14:29:26.616Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "a65a68f9-0895-40f5-8a77-05d1bf1789ad",
      "number": 12,
      "content": "Hỡi Si-ôn, hãy tôn ngợi Đức Chúa Trời!",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.616Z",
      "updatedAt": "2024-09-07T14:29:26.616Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "0ff80389-b62c-4a42-a6b1-c5d048852e6b",
      "number": 13,
      "content": "Vì Ngài làm vững mạnh các song sắt ở các cổng thành của ngươi;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.619Z",
      "updatedAt": "2024-09-07T14:29:26.619Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "80724b07-ee5e-4012-9402-eecb08164213",
      "number": 13,
      "content": "Ngài ban phước cho con dân ngươi sống trong ngươi.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.619Z",
      "updatedAt": "2024-09-07T14:29:26.619Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "02d75d78-c656-4933-babb-1110c05578e7",
      "number": 14,
      "content": "Ngài ban hòa bình nơi các biên giới ngươi;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.621Z",
      "updatedAt": "2024-09-07T14:29:26.621Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "f8685e4b-84a5-4314-bd67-b9bba6d9dc12",
      "number": 14,
      "content": "Ngài cho ngươi no thỏa bằng gạo ngon hảo hạng.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.621Z",
      "updatedAt": "2024-09-07T14:29:26.621Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "6e8b62e6-e928-4415-a000-6f5250b206f1",
      "number": 15,
      "content": "Ngài truyền lịnh của Ngài cho trái đất;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.623Z",
      "updatedAt": "2024-09-07T14:29:26.623Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "4c432ebe-be92-4bfa-b6d3-8c03337e0df7",
      "number": 15,
      "content": "Lời Ngài phán ra được cấp tốc truyền đi.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.623Z",
      "updatedAt": "2024-09-07T14:29:26.623Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "69ef2b4b-475a-4444-863c-09fa593b13c6",
      "number": 16,
      "content": "Ngài ban tuyết xuống phủ trắng như lông chiên;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.626Z",
      "updatedAt": "2024-09-07T14:29:26.626Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "727ccb55-834c-44d2-9f4b-d25db3bf819d",
      "number": 16,
      "content": "Ngài rải sương giá ra như tro bụi.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.626Z",
      "updatedAt": "2024-09-07T14:29:26.626Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "c8a3adad-bcc6-4d8a-88b2-1801b94c3ce6",
      "number": 17,
      "content": "Ngài quăng băng giá ra từng mảng;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.628Z",
      "updatedAt": "2024-09-07T14:29:26.628Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "9bbb0aa7-5cc6-4da5-9e11-90acc5e99ead",
      "number": 17,
      "content": "Ai có thể chịu nổi cơn buốt giá của Ngài?",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.628Z",
      "updatedAt": "2024-09-07T14:29:26.628Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "c0af48e0-ce32-4430-a686-157a6b2aacfc",
      "number": 18,
      "content": "Ngài ban lịnh, chúng liền tan chảy;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.630Z",
      "updatedAt": "2024-09-07T14:29:26.630Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "6f8f6d25-caf2-43c0-9914-2f81f12446cc",
      "number": 18,
      "content": "Ngài khiến gió thổi qua, nước chảy xuống thành dòng.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.630Z",
      "updatedAt": "2024-09-07T14:29:26.630Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "a563c9e8-bce1-4bc6-af45-15b5dd87e834",
      "number": 19,
      "content": "Ngài tuyên bố lời Ngài cho Gia-cốp,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.632Z",
      "updatedAt": "2024-09-07T14:29:26.632Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "4bca75d1-0b17-44b6-aeef-67ad61f40475",
      "number": 19,
      "content": "Luật lệ Ngài và mạng lịnh Ngài cho I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.632Z",
      "updatedAt": "2024-09-07T14:29:26.632Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "5f376f2a-a9e9-48c8-9cf7-4166b0f7a6ef",
      "number": 20,
      "content": "Ngài chẳng làm như vậy cho một dân nào khác;",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.634Z",
      "updatedAt": "2024-09-07T14:29:26.634Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "a2473ef5-b756-4e7a-b314-7d927d2da50c",
      "number": 20,
      "content": "Họ chẳng biết gì về các mạng lịnh của Ngài.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.634Z",
      "updatedAt": "2024-09-07T14:29:26.634Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    },
    {
      "id": "ad74af02-4e56-4305-b39d-50e6d2cb54c1",
      "number": 20,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 2,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:29:26.634Z",
      "updatedAt": "2024-09-07T14:29:26.634Z",
      "chapterId": "9a1c924f-8373-4648-a71b-f979faeaebe3"
    }
  ],
