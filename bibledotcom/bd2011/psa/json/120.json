{
  "chapter": {
    "id": "82ce5645-7bc3-413b-a52d-ac21651971e9",
    "number": 120,
    "ref": "https://www.bible.com/bible/19/PSA.120.BD2011",
    "createdAt": "2024-09-07T11:51:11.518Z",
    "updatedAt": "2024-09-07T11:51:11.518Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "ba140c2d-334e-468c-ade1-1063c2713efb",
      "number": 1,
      "content": "Trong cơn hoạn nạn tôi kêu cầu cùng CHÚA,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.449Z",
      "updatedAt": "2024-09-07T14:21:18.449Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "01168891-678d-497e-8f40-15228236e761",
      "number": 1,
      "content": "Và Ngài đã đáp lời tôi.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.449Z",
      "updatedAt": "2024-09-07T14:21:18.449Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "ca1f3abd-5586-4176-9299-ea2c62b864bf",
      "number": 2,
      "content": "CHÚA ôi, xin giải cứu linh hồn con",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.464Z",
      "updatedAt": "2024-09-07T14:21:18.464Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "8940cf86-f3ca-4c1c-8bea-4f6554794d6b",
      "number": 2,
      "content": "Khỏi môi dối trá,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.464Z",
      "updatedAt": "2024-09-07T14:21:18.464Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "8ad6e672-fcd8-41ca-a08f-0be4331eab6c",
      "number": 2,
      "content": "Và khỏi lưỡi dối lừa.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.464Z",
      "updatedAt": "2024-09-07T14:21:18.464Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "bd27bb31-71ca-43bc-aa8c-226aa6171a72",
      "number": 3,
      "content": "Hỡi lưỡi dối trá,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.466Z",
      "updatedAt": "2024-09-07T14:21:18.466Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "c7ebd727-f775-4d2d-953b-1fec07385228",
      "number": 3,
      "content": "Ngài sẽ làm gì cho ngươi?",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.466Z",
      "updatedAt": "2024-09-07T14:21:18.466Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "e68053a2-7c79-4703-93d4-42ad748dac07",
      "number": 3,
      "content": "Ngài sẽ làm gì nữa cho ngươi?",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.466Z",
      "updatedAt": "2024-09-07T14:21:18.466Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "006df8f2-7b1b-4cdc-9d9e-a99e5dfd4d64",
      "number": 4,
      "content": "Ngài sẽ dùng những tên nhọn của người dũng sĩ,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.469Z",
      "updatedAt": "2024-09-07T14:21:18.469Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "9b15d6a5-01d2-43d1-842e-d88731a0f3a6",
      "number": 4,
      "content": "Vàn hững than lửa hừng của bụi cây chổi cho ngươi.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.469Z",
      "updatedAt": "2024-09-07T14:21:18.469Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "24968d7d-c157-46b8-8967-19051f222720",
      "number": 5,
      "content": "Khốn thay cho tôi vì tôi trú ngụ ở Mê-sếch,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.471Z",
      "updatedAt": "2024-09-07T14:21:18.471Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "2e803f24-7cd2-4341-b650-1bd60875b44e",
      "number": 5,
      "content": "Vì tôi cư ngụ giữa các trại của Kê-đa.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.471Z",
      "updatedAt": "2024-09-07T14:21:18.471Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "c91bba93-d1b4-4cc9-b52c-74e405c4cf92",
      "number": 6,
      "content": "Linh hồn tôi đã sống quá lâu",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.476Z",
      "updatedAt": "2024-09-07T14:21:18.476Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "65750232-e48f-4318-bb3e-fabcad00a718",
      "number": 6,
      "content": "Với những kẻ ghét hòa bình.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.476Z",
      "updatedAt": "2024-09-07T14:21:18.476Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "77164a1e-e0e6-4a0e-b8bd-52254f00b9aa",
      "number": 7,
      "content": "Tôi yêu chuộng hòa bình,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.478Z",
      "updatedAt": "2024-09-07T14:21:18.478Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    },
    {
      "id": "df85590d-055a-45bd-b126-2a3808782e90",
      "number": 7,
      "content": "Nhưng khi tôi nói đến hòa bình, chúng chỉ muốn chiến tranh.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:18.478Z",
      "updatedAt": "2024-09-07T14:21:18.478Z",
      "chapterId": "82ce5645-7bc3-413b-a52d-ac21651971e9"
    }
  ],
