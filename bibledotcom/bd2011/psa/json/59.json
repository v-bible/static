{
  "chapter": {
    "id": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254",
    "number": 59,
    "ref": "https://www.bible.com/bible/19/PSA.59.BD2011",
    "createdAt": "2024-09-07T11:51:11.072Z",
    "updatedAt": "2024-09-07T11:51:11.072Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "4c2afde4-e9ec-4b69-a597-4770919363bc",
      "number": 1,
      "content": "Lạy Đức Chúa Trời của con, xin giải cứu con khỏi những kẻ thù con;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.334Z",
      "updatedAt": "2024-09-07T14:05:57.334Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "b9515c50-0837-4ad7-88ed-46f70185b8c7",
      "number": 1,
      "content": "Xin bảo vệ con khỏi những kẻ nổi lên chống lại con;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.334Z",
      "updatedAt": "2024-09-07T14:05:57.334Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "effbb24d-0b93-4e2c-a3de-650a1f95999b",
      "number": 2,
      "content": "Xin giải cứu con khỏi những kẻ gây ra tội ác;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.344Z",
      "updatedAt": "2024-09-07T14:05:57.344Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "ca8035c3-96f3-41c8-b059-2f821218729a",
      "number": 2,
      "content": "Xin cứu thoát con khỏi tay kẻ khát máu giết người.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.344Z",
      "updatedAt": "2024-09-07T14:05:57.344Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "45a095a4-0f26-430d-99b0-0a5e54e2898c",
      "number": 3,
      "content": "Vì kìa, chúng đang rình rập để hại mạng sống con;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.346Z",
      "updatedAt": "2024-09-07T14:05:57.346Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "fabacc24-72b0-49d1-8ed8-323a55e115bf",
      "number": 3,
      "content": "Quân hung bạo đang chực tấn công con,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.346Z",
      "updatedAt": "2024-09-07T14:05:57.346Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "6eaa3a6e-0cb8-41f0-8b8f-11d996f5f885",
      "number": 3,
      "content": "Cho dù con chẳng vi phạm điều gì hoặc có lầm lỗi gì, CHÚA ôi.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.346Z",
      "updatedAt": "2024-09-07T14:05:57.346Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "ed8ab28a-a934-4c29-ab39-5957a1659472",
      "number": 4,
      "content": "Mặc dù con không có lỗi gì, chúng vẫn cứ xáp lại chực đánh con;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.349Z",
      "updatedAt": "2024-09-07T14:05:57.349Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "74fa34f1-12ca-4a5e-b8d4-dd7b43e64709",
      "number": 4,
      "content": "Xin trỗi dậy cứu giúp con và đoái xem tình cảnh của con.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.349Z",
      "updatedAt": "2024-09-07T14:05:57.349Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "187dd2fe-c3cf-4ecb-b702-01551df27d58",
      "number": 5,
      "content": "CHÚA ôi, xin Ngài, Đức Chúa Trời của các đạo quân và Đức Chúa Trời của I-sơ-ra-ên,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.352Z",
      "updatedAt": "2024-09-07T14:05:57.352Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "b86314f7-9c7a-42b0-b2a0-b984c195b898",
      "number": 5,
      "content": "Trỗi dậy để đoán phạt các dân;",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.352Z",
      "updatedAt": "2024-09-07T14:05:57.352Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "8e6bc927-13ee-4eed-b0f1-3e58f728a0b8",
      "number": 5,
      "content": "Xin Ngài đừng thương xót những kẻ phản bội gian ác. (Sê-la)",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.352Z",
      "updatedAt": "2024-09-07T14:05:57.352Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "2bc2e00f-d58a-4e3b-9e9b-cda922fc5581",
      "number": 6,
      "content": "Chiều tối chúng trở lại",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.355Z",
      "updatedAt": "2024-09-07T14:05:57.355Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "7a37e82f-9480-4908-aa20-fd0ad52fbd22",
      "number": 6,
      "content": "Và tru như chó,",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.355Z",
      "updatedAt": "2024-09-07T14:05:57.355Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "ee09e2a7-ea34-4e2a-8747-06cf85398452",
      "number": 6,
      "content": "Rồi đi lùng soát khắp thành.",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.355Z",
      "updatedAt": "2024-09-07T14:05:57.355Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "48109646-8698-4f9e-a135-cfbfbac1df5c",
      "number": 7,
      "content": "Kìa, miệng chúng buông ra những lời nguyền rủa;",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.357Z",
      "updatedAt": "2024-09-07T14:05:57.357Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "36f8dda5-a9d1-4c59-befa-d6fa109ae326",
      "number": 7,
      "content": "Môi chúng nguy hiểm tựa lưỡi gươm,",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.357Z",
      "updatedAt": "2024-09-07T14:05:57.357Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "e342ac95-5660-4906-a896-89dcb1f3c2a1",
      "number": 7,
      "content": "Vì chúng nói rằng, “Có ai nghe không?”",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.357Z",
      "updatedAt": "2024-09-07T14:05:57.357Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "c9bc04f4-828d-4ca7-bbe4-94323381357c",
      "number": 8,
      "content": "Nhưng CHÚA ôi, Ngài sẽ cười chúng;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.360Z",
      "updatedAt": "2024-09-07T14:05:57.360Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "3ba5e6c5-9956-4481-b564-4542c30698b2",
      "number": 8,
      "content": "Ngài sẽ nhạo cười tất cả các dân.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.360Z",
      "updatedAt": "2024-09-07T14:05:57.360Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "ef8392ab-43f8-4e69-9b7a-7beefa252430",
      "number": 9,
      "content": "Sức Mạnh của con ôi, con trông đợi Ngài.",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.362Z",
      "updatedAt": "2024-09-07T14:05:57.362Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "3954ccc3-530a-42f5-bffd-2f32424a1542",
      "number": 9,
      "content": "Vì Ngài, Đức Chúa Trời ôi, là thành trì kiên cố của con.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.362Z",
      "updatedAt": "2024-09-07T14:05:57.362Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "66ce6ea7-bde9-4a50-9143-cc68ef2cf474",
      "number": 10,
      "content": "Đức Chúa Trời giàu lòng thương xót của con sẽ đi trước con;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.364Z",
      "updatedAt": "2024-09-07T14:05:57.364Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "cc628e8a-b85d-4e52-8034-300cea7a0e8e",
      "number": 10,
      "content": "Đức Chúa Trời sẽ cho con hả hê nhìn những kẻ thù của con.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.364Z",
      "updatedAt": "2024-09-07T14:05:57.364Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "bdf91b8d-5df1-4efc-b74e-21e0f12200b3",
      "number": 11,
      "content": "Xin đừng giết chúng, kẻo dân tộc con sẽ quên đi chăng;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.366Z",
      "updatedAt": "2024-09-07T14:05:57.366Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "49ba57b6-b48c-4cec-8aca-57c42ee5d126",
      "number": 11,
      "content": "Nhưng xin dùng quyền năng Ngài làm cho chúng tan tác và sụp đổ,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.366Z",
      "updatedAt": "2024-09-07T14:05:57.366Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "b1764cf3-9f3f-4583-ade5-aa0cc5df6d17",
      "number": 11,
      "content": "Ôi lạy Chúa, thuẫn khiên của con.",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.366Z",
      "updatedAt": "2024-09-07T14:05:57.366Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "f00f7eb1-ed18-4efa-846e-563a4763addb",
      "number": 12,
      "content": "Do tội của miệng chúng và những lời của môi chúng nói ra,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.368Z",
      "updatedAt": "2024-09-07T14:05:57.368Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "0a6b7960-dba4-4d98-9841-792a7bd1f103",
      "number": 12,
      "content": "Nguyện chúng bị mắc tội vì sự kiêu ngạo của chúng,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.368Z",
      "updatedAt": "2024-09-07T14:05:57.368Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "07efdae8-087f-4d7d-a3ae-8cbca66d33bc",
      "number": 12,
      "content": "Và vì những lời nguyền rủa và những lời dối trá chúng đã nói.",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.368Z",
      "updatedAt": "2024-09-07T14:05:57.368Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "3a81de7e-4da6-45a6-927a-7b97e1dd04d8",
      "number": 13,
      "content": "Cầu xin Ngài nổi thịnh nộ và tiêu diệt chúng;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.371Z",
      "updatedAt": "2024-09-07T14:05:57.371Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "55b06190-30c8-4f82-94c2-3423cbdd09a6",
      "number": 13,
      "content": "Xin tiêu diệt chúng để chúng không còn nữa,",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.371Z",
      "updatedAt": "2024-09-07T14:05:57.371Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "c26e8db1-2cb0-441c-9301-616e38ae07ff",
      "number": 13,
      "content": "Để thiên hạ ở những nơi tận cùng trái đất",
      "order": 2,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.371Z",
      "updatedAt": "2024-09-07T14:05:57.371Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "a79fbe8b-17ab-4793-aac1-70a088921253",
      "number": 13,
      "content": "Biết rằng Đức Chúa Trời đang trị vì trong Gia-cốp. (Sê-la)",
      "order": 3,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.371Z",
      "updatedAt": "2024-09-07T14:05:57.371Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "2d7d4afd-de64-461b-a490-b5a3f1d52b27",
      "number": 14,
      "content": "Đến chiều tối chúng trở lại",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.384Z",
      "updatedAt": "2024-09-07T14:05:57.384Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "5f2e82ca-5e1c-4444-bd21-9abf065b0e5d",
      "number": 14,
      "content": "Và tru như chó,",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.384Z",
      "updatedAt": "2024-09-07T14:05:57.384Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "7571acae-4593-440b-aca5-82169cb9b512",
      "number": 14,
      "content": "Rồi đi lùng soát khắp thành.",
      "order": 2,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.384Z",
      "updatedAt": "2024-09-07T14:05:57.384Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "4ed9c2b2-9123-4824-bcd8-371d3ef01259",
      "number": 15,
      "content": "Chúng cứ đi lên đi xuống để kiếm thức ăn,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.393Z",
      "updatedAt": "2024-09-07T14:05:57.393Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "016854cc-524e-45a5-bc50-b91807bc7c78",
      "number": 15,
      "content": "Và gầm gừ nếu không được no bụng.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.393Z",
      "updatedAt": "2024-09-07T14:05:57.393Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "1b2ab1fd-c52e-4e26-984e-b1825de1e03c",
      "number": 16,
      "content": "Nhưng về phần con, con sẽ ca ngợi quyền năng Ngài;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.395Z",
      "updatedAt": "2024-09-07T14:05:57.395Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "cb2fe369-4fac-4082-b317-f043f480ab25",
      "number": 16,
      "content": "Sáng sớm con sẽ lớn tiếng ca ngợi ơn thương xót của Ngài,",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.395Z",
      "updatedAt": "2024-09-07T14:05:57.395Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "4dbb697d-39ee-4389-be3b-4eaffc1b9146",
      "number": 16,
      "content": "Vì Ngài là thành trì kiên cố của con,",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.395Z",
      "updatedAt": "2024-09-07T14:05:57.395Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "b61fb5e6-08fe-4ad9-aa1e-e8b27255c23d",
      "number": 16,
      "content": "Nơi con nương náu trong ngày hoạn nạn.",
      "order": 3,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.395Z",
      "updatedAt": "2024-09-07T14:05:57.395Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "11a79849-2eb0-4cc6-8caf-1dfedf5a9f29",
      "number": 17,
      "content": "Sức Mạnh của con ôi, con sẽ hát ca chúc tụng Ngài,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.398Z",
      "updatedAt": "2024-09-07T14:05:57.398Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "587edb9c-524a-4083-b1da-6a01c8eff759",
      "number": 17,
      "content": "Vì Ngài, Đức Chúa Trời ôi, là thành trì kiên cố của con,",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.398Z",
      "updatedAt": "2024-09-07T14:05:57.398Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    },
    {
      "id": "1fb60ed4-c6ad-4fe3-b072-3f8a40055974",
      "number": 17,
      "content": "Và là Đức Chúa Trời giàu lòng thương xót của con.",
      "order": 2,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:57.398Z",
      "updatedAt": "2024-09-07T14:05:57.398Z",
      "chapterId": "a5c314a3-a8c1-4fa4-b8b1-f956f5d1f254"
    }
  ],
