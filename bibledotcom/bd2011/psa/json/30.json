    },
    {
      "id": "a996630d-2901-4c73-a192-0fc81004baa6",
      "content": "CHÚA",
      "order": 2,
      "createdAt": "2024-09-23T05:46:30.849Z",
      "updatedAt": "2024-09-23T05:46:30.849Z",
      "verseId": "f2274f80-42b0-4739-9c40-80fe048bfd60",
      "chapterId": "cbea8c98-d6c8-4f02-bc12-c3c947fbae49"
    },
    {
      "id": "b9e28c7f-18f7-4e97-9e25-125fbad3e74b",
      "content": "vì Được Giải Cứu và Thoát Chết",
      "order": 3,
      "createdAt": "2024-09-23T05:46:30.854Z",
      "updatedAt": "2024-09-23T05:46:30.854Z",
