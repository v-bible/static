    },
    {
      "id": "8e6477f5-0595-4bfb-b85e-c4e7ff2863ee",
      "number": 20,
      "content": "Xin làm cho họ sợ hãi, CHÚA ôi,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.674Z",
