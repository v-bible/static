    {
      "id": "ef3a8a73-8eb1-41fb-8da8-346798f53476",
      "number": 19,
      "content": "Nguyện các dân bị xét xử trước mặt Ngài.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
