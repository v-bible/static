    },
    {
      "id": "de78d5ce-6175-483c-98ac-bdc8208de63f",
      "number": 20,
      "content": "Xin để các dân biết rằng họ chẳng qua chỉ là loài người. (Sê-la)",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
