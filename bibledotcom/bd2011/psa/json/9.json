{
  "chapter": {
    "id": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc",
    "number": 9,
    "ref": "https://www.bible.com/bible/19/PSA.9.BD2011",
    "createdAt": "2024-09-07T11:51:10.683Z",
    "updatedAt": "2024-09-07T11:51:10.683Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "23657941-dd8c-4acc-bae5-8c69bc99a7c6",
      "number": 1,
      "content": "Con sẽ hết lòng dâng lời cảm tạ CHÚA;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.578Z",
      "updatedAt": "2024-09-07T13:56:31.578Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "78e1f033-8cd9-4a46-87cc-10fbeb5e32e3",
      "number": 1,
      "content": "Con sẽ kể ra mọi việc kỳ diệu của Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.578Z",
      "updatedAt": "2024-09-07T13:56:31.578Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "b03bb0b0-3cae-4886-88f1-daf122f82d98",
      "number": 2,
      "content": "Con sẽ hân hoan và hớn hở trong Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.588Z",
      "updatedAt": "2024-09-07T13:56:31.588Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "78333c18-9c2f-4547-98e5-33eb1d59b591",
      "number": 2,
      "content": "Con sẽ ca tụng danh Ngài, ôi Đấng Tối Cao.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.588Z",
      "updatedAt": "2024-09-07T13:56:31.588Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "f19e5593-78ac-4210-a970-47a38e7e2ea6",
      "number": 3,
      "content": "Khi những kẻ thù của con quay trở lại,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.596Z",
      "updatedAt": "2024-09-07T13:56:31.596Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "65d35e97-1ff3-4097-9ee1-dc0e929c0002",
      "number": 3,
      "content": "Chúng sẽ ngã xuống và bị diệt vong trước mặt Ngài,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.596Z",
      "updatedAt": "2024-09-07T13:56:31.596Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "7705be37-7c08-42cb-8eb6-e50038c0dbf2",
      "number": 4,
      "content": "Vì Ngài binh vực quyền lợi con và duyên cớ con;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.600Z",
      "updatedAt": "2024-09-07T13:56:31.600Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "2662f2f1-64d9-4e8a-98d0-f43fd6995c72",
      "number": 4,
      "content": "Ngài ngự trên ngai và phán xét công minh.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.600Z",
      "updatedAt": "2024-09-07T13:56:31.600Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "a1acceea-dc41-424c-acca-53209e3a4f00",
      "number": 5,
      "content": "Ngài quở trách các dân; Ngài hủy diệt những kẻ ác;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.603Z",
      "updatedAt": "2024-09-07T13:56:31.603Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "76d92dfb-1647-44e3-b6bf-2b459d736224",
      "number": 5,
      "content": "Ngài xóa tên chúng nó đời đời bất tận.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.603Z",
      "updatedAt": "2024-09-07T13:56:31.603Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "5023b145-18e2-454f-9c48-f125b7d4929c",
      "number": 6,
      "content": "Kẻ thù của con đã bị kết liễu trong hoang tàn vĩnh viễn;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.607Z",
      "updatedAt": "2024-09-07T13:56:31.607Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "f4f682a7-f299-4870-bbd3-9a78eb061022",
      "number": 6,
      "content": "Những thành trì mà Ngài đã diệt trừ,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.607Z",
      "updatedAt": "2024-09-07T13:56:31.607Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "d5955b8b-4f3a-4e53-9416-28ef76a2d2d3",
      "number": 6,
      "content": "Dù kỷ niệm về chúng cũng chẳng còn;",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.607Z",
      "updatedAt": "2024-09-07T13:56:31.607Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "b2e14450-de05-4e46-bb85-af38526f8e2a",
      "number": 7,
      "content": "Nhưng CHÚA sẽ còn lại đời đời;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.610Z",
      "updatedAt": "2024-09-07T13:56:31.610Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "dede76aa-2693-43ca-9a9c-ac7d4bf372d9",
      "number": 7,
      "content": "Ngài đã lập ngôi Ngài để xét đoán.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.610Z",
      "updatedAt": "2024-09-07T13:56:31.610Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "2aac9b38-bcf7-4801-82fb-1d35a9868be8",
      "number": 8,
      "content": "Ngài sẽ phán xét thế gian theo lẽ công chính;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.613Z",
      "updatedAt": "2024-09-07T13:56:31.613Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "876ba92f-f3d8-4fd2-96c1-5641cb8efa96",
      "number": 8,
      "content": "Ngài sẽ xét xử muôn dân theo lẽ công bình.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.613Z",
      "updatedAt": "2024-09-07T13:56:31.613Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "a72c906e-2007-46fa-8cc1-2ce82e74e494",
      "number": 9,
      "content": "CHÚA là nơi nương náu cho những người bị hà hiếp;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.617Z",
      "updatedAt": "2024-09-07T13:56:31.617Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "8d154144-2b98-431e-b5bf-7a675ec4decc",
      "number": 9,
      "content": "Ngài là chỗ ẩn núp trong lúc nguy nan.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.617Z",
      "updatedAt": "2024-09-07T13:56:31.617Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "d99fc77b-6c7f-4d29-8806-d97ee2360a00",
      "number": 10,
      "content": "Phàm ai biết đến danh Ngài sẽ để lòng tin cậy Ngài,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.620Z",
      "updatedAt": "2024-09-07T13:56:31.620Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "0e7132bc-1897-47fd-b8d8-4e4817740672",
      "number": 10,
      "content": "Vì CHÚA ôi, Ngài không bỏ rơi những ai tìm kiếm Ngài.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.620Z",
      "updatedAt": "2024-09-07T13:56:31.620Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "6eed7bcd-e57b-48e3-aa69-4edae53bcc6a",
      "number": 11,
      "content": "Hãy ca tụng CHÚA, Đấng ngự tại Si-ôn!",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.623Z",
      "updatedAt": "2024-09-07T13:56:31.623Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "fc1754fa-5dcb-44f3-9883-74b86e9354c2",
      "number": 11,
      "content": "Hãy rao ra những công việc Ngài giữa các dân.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.623Z",
      "updatedAt": "2024-09-07T13:56:31.623Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "b00574bf-51bc-4650-8062-919e09885f5f",
      "number": 12,
      "content": "Vì Đấng báo thù kẻ gây đổ máu đã nhớ lại họ,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.627Z",
      "updatedAt": "2024-09-07T13:56:31.627Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "828cf5ba-373a-4add-ad24-09cd39b926ce",
      "number": 12,
      "content": "Ngài không quên tiếng kêu cầu của những người khốn khổ.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.627Z",
      "updatedAt": "2024-09-07T13:56:31.627Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "7cf51cd8-50e7-4d0f-ad5f-3d28713bcdf2",
      "number": 13,
      "content": "Xin thương xót con, CHÚA ôi!",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.632Z",
      "updatedAt": "2024-09-07T13:56:31.632Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "a8fc06f2-e7fd-4407-ac0a-c691a3a0fe63",
      "number": 13,
      "content": "Xin đoái xem những khổ đau con chịu vì những kẻ ghét con;",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.632Z",
      "updatedAt": "2024-09-07T13:56:31.632Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "94dec8dd-d264-4dbb-9221-99b06a71b80e",
      "number": 13,
      "content": "Xin nhấc đưa con ra khỏi cổng của tử thần,",
      "order": 2,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.632Z",
      "updatedAt": "2024-09-07T13:56:31.632Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "b2c6c902-d28c-494a-acbc-5fb608d386f0",
      "number": 14,
      "content": "Để con có thể thuật lại mọi sự hầu ca ngợi Ngài",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.638Z",
      "updatedAt": "2024-09-07T13:56:31.638Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "d781aad4-f66a-4b30-9953-d2483113275c",
      "number": 14,
      "content": "Nơi các cổng của Ái Nữ Si-ôn,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.638Z",
      "updatedAt": "2024-09-07T13:56:31.638Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "e042832b-9ce6-4c6c-8806-e7e5f3439b01",
      "number": 14,
      "content": "Hầu bày tỏ niềm vui vì được hưởng ơn giải cứu của Ngài.",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.638Z",
      "updatedAt": "2024-09-07T13:56:31.638Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "a3eb9243-678a-4c84-a131-487997421fec",
      "number": 15,
      "content": "Các dân đã lún sâu vào hố chúng đào;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.644Z",
      "updatedAt": "2024-09-07T13:56:31.644Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "294006a7-ecd8-4e11-8821-f08f0a55732d",
      "number": 15,
      "content": "Chân chúng đã vướng vào bẫy chúng giăng.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.644Z",
      "updatedAt": "2024-09-07T13:56:31.644Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "9e034583-534f-49dc-9b91-8161af2968df",
      "number": 16,
      "content": "CHÚA đã bày tỏ Ngài ra khi Ngài thi hành sự đoán phạt;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.649Z",
      "updatedAt": "2024-09-07T13:56:31.649Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "b0bc3479-0b7c-4e43-b5f5-1a0b659b0e41",
      "number": 16,
      "content": "Kẻ ác đã bị mắc vào bẫy do chính tay nó gài. (Hãy suy gẫm – Sê-la)",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.649Z",
      "updatedAt": "2024-09-07T13:56:31.649Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "db5e7bdb-da4e-4e64-980d-ce0b29f0774e",
      "number": 17,
      "content": "Kẻ ác sẽ đi vào âm phủ;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.655Z",
      "updatedAt": "2024-09-07T13:56:31.655Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "3e67fc2f-a731-4826-bdba-3412b9cfc252",
      "number": 17,
      "content": "Mọi dân quên Đức Chúa Trời cũng vậy.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.655Z",
      "updatedAt": "2024-09-07T13:56:31.655Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "b904c9a9-40e7-43c1-994e-6e125245531a",
      "number": 18,
      "content": "Người thiếu thốn sẽ chẳng bị bỏ quên luôn luôn,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.660Z",
      "updatedAt": "2024-09-07T13:56:31.660Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "131c10a7-e081-472f-961e-ad377af52f70",
      "number": 18,
      "content": "Người nghèo khó sẽ chẳng bị thất vọng mãi mãi.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.660Z",
      "updatedAt": "2024-09-07T13:56:31.660Z",
      "chapterId": "07ac8cbd-78bb-468b-a6fb-a94d350f55fc"
    },
    {
      "id": "a0661065-e1d4-42df-8ccd-c3deb3fbe030",
      "number": 19,
      "content": "CHÚA ôi, xin trỗi dậy, xin đừng để loài người thắng hơn.",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:31.666Z",
