{
  "chapter": {
    "id": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0",
    "number": 107,
    "ref": "https://www.bible.com/bible/19/PSA.107.BD2011",
    "createdAt": "2024-09-07T11:51:11.470Z",
    "updatedAt": "2024-09-07T11:51:11.470Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "31565e3a-a387-471d-a6ca-0875d7ee7791",
      "number": 1,
      "content": "Hãy cảm tạ CHÚA, vì Ngài thật tốt,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.326Z",
      "updatedAt": "2024-09-07T14:17:33.326Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "ec92e072-e190-4c8d-a3a4-b720d5778d83",
      "number": 1,
      "content": "Vì tình thương của Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.326Z",
      "updatedAt": "2024-09-07T14:17:33.326Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "8c8f804d-6395-409c-9983-e6158dae52d4",
      "number": 2,
      "content": "Nguyện những người được cứu chuộc của CHÚA nói như vậy,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.336Z",
      "updatedAt": "2024-09-07T14:17:33.336Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "b31f473d-8628-4cf6-a0d5-f99184eaf9d4",
      "number": 2,
      "content": "Những người được Ngài cứu khỏi tay kẻ thù,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.336Z",
      "updatedAt": "2024-09-07T14:17:33.336Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "34955cc0-8a6a-41d9-a6d3-3f5ab3519222",
      "number": 3,
      "content": "Rồi gom nhóm họ từ các nước trở về;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.338Z",
      "updatedAt": "2024-09-07T14:17:33.338Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "5bcbc2fe-7c5b-4ba3-b840-0f58c1b0f231",
      "number": 3,
      "content": "Từ phương đông và phương tây,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.338Z",
      "updatedAt": "2024-09-07T14:17:33.338Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "58f6e797-e286-4842-af62-d8b15627ac37",
      "number": 3,
      "content": "Phương bắc và phương nam.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.338Z",
      "updatedAt": "2024-09-07T14:17:33.338Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "7b6bcc91-5aa7-4b65-baf8-6d0073f3bc62",
      "number": 4,
      "content": "Họ lang thang trong đồng hoang, trong nơi hoang vắng;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.341Z",
      "updatedAt": "2024-09-07T14:17:33.341Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "3327567a-1631-4d7c-8271-8877ba7ac34e",
      "number": 4,
      "content": "Họ chẳng tìm được đường về một thành để ở.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.341Z",
      "updatedAt": "2024-09-07T14:17:33.341Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "9def32d8-65e3-455a-ae93-2bab7ab4b533",
      "number": 5,
      "content": "Họ đói khát;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.343Z",
      "updatedAt": "2024-09-07T14:17:33.343Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "2a65b49a-5d24-403a-a1ab-46eefc477525",
      "number": 5,
      "content": "Linh hồn họ mòn mỏi nao sờn.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.343Z",
      "updatedAt": "2024-09-07T14:17:33.343Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "ad4b8268-a59e-4e8a-a3c8-00d16ed7131b",
      "number": 6,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với CHÚA,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.345Z",
      "updatedAt": "2024-09-07T14:17:33.345Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "390e12b4-ad25-4271-83a7-d0eba2026c56",
      "number": 6,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.345Z",
      "updatedAt": "2024-09-07T14:17:33.345Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "de448e1d-c4c3-43bf-bdca-861aa18ea0e9",
      "number": 7,
      "content": "Ngài dẫn đưa họ vào đúng con đường phải đi,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.347Z",
      "updatedAt": "2024-09-07T14:17:33.347Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "7866d677-d270-42ef-b71d-b7e65b41cbd8",
      "number": 7,
      "content": "Để họ đến một thành, một nơi để ở.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.347Z",
      "updatedAt": "2024-09-07T14:17:33.347Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "5d44f6a6-f108-483e-a9a4-de41061e64f9",
      "number": 8,
      "content": "Hãy để họ cảm tạ CHÚA vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.349Z",
      "updatedAt": "2024-09-07T14:17:33.349Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "a96e31f2-d820-46cb-a7ed-f0b90239e314",
      "number": 8,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người;",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.349Z",
      "updatedAt": "2024-09-07T14:17:33.349Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "945b43f0-4f7d-49c1-90c8-16f5f6cde524",
      "number": 9,
      "content": "Vì Ngài làm thỏa mãn linh hồn khao khát,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.352Z",
      "updatedAt": "2024-09-07T14:17:33.352Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "e02c8a02-5faa-4054-800a-81f89282724b",
      "number": 9,
      "content": "Và làm no nê linh hồn đói lả bằng các thức ăn ngon.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.352Z",
      "updatedAt": "2024-09-07T14:17:33.352Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "5ce3c8e3-4f8e-40b4-bdfc-56b6b545d0b0",
      "number": 10,
      "content": "Có những kẻ sống trong tối tăm và trong bóng tử thần,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.354Z",
      "updatedAt": "2024-09-07T14:17:33.354Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "0ee63d19-6763-445a-9a7a-00d5f4a73573",
      "number": 10,
      "content": "Bị giam cầm trong khổ đau và xiềng xích,",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.354Z",
      "updatedAt": "2024-09-07T14:17:33.354Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "f7fee46f-36f5-4564-bd35-07de557001f6",
      "number": 11,
      "content": "Bởi vì họ chống lại những lời của Đức Chúa Trời,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.356Z",
      "updatedAt": "2024-09-07T14:17:33.356Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "5bcddc6a-a60b-4468-9c0c-881559756f27",
      "number": 11,
      "content": "Và khinh bỉ lời chỉ bảo của Đấng Chí Cao.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.356Z",
      "updatedAt": "2024-09-07T14:17:33.356Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "b9afb4ad-c9d5-4794-9f51-aa88c5f5f825",
      "number": 12,
      "content": "Vì thế Ngài lấy gian lao hạ lòng họ xuống;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.358Z",
      "updatedAt": "2024-09-07T14:17:33.358Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "458a5cf2-afe7-4f52-bdd9-10c3ce529d15",
      "number": 12,
      "content": "Họ bị ngã nhào và chẳng ai giúp họ đứng lên.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.358Z",
      "updatedAt": "2024-09-07T14:17:33.358Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "98bee9bf-bae7-4ba3-9bec-3bb072dcb2f3",
      "number": 13,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với CHÚA,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.360Z",
      "updatedAt": "2024-09-07T14:17:33.360Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "f43a7a82-4eed-466c-9952-d7e7d655ed91",
      "number": 13,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.360Z",
      "updatedAt": "2024-09-07T14:17:33.360Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "f72d09fe-daaf-458b-aae9-b776c4b1a4e3",
      "number": 14,
      "content": "Ngài đem họ ra khỏi cảnh tối tăm và bóng tử thần,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.363Z",
      "updatedAt": "2024-09-07T14:17:33.363Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "f5df2292-0143-44ae-b858-6aba255137d1",
      "number": 14,
      "content": "Và đập tan xiềng xích gông cùm trói buộc họ.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.363Z",
      "updatedAt": "2024-09-07T14:17:33.363Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "c1b06f7b-445d-42be-aa6b-300d60d1b118",
      "number": 15,
      "content": "Hãy để họ cảm tạ CHÚA vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.368Z",
      "updatedAt": "2024-09-07T14:17:33.368Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "1b016714-b011-4a5c-93f6-0cc8abc14adc",
      "number": 15,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người,",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.368Z",
      "updatedAt": "2024-09-07T14:17:33.368Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "98edde19-c73b-4c68-be62-5f0bd5db2494",
      "number": 16,
      "content": "Vì Ngài phá tan các cổng bằng đồng,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.372Z",
      "updatedAt": "2024-09-07T14:17:33.372Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "1b3f7229-0cda-48c2-96e6-dab65eea708e",
      "number": 16,
      "content": "Và Ngài cắt rời những song bằng sắt.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.372Z",
      "updatedAt": "2024-09-07T14:17:33.372Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "ba090906-0a76-4da2-94b8-d0997e3abaa3",
      "number": 17,
      "content": "Có những kẻ cứ bội nghịch nên hóa ra điên dại,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.377Z",
      "updatedAt": "2024-09-07T14:17:33.377Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "8f9f649e-ac8b-4c90-a9cf-73499b3d2a17",
      "number": 17,
      "content": "Và vì tội lỗi mình nên phải chuốc lấy khổ đau.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.377Z",
      "updatedAt": "2024-09-07T14:17:33.377Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "1dbc04ef-f624-45f2-a3ee-822282b4c759",
      "number": 18,
      "content": "Linh hồn họ gớm ghê mọi thực phẩm,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.382Z",
      "updatedAt": "2024-09-07T14:17:33.382Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "7e2c2458-9f78-47e7-a6ef-6b6ecdc9dd45",
      "number": 18,
      "content": "Và họ bị kéo gần đến cổng của tử thần.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.382Z",
      "updatedAt": "2024-09-07T14:17:33.382Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "c46c3502-e9f8-4660-a567-ddea7b56e208",
      "number": 19,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với CHÚA,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.387Z",
      "updatedAt": "2024-09-07T14:17:33.387Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "bbd93624-ea91-43f8-91e7-9bc302f1f72d",
      "number": 19,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.387Z",
      "updatedAt": "2024-09-07T14:17:33.387Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "dd029360-53a7-4554-9489-5af7314ccee6",
      "number": 20,
      "content": "Ngài ban lời Ngài và chữa lành họ;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.392Z",
      "updatedAt": "2024-09-07T14:17:33.392Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "7f6a893e-8898-4502-b873-f20a6245fc30",
      "number": 20,
      "content": "Và giải cứu họ khỏi miệng của mộ phần.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.392Z",
      "updatedAt": "2024-09-07T14:17:33.392Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "539a4536-760a-44db-8598-b244f9b35e7a",
      "number": 21,
      "content": "Hãy để họ cảm tạ CHÚA vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.397Z",
      "updatedAt": "2024-09-07T14:17:33.397Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "f0aa6b57-f871-45df-a737-9100889f6124",
      "number": 21,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.397Z",
      "updatedAt": "2024-09-07T14:17:33.397Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "b0ef093c-1277-493a-9a09-9af4c43cdc28",
      "number": 22,
      "content": "Cũng hãy để họ dâng những của lễ cảm tạ lên Ngài,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.402Z",
      "updatedAt": "2024-09-07T14:17:33.402Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "ecbeb13e-c27f-4c81-be06-af0d014ffc40",
      "number": 22,
      "content": "Và thuật lại các công việc Ngài bằng những lời ca ngợi vui mừng.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.402Z",
      "updatedAt": "2024-09-07T14:17:33.402Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "14a033aa-10c9-4d76-a096-df507d13c9a1",
      "number": 23,
      "content": "Có những kẻ xuống tàu vượt đại dương thăm thẳm,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.408Z",
      "updatedAt": "2024-09-07T14:17:33.408Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "2549e245-ad4c-456c-989b-80276af8711c",
      "number": 23,
      "content": "Những người làm ăn trên biển cả mênh mông;",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.408Z",
      "updatedAt": "2024-09-07T14:17:33.408Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "760d2f5d-9625-4bc9-9c70-2f0c5d272ebd",
      "number": 24,
      "content": "Họ trông thấy những việc làm của CHÚA,",
      "order": 0,
      "parNumber": 50,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.413Z",
      "updatedAt": "2024-09-07T14:17:33.413Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "38746e80-3812-468a-a191-5ac8e529bc1c",
      "number": 24,
      "content": "Và những việc lạ lùng của Ngài ở giữa biển sâu.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.413Z",
      "updatedAt": "2024-09-07T14:17:33.413Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "1e28602d-26d9-41c3-94b2-5a9d7a3c75bd",
      "number": 25,
      "content": "Vì Ngài phán một tiếng, bão tố liền nổi lên cuồn cuộn,",
      "order": 0,
      "parNumber": 52,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.418Z",
      "updatedAt": "2024-09-07T14:17:33.418Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "be9dfad2-5090-43cc-a1a3-7f2b4c466316",
      "number": 25,
      "content": "Cuồng phong nâng cao những ngọn sóng kinh hồn.",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.418Z",
      "updatedAt": "2024-09-07T14:17:33.418Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "98e79023-ae45-4921-8036-b5cb8de8d558",
      "number": 26,
      "content": "Sóng cả dâng cao đến tận trời, rồi trụt sâu xuống vực thẳm;",
      "order": 0,
      "parNumber": 54,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.423Z",
      "updatedAt": "2024-09-07T14:17:33.423Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "a8b411a2-96bb-419e-97cb-642e1bd969ee",
      "number": 26,
      "content": "Giữa cảnh khốn cùng họ hồn phi phách tán.",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.423Z",
      "updatedAt": "2024-09-07T14:17:33.423Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "ce0c2152-d1f3-4c73-8400-2248474ba70f",
      "number": 27,
      "content": "Họ bị quay cuồng điên đảo tựa người say;",
      "order": 0,
      "parNumber": 56,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.428Z",
      "updatedAt": "2024-09-07T14:17:33.428Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "65980555-3eef-43df-8ce8-906df438bebd",
      "number": 27,
      "content": "Mọi tài trí khôn ngoan đều biến mất.",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.428Z",
      "updatedAt": "2024-09-07T14:17:33.428Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "3056f76f-a84e-4f73-b566-f95366696658",
      "number": 28,
      "content": "Bấy giờ trong cơn khốn quẫn họ kêu cầu với CHÚA,",
      "order": 0,
      "parNumber": 58,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.433Z",
      "updatedAt": "2024-09-07T14:17:33.433Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "a5b3169f-c72f-485c-8dc0-7f8f97055b47",
      "number": 28,
      "content": "Và Ngài giải cứu họ ra khỏi cảnh khốn cùng.",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.433Z",
      "updatedAt": "2024-09-07T14:17:33.433Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "0036c60b-c4e2-4605-bdb8-5892da5a0408",
      "number": 29,
      "content": "Ngài khiến bão tố yên lặng như tờ;",
      "order": 0,
      "parNumber": 60,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.438Z",
      "updatedAt": "2024-09-07T14:17:33.438Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "4aa6d6af-338d-4baa-9de7-d105766ed502",
      "number": 29,
      "content": "Những ngọn sóng biển lặng im phăng phắc.",
      "order": 1,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.438Z",
      "updatedAt": "2024-09-07T14:17:33.438Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "50fb0028-0e56-463e-8cc2-71241e5fcdaa",
      "number": 30,
      "content": "Họ rất đỗi vui mừng vì biển êm gió lặng;",
      "order": 0,
      "parNumber": 62,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.443Z",
      "updatedAt": "2024-09-07T14:17:33.443Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "703c978f-c198-4b49-ad02-db07c5222208",
      "number": 30,
      "content": "Ngài lại dẫn đưa họ đến hải cảng họ ước mong.",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.443Z",
      "updatedAt": "2024-09-07T14:17:33.443Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "6695130f-fc35-408e-ae89-72358521fe31",
      "number": 31,
      "content": "Hãy để họ cảm tạ CHÚA vì tình thương của Ngài,",
      "order": 0,
      "parNumber": 64,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.448Z",
      "updatedAt": "2024-09-07T14:17:33.448Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "0c8cdd8e-7ac6-4673-a493-45840bb97b34",
      "number": 31,
      "content": "Và vì những việc lạ lùng Ngài làm cho con cái loài người.",
      "order": 1,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.448Z",
      "updatedAt": "2024-09-07T14:17:33.448Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "1019fe7a-e0ba-49db-bb83-8c9f9a181a52",
      "number": 32,
      "content": "Hãy để họ tôn cao Ngài giữa đại hội đông người,",
      "order": 0,
      "parNumber": 66,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.454Z",
      "updatedAt": "2024-09-07T14:17:33.454Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "c1e5eaf3-6e4f-4216-b65f-bde5066fd641",
      "number": 32,
      "content": "Và ca tụng Ngài giữa hội đồng trưởng lão.",
      "order": 1,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.454Z",
      "updatedAt": "2024-09-07T14:17:33.454Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "e5c59e68-b58e-41a2-bb4b-76ab2e3c6659",
      "number": 33,
      "content": "Ngài biến các dòng sông thành sa mạc hoang vu,",
      "order": 0,
      "parNumber": 68,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.459Z",
      "updatedAt": "2024-09-07T14:17:33.459Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "4d6ba56c-f9f5-469c-bf98-95c177776ae9",
      "number": 33,
      "content": "Các dòng suối thành đất khô cằn cỗi,",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.459Z",
      "updatedAt": "2024-09-07T14:17:33.459Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "e597a5ef-d718-4e0b-93b5-98a07f3294fb",
      "number": 34,
      "content": "Đất đai màu mỡ thành đồng chua nước mặn,",
      "order": 0,
      "parNumber": 70,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.464Z",
      "updatedAt": "2024-09-07T14:17:33.464Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "e7e0d0f9-ed1d-486a-8346-cf25d384e1ec",
      "number": 34,
      "content": "Vì sự gian ác của dân cư ở đó.",
      "order": 1,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.464Z",
      "updatedAt": "2024-09-07T14:17:33.464Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "dd5b90a5-5935-4a90-83d8-e9cf6e515797",
      "number": 35,
      "content": "Ngài biến đồng hoang thành đất có ao hồ,",
      "order": 0,
      "parNumber": 72,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.469Z",
      "updatedAt": "2024-09-07T14:17:33.469Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "7b68c988-c784-43c1-8d3b-66d55f650942",
      "number": 35,
      "content": "Và xứ khô cằn thành nơi có các dòng nước ngọt;",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.469Z",
      "updatedAt": "2024-09-07T14:17:33.469Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "16b1d6d5-7049-4882-bb01-7a88b1aaba45",
      "number": 36,
      "content": "Ngài đã cho những kẻ đói khát đến cư ngụ tại đó,",
      "order": 0,
      "parNumber": 74,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.474Z",
      "updatedAt": "2024-09-07T14:17:33.474Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "7e501557-f340-49d9-8bf8-bc2d697eff05",
      "number": 36,
      "content": "Để họ xây dựng một thành mà an cư lạc nghiệp;",
      "order": 1,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.474Z",
      "updatedAt": "2024-09-07T14:17:33.474Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "b0006712-8791-454b-beb3-3efb16c1a73e",
      "number": 37,
      "content": "Họ gieo hạt ngoài đồng và trồng tỉa vườn nho,",
      "order": 0,
      "parNumber": 76,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.479Z",
      "updatedAt": "2024-09-07T14:17:33.479Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "83312048-2db6-41e5-b2ec-520e878a1a87",
      "number": 37,
      "content": "Rồi thu hoạch hoa màu sung túc.",
      "order": 1,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.479Z",
      "updatedAt": "2024-09-07T14:17:33.479Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "8d3c106e-5ce1-4943-912f-03a9ff585376",
      "number": 38,
      "content": "Ngài ban phước cho họ, và dân số họ gia tăng;",
      "order": 0,
      "parNumber": 78,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.484Z",
      "updatedAt": "2024-09-07T14:17:33.484Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "340f91a6-34ae-4ac7-9e36-1c56d01f28b6",
      "number": 38,
      "content": "Ngài chẳng để cho bầy súc vật của họ bị giảm thiểu.",
      "order": 1,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.484Z",
      "updatedAt": "2024-09-07T14:17:33.484Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "21bc11fa-b9fe-40c4-9602-082a36eecf4c",
      "number": 39,
      "content": "Có lúc họ bị tiêu hao lụn bại, phải cúi đầu khuất phục ngoại bang,",
      "order": 0,
      "parNumber": 80,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.489Z",
      "updatedAt": "2024-09-07T14:17:33.489Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "24962f3c-dfdc-4002-a006-292913d225dc",
      "number": 39,
      "content": "Bị áp bức, khổ đau, và sầu não.",
      "order": 1,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.489Z",
      "updatedAt": "2024-09-07T14:17:33.489Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "4d65d898-d03f-4567-9c06-1242fca63bc3",
      "number": 40,
      "content": "Ngài trút đổ ô nhục trên những người quyền quý của họ,",
      "order": 0,
      "parNumber": 82,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.494Z",
      "updatedAt": "2024-09-07T14:17:33.494Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "41ebffee-582e-4def-b271-1e68e243a178",
      "number": 40,
      "content": "Khiến họ lang thang trong nơi hoang vắng không đường.",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.494Z",
      "updatedAt": "2024-09-07T14:17:33.494Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "080dd026-335e-4744-9eaa-55f2819c90aa",
      "number": 41,
      "content": "Nhưng Ngài lại cất những người nghèo ra khỏi cảnh lầm than,",
      "order": 0,
      "parNumber": 84,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.498Z",
      "updatedAt": "2024-09-07T14:17:33.498Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "d238d9fd-814f-4084-9483-ad27f97ab37e",
      "number": 41,
      "content": "Và cho con cháu họ được gia tăng đông đúc.",
      "order": 1,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.498Z",
      "updatedAt": "2024-09-07T14:17:33.498Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "7f21705d-f2b2-47ac-a918-fa6fef3145a3",
      "number": 42,
      "content": "Những người ngay lành thấy thế bèn vui mừng;",
      "order": 0,
      "parNumber": 86,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.501Z",
      "updatedAt": "2024-09-07T14:17:33.501Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "f0abb1f8-6a89-41e7-96b5-cc435a007911",
      "number": 42,
      "content": "Còn những kẻ gian ác thảy đều câm miệng lại.",
      "order": 1,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.501Z",
      "updatedAt": "2024-09-07T14:17:33.501Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "be96270e-9237-4f61-a859-5953c8f79216",
      "number": 43,
      "content": "Hỡi ai là người khôn ngoan, hãy suy gẫm những điều ấy,",
      "order": 0,
      "parNumber": 88,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.504Z",
      "updatedAt": "2024-09-07T14:17:33.504Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    },
    {
      "id": "6f7f4cd9-350e-4a55-aa0a-2a1666b31b83",
      "number": 43,
      "content": "Rồi sẽ hiểu được thế nào là tình thương của CHÚA.",
      "order": 1,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:33.504Z",
      "updatedAt": "2024-09-07T14:17:33.504Z",
      "chapterId": "a329d03c-8fb4-47c3-9d6f-c77710bd6db0"
    }
  ],
