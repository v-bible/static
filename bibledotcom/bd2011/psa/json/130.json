{
  "chapter": {
    "id": "d32c63db-e1ef-4210-b391-b87059d55ed2",
    "number": 130,
    "ref": "https://www.bible.com/bible/19/PSA.130.BD2011",
    "createdAt": "2024-09-07T11:51:11.546Z",
    "updatedAt": "2024-09-07T11:51:11.546Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "5b39aa05-5f56-4108-b219-6859c73fdf30",
      "number": 1,
      "content": "Lạy CHÚA, từ chốn vực sâu con kêu cầu Ngài.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:22.982Z",
      "updatedAt": "2024-09-07T14:23:22.982Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "391feca8-115c-4158-8970-7a799a2a0e93",
      "number": 2,
      "content": "Chúa ôi, xin nghe tiếng cầu xin của con;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:22.992Z",
      "updatedAt": "2024-09-07T14:23:22.992Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "03ba560a-dfe8-458d-893f-2f62d5e45be7",
      "number": 2,
      "content": "Xin lắng tai nghe",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:22.992Z",
      "updatedAt": "2024-09-07T14:23:22.992Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "e9ba3680-97ff-4c9d-8b18-daa74a671e67",
      "number": 2,
      "content": "Lời cầu nguyện của con.",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:22.992Z",
      "updatedAt": "2024-09-07T14:23:22.992Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "ca6f7ff8-5040-4417-8498-ebb9bcfbc342",
      "number": 3,
      "content": "Lạy CHÚA, nếu Ngài ghi nhớ mọi tội lỗi chúng con,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:22.997Z",
      "updatedAt": "2024-09-07T14:23:22.997Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "57cf8843-e699-4e7f-bbc0-5e2ea7f01b0d",
      "number": 3,
      "content": "Chúa ôi, còn ai đứng nổi trước mặt Ngài?",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:22.997Z",
      "updatedAt": "2024-09-07T14:23:22.997Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "a522a625-f6f2-4413-b741-b66310e6d4f6",
      "number": 4,
      "content": "Nhưng vì Ngài ban ơn tha thứ,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23Z",
      "updatedAt": "2024-09-07T14:23:23Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "11d3218c-2935-45b1-a666-873331fdddfc",
      "number": 4,
      "content": "Nên chúng con càng kính sợ Ngài hơn.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23Z",
      "updatedAt": "2024-09-07T14:23:23Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "db473802-e1d7-4b01-a082-1784bc1984ee",
      "number": 5,
      "content": "Tôi trông đợi CHÚA, linh hồn tôi trông đợi Ngài;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23.003Z",
      "updatedAt": "2024-09-07T14:23:23.003Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "e2a97a95-a9f1-49ea-8e12-87a954a2cb09",
      "number": 5,
      "content": "Tôi đặt trọn hy vọng nơi lời Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23.003Z",
      "updatedAt": "2024-09-07T14:23:23.003Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "ff8e05ab-5d35-4ce4-a566-2c298e66ee53",
      "number": 6,
      "content": "Linh hồn tôi trông đợi Chúa",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23.007Z",
      "updatedAt": "2024-09-07T14:23:23.007Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "485282bb-aa25-43f4-893b-02fd3e6275b1",
      "number": 6,
      "content": "Hơn người lính canh trông đợi sáng,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23.007Z",
      "updatedAt": "2024-09-07T14:23:23.007Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "88362c14-499f-4cd8-a421-9fa91fdc98a0",
      "number": 6,
      "content": "Thật hơn người lính canh trông đợi sáng.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23.007Z",
      "updatedAt": "2024-09-07T14:23:23.007Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "2a051610-c9f9-4ac7-be93-ac1e3ac0a8be",
      "number": 7,
      "content": "Hỡi I-sơ-ra-ên, hãy đặt trọn hy vọng nơi CHÚA,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23.013Z",
      "updatedAt": "2024-09-07T14:23:23.013Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "bf3b583d-83b3-4796-b4e8-ca2fecf0527a",
      "number": 7,
      "content": "Vì nơi CHÚA có tình thương;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23.013Z",
      "updatedAt": "2024-09-07T14:23:23.013Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "47c0e1aa-8009-4e45-afbb-879d1136ed62",
      "number": 7,
      "content": "Ngài ban ơn cứu chuộc cách dồi dào.",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23.013Z",
      "updatedAt": "2024-09-07T14:23:23.013Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "15903095-3b23-4dad-86b0-0562eb652694",
      "number": 8,
      "content": "Ngài sẽ cứu chuộc I-sơ-ra-ên",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23.019Z",
      "updatedAt": "2024-09-07T14:23:23.019Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    },
    {
      "id": "1e10cc62-d900-4472-8256-2148c92a09c1",
      "number": 8,
      "content": "Khỏi mọi tội lỗi của họ.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:23.019Z",
      "updatedAt": "2024-09-07T14:23:23.019Z",
      "chapterId": "d32c63db-e1ef-4210-b391-b87059d55ed2"
    }
  ],
