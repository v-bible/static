{
  "chapter": {
    "id": "18253d4b-6d7a-4fef-9806-aa4150148ebe",
    "number": 23,
    "ref": "https://www.bible.com/bible/19/PSA.23.BD2011",
    "createdAt": "2024-09-07T11:51:10.803Z",
    "updatedAt": "2024-09-07T11:51:10.803Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "16fd7acd-da30-4ac4-93d7-e25561c11963",
      "number": 1,
      "content": "CHÚA là Đấng chăn giữ tôi, tôi sẽ chẳng thiếu thốn gì.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.766Z",
      "updatedAt": "2024-09-07T14:00:38.766Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "cc858969-c291-4db2-a631-0933c774a1ce",
      "number": 2,
      "content": "Ngài cho tôi nằm nghỉ trong đồng cỏ xanh tươi;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.775Z",
      "updatedAt": "2024-09-07T14:00:38.775Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "e3a35bfa-ef00-415b-bf13-dd1e499d1212",
      "number": 2,
      "content": "Ngài dẫn tôi đến mé nước bình tịnh.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.775Z",
      "updatedAt": "2024-09-07T14:00:38.775Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "5522a129-aee8-4a0b-97db-56d8c5ca558c",
      "number": 3,
      "content": "Ngài phục hồi sức lực cho linh hồn tôi;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.778Z",
      "updatedAt": "2024-09-07T14:00:38.778Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "436aca98-174f-42ca-8a0c-7208b4c0f1ac",
      "number": 3,
      "content": "Ngài dẫn tôi vào các lối công chính,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.778Z",
      "updatedAt": "2024-09-07T14:00:38.778Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "71bb0649-0e10-478e-a7b6-3b9c87b53a16",
      "number": 3,
      "content": "Vì cớ danh Ngài.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.778Z",
      "updatedAt": "2024-09-07T14:00:38.778Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "bf79150b-7f6e-4da5-a852-a7d50823bb92",
      "number": 4,
      "content": "Thật vậy, dù khi con đi qua thung lũng rợp bóng tử thần,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.781Z",
      "updatedAt": "2024-09-07T14:00:38.781Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "988d9197-fdd4-453b-ae98-2f4a021df6d5",
      "number": 4,
      "content": "Con sẽ chẳng sợ tai họa nào,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.781Z",
      "updatedAt": "2024-09-07T14:00:38.781Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "21a6c3df-1c09-4bdd-abaa-9fcfefe05dc3",
      "number": 4,
      "content": "Vì Ngài ở với con;",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.781Z",
      "updatedAt": "2024-09-07T14:00:38.781Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "41c99607-4f8e-490d-8148-09e1da07a00b",
      "number": 4,
      "content": "Cây trượng và cây gậy của Ngài",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.781Z",
      "updatedAt": "2024-09-07T14:00:38.781Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "292a870f-7d16-47ed-84c0-b39665bf6ac3",
      "number": 4,
      "content": "An ủi con.",
      "order": 4,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.781Z",
      "updatedAt": "2024-09-07T14:00:38.781Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "3f01618e-80b2-4c39-a7c1-08232b34afcd",
      "number": 5,
      "content": "Ngài dọn bàn cho con",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.786Z",
      "updatedAt": "2024-09-07T14:00:38.786Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "fd380ec1-e0a0-48a4-a079-5491894fd424",
      "number": 5,
      "content": "Trước mặt những kẻ thù của con;",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.786Z",
      "updatedAt": "2024-09-07T14:00:38.786Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "93e842d4-5827-4556-b1d4-c28b64488f1f",
      "number": 5,
      "content": "Ngài xức dầu cho đầu con;",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.786Z",
      "updatedAt": "2024-09-07T14:00:38.786Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "e2fa86eb-db8b-43d3-8135-8d4c8cd03963",
      "number": 5,
      "content": "Chén con đầy tràn.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.786Z",
      "updatedAt": "2024-09-07T14:00:38.786Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "7b4ffadf-6415-4a14-99ea-6adb834809b0",
      "number": 6,
      "content": "Quả thật, trọn đời tôi,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.788Z",
      "updatedAt": "2024-09-07T14:00:38.788Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "c2bd8f92-c182-4276-b0a6-a298f2f95ca7",
      "number": 6,
      "content": "Phước hạnh và ơn thương xót sẽ theo tôi;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.788Z",
      "updatedAt": "2024-09-07T14:00:38.788Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "84a22eb7-2bfd-45fa-af17-e319ed80244e",
      "number": 6,
      "content": "Tôi sẽ ở trong nhà của CHÚA",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.788Z",
      "updatedAt": "2024-09-07T14:00:38.788Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    },
    {
      "id": "f883ec9e-08d8-4f4c-adc6-c183a6fb05dd",
      "number": 6,
      "content": "Cho đến đời đời.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:00:38.788Z",
      "updatedAt": "2024-09-07T14:00:38.788Z",
      "chapterId": "18253d4b-6d7a-4fef-9806-aa4150148ebe"
    }
  ],
