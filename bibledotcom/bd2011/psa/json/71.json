{
  "chapter": {
    "id": "e09abe58-01c2-4959-a52a-62186a316440",
    "number": 71,
    "ref": "https://www.bible.com/bible/19/PSA.71.BD2011",
    "createdAt": "2024-09-07T11:51:11.174Z",
    "updatedAt": "2024-09-07T11:51:11.174Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "13450035-3a7a-4ca2-9827-4a24c7bffca6",
      "number": 1,
      "content": "CHÚA ôi, con nương náu nơi Ngài,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.358Z",
      "updatedAt": "2024-09-07T14:10:18.358Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "bb31bba4-f444-4a08-b7d4-afe3d7775647",
      "number": 1,
      "content": "Xin đừng để con bị hổ thẹn.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.358Z",
      "updatedAt": "2024-09-07T14:10:18.358Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "a25d392f-e496-4be0-acaf-06f8e6e9dd8b",
      "number": 2,
      "content": "Xin giải nguy con và giải cứu con vì đức công chính của Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.368Z",
      "updatedAt": "2024-09-07T14:10:18.368Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "f861b783-b26e-44b8-9ffa-c0a3e4d2ddc7",
      "number": 2,
      "content": "Xin nghiêng tai nghe con và cứu con.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.368Z",
      "updatedAt": "2024-09-07T14:10:18.368Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "3ba85c10-c164-420b-97ca-063bd23ef638",
      "number": 3,
      "content": "Xin làm vầng đá cho con trú ẩn,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.370Z",
      "updatedAt": "2024-09-07T14:10:18.370Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "0dc36e68-900e-41ce-a4c1-9abf54aebf52",
      "number": 3,
      "content": "Nơi con sẽ đến đó luôn luôn;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.370Z",
      "updatedAt": "2024-09-07T14:10:18.370Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "4ef4effe-fc16-45da-94ba-90d157e87ccc",
      "number": 3,
      "content": "Xin ban lịnh giải cứu con,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.370Z",
      "updatedAt": "2024-09-07T14:10:18.370Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "6b93400a-661d-4e8a-b5fc-bddca5ba6dab",
      "number": 3,
      "content": "Vì Ngài là vầng đá và thành trì của con.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.370Z",
      "updatedAt": "2024-09-07T14:10:18.370Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "719a8a65-4290-413b-a9cc-86f792503b77",
      "number": 4,
      "content": "Đức Chúa Trời của con ôi, xin giải cứu con khỏi tay kẻ ác,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.373Z",
      "updatedAt": "2024-09-07T14:10:18.373Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "5ec51a37-0255-426e-9a5d-b42280823799",
      "number": 4,
      "content": "Tức khỏi tay kẻ bất chính và bạo tàn.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.373Z",
      "updatedAt": "2024-09-07T14:10:18.373Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "a7834f7a-71db-4487-b8c7-c13df1bd3422",
      "number": 5,
      "content": "Vì Ngài là nguồn hy vọng của con;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.376Z",
      "updatedAt": "2024-09-07T14:10:18.376Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "b385353b-f587-4330-aa4f-9ece3b1cf0ff",
      "number": 5,
      "content": "Lạy CHÚA Hằng Hữu, Đấng con tin cậy từ thuở ấu thơ,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.376Z",
      "updatedAt": "2024-09-07T14:10:18.376Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "41724d42-d30f-41c5-8418-cc61674ac67d",
      "number": 6,
      "content": "Con đã nhờ cậy Ngài từ lúc chào đời;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.379Z",
      "updatedAt": "2024-09-07T14:10:18.379Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "66faa939-4f1b-4704-996b-3ad51b2a31be",
      "number": 6,
      "content": "Ngài đã tạo thành con rồi đem con ra khỏi lòng mẹ con,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.379Z",
      "updatedAt": "2024-09-07T14:10:18.379Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "f22491c8-6c9f-424f-83e2-85c6c08ea9d7",
      "number": 6,
      "content": "Nên con sẽ ca ngợi Ngài mãi mãi.",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.379Z",
      "updatedAt": "2024-09-07T14:10:18.379Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "8eca4395-ce59-44b6-975e-f463c301ecbd",
      "number": 7,
      "content": "Con đã trở thành một điềm lạ cho nhiều người;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.381Z",
      "updatedAt": "2024-09-07T14:10:18.381Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "77be55e5-50b6-4ef2-999a-910ec384d2c9",
      "number": 7,
      "content": "Dù vậy Ngài vẫn là nơi nương náu vững chắc của con.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.381Z",
      "updatedAt": "2024-09-07T14:10:18.381Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "c510d9e8-b312-494f-af26-865ea501c08e",
      "number": 8,
      "content": "Miệng con tràn đầy những lời ca ngợi Ngài;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.383Z",
      "updatedAt": "2024-09-07T14:10:18.383Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "7d64efb7-6444-4b56-aa57-7cb2fb854b14",
      "number": 8,
      "content": "Hằng ngày con không ngớt ca tụng vinh hiển Ngài.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.383Z",
      "updatedAt": "2024-09-07T14:10:18.383Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "e727cb46-b822-4552-8e00-49a209c22cff",
      "number": 9,
      "content": "Xin đừng lìa khỏi con trong buổi già nua;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.386Z",
      "updatedAt": "2024-09-07T14:10:18.386Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "4102d818-8b04-43fc-a0b5-3d76b637cf03",
      "number": 9,
      "content": "Xin đừng từ bỏ con khi sức lực con không còn nữa.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.386Z",
      "updatedAt": "2024-09-07T14:10:18.386Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "8a7c9f02-7819-4fd3-9f01-c2616298ee83",
      "number": 10,
      "content": "Vì những kẻ thù ghét con đang nói những lời chống lại con;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.388Z",
      "updatedAt": "2024-09-07T14:10:18.388Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "d283fb20-d76f-486b-8a28-f621edfa62c3",
      "number": 10,
      "content": "Những kẻ rình rập mạng sống con đang bàn luận với nhau;",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.388Z",
      "updatedAt": "2024-09-07T14:10:18.388Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "3bfdea3c-5d33-409e-b8ba-53ca29c1bdc0",
      "number": 11,
      "content": "Chúng nói rằng, “Đức Chúa Trời đã lìa bỏ nó rồi;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.390Z",
      "updatedAt": "2024-09-07T14:10:18.390Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "67b4f19c-aecf-4ecc-9e16-33594bb43cd0",
      "number": 11,
      "content": "Hãy đuổi theo và bắt nó,",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.390Z",
      "updatedAt": "2024-09-07T14:10:18.390Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "ead74fe3-75a9-4951-bed0-ea6be1649298",
      "number": 11,
      "content": "Vì chẳng ai giải cứu nó đâu.”",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.390Z",
      "updatedAt": "2024-09-07T14:10:18.390Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "4b45181a-4ecb-4ba9-bff9-c231f7375a9c",
      "number": 12,
      "content": "Đức Chúa Trời ôi, xin đừng xa con;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.395Z",
      "updatedAt": "2024-09-07T14:10:18.395Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "2bd3a6d2-0f16-4d1e-8e99-7a106c2b7963",
      "number": 12,
      "content": "Đức Chúa Trời của con ôi, xin mau đến giúp đỡ con!",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.395Z",
      "updatedAt": "2024-09-07T14:10:18.395Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "584402bc-748e-442d-a976-8f4170acc948",
      "number": 13,
      "content": "Nguyện những kẻ thù của con bị hủy diệt trong sỉ nhục.",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.397Z",
      "updatedAt": "2024-09-07T14:10:18.397Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "f0372b84-f2ac-490f-9a2c-82ec3ebb6518",
      "number": 13,
      "content": "Nguyện những kẻ tìm cách hại con",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.397Z",
      "updatedAt": "2024-09-07T14:10:18.397Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "083cba7a-4015-4b73-bbe6-61220495b131",
      "number": 13,
      "content": "Bị bao phủ bằng nhục nhã và nhuốc nhơ.",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.397Z",
      "updatedAt": "2024-09-07T14:10:18.397Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "0b166c84-de69-4b26-bd4e-8443bbad8365",
      "number": 14,
      "content": "Nhưng về phần con, con sẽ tiếp tục hy vọng;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.399Z",
      "updatedAt": "2024-09-07T14:10:18.399Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "72cf2e14-2a6c-4ddf-94ee-bc1bc58f40ed",
      "number": 14,
      "content": "Con sẽ ca ngợi Ngài càng ngày càng thêm.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.399Z",
      "updatedAt": "2024-09-07T14:10:18.399Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "8bffef5d-fb53-4dbd-b083-8bd6d1ccb2ad",
      "number": 15,
      "content": "Miệng con sẽ nói ra đức công chính của Ngài;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.402Z",
      "updatedAt": "2024-09-07T14:10:18.402Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "be6d31de-ba9c-47ce-95b5-3bc547478a0c",
      "number": 15,
      "content": "Con sẽ thuật lại ơn cứu rỗi của Ngài suốt ngày,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.402Z",
      "updatedAt": "2024-09-07T14:10:18.402Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "08306985-9146-4c37-80c8-53f5dee5e3ab",
      "number": 15,
      "content": "Vì con không biết sẽ phải nói bao nhiêu cho đủ.",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.402Z",
      "updatedAt": "2024-09-07T14:10:18.402Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "3b152db7-c688-43f0-b343-a712d36830b0",
      "number": 16,
      "content": "Con sẽ đến trong quyền năng của CHÚA Hằng Hữu;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.404Z",
      "updatedAt": "2024-09-07T14:10:18.404Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "7d6e388a-a802-47ee-9aa5-190e310c9427",
      "number": 16,
      "content": "Con sẽ nói ra đức công chính của Ngài, chỉ của Ngài mà thôi.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.404Z",
      "updatedAt": "2024-09-07T14:10:18.404Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "e6282e6f-1b7f-4b1d-809a-e248ea94a625",
      "number": 17,
      "content": "Đức Chúa Trời ôi, Ngài đã dạy con từ thuở còn thơ,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.406Z",
      "updatedAt": "2024-09-07T14:10:18.406Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "1357a737-5a00-41c8-b07a-68efd3155e79",
      "number": 17,
      "content": "Đến bây giờ con vẫn còn rao báo những việc diệu kỳ của Ngài.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.406Z",
      "updatedAt": "2024-09-07T14:10:18.406Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "20229dfc-b2c8-4f17-94e5-1a2ffc1086cc",
      "number": 18,
      "content": "Đức Chúa Trời ôi, giờ đây con già yếu và tóc đã bạc rồi,",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.408Z",
      "updatedAt": "2024-09-07T14:10:18.408Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "b57ab801-ce5e-4a2d-91ad-8d3f99b8125b",
      "number": 18,
      "content": "Xin Ngài đừng lìa bỏ con,",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.408Z",
      "updatedAt": "2024-09-07T14:10:18.408Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "8b3b35cf-1441-4eb7-a114-62012da7198c",
      "number": 18,
      "content": "Cho đến khi con truyền xong cho thế hệ kế tiếp những việc quyền năng của Ngài,",
      "order": 2,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.408Z",
      "updatedAt": "2024-09-07T14:10:18.408Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "b37177a5-8679-4c26-9265-063cc43ad375",
      "number": 18,
      "content": "Và nói cho thế hệ đến sau biết quyền phép lạ lùng của Ngài.",
      "order": 3,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.408Z",
      "updatedAt": "2024-09-07T14:10:18.408Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "809b9aa7-f448-4918-8a90-8fc24fe9856e",
      "number": 19,
      "content": "Vì Đức Chúa Trời ôi, đức công chính Ngài",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.411Z",
      "updatedAt": "2024-09-07T14:10:18.411Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "3ecdf284-c4b9-4ede-a972-d1d8518c3a7d",
      "number": 19,
      "content": "Cao đến tận trời;",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.411Z",
      "updatedAt": "2024-09-07T14:10:18.411Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "b316de1f-ab67-4812-8e3b-5b0717b46252",
      "number": 19,
      "content": "Ngài đã làm những việc thật lớn lao vĩ đại.",
      "order": 2,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.411Z",
      "updatedAt": "2024-09-07T14:10:18.411Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "a1bd7a7f-397e-4cd2-a699-d344d035e9c1",
      "number": 19,
      "content": "Đức Chúa Trời ôi, nào ai giống như Ngài?",
      "order": 3,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.411Z",
      "updatedAt": "2024-09-07T14:10:18.411Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "38d9fc6b-1321-4a0f-80a1-692ca8c81953",
      "number": 20,
      "content": "Ngài đã cho con kinh nghiệm những gian khổ và buồn đau;",
      "order": 0,
      "parNumber": 49,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.413Z",
      "updatedAt": "2024-09-07T14:10:18.413Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "769a8b4e-2b19-460b-ab08-3a030256b31e",
      "number": 20,
      "content": "Sau đó Ngài đã làm cho con được sống lại.",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.413Z",
      "updatedAt": "2024-09-07T14:10:18.413Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "b3654793-fe1c-40b2-a4b9-cf6299190a79",
      "number": 20,
      "content": "Ngài đã đem con lên",
      "order": 2,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.413Z",
      "updatedAt": "2024-09-07T14:10:18.413Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "9abf9057-2f70-4ee0-8d5f-8d8ca65e6db9",
      "number": 20,
      "content": "Từ vực sâu của lòng đất.",
      "order": 3,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.413Z",
      "updatedAt": "2024-09-07T14:10:18.413Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "4542598d-b321-45a3-b129-182cd7b49a2f",
      "number": 21,
      "content": "Xin Ngài gia thêm sự tôn trọng cho con;",
      "order": 0,
      "parNumber": 53,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.415Z",
      "updatedAt": "2024-09-07T14:10:18.415Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "7556a225-47a2-4233-932d-00add734fc80",
      "number": 21,
      "content": "Và quay lại để an ủi con.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.415Z",
      "updatedAt": "2024-09-07T14:10:18.415Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "a23011d5-e578-42e6-9b5f-08607c7317c1",
      "number": 22,
      "content": "Đức Chúa Trời của con ôi,",
      "order": 0,
      "parNumber": 55,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.418Z",
      "updatedAt": "2024-09-07T14:10:18.418Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "a19a8b47-087e-4288-b876-d9c3afb19e95",
      "number": 22,
      "content": "Con sẽ dùng đàn lia ca ngợi đức thành tín Ngài;",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.418Z",
      "updatedAt": "2024-09-07T14:10:18.418Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "757831a3-2fe4-437d-96e5-a6494ef2852e",
      "number": 22,
      "content": "Lạy Chúa, Đấng Thánh của I-sơ-ra-ên,",
      "order": 2,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.418Z",
      "updatedAt": "2024-09-07T14:10:18.418Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "139ed669-14eb-4b2c-9eb9-366782e06b99",
      "number": 22,
      "content": "Con sẽ hòa theo hạc cầm ca tụng Ngài.",
      "order": 3,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.418Z",
      "updatedAt": "2024-09-07T14:10:18.418Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "9455a43f-f30b-4af7-b3bc-ed1b99a83d6c",
      "number": 23,
      "content": "Môi con sẽ reo mừng",
      "order": 0,
      "parNumber": 59,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.420Z",
      "updatedAt": "2024-09-07T14:10:18.420Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "cb39b311-13a2-4b14-bf3a-f42f977bfd36",
      "number": 23,
      "content": "Khi con ca hát tôn ngợi Ngài,",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.420Z",
      "updatedAt": "2024-09-07T14:10:18.420Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "74f0b6ce-2594-4729-b9f1-a4969d05ef11",
      "number": 23,
      "content": "Và linh hồn con đã được Ngài cứu chuộc cũng vậy.",
      "order": 2,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.420Z",
      "updatedAt": "2024-09-07T14:10:18.420Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "e5811076-e6cd-4fbb-8b98-ab15a62b589b",
      "number": 24,
      "content": "Lưỡi con sẽ nói ra đức công chính Ngài suốt ngày,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.422Z",
      "updatedAt": "2024-09-07T14:10:18.422Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "25bdf86c-0b2d-4010-818e-893aa432917c",
      "number": 24,
      "content": "Vì những kẻ tìm cách hại con",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.422Z",
      "updatedAt": "2024-09-07T14:10:18.422Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    },
    {
      "id": "bcf410e7-8cba-422a-a340-6d55c58ecdfa",
      "number": 24,
      "content": "Đã bị làm cho xấu hổ và sỉ nhục.",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:18.422Z",
      "updatedAt": "2024-09-07T14:10:18.422Z",
      "chapterId": "e09abe58-01c2-4959-a52a-62186a316440"
    }
  ],
