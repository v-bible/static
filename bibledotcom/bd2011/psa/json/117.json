  "verses": [
    {
      "id": "ca2576ff-a5a7-49d5-b5b0-abd4a3a43d7f",
      "number": 1,
      "content": "Hỡi các nước, hãy ca ngợi CHÚA;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:19:55.753Z",
      "updatedAt": "2024-09-07T14:19:55.753Z",
      "chapterId": "19590c20-abb1-4cc1-835c-7ba1bea03ce3"
    },
    {
      "id": "821b9688-6037-4127-8eaa-3bc950a9740b",
      "number": 1,
      "content": "Hỡi muôn dân, khá tôn ngợi Ngài!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:19:55.753Z",
      "updatedAt": "2024-09-07T14:19:55.753Z",
      "chapterId": "19590c20-abb1-4cc1-835c-7ba1bea03ce3"
    },
    {
      "id": "242151de-1981-483b-a467-c9083a8af8d1",
      "number": 2,
      "content": "Vì tình thương của Ngài đối với chúng ta rất lớn,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:19:55.763Z",
      "updatedAt": "2024-09-07T14:19:55.763Z",
      "chapterId": "19590c20-abb1-4cc1-835c-7ba1bea03ce3"
    },
    {
      "id": "5fa0e550-47c3-4d48-b13b-12d03636b5d2",
      "number": 2,
      "content": "Và đức thành tín của CHÚA hằng còn đến đời đời.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:19:55.763Z",
      "updatedAt": "2024-09-07T14:19:55.763Z",
      "chapterId": "19590c20-abb1-4cc1-835c-7ba1bea03ce3"
    },
    {
      "id": "9ee81fde-ffef-4bd8-80a3-503de90de8f7",
      "number": 2,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:19:55.763Z",
      "updatedAt": "2024-09-07T14:19:55.763Z",
      "chapterId": "19590c20-abb1-4cc1-835c-7ba1bea03ce3"
    }
  ],
