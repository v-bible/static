{
  "chapter": {
    "id": "1892aec4-520a-4dc9-a810-cbc55048b42b",
    "number": 53,
    "ref": "https://www.bible.com/bible/19/PSA.53.BD2011",
    "createdAt": "2024-09-07T11:51:11.047Z",
    "updatedAt": "2024-09-07T11:51:11.047Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "4106882d-c69d-4d06-85ed-2228ad15c1ff",
      "number": 1,
      "content": "Kẻ dại nói trong lòng rằng, “Không có Đức Chúa Trời!”",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.865Z",
      "updatedAt": "2024-09-07T14:05:11.865Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "2ef00719-274d-4e94-9868-fca6cb7f8db4",
      "number": 1,
      "content": "Chúng nó đều bại hoại; chúng nó phạm những tội lỗi đáng ghê tởm;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.865Z",
      "updatedAt": "2024-09-07T14:05:11.865Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "b33e77e4-152f-45f3-9ec8-7164d4a23e7f",
      "number": 1,
      "content": "Chẳng ai làm điều thiện.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.865Z",
      "updatedAt": "2024-09-07T14:05:11.865Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "1173232d-e16d-407a-9023-8cd2caa9e91d",
      "number": 2,
      "content": "Đức Chúa Trời từ trời",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.875Z",
      "updatedAt": "2024-09-07T14:05:11.875Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "c639019e-db43-4f45-b3b9-5c91a7444378",
      "number": 2,
      "content": "Nhìn xuống con cái loài người,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.875Z",
      "updatedAt": "2024-09-07T14:05:11.875Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "89dfcbf9-8ce9-4277-9985-6aa98133e7b6",
      "number": 2,
      "content": "Xem có ai là người trí thức,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.875Z",
      "updatedAt": "2024-09-07T14:05:11.875Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "cf249ae8-2857-417b-bc51-9f03dc15ee30",
      "number": 2,
      "content": "Biết tìm kiếm Đức Chúa Trời chăng.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.875Z",
      "updatedAt": "2024-09-07T14:05:11.875Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "29b59db3-401c-4a05-862d-8e280e5d8849",
      "number": 3,
      "content": "Họ thảy đều bại hoại;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.878Z",
      "updatedAt": "2024-09-07T14:05:11.878Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "d0aebcaa-9d9e-48bf-b882-d955eaa55d4d",
      "number": 3,
      "content": "Họ cùng nhau trở nên ô uế;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.878Z",
      "updatedAt": "2024-09-07T14:05:11.878Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "f0ac37db-a10c-481d-bc68-dc176863299c",
      "number": 3,
      "content": "Chẳng có người nào làm điều thiện,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.878Z",
      "updatedAt": "2024-09-07T14:05:11.878Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "889b27b0-7443-4504-9894-27ae6e7d0ca6",
      "number": 3,
      "content": "Dẫu một người cũng không.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.878Z",
      "updatedAt": "2024-09-07T14:05:11.878Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "7c354a8f-8797-4ad0-9daf-bb06debff79e",
      "number": 4,
      "content": "Tại sao những kẻ làm ác chẳng hiểu biết?",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.881Z",
      "updatedAt": "2024-09-07T14:05:11.881Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "efbf9d04-6616-4b30-a81e-c6107f5f79d7",
      "number": 4,
      "content": "Tại sao chúng cứ ăn nuốt dân tôi như thể ăn bánh?",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.881Z",
      "updatedAt": "2024-09-07T14:05:11.881Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "2dcdce03-167b-438f-8b3b-2c3f85495fb9",
      "number": 4,
      "content": "Tại sao chúng chẳng kêu cầu Đức Chúa Trời?",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.881Z",
      "updatedAt": "2024-09-07T14:05:11.881Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "16210e8c-e3d0-49d8-854f-156c4e8b43eb",
      "number": 5,
      "content": "Chúng sẽ rất sợ hãi dầu chẳng có gì đáng sợ,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.884Z",
      "updatedAt": "2024-09-07T14:05:11.884Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "efd95d86-5506-499a-a20c-c66e34b31439",
      "number": 5,
      "content": "Vì Đức Chúa Trời sẽ rải xương những kẻ vây hãm bạn;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.884Z",
      "updatedAt": "2024-09-07T14:05:11.884Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "b4bc96d7-cad0-44a7-b4d9-c61641efc3fe",
      "number": 5,
      "content": "Bạn làm cho chúng phải hổ thẹn,",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.884Z",
      "updatedAt": "2024-09-07T14:05:11.884Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "e2660ea7-edde-457e-a438-a2228c58cbe5",
      "number": 5,
      "content": "Vì Đức Chúa Trời đã loại bỏ chúng.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.884Z",
      "updatedAt": "2024-09-07T14:05:11.884Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "56f6efe6-92d2-42ff-8654-97c11262aba9",
      "number": 6,
      "content": "Kìa, ơn cứu rỗi của I-sơ-ra-ên đến từ Si-ôn!",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.886Z",
      "updatedAt": "2024-09-07T14:05:11.886Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "0a34e814-437e-4a22-a2c1-9b1325ac555b",
      "number": 6,
      "content": "Khi Đức Chúa Trời tái lập vận mạng của con dân Ngài,",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.886Z",
      "updatedAt": "2024-09-07T14:05:11.886Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    },
    {
      "id": "bd9c150b-bc7c-4347-afe9-41ca09f87247",
      "number": 6,
      "content": "Gia-cốp sẽ mừng rỡ, I-sơ-ra-ên sẽ vui mừng.",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:11.886Z",
      "updatedAt": "2024-09-07T14:05:11.886Z",
      "chapterId": "1892aec4-520a-4dc9-a810-cbc55048b42b"
    }
  ],
