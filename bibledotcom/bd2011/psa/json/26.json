{
  "chapter": {
    "id": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546",
    "number": 26,
    "ref": "https://www.bible.com/bible/19/PSA.26.BD2011",
    "createdAt": "2024-09-07T11:51:10.827Z",
    "updatedAt": "2024-09-07T11:51:10.827Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "30f45618-434c-4d55-88fc-bf06ffb8b7ce",
      "number": 1,
      "content": "CHÚA ôi, xin minh oan cho con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.200Z",
      "updatedAt": "2024-09-07T14:01:05.200Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "3da78570-9b88-43ec-8bbc-cc7a26b5311c",
      "number": 1,
      "content": "Vì con đã bước đi cách thanh liêm,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.200Z",
      "updatedAt": "2024-09-07T14:01:05.200Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "f69546c6-9baa-420f-9216-b192dd947392",
      "number": 1,
      "content": "Vì con đã tin cậy CHÚA cách không nao núng.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.200Z",
      "updatedAt": "2024-09-07T14:01:05.200Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "d2310580-1edc-4918-88bb-c296e0262e6f",
      "number": 2,
      "content": "CHÚA ôi, xin tra xét con và thử nghiệm con,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.204Z",
      "updatedAt": "2024-09-07T14:01:05.204Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "130aa958-f360-4615-a8de-1306a0bc9fd4",
      "number": 2,
      "content": "Xin dò xem lòng dạ con và tâm trí con;",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.204Z",
      "updatedAt": "2024-09-07T14:01:05.204Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "65c73e75-660b-4202-8418-90e0af2df122",
      "number": 3,
      "content": "Vì tình thương của Ngài hằng ở trước mặt con,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.207Z",
      "updatedAt": "2024-09-07T14:01:05.207Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "351f481d-be2c-4dba-98cf-89263e668b32",
      "number": 3,
      "content": "Và con đã bước đi trong sự thành tín của Ngài.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.207Z",
      "updatedAt": "2024-09-07T14:01:05.207Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "6d9499e2-9785-45c6-85b9-8092a23c802a",
      "number": 4,
      "content": "Con không ngồi chung với những kẻ dối gian;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.209Z",
      "updatedAt": "2024-09-07T14:01:05.209Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "e64b29ef-14a1-4bd6-837d-625cd8737b8b",
      "number": 4,
      "content": "Con cũng không kết giao với những kẻ đạo đức giả.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.209Z",
      "updatedAt": "2024-09-07T14:01:05.209Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "3e69a9c3-722c-42bc-8abf-d745c198c5f4",
      "number": 5,
      "content": "Con ghét sự hội hiệp của những kẻ bất lương;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.211Z",
      "updatedAt": "2024-09-07T14:01:05.211Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "6fdb071f-c076-4df8-bb6f-afa4c5ae2173",
      "number": 5,
      "content": "Con sẽ không ngồi chung với những kẻ gian ác.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.211Z",
      "updatedAt": "2024-09-07T14:01:05.211Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "37af8e40-8500-499d-ae84-d041c6a65015",
      "number": 6,
      "content": "Con rửa tay để xác định con vô tội,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.213Z",
      "updatedAt": "2024-09-07T14:01:05.213Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "1542c181-ee80-4990-addb-b73f9ecb6597",
      "number": 6,
      "content": "Và đi quanh bàn thờ Ngài, CHÚA ôi,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.213Z",
      "updatedAt": "2024-09-07T14:01:05.213Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "4d127f60-ad3a-4859-897d-210c0ced042d",
      "number": 7,
      "content": "Để con có thể lớn tiếng tạ ơn Ngài,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.216Z",
      "updatedAt": "2024-09-07T14:01:05.216Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "b0b4393d-baa1-444d-8e39-aca2a080004f",
      "number": 7,
      "content": "Và cao rao những việc kỳ diệu của Ngài.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.216Z",
      "updatedAt": "2024-09-07T14:01:05.216Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "8ce2a5d9-0dce-463c-8e45-bbc8168db2e9",
      "number": 8,
      "content": "CHÚA ôi, con yêu mến nhà Ngài, nơi Ngài ngự,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.219Z",
      "updatedAt": "2024-09-07T14:01:05.219Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "ba2212d4-ebc5-4f3a-b942-ecbc11aaa331",
      "number": 8,
      "content": "Nơi vinh hiển Ngài hiện diện.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.219Z",
      "updatedAt": "2024-09-07T14:01:05.219Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "850686ff-2b23-455e-967b-535cd568cd7b",
      "number": 9,
      "content": "Xin đừng cất linh hồn con chung với những kẻ tội lỗi;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.221Z",
      "updatedAt": "2024-09-07T14:01:05.221Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "5e866e42-ff6a-4775-839b-a69a65d82253",
      "number": 9,
      "content": "Hoặc lấy mạng con cùng với những kẻ khát máu,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.221Z",
      "updatedAt": "2024-09-07T14:01:05.221Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "652f44f4-ac8e-4eb6-bc81-4a8c556ecfb4",
      "number": 10,
      "content": "Những kẻ mà trong tay đã có âm mưu hiểm độc,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.224Z",
      "updatedAt": "2024-09-07T14:01:05.224Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "f5441910-1cf5-4ebc-9870-1266974257c8",
      "number": 10,
      "content": "Và trong tay phải chúng nắm đầy của hối lộ.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.224Z",
      "updatedAt": "2024-09-07T14:01:05.224Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "62a53c86-ae8b-4f1a-b341-57423bcfde24",
      "number": 11,
      "content": "Nhưng con, con sẽ bước đi trong sự thanh liêm;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.226Z",
      "updatedAt": "2024-09-07T14:01:05.226Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "b00c8f44-c1b1-4b28-8eae-241dcad3c299",
      "number": 11,
      "content": "Xin giải cứu con và tỏ lòng khoan dung đối với con.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.226Z",
      "updatedAt": "2024-09-07T14:01:05.226Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "ff77fead-236b-43c0-b91c-157cd92263f6",
      "number": 12,
      "content": "Nguyện chân con được đứng vững trên nơi bằng phẳng;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.228Z",
      "updatedAt": "2024-09-07T14:01:05.228Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    },
    {
      "id": "9b5e406a-c85a-456e-89b8-fc7b568b01bd",
      "number": 12,
      "content": "Để ở giữa các đại hội, con sẽ chúc tụng CHÚA.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:01:05.228Z",
      "updatedAt": "2024-09-07T14:01:05.228Z",
      "chapterId": "aeec4a5c-572e-4f2a-bc4c-37c8ab36e546"
    }
  ],
