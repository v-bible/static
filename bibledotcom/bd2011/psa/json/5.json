{
  "chapter": {
    "id": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc",
    "number": 5,
    "ref": "https://www.bible.com/bible/19/PSA.5.BD2011",
    "createdAt": "2024-09-07T11:51:10.650Z",
    "updatedAt": "2024-09-07T11:51:10.650Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "646c7906-82f9-403b-92a7-e689b37d7d54",
      "number": 1,
      "content": "CHÚA ôi, xin lắng nghe những lời con cầu xin;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.023Z",
      "updatedAt": "2024-09-07T13:54:47.023Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "f29add9b-73d7-4bbf-8210-1091c7ef6bfa",
      "number": 1,
      "content": "Xin đoái đến những lời con than thở.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.023Z",
      "updatedAt": "2024-09-07T13:54:47.023Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "ac411e63-f181-4e64-9093-1b5182d21a31",
      "number": 2,
      "content": "Ôi Vua của con và Đức Chúa Trời của con,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.025Z",
      "updatedAt": "2024-09-07T13:54:47.025Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "8cd8bca9-f1e7-4292-8f2e-e9345493cf5b",
      "number": 2,
      "content": "Xin nghe tiếng con kêu cầu,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.025Z",
      "updatedAt": "2024-09-07T13:54:47.025Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "e501e58e-4870-4be8-bd0c-11e81664a2d2",
      "number": 2,
      "content": "Vì con cầu nguyện cùng Ngài.",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.025Z",
      "updatedAt": "2024-09-07T13:54:47.025Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "9e1bca5c-0ff4-42a1-b733-97a8cb0db0cf",
      "number": 3,
      "content": "CHÚA ôi, sáng sớm xin Ngài nghe tiếng con;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.028Z",
      "updatedAt": "2024-09-07T13:54:47.028Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "8a98d1cb-de29-4572-b380-4ee693e99750",
      "number": 3,
      "content": "Hừng sáng con trình duyên cớ con lên Ngài, rồi trông đợi.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.028Z",
      "updatedAt": "2024-09-07T13:54:47.028Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "4a6bf5fa-d687-44b7-9b7c-63342206f8ed",
      "number": 4,
      "content": "Vì Ngài chẳng phải là Đức Chúa Trời ưa thích điều ác;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.031Z",
      "updatedAt": "2024-09-07T13:54:47.031Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "1d83b0c9-124d-4564-b953-ec3518ce8c7c",
      "number": 4,
      "content": "Kẻ ác chẳng sống nổi trước mặt Ngài.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.031Z",
      "updatedAt": "2024-09-07T13:54:47.031Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "d64452e6-b546-4afc-962b-9a5cd9d6d8c3",
      "number": 5,
      "content": "Kẻ khoác lác sẽ chẳng đứng nổi trước thánh nhan Ngài;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.036Z",
      "updatedAt": "2024-09-07T13:54:47.036Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "c5211fd6-ae54-4ceb-8d83-df91e38aff12",
      "number": 5,
      "content": "Ngài ghét tất cả những kẻ làm ác.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.036Z",
      "updatedAt": "2024-09-07T13:54:47.036Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "3d58beb9-276c-4ff8-b856-649f91dc6105",
      "number": 6,
      "content": "Ngài tiêu diệt những kẻ nói dối;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.040Z",
      "updatedAt": "2024-09-07T13:54:47.040Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "b0b3cc9a-1365-4fc2-bf45-4fa0cd9fe907",
      "number": 6,
      "content": "CHÚA gớm ghiếc những kẻ khát máu và lừa gạt.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.040Z",
      "updatedAt": "2024-09-07T13:54:47.040Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "d58f4514-3fea-4946-86b7-7fa1c41500f0",
      "number": 7,
      "content": "Nhưng con, nhờ lòng thương xót dồi dào của Ngài",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.044Z",
      "updatedAt": "2024-09-07T13:54:47.044Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "5517f362-0edd-43ca-ad82-0e1285dbe9eb",
      "number": 7,
      "content": "Con sẽ vào nhà Ngài;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.044Z",
      "updatedAt": "2024-09-07T13:54:47.044Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "39464864-9296-4115-8e4c-3902457b08a4",
      "number": 7,
      "content": "Con sẽ hướng về đền thánh Ngài",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.044Z",
      "updatedAt": "2024-09-07T13:54:47.044Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "9cb111d8-efb9-4257-9a94-a969d5f9f9ea",
      "number": 7,
      "content": "Mà thờ phượng Ngài với lòng kính sợ.",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.044Z",
      "updatedAt": "2024-09-07T13:54:47.044Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "0360b077-ee1d-4bb3-b72a-78fbaa02f382",
      "number": 8,
      "content": "CHÚA ôi, vì cớ những kẻ thù con,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.048Z",
      "updatedAt": "2024-09-07T13:54:47.048Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "5b506a14-f506-4225-8582-7a2db9d5fb5a",
      "number": 8,
      "content": "Xin dẫn dắt con đi trong đường công chính của Ngài;",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.048Z",
      "updatedAt": "2024-09-07T13:54:47.048Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "943804e3-797e-40f9-ae8a-860260298487",
      "number": 8,
      "content": "Xin làm cho đường lối Ngài bằng phẳng trước mặt con.",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.048Z",
      "updatedAt": "2024-09-07T13:54:47.048Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "f83c588c-2030-477e-8015-bdd0ecadbae0",
      "number": 9,
      "content": "Thật chẳng có gì đáng tin cậy nơi miệng chúng;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.052Z",
      "updatedAt": "2024-09-07T13:54:47.052Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "03c6bfd8-bf3c-4cfd-8657-bb78cb38ada3",
      "number": 9,
      "content": "Tâm địa chúng đầy ý đồ hủy diệt;",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.052Z",
      "updatedAt": "2024-09-07T13:54:47.052Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "086f6529-be6c-4e1a-8801-c5babd262a77",
      "number": 9,
      "content": "Cổ họng chúng khác nào phần mộ mở ra;",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.052Z",
      "updatedAt": "2024-09-07T13:54:47.052Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "b8311d0e-ba6d-431e-90c6-cead6719119f",
      "number": 9,
      "content": "Lưỡi chúng nói toàn những lời dua nịnh.",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.052Z",
      "updatedAt": "2024-09-07T13:54:47.052Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "2c86d9d9-d7b1-453f-b288-3a560d452c7e",
      "number": 10,
      "content": "Đức Chúa Trời ôi, xin bắt chúng phải mang tội;",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.057Z",
      "updatedAt": "2024-09-07T13:54:47.057Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "8c756049-6e6e-43fc-9c7f-4a0237141a6c",
      "number": 10,
      "content": "Xin để chúng sụp ngã trên mưu chước chúng;",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.057Z",
      "updatedAt": "2024-09-07T13:54:47.057Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "6e2afbe6-e322-4045-a5d6-9cd235b30fe4",
      "number": 10,
      "content": "Xin đuổi chúng đi vì tội chúng quá nhiều;",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.057Z",
      "updatedAt": "2024-09-07T13:54:47.057Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "75b50c19-8f24-4356-ac2d-f2d71ee0818b",
      "number": 10,
      "content": "Vì chúng đã dám nổi loạn chống lại Ngài.",
      "order": 3,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.057Z",
      "updatedAt": "2024-09-07T13:54:47.057Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "8e421b65-d8b9-4b77-a60e-44d8f469bce9",
      "number": 11,
      "content": "Nhưng xin cho những ai để lòng tin cậy Ngài được vui mừng;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.061Z",
      "updatedAt": "2024-09-07T13:54:47.061Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "7b5c9cf5-1b3f-4faa-99ff-b5fc1dbc7ec3",
      "number": 11,
      "content": "Họ sẽ hoan ca mãi mãi,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.061Z",
      "updatedAt": "2024-09-07T13:54:47.061Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "c1cb8657-0cd2-4930-a0dd-433ecf137700",
      "number": 11,
      "content": "Vì Ngài bảo vệ những người ấy;",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.061Z",
      "updatedAt": "2024-09-07T13:54:47.061Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "d4743cbf-137e-4bce-a719-29f62190bbf8",
      "number": 11,
      "content": "Để ai yêu mến danh Ngài sẽ được vui thỏa trong Ngài.",
      "order": 3,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.061Z",
      "updatedAt": "2024-09-07T13:54:47.061Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "7311e175-348e-4c23-a651-77a77551f73b",
      "number": 12,
      "content": "Vì CHÚA ôi, chính Ngài sẽ ban phước cho người ngay lành;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.065Z",
      "updatedAt": "2024-09-07T13:54:47.065Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    },
    {
      "id": "c68d0520-98d5-4d21-8e30-e09c75189047",
      "number": 12,
      "content": "Ngài sẽ lấy ơn huệ Ngài che chở người ấy như thuẫn khiên.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:54:47.065Z",
      "updatedAt": "2024-09-07T13:54:47.065Z",
      "chapterId": "8c36d6df-8ee9-4653-a88a-47cd39ea12dc"
    }
  ],
