{
  "chapter": {
    "id": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4",
    "number": 105,
    "ref": "https://www.bible.com/bible/19/PSA.105.BD2011",
    "createdAt": "2024-09-07T11:51:11.456Z",
    "updatedAt": "2024-09-07T11:51:11.456Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "6b52e0e0-e3f1-4ddc-9719-155e03a26840",
      "number": 1,
      "content": "Hãy cảm tạ CHÚA, hãy cầu khẩn danh Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.518Z",
      "updatedAt": "2024-09-07T14:17:15.518Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "643e6cf4-c460-44a4-95bc-541580c25122",
      "number": 1,
      "content": "Hãy rao truyền giữa các dân các công việc Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.518Z",
      "updatedAt": "2024-09-07T14:17:15.518Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "c317cb70-ba21-4512-9b17-c3b414a274a2",
      "number": 2,
      "content": "Hãy ca ngợi Ngài, hãy ca hát chúc tụng Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.528Z",
      "updatedAt": "2024-09-07T14:17:15.528Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "62ae23dd-e4bf-4a9d-b9b4-6c0cbd37bd77",
      "number": 2,
      "content": "Hãy kể ra tất cả việc lạ lùng của Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.528Z",
      "updatedAt": "2024-09-07T14:17:15.528Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "c1a884ac-8db8-4542-b330-f063b5466cff",
      "number": 3,
      "content": "Vinh hiển thay cho ai được ở trong danh thánh Ngài;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.531Z",
      "updatedAt": "2024-09-07T14:17:15.531Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "f9702ede-4bfe-498f-b89a-814a829141d9",
      "number": 3,
      "content": "Nguyện lòng những người tìm kiếm CHÚA được vui mừng.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.531Z",
      "updatedAt": "2024-09-07T14:17:15.531Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "f2d3cbed-9209-48cb-b5da-36eecfad1d2a",
      "number": 4,
      "content": "Hãy tìm kiếm CHÚA và quyền năng Ngài;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.534Z",
      "updatedAt": "2024-09-07T14:17:15.534Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "16efd946-0f47-4aab-93ab-e76e0f616da8",
      "number": 4,
      "content": "Hãy tìm kiếm mặt Ngài luôn luôn.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.534Z",
      "updatedAt": "2024-09-07T14:17:15.534Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "52246d14-aff8-424b-b2ff-d958a4827f64",
      "number": 5,
      "content": "Hãy nhớ mãi những việc lạ lùng Ngài làm,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.536Z",
      "updatedAt": "2024-09-07T14:17:15.536Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "1cfc13f5-e20e-4da0-9a5b-e38b6b37d28f",
      "number": 5,
      "content": "Tức những việc diệu kỳ và những phán quyết miệng Ngài đã phán,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.536Z",
      "updatedAt": "2024-09-07T14:17:15.536Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "63c69586-850b-481e-b9b4-b2450ca48af5",
      "number": 6,
      "content": "Hỡi dòng dõi Áp-ra-ham, tôi tớ Ngài,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.539Z",
      "updatedAt": "2024-09-07T14:17:15.539Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "33a9b3a4-1f31-4819-ad21-f903360e73ae",
      "number": 6,
      "content": "Hỡi con cháu Gia-cốp, những người Ngài chọn.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.539Z",
      "updatedAt": "2024-09-07T14:17:15.539Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "1a0c155c-d3ae-4b06-9b90-b0286f498ac8",
      "number": 7,
      "content": "Ngài là CHÚA, Đức Chúa Trời chúng ta;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.541Z",
      "updatedAt": "2024-09-07T14:17:15.541Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "2c60c666-e9dd-4238-aeb3-00ca4e16b99a",
      "number": 7,
      "content": "Các phán quyết của Ngài là cho cả địa cầu.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.541Z",
      "updatedAt": "2024-09-07T14:17:15.541Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "04a6df6a-748f-46b4-a662-245542713b14",
      "number": 8,
      "content": "Ngài ghi nhớ giao ước Ngài mãi mãi;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.543Z",
      "updatedAt": "2024-09-07T14:17:15.543Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "bbb5b500-d447-48d9-ba36-e71ce4c694c2",
      "number": 8,
      "content": "Lời Ngài truyền có hiệu lực đến ngàn thế hệ về sau;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.543Z",
      "updatedAt": "2024-09-07T14:17:15.543Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "3ed0922b-9b94-4c1f-bf83-8a2eac41b820",
      "number": 9,
      "content": "Ấy là giao ước Ngài đã lập với Áp-ra-ham,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.545Z",
      "updatedAt": "2024-09-07T14:17:15.545Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "b818953e-0379-4d12-bb5c-7ae355714973",
      "number": 9,
      "content": "Và lời thề Ngài đã thề cùng I-sác.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.545Z",
      "updatedAt": "2024-09-07T14:17:15.545Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "af946c33-31bb-4e1b-95d9-2fd80b8c8909",
      "number": 10,
      "content": "Sau đó Ngài đã xác nhận với Gia-cốp như một đạo luật,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.547Z",
      "updatedAt": "2024-09-07T14:17:15.547Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "884250da-7852-494b-99db-c6ddd87f3c58",
      "number": 10,
      "content": "Với I-sơ-ra-ên như một giao ước đời đời,",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.547Z",
      "updatedAt": "2024-09-07T14:17:15.547Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "319c4294-c416-42d8-8177-56162465789c",
      "number": 11,
      "content": "Rằng, “Ta sẽ ban xứ Ca-na-an cho các ngươi",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.549Z",
      "updatedAt": "2024-09-07T14:17:15.549Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "dba3f780-193a-4149-8627-338efccd0272",
      "number": 11,
      "content": "Để làm sản nghiệp,”",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.549Z",
      "updatedAt": "2024-09-07T14:17:15.549Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "7110268f-482c-4bc3-a41e-43e8ad42189a",
      "number": 12,
      "content": "Trong khi họ chỉ là một số ít người,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.552Z",
      "updatedAt": "2024-09-07T14:17:15.552Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "36a8026e-1753-4b03-85f5-bd3bf5521893",
      "number": 12,
      "content": "Một nhóm thiểu số và kiều dân trong xứ.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.552Z",
      "updatedAt": "2024-09-07T14:17:15.552Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "8e963733-096f-4b49-838d-f8592f846c51",
      "number": 13,
      "content": "Họ đã đi lang thang từ nước này sang nước khác,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.554Z",
      "updatedAt": "2024-09-07T14:17:15.554Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "bc5d8ea0-1318-405a-af63-14e083bea6ca",
      "number": 13,
      "content": "Từ vương quốc nọ đến vương quốc kia.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.554Z",
      "updatedAt": "2024-09-07T14:17:15.554Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "10b19f5e-3b39-4f6f-9402-2925c44d4fd4",
      "number": 14,
      "content": "Nhưng Ngài đã không cho phép ai hà hiếp họ;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.556Z",
      "updatedAt": "2024-09-07T14:17:15.556Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "368cda26-2d70-4216-a9fb-9bc7adab57f8",
      "number": 14,
      "content": "Vì cớ họ Ngài đã quở trách các vua,",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.556Z",
      "updatedAt": "2024-09-07T14:17:15.556Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "a7d2fe2d-43a1-4567-9ebd-685980afd03b",
      "number": 15,
      "content": "“Đừng đụng đến những người được xức dầu của Ta,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.558Z",
      "updatedAt": "2024-09-07T14:17:15.558Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "fbada859-7911-46e6-863c-b9d57eef24cf",
      "number": 15,
      "content": "Chớ làm hại các tiên tri của Ta.”",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.558Z",
      "updatedAt": "2024-09-07T14:17:15.558Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "c44a67fc-18bb-44bf-a690-609f46f4739d",
      "number": 16,
      "content": "Khi Ngài sai nạn đói đến trong xứ;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.560Z",
      "updatedAt": "2024-09-07T14:17:15.560Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "bc7f9814-2c85-4b38-bab7-472551eed48a",
      "number": 16,
      "content": "Và dẹp hết mọi nguồn lương thực của họ.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.560Z",
      "updatedAt": "2024-09-07T14:17:15.560Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "b1c5bcf6-e8fd-4fd7-a014-2bbf92925890",
      "number": 17,
      "content": "Thì Ngài đã sai một người ra đi trước họ,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.562Z",
      "updatedAt": "2024-09-07T14:17:15.562Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "5b045138-388e-4526-9f3a-f45ad6b578dc",
      "number": 17,
      "content": "Ấy là Giô-sép, một người bị bán làm nô lệ.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.562Z",
      "updatedAt": "2024-09-07T14:17:15.562Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "1d7680da-af85-417e-9687-05908c94938f",
      "number": 18,
      "content": "Họ tra chân ông vào cùm, khiến ông bị đau đớn;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.564Z",
      "updatedAt": "2024-09-07T14:17:15.564Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "9f37118c-6008-4f46-9b48-f3d3c8093b11",
      "number": 18,
      "content": "Họ bắt cổ ông phải mang xích sắt nặng nề,",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.564Z",
      "updatedAt": "2024-09-07T14:17:15.564Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "d0a99d72-77d3-47bf-a696-298d09b7f18d",
      "number": 19,
      "content": "Cho đến khi những gì ông nói được ứng nghiệm;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.566Z",
      "updatedAt": "2024-09-07T14:17:15.566Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "2d20cf01-1beb-45f3-b08b-f7ea19a49134",
      "number": 19,
      "content": "Lời CHÚA xác minh lời ông đúng hoàn toàn.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.566Z",
      "updatedAt": "2024-09-07T14:17:15.566Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "17ac2031-de1c-4b8b-9aed-ef3a996e35d7",
      "number": 20,
      "content": "Do đó vua phải sai người đến thả ông ra;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.568Z",
      "updatedAt": "2024-09-07T14:17:15.568Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "54cf71ca-fb49-4fc6-b631-cb26f05847b4",
      "number": 20,
      "content": "Kẻ cai trị các dân đã cho ông được tự do.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.568Z",
      "updatedAt": "2024-09-07T14:17:15.568Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "a2175591-7f91-4e4d-820c-8b260873933d",
      "number": 21,
      "content": "Rồi vua ấy lập ông làm tể tướng của vương quốc mình,",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.571Z",
      "updatedAt": "2024-09-07T14:17:15.571Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "0320a24b-6193-448b-bb13-a397611e8d86",
      "number": 21,
      "content": "Làm người cai trị trên tất cả cơ nghiệp mình,",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.571Z",
      "updatedAt": "2024-09-07T14:17:15.571Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "fe9867d3-c102-41f3-bcfd-ef8b400ba12f",
      "number": 22,
      "content": "Để ông chỉ huy những người quyền thế mình,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.573Z",
      "updatedAt": "2024-09-07T14:17:15.573Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "f3f8c52a-7e20-426a-ae08-b6e66281e588",
      "number": 22,
      "content": "Và bày diệu kế cho các thượng quan mình.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.573Z",
      "updatedAt": "2024-09-07T14:17:15.573Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "c5456ce2-6d1f-4079-911e-5a5907bcae6d",
      "number": 23,
      "content": "Sau đó I-sơ-ra-ên di cư vào Ai-cập;",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.575Z",
      "updatedAt": "2024-09-07T14:17:15.575Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "1f7a7e27-3cf6-43d7-9671-0a8d117010df",
      "number": 23,
      "content": "Gia-cốp đến kiều ngụ trong xứ của Cham.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.575Z",
      "updatedAt": "2024-09-07T14:17:15.575Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "8f3da15f-a2f9-4dc3-b136-3568c6ce17f8",
      "number": 24,
      "content": "Tại đó CHÚA làm cho dân Ngài gia tăng đông đúc,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.577Z",
      "updatedAt": "2024-09-07T14:17:15.577Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "af072dfc-6c6f-431b-85db-16b4fdfd715c",
      "number": 24,
      "content": "Khiến họ mạnh hơn những kẻ áp bức mình.",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.577Z",
      "updatedAt": "2024-09-07T14:17:15.577Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "a7e1bc4e-87fe-4c90-985c-d8ba4339e362",
      "number": 25,
      "content": "Ngài để cho chúng thay lòng đổi dạ ghen ghét dân Ngài,",
      "order": 0,
      "parNumber": 49,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.580Z",
      "updatedAt": "2024-09-07T14:17:15.580Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "89153278-5a2a-4637-a396-7ee1bb31ddfa",
      "number": 25,
      "content": "Và lập mưu hãm hại các tôi tớ Ngài.",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.580Z",
      "updatedAt": "2024-09-07T14:17:15.580Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "f447b9da-0421-4e52-a699-c1047b538b01",
      "number": 26,
      "content": "Ngài sai Môi-se đầy tớ Ngài,",
      "order": 0,
      "parNumber": 51,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.582Z",
      "updatedAt": "2024-09-07T14:17:15.582Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "8ff3f751-6aee-45b4-826b-e85a49d401f7",
      "number": 26,
      "content": "Và A-rôn người Ngài đã chọn.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.582Z",
      "updatedAt": "2024-09-07T14:17:15.582Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "40e9572e-e7ed-46fc-a4e0-5e078432efab",
      "number": 27,
      "content": "Họ thực hiện những việc diệu kỳ của Ngài giữa chúng,",
      "order": 0,
      "parNumber": 53,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.584Z",
      "updatedAt": "2024-09-07T14:17:15.584Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "e3e5c8ad-ef2b-4f11-887d-b0eee12038b8",
      "number": 27,
      "content": "Và tỏ những phép lạ của Ngài trong xứ của Cham.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.584Z",
      "updatedAt": "2024-09-07T14:17:15.584Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "7448d519-1272-406e-9810-1d10761d629e",
      "number": 28,
      "content": "Ngài giáng tối tăm, khiến cả xứ trở nên đen kịt,",
      "order": 0,
      "parNumber": 55,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.587Z",
      "updatedAt": "2024-09-07T14:17:15.587Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "e3d561bf-cea6-4d83-b04e-d44740145e1c",
      "number": 28,
      "content": "Để chúng không nghịch lại lời Ngài.",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.587Z",
      "updatedAt": "2024-09-07T14:17:15.587Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "3d438dea-cf0d-4b5a-9988-e2c40fd1ad51",
      "number": 29,
      "content": "Ngài làm cho nước của chúng trở nên như máu,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.589Z",
      "updatedAt": "2024-09-07T14:17:15.589Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "ff3ad0b4-9267-4df7-afde-07aae57e6b0c",
      "number": 29,
      "content": "Và khiến cá của chúng đều phải chết toi.",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.589Z",
      "updatedAt": "2024-09-07T14:17:15.589Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "50c8049a-f0bb-4998-b1ab-712ce8426743",
      "number": 30,
      "content": "Ếch nhái nhung nhúc tràn lan khắp xứ,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.591Z",
      "updatedAt": "2024-09-07T14:17:15.591Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "4e5fda6b-53c5-484f-9e00-ff5663f66744",
      "number": 30,
      "content": "Chúng tràn tận vào cung thất của nhà vua.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.591Z",
      "updatedAt": "2024-09-07T14:17:15.591Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "54d3e9e3-5e7c-407f-a51b-b819443d21fd",
      "number": 31,
      "content": "Ngài vừa phán, vô số ruồi mòng từ đâu bay đến;",
      "order": 0,
      "parNumber": 61,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.593Z",
      "updatedAt": "2024-09-07T14:17:15.593Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "46c245ac-666d-4c8b-aa58-72305bb801cf",
      "number": 31,
      "content": "Muỗi vo ve tràn khắp cõi bờ.",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.593Z",
      "updatedAt": "2024-09-07T14:17:15.593Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "e92d8183-3087-4049-9554-1c359fae9a13",
      "number": 32,
      "content": "Ngài cho trận mưa nhưng là mưa đá;",
      "order": 0,
      "parNumber": 63,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.596Z",
      "updatedAt": "2024-09-07T14:17:15.596Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "8f829cfd-2b91-49dd-8eea-abffc1456fc2",
      "number": 32,
      "content": "Sấm sét nổ vang, gây hỏa hoạn khắp nơi.",
      "order": 1,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.596Z",
      "updatedAt": "2024-09-07T14:17:15.596Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "0e37a703-b9f4-4457-853c-8c434833bc0d",
      "number": 33,
      "content": "Ngài quật ngã cây nho và cây vả của chúng,",
      "order": 0,
      "parNumber": 65,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.598Z",
      "updatedAt": "2024-09-07T14:17:15.598Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "51a60843-b608-4530-9866-8007eb34d771",
      "number": 33,
      "content": "Khiến cây cối của chúng ngã rạp khắp xứ.",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.598Z",
      "updatedAt": "2024-09-07T14:17:15.598Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "e3c865e0-5420-4038-af39-5c97dcf7b034",
      "number": 34,
      "content": "Ngài phán nữa, vô số cào cào từ đâu bay đến;",
      "order": 0,
      "parNumber": 67,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.601Z",
      "updatedAt": "2024-09-07T14:17:15.601Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "ebd7a8b3-4837-46e6-9b0f-4b40a76d2d60",
      "number": 34,
      "content": "Châu chấu phủ tràn lan, không thể đếm cho cùng.",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.601Z",
      "updatedAt": "2024-09-07T14:17:15.601Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "dc7bb792-be54-4b25-8c55-d522091dbaaa",
      "number": 35,
      "content": "Chúng cắn phá mọi cỏ cây trong xứ;",
      "order": 0,
      "parNumber": 69,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.603Z",
      "updatedAt": "2024-09-07T14:17:15.603Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "2e09bc2f-58ed-41b3-9bf9-9b1472d8f7f9",
      "number": 35,
      "content": "Chúng cắn ăn mọi sản vật của đất đai.",
      "order": 1,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.603Z",
      "updatedAt": "2024-09-07T14:17:15.603Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "12d20be4-ecea-4d33-ae3c-8ae28d1518dc",
      "number": 36,
      "content": "Ngài cũng đánh hạ các con đầu lòng trong xứ chúng,",
      "order": 0,
      "parNumber": 71,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.605Z",
      "updatedAt": "2024-09-07T14:17:15.605Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "74735b05-1987-4812-b104-d20e2da055a2",
      "number": 36,
      "content": "Những đứa con sinh ra khi sức lực chúng dồi dào.",
      "order": 1,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.605Z",
      "updatedAt": "2024-09-07T14:17:15.605Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "bf20ae6a-36ff-4453-9d0f-b7c2cce07bf5",
      "number": 37,
      "content": "Sau đó Ngài đem dân I-sơ-ra-ên ra đi với biết bao vàng bạc;",
      "order": 0,
      "parNumber": 73,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.608Z",
      "updatedAt": "2024-09-07T14:17:15.608Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "2048af4a-ee4a-4dfe-818f-0bcefa23c939",
      "number": 37,
      "content": "Chẳng có ai chùn chân trong các chi tộc của Ngài.",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.608Z",
      "updatedAt": "2024-09-07T14:17:15.608Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "6b528bc2-ff2f-48be-be72-fbb0e5b0f423",
      "number": 38,
      "content": "Ai-cập vui mừng khi họ lìa khỏi xứ,",
      "order": 0,
      "parNumber": 75,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.610Z",
      "updatedAt": "2024-09-07T14:17:15.610Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "93d69c94-dc2f-4b57-bea1-6a441da0cb3a",
      "number": 38,
      "content": "Vì nỗi sợ hãi họ giáng trên chúng khắp nơi.",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.610Z",
      "updatedAt": "2024-09-07T14:17:15.610Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "e8e25eda-97ab-49af-bff1-c41c0e382011",
      "number": 39,
      "content": "Ngài giăng đám mây che mát trên đầu họ,",
      "order": 0,
      "parNumber": 77,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.612Z",
      "updatedAt": "2024-09-07T14:17:15.612Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "ab44d053-ac86-416b-a3ec-3c342b7084ea",
      "number": 39,
      "content": "Và ban lửa hồng soi sáng lúc đêm về.",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.612Z",
      "updatedAt": "2024-09-07T14:17:15.612Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "fdeb3354-811f-4cbd-8916-c7b97040c297",
      "number": 40,
      "content": "Họ đòi thịt, Ngài mang đến họ chim cút nhiều vô kể;",
      "order": 0,
      "parNumber": 79,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.614Z",
      "updatedAt": "2024-09-07T14:17:15.614Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "80e7223a-ccaf-4a1f-8a59-799e4f45e1e3",
      "number": 40,
      "content": "Và họ được no nê bằng bánh đến từ trời.",
      "order": 1,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.614Z",
      "updatedAt": "2024-09-07T14:17:15.614Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "651b8707-404a-4a4a-a9b4-61abaa616aa3",
      "number": 41,
      "content": "Ngài mở vầng đá, và nước ngọt tuôn ra lai láng;",
      "order": 0,
      "parNumber": 81,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.616Z",
      "updatedAt": "2024-09-07T14:17:15.616Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "a7737677-d030-4441-bdcd-11732d8549cc",
      "number": 41,
      "content": "Nước phun trào giữa nơi khô hạn tựa dòng sông.",
      "order": 1,
      "parNumber": 82,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.616Z",
      "updatedAt": "2024-09-07T14:17:15.616Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "752d63cc-f219-4f86-a91e-dc9daa77ade5",
      "number": 42,
      "content": "Vì Ngài nhớ lại lời hứa thánh của Ngài,",
      "order": 0,
      "parNumber": 83,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.619Z",
      "updatedAt": "2024-09-07T14:17:15.619Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "f337a42c-c8ba-4966-b2de-6b8396521f3d",
      "number": 42,
      "content": "Tức lời hứa với Áp-ra-ham tôi tớ Ngài.",
      "order": 1,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.619Z",
      "updatedAt": "2024-09-07T14:17:15.619Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "929d02dc-f590-47b5-bbfd-c6cd7ded1a72",
      "number": 43,
      "content": "Vậy Ngài đã đem dân Ngài ra đi trong vui mừng hớn hở;",
      "order": 0,
      "parNumber": 85,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.621Z",
      "updatedAt": "2024-09-07T14:17:15.621Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "6565ad96-4ce2-4d7c-bdd0-fa479c3ca454",
      "number": 43,
      "content": "Đem những người Ngài chọn xuất hành với tiếng vui vẻ reo hò.",
      "order": 1,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.621Z",
      "updatedAt": "2024-09-07T14:17:15.621Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "4e5ac7f1-23e1-4df5-a4a1-b2489f26ea88",
      "number": 44,
      "content": "Ngài ban xứ sở của các dân cho họ làm sản nghiệp;",
      "order": 0,
      "parNumber": 87,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.623Z",
      "updatedAt": "2024-09-07T14:17:15.623Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "a807f22b-e95e-4512-aa2e-a32378ce0b80",
      "number": 44,
      "content": "Và cho họ hưởng công lao làm lụng của các dân,",
      "order": 1,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.623Z",
      "updatedAt": "2024-09-07T14:17:15.623Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "ec0d513c-e7d9-456a-a4de-bfbee5337894",
      "number": 45,
      "content": "Để họ vâng giữ luật lệ Ngài,",
      "order": 0,
      "parNumber": 89,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.624Z",
      "updatedAt": "2024-09-07T14:17:15.624Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "88489fe7-a042-4421-ac7c-79b916a23aca",
      "number": 45,
      "content": "Và tuân theo luật pháp Ngài.",
      "order": 1,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.624Z",
      "updatedAt": "2024-09-07T14:17:15.624Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    },
    {
      "id": "de0fc386-812d-492a-8a39-7df305f5646d",
      "number": 45,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 2,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:15.624Z",
      "updatedAt": "2024-09-07T14:17:15.624Z",
      "chapterId": "556f8b56-8f13-42d2-9707-4e7d4a13e1d4"
    }
  ],
