{
  "chapter": {
    "id": "3e475e25-8ed5-492a-a91a-3f02f894d746",
    "number": 69,
    "ref": "https://www.bible.com/bible/19/PSA.69.BD2011",
    "createdAt": "2024-09-07T11:51:11.158Z",
    "updatedAt": "2024-09-07T11:51:11.158Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "6244f86b-b051-493c-846e-0d995380aa3e",
      "number": 1,
      "content": "Đức Chúa Trời ôi, xin cứu con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.709Z",
      "updatedAt": "2024-09-07T14:09:53.709Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "396ba9f6-e41c-4a2d-87ea-032491c55ea6",
      "number": 1,
      "content": "Vì các dòng nước đã ngập đến cổ con.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.709Z",
      "updatedAt": "2024-09-07T14:09:53.709Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "5d72f3a2-3280-48cb-9095-d427b8ac528b",
      "number": 2,
      "content": "Con đã bị lún sâu trong một vũng lầy;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.712Z",
      "updatedAt": "2024-09-07T14:09:53.712Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "caaa2289-8706-4271-be7b-6b176a275df5",
      "number": 2,
      "content": "Thật là một vũng lầy không đáy.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.712Z",
      "updatedAt": "2024-09-07T14:09:53.712Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "9eac9d31-58e6-4d50-8a90-1587a1746f71",
      "number": 2,
      "content": "Con bị rơi vào giữa dòng nước sâu;",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.712Z",
      "updatedAt": "2024-09-07T14:09:53.712Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "42ed478d-2dd2-4753-9a1f-f889235a9e12",
      "number": 2,
      "content": "Dòng cuồng lưu cuốn hút lấy con.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.712Z",
      "updatedAt": "2024-09-07T14:09:53.712Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "78493702-eb12-4451-8c53-9a73753b17a8",
      "number": 3,
      "content": "Con kêu van đến kiệt lực;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.716Z",
      "updatedAt": "2024-09-07T14:09:53.716Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "1e1a64d3-7771-45f9-b08e-8e89b75715e9",
      "number": 3,
      "content": "Cổ họng con đã gần tắt tiếng;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.716Z",
      "updatedAt": "2024-09-07T14:09:53.716Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "8119251b-5f11-4036-b58a-0ec4ac243924",
      "number": 3,
      "content": "Mắt con đã lờ đờ,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.716Z",
      "updatedAt": "2024-09-07T14:09:53.716Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "4ff6401e-f7b6-4624-8b83-e309285bb61b",
      "number": 3,
      "content": "Trong khi trông đợi Đức Chúa Trời của con.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.716Z",
      "updatedAt": "2024-09-07T14:09:53.716Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "b3bcaa1f-d636-4537-b003-b0c4d56d0567",
      "number": 4,
      "content": "Những kẻ ghét con vô cớ",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.719Z",
      "updatedAt": "2024-09-07T14:09:53.719Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "fb6497ea-910d-4d93-8983-c833abb094cd",
      "number": 4,
      "content": "Thật nhiều hơn tóc trên đầu con;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.719Z",
      "updatedAt": "2024-09-07T14:09:53.719Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "be00c591-1ca6-4307-be8d-d6efcddfe489",
      "number": 4,
      "content": "Những kẻ muốn tiêu diệt con thật mạnh mẽ thay!",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.719Z",
      "updatedAt": "2024-09-07T14:09:53.719Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "7908553e-a1f9-4e7d-a2a4-ac1928b29271",
      "number": 4,
      "content": "Đó là những kẻ thù ghét con vô cớ.",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.719Z",
      "updatedAt": "2024-09-07T14:09:53.719Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "57cdf899-9bfb-4025-991a-02e54c52d7ac",
      "number": 4,
      "content": "Con đã bị bắt bồi thường",
      "order": 4,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.719Z",
      "updatedAt": "2024-09-07T14:09:53.719Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "7f5878c3-fa88-4753-8980-cfe6326ed33a",
      "number": 4,
      "content": "Cho những gì con không lấy.",
      "order": 5,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.719Z",
      "updatedAt": "2024-09-07T14:09:53.719Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "98aaa760-ce5e-4e3a-bb97-9354aa82d655",
      "number": 5,
      "content": "Đức Chúa Trời ôi, Ngài đã biết sự dại dột của con;",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.722Z",
      "updatedAt": "2024-09-07T14:09:53.722Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "439de3ea-3c9f-45cc-bf87-ea99b95ef3ae",
      "number": 5,
      "content": "Những tội lỗi của con chẳng giấu được Ngài.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.722Z",
      "updatedAt": "2024-09-07T14:09:53.722Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "e1a34fe2-7638-47fd-b59b-43d9ca93203b",
      "number": 6,
      "content": "Lạy Chúa, CHÚA các đạo quân!",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.724Z",
      "updatedAt": "2024-09-07T14:09:53.724Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "85869837-e0e3-490d-84f3-b9ebdc9680e6",
      "number": 6,
      "content": "Nguyện ai trông cậy Ngài sẽ không vì con mà hổ thẹn;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.724Z",
      "updatedAt": "2024-09-07T14:09:53.724Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "d4469bcd-d997-44c6-863b-f827102c7557",
      "number": 6,
      "content": "Đức Chúa Trời của I-sơ-ra-ên ôi,",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.724Z",
      "updatedAt": "2024-09-07T14:09:53.724Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "e0ff9706-f85c-47c9-8351-082bdd2d6cc3",
      "number": 6,
      "content": "Nguyện những ai tìm kiếm Ngài sẽ không bị sỉ nhục vì con.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.724Z",
      "updatedAt": "2024-09-07T14:09:53.724Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "2c5b2055-ef19-4057-bbb7-15b9fd66c42e",
      "number": 7,
      "content": "Vì cớ Ngài con bị người ta mắng nhiếc;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.728Z",
      "updatedAt": "2024-09-07T14:09:53.728Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "dde73818-618f-4149-a8ba-e4b5d26047b5",
      "number": 7,
      "content": "Mặt con chẳng còn thể diện gì nữa.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.728Z",
      "updatedAt": "2024-09-07T14:09:53.728Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "18cca56e-caef-47fc-88c6-9faa2ef0954c",
      "number": 8,
      "content": "Con đã trở thành kẻ xa lạ đối với anh chị em con,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.732Z",
      "updatedAt": "2024-09-07T14:09:53.732Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "bec34d87-463c-4d31-9dca-b2250e8ddc87",
      "number": 8,
      "content": "Thành người ngoại tộc đối với các con cái của mẹ con;",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.732Z",
      "updatedAt": "2024-09-07T14:09:53.732Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "96ce4e57-3e93-4209-a3fa-c5ad52840ddd",
      "number": 9,
      "content": "Vì lòng nhiệt thành về nhà Ngài đã thiêu đốt con,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.735Z",
      "updatedAt": "2024-09-07T14:09:53.735Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "1239d82b-72b4-43b9-b164-5573cc9e2b08",
      "number": 9,
      "content": "Những lời sỉ nhục của những kẻ sỉ nhục Ngài đã đổ trên con.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.735Z",
      "updatedAt": "2024-09-07T14:09:53.735Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "fee8dbfc-d4da-4374-af39-a2d6933a07dc",
      "number": 10,
      "content": "Khi con than khóc và kiêng ép linh hồn,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.737Z",
      "updatedAt": "2024-09-07T14:09:53.737Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "9b4e8922-a403-4070-9e01-9047556e11ac",
      "number": 10,
      "content": "Con trở thành đối tượng cho người ta mắng nhiếc.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.737Z",
      "updatedAt": "2024-09-07T14:09:53.737Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "4d3f3e49-0186-442f-b0c7-c6ac42a17d63",
      "number": 11,
      "content": "Khi con mặc bao gai,",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.738Z",
      "updatedAt": "2024-09-07T14:09:53.738Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "786607a3-e63d-4908-927b-e7cde8a2e04a",
      "number": 11,
      "content": "Con trở thành đề tài giễu cợt cho người ta.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.738Z",
      "updatedAt": "2024-09-07T14:09:53.738Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "a6f5b504-d8ce-43a8-824b-f5107dad754f",
      "number": 12,
      "content": "Những viên chức ngồi ở cổng thành chế giễu con;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.740Z",
      "updatedAt": "2024-09-07T14:09:53.740Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "9679d5a0-8835-475c-a59d-d562290de258",
      "number": 12,
      "content": "Con trở thành câu hát cho những kẻ say sưa châm biếm.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.740Z",
      "updatedAt": "2024-09-07T14:09:53.740Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "93b4ee4e-2116-4239-91fe-1f48113d7666",
      "number": 13,
      "content": "Nhưng phần con, CHÚA ôi, con vẫn cầu nguyện với Ngài;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.743Z",
      "updatedAt": "2024-09-07T14:09:53.743Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "a677d5e6-9b11-4958-87ae-b504e31be977",
      "number": 13,
      "content": "Đức Chúa Trời ôi, để trong lúc Ngài gia ân ban phước,",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.743Z",
      "updatedAt": "2024-09-07T14:09:53.743Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "b10e5d03-09aa-4868-a6c6-4fefc170ed1b",
      "number": 13,
      "content": "Theo tình thương lớn lao của Ngài, Ngài đáp lời con bằng chân lý cứu rỗi của Ngài.",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.743Z",
      "updatedAt": "2024-09-07T14:09:53.743Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "0d80937c-93d5-423a-999b-d9f1cab3faf6",
      "number": 14,
      "content": "Xin cứu con khỏi chốn bùn lầy;",
      "order": 0,
      "parNumber": 38,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.745Z",
      "updatedAt": "2024-09-07T14:09:53.745Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "6cd874f3-68e9-447f-9eb3-b5ccab3e8409",
      "number": 14,
      "content": "Xin đừng để con bị lún chìm xuống nữa;",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.745Z",
      "updatedAt": "2024-09-07T14:09:53.745Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "10868aaa-81d6-43c3-935c-fe731acf6263",
      "number": 14,
      "content": "Xin giải cứu con khỏi những kẻ ghét con,",
      "order": 2,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.745Z",
      "updatedAt": "2024-09-07T14:09:53.745Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "d419610b-64c3-49cc-aa5a-f65e13d83a78",
      "number": 14,
      "content": "Và khỏi những chỗ nước sâu.",
      "order": 3,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.745Z",
      "updatedAt": "2024-09-07T14:09:53.745Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "66ed53f0-4fd2-4745-b305-b17172d5379a",
      "number": 15,
      "content": "Xin đừng để dòng cuồng lưu cuốn hút lấy con;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.747Z",
      "updatedAt": "2024-09-07T14:09:53.747Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "4112294e-7062-4c9b-9c9a-d5a286ce4a8c",
      "number": 15,
      "content": "Xin đừng để vực sâu nuốt chửng con;",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.747Z",
      "updatedAt": "2024-09-07T14:09:53.747Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "97d09cae-dba4-48a4-b181-7719cd36197e",
      "number": 15,
      "content": "Và xin đừng để vực thẳm khép miệng lại trên con.",
      "order": 2,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.747Z",
      "updatedAt": "2024-09-07T14:09:53.747Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "ebea4eea-9f09-4567-9283-ab36e3036ec6",
      "number": 16,
      "content": "Xin nhậm lời con, CHÚA ôi, vì tình yêu của Ngài thật tuyệt vời;",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.749Z",
      "updatedAt": "2024-09-07T14:09:53.749Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "8a3e9d20-91a6-46b0-937a-823b037aac15",
      "number": 16,
      "content": "Xin quay lại với con theo ơn thương xót lớn lao của Ngài.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.749Z",
      "updatedAt": "2024-09-07T14:09:53.749Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "a576cb55-1215-4374-b7e9-6192dd2fe8ee",
      "number": 17,
      "content": "Xin đừng ẩn mặt Ngài đối với tôi tớ Ngài;",
      "order": 0,
      "parNumber": 47,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.751Z",
      "updatedAt": "2024-09-07T14:09:53.751Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "109de0c2-1feb-4da2-8713-7cfb8d9ef97b",
      "number": 17,
      "content": "Xin mau đáp lời cầu nguyện của con vì con đang gặp hoạn nạn.",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.751Z",
      "updatedAt": "2024-09-07T14:09:53.751Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "037ad2a4-0edf-4113-beb7-f8438eafc28f",
      "number": 18,
      "content": "Xin đến gần linh hồn con và cứu con;",
      "order": 0,
      "parNumber": 49,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.753Z",
      "updatedAt": "2024-09-07T14:09:53.753Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "4f0e9499-f6a5-4785-901c-db4382fe612c",
      "number": 18,
      "content": "Xin giải cứu con vì những kẻ thù của con.",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.753Z",
      "updatedAt": "2024-09-07T14:09:53.753Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "1e46593b-a69a-42f0-bcf5-a57ed0d13487",
      "number": 19,
      "content": "Ngài biết con bị mắng nhiếc, sỉ nhục,",
      "order": 0,
      "parNumber": 51,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.759Z",
      "updatedAt": "2024-09-07T14:09:53.759Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "f8559aee-e902-4f61-96ef-fc7239c4962f",
      "number": 19,
      "content": "Và xấu hổ đến thế nào rồi.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.759Z",
      "updatedAt": "2024-09-07T14:09:53.759Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "f3743795-6ada-42a0-bc8b-a37ef807bbbd",
      "number": 19,
      "content": "Tất cả kẻ thù của con đều ở trước mặt Ngài.",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.759Z",
      "updatedAt": "2024-09-07T14:09:53.759Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "75186062-e754-43c5-b6a8-db826054c3e7",
      "number": 20,
      "content": "Những lời mắng nhiếc của chúng làm tan nát lòng con.",
      "order": 0,
      "parNumber": 54,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.764Z",
      "updatedAt": "2024-09-07T14:09:53.764Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "48f432be-a2be-47d8-b090-a89100791556",
      "number": 20,
      "content": "Con đau đớn lắm.",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.764Z",
      "updatedAt": "2024-09-07T14:09:53.764Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "d8d804f0-f78f-4c4d-b68a-bf4f3e16d2f1",
      "number": 20,
      "content": "Con mong có được người cảm thương, nhưng chẳng có ai.",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.764Z",
      "updatedAt": "2024-09-07T14:09:53.764Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "b2ce320a-62ad-4728-9bdc-ccc9fae188a4",
      "number": 20,
      "content": "Con ước ao được có người an ủi, nhưng nào đâu thấy.",
      "order": 3,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.764Z",
      "updatedAt": "2024-09-07T14:09:53.764Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "d20bd2c3-1176-4e1c-9958-89965cb4b2dd",
      "number": 21,
      "content": "Chúng đã đưa cho con mật đắng làm thức ăn.",
      "order": 0,
      "parNumber": 58,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.769Z",
      "updatedAt": "2024-09-07T14:09:53.769Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "d692c473-e346-40da-a488-d71781a5b4e4",
      "number": 21,
      "content": "Khi con khát chúng đã trao giấm chua cho con làm thức uống.",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.769Z",
      "updatedAt": "2024-09-07T14:09:53.769Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "75bb84fa-e633-45b3-bf64-9173f3ee1f7f",
      "number": 22,
      "content": "Nguyện bàn tiệc của chúng trở thành một cái bẫy.",
      "order": 0,
      "parNumber": 60,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.774Z",
      "updatedAt": "2024-09-07T14:09:53.774Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "2be4ce3a-5c3c-48ae-9d3c-f4a67ba16fc8",
      "number": 22,
      "content": "Nguyện nó thành một hình phạt đích đáng và một cạm bẫy.",
      "order": 1,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.774Z",
      "updatedAt": "2024-09-07T14:09:53.774Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "efe2e071-5788-4af2-af02-ffe4c8f89308",
      "number": 23,
      "content": "Nguyện mắt chúng bị đui mù, chẳng còn trông thấy nữa.",
      "order": 0,
      "parNumber": 62,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.779Z",
      "updatedAt": "2024-09-07T14:09:53.779Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "4c2c7f8c-2269-405b-9dd1-02e2b03b0943",
      "number": 23,
      "content": "Nguyện lưng chúng cụp xuống và run rẩy luôn luôn.",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.779Z",
      "updatedAt": "2024-09-07T14:09:53.779Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "74b227a0-fd08-4196-bcde-a528ce0a924d",
      "number": 24,
      "content": "Xin Ngài trút cơn thịnh nộ của Ngài trên chúng;",
      "order": 0,
      "parNumber": 64,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.784Z",
      "updatedAt": "2024-09-07T14:09:53.784Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "e91f3dbf-8c83-43f8-be58-abf4451383e5",
      "number": 24,
      "content": "Xin Ngài đổ cơn giận của Ngài xuống đầu chúng.",
      "order": 1,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.784Z",
      "updatedAt": "2024-09-07T14:09:53.784Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "1678e598-fa68-439b-b768-3e95068cfd5a",
      "number": 25,
      "content": "Nguyện chỗ ở của chúng bị bỏ hoang;",
      "order": 0,
      "parNumber": 66,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.789Z",
      "updatedAt": "2024-09-07T14:09:53.789Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "b295d53e-5b54-4e27-ab2b-68ccf3b4a45f",
      "number": 25,
      "content": "Nguyện trại của chúng chẳng còn ai ở,",
      "order": 1,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.789Z",
      "updatedAt": "2024-09-07T14:09:53.789Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "e3a5b51e-a0c4-41d2-89ae-88e276be1089",
      "number": 26,
      "content": "Bởi vì chúng bách hại người bị Ngài sửa phạt,",
      "order": 0,
      "parNumber": 68,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.794Z",
      "updatedAt": "2024-09-07T14:09:53.794Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "711cba32-d80f-4802-bbc6-07461dde2fa6",
      "number": 26,
      "content": "Chúng làm cho đau đớn thêm người bị Ngài làm cho đau.",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.794Z",
      "updatedAt": "2024-09-07T14:09:53.794Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "fb4a3afd-47ef-4927-9704-f1f6d375c7a7",
      "number": 27,
      "content": "Xin Ngài cộng thêm tội ấy vào các tội khác của chúng;",
      "order": 0,
      "parNumber": 70,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.800Z",
      "updatedAt": "2024-09-07T14:09:53.800Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "7916c57e-4e4a-4dcc-b66a-1c2ed961223b",
      "number": 27,
      "content": "Xin đừng cho chúng hưởng sự công chính của Ngài.",
      "order": 1,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.800Z",
      "updatedAt": "2024-09-07T14:09:53.800Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "d6529b0b-c290-4efd-aa35-69f9d5ebdba2",
      "number": 28,
      "content": "Nguyện chúng bị xóa tên khỏi sách sự sống,",
      "order": 0,
      "parNumber": 72,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.805Z",
      "updatedAt": "2024-09-07T14:09:53.805Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "006111ed-d5f6-48d9-9ab3-73b08b5e96dd",
      "number": 28,
      "content": "Và không được ghi tên trong danh sách những người ngay lành.",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.805Z",
      "updatedAt": "2024-09-07T14:09:53.805Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "50e63a1d-f155-4bca-a75f-79a9fe3d195d",
      "number": 29,
      "content": "Còn con, con bị khốn cùng và đau khổ;",
      "order": 0,
      "parNumber": 74,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.810Z",
      "updatedAt": "2024-09-07T14:09:53.810Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "f28684c4-cae1-4b83-86b5-c6ba4737b934",
      "number": 29,
      "content": "Đức Chúa Trời ôi, nguyện ơn giải cứu của Ngài đặt con lên nơi an toàn trên cao.",
      "order": 1,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.810Z",
      "updatedAt": "2024-09-07T14:09:53.810Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "848e2633-bd75-4941-8f2e-b15c8ba7507d",
      "number": 30,
      "content": "Tôi sẽ dùng bài hát để ca ngợi danh Đức Chúa Trời;",
      "order": 0,
      "parNumber": 76,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.815Z",
      "updatedAt": "2024-09-07T14:09:53.815Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "1f58f990-01d8-4eae-823f-400001a5ed54",
      "number": 30,
      "content": "Tôi sẽ tán dương Ngài bằng sự tạ ơn.",
      "order": 1,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.815Z",
      "updatedAt": "2024-09-07T14:09:53.815Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "3850075e-e0df-4e07-809f-40516530476c",
      "number": 31,
      "content": "Điều ấy sẽ đẹp lòng CHÚA hơn dâng một con bò đực;",
      "order": 0,
      "parNumber": 78,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.820Z",
      "updatedAt": "2024-09-07T14:09:53.820Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "cf6be4f7-1ac0-44e7-bd33-f1e2d9fd244a",
      "number": 31,
      "content": "Thật hơn dâng một con bò đực có đủ móng và đủ sừng.",
      "order": 1,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.820Z",
      "updatedAt": "2024-09-07T14:09:53.820Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "901c88f5-554f-46ec-96ef-0fd621bdb7e1",
      "number": 32,
      "content": "Hỡi những người khiêm hạ, hãy thấy điều đó mà vui mừng;",
      "order": 0,
      "parNumber": 80,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.825Z",
      "updatedAt": "2024-09-07T14:09:53.825Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "fcb80430-0f73-4627-9bcf-94991129016c",
      "number": 32,
      "content": "Hỡi các bạn là những người tìm kiếm Đức Chúa Trời, hãy nức lòng phấn khởi.",
      "order": 1,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.825Z",
      "updatedAt": "2024-09-07T14:09:53.825Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "264c8b1b-0b69-4a0a-a8b6-015bc67f27c7",
      "number": 33,
      "content": "Vì CHÚA lắng nghe những người khốn khổ;",
      "order": 0,
      "parNumber": 82,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.830Z",
      "updatedAt": "2024-09-07T14:09:53.830Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "36265339-857f-4bfc-9af0-4d3e26237f02",
      "number": 33,
      "content": "Ngài không khinh khi con dân Ngài khi họ bị tù đày.",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.830Z",
      "updatedAt": "2024-09-07T14:09:53.830Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "d95e1a4a-077e-4cc0-a07b-872fe473bbfe",
      "number": 34,
      "content": "Nguyện trời và đất ca ngợi Ngài;",
      "order": 0,
      "parNumber": 84,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.835Z",
      "updatedAt": "2024-09-07T14:09:53.835Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "52d22b64-a3b5-4f12-bbe7-97de0ae167a6",
      "number": 34,
      "content": "Nguyện biển và mọi vật trong biển ca tụng Ngài.",
      "order": 1,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.835Z",
      "updatedAt": "2024-09-07T14:09:53.835Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "d8992415-a569-49b8-9083-2e2d499f74e4",
      "number": 35,
      "content": "Vì Đức Chúa Trời sẽ cứu Si-ôn;",
      "order": 0,
      "parNumber": 86,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.844Z",
      "updatedAt": "2024-09-07T14:09:53.844Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "b44dd660-d1e3-49aa-a081-4f8b18e8c3b2",
      "number": 35,
      "content": "Ngài sẽ xây dựng lại những thành trì của Giu-đa;",
      "order": 1,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.844Z",
      "updatedAt": "2024-09-07T14:09:53.844Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "74ecaa10-4387-4c9c-ac17-ee68af49534e",
      "number": 35,
      "content": "Rồi người ta sẽ định cư ở đó và lập nghiệp trong xứ ấy.",
      "order": 2,
      "parNumber": 88,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.844Z",
      "updatedAt": "2024-09-07T14:09:53.844Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "27880559-e563-4053-acf9-a12c3a7a5a30",
      "number": 36,
      "content": "Con cháu của các tôi tớ Ngài sẽ thừa hưởng xứ đó,",
      "order": 0,
      "parNumber": 89,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.849Z",
      "updatedAt": "2024-09-07T14:09:53.849Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    },
    {
      "id": "bd4a84d2-80f1-4754-9e64-142e0166d440",
      "number": 36,
      "content": "Và những ai yêu mến danh Ngài sẽ cư ngụ trong đó.",
      "order": 1,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:09:53.849Z",
      "updatedAt": "2024-09-07T14:09:53.849Z",
      "chapterId": "3e475e25-8ed5-492a-a91a-3f02f894d746"
    }
  ],
