{
  "chapter": {
    "id": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c",
    "number": 149,
    "ref": "https://www.bible.com/bible/19/PSA.149.BD2011",
    "createdAt": "2024-09-23T05:08:40.711Z",
    "updatedAt": "2024-09-23T05:08:40.711Z",
