{
  "chapter": {
    "id": "a0754cf0-624e-472b-8e93-1f50da0451f3",
    "number": 149,
    "ref": "https://www.bible.com/bible/19/PSA.149.BD2011",
    "createdAt": "2024-09-07T11:51:11.598Z",
    "updatedAt": "2024-09-07T11:51:11.598Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "59ba66bd-90dc-4ac1-9f98-8266704eb497",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.812Z",
      "updatedAt": "2024-09-07T14:30:06.812Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "3a147c36-420c-472a-8b68-a7ce564e324e",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA một bài ca mới.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.812Z",
      "updatedAt": "2024-09-07T14:30:06.812Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "5dfc908c-c7c1-46bf-a83b-89cbb3ec387e",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong đại hội của các thánh dân.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.812Z",
      "updatedAt": "2024-09-07T14:30:06.812Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "c9905cc1-6a22-4b92-ac78-152fc360eeb0",
      "number": 2,
      "content": "Nguyện I-sơ-ra-ên vui mừng nơi Đấng tạo nên mình.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.822Z",
      "updatedAt": "2024-09-07T14:30:06.822Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "8d940514-c63b-40d1-a92e-4ce91f199f33",
      "number": 2,
      "content": "Nguyện dân cư của Si-ôn vui vẻ nơi Vua mình.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.822Z",
      "updatedAt": "2024-09-07T14:30:06.822Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "2e01214d-dc97-4c50-a9e0-de303224f7b8",
      "number": 3,
      "content": "Nguyện họ nhảy múa ca ngợi danh Ngài.",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.824Z",
      "updatedAt": "2024-09-07T14:30:06.824Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "90ef65d2-bf56-4390-ace9-2b99260453e2",
      "number": 3,
      "content": "Nguyện họ dùng trống và đàn ca tụng Ngài,",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.824Z",
      "updatedAt": "2024-09-07T14:30:06.824Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "02e9e5f7-7df5-4d2d-b4d6-adb287f15f8a",
      "number": 4,
      "content": "Vì CHÚA lấy làm vui nơi con dân Ngài.",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.827Z",
      "updatedAt": "2024-09-07T14:30:06.827Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "87679693-4247-4d2c-b245-06418da52134",
      "number": 4,
      "content": "Ngài điểm tô cho người khiêm nhường bằng ơn cứu rỗi.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.827Z",
      "updatedAt": "2024-09-07T14:30:06.827Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "8e2f04fe-0263-4f64-872f-70f0d60f3baa",
      "number": 5,
      "content": "Nguyện các thánh đồ vui mừng trong vinh hạnh ấy.",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.829Z",
      "updatedAt": "2024-09-07T14:30:06.829Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "54b8dc3d-3a03-44af-bf27-8da63db76069",
      "number": 5,
      "content": "Nguyện họ hát ca ngay cả trên giường.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.829Z",
      "updatedAt": "2024-09-07T14:30:06.829Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "e92d25b7-f845-419a-9263-2c0a6faebc3a",
      "number": 6,
      "content": "Nguyện những lời ca ngợi Đức Chúa Trời ở nơi miệng họ,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.832Z",
      "updatedAt": "2024-09-07T14:30:06.832Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "c075690c-e212-49c2-9686-a2e8a6665790",
      "number": 6,
      "content": "Và nguyện một thanh gươm hai lưỡi ở trong tay họ,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.832Z",
      "updatedAt": "2024-09-07T14:30:06.832Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "2ca303c3-cc42-4395-aadf-1b63fcaf47da",
      "number": 7,
      "content": "Để họ báo thù các nước,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.834Z",
      "updatedAt": "2024-09-07T14:30:06.834Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "e5307a52-c7a5-494f-81d5-ba22c99166bd",
      "number": 7,
      "content": "Và trừng phạt các dân,",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.834Z",
      "updatedAt": "2024-09-07T14:30:06.834Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "3e63b04a-0804-47ef-8a2c-94ffe5184342",
      "number": 8,
      "content": "Để trói các vua của chúng bằng xích,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.836Z",
      "updatedAt": "2024-09-07T14:30:06.836Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "3f8a7118-943e-487e-9a7d-24fde71926e3",
      "number": 8,
      "content": "Và những kẻ quyền quý của chúng bằng xiềng sắt,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.836Z",
      "updatedAt": "2024-09-07T14:30:06.836Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "1fea1365-d533-4e2c-acbb-2b00c6741539",
      "number": 9,
      "content": "Để thi hành án phạt đã ghi sẵn cho chúng.",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.838Z",
      "updatedAt": "2024-09-07T14:30:06.838Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "a73d769b-9350-4c3c-abb5-0e0bf98b5754",
      "number": 9,
      "content": "Đó là niềm vinh dự cho tất cả thánh đồ.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.838Z",
      "updatedAt": "2024-09-07T14:30:06.838Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    },
    {
      "id": "985b90b8-01f0-4f55-b6f4-aa5038542857",
      "number": 9,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:30:06.838Z",
      "updatedAt": "2024-09-07T14:30:06.838Z",
      "chapterId": "a0754cf0-624e-472b-8e93-1f50da0451f3"
    }
  ],
