  },
  "verses": [
    {
      "id": "4135623f-53fe-4f60-b956-b18ab62743e2",
      "number": 1,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.158Z",
      "updatedAt": "2024-09-23T05:44:26.158Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "13552fb8-9b6f-4954-ae65-1d77fe5637a4",
      "number": 9,
      "content": "Hãy ca ngợi **CHÚA**!",
      "order": 2,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.276Z",
      "updatedAt": "2024-09-23T05:44:26.276Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "70ad9e6a-692a-4cb0-9859-39dc9ffc18df",
      "number": 1,
      "content": "Hãy ca ngợi **CHÚA** một bài ca mới.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.186Z",
      "updatedAt": "2024-09-23T05:44:26.186Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "c343139e-18c5-4510-bd87-3bc79d3ca937",
      "number": 1,
      "content": "Hãy ca ngợi Ngài trong đại hội của các thánh dân.",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.191Z",
      "updatedAt": "2024-09-23T05:44:26.191Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "b33df5b2-96f6-4e73-a79a-bdf4d5aabe89",
      "number": 2,
      "content": "Nguyện I-sơ-ra-ên vui mừng nơi Đấng tạo nên mình.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.196Z",
      "updatedAt": "2024-09-23T05:44:26.196Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "32435d10-20ae-4933-8d47-665cb8423174",
      "number": 2,
      "content": "Nguyện dân cư của Si-ôn vui vẻ nơi Vua mình.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.200Z",
      "updatedAt": "2024-09-23T05:44:26.200Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "55775def-85e6-4a02-89aa-57e0367a325c",
      "number": 3,
      "content": "Nguyện họ nhảy múa ca ngợi danh Ngài.",
      "order": 0,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.205Z",
      "updatedAt": "2024-09-23T05:44:26.205Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "b767d6f5-5934-4593-9d8f-acfa24988570",
      "number": 3,
      "content": "Nguyện họ dùng trống và đàn ca tụng Ngài,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.210Z",
      "updatedAt": "2024-09-23T05:44:26.210Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "a8dd4174-e67b-40a6-82c1-521a3cf2afbf",
      "number": 4,
      "content": "Vì **CHÚA** lấy làm vui nơi con dân Ngài.",
      "order": 0,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.215Z",
      "updatedAt": "2024-09-23T05:44:26.215Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "cd1e942d-aabb-465a-adbc-a17792f917ff",
      "number": 4,
      "content": "Ngài điểm tô cho người khiêm nhường bằng ơn cứu rỗi.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.220Z",
      "updatedAt": "2024-09-23T05:44:26.220Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "8ff4a71e-7c5e-4fb2-8242-43439742cbf6",
      "number": 5,
      "content": "Nguyện các thánh đồ vui mừng trong vinh hạnh ấy.",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.225Z",
      "updatedAt": "2024-09-23T05:44:26.225Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "b1222922-e626-4b21-8626-7c620e725f74",
      "number": 5,
      "content": "Nguyện họ hát ca ngay cả trên giường.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.231Z",
      "updatedAt": "2024-09-23T05:44:26.231Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "5e1ddfa8-ccc8-40a5-aa17-56c84b324abc",
      "number": 6,
      "content": "Nguyện những lời ca ngợi Đức Chúa Trời ở nơi miệng họ,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.236Z",
      "updatedAt": "2024-09-23T05:44:26.236Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "bbe25c19-a867-4ee8-a2a3-af81afcef5d2",
      "number": 6,
      "content": "Và nguyện một thanh gươm hai lưỡi ở trong tay họ,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.241Z",
      "updatedAt": "2024-09-23T05:44:26.241Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "ea6569af-efdb-442e-a064-8128ac95f01a",
      "number": 7,
      "content": "Để họ báo thù các nước,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.245Z",
      "updatedAt": "2024-09-23T05:44:26.245Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "ba9d349c-ed9a-4ecc-b4a8-d7b95a441692",
      "number": 7,
      "content": "Và trừng phạt các dân,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.250Z",
      "updatedAt": "2024-09-23T05:44:26.250Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "4dc80d8d-7fe5-4de8-9900-14264e3de0a7",
      "number": 8,
      "content": "Để trói các vua của chúng bằng xích,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.255Z",
      "updatedAt": "2024-09-23T05:44:26.255Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "e7599fe7-1a71-4671-a305-f6c21cda4050",
      "number": 8,
      "content": "Và những kẻ quyền quý của chúng bằng xiềng sắt,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.260Z",
      "updatedAt": "2024-09-23T05:44:26.260Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "3b2f183f-b7db-4de7-aeb9-36b5b41a6d23",
      "number": 9,
      "content": "Để thi hành án phạt đã ghi sẵn cho chúng.",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.265Z",
      "updatedAt": "2024-09-23T05:44:26.265Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    },
    {
      "id": "aeb41ee0-f00f-458c-bdb6-88bc907a3685",
      "number": 9,
      "content": "Đó là niềm vinh dự cho tất cả thánh đồ.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:44:26.271Z",
      "updatedAt": "2024-09-23T05:44:26.271Z",
      "chapterId": "f02b6502-3d03-4ed4-b5ce-3038c5c08f1c"
    }
  ],
