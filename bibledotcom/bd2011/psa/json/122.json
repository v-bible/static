{
  "chapter": {
    "id": "81d11ff6-a1f3-4459-aeaa-83d9b4634460",
    "number": 122,
    "ref": "https://www.bible.com/bible/19/PSA.122.BD2011",
    "createdAt": "2024-09-07T11:51:11.523Z",
    "updatedAt": "2024-09-07T11:51:11.523Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "4660ea8c-d704-408c-87b1-6f595d4aba2e",
      "number": 1,
      "content": "Tôi vui mừng khi người ta nói với tôi rằng,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.546Z",
      "updatedAt": "2024-09-07T14:21:31.546Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "b3cca8af-e789-4dcb-8ac8-abc86c6ccfa4",
      "number": 1,
      "content": "“Chúng ta hãy đi đến nhà CHÚA.”",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.546Z",
      "updatedAt": "2024-09-07T14:21:31.546Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "bd709ebb-1e5c-4d35-8c7a-1383c62e77b6",
      "number": 2,
      "content": "Hỡi Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.556Z",
      "updatedAt": "2024-09-07T14:21:31.556Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "047fe376-f0f4-4080-a043-604a93d5008b",
      "number": 2,
      "content": "Chân chúng ta đang đứng bên trong cổng thành ngươi.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.556Z",
      "updatedAt": "2024-09-07T14:21:31.556Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "56a3091a-6ac4-42b2-8bb9-4080ea5aa838",
      "number": 3,
      "content": "Giê-ru-sa-lem đã được xây dựng thành một kinh đô,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.560Z",
      "updatedAt": "2024-09-07T14:21:31.560Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "f4e14b78-5669-43e7-8d49-75436e59443d",
      "number": 3,
      "content": "Các tòa nhà trong thành nối kết nhau chặt chẽ.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.560Z",
      "updatedAt": "2024-09-07T14:21:31.560Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "298178cc-509f-494c-bc8d-20a708c97161",
      "number": 4,
      "content": "Các chi tộc đi lên thành ấy,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.564Z",
      "updatedAt": "2024-09-07T14:21:31.564Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "821041cf-18f7-46af-8e60-7fb4cd4249f5",
      "number": 4,
      "content": "Tức các chi tộc của CHÚA,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.564Z",
      "updatedAt": "2024-09-07T14:21:31.564Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "515ab77d-244e-4de9-a1fb-0020776e9e78",
      "number": 4,
      "content": "Theo như luật định cho I-sơ-ra-ên,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.564Z",
      "updatedAt": "2024-09-07T14:21:31.564Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "ad25d190-4abc-41cf-8034-0c93b47a64ef",
      "number": 4,
      "content": "Để cảm tạ danh CHÚA,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.564Z",
      "updatedAt": "2024-09-07T14:21:31.564Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "e0d74623-3945-49fc-a321-93bcecc30e8a",
      "number": 5,
      "content": "Vì tại đó đã thiết lập các ngai đoán xét,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.567Z",
      "updatedAt": "2024-09-07T14:21:31.567Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "5f8c04fa-05fa-4ad8-9669-482156e8031e",
      "number": 5,
      "content": "Đó là các ngai của nhà Đa-vít.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.567Z",
      "updatedAt": "2024-09-07T14:21:31.567Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "0aa01f26-fb08-421d-8052-2379612a4f7d",
      "number": 6,
      "content": "Hãy cầu hòa bình cho Giê-ru-sa-lem:",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.569Z",
      "updatedAt": "2024-09-07T14:21:31.569Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "97caf116-eaa8-4fd2-adce-2897a99dde35",
      "number": 6,
      "content": "“Nguyện những ai yêu mến ngươi được thịnh vượng.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.569Z",
      "updatedAt": "2024-09-07T14:21:31.569Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "bac7b3f9-e890-404f-a37d-5fd9617a49de",
      "number": 7,
      "content": "Nguyện bình an ở phía trong tường ngươi.",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.571Z",
      "updatedAt": "2024-09-07T14:21:31.571Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "5687870a-7f9c-4caf-91af-d016828833d4",
      "number": 7,
      "content": "Nguyện giàu sang phú quý ở trong các cung điện ngươi.”",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.571Z",
      "updatedAt": "2024-09-07T14:21:31.571Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "7bcd25e8-814d-4965-bd11-2c86e52c5628",
      "number": 8,
      "content": "Nay vì cớ anh chị em ta và các bạn hữu ta,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.574Z",
      "updatedAt": "2024-09-07T14:21:31.574Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "6ed86af9-2d1d-48f9-9dd6-15930235498a",
      "number": 8,
      "content": "Ta cầu chúc ngươi rằng, “Chúc ngươi luôn được bình an thịnh vượng.”",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.574Z",
      "updatedAt": "2024-09-07T14:21:31.574Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "ca95a688-9ccd-4eef-b2c9-ca7cfff2e5a8",
      "number": 9,
      "content": "Vì cớ nhà CHÚA, Đức Chúa Trời chúng ta,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.576Z",
      "updatedAt": "2024-09-07T14:21:31.576Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    },
    {
      "id": "754bff47-d85a-4ca4-9539-d7e27f209ce4",
      "number": 9,
      "content": "Ta cầu phước cho ngươi.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:31.576Z",
      "updatedAt": "2024-09-07T14:21:31.576Z",
      "chapterId": "81d11ff6-a1f3-4459-aeaa-83d9b4634460"
    }
  ],
