{
  "chapter": {
    "id": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3",
    "number": 1,
    "ref": "https://www.bible.com/bible/19/PSA.1.BD2011",
    "createdAt": "2024-09-07T11:51:10.615Z",
    "updatedAt": "2024-09-07T11:51:10.615Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "c3461d27-6a05-46f8-bff8-012bfc8a1d87",
      "number": 1,
      "content": "Phước cho người",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.354Z",
      "updatedAt": "2024-09-07T13:52:46.354Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "dcf0f87e-e136-4f35-b1ca-054e783da618",
      "number": 1,
      "content": "Không đi theo kế kẻ gian ác,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.354Z",
      "updatedAt": "2024-09-07T13:52:46.354Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "c3bf7b4d-9c40-41fb-b311-4f81765e52d7",
      "number": 1,
      "content": "Không đứng chung đường với phường tội lỗi,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.354Z",
      "updatedAt": "2024-09-07T13:52:46.354Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "75bb8bf3-4cd8-4575-b80b-b4b742f13236",
      "number": 1,
      "content": "Không ngồi cùng chỗ với đồ nhạo báng,",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.354Z",
      "updatedAt": "2024-09-07T13:52:46.354Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "fee16001-86ea-4852-b79b-c8cf892bb0ff",
      "number": 2,
      "content": "Nhưng vui thích về luật pháp của CHÚA,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.364Z",
      "updatedAt": "2024-09-07T13:52:46.364Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "13a0326c-15cd-403e-b20b-b342d453bcfe",
      "number": 2,
      "content": "Và suy gẫm luật pháp ấy ngày và đêm.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.364Z",
      "updatedAt": "2024-09-07T13:52:46.364Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "be790c66-df5f-46fd-a559-210db4dec859",
      "number": 3,
      "content": "Người ấy sẽ như cây",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.366Z",
      "updatedAt": "2024-09-07T13:52:46.366Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "72329c6e-450f-421b-b68b-547bd4b99d9f",
      "number": 3,
      "content": "Trồng gần dòng nước,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.366Z",
      "updatedAt": "2024-09-07T13:52:46.366Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "9b34a8f1-7a45-4e5b-89c4-1e277bf4068f",
      "number": 3,
      "content": "Cứ đến mùa được hoa quả đầy cành,",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.366Z",
      "updatedAt": "2024-09-07T13:52:46.366Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "d8f85299-e346-4edf-b266-8a5cf189635e",
      "number": 3,
      "content": "Lá vẫn xanh chứ chẳng hề héo úa;",
      "order": 3,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.366Z",
      "updatedAt": "2024-09-07T13:52:46.366Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "e8b19503-bcd2-409c-86cb-7c8bab5cd2b7",
      "number": 3,
      "content": "Mọi sự người ấy làm đều sẽ thành công.",
      "order": 4,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.366Z",
      "updatedAt": "2024-09-07T13:52:46.366Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "39d02305-7af1-4e35-8e30-28a5374562b9",
      "number": 4,
      "content": "Những kẻ gian ác sẽ không được như vậy,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.369Z",
      "updatedAt": "2024-09-07T13:52:46.369Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "7ed0ddd9-6d71-442b-ad73-062822c130e9",
      "number": 4,
      "content": "Nhưng chúng chẳng khác gì rác rến bị gió đùa đi.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.369Z",
      "updatedAt": "2024-09-07T13:52:46.369Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "442747a7-f195-4d35-b2d7-17d32d2ad09f",
      "number": 5,
      "content": "Vì thế những kẻ gian ác sẽ chẳng đứng nổi trong ngày phán xét;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.372Z",
      "updatedAt": "2024-09-07T13:52:46.372Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "a2cccc83-7e48-45a3-9a17-63a9ae2659b3",
      "number": 5,
      "content": "Những kẻ tội lỗi sẽ không được vào hội người ngay lành;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.372Z",
      "updatedAt": "2024-09-07T13:52:46.372Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "f938ff71-039c-4f46-9765-717098cf8a1d",
      "number": 6,
      "content": "Vì CHÚA biết rõ đường lối người ngay lành,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.377Z",
      "updatedAt": "2024-09-07T13:52:46.377Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    },
    {
      "id": "8d784b0d-ff9d-43c3-8d2e-b6fcf7f60427",
      "number": 6,
      "content": "Còn đường lối kẻ gian ác sẽ dẫn đến diệt vong.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:52:46.377Z",
      "updatedAt": "2024-09-07T13:52:46.377Z",
      "chapterId": "f2c4b092-3d5a-40a9-b7e7-0e5cf13dcfe3"
    }
  ],
