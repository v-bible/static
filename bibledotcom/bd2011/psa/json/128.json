  },
  "verses": [
    {
      "id": "38a4de6c-6dac-4eb5-bea7-25160d0b2529",
      "number": 1,
      "content": "Phước cho người kính sợ CHÚA,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.924Z",
      "updatedAt": "2024-09-07T14:23:09.924Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "d4d3cde1-6fff-447f-9e6c-c4d22ab52809",
      "number": 1,
      "content": "Người đi trong đường lối Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.924Z",
      "updatedAt": "2024-09-07T14:23:09.924Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "e04e5067-838d-4eb2-9355-d4b626c8a613",
      "number": 2,
      "content": "Bạn sẽ hưởng hoa lợi của tay mình;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.934Z",
      "updatedAt": "2024-09-07T14:23:09.934Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "fee66bb0-93c1-4ad1-b56e-6aa95ea54f20",
      "number": 2,
      "content": "Bạn sẽ được phước và thịnh vượng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.934Z",
      "updatedAt": "2024-09-07T14:23:09.934Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "abbc580e-a776-4a3d-a153-abcbb5cd1c24",
      "number": 3,
      "content": "Vợ bạn sẽ như cây nho sai trái trong nhà bạn;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.936Z",
      "updatedAt": "2024-09-07T14:23:09.936Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "caf3d6d5-e8e9-4759-8713-a27ba0d4cbbe",
      "number": 3,
      "content": "Con cái bạn sẽ như các chồi ô-liu quanh bàn bạn.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.936Z",
      "updatedAt": "2024-09-07T14:23:09.936Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "4cf5ec54-9593-4999-bda9-364118a5ef8d",
      "number": 4,
      "content": "Kìa, người nào kính sợ CHÚA",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.939Z",
      "updatedAt": "2024-09-07T14:23:09.939Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "1f173c10-e00a-42b3-861e-d6ffe93e4568",
      "number": 4,
      "content": "Sẽ được phước như vậy.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.939Z",
      "updatedAt": "2024-09-07T14:23:09.939Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "5065310f-0844-487c-b778-f10306fcee61",
      "number": 5,
      "content": "Nguyện CHÚA ban phước cho bạn từ Si-ôn.",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.941Z",
      "updatedAt": "2024-09-07T14:23:09.941Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "ecaa89ac-118a-4ba9-8732-280309a797be",
      "number": 5,
      "content": "Nguyện suốt đời bạn",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.941Z",
      "updatedAt": "2024-09-07T14:23:09.941Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "5aa9d99e-49a7-47bc-b8c2-9f4cf73fd0fd",
      "number": 5,
      "content": "Chứng kiến những phước hạnh của Giê-ru-sa-lem.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.941Z",
      "updatedAt": "2024-09-07T14:23:09.941Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "d06316b3-e05b-408b-b217-05010b2e61e1",
      "number": 6,
      "content": "Nguyện bạn được nhìn thấy con cháu của con cái bạn.",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.943Z",
      "updatedAt": "2024-09-07T14:23:09.943Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    },
    {
      "id": "37dadef4-cf1f-475b-bea9-9e1e51749c9e",
      "number": 6,
      "content": "Nguyện hòa bình thịnh vượng đến trên I-sơ-ra-ên.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:09.943Z",
      "updatedAt": "2024-09-07T14:23:09.943Z",
      "chapterId": "4f08bd62-9c12-4140-8580-f16709fa106c"
    }
  ],
