{
  "chapter": {
    "id": "4f08bd62-9c12-4140-8580-f16709fa106c",
    "number": 128,
    "ref": "https://www.bible.com/bible/19/PSA.128.BD2011",
    "createdAt": "2024-09-07T11:51:11.541Z",
