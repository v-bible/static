{
  "chapter": {
    "id": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f",
    "number": 111,
    "ref": "https://www.bible.com/bible/19/PSA.111.BD2011",
    "createdAt": "2024-09-07T11:51:11.491Z",
    "updatedAt": "2024-09-07T11:51:11.491Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "6dbc1934-eb15-4175-bf1c-a1613235a5cd",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.228Z",
      "updatedAt": "2024-09-07T14:18:10.228Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "b93a493f-50a9-4568-878f-9fb4241a4049",
      "number": 1,
      "content": "Tôi sẽ hết lòng cảm tạ CHÚA,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.228Z",
      "updatedAt": "2024-09-07T14:18:10.228Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "e1925716-0c31-45d0-a2ad-7e2e627c2494",
      "number": 1,
      "content": "Giữa đại hội của người chính trực và giữa hội chúng.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.228Z",
      "updatedAt": "2024-09-07T14:18:10.228Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "2f35dd1a-ad1b-439a-a3d6-24def5ae7aec",
      "number": 2,
      "content": "Những công việc của CHÚA thật lớn lao thay.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.231Z",
      "updatedAt": "2024-09-07T14:18:10.231Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "75d252b5-7dd8-47fb-8411-de1827e3a3ab",
      "number": 2,
      "content": "Phàm ai ái mộ ắt phải suy nghĩ.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.231Z",
      "updatedAt": "2024-09-07T14:18:10.231Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "15b8a063-743c-4bd5-8930-a0f1da05a7cb",
      "number": 3,
      "content": "Công việc Ngài thật huy hoàng và tráng lệ.",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.233Z",
      "updatedAt": "2024-09-07T14:18:10.233Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "3964b1b7-7f19-4e05-a75c-987790f99667",
      "number": 3,
      "content": "Đức công chính Ngài còn đến đời đời.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.233Z",
      "updatedAt": "2024-09-07T14:18:10.233Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "a7d8176b-d7e5-4c9a-8738-b3f5c90b1451",
      "number": 4,
      "content": "Những việc lạ lùng Ngài làm sẽ được ghi nhớ mãi.",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.235Z",
      "updatedAt": "2024-09-07T14:18:10.235Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "c6d9652e-a0c9-47db-a27c-139133448cfb",
      "number": 4,
      "content": "CHÚA là Đấng khoan dung độ lượng và thương xót.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.235Z",
      "updatedAt": "2024-09-07T14:18:10.235Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "1acd30b3-36f7-46b7-9885-46a6a3b3464d",
      "number": 5,
      "content": "Ngài ban thực phẩm cho những người kính sợ Ngài.",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.238Z",
      "updatedAt": "2024-09-07T14:18:10.238Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "d8fb17ec-d6b9-452b-967f-6e341119c686",
      "number": 5,
      "content": "Ngài ghi nhớ giao ước Ngài mãi mãi.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.238Z",
      "updatedAt": "2024-09-07T14:18:10.238Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "8e7d7751-27f6-4448-ba38-cd3e8dd077e0",
      "number": 6,
      "content": "Ngài bày tỏ cho dân Ngài mãnh lực của công việc Ngài,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.240Z",
      "updatedAt": "2024-09-07T14:18:10.240Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "fbfb4eaa-0099-4b41-829f-9cf81d4e1394",
      "number": 6,
      "content": "Khi Ngài ban cho họ sản nghiệp của các dân.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.240Z",
      "updatedAt": "2024-09-07T14:18:10.240Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "542249bc-0a26-4d14-8a73-46afe79328a5",
      "number": 7,
      "content": "Các việc của tay Ngài đều chính đáng và công bình.",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.244Z",
      "updatedAt": "2024-09-07T14:18:10.244Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "071eddd4-a57d-418b-8173-85c23b90585f",
      "number": 7,
      "content": "Tất cả giới luật Ngài đều đáng tin cậy hoàn toàn.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.244Z",
      "updatedAt": "2024-09-07T14:18:10.244Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "8a3f5432-e879-4a8b-ae93-d0a95d6dcea0",
      "number": 8,
      "content": "Các giới luật ấy được vững lập đời đời vô cùng,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.246Z",
      "updatedAt": "2024-09-07T14:18:10.246Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "853a3062-d3e1-49ac-b0be-43bdff8115d3",
      "number": 8,
      "content": "Và được thực thi cách chân thành và ngay thẳng.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.246Z",
      "updatedAt": "2024-09-07T14:18:10.246Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "0f573854-2652-49c5-b835-5763ccd45522",
      "number": 9,
      "content": "Ngài đã ban ơn cứu chuộc cho con dân Ngài.",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.249Z",
      "updatedAt": "2024-09-07T14:18:10.249Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "e10cdab3-ceda-4552-8372-4db6bdf4956b",
      "number": 9,
      "content": "Ngài đã lập giao ước Ngài để nó trường tồn mãi mãi.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.249Z",
      "updatedAt": "2024-09-07T14:18:10.249Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "6c94bef1-a1a0-48a6-ae42-7c1ec03508ad",
      "number": 9,
      "content": "Danh Ngài là thánh và đáng tôn kính thay!",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.249Z",
      "updatedAt": "2024-09-07T14:18:10.249Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "28702256-4373-4b06-9340-5f06b061836f",
      "number": 10,
      "content": "Kính sợ CHÚA là khởi đầu của khôn ngoan.",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.251Z",
      "updatedAt": "2024-09-07T14:18:10.251Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "cfc6da30-285e-4b9c-a3d6-00830c2d620a",
      "number": 10,
      "content": "Tất cả những ai làm thế đều là người thông sáng.",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.251Z",
      "updatedAt": "2024-09-07T14:18:10.251Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    },
    {
      "id": "a043e391-5f80-4a95-9f50-5d7d7d0b8ea7",
      "number": 10,
      "content": "Nguyện sự ca ngợi Ngài tồn tại đời đời.",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:10.251Z",
      "updatedAt": "2024-09-07T14:18:10.251Z",
      "chapterId": "f4539408-bfc4-4c61-83f9-f6ca0986fd9f"
    }
  ],
