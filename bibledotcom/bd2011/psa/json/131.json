  },
  "verses": [
    {
      "id": "5b3a3e9d-f7ca-439b-a80c-04de4cd9bdb7",
      "number": 1,
      "content": "CHÚA ôi, lòng con không dám tự cao;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:29.503Z",
      "updatedAt": "2024-09-07T14:23:29.503Z",
      "chapterId": "4db10064-3923-4069-b6c9-bd186148c573"
    },
    {
      "id": "23ea9572-b4ed-4c5a-8859-777d3f0dc9d4",
      "number": 1,
      "content": "Mắt con chẳng dám kiêu kỳ;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:29.503Z",
      "updatedAt": "2024-09-07T14:23:29.503Z",
      "chapterId": "4db10064-3923-4069-b6c9-bd186148c573"
    },
    {
      "id": "557b3158-e105-417f-8803-085947298025",
      "number": 1,
      "content": "Con không dám tham dự vào những việc lớn lao quá sức,",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:29.503Z",
      "updatedAt": "2024-09-07T14:23:29.503Z",
      "chapterId": "4db10064-3923-4069-b6c9-bd186148c573"
    },
    {
      "id": "79278c45-1758-402c-aaa4-cb5139dbfb4a",
      "number": 1,
      "content": "Hay những việc vượt quá khả năng mình.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:29.503Z",
      "updatedAt": "2024-09-07T14:23:29.503Z",
      "chapterId": "4db10064-3923-4069-b6c9-bd186148c573"
    },
    {
      "id": "fff24de8-efc3-46b2-9eae-46c95f00752b",
      "number": 2,
      "content": "Nhưng con đã tự trấn tĩnh và làm cho linh hồn mình lắng dịu,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:29.514Z",
      "updatedAt": "2024-09-07T14:23:29.514Z",
      "chapterId": "4db10064-3923-4069-b6c9-bd186148c573"
    },
    {
      "id": "a97d7125-910f-4000-83e8-4ce5cfd388e8",
      "number": 2,
      "content": "Như trẻ thơ no sữa nép mình bên lòng mẹ;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:29.514Z",
      "updatedAt": "2024-09-07T14:23:29.514Z",
      "chapterId": "4db10064-3923-4069-b6c9-bd186148c573"
    },
    {
      "id": "b87cd6f0-2941-493c-90fc-2d3b9522e840",
      "number": 2,
      "content": "Linh hồn con ở trong con như một em bé vừa no sữa mẹ.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:29.514Z",
      "updatedAt": "2024-09-07T14:23:29.514Z",
      "chapterId": "4db10064-3923-4069-b6c9-bd186148c573"
    },
    {
      "id": "7d620ab6-5c33-4b3c-a4bf-eb76bd93ea82",
      "number": 3,
      "content": "Hỡi I-sơ-ra-ên, hãy hy vọng nơi CHÚA,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:29.518Z",
      "updatedAt": "2024-09-07T14:23:29.518Z",
      "chapterId": "4db10064-3923-4069-b6c9-bd186148c573"
    },
    {
      "id": "62c87181-105f-4e09-bde5-8abc5f04ab45",
      "number": 3,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:23:29.518Z",
      "updatedAt": "2024-09-07T14:23:29.518Z",
      "chapterId": "4db10064-3923-4069-b6c9-bd186148c573"
    }
  ],
