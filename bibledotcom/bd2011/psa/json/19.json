{
  "chapter": {
    "id": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed",
    "number": 19,
    "ref": "https://www.bible.com/bible/19/PSA.19.BD2011",
    "createdAt": "2024-09-07T11:51:10.771Z",
    "updatedAt": "2024-09-07T11:51:10.771Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "66468e53-de70-490b-b21e-9d7d89a5384c",
      "number": 1,
      "content": "Các tầng trời rao truyền vinh hiển Đức Chúa Trời;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.717Z",
      "updatedAt": "2024-09-07T13:59:57.717Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "441a8790-dcc2-4703-ab5f-e00772b7d4fe",
      "number": 1,
      "content": "Bầu trời công bố công việc tay Ngài làm.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.717Z",
      "updatedAt": "2024-09-07T13:59:57.717Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "e1c78342-5ed2-4b01-946b-25ed85270a2d",
      "number": 2,
      "content": "Ngày ngày truyền rao sứ điệp;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.727Z",
      "updatedAt": "2024-09-07T13:59:57.727Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "a12ea135-184b-42cb-89dc-13ebe59803e1",
      "number": 2,
      "content": "Đêm đêm bày tỏ tri thức.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.727Z",
      "updatedAt": "2024-09-07T13:59:57.727Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "7cc35860-0208-49bf-b370-6af39258dbed",
      "number": 3,
      "content": "Không dùng lời, chẳng cần tiếng;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.731Z",
      "updatedAt": "2024-09-07T13:59:57.731Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "6271dade-d9b0-4faf-868a-71f41cc84ecf",
      "number": 3,
      "content": "Chẳng ai nghe giọng nói chúng ra sao;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.731Z",
      "updatedAt": "2024-09-07T13:59:57.731Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "09dafd01-d2df-43fd-9d4a-192d6b8a42fe",
      "number": 4,
      "content": "Nhưng âm thanh chúng đã lan truyền khắp đất,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.735Z",
      "updatedAt": "2024-09-07T13:59:57.735Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "2800679c-2238-436f-ab6c-59710eadfc13",
      "number": 4,
      "content": "Lời chúng đã vang ra đến cùng tận địa cầu.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.735Z",
      "updatedAt": "2024-09-07T13:59:57.735Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "b4b1d659-3c70-4cba-8e67-09608daa869b",
      "number": 4,
      "content": "Ngài đã dựng lều cho mặt trời trong chúng.",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.735Z",
      "updatedAt": "2024-09-07T13:59:57.735Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "fb633200-57a3-4695-a395-19fa42c953e4",
      "number": 5,
      "content": "Mặt trời như một tân lang ra khỏi loan phòng,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.738Z",
      "updatedAt": "2024-09-07T13:59:57.738Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "375c43d4-c07b-4aa8-a039-0bd66eb6b0aa",
      "number": 5,
      "content": "Như một lực sĩ hân hoan chạy trên tuyến đường mình.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.738Z",
      "updatedAt": "2024-09-07T13:59:57.738Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "c1187159-24d2-43ee-b8e5-47c8f20a52af",
      "number": 6,
      "content": "Mặt trời mọc lên từ phương trời này,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.741Z",
      "updatedAt": "2024-09-07T13:59:57.741Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "9a7dc62c-9c76-4e09-a9ed-8ab7bc234b60",
      "number": 6,
      "content": "Chạy vòng đến phương trời kia;",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.741Z",
      "updatedAt": "2024-09-07T13:59:57.741Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "265d7e5c-1161-4db2-a126-4db2ead7f76e",
      "number": 6,
      "content": "Không vật gì tránh khỏi hơi nóng của nó được.",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.741Z",
      "updatedAt": "2024-09-07T13:59:57.741Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "c4aac759-1a44-4f45-b352-68d18cdf73c3",
      "number": 7,
      "content": "Luật pháp của CHÚA thật trọn vẹn,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.744Z",
      "updatedAt": "2024-09-07T13:59:57.744Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "0b61211b-a506-4644-8bc7-188f7e46c8c1",
      "number": 7,
      "content": "Làm linh hồn hồi sức;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.744Z",
      "updatedAt": "2024-09-07T13:59:57.744Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "080e97d8-1366-456a-a519-5f43991de000",
      "number": 7,
      "content": "Quy tắc của CHÚA thật vững chắc,",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.744Z",
      "updatedAt": "2024-09-07T13:59:57.744Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "25ace70b-35f9-4a4e-bd88-f1b0d3e2d3e7",
      "number": 7,
      "content": "Khiến kẻ dại khôn ra.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.744Z",
      "updatedAt": "2024-09-07T13:59:57.744Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "bfa3e7af-31df-4dde-b9b9-e58aa764741d",
      "number": 8,
      "content": "Luật lệ của CHÚA thật đúng đắn,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.748Z",
      "updatedAt": "2024-09-07T13:59:57.748Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "470e8690-f336-4713-9cc4-a97835bc016b",
      "number": 8,
      "content": "Làm cho lòng vui vẻ;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.748Z",
      "updatedAt": "2024-09-07T13:59:57.748Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "65b52fa4-cc69-4366-9508-e3f9a17c06c6",
      "number": 8,
      "content": "Điều răn của CHÚA thật minh bạch,",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.748Z",
      "updatedAt": "2024-09-07T13:59:57.748Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "f2723e39-0665-47e8-8714-5f791dfa4b83",
      "number": 8,
      "content": "Khiến mắt lòng thấy rõ.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.748Z",
      "updatedAt": "2024-09-07T13:59:57.748Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "09edd504-38ed-4487-9c3e-14308672302b",
      "number": 9,
      "content": "Sự kính sợ CHÚA thật trong sạch,",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.752Z",
      "updatedAt": "2024-09-07T13:59:57.752Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "1cb9f4d3-46ee-46a2-beba-2c2b6cd36a4a",
      "number": 9,
      "content": "Nên còn lại đời đời;",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.752Z",
      "updatedAt": "2024-09-07T13:59:57.752Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "26881291-6a3a-45fb-9ba5-ebab1af89aba",
      "number": 9,
      "content": "Phán quyết của CHÚA thật chính đáng,",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.752Z",
      "updatedAt": "2024-09-07T13:59:57.752Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "d44db532-9ee8-43c0-b55d-de609fb22f73",
      "number": 9,
      "content": "Thảy đều công bình cả.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.752Z",
      "updatedAt": "2024-09-07T13:59:57.752Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "df2d0545-4762-42a9-9e74-1a761b3eb89e",
      "number": 10,
      "content": "Những điều ấy quý hơn vàng,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.755Z",
      "updatedAt": "2024-09-07T13:59:57.755Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "b9c8b381-991d-4c67-b115-9f9616634717",
      "number": 10,
      "content": "Thật báu hơn vàng ròng;",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.755Z",
      "updatedAt": "2024-09-07T13:59:57.755Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "6f3bb23f-badb-4be8-a812-c543a065c1b9",
      "number": 10,
      "content": "Chúng ngọt hơn mật,",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.755Z",
      "updatedAt": "2024-09-07T13:59:57.755Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "716cd0cc-ea5d-419d-a73b-552aba927bea",
      "number": 10,
      "content": "Thật ngọt hơn mật nhểu từ tàng ong.",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.755Z",
      "updatedAt": "2024-09-07T13:59:57.755Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "6ff18f36-2092-4dda-be10-5fe333c836aa",
      "number": 11,
      "content": "Hơn nữa nhờ những điều ấy, đầy tớ Ngài được cảnh giác;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.759Z",
      "updatedAt": "2024-09-07T13:59:57.759Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "3b4f97ca-b6fb-407a-9029-f182e264aea8",
      "number": 11,
      "content": "Nhờ giữ những điều ấy, tôi tớ Ngài được ban thưởng lớn lao.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.759Z",
      "updatedAt": "2024-09-07T13:59:57.759Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "d57a26b6-ed49-4bd1-b686-5b43f5565f63",
      "number": 12,
      "content": "Ai có thể biết hết mọi lầm lỗi của mình chăng?",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.762Z",
      "updatedAt": "2024-09-07T13:59:57.762Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "60df07f6-b559-4cee-be52-d728fea81f32",
      "number": 12,
      "content": "Cầu xin Ngài tha thứ con vì những lỗi lầm con không biết.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.762Z",
      "updatedAt": "2024-09-07T13:59:57.762Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "3bf56fc7-a1c7-43f5-951b-88743150337f",
      "number": 13,
      "content": "Xin giữ tôi tớ Ngài khỏi cố ý phạm tội;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.766Z",
      "updatedAt": "2024-09-07T13:59:57.766Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "ef6680cd-9578-4f48-b000-83c493a692f1",
      "number": 13,
      "content": "Xin đừng để tội lỗi cầm quyền trên con;",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.766Z",
      "updatedAt": "2024-09-07T13:59:57.766Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "80533b98-80a1-4208-a49c-9e95bc126f91",
      "number": 13,
      "content": "Để con sẽ không có chỗ nào đáng trách,",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.766Z",
      "updatedAt": "2024-09-07T13:59:57.766Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "403682a3-7617-4c0f-b338-6f5e413289d9",
      "number": 13,
      "content": "Và con sẽ không phạm tội trọng.",
      "order": 3,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.766Z",
      "updatedAt": "2024-09-07T13:59:57.766Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "5d51b2b7-bf72-4581-a018-205d14b96b33",
      "number": 14,
      "content": "Nguyện lời nói của miệng con và sự suy gẫm của lòng con",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.769Z",
      "updatedAt": "2024-09-07T13:59:57.769Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
    {
      "id": "eb2d8b37-9522-4a81-b5c2-785b646cef56",
      "number": 14,
      "content": "Được đẹp ý Ngài,",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:59:57.769Z",
      "updatedAt": "2024-09-07T13:59:57.769Z",
      "chapterId": "e055a3c8-6348-4a96-b07b-ae1d8ab850ed"
    },
