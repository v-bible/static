{
  "chapter": {
    "id": "2d3561a8-a743-459e-b9d8-dc772657a7e0",
    "number": 142,
    "ref": "https://www.bible.com/bible/19/PSA.142.BD2011",
    "createdAt": "2024-09-07T11:51:11.577Z",
    "updatedAt": "2024-09-07T11:51:11.577Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "c94185e7-3a3c-4cca-ada1-26f1aa89ec45",
      "number": 1,
      "content": "Tôi cất tiếng kêu cầu với CHÚA;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.216Z",
      "updatedAt": "2024-09-07T14:27:44.216Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "ec8e0957-d170-4825-8f65-6d3892cd9579",
      "number": 1,
      "content": "Tôi cất tiếng dâng lên CHÚA lời cầu nguyện của tôi.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.216Z",
      "updatedAt": "2024-09-07T14:27:44.216Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "cc0fa7e3-6e38-40bb-b7b7-9aa8f7d6f48c",
      "number": 2,
      "content": "Tôi dốc đổ phiền muộn của lòng tôi ra trước mặt Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.225Z",
      "updatedAt": "2024-09-07T14:27:44.225Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "12d755ee-0ea9-42fb-8bc1-2c83621bae44",
      "number": 2,
      "content": "Tôi trình dâng nỗi khổ của tôi lên trước thánh nhan Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.225Z",
      "updatedAt": "2024-09-07T14:27:44.225Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "532157c1-543d-4bfb-9848-4f81964ce3e2",
      "number": 3,
      "content": "Khi tâm linh con nao sờn ở trong con,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.228Z",
      "updatedAt": "2024-09-07T14:27:44.228Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "67ea6c55-1714-490b-83ee-aa1a0ae0acd9",
      "number": 3,
      "content": "Thì chính Ngài biết rõ lối con đi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.228Z",
      "updatedAt": "2024-09-07T14:27:44.228Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "ecaf4eeb-3c54-443b-89f3-e325591908a8",
      "number": 3,
      "content": "Trong con đường con tiến bước,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.228Z",
      "updatedAt": "2024-09-07T14:27:44.228Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "467605d6-b298-4cd2-ba14-8704e4478381",
      "number": 3,
      "content": "Chúng đã âm thầm gài bẫy hại con.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.228Z",
      "updatedAt": "2024-09-07T14:27:44.228Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "6a06d482-e0b6-4786-a744-c12b7343b668",
      "number": 4,
      "content": "Xin Ngài nhìn bên phải con và xem;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.234Z",
      "updatedAt": "2024-09-07T14:27:44.234Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "1e790f5f-7f78-46b9-bac1-05d5b7e33ac2",
      "number": 4,
      "content": "Chẳng ai ngó ngàng gì đến con cả.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.234Z",
      "updatedAt": "2024-09-07T14:27:44.234Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "224ba5e2-ecbd-4f6f-9d9f-7f18f23cdca0",
      "number": 4,
      "content": "Con chẳng còn một lối thoát nào;",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.234Z",
      "updatedAt": "2024-09-07T14:27:44.234Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "9d5d8577-37a6-4976-a298-d7d4246aea8a",
      "number": 4,
      "content": "Thật chẳng ai quan tâm gì đến linh hồn con cả.",
      "order": 3,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.234Z",
      "updatedAt": "2024-09-07T14:27:44.234Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "54658564-d0bf-4390-80c2-4d8a376610bc",
      "number": 5,
      "content": "CHÚA ôi, con kêu cầu Ngài;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.239Z",
      "updatedAt": "2024-09-07T14:27:44.239Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "1a6a4a9a-d48c-40a5-8167-04e3aad40823",
      "number": 5,
      "content": "Con nói, “Ngài là nơi ẩn náu của con,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.239Z",
      "updatedAt": "2024-09-07T14:27:44.239Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "59885ae3-4b71-4ed1-b0da-2348b594cb56",
      "number": 5,
      "content": "Và là phần của con trong cõi người sống.",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.239Z",
      "updatedAt": "2024-09-07T14:27:44.239Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "5f25cd24-b6a0-4ecd-8c4b-6ce6b753c99a",
      "number": 6,
      "content": "Xin đoái đến tiếng kêu cầu của con,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.242Z",
      "updatedAt": "2024-09-07T14:27:44.242Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "6a27ad43-052b-48cb-9c80-c5b04e224b5c",
      "number": 6,
      "content": "Vì con khốn khổ vô cùng;",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.242Z",
      "updatedAt": "2024-09-07T14:27:44.242Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "fd365235-bd55-4bc4-9ee4-0aa22d01d0ce",
      "number": 6,
      "content": "Xin cứu con khỏi những kẻ bách hại con,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.242Z",
      "updatedAt": "2024-09-07T14:27:44.242Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "16c3d4cc-c8d3-46f3-abe7-fe79828193d9",
      "number": 6,
      "content": "Vì chúng mạnh hơn con.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.242Z",
      "updatedAt": "2024-09-07T14:27:44.242Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "6a5864b5-d782-4174-96ba-d255735f9b8b",
      "number": 7,
      "content": "Xin đem linh hồn con ra khỏi ngục tù,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.244Z",
      "updatedAt": "2024-09-07T14:27:44.244Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "cffdb36a-74b1-4411-97ab-8ce8381a4106",
      "number": 7,
      "content": "Để con có thể ca ngợi danh Ngài;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.244Z",
      "updatedAt": "2024-09-07T14:27:44.244Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "80766b00-3d6c-43dc-8995-1d90166a4078",
      "number": 7,
      "content": "Rồi những người ngay lành sẽ tụ lại quanh con,",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.244Z",
      "updatedAt": "2024-09-07T14:27:44.244Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    },
    {
      "id": "1ac11466-ce30-445a-8275-3ce3f3e7a6de",
      "number": 7,
      "content": "Vì Ngài hậu đãi con.”",
      "order": 3,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:27:44.244Z",
      "updatedAt": "2024-09-07T14:27:44.244Z",
      "chapterId": "2d3561a8-a743-459e-b9d8-dc772657a7e0"
    }
  ],
