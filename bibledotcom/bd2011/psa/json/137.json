{
  "chapter": {
    "id": "f7c94738-ead6-42b4-9060-54cd229bda43",
    "number": 137,
    "ref": "https://www.bible.com/bible/19/PSA.137.BD2011",
    "createdAt": "2024-09-07T11:51:11.565Z",
    "updatedAt": "2024-09-07T11:51:11.565Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "802d1f92-760c-4221-95b0-c4d011e7173b",
      "number": 1,
      "content": "Chúng tôi ngồi bên bờ sông ở Ba-by-lôn;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.835Z",
      "updatedAt": "2024-09-07T14:25:12.835Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "406ee991-aa39-4580-af8f-a81902c8dcb2",
      "number": 1,
      "content": "Chúng tôi nhớ đến Si-ôn và khóc.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.835Z",
      "updatedAt": "2024-09-07T14:25:12.835Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "83ec24ef-96c5-488c-a751-0bcf92cc26fd",
      "number": 2,
      "content": "Chúng tôi treo cây đàn của mình",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.837Z",
      "updatedAt": "2024-09-07T14:25:12.837Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "85cd925a-0275-4510-bb2a-5ef6bb551709",
      "number": 2,
      "content": "Trên cành liễu bên sông,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.837Z",
      "updatedAt": "2024-09-07T14:25:12.837Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "31d3f8b1-3ad7-4e14-bc3d-8332dffc6e53",
      "number": 3,
      "content": "Vì những kẻ đem chúng tôi lưu đày",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.840Z",
      "updatedAt": "2024-09-07T14:25:12.840Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "99586525-0747-4e2a-8279-639127912efb",
      "number": 3,
      "content": "Bắt chúng tôi phải hát;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.840Z",
      "updatedAt": "2024-09-07T14:25:12.840Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "5b105efc-30fd-483f-97d4-99882ba3b671",
      "number": 3,
      "content": "Những kẻ hành hạ chúng tôi bắt chúng tôi phải ca;",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.840Z",
      "updatedAt": "2024-09-07T14:25:12.840Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "99d0c9c0-784c-452d-b780-daa680e473ee",
      "number": 3,
      "content": "Chúng bảo, “Hãy hát cho chúng tôi nghe những bài ca của Si-ôn.”",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.840Z",
      "updatedAt": "2024-09-07T14:25:12.840Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "bf49eb31-e89b-4701-9caa-b48399c06e9f",
      "number": 4,
      "content": "Nơi đất khách quê người,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.843Z",
      "updatedAt": "2024-09-07T14:25:12.843Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "508f718d-6f51-489a-b132-23a78c9a9780",
      "number": 4,
      "content": "Làm sao chúng tôi có thể hát được bài ca tôn ngợi CHÚA?",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.843Z",
      "updatedAt": "2024-09-07T14:25:12.843Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "ab8c241c-94a9-4599-8ee4-8c95cdefa8c2",
      "number": 5,
      "content": "Hỡi Giê-ru-sa-lem, nếu ta quên ngươi,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.845Z",
      "updatedAt": "2024-09-07T14:25:12.845Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "fdf0dbc5-251b-44f1-bd37-3d5d12616e2b",
      "number": 5,
      "content": "Nguyện tay phải ta quên tài năng của nó.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.845Z",
      "updatedAt": "2024-09-07T14:25:12.845Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "18a998fa-14f2-415c-8d55-1522eabb7c3c",
      "number": 6,
      "content": "Nguyện lưỡi ta dính cứng vào hốc miệng ta,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.848Z",
      "updatedAt": "2024-09-07T14:25:12.848Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "56a03945-8bdc-4c1a-8542-291c08e1b079",
      "number": 6,
      "content": "Nếu ta không còn nhớ đến ngươi,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.848Z",
      "updatedAt": "2024-09-07T14:25:12.848Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "61544ca1-0bb6-4f47-8e2f-991d9254a9f2",
      "number": 6,
      "content": "Nếu ta không thích Giê-ru-sa-lem",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.848Z",
      "updatedAt": "2024-09-07T14:25:12.848Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "77d44b5a-e35e-462c-bacd-889f89de59bf",
      "number": 6,
      "content": "Hơn điều gì vui nhất của ta,",
      "order": 3,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.848Z",
      "updatedAt": "2024-09-07T14:25:12.848Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "05e596c2-41a9-4e94-a5eb-bf7da9495144",
      "number": 7,
      "content": "CHÚA ôi, xin nhớ lại những lời dân Ê-đôm đã nói",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.851Z",
      "updatedAt": "2024-09-07T14:25:12.851Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "4f42b3d2-239e-4104-bc53-18ee6692dac6",
      "number": 7,
      "content": "Trong ngày Giê-ru-sa-lem bị thất thủ,",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.851Z",
      "updatedAt": "2024-09-07T14:25:12.851Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "1e4dcf85-1b43-4fb2-a2c0-7a374ef74282",
      "number": 7,
      "content": "Chúng bảo, “Hãy san bằng nó.",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.851Z",
      "updatedAt": "2024-09-07T14:25:12.851Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "456e32a2-1c6b-438c-a1e9-49a9ec9a594d",
      "number": 7,
      "content": "Hãy san bằng nó thành bình địa.”",
      "order": 3,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.851Z",
      "updatedAt": "2024-09-07T14:25:12.851Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "d55dce8c-19ea-46f7-b091-c3be43306e9c",
      "number": 8,
      "content": "Hỡi con gái của Ba-by-lôn, kẻ sẽ bị tiêu diệt,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.856Z",
      "updatedAt": "2024-09-07T14:25:12.856Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "b55f8f47-5219-4356-9d9a-6cb69187a870",
      "number": 8,
      "content": "Phước cho người nào báo trả ngươi,",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.856Z",
      "updatedAt": "2024-09-07T14:25:12.856Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "6e3dec05-5c42-4d60-ac80-247c3ee8db9f",
      "number": 8,
      "content": "Về những đau khổ ngươi gây cho chúng ta.",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.856Z",
      "updatedAt": "2024-09-07T14:25:12.856Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "d1aa2947-4e5d-43f4-be86-1ea1199e6777",
      "number": 9,
      "content": "Phước cho người nào bắt các con thơ của ngươi,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.859Z",
      "updatedAt": "2024-09-07T14:25:12.859Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    },
    {
      "id": "9bb9185e-cbdf-44ef-9cca-e1a74469f1ea",
      "number": 9,
      "content": "Và đập chúng vào các gành đá!",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:25:12.859Z",
      "updatedAt": "2024-09-07T14:25:12.859Z",
      "chapterId": "f7c94738-ead6-42b4-9060-54cd229bda43"
    }
  ],
