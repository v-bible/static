{
  "chapter": {
    "id": "13b2a687-5355-4af7-bd88-9b45ab0e5f88",
    "number": 93,
    "ref": "https://www.bible.com/bible/19/PSA.93.BD2011",
    "createdAt": "2024-09-07T11:51:11.353Z",
    "updatedAt": "2024-09-07T11:51:11.353Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "0fb9e755-e05a-4f79-969a-25fe3e4b4d9e",
      "number": 1,
      "content": "CHÚA trị vì, Ngài đã mặc lấy vẻ uy nghi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.486Z",
      "updatedAt": "2024-09-07T14:15:46.486Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "2ba2beaa-6739-4f21-9c6d-05939a851b4f",
      "number": 1,
      "content": "CHÚA đã khoát nó trên mình và nai nịt cho Ngài bằng quyền năng.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.486Z",
      "updatedAt": "2024-09-07T14:15:46.486Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "62d8a747-fa1c-4b45-be67-50d6f6f9bf30",
      "number": 1,
      "content": "Ngài đã thiết lập trái đất vững vàng; nó sẽ không bị rúng động.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.486Z",
      "updatedAt": "2024-09-07T14:15:46.486Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "30b38b6d-e9ac-4900-9e45-26f2cf955b80",
      "number": 2,
      "content": "Ngai của Ngài đã được thiết lập tự ngàn xưa;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.496Z",
      "updatedAt": "2024-09-07T14:15:46.496Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "278a9f1c-a8c7-4163-877f-9ae3a67dd90d",
      "number": 2,
      "content": "Ngài đã hiện hữu từ vô cực trong quá khứ.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.496Z",
      "updatedAt": "2024-09-07T14:15:46.496Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "615b7e13-e8da-4b10-bc0e-7c7856e0ce4f",
      "number": 3,
      "content": "Đại dương đã dâng nước lên, CHÚA ôi,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.498Z",
      "updatedAt": "2024-09-07T14:15:46.498Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "e9b154fc-55b5-4736-8636-d13f2b34a3d9",
      "number": 3,
      "content": "Đại dương đã trỗi tiếng gầm vang;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.498Z",
      "updatedAt": "2024-09-07T14:15:46.498Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "8b19c901-0c36-4141-b675-2ca0184a79a6",
      "number": 3,
      "content": "Đại dương đã nổi dậy với hàng hàng lớp lớp ba đào dồn dập.",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.498Z",
      "updatedAt": "2024-09-07T14:15:46.498Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "e181975e-d39d-4136-8a8e-5f4087f772be",
      "number": 4,
      "content": "Mạnh hơn tiếng gầm thét của đại dương,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.502Z",
      "updatedAt": "2024-09-07T14:15:46.502Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "54b815aa-d921-415e-9925-8d2c76abfd27",
      "number": 4,
      "content": "Mạnh hơn những ngọn sóng bạc đầu của biển cả,",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.502Z",
      "updatedAt": "2024-09-07T14:15:46.502Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "0a3f5d50-8531-4d83-a47d-37dac75cf751",
      "number": 4,
      "content": "Là CHÚA, Đấng Toàn Năng đang ngự trên cao.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.502Z",
      "updatedAt": "2024-09-07T14:15:46.502Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "a1efbfde-543c-44e4-8af9-03d56fdca661",
      "number": 5,
      "content": "Các định luật của Ngài thật vững chãi;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.505Z",
      "updatedAt": "2024-09-07T14:15:46.505Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "7a38193a-a544-4131-8f7d-58ea40f3a0b6",
      "number": 5,
      "content": "Sự thánh khiết thật thích hiệp",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.505Z",
      "updatedAt": "2024-09-07T14:15:46.505Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    },
    {
      "id": "0836edcb-a755-453c-827f-31b6e7982b0e",
      "number": 5,
      "content": "Với nhà Ngài đời đời, CHÚA ôi.",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:15:46.505Z",
      "updatedAt": "2024-09-07T14:15:46.505Z",
      "chapterId": "13b2a687-5355-4af7-bd88-9b45ab0e5f88"
    }
  ],
