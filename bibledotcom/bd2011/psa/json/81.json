  "headings": [
    {
      "id": "22315035-e932-4133-93aa-cba97a614e94",
      "content": "Lòng Tốt của Đức Chúa Trời Đối Với Sự Lầm Lạc của Dân I-sơ-ra-ên",
      "order": 0,
      "createdAt": "2024-09-07T14:11:50.250Z",
      "updatedAt": "2024-09-07T14:11:50.250Z",
      "verseId": "b4c1fda5-167e-4f9a-9014-5aa32100d6ce",
