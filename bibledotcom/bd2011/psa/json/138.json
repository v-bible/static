{
  "chapter": {
    "id": "f3719a39-2f57-47a6-ad7c-1681722b71cb",
    "number": 138,
    "ref": "https://www.bible.com/bible/19/PSA.138.BD2011",
    "createdAt": "2024-09-07T11:51:11.567Z",
    "updatedAt": "2024-09-07T11:51:11.567Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "4fc29397-2ea4-4997-a907-c1bc8c1300a7",
      "number": 1,
      "content": "Con sẽ hết lòng ca ngợi Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.294Z",
      "updatedAt": "2024-09-07T14:26:00.294Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "5fdd3e15-870a-4ce7-a003-d494426f29c9",
      "number": 1,
      "content": "Con sẽ ca ngợi Ngài trước mặt các thần.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.294Z",
      "updatedAt": "2024-09-07T14:26:00.294Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "305843ed-77e1-4e34-8591-81edf9ac7277",
      "number": 2,
      "content": "Con sẽ hướng về đền thánh Ngài sấp mình thờ lạy và ca ngợi danh Ngài,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.297Z",
      "updatedAt": "2024-09-07T14:26:00.297Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "ff5cb14d-62c3-4db9-86e9-048940652dc1",
      "number": 2,
      "content": "Vì tình thương và đức thành tín của Ngài,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.297Z",
      "updatedAt": "2024-09-07T14:26:00.297Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "61abc039-00d2-4afa-9414-2920737a34f6",
      "number": 2,
      "content": "Vì Ngài đã làm cho lời Ngài",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.297Z",
      "updatedAt": "2024-09-07T14:26:00.297Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "56aba5b6-1881-409a-93ab-9302aaa1d821",
      "number": 2,
      "content": "Và danh Ngài được tôn cao trên mọi sự.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.297Z",
      "updatedAt": "2024-09-07T14:26:00.297Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "4381a808-7471-4e30-b205-bfa77f30e1f4",
      "number": 3,
      "content": "Vì trong ngày con kêu cầu Ngài đã đáp lời con;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.300Z",
      "updatedAt": "2024-09-07T14:26:00.300Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "b6f35229-fe68-4055-b004-8c47e84bf879",
      "number": 3,
      "content": "Ngài khiến con dũng cảm với sức mạnh trong tâm hồn.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.300Z",
      "updatedAt": "2024-09-07T14:26:00.300Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "599b42c0-a52a-4b61-b274-421aa45ade3b",
      "number": 4,
      "content": "CHÚA ôi, tất cả các vua trên đất sẽ cảm tạ Ngài,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.302Z",
      "updatedAt": "2024-09-07T14:26:00.302Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "d5d35534-36a9-4e12-92b2-367fd65aced3",
      "number": 4,
      "content": "Khi họ được nghe những lời từ miệng Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.302Z",
      "updatedAt": "2024-09-07T14:26:00.302Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "1985eacb-9a42-4895-9139-338828f5e3f0",
      "number": 5,
      "content": "Thật vậy họ sẽ ca ngợi các đường lối CHÚA,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.305Z",
      "updatedAt": "2024-09-07T14:26:00.305Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "c8596c94-b6a2-4e4d-b185-c08ce028d3ab",
      "number": 5,
      "content": "Vì vinh hiển CHÚA thật lớn lao thay.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.305Z",
      "updatedAt": "2024-09-07T14:26:00.305Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "d0381cc1-d081-41d3-84c9-9e6ceba30afb",
      "number": 6,
      "content": "Vì dù CHÚA ngự trên trời cao thăm thẳm, Ngài vẫn đoái thương đến những kẻ thấp hèn;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.307Z",
      "updatedAt": "2024-09-07T14:26:00.307Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "c889e91c-d9b5-48ba-858d-7b625c7a8bef",
      "number": 6,
      "content": "Còn những kẻ kiêu ngạo, Ngài đã biết rõ từ xa.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.307Z",
      "updatedAt": "2024-09-07T14:26:00.307Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "253ee6f6-faa5-4ed7-9025-220c427c291d",
      "number": 7,
      "content": "Dù con phải bước đi giữa cơn nguy khốn,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.310Z",
      "updatedAt": "2024-09-07T14:26:00.310Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "e7141d34-24cb-4708-9cff-189814dadaaa",
      "number": 7,
      "content": "Ngài sẽ giữ gìn mạng sống con.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.310Z",
      "updatedAt": "2024-09-07T14:26:00.310Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "b171ae4f-01af-49aa-8c5d-3c8c5cfd682f",
      "number": 7,
      "content": "Ngài sẽ đưa tay ra đối phó với cơn giận của những kẻ thù ghét con;",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.310Z",
      "updatedAt": "2024-09-07T14:26:00.310Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "6055ad3e-fefb-457a-8b0e-bcbd8a6dcdbd",
      "number": 7,
      "content": "Tay phải Ngài sẽ cứu con.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.310Z",
      "updatedAt": "2024-09-07T14:26:00.310Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "7b0558a4-911a-4b46-80aa-52228f155d8c",
      "number": 8,
      "content": "CHÚA sẽ thực hiện những gì Ngài muốn làm cho đời sống con;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.313Z",
      "updatedAt": "2024-09-07T14:26:00.313Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "a4512c92-4f42-4ba2-876a-f51adfe03764",
      "number": 8,
      "content": "CHÚA ôi, lòng thương xót của Ngài còn đến đời đời;",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.313Z",
      "updatedAt": "2024-09-07T14:26:00.313Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    },
    {
      "id": "8a115fbc-f360-4df6-ba3d-d2744276ea83",
      "number": 8,
      "content": "Xin đừng bỏ công việc của tay Ngài.",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:26:00.313Z",
      "updatedAt": "2024-09-07T14:26:00.313Z",
      "chapterId": "f3719a39-2f57-47a6-ad7c-1681722b71cb"
    }
  ],
