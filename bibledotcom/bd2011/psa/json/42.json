{
  "chapter": {
    "id": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d",
    "number": 42,
    "ref": "https://www.bible.com/bible/19/PSA.42.BD2011",
    "createdAt": "2024-09-07T11:51:10.958Z",
    "updatedAt": "2024-09-07T11:51:10.958Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "d88a63a5-038a-450d-b02f-9e9cd8164877",
      "number": 1,
      "content": "Đức Chúa Trời ôi, như con nai thèm khát khe nước thể nào,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.023Z",
      "updatedAt": "2024-09-07T14:03:38.023Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "fe996ff6-20bb-48fa-8924-29d307b599b6",
      "number": 1,
      "content": "Linh hồn con cũng khát khao Ngài thể ấy.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.023Z",
      "updatedAt": "2024-09-07T14:03:38.023Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "0ba17b35-eeae-4825-8107-c13df56dd20b",
      "number": 2,
      "content": "Linh hồn tôi khát khao Đức Chúa Trời,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.033Z",
      "updatedAt": "2024-09-07T14:03:38.033Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "3e4bf0f2-36da-4338-81bb-b0e7a0a0ccc4",
      "number": 2,
      "content": "Tức Đức Chúa Trời hằng sống;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.033Z",
      "updatedAt": "2024-09-07T14:03:38.033Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "c8a00494-472f-4b47-822d-8e319d9ecd27",
      "number": 2,
      "content": "Khi nào tôi mới được đến để ra mắt Đức Chúa Trời đây?",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.033Z",
      "updatedAt": "2024-09-07T14:03:38.033Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "d0dad15d-a98e-43a9-88fd-062d4deabe37",
      "number": 3,
      "content": "Suốt ngày họ cứ hỏi tôi,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.037Z",
      "updatedAt": "2024-09-07T14:03:38.037Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "8041bea3-3abb-4ca5-a35c-e9d32bd7d522",
      "number": 3,
      "content": "“Đức Chúa Trời của mi đâu rồi?”",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.037Z",
      "updatedAt": "2024-09-07T14:03:38.037Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "81687c55-16d9-4f3d-9274-c69d40e89613",
      "number": 3,
      "content": "Nước mắt đã làm thực phẩm cho tôi",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.037Z",
      "updatedAt": "2024-09-07T14:03:38.037Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "d05705f6-5325-4fc9-be53-1f93bf30cbfd",
      "number": 3,
      "content": "Cả ngày lẫn đêm.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.037Z",
      "updatedAt": "2024-09-07T14:03:38.037Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "ad1cbfb6-068c-4d12-be24-f063a584559b",
      "number": 4,
      "content": "Mỗi khi tôi nhớ lại những kỷ niệm phước hạnh ngày xưa,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.040Z",
      "updatedAt": "2024-09-07T14:03:38.040Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "70337408-569b-4a6c-abea-9e5dc963459f",
      "number": 4,
      "content": "Lòng tôi dâng lên nỗi buồn vô hạn.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.040Z",
      "updatedAt": "2024-09-07T14:03:38.040Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "ede9e285-22a2-4a55-8ae7-999072386b0a",
      "number": 4,
      "content": "Nhớ trước kia tôi đi theo một đoàn người đông đúc,",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.040Z",
      "updatedAt": "2024-09-07T14:03:38.040Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "125d50c2-cfff-458e-826a-5237fe01cd62",
      "number": 4,
      "content": "Cùng họ tiến về nhà Đức Chúa Trời,",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.040Z",
      "updatedAt": "2024-09-07T14:03:38.040Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "ef3575cc-516d-4757-91f8-9f969f7d6a66",
      "number": 4,
      "content": "Cùng cất tiếng reo mừng và dâng những lời cảm tạ,",
      "order": 4,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.040Z",
      "updatedAt": "2024-09-07T14:03:38.040Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "17f382f5-4563-40bc-b910-7a14035857b9",
      "number": 4,
      "content": "Một đoàn người vui mừng giữ đại lễ thờ phượng.",
      "order": 5,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.040Z",
      "updatedAt": "2024-09-07T14:03:38.040Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "98ea348f-4126-422a-befe-ab8d46f07419",
      "number": 5,
      "content": "Hỡi linh hồn ta, cớ sao ngươi buồn bã?",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.044Z",
      "updatedAt": "2024-09-07T14:03:38.044Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "81dabc7f-88e1-4821-9315-390eab63dab3",
      "number": 5,
      "content": "Vì sao ngươi than thở trong ta?",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.044Z",
      "updatedAt": "2024-09-07T14:03:38.044Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "ce939de7-7d0f-4b2a-9d25-855cf8473b71",
      "number": 5,
      "content": "Hãy tin cậy Đức Chúa Trời, vì ta sẽ còn ca ngợi Ngài,",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.044Z",
      "updatedAt": "2024-09-07T14:03:38.044Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "e60c558b-f146-4e25-a2c1-e85e2bfde921",
      "number": 5,
      "content": "Đấng Giải Cứu của ta và Đức Chúa Trời của ta.",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.044Z",
      "updatedAt": "2024-09-07T14:03:38.044Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "78633a0b-7098-452e-a76f-03509b217abb",
      "number": 6,
      "content": "Đức Chúa Trời của con ôi,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.048Z",
      "updatedAt": "2024-09-07T14:03:38.048Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "aa76a18f-19b5-4e3e-9d7a-e7766c2caa4c",
      "number": 6,
      "content": "Linh hồn con buồn bã trong con;",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.048Z",
      "updatedAt": "2024-09-07T14:03:38.048Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "0a705c32-d94f-4d46-9727-52349bc72592",
      "number": 6,
      "content": "Từ xứ Giô-đanh, từ các đỉnh Hẹt-môn,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.048Z",
      "updatedAt": "2024-09-07T14:03:38.048Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "6a4c9b65-e5d2-4a46-b68a-15ed599c6a60",
      "number": 6,
      "content": "Từ Đồi Mi-xa, con nhớ đến Ngài.",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.048Z",
      "updatedAt": "2024-09-07T14:03:38.048Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "50f34dbd-1cee-41a3-b772-486186ea3a72",
      "number": 7,
      "content": "Vực gọi vực theo tiếng ầm ầm các thác nước của Ngài;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.051Z",
      "updatedAt": "2024-09-07T14:03:38.051Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "00ac8593-e1d0-479e-928b-d9eb44c1bb60",
      "number": 7,
      "content": "Những lượn sóng lớn và nhỏ của Ngài đã vùi dập con.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.051Z",
      "updatedAt": "2024-09-07T14:03:38.051Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "8e4f14ae-3dc9-4ce8-953c-e834f9e7d3e4",
      "number": 8,
      "content": "Ban ngày CHÚA ban lịnh cho tình thương Ngài ở với tôi;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.055Z",
      "updatedAt": "2024-09-07T14:03:38.055Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "a5193c95-6582-46be-80e1-bfc5c17ab5bc",
      "number": 8,
      "content": "Còn ban đêm bài ca của Ngài ở với tôi.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.055Z",
      "updatedAt": "2024-09-07T14:03:38.055Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "d4fa0c8b-2427-4700-b297-7f1a0515f238",
      "number": 8,
      "content": "Đó là bài cầu nguyện dâng lên Đức Chúa Trời của đời sống tôi.",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.055Z",
      "updatedAt": "2024-09-07T14:03:38.055Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "d471cef5-1805-44d4-a035-5b0606f486b1",
      "number": 9,
      "content": "Tôi đã thưa với Đức Chúa Trời, vầng đá của tôi,",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.058Z",
      "updatedAt": "2024-09-07T14:03:38.058Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "5bcf36e5-cbc5-4229-935f-2cbb5dd0e3c7",
      "number": 9,
      "content": "“Sao Ngài đã quên con?",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.058Z",
      "updatedAt": "2024-09-07T14:03:38.058Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "a881b4f7-acc4-4f8a-9f67-4044348dd9c8",
      "number": 9,
      "content": "Sao con cứ phải than khóc",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.058Z",
      "updatedAt": "2024-09-07T14:03:38.058Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "088a7931-d197-4922-b67a-2c5bc3ecb7e0",
      "number": 9,
      "content": "Vì bị kẻ thù của con áp bức mãi thế này?",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.058Z",
      "updatedAt": "2024-09-07T14:03:38.058Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "79a11912-bb5c-4a27-8e42-cd58d8046a66",
      "number": 10,
      "content": "Con đau đớn như bị gươm đâm thấu xương",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.061Z",
      "updatedAt": "2024-09-07T14:03:38.061Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "891aed27-51f0-4e30-8b7c-c54d1f7562d3",
      "number": 10,
      "content": "Mỗi khi con bị kẻ thù sỉ nhục;",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.061Z",
      "updatedAt": "2024-09-07T14:03:38.061Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "053939ed-659a-4c3a-9562-39685d1a7d7d",
      "number": 10,
      "content": "Suốt ngày chúng cứ hỏi con,",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.061Z",
      "updatedAt": "2024-09-07T14:03:38.061Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "712d12f1-d065-4491-9654-b0c7ff56bd2f",
      "number": 10,
      "content": "‘Đức Chúa Trời của mi đâu rồi?’”",
      "order": 3,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.061Z",
      "updatedAt": "2024-09-07T14:03:38.061Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "969dbe26-4264-4528-bbae-2d908e5c1f12",
      "number": 11,
      "content": "Hỡi linh hồn ta, cớ sao ngươi buồn bã?",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.065Z",
      "updatedAt": "2024-09-07T14:03:38.065Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "87be3cc5-f901-4df4-9138-4435e5ab9e1a",
      "number": 11,
      "content": "Vì sao ngươi than thở trong ta?",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.065Z",
      "updatedAt": "2024-09-07T14:03:38.065Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "1106cc9b-4a7f-408e-b589-c699a47c1b4f",
      "number": 11,
      "content": "Hãy tin cậy Đức Chúa Trời, vì ta sẽ còn ca ngợi Ngài,",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:03:38.065Z",
      "updatedAt": "2024-09-07T14:03:38.065Z",
      "chapterId": "dd3b5f88-6ad2-489a-9729-1b7a98d6561d"
    },
    {
      "id": "46bb3392-d629-4629-84b1-396c131d47d2",
      "number": 11,
      "content": "Đấng Giải Cứu của ta và Đức Chúa Trời của ta.",
