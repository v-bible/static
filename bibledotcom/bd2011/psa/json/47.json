{
  "chapter": {
    "id": "230515be-080e-4312-96ad-f9c4505b1393",
    "number": 47,
    "ref": "https://www.bible.com/bible/19/PSA.47.BD2011",
    "createdAt": "2024-09-07T11:51:10.998Z",
    "updatedAt": "2024-09-07T11:51:10.998Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "6ff782eb-6ba4-4a68-86fd-79f695805117",
      "number": 1,
      "content": "Hãy vỗ tay, hỡi mọi người!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.330Z",
      "updatedAt": "2024-09-07T14:04:17.330Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "6e8dfc84-08c4-413c-901f-afc03330b42d",
      "number": 1,
      "content": "Hãy lớn tiếng reo mừng ca ngợi Đức Chúa Trời!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.330Z",
      "updatedAt": "2024-09-07T14:04:17.330Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "87902a62-ed12-4d47-a03c-e21ef127b13f",
      "number": 2,
      "content": "Vì CHÚA, Đấng Tối Cao, thật đáng kính sợ.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.340Z",
      "updatedAt": "2024-09-07T14:04:17.340Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "290b578d-dd69-468e-8b3f-35ac727576bc",
      "number": 2,
      "content": "Ngài là Vua cao cả trên toàn thế giới.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.340Z",
      "updatedAt": "2024-09-07T14:04:17.340Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "eedeab3c-ecdd-41d6-b512-9e730b7f8961",
      "number": 3,
      "content": "Ngài làm cho các dân bị khuất phục dưới chúng tôi,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.343Z",
      "updatedAt": "2024-09-07T14:04:17.343Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "707f2620-5624-4793-91f2-df415b8cfc0a",
      "number": 3,
      "content": "Và các nước ở dưới chân chúng tôi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.343Z",
      "updatedAt": "2024-09-07T14:04:17.343Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "d4df62fa-8151-4b1b-a878-9a94b1a533e8",
      "number": 4,
      "content": "Ngài chọn cơ nghiệp cho chúng tôi,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.346Z",
      "updatedAt": "2024-09-07T14:04:17.346Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "b0afa92a-0c3a-4112-b498-cecbe19db842",
      "number": 4,
      "content": "Vinh hiển của Gia-cốp, người Ngài yêu mến. (Sê-la)",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.346Z",
      "updatedAt": "2024-09-07T14:04:17.346Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "41f8dabd-be4b-47ae-8dec-2797dba3213e",
      "number": 5,
      "content": "Đức Chúa Trời ngự lên giữa tiếng reo mừng;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.348Z",
      "updatedAt": "2024-09-07T14:04:17.348Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "a29f88d8-8af5-483f-98c8-68ffc0fbf3d5",
      "number": 5,
      "content": "CHÚA ngự lên giữa tiếng kèn trỗi lên vang lừng.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.348Z",
      "updatedAt": "2024-09-07T14:04:17.348Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "aedcc2c0-eea7-4be8-905a-7115d3937079",
      "number": 6,
      "content": "Hãy ca ngợi Đức Chúa Trời; khá hát lên những bài chúc tụng;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.351Z",
      "updatedAt": "2024-09-07T14:04:17.351Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "a3af648d-f6c4-4908-920f-fd5e7a324d43",
      "number": 6,
      "content": "Hãy ca ngợi Vua chúng ta; khá hát lên những bài chúc tụng,",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.351Z",
      "updatedAt": "2024-09-07T14:04:17.351Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "e6d8a2b9-fff9-47d5-bfb2-e56b3d9b0ad5",
      "number": 7,
      "content": "Vì Đức Chúa Trời là Vua của toàn thế giới;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.353Z",
      "updatedAt": "2024-09-07T14:04:17.353Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "c98e52bb-25e5-4192-a5be-a9290c08d9c9",
      "number": 7,
      "content": "Khá khôn ngoan mà ca ngợi Ngài.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.353Z",
      "updatedAt": "2024-09-07T14:04:17.353Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "2d9dee3e-37bb-4649-b37e-2486c598a4d3",
      "number": 8,
      "content": "Đức Chúa Trời trị vì trên các dân các nước;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.355Z",
      "updatedAt": "2024-09-07T14:04:17.355Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "d748d342-a22f-4e1b-b492-632345413ec4",
      "number": 8,
      "content": "Đức Chúa Trời ngự trên ngôi thánh của Ngài.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.355Z",
      "updatedAt": "2024-09-07T14:04:17.355Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "c9af0e3e-4504-4017-a9c7-5e2e818d79e2",
      "number": 9,
      "content": "Các lãnh tụ thế giới họp nhau lại",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.358Z",
      "updatedAt": "2024-09-07T14:04:17.358Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "8c94ece7-063c-41d9-9619-3fac672eaf93",
      "number": 9,
      "content": "Làm dân của Đức Chúa Trời của Áp-ra-ham,",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.358Z",
      "updatedAt": "2024-09-07T14:04:17.358Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "e61378c3-c036-4f3a-9947-a1b0c583774d",
      "number": 9,
      "content": "Bởi vì các thủ lãnh trên thế gian đều thuộc về Đức Chúa Trời.",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.358Z",
      "updatedAt": "2024-09-07T14:04:17.358Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    },
    {
      "id": "2ea67779-bdb0-48f1-aa44-61d40e80837c",
      "number": 9,
      "content": "Ngài được tôn chí cao!",
      "order": 3,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:04:17.358Z",
      "updatedAt": "2024-09-07T14:04:17.358Z",
      "chapterId": "230515be-080e-4312-96ad-f9c4505b1393"
    }
  ],
