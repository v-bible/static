{
  "chapter": {
    "id": "20c185cf-7293-4eef-b6f3-1d4243d170eb",
    "number": 113,
    "ref": "https://www.bible.com/bible/19/PSA.113.BD2011",
    "createdAt": "2024-09-07T11:51:11.498Z",
    "updatedAt": "2024-09-07T11:51:11.498Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "9d28ebb4-015f-4ff8-b08b-09704236a542",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.814Z",
      "updatedAt": "2024-09-07T14:18:22.814Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "fcb1a34a-c1b0-4567-a3aa-73fc0279f33c",
      "number": 1,
      "content": "Hỡi các tôi tớ CHÚA, hãy ca ngợi Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.814Z",
      "updatedAt": "2024-09-07T14:18:22.814Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "20b7b39f-decb-4085-9f3b-461d552f759a",
      "number": 1,
      "content": "Hãy ca ngợi danh CHÚA.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.814Z",
      "updatedAt": "2024-09-07T14:18:22.814Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "65618875-0fcd-4af6-8022-b8066f6a68d5",
      "number": 2,
      "content": "Hãy chúc tụng danh CHÚA,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.824Z",
      "updatedAt": "2024-09-07T14:18:22.824Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "6b171e39-71c3-4fbd-b88f-b131df32d86f",
      "number": 2,
      "content": "Từ nay cho đến đời đời.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.824Z",
      "updatedAt": "2024-09-07T14:18:22.824Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "c1d0ef38-25e4-4298-a740-964d7cd9fe28",
      "number": 3,
      "content": "Từ bình minh cho đến hoàng hôn,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.830Z",
      "updatedAt": "2024-09-07T14:18:22.830Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "c5538244-f1cb-4bb0-94b9-92c3c1b9398d",
      "number": 3,
      "content": "Danh CHÚA phải được tôn vinh ca ngợi.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.830Z",
      "updatedAt": "2024-09-07T14:18:22.830Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "0947b571-918d-485c-97ac-5a26a60db304",
      "number": 4,
      "content": "CHÚA được tôn cao hơn tất cả các dân các nước;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.835Z",
      "updatedAt": "2024-09-07T14:18:22.835Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "a79efad5-1824-4539-ad3e-f1a1fd60f620",
      "number": 4,
      "content": "Vinh quang Ngài vượt trỗi hơn các tầng trời.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.835Z",
      "updatedAt": "2024-09-07T14:18:22.835Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "f580621a-5d75-45a5-abec-dc1df8f7f796",
      "number": 5,
      "content": "Có ai giống như CHÚA, Đức Chúa Trời chúng ta,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.838Z",
      "updatedAt": "2024-09-07T14:18:22.838Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "3cd0c647-0f02-42bc-bece-161229edf7db",
      "number": 5,
      "content": "Đấng ngự trên ngai cao cả,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.838Z",
      "updatedAt": "2024-09-07T14:18:22.838Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "7bf6c272-0a84-48ca-9f61-253aa3940cdf",
      "number": 6,
      "content": "Lại tự hạ mình xuống,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.840Z",
      "updatedAt": "2024-09-07T14:18:22.840Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "28bb42b0-bcce-44d5-8210-a1ceed87cc6a",
      "number": 6,
      "content": "Để đoái xem mọi vật trong bầu trời và trên mặt đất chăng?",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.840Z",
      "updatedAt": "2024-09-07T14:18:22.840Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "60480e8c-e68e-4703-bac8-f3ae4beede98",
      "number": 7,
      "content": "Ngài nâng đỡ kẻ nghèo khổ ra khỏi chốn bụi tro,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.842Z",
      "updatedAt": "2024-09-07T14:18:22.842Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "2b8b405a-341b-4e02-a302-7fddff200f4a",
      "number": 7,
      "content": "Và nhấc đưa kẻ khó nghèo ra khỏi gò rác rến.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.842Z",
      "updatedAt": "2024-09-07T14:18:22.842Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "74885a63-5bf7-453f-a29f-a1bb4b772808",
      "number": 8,
      "content": "Ngài đặt họ ngồi ngang hàng với các bậc quan quyền,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.844Z",
      "updatedAt": "2024-09-07T14:18:22.844Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "3b0cc34d-f5b4-48a7-b887-cddb28a1dfc7",
      "number": 8,
      "content": "Tức với những người quyền thế của con dân Ngài.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.844Z",
      "updatedAt": "2024-09-07T14:18:22.844Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "38cb4caf-be7b-4d81-aeec-90b5844e62f7",
      "number": 9,
      "content": "Ngài làm cho người đàn bà son sẻ sống quạnh quẽ trong nhà",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.846Z",
      "updatedAt": "2024-09-07T14:18:22.846Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "f420a143-714b-47b7-9f60-4220607fcb78",
      "number": 9,
      "content": "Thành người mẹ hiền vui vẻ của một đàn con.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.846Z",
      "updatedAt": "2024-09-07T14:18:22.846Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    },
    {
      "id": "816b868f-2e25-4adb-a16e-5e512d89bff4",
      "number": 9,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:22.846Z",
      "updatedAt": "2024-09-07T14:18:22.846Z",
      "chapterId": "20c185cf-7293-4eef-b6f3-1d4243d170eb"
    }
  ],
