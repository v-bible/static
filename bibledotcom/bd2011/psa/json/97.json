{
  "chapter": {
    "id": "ed2cf092-9336-4e41-92e4-b111a8f978ae",
    "number": 97,
    "ref": "https://www.bible.com/bible/19/PSA.97.BD2011",
    "createdAt": "2024-09-07T11:51:11.388Z",
    "updatedAt": "2024-09-07T11:51:11.388Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "a548df32-4691-4bec-bc0a-a47fb3077ec1",
      "number": 1,
      "content": "CHÚA trị vì! Hỡi trái đất, hãy vui mừng;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.413Z",
      "updatedAt": "2024-09-07T14:16:17.413Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "bda27a3a-7967-4b61-acc6-08da6fa7bb9f",
      "number": 1,
      "content": "Hỡi muôn ngàn hải đảo, hãy hân hoan.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.413Z",
      "updatedAt": "2024-09-07T14:16:17.413Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "13fcbdda-feb5-4923-9aa6-766418a4a91b",
      "number": 2,
      "content": "Mây dày và bóng tối mù mịt bao quanh Ngài;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.422Z",
      "updatedAt": "2024-09-07T14:16:17.422Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "45b0c458-3756-4ef3-af60-d16c0ad444ff",
      "number": 2,
      "content": "Công chính và công lý là nền tảng của ngai Ngài.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.422Z",
      "updatedAt": "2024-09-07T14:16:17.422Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "07554e30-160e-4ef3-8d6a-9ca204b81531",
      "number": 3,
      "content": "Lửa hừng đi trước mặt Ngài,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.425Z",
      "updatedAt": "2024-09-07T14:16:17.425Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "f715649d-2da3-42f3-aaaa-7079c106c98e",
      "number": 3,
      "content": "Thiêu hủy mọi kẻ chống nghịch chung quanh.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.425Z",
      "updatedAt": "2024-09-07T14:16:17.425Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "439d95d1-ebd3-4073-8de2-ba9b86f4c875",
      "number": 4,
      "content": "Các tia chớp của Ngài lóe lên rực sáng khắp thế gian;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.428Z",
      "updatedAt": "2024-09-07T14:16:17.428Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "c2080c1f-9922-4b53-a22f-f54374935000",
      "number": 4,
      "content": "Đất thấy vậy bèn run lên lẩy bẩy.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.428Z",
      "updatedAt": "2024-09-07T14:16:17.428Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "08a14bdf-5bd4-4c05-930c-fd25a7890b82",
      "number": 5,
      "content": "Các núi đồi tan chảy như sáp trước mặt CHÚA,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.432Z",
      "updatedAt": "2024-09-07T14:16:17.432Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "432dce97-acca-425b-a305-299c5d0ac334",
      "number": 5,
      "content": "Tức trước thánh nhan của Đấng Chúa Tể toàn cầu.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.432Z",
      "updatedAt": "2024-09-07T14:16:17.432Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "85325eb5-d818-40f9-8214-aba15057d064",
      "number": 6,
      "content": "Các tầng trời rao truyền đức công chính Ngài;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.435Z",
      "updatedAt": "2024-09-07T14:16:17.435Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "d037ee56-3bf7-4ff2-bcbf-8c109a6a6ef4",
      "number": 6,
      "content": "Muôn dân thấy vinh quang Ngài.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.435Z",
      "updatedAt": "2024-09-07T14:16:17.435Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "a79231c0-2c9d-4ac5-87b1-78088f96c9e7",
      "number": 7,
      "content": "Hãy để mọi kẻ thờ lạy các hình tượng bị xấu hổ,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.437Z",
      "updatedAt": "2024-09-07T14:16:17.437Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "5fba197e-120f-493c-8129-5328af79e715",
      "number": 7,
      "content": "Tức những kẻ khoe mình về các hình tượng vô tri của chúng.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.437Z",
      "updatedAt": "2024-09-07T14:16:17.437Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "fcfdbd60-aef1-49a4-93c6-22916b8905fa",
      "number": 7,
      "content": "Hãy sấp mình xuống thờ lạy Ngài, hỡi tất cả các thần.",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.437Z",
      "updatedAt": "2024-09-07T14:16:17.437Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "85516aa3-39fb-4209-b033-15c3edcb1a6c",
      "number": 8,
      "content": "Si-ôn nghe thế bèn vui mừng,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.440Z",
      "updatedAt": "2024-09-07T14:16:17.440Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "281cb20f-8bcb-49b5-8348-b6a49c8cdc98",
      "number": 8,
      "content": "Các ái nữ của Giu-đa đều vui vẻ,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.440Z",
      "updatedAt": "2024-09-07T14:16:17.440Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "22952159-9b5c-4618-a312-1d4add86a570",
      "number": 8,
      "content": "Vì những phán quyết của Ngài, CHÚA ôi;",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.440Z",
      "updatedAt": "2024-09-07T14:16:17.440Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "66990b17-6ce4-4c51-bc07-c460ae4f2961",
      "number": 9,
      "content": "Vì Ngài, CHÚA ôi, là Đấng Tối Cao trên khắp đất;",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.442Z",
      "updatedAt": "2024-09-07T14:16:17.442Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "179655d6-440d-4b83-afbf-7bb903f20c5b",
      "number": 9,
      "content": "Ngài vượt trỗi bỏ xa tất cả các thần.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.442Z",
      "updatedAt": "2024-09-07T14:16:17.442Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "af2867de-eb65-4d3e-92b4-0c0561a12ade",
      "number": 10,
      "content": "Hỡi những người yêu mến CHÚA, hãy ghét điều ác;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.444Z",
      "updatedAt": "2024-09-07T14:16:17.444Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "ebf47014-c24a-4ca5-802b-d45497dbfc28",
      "number": 10,
      "content": "Ngài bảo vệ sinh mạng các thánh đồ của Ngài;",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.444Z",
      "updatedAt": "2024-09-07T14:16:17.444Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "3a48dac3-8fa2-491d-a1e4-4bbe791e4f6d",
      "number": 10,
      "content": "Ngài giải thoát họ khỏi tay kẻ ác.",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.444Z",
      "updatedAt": "2024-09-07T14:16:17.444Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "8ff7089b-d45a-41c4-98e3-270253b5ec73",
      "number": 11,
      "content": "Ánh sáng tỏa rạng trên người ngay lành,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.446Z",
      "updatedAt": "2024-09-07T14:16:17.446Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "3a571488-0315-403b-9792-8d7508be925f",
      "number": 11,
      "content": "Và niềm vui đến với người có lòng ngay thẳng.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.446Z",
      "updatedAt": "2024-09-07T14:16:17.446Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "121a2a96-22c0-4787-8e49-53aced723f6c",
      "number": 12,
      "content": "Hãy vui mừng trong CHÚA, hỡi những người ngay lành,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.448Z",
      "updatedAt": "2024-09-07T14:16:17.448Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    },
    {
      "id": "951083b1-7503-4643-9809-781f512a6a62",
      "number": 12,
      "content": "Và hãy dâng lòng biết ơn lên danh thánh của Ngài.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:16:17.448Z",
      "updatedAt": "2024-09-07T14:16:17.448Z",
      "chapterId": "ed2cf092-9336-4e41-92e4-b111a8f978ae"
    }
  ],
