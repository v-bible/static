{
  "chapter": {
    "id": "4eed8a99-3ce3-480f-9896-603a68261135",
    "number": 57,
    "ref": "https://www.bible.com/bible/19/PSA.57.BD2011",
    "createdAt": "2024-09-07T11:51:11.066Z",
    "updatedAt": "2024-09-07T11:51:11.066Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "85b81d09-90b9-444b-9f55-40b86fac4761",
      "number": 1,
      "content": "Xin thương xót con, Đức Chúa Trời ôi, xin thương xót con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.395Z",
      "updatedAt": "2024-09-07T14:05:43.395Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "d56332dd-7e2a-477b-bf62-e72ef13a8559",
      "number": 1,
      "content": "Vì linh hồn con tin cậy Ngài;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.395Z",
      "updatedAt": "2024-09-07T14:05:43.395Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "be51cea0-55a4-448b-85e2-0591c247adf5",
      "number": 1,
      "content": "Thật vậy con nương náu mình dưới bóng cánh Ngài",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.395Z",
      "updatedAt": "2024-09-07T14:05:43.395Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "56489862-3d1d-4d1b-8d31-be97f1204981",
      "number": 1,
      "content": "Cho đến khi những cơn hủy diệt đã qua.",
      "order": 3,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.395Z",
      "updatedAt": "2024-09-07T14:05:43.395Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "c6a789d5-124d-4e45-86e9-a45cf79d3827",
      "number": 2,
      "content": "Tôi kêu cầu với Đức Chúa Trời Tối Cao,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.403Z",
      "updatedAt": "2024-09-07T14:05:43.403Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "3f34aa14-8d41-4261-9f47-8e3fdeee11d8",
      "number": 2,
      "content": "Tôi kêu cầu với Đức Chúa Trời, Đấng thực hiện ý định của Ngài cho tôi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.403Z",
      "updatedAt": "2024-09-07T14:05:43.403Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "f12d6199-bb3b-4646-a57f-f98dcd137414",
      "number": 3,
      "content": "Từ trời Ngài ban ơn và cứu giúp tôi;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.405Z",
      "updatedAt": "2024-09-07T14:05:43.405Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "9daac9e5-0ae8-4462-a95d-e4b01dd58c31",
      "number": 3,
      "content": "Ngài làm cho những kẻ chà đạp tôi phải hổ thẹn. (Sê-la)",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.405Z",
      "updatedAt": "2024-09-07T14:05:43.405Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "ef77ab06-6870-48bb-b336-fa2834be0a46",
      "number": 3,
      "content": "Đức Chúa Trời sẽ thể hiện tình thương và đức thành tín của Ngài.",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.405Z",
      "updatedAt": "2024-09-07T14:05:43.405Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "c86b4b28-93fd-46e5-80a0-fc79376fa4df",
      "number": 4,
      "content": "Linh hồn tôi ở giữa một bầy sư tử;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.408Z",
      "updatedAt": "2024-09-07T14:05:43.408Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "7c95ab2d-77b4-4c96-b785-beb92c19110b",
      "number": 4,
      "content": "Tôi nằm giữa những kẻ hung dữ như lửa chực thiêu đốt,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.408Z",
      "updatedAt": "2024-09-07T14:05:43.408Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "224cd427-ee94-4493-9495-8d3e91e21cdf",
      "number": 4,
      "content": "Những kẻ có răng như giáo mác và tên nhọn,",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.408Z",
      "updatedAt": "2024-09-07T14:05:43.408Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "751393ef-c8d6-4457-80a1-5aa5fa28de3e",
      "number": 4,
      "content": "Và lưỡi chúng sắc bén như gươm đao.",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.408Z",
      "updatedAt": "2024-09-07T14:05:43.408Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "a6f0557d-8e6f-4df0-8055-26656d60b2cd",
      "number": 5,
      "content": "Đức Chúa Trời ôi, nguyện Ngài được tôn cao trên khắp trời!",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.411Z",
      "updatedAt": "2024-09-07T14:05:43.411Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "112ed646-9ebb-47cd-98f3-7ec3e69fd85c",
      "number": 5,
      "content": "Nguyện vinh hiển Ngài luôn trỗi cao trên khắp đất!",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.411Z",
      "updatedAt": "2024-09-07T14:05:43.411Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "6b9e4c66-7208-4014-860c-896a46bfafa7",
      "number": 6,
      "content": "Chúng đã giăng lưới để đón bắt các bước chân con,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.413Z",
      "updatedAt": "2024-09-07T14:05:43.413Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "076be83b-5c6b-4c8a-b363-6ed4bcf351d3",
      "number": 6,
      "content": "Khiến linh hồn con phải cúi xuống lo âu;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.413Z",
      "updatedAt": "2024-09-07T14:05:43.413Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "ff347b74-01b2-4e26-bf60-06ac1dec88c7",
      "number": 6,
      "content": "Chúng đã đào hầm đằng trước con,",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.413Z",
      "updatedAt": "2024-09-07T14:05:43.413Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "99f098cf-44aa-4307-b92a-78937bbe0dc0",
      "number": 6,
      "content": "Nhưng chúng đã bị sa ngay vào giữa hầm đó! (Sê-la)",
      "order": 3,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.413Z",
      "updatedAt": "2024-09-07T14:05:43.413Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "3978db8d-05a8-4bc9-8e50-71f65f655559",
      "number": 7,
      "content": "Lòng con vững rồi, Đức Chúa Trời ôi,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.416Z",
      "updatedAt": "2024-09-07T14:05:43.416Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "7e3c926e-88b6-4d47-a779-0173ad8dea17",
      "number": 7,
      "content": "Lòng con vững rồi;",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.416Z",
      "updatedAt": "2024-09-07T14:05:43.416Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "e331c807-1e77-420f-9660-7cfaeded5a36",
      "number": 7,
      "content": "Con sẽ ca hát, thật vậy, con sẽ ca hát chúc tụng Ngài.",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.416Z",
      "updatedAt": "2024-09-07T14:05:43.416Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "223d5242-985b-4468-9765-6a4ace853023",
      "number": 8,
      "content": "Hỡi linh hồn ta, hãy thức dậy!",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.418Z",
      "updatedAt": "2024-09-07T14:05:43.418Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "031fc658-9c31-4238-8637-f40f23c8ea98",
      "number": 8,
      "content": "Hỡi đàn lia và hạc cầm, hãy trỗi lên!",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.418Z",
      "updatedAt": "2024-09-07T14:05:43.418Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "1a9be7ea-62b1-4eea-a753-ee38ec1473c1",
      "number": 8,
      "content": "Tôi sẽ thức dậy từ lúc bình minh để ca ngợi Ngài!",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.418Z",
      "updatedAt": "2024-09-07T14:05:43.418Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "9a74d919-d987-49cf-ae5c-8abca5b1bc87",
      "number": 9,
      "content": "Chúa ôi, con sẽ cảm tạ Ngài giữa mọi người;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.421Z",
      "updatedAt": "2024-09-07T14:05:43.421Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "daebc666-a486-447a-98b7-7621c8dc1ff3",
      "number": 9,
      "content": "Con sẽ hát ca tôn ngợi Ngài giữa các dân,",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.421Z",
      "updatedAt": "2024-09-07T14:05:43.421Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "3c95afb5-3a71-4fa8-a299-451cc5654338",
      "number": 10,
      "content": "Vì tình thương của Ngài cao đến tận trời,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.423Z",
      "updatedAt": "2024-09-07T14:05:43.423Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "32f7407a-28eb-4cb4-84b7-a395eaf5d469",
      "number": 10,
      "content": "Đức thành tín của Ngài vượt quá các tầng mây.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.423Z",
      "updatedAt": "2024-09-07T14:05:43.423Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "b2226171-320e-4fb5-b9ce-77458221b219",
      "number": 11,
      "content": "Đức Chúa Trời ôi, nguyện Ngài được tôn cao trên các tầng trời!",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.425Z",
      "updatedAt": "2024-09-07T14:05:43.425Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    },
    {
      "id": "52d3b307-7f98-4406-b4ef-b2d7fea7d47d",
      "number": 11,
      "content": "Nguyện vinh hiển Ngài trỗi cao trên khắp đất!",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:05:43.425Z",
      "updatedAt": "2024-09-07T14:05:43.425Z",
      "chapterId": "4eed8a99-3ce3-480f-9896-603a68261135"
    }
  ],
