{
  "chapter": {
    "id": "fc70a6a7-08f0-4587-8701-fe455629fc09",
    "number": 104,
    "ref": "https://www.bible.com/bible/19/PSA.104.BD2011",
    "createdAt": "2024-09-07T11:51:11.448Z",
    "updatedAt": "2024-09-07T11:51:11.448Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "b6e9ae76-cc1c-47ea-9765-04a478b0c63e",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA, hỡi linh hồn ta!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.876Z",
      "updatedAt": "2024-09-07T14:17:07.876Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "3fe954b9-f130-48e4-a110-b41004b783ac",
      "number": 1,
      "content": "Lạy CHÚA, Đức Chúa Trời của con, Ngài thật vĩ đại thay!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.876Z",
      "updatedAt": "2024-09-07T14:17:07.876Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "322517e8-964a-42d3-be34-a6db7fe8f907",
      "number": 1,
      "content": "Ngài mặc bằng sự huy hoàng và sự uy nghi.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.876Z",
      "updatedAt": "2024-09-07T14:17:07.876Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "e6f5f63d-4527-4db5-bfac-d2d6daeed03d",
      "number": 2,
      "content": "Ngài bao phủ mình bằng ánh sáng như chiếc áo choàng;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.886Z",
      "updatedAt": "2024-09-07T14:17:07.886Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "de565449-7571-4167-bdbf-b4b0a116b87f",
      "number": 2,
      "content": "Ngài giăng các tầng trời ra như căng một cái trại.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.886Z",
      "updatedAt": "2024-09-07T14:17:07.886Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "ede7a7ed-b63a-45e2-8d20-bd2ef97e870d",
      "number": 3,
      "content": "Ngài đặt các đà ngang cho các thiên cung của Ngài trên nước;",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.888Z",
      "updatedAt": "2024-09-07T14:17:07.888Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "c558d170-47da-44fe-86ca-cf4e03eae311",
      "number": 3,
      "content": "Ngài dùng mây làm xa giá;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.888Z",
      "updatedAt": "2024-09-07T14:17:07.888Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "50b13101-a47b-4c33-b397-ae10c1d5aec5",
      "number": 3,
      "content": "Ngài bước đi trên cánh gió;",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.888Z",
      "updatedAt": "2024-09-07T14:17:07.888Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "ef40149a-e4ec-4f60-ac5c-998d45e92068",
      "number": 4,
      "content": "Ngài dùng gió làm các sứ giả Ngài,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.892Z",
      "updatedAt": "2024-09-07T14:17:07.892Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "4e0d4e18-32cd-40f6-88cb-adcc57083f08",
      "number": 4,
      "content": "Lửa hừng làm bầy tôi hầu hạ Ngài.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.892Z",
      "updatedAt": "2024-09-07T14:17:07.892Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "a8f0e17d-0291-48e6-9d93-ea29f8c37cc9",
      "number": 5,
      "content": "Ngài đặt trái đất trên nền của nó,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.894Z",
      "updatedAt": "2024-09-07T14:17:07.894Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "06e37cb9-9b6e-4b4d-aceb-1d2dbf35b958",
      "number": 5,
      "content": "Để đến đời đời nó chẳng bị chuyển lay.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.894Z",
      "updatedAt": "2024-09-07T14:17:07.894Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "1e643bdd-bc71-4f81-98bb-51b8309bcb8f",
      "number": 6,
      "content": "Ngài bao phủ đất bằng biển sâu như phủ một chiếc áo;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.897Z",
      "updatedAt": "2024-09-07T14:17:07.897Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "4983ebf8-c5d8-4730-b3cb-97be64392b2d",
      "number": 6,
      "content": "Các nguồn nước lại đứng trên các ngọn núi cao.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.897Z",
      "updatedAt": "2024-09-07T14:17:07.897Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "3f1b774d-38df-4196-ad0f-13b04453c791",
      "number": 7,
      "content": "Khi Ngài quở trách, chúng thất kinh chạy trốn;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.899Z",
      "updatedAt": "2024-09-07T14:17:07.899Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "5726a42b-72ef-483e-ac39-29aacec0664e",
      "number": 7,
      "content": "Nghe tiếng sấm sét của Ngài, chúng vụt chạy cho xa.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.899Z",
      "updatedAt": "2024-09-07T14:17:07.899Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "6f59a563-9b92-410b-8b12-224b40ae4857",
      "number": 8,
      "content": "Núi đồi trồi lên, thung lũng sụp xuống,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.900Z",
      "updatedAt": "2024-09-07T14:17:07.900Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "91a1d23a-1914-476a-9edf-d7ef2bf07e5a",
      "number": 8,
      "content": "Đúng vào nơi Ngài định cho chúng.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.900Z",
      "updatedAt": "2024-09-07T14:17:07.900Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "832cc2d3-9db6-429f-bf7b-e02e2efbfcf0",
      "number": 9,
      "content": "Ngài định giới hạn để nước không thể vượt qua,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.902Z",
      "updatedAt": "2024-09-07T14:17:07.902Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "af62def8-689b-4c6c-ad85-5cb34f8c1cc1",
      "number": 9,
      "content": "Hầu không được trở lại bao phủ mặt đất nữa.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.902Z",
      "updatedAt": "2024-09-07T14:17:07.902Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "b1768c74-dac2-4231-8c74-2402380544f0",
      "number": 10,
      "content": "Trong các thung lũng Ngài ban các dòng nước ngọt;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.905Z",
      "updatedAt": "2024-09-07T14:17:07.905Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "a59e3bed-5b4c-4f4b-bb67-fb3d6e502f1a",
      "number": 10,
      "content": "Chúng lượn quanh tuôn chảy giữa núi đồi;",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.905Z",
      "updatedAt": "2024-09-07T14:17:07.905Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "7d051b22-13b9-4498-961d-6f86b6b8d5d4",
      "number": 11,
      "content": "Chúng cung cấp thức uống cho mọi sinh vật trong đồng;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.907Z",
      "updatedAt": "2024-09-07T14:17:07.907Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "d3d71539-b28a-44df-bcd6-f88bb797ac4a",
      "number": 11,
      "content": "Nhờ chúng những lừa rừng được thỏa mãn cơn khát.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.907Z",
      "updatedAt": "2024-09-07T14:17:07.907Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "a8705524-754c-4b04-bb43-f26b0dcb22ec",
      "number": 12,
      "content": "Bên cạnh chúng chim trời làm tổ,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.909Z",
      "updatedAt": "2024-09-07T14:17:07.909Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "121f71bf-7337-4da5-9bc4-2452bbd960dc",
      "number": 12,
      "content": "Cất tiếng hót ca giữa các cành cây.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.909Z",
      "updatedAt": "2024-09-07T14:17:07.909Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "c26ebbab-8512-4f2a-adbb-765af81db1f7",
      "number": 13,
      "content": "Từ thiên cung Ngài đổ nước xuống tưới các núi rừng;",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.912Z",
      "updatedAt": "2024-09-07T14:17:07.912Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "3055bbda-a7da-4596-a5ef-962e942ecf1d",
      "number": 13,
      "content": "Trái đất được no nê nhờ bông trái của công việc Ngài.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.912Z",
      "updatedAt": "2024-09-07T14:17:07.912Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "468ef30d-da7a-412d-b66f-25c10f7977be",
      "number": 14,
      "content": "Ngài khiến cỏ xanh mọc lên cho súc vật,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.914Z",
      "updatedAt": "2024-09-07T14:17:07.914Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "25946394-1980-4156-881a-ef7bdb5ef17f",
      "number": 14,
      "content": "Và rau trái sinh ra để phục vụ loài người;",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.914Z",
      "updatedAt": "2024-09-07T14:17:07.914Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "d3b17585-6c8b-4a3a-b199-090dc35411d4",
      "number": 14,
      "content": "Từ ruộng đất người ta có thể đem về thực phẩm,",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.914Z",
      "updatedAt": "2024-09-07T14:17:07.914Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "5381690b-5adf-4efc-b9d4-5496c54e3a01",
      "number": 15,
      "content": "Rồi chế thành rượu, uống cho lòng vui vẻ,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.916Z",
      "updatedAt": "2024-09-07T14:17:07.916Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "3012e4f3-317b-4961-ad0b-2dc8471ea2c0",
      "number": 15,
      "content": "Chế thành dầu, xức cho tươi sáng mặt mày,",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.916Z",
      "updatedAt": "2024-09-07T14:17:07.916Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "9f41cf03-5017-40f0-9e2b-c7f271080e38",
      "number": 15,
      "content": "Và làm cơm bánh, ăn cho chắc dạ vững lòng.",
      "order": 2,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.916Z",
      "updatedAt": "2024-09-07T14:17:07.916Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "470c5ac9-06ba-454d-948a-e5140118178e",
      "number": 16,
      "content": "Những cây cối của CHÚA đều được nước uống no nê,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.918Z",
      "updatedAt": "2024-09-07T14:17:07.918Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "b378769c-a202-4da6-90f1-55a88fc8a05c",
      "number": 16,
      "content": "Kể cả những cây bá hương Ngài đã trồng ở Li-băng,",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.918Z",
      "updatedAt": "2024-09-07T14:17:07.918Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "bf652b51-7022-4a47-9f18-5eb572432dc2",
      "number": 17,
      "content": "Cùng những cây cối nơi các chim trời làm tổ,",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.921Z",
      "updatedAt": "2024-09-07T14:17:07.921Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "8d07ce39-a202-4cbd-a0ac-c8330a09eec7",
      "number": 17,
      "content": "Cùng những cây bách nơi chim hạc chọn làm nhà.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.921Z",
      "updatedAt": "2024-09-07T14:17:07.921Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "817546c8-58c5-436f-a74f-1bf4bcb1dca9",
      "number": 18,
      "content": "Ngài dựng nên những núi cao nơi các dê rừng làm chỗ ở;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.923Z",
      "updatedAt": "2024-09-07T14:17:07.923Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "8fb9489a-0839-4573-925a-92ce555c9185",
      "number": 18,
      "content": "Các gành đá cheo leo nơi các chuột núi nương thân.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.923Z",
      "updatedAt": "2024-09-07T14:17:07.923Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "806ce947-58d9-4188-933b-4ecb0827ccd1",
      "number": 19,
      "content": "Ngài dựng nên mặt trăng để ấn định các mùa;",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.925Z",
      "updatedAt": "2024-09-07T14:17:07.925Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "c97f5775-11f2-424e-9b08-648afca55dc0",
      "number": 19,
      "content": "Cùng mặt trời tự biết khi nào phải lặn xuống.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.925Z",
      "updatedAt": "2024-09-07T14:17:07.925Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "34937a4b-0d47-4ad5-b217-659adc6bf4b2",
      "number": 20,
      "content": "Ngài dựng nên bóng tối, và nó trở thành ban đêm;",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.927Z",
      "updatedAt": "2024-09-07T14:17:07.927Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "1596237b-8ff0-462e-bd94-ac7a092e1833",
      "number": 20,
      "content": "Ấy là khi các thú rừng đi ra săn mồi.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.927Z",
      "updatedAt": "2024-09-07T14:17:07.927Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "d514a0c8-1c84-4a37-bebe-d052bd97a436",
      "number": 21,
      "content": "Các sư tử tơ gầm rống tìm mồi,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.929Z",
      "updatedAt": "2024-09-07T14:17:07.929Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "0628752e-38be-495f-aa4e-7dcd64cea623",
      "number": 21,
      "content": "Và tìm kiếm thức ăn từ Đức Chúa Trời cung cấp.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.929Z",
      "updatedAt": "2024-09-07T14:17:07.929Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "ca4aad96-cd6d-476d-8df6-6c71d565ff5c",
      "number": 22,
      "content": "Đến khi mặt trời mọc lên chúng lại rút về,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.931Z",
      "updatedAt": "2024-09-07T14:17:07.931Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "62fb018f-9dd7-47fd-b036-db51fcf945f2",
      "number": 22,
      "content": "Và nằm xuống trong hang của chúng.",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.931Z",
      "updatedAt": "2024-09-07T14:17:07.931Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "8242d3a2-7e3d-4bb3-a92a-69b62776948c",
      "number": 23,
      "content": "Người ta lại đi ra làm việc,",
      "order": 0,
      "parNumber": 49,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.933Z",
      "updatedAt": "2024-09-07T14:17:07.933Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "b483a3e5-23df-4467-8f03-6e22b013acee",
      "number": 23,
      "content": "Và làm lụng cho đến hoàng hôn.",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.933Z",
      "updatedAt": "2024-09-07T14:17:07.933Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "e4fea081-1af3-444b-85a0-ea357ada404a",
      "number": 24,
      "content": "CHÚA ôi, công việc của Ngài thật nhiều thay!",
      "order": 0,
      "parNumber": 51,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.935Z",
      "updatedAt": "2024-09-07T14:17:07.935Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "382c18ee-2a45-4da3-8217-901309a28d22",
      "number": 24,
      "content": "Ngài đã dựng nên tất cả bằng sự khôn ngoan của Ngài;",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.935Z",
      "updatedAt": "2024-09-07T14:17:07.935Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "27e46922-d8eb-42da-be01-74c6e154f4fb",
      "number": 24,
      "content": "Trái đất đầy dẫy muôn vật do Ngài dựng nên.",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.935Z",
      "updatedAt": "2024-09-07T14:17:07.935Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "2b98ab39-0b5f-4afc-8e06-56d47f632415",
      "number": 25,
      "content": "Kìa là đại dương sâu rộng mênh mông,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.938Z",
      "updatedAt": "2024-09-07T14:17:07.938Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "bc7b1764-aaea-4306-948d-7130a13c86ef",
      "number": 25,
      "content": "Trong đó chứa nhung nhúc biết bao sinh vật;",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.938Z",
      "updatedAt": "2024-09-07T14:17:07.938Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "3a014bfc-24fa-4f55-a1be-bb54c9895262",
      "number": 25,
      "content": "Thật là vô số sinh vật lớn và nhỏ.",
      "order": 2,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.938Z",
      "updatedAt": "2024-09-07T14:17:07.938Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "3682e914-77f2-4173-9210-ea0faa3a5290",
      "number": 26,
      "content": "Rồi cũng có các ghe thuyền tàu bè qua lại,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.943Z",
      "updatedAt": "2024-09-07T14:17:07.943Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "b2c4b39f-98e4-4224-83bc-3821718c7955",
      "number": 26,
      "content": "Và con quái vật Ngài đã dựng nên đùa chơi trong đó.",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.943Z",
      "updatedAt": "2024-09-07T14:17:07.943Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "a3044eae-e898-46ed-913d-c9efa5af15cd",
      "number": 27,
      "content": "Tất cả đều trông chờ nơi Ngài,",
      "order": 0,
      "parNumber": 59,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.945Z",
      "updatedAt": "2024-09-07T14:17:07.945Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "941444d6-14c2-43f0-abee-71d173c1c994",
      "number": 27,
      "content": "Để Ngài ban cho chúng vật thực phải thì.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.945Z",
      "updatedAt": "2024-09-07T14:17:07.945Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "81dcf00f-3d73-4c31-bd07-d2a44c9f21f4",
      "number": 28,
      "content": "Khi Ngài cho ăn, chúng liền tụ lại;",
      "order": 0,
      "parNumber": 61,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.948Z",
      "updatedAt": "2024-09-07T14:17:07.948Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "fafebec2-2865-4c3e-9997-fbe1ad2b7503",
      "number": 28,
      "content": "Khi Ngài sè tay ra, chúng được ăn uống no nê.",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.948Z",
      "updatedAt": "2024-09-07T14:17:07.948Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "9f45732e-b459-490e-9183-05158ac951a1",
      "number": 29,
      "content": "Khi Ngài ẩn mặt, chúng hoảng kinh bối rối;",
      "order": 0,
      "parNumber": 63,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.950Z",
      "updatedAt": "2024-09-07T14:17:07.950Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "02515676-95e6-46a6-bce1-9253f811d296",
      "number": 29,
      "content": "Khi Ngài lấy hơi thở chúng đi, chúng liền chết",
      "order": 1,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.950Z",
      "updatedAt": "2024-09-07T14:17:07.950Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "c19a3064-d5cb-4130-a829-30d0ca10e496",
      "number": 29,
      "content": "Và trở về cát bụi.",
      "order": 2,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.950Z",
      "updatedAt": "2024-09-07T14:17:07.950Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "c44e1129-ae81-42dc-add9-f45dc663f946",
      "number": 30,
      "content": "Khi Ngài ban Thần Khí Ngài ra, chúng được dựng nên,",
      "order": 0,
      "parNumber": 66,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.953Z",
      "updatedAt": "2024-09-07T14:17:07.953Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "65293328-582f-43e7-bf86-8997a80303d0",
      "number": 30,
      "content": "Và Ngài làm cho mặt đất mới lại.",
      "order": 1,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.953Z",
      "updatedAt": "2024-09-07T14:17:07.953Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "09c40351-c32e-41c9-a5a5-f13719f26260",
      "number": 31,
      "content": "Nguyện vinh hiển của CHÚA còn đến đời đời.",
      "order": 0,
      "parNumber": 68,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.955Z",
      "updatedAt": "2024-09-07T14:17:07.955Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "e3b80e69-9e33-41a3-9ac8-f4b74028aa46",
      "number": 31,
      "content": "Nguyện CHÚA vui thích về các công việc Ngài.",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.955Z",
      "updatedAt": "2024-09-07T14:17:07.955Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "3d65b547-3f02-4020-81ab-02b10d6f71ed",
      "number": 32,
      "content": "Ngài nhìn vào trái đất, nó run lên lẩy bẩy;",
      "order": 0,
      "parNumber": 70,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.958Z",
      "updatedAt": "2024-09-07T14:17:07.958Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "1307daa8-871e-4b44-9fba-793e45532926",
      "number": 32,
      "content": "Ngài đụng đến núi non, chúng bốc khói mịt mù.",
      "order": 1,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.958Z",
      "updatedAt": "2024-09-07T14:17:07.958Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "42391436-55c4-4fe7-9c7f-3eb9eb50410c",
      "number": 33,
      "content": "Hễ tôi còn sống bao lâu, tôi sẽ ca tụng CHÚA bấy lâu;",
      "order": 0,
      "parNumber": 72,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.960Z",
      "updatedAt": "2024-09-07T14:17:07.960Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "a54000e4-52ec-4503-9a93-79caf0357f34",
      "number": 33,
      "content": "Hễ tôi còn sống chừng nào, tôi sẽ ca ngợi Đức Chúa Trời tôi chừng nấy.",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.960Z",
      "updatedAt": "2024-09-07T14:17:07.960Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "9f4bd514-7d4f-4417-823d-67b52868abec",
      "number": 34,
      "content": "Nguyện những suy nghĩ của tôi được đẹp lòng Ngài;",
      "order": 0,
      "parNumber": 74,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.963Z",
      "updatedAt": "2024-09-07T14:17:07.963Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "88d97779-1849-41d4-805c-0835a51310cb",
      "number": 34,
      "content": "Về phần tôi, tôi sẽ cứ vui mừng trong CHÚA.",
      "order": 1,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.963Z",
      "updatedAt": "2024-09-07T14:17:07.963Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "45e37511-16d5-4c7d-ad5f-c50bbf4dc057",
      "number": 35,
      "content": "Nguyện những kẻ tội lỗi bị diệt trừ khỏi mặt đất;",
      "order": 0,
      "parNumber": 76,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.966Z",
      "updatedAt": "2024-09-07T14:17:07.966Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "bae09153-2caf-4b40-b311-14a99a09b30d",
      "number": 35,
      "content": "Nguyện những kẻ ác không còn nữa.",
      "order": 1,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.966Z",
      "updatedAt": "2024-09-07T14:17:07.966Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "f22dcc5e-d5f8-4743-91d5-15b21ad881cb",
      "number": 35,
      "content": "Hãy chúc tụng CHÚA, hỡi linh hồn ta,",
      "order": 2,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.966Z",
      "updatedAt": "2024-09-07T14:17:07.966Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    },
    {
      "id": "d154ff96-f816-4cb6-9f47-751c9e922c8a",
      "number": 35,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 3,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:17:07.966Z",
      "updatedAt": "2024-09-07T14:17:07.966Z",
      "chapterId": "fc70a6a7-08f0-4587-8701-fe455629fc09"
    }
  ],
