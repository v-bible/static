{
  "chapter": {
    "id": "0a8910d4-dc9d-45aa-9f33-108cc91449ef",
    "number": 11,
    "ref": "https://www.bible.com/bible/19/PSA.11.BD2011",
    "createdAt": "2024-09-07T11:51:10.699Z",
    "updatedAt": "2024-09-07T11:51:10.699Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "3e8007a9-f2d2-48f4-8b57-2cdfb567776d",
      "number": 1,
      "content": "Tôi nương náu mình nơi CHÚA.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.792Z",
      "updatedAt": "2024-09-07T13:56:59.792Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "256bd089-903b-4699-864c-be6c60ce34de",
      "number": 1,
      "content": "Thế mà sao các người lại bảo linh hồn tôi rằng,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.792Z",
      "updatedAt": "2024-09-07T13:56:59.792Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "f877caf1-f885-4786-8c1e-afe3eebe9986",
      "number": 1,
      "content": "“Hãy làm như con chim và bay lên núi trốn đi”?",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.792Z",
      "updatedAt": "2024-09-07T13:56:59.792Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "8fab01f5-edd1-44c6-b857-d8e2977a949a",
      "number": 2,
      "content": "Vì kìa, kẻ ác đã giương cung;",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.802Z",
      "updatedAt": "2024-09-07T13:56:59.802Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "44336ad7-7877-47aa-a8f5-89262dbe6bdf",
      "number": 2,
      "content": "Chúng đã đặt mũi tên vào dây cung,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.802Z",
      "updatedAt": "2024-09-07T13:56:59.802Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "2cc5f8a1-b6c1-4542-8fbd-330599daff4f",
      "number": 2,
      "content": "Để từ trong bóng tối nhắm bắn người có lòng chính trực.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.802Z",
      "updatedAt": "2024-09-07T13:56:59.802Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "de0b8a78-2d16-4cce-a73a-90fddcc1b325",
      "number": 3,
      "content": "Nếu các nền tảng bị phá hủy,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.804Z",
      "updatedAt": "2024-09-07T13:56:59.804Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "2900e80d-e319-46a7-b0aa-064290cca770",
      "number": 3,
      "content": "Người ngay lành còn có thể làm gì?",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.804Z",
      "updatedAt": "2024-09-07T13:56:59.804Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "596cbac1-e9ff-493e-a0ef-3c9839af77ea",
      "number": 4,
      "content": "CHÚA ngự trong đền thánh Ngài;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.806Z",
      "updatedAt": "2024-09-07T13:56:59.806Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "a832c5ae-69fa-4501-b78a-23bd73fb8afd",
      "number": 4,
      "content": "CHÚA ngự trên ngôi Ngài trên thiên đàng.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.806Z",
      "updatedAt": "2024-09-07T13:56:59.806Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "8beb7c22-db75-404e-99da-b1fe2878de0b",
      "number": 4,
      "content": "Mắt Ngài nhìn thấy rõ hết; mí mắt Ngài dò xét con cái loài người.",
      "order": 2,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.806Z",
      "updatedAt": "2024-09-07T13:56:59.806Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "2d516804-a1ae-4a76-8ca8-445381e952d9",
      "number": 5,
      "content": "CHÚA thử nghiệm người ngay lành,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.808Z",
      "updatedAt": "2024-09-07T13:56:59.808Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "d52f9cf8-5530-42cf-af21-f0f81e1911c1",
      "number": 5,
      "content": "Nhưng linh hồn Ngài ghét kẻ ác và kẻ ham mến bạo tàn.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.808Z",
      "updatedAt": "2024-09-07T13:56:59.808Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "c546e613-9d3e-41b6-88a4-d98c275431f8",
      "number": 6,
      "content": "Ngài sẽ giáng mưa than lửa và diêm sinh xuống kẻ gian ác,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.810Z",
      "updatedAt": "2024-09-07T13:56:59.810Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "d193a1fd-e688-46ba-8dfe-be386f06358f",
      "number": 6,
      "content": "Gió phỏng sẽ là phần trong chén của chúng.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.810Z",
      "updatedAt": "2024-09-07T13:56:59.810Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "6bd2403e-da58-438b-a310-b6515b81ba38",
      "number": 7,
      "content": "Vì CHÚA là Đấng công chính.",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.813Z",
      "updatedAt": "2024-09-07T13:56:59.813Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "6772c69f-c8b1-41ea-8f20-48607674eb21",
      "number": 7,
      "content": "Ngài yêu mến sự công chính.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.813Z",
      "updatedAt": "2024-09-07T13:56:59.813Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    },
    {
      "id": "71173d13-f113-4cd4-9efd-4002a4358550",
      "number": 7,
      "content": "Người ngay lành sẽ được nhìn thấy thánh nhan Ngài.",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:59.813Z",
      "updatedAt": "2024-09-07T13:56:59.813Z",
      "chapterId": "0a8910d4-dc9d-45aa-9f33-108cc91449ef"
    }
  ],
