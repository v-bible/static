{
  "chapter": {
    "id": "d60d9773-5727-4a43-8f49-fecae1d4c922",
    "number": 84,
    "ref": "https://www.bible.com/bible/19/PSA.84.BD2011",
    "createdAt": "2024-09-07T11:51:11.281Z",
    "updatedAt": "2024-09-07T11:51:11.281Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "02056109-fa04-41cf-ba51-d96c6bd86a5d",
      "number": 1,
      "content": "Lạy CHÚA của các đạo quân,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.114Z",
      "updatedAt": "2024-09-07T14:12:33.114Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "6de3f96b-3863-4810-82c6-16e4c346fa09",
      "number": 1,
      "content": "Chỗ ở của Ngài thật đáng yêu mến thay!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.114Z",
      "updatedAt": "2024-09-07T14:12:33.114Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "75f8696d-893b-49f5-be27-be992e8f534f",
      "number": 2,
      "content": "Linh hồn con khao khát và thèm muốn",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.124Z",
      "updatedAt": "2024-09-07T14:12:33.124Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "6ac75d44-db86-412a-a6fd-23af6ca45388",
      "number": 2,
      "content": "Được vào sân của nhà CHÚA biết bao.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.124Z",
      "updatedAt": "2024-09-07T14:12:33.124Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "24be0a21-cec5-472b-9ac5-c2cae01121da",
      "number": 2,
      "content": "Lòng con và thân xác con ước ao được về đó",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.124Z",
      "updatedAt": "2024-09-07T14:12:33.124Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "b36bb994-6da1-4e1d-b4b9-0a1126748cf0",
      "number": 2,
      "content": "Để reo mừng chúc tụng Đức Chúa Trời hằng sống biết dường nào.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.124Z",
      "updatedAt": "2024-09-07T14:12:33.124Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "c8ba6267-850c-4d22-8c30-2fa97b80bbc2",
      "number": 3,
      "content": "Ngay cả con chim sẻ còn kiếm được một chỗ ở,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.126Z",
      "updatedAt": "2024-09-07T14:12:33.126Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "5725cbb8-00e8-43f7-aeb7-252f9222d53e",
      "number": 3,
      "content": "Con chim én còn có nơi để làm tổ",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.126Z",
      "updatedAt": "2024-09-07T14:12:33.126Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "ef438855-b9e2-47d3-b866-5fb1fd0718d2",
      "number": 3,
      "content": "Cho con nó gần bàn thờ Ngài,",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.126Z",
      "updatedAt": "2024-09-07T14:12:33.126Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "64c04193-e2a3-4537-a0e3-0ca9fe4dbba6",
      "number": 3,
      "content": "Ôi CHÚA của các đạo quân,",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.126Z",
      "updatedAt": "2024-09-07T14:12:33.126Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "20654ed2-7f74-4e1a-b2ef-011f894e62e0",
      "number": 3,
      "content": "Vua của con và Đức Chúa Trời của con.",
      "order": 4,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.126Z",
      "updatedAt": "2024-09-07T14:12:33.126Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "5e4a0b94-0ee7-4210-ab82-3c660683197a",
      "number": 4,
      "content": "Phước cho người nào được ở trong nhà Ngài!",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.129Z",
      "updatedAt": "2024-09-07T14:12:33.129Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "cf80b3bd-2513-4566-a870-e87ec1d644f3",
      "number": 4,
      "content": "Họ sẽ được ca ngợi Ngài mãi mãi. (Sê-la)",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.129Z",
      "updatedAt": "2024-09-07T14:12:33.129Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "fb8a7c6b-1ae3-476f-a32a-7b8d747b2769",
      "number": 5,
      "content": "Phước thay cho người cậy Ngài làm sức mạnh mình,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.131Z",
      "updatedAt": "2024-09-07T14:12:33.131Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "e59e8fd8-c361-441f-abf5-2da0d6de8815",
      "number": 5,
      "content": "Và để lòng mình vào cuộc hành trình về nhà Ngài.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.131Z",
      "updatedAt": "2024-09-07T14:12:33.131Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "d9267b4e-be42-4ed4-bceb-bae44dddb508",
      "number": 6,
      "content": "Khi họ đi qua thung lũng buồn đau than khóc,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.134Z",
      "updatedAt": "2024-09-07T14:12:33.134Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "43648ed9-ae9c-4ac6-b67d-49b2f32f6bf1",
      "number": 6,
      "content": "Họ sẽ biến nó thành nơi có những suối nước trào tuôn,",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.134Z",
      "updatedAt": "2024-09-07T14:12:33.134Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "946242db-60b3-478b-9282-4738de98e113",
      "number": 6,
      "content": "Như mưa đầu mùa biến nơi ấy thành vùng có nhiều vũng nước.",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.134Z",
      "updatedAt": "2024-09-07T14:12:33.134Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "65667da9-7455-40d5-9489-c4abd0fa6bd0",
      "number": 7,
      "content": "Họ càng đi tới, sức lực họ càng tăng lên,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.136Z",
      "updatedAt": "2024-09-07T14:12:33.136Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "381e9dff-cb49-450a-b3ed-953e2d8264fb",
      "number": 7,
      "content": "Cho đến khi mọi người ra mắt Đức Chúa Trời tại Si-ôn.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.136Z",
      "updatedAt": "2024-09-07T14:12:33.136Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "ed4ccb6b-5bd2-4224-8be2-b3c5b5d427a5",
      "number": 8,
      "content": "Lạy CHÚA, Đức Chúa Trời của các đạo quân, xin lắng nghe lời cầu nguyện của con;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.138Z",
      "updatedAt": "2024-09-07T14:12:33.138Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "6c753604-24d1-4a8b-bf83-da72fd180f42",
      "number": 8,
      "content": "Đức Chúa Trời của Gia-cốp ôi, xin nghiêng tai nghe tiếng cầu xin của con. (Sê-la)",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.138Z",
      "updatedAt": "2024-09-07T14:12:33.138Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "ad806e71-a5d5-4357-9b8c-2460b69d4f56",
      "number": 9,
      "content": "Đức Chúa Trời ôi, xin đoái xem người lãnh đạo của chúng con;",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.140Z",
      "updatedAt": "2024-09-07T14:12:33.140Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "4fabf5c7-31eb-4c8c-bf2e-518516a2d182",
      "number": 9,
      "content": "Xin cúi nhìn vào mặt người được xức dầu của Ngài.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.140Z",
      "updatedAt": "2024-09-07T14:12:33.140Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "9909f6f3-e401-4046-9623-4e7adc40d607",
      "number": 10,
      "content": "Vì một ngày được ở trong các sân của nhà Ngài",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.142Z",
      "updatedAt": "2024-09-07T14:12:33.142Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "7f408bb9-4c33-4d53-b07c-91214d7bdd53",
      "number": 10,
      "content": "Thật đáng hơn cả ngàn ngày ở những nơi khác.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.142Z",
      "updatedAt": "2024-09-07T14:12:33.142Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "b65bc981-f278-4f0d-8f3d-053451cedc28",
      "number": 10,
      "content": "Tôi thà đứng giữ cửa trong nhà Đức Chúa Trời của tôi,",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.142Z",
      "updatedAt": "2024-09-07T14:12:33.142Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "33da5dc7-d994-44b2-ad5b-6712af130b98",
      "number": 10,
      "content": "Còn hơn ở trong biệt thự của những kẻ gian ác.",
      "order": 3,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.142Z",
      "updatedAt": "2024-09-07T14:12:33.142Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "cad27c5c-ee3c-4be5-86f5-2f1c07a4e66f",
      "number": 11,
      "content": "Vì CHÚA Đức Chúa Trời là mặt trời và thuẫn khiên;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.145Z",
      "updatedAt": "2024-09-07T14:12:33.145Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "9aeb9e34-936b-4b20-adb6-ddcbe3327654",
      "number": 11,
      "content": "CHÚA sẽ ban ân sủng và vinh hiển;",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.145Z",
      "updatedAt": "2024-09-07T14:12:33.145Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "2f8288f6-a2d2-45dd-8e8d-90e4a10428f0",
      "number": 11,
      "content": "Ngài chẳng giữ lại phước hạnh nào",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.145Z",
      "updatedAt": "2024-09-07T14:12:33.145Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "4b36114f-972e-4250-90fd-d025284019c0",
      "number": 11,
      "content": "Đối với người ăn ở ngay lành.",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.145Z",
      "updatedAt": "2024-09-07T14:12:33.145Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "ed594b97-08b1-40a4-8f43-36b91e5adc3a",
      "number": 12,
      "content": "Lạy CHÚA của các đạo quân,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.147Z",
      "updatedAt": "2024-09-07T14:12:33.147Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    },
    {
      "id": "090f8f55-675c-4b75-919f-fc2a6b2f6d23",
      "number": 12,
      "content": "Phước thay cho người tin cậy Ngài!",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:12:33.147Z",
      "updatedAt": "2024-09-07T14:12:33.147Z",
      "chapterId": "d60d9773-5727-4a43-8f49-fecae1d4c922"
    }
  ],
