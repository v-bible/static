{
  "chapter": {
    "id": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c",
    "number": 123,
    "ref": "https://www.bible.com/bible/19/PSA.123.BD2011",
    "createdAt": "2024-09-07T11:51:11.526Z",
    "updatedAt": "2024-09-07T11:51:11.526Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "ccd42978-8dc5-46c1-9b9b-8ba9f17a1640",
      "number": 1,
      "content": "Lạy Đấng ngự trên các tầng trời,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.779Z",
      "updatedAt": "2024-09-07T14:21:37.779Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "d3f6676a-7626-4c74-abce-98c1c78da270",
      "number": 1,
      "content": "Con ngước mắt lên hướng về Ngài.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.779Z",
      "updatedAt": "2024-09-07T14:21:37.779Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "856d842d-09a1-46a8-8086-da12ae88cbdb",
      "number": 2,
      "content": "Kìa, như mắt các đầy tớ",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.788Z",
      "updatedAt": "2024-09-07T14:21:37.788Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "34cbf931-4d99-4d66-a547-744a4e8568e5",
      "number": 2,
      "content": "Trông nơi tay ông chủ họ thể nào,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.788Z",
      "updatedAt": "2024-09-07T14:21:37.788Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "0e356d4a-6a26-4df4-8c95-e5ec60fd55a9",
      "number": 2,
      "content": "Như mắt người tớ gái",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.788Z",
      "updatedAt": "2024-09-07T14:21:37.788Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "87ce6c84-d712-48a7-895b-e354a91ad811",
      "number": 2,
      "content": "Trông nơi tay bà chủ nàng thể nào,",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.788Z",
      "updatedAt": "2024-09-07T14:21:37.788Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "3828f8ea-ac67-4703-a820-a78d2cd0b4f3",
      "number": 2,
      "content": "Mắt chúng con cũng vọng trông nơi CHÚA, Đức Chúa Trời chúng con, thể ấy,",
      "order": 4,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.788Z",
      "updatedAt": "2024-09-07T14:21:37.788Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "f6c80aae-a133-4240-a306-3f90c79bb36f",
      "number": 2,
      "content": "Cho đến khi Ngài bày tỏ ơn thương xót đối với chúng con.",
      "order": 5,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.788Z",
      "updatedAt": "2024-09-07T14:21:37.788Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "91600a8e-68a3-4687-bb4f-71ee6623adc9",
      "number": 3,
      "content": "Xin thương xót chúng con, CHÚA ôi, xin thương xót chúng con,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.792Z",
      "updatedAt": "2024-09-07T14:21:37.792Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "ea86f779-96f7-4529-9c1c-9b64f8ab6fdc",
      "number": 3,
      "content": "Vì chúng con bị khinh khi quá đỗi.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.792Z",
      "updatedAt": "2024-09-07T14:21:37.792Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "699cc0f9-e398-429a-9145-05626fd975f2",
      "number": 4,
      "content": "Linh hồn chúng con quá ngao ngán",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.794Z",
      "updatedAt": "2024-09-07T14:21:37.794Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "6fa4095c-c8de-4d07-92ad-eeba1780be96",
      "number": 4,
      "content": "Những lời nhạo báng của đám người kiêu ngạo,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.794Z",
      "updatedAt": "2024-09-07T14:21:37.794Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    },
    {
      "id": "b09b53d3-ac20-4a9f-8428-79d02cb88db8",
      "number": 4,
      "content": "Cùng với giọng khinh người của những kẻ kiêu căng.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:37.794Z",
      "updatedAt": "2024-09-07T14:21:37.794Z",
      "chapterId": "3b1084ef-bb08-4ba7-99c4-f1b65abfc49c"
    }
  ],
