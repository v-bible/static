{
  "chapter": {
    "id": "a3df845a-9e80-4804-a5e7-7f85a1f72724",
    "number": 75,
    "ref": "https://www.bible.com/bible/19/PSA.75.BD2011",
    "createdAt": "2024-09-07T11:51:11.207Z",
    "updatedAt": "2024-09-07T11:51:11.207Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "2e7947d4-f69e-4482-a1ed-388aba96f2ce",
      "number": 1,
      "content": "Đức Chúa Trời ôi, chúng con cảm tạ Ngài;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.004Z",
      "updatedAt": "2024-09-07T14:10:46.004Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "67961613-11b6-41e1-b172-288f0ce197a9",
      "number": 1,
      "content": "Chúng con cảm tạ Ngài vì danh Ngài ở gần,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.004Z",
      "updatedAt": "2024-09-07T14:10:46.004Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "807d68c5-a0ff-47b9-8019-db73cff1ea4e",
      "number": 1,
      "content": "Vì các việc diệu kỳ của Ngài luôn được truyền tụng cao rao.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.004Z",
      "updatedAt": "2024-09-07T14:10:46.004Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "01b3af0e-2f4c-441a-be04-8938f9ad819f",
      "number": 2,
      "content": "Khi đến thời điểm Ta đã định,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.013Z",
      "updatedAt": "2024-09-07T14:10:46.013Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "83d0b923-cc54-4f6c-9ea1-39484d808221",
      "number": 2,
      "content": "Chính Ta sẽ xét xử công minh.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.013Z",
      "updatedAt": "2024-09-07T14:10:46.013Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "10313120-c6fe-4655-85cb-92388daf5e77",
      "number": 3,
      "content": "Khi đất và mọi kẻ sống trong nó bị lung lay,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.015Z",
      "updatedAt": "2024-09-07T14:10:46.015Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "ef1df7a4-7aa8-413b-a818-a20dbf3f1575",
      "number": 3,
      "content": "Chính Ta nắm giữ các trụ nó để nó được vững vàng. (Sê-la)",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.015Z",
      "updatedAt": "2024-09-07T14:10:46.015Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "8dd1ebeb-f459-4aaf-887b-1ac31b7c65a0",
      "number": 4,
      "content": "Ta phán với những kẻ kiêu căng, “Chớ kiêu căng nữa!”",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.018Z",
      "updatedAt": "2024-09-07T14:10:46.018Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "b28354d9-c406-4d9c-901a-f6ea875ed1d3",
      "number": 4,
      "content": "Với những kẻ gian ác, “Chớ ngước sừng các ngươi lên;",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.018Z",
      "updatedAt": "2024-09-07T14:10:46.018Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "6a183af4-6ed1-4423-90be-ff538b77e4d2",
      "number": 5,
      "content": "Chớ ngước sừng ngưỡng cổ dương oai;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.020Z",
      "updatedAt": "2024-09-07T14:10:46.020Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "d69ada96-c7a9-48f7-ae9b-c16f9184c62d",
      "number": 5,
      "content": "Chớ nói ra những lời kiêu căng xấc xược.”",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.020Z",
      "updatedAt": "2024-09-07T14:10:46.020Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "9a3147b1-4686-4a9c-81e8-0b60364ffed0",
      "number": 6,
      "content": "Vì chẳng phải từ phương đông hoặc từ phương tây,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.023Z",
      "updatedAt": "2024-09-07T14:10:46.023Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "f1c66e44-f864-48d3-a654-3af5e1b800fd",
      "number": 6,
      "content": "Hay từ miền sa mạc mà người ta được tôn trọng,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.023Z",
      "updatedAt": "2024-09-07T14:10:46.023Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "4c761af8-2507-43dc-bf03-a8fba50a4f25",
      "number": 7,
      "content": "Nhưng ấy là từ Đức Chúa Trời, Đấng Đoán Xét;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.025Z",
      "updatedAt": "2024-09-07T14:10:46.025Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "053e8c74-0c59-47b2-a913-0d60ab4a2886",
      "number": 7,
      "content": "Ngài hạ kẻ này xuống và nhấc người kia lên.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.025Z",
      "updatedAt": "2024-09-07T14:10:46.025Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "a35d15d7-316c-49a2-b5fb-d6d14fe5a8a6",
      "number": 8,
      "content": "Trong tay CHÚA có một chén rượu,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.028Z",
      "updatedAt": "2024-09-07T14:10:46.028Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "eddaf911-6272-430d-810d-393b67fc00b6",
      "number": 8,
      "content": "Một chén rượu pha, đỏ hồng, và sủi bọt;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.028Z",
      "updatedAt": "2024-09-07T14:10:46.028Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "bb9053eb-22c4-491c-ba02-b81de007509c",
      "number": 8,
      "content": "Ngài rót nó ra",
      "order": 2,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.028Z",
      "updatedAt": "2024-09-07T14:10:46.028Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "ae6f61d6-3ef8-4758-bcc5-9a402b74cd7c",
      "number": 8,
      "content": "Và bắt mọi kẻ ác trên đất phải uống;",
      "order": 3,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.028Z",
      "updatedAt": "2024-09-07T14:10:46.028Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "f2ad8779-ee1f-4fca-8a77-e4564576e723",
      "number": 8,
      "content": "Chúng phải uống cạn cho đến giọt rượu cuối cùng.",
      "order": 4,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.028Z",
      "updatedAt": "2024-09-07T14:10:46.028Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "c6971cd7-afd7-4117-ae81-ea54ec1f78a6",
      "number": 9,
      "content": "Riêng phần tôi, tôi sẽ ca tụng Ngài mãi mãi;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.034Z",
      "updatedAt": "2024-09-07T14:10:46.034Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "99fd5c31-d106-4c5e-8233-60af6dcd690e",
      "number": 9,
      "content": "Tôi sẽ ca ngợi Đức Chúa Trời của Gia-cốp.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.034Z",
      "updatedAt": "2024-09-07T14:10:46.034Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "33c78d69-b95a-4921-9eb1-2f57758f91fa",
      "number": 10,
      "content": "Vì Ngài sẽ chặt trụi các sừng của quân gian ác,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.036Z",
      "updatedAt": "2024-09-07T14:10:46.036Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    },
    {
      "id": "ec07a9e1-cff9-4f53-a687-3331f6ae4eef",
      "number": 10,
      "content": "Còn sừng của người ngay lành sẽ được nhấc cao lên.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:10:46.036Z",
      "updatedAt": "2024-09-07T14:10:46.036Z",
      "chapterId": "a3df845a-9e80-4804-a5e7-7f85a1f72724"
    }
  ],
