{
  "chapter": {
    "id": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27",
    "number": 8,
    "ref": "https://www.bible.com/bible/19/PSA.8.BD2011",
    "createdAt": "2024-09-07T11:51:10.674Z",
    "updatedAt": "2024-09-07T11:51:10.674Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "08d8bd0c-76b6-430f-8138-bb87d688bcdc",
      "number": 1,
      "content": "Lạy CHÚA, Chúa của chúng con,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.796Z",
      "updatedAt": "2024-09-07T13:56:01.796Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "640ceef5-d667-495b-bf9b-b4beef606882",
      "number": 1,
      "content": "Danh Ngài thật uy nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.796Z",
      "updatedAt": "2024-09-07T13:56:01.796Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "1c23dc2b-d9a8-4328-bd88-9c78bf0183d6",
      "number": 1,
      "content": "Ngài đã đặt vinh quang Ngài trên các tầng trời!",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.796Z",
      "updatedAt": "2024-09-07T13:56:01.796Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "ab321b9a-1107-4e03-97a1-2fc5203f22a3",
      "number": 2,
      "content": "Từ miệng các trẻ thơ và các ấu nhi Ngài đã biểu lộ quyền năng,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.806Z",
      "updatedAt": "2024-09-07T13:56:01.806Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "e8ff38ce-1ce4-43ae-be65-e1609039bf11",
      "number": 2,
      "content": "Để quân chống lại Ngài thấy rõ,",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.806Z",
      "updatedAt": "2024-09-07T13:56:01.806Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "0ee25780-d597-48e6-8aad-851b56048473",
      "number": 2,
      "content": "Để kẻ thù và kẻ báo thù phải im hơi bỏ cuộc.",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.806Z",
      "updatedAt": "2024-09-07T13:56:01.806Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "b1796a83-98ec-4a91-bacd-56356e2c5711",
      "number": 3,
      "content": "Khi con lặng ngắm cõi trời cao, công việc của ngón tay Ngài,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.808Z",
      "updatedAt": "2024-09-07T13:56:01.808Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "becfd109-822f-41df-acad-382fe3856dec",
      "number": 3,
      "content": "Mặt trăng và các ngôi sao mà Ngài đã đặt trên không trung;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.808Z",
      "updatedAt": "2024-09-07T13:56:01.808Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "1dd95186-35c3-496f-ade6-b0f038341b68",
      "number": 4,
      "content": "Loài người là gì mà Ngài phải quan tâm đến?",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.811Z",
      "updatedAt": "2024-09-07T13:56:01.811Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "20e760ca-f957-41c2-9366-ca5398392f49",
      "number": 4,
      "content": "Con của loài người là chi mà Ngài phải bận lòng?",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.811Z",
      "updatedAt": "2024-09-07T13:56:01.811Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "91a8d8cc-45aa-4150-acb9-60840156a099",
      "number": 5,
      "content": "Ngài đã dựng nên loài người thấp hơn các vị thần một chút;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.813Z",
      "updatedAt": "2024-09-07T13:56:01.813Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "b4691317-1f06-4548-a710-e3daf38fe01c",
      "number": 5,
      "content": "Ngài đội trên đầu loài người vinh hiển và tôn trọng.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.813Z",
      "updatedAt": "2024-09-07T13:56:01.813Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "a22500d1-bc45-41d4-afc6-d1e0143ef587",
      "number": 6,
      "content": "Ngài cho loài người quản trị mọi vật do tay Ngài dựng nên;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.816Z",
      "updatedAt": "2024-09-07T13:56:01.816Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "aee90ec4-bd41-4031-a65d-02e3401f130b",
      "number": 6,
      "content": "Ngài bắt muôn vật phục dưới chân loài người,",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.816Z",
      "updatedAt": "2024-09-07T13:56:01.816Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "cece206d-8435-43e6-90c7-d0be980d8e62",
      "number": 7,
      "content": "Tức tất cả chiên và bò,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.818Z",
      "updatedAt": "2024-09-07T13:56:01.818Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "ad9e0c66-36a7-403a-8ab1-ec7da62efb1e",
      "number": 7,
      "content": "Cùng các thú vật ngoài đồng,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.818Z",
      "updatedAt": "2024-09-07T13:56:01.818Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "67183997-c130-4f2b-890d-9c85071cee16",
      "number": 8,
      "content": "Các loài chim trên trời và mọi loài cá dưới biển,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.821Z",
      "updatedAt": "2024-09-07T13:56:01.821Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "a8a5c40f-b80b-494d-bc6c-ff026d03cc59",
      "number": 8,
      "content": "Tức mọi vật bơi lội trong các dòng nước biển.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.821Z",
      "updatedAt": "2024-09-07T13:56:01.821Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "d25021e5-9666-4178-8f19-6b200b636dd5",
      "number": 9,
      "content": "Lạy CHÚA, Chúa của chúng con,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.824Z",
      "updatedAt": "2024-09-07T13:56:01.824Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    },
    {
      "id": "581dae64-6e41-4f1b-8ff1-306f0583c2a5",
      "number": 9,
      "content": "Danh Ngài thật uy nghi trên khắp đất biết bao!",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T13:56:01.824Z",
      "updatedAt": "2024-09-07T13:56:01.824Z",
      "chapterId": "15b0a23d-ff3d-4a01-9c70-6f6c09b54d27"
    }
  ],
