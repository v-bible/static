{
  "chapter": {
    "id": "6b74277f-2174-4c66-bb8e-25ed2e551cb0",
    "number": 124,
    "ref": "https://www.bible.com/bible/19/PSA.124.BD2011",
    "createdAt": "2024-09-07T11:51:11.529Z",
    "updatedAt": "2024-09-07T11:51:11.529Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "5a0f5510-0731-4f04-93c8-68c47cc0bdcb",
      "number": 1,
      "content": "Nếu chẳng phải CHÚA bênh vực chúng ta,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.116Z",
      "updatedAt": "2024-09-07T14:21:44.116Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "8916b97c-34c6-4aed-96fc-0d1499c9c357",
      "number": 1,
      "content": "Hỡi I-sơ-ra-ên, hãy nói đi.",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.116Z",
      "updatedAt": "2024-09-07T14:21:44.116Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "f082cec0-f17d-400c-a109-906d91de6132",
      "number": 2,
      "content": "Nếu chẳng phải CHÚA bênh vực chúng ta,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.126Z",
      "updatedAt": "2024-09-07T14:21:44.126Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "41820090-a955-4b74-a5e7-8538967540cd",
      "number": 2,
      "content": "Thì khi người ta nổi lên chống lại chúng ta,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.126Z",
      "updatedAt": "2024-09-07T14:21:44.126Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "6aed8e6b-0aa6-4b25-be4f-41c8791ee6d3",
      "number": 3,
      "content": "Khi cơn cuồng nộ của chúng bừng lên nghịch lại chúng ta,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.128Z",
      "updatedAt": "2024-09-07T14:21:44.128Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "3314353f-b8ef-4fd1-badc-8d2a5845d03b",
      "number": 3,
      "content": "Chắc hẳn chúng đã nuốt chửng chúng ta rồi.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.128Z",
      "updatedAt": "2024-09-07T14:21:44.128Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "66f47010-810d-44b0-a8de-d04dbb9ba33c",
      "number": 4,
      "content": "Các dòng nước lũ chắc đã nhận chìm chúng ta;",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.131Z",
      "updatedAt": "2024-09-07T14:21:44.131Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "ebddc932-2ade-4322-bf1b-87e1506b3516",
      "number": 4,
      "content": "Luồng nước mạnh chắc đã cuốn trôi linh hồn chúng ta;",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.131Z",
      "updatedAt": "2024-09-07T14:21:44.131Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "76805701-f4fe-4128-ac0e-4d3290bdb5c1",
      "number": 5,
      "content": "Các đợt sóng hung hãn",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.133Z",
      "updatedAt": "2024-09-07T14:21:44.133Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "fc6e7554-1a5b-490f-94b8-0866b183a81e",
      "number": 5,
      "content": "Chắc đã cuốn mất linh hồn chúng ta rồi.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.133Z",
      "updatedAt": "2024-09-07T14:21:44.133Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "aa70f7c9-38e9-445b-99e1-a091d3f07b9e",
      "number": 6,
      "content": "Chúc tụng CHÚA,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.136Z",
      "updatedAt": "2024-09-07T14:21:44.136Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "dfd2ccce-abd9-4ebe-9b18-210bcaf68e7f",
      "number": 6,
      "content": "Đấng không cho phép chúng ta",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.136Z",
      "updatedAt": "2024-09-07T14:21:44.136Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "37fa1ff5-d3e4-4f85-bcb2-7856fbe7e68d",
      "number": 6,
      "content": "Làm mồi cho răng chúng.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.136Z",
      "updatedAt": "2024-09-07T14:21:44.136Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "b0cd758e-ed78-4dc6-8022-c088cb666b49",
      "number": 7,
      "content": "Linh hồn chúng ta được giải thoát,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.138Z",
      "updatedAt": "2024-09-07T14:21:44.138Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "11c9b2d3-2c6e-4e99-aeba-c562bddf7825",
      "number": 7,
      "content": "Như con chim thoát khỏi bẫy của kẻ bẫy chim;",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.138Z",
      "updatedAt": "2024-09-07T14:21:44.138Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "7ae1bce3-21c2-4e4e-a80a-36e4d2c02372",
      "number": 7,
      "content": "Cái bẫy bị bẻ gãy,",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.138Z",
      "updatedAt": "2024-09-07T14:21:44.138Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "5c6a133c-130b-47a5-a41c-706407ed4fe8",
      "number": 7,
      "content": "Và chúng ta thoát ra ngoài.",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.138Z",
      "updatedAt": "2024-09-07T14:21:44.138Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "4ef26ab9-f280-466c-9788-70bfbf0ec307",
      "number": 8,
      "content": "Chúng ta được giúp đỡ nhờ danh CHÚA,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.141Z",
      "updatedAt": "2024-09-07T14:21:44.141Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    },
    {
      "id": "7fb70564-ad21-44de-abd2-bd9cae876b2b",
      "number": 8,
      "content": "Đấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:44.141Z",
      "updatedAt": "2024-09-07T14:21:44.141Z",
      "chapterId": "6b74277f-2174-4c66-bb8e-25ed2e551cb0"
    }
  ],
