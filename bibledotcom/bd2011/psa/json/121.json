{
  "chapter": {
    "id": "9c1319f9-57b8-4621-8637-013c6ce4a350",
    "number": 121,
    "ref": "https://www.bible.com/bible/19/PSA.121.BD2011",
    "createdAt": "2024-09-07T11:51:11.521Z",
    "updatedAt": "2024-09-07T11:51:11.521Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "1639d0f0-cacc-4053-8733-7c5e0c96ce56",
      "number": 1,
      "content": "Tôi ngước mắt nhìn lên đồi núi;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.225Z",
      "updatedAt": "2024-09-07T14:21:25.225Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "905becfd-4f39-4bdd-a146-2ff8b2bebe6c",
      "number": 1,
      "content": "Sự giúp đỡ tôi đến từ đâu?",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.225Z",
      "updatedAt": "2024-09-07T14:21:25.225Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "a7efeff9-97ec-4e48-ba37-74fb95a9fc2d",
      "number": 2,
      "content": "Sự giúp đỡ tôi đến từ CHÚA,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.235Z",
      "updatedAt": "2024-09-07T14:21:25.235Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "f7c9992c-8e67-4165-8cfb-38123e6959f3",
      "number": 2,
      "content": "Đấng dựng nên trời và đất.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.235Z",
      "updatedAt": "2024-09-07T14:21:25.235Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "8f6c1615-43fc-404f-92e6-35069c65cfad",
      "number": 3,
      "content": "Ngài sẽ không để cho chân bạn trượt ngã;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.239Z",
      "updatedAt": "2024-09-07T14:21:25.239Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "5f6642dc-b137-4ce9-8155-168164ae2aee",
      "number": 3,
      "content": "Đấng gìn giữ bạn không hề buồn ngủ.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.239Z",
      "updatedAt": "2024-09-07T14:21:25.239Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "903741a8-47dd-45a8-95cc-a7310838799f",
      "number": 4,
      "content": "Thật vậy Đấng gìn giữ I-sơ-ra-ên",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.241Z",
      "updatedAt": "2024-09-07T14:21:25.241Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "3e76a57a-a9ad-4a43-b21f-f583ea145b23",
      "number": 4,
      "content": "Không hề buồn ngủ và không bao giờ ngủ.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.241Z",
      "updatedAt": "2024-09-07T14:21:25.241Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "ff9e2cf2-cbc6-448b-b0e4-1402cd19511d",
      "number": 5,
      "content": "CHÚA là Đấng gìn giữ bạn;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.244Z",
      "updatedAt": "2024-09-07T14:21:25.244Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "0303bdc7-b598-4566-bb5f-1228255be7c2",
      "number": 5,
      "content": "CHÚA là bóng mát bên phải bạn.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.244Z",
      "updatedAt": "2024-09-07T14:21:25.244Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "10f1bef5-62c7-4255-9cd8-8c381174362a",
      "number": 6,
      "content": "Mặt trời sẽ không hại bạn ban ngày;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.247Z",
      "updatedAt": "2024-09-07T14:21:25.247Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "2244f6d4-4059-4e27-af33-73fa2773ba42",
      "number": 6,
      "content": "Mặt trăng sẽ không hại bạn ban đêm.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.247Z",
      "updatedAt": "2024-09-07T14:21:25.247Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "eb2fadf3-7c90-410c-a338-ef3a28ead228",
      "number": 7,
      "content": "CHÚA sẽ gìn giữ bạn khỏi mọi tai họa;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.249Z",
      "updatedAt": "2024-09-07T14:21:25.249Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "1521d7d2-5525-4ca3-8eb6-79035e84338e",
      "number": 7,
      "content": "Ngài sẽ gìn giữ mạng sống bạn.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.249Z",
      "updatedAt": "2024-09-07T14:21:25.249Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "eb9d75c1-c9e2-40ed-a75d-376369d547c2",
      "number": 8,
      "content": "CHÚA sẽ gìn giữ bạn",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.251Z",
      "updatedAt": "2024-09-07T14:21:25.251Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "ad42f213-f779-4cd9-8aa9-4acb77850605",
      "number": 8,
      "content": "Khi ra khi vào,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.251Z",
      "updatedAt": "2024-09-07T14:21:25.251Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    },
    {
      "id": "6125d97e-8bb5-4edf-beff-20306553333f",
      "number": 8,
      "content": "Từ nay cho đến đời đời.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:21:25.251Z",
      "updatedAt": "2024-09-07T14:21:25.251Z",
      "chapterId": "9c1319f9-57b8-4621-8637-013c6ce4a350"
    }
  ],
