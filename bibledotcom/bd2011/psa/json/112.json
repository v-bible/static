{
  "chapter": {
    "id": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4",
    "number": 112,
    "ref": "https://www.bible.com/bible/19/PSA.112.BD2011",
    "createdAt": "2024-09-07T11:51:11.494Z",
    "updatedAt": "2024-09-07T11:51:11.494Z",
    "bookId": "67614455-e41d-4311-bd36-6e33fe552385"
  },
  "verses": [
    {
      "id": "c6d33486-b84b-4180-9052-ad0b68c94831",
      "number": 1,
      "content": "Hãy ca ngợi CHÚA!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.505Z",
      "updatedAt": "2024-09-07T14:18:16.505Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "daca575c-c5da-4160-8023-001ba8f2159c",
      "number": 1,
      "content": "Phước cho người nào kính sợ CHÚA,",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.505Z",
      "updatedAt": "2024-09-07T14:18:16.505Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "b243f325-7bd6-47e7-b244-51ad63733f95",
      "number": 1,
      "content": "Người thích thú vô vàn các mạng lịnh Ngài.",
      "order": 2,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.505Z",
      "updatedAt": "2024-09-07T14:18:16.505Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "7351a4ac-6e4f-4ac7-ac36-05fc32005c28",
      "number": 2,
      "content": "Con cháu người ấy sẽ được cường thịnh trên đất.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.515Z",
      "updatedAt": "2024-09-07T14:18:16.515Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "944da1da-a6af-4cb2-9110-378ebdfa43e7",
      "number": 2,
      "content": "Dòng dõi người chính trực sẽ được phước.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.515Z",
      "updatedAt": "2024-09-07T14:18:16.515Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "9e0fec45-e0de-47c2-bc55-127323e2eae6",
      "number": 3,
      "content": "Giàu sang và phú quý sẽ ở trong nhà người ấy.",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.518Z",
      "updatedAt": "2024-09-07T14:18:16.518Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "f6bd6417-9529-4fbc-b624-af8b481324f1",
      "number": 3,
      "content": "Đức công chính của người ấy sẽ còn lại đời đời.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.518Z",
      "updatedAt": "2024-09-07T14:18:16.518Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "88440cf1-ebab-424b-9331-4d6e69f99402",
      "number": 4,
      "content": "Ánh sáng bừng lên trong tăm tối cho người chính trực,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.520Z",
      "updatedAt": "2024-09-07T14:18:16.520Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "badc9dea-1019-49a2-936d-f6fb30f5e782",
      "number": 4,
      "content": "Tức cho người nhân từ, thương xót, và ngay lành;",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.520Z",
      "updatedAt": "2024-09-07T14:18:16.520Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "68ec1bd8-e41a-4a1f-8f8f-a6e9e4c825c8",
      "number": 5,
      "content": "Phước cho người hay làm ơn và cho vay bất vụ lợi,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.522Z",
      "updatedAt": "2024-09-07T14:18:16.522Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "1fe85562-95ab-4142-9f86-7d3aca540fed",
      "number": 5,
      "content": "Một người điều hành công việc mình theo lẽ phải.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.522Z",
      "updatedAt": "2024-09-07T14:18:16.522Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "58d83696-389a-44e4-8a16-e6764c8e02aa",
      "number": 6,
      "content": "Chắc chắn người ấy sẽ không bao giờ bị rúng động.",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.526Z",
      "updatedAt": "2024-09-07T14:18:16.526Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "064b9f80-bad1-422e-970c-7da87b90e767",
      "number": 6,
      "content": "Người ngay lành sẽ được tưởng nhớ đến muôn đời.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.526Z",
      "updatedAt": "2024-09-07T14:18:16.526Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "5dd9f1d5-43b9-4092-8241-ecba4a29b01e",
      "number": 7,
      "content": "Người ấy sẽ không sợ các hung tin.",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.529Z",
      "updatedAt": "2024-09-07T14:18:16.529Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "f3ef3a24-e128-488f-9a55-64ab2c76a52e",
      "number": 7,
      "content": "Người ấy cứ vững lòng, tin cậy CHÚA.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.529Z",
      "updatedAt": "2024-09-07T14:18:16.529Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "30dbe375-a98b-4864-bd60-f3fbc03689e5",
      "number": 8,
      "content": "Người ấy luôn an tâm và chẳng sợ hãi gì,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.531Z",
      "updatedAt": "2024-09-07T14:18:16.531Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "bf26b1b9-9941-4d2e-b2ce-db95b630d8bd",
      "number": 8,
      "content": "Cho đến khi người ấy nhìn kẻ thù mình một cách đắc thắng.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.531Z",
      "updatedAt": "2024-09-07T14:18:16.531Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "1545df86-179c-47d8-83fb-727fe5349495",
      "number": 9,
      "content": "Người ấy rải của mình ra giúp người nghèo khó.",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.533Z",
      "updatedAt": "2024-09-07T14:18:16.533Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "820ff06d-6f89-4098-91bf-3bbf40948482",
      "number": 9,
      "content": "Đức công chính của người ấy trường tồn mãi mãi.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.533Z",
      "updatedAt": "2024-09-07T14:18:16.533Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "924773d6-95e0-4c75-af11-722f17cc25ed",
      "number": 9,
      "content": "Sừng người ấy sẽ ngước cao trong vinh hiển.",
      "order": 2,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.533Z",
      "updatedAt": "2024-09-07T14:18:16.533Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "45d3b971-49f9-4781-8b23-94f458c6e9d1",
      "number": 10,
      "content": "Kẻ ác thấy thế bèn nổi điên tức giận.",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.535Z",
      "updatedAt": "2024-09-07T14:18:16.535Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "6a8c7960-2519-474e-84a8-2f4a60393fa9",
      "number": 10,
      "content": "Nó nghiến răng tức tối rồi bị tiêu tan.",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.535Z",
      "updatedAt": "2024-09-07T14:18:16.535Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    },
    {
      "id": "a4da0432-5fcb-4097-b774-5aff9c81885d",
      "number": 10,
      "content": "Ước vọng của kẻ ác sẽ hoàn toàn mất tiệt.",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:18:16.535Z",
      "updatedAt": "2024-09-07T14:18:16.535Z",
      "chapterId": "79e69ad4-3b46-4d96-8e79-a9c03b2fc0f4"
    }
  ],
