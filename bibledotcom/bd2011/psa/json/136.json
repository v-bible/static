      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "47c304a1-1cb2-498c-99e4-f5ac362d17ea",
      "number": 15,
      "content": "Nhưng Ngài lật đổ Pha-ra-ôn và đạo quân vua ấy xuống Hồng Hải,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.370Z",
      "updatedAt": "2024-09-23T05:42:54.370Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "b38bed57-314e-499b-ba35-d6c7cbd0af77",
      "number": 16,
      "content": "Đấng dẫn dắt dân Ngài đi qua đồng hoang,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.380Z",
      "updatedAt": "2024-09-23T05:42:54.380Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "3226f782-f181-4fe7-bdad-6147896e32ea",
      "number": 17,
      "content": "Đấng đánh bại các vua lớn,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.390Z",
      "updatedAt": "2024-09-23T05:42:54.390Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "521f972d-1102-4328-9a7d-a6a396f9a88e",
      "number": 18,
      "content": "Và tiêu diệt những vua mạnh,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.400Z",
      "updatedAt": "2024-09-23T05:42:54.400Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "721102d8-80a0-4148-8395-0b0b1dbfa39b",
      "number": 19,
      "content": "Đó là Si-hôn vua dân A-mô-ri,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.409Z",
      "updatedAt": "2024-09-23T05:42:54.409Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "2a0c0883-d70c-4ef9-9752-9144662067df",
      "number": 20,
      "content": "Và Óc vua Ba-san,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.419Z",
      "updatedAt": "2024-09-23T05:42:54.419Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "664569d6-9bd9-43e5-a0cb-76bfaeda20bd",
      "number": 21,
      "content": "Rồi ban xứ của họ để làm sản nghiệp,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.429Z",
      "updatedAt": "2024-09-23T05:42:54.429Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "d2ac02b1-a75a-4480-8873-69f7c60f2e30",
      "number": 22,
      "content": "Tức làm sản nghiệp cho I-sơ-ra-ên tôi tớ Ngài,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.439Z",
      "updatedAt": "2024-09-23T05:42:54.439Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "a92d3dbe-b49b-4bc1-a36f-9231b24d1dee",
      "number": 23,
      "content": "Đấng nhớ đến thân phận thấp hèn của chúng ta,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.448Z",
      "updatedAt": "2024-09-23T05:42:54.448Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "3d4a376e-be12-414f-9902-e2f1b58527f0",
      "number": 24,
      "content": "Và giải cứu chúng ta khỏi những kẻ thù của chúng ta,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.457Z",
      "updatedAt": "2024-09-23T05:42:54.457Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "553cf8e9-a4b4-41ef-9a17-ba5eef674173",
      "number": 25,
      "content": "Đấng ban thực phẩm cho mọi loài xác thịt,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.467Z",
      "updatedAt": "2024-09-23T05:42:54.467Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    },
    {
      "id": "f0a173d6-c579-4eec-a1bf-48cf5ab7edae",
      "number": 26,
      "content": "Hãy cảm tạ Đức Chúa Trời của các tầng trời,",
      "order": 0,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:42:54.476Z",
      "updatedAt": "2024-09-23T05:42:54.476Z",
      "chapterId": "20c60beb-3755-4917-98cc-a27ff6a70081"
    }
  ],
