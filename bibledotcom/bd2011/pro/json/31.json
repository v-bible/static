    {
      "id": "1008b6c6-1ad3-4029-83e8-e675857e88ba",
      "content": "Những Lời của Mẹ Vua Lê-mu-ên Khuyên Con Bà",
      "order": 0,
      "createdAt": "2024-09-07T14:37:04.257Z",
      "updatedAt": "2024-09-07T14:37:04.257Z",
      "verseId": "e4745e50-f3c3-42a1-b4b4-dc226c4848fa",
