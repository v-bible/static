    },
    {
      "id": "754efe92-4b85-410d-8b83-998e5bac2cea",
      "content": "Khen Ngợi Người Vợ Tài Đức",
      "order": 0,
      "createdAt": "2024-09-07T14:37:04.257Z",
      "updatedAt": "2024-09-07T14:37:04.257Z",
