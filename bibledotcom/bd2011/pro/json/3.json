{
  "chapter": {
    "id": "5653303a-ffd8-4ad8-a904-98885e3e6d10",
    "number": 3,
    "ref": "https://www.bible.com/bible/19/PRO.3.BD2011",
    "createdAt": "2024-09-07T11:51:11.626Z",
    "updatedAt": "2024-09-07T11:51:11.626Z",
    "bookId": "71c3c74b-28d8-4c20-87d4-7d45fc5c93e2"
  },
  "verses": [
    {
      "id": "47a3ed6c-838f-49d1-9858-fb8b02850bea",
      "number": 1,
      "content": "Con ơi, chớ quên luật pháp ta,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.374Z",
      "updatedAt": "2024-09-07T14:32:15.374Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "ce8cbc04-faef-4c12-915f-173315c6c236",
      "number": 1,
      "content": "Nhưng hãy để lòng con gìn giữ mệnh lệnh ta;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.374Z",
      "updatedAt": "2024-09-07T14:32:15.374Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "b4a9474f-5060-416e-9a6e-57e17c28b5a4",
      "number": 2,
      "content": "Vì chúng sẽ làm gia tăng số ngày và số năm của đời con;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.384Z",
      "updatedAt": "2024-09-07T14:32:15.384Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "f43930b7-0549-407c-b54a-184aa22f0af7",
      "number": 2,
      "content": "Chúng sẽ đem lại cho con bình an thịnh vượng.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.384Z",
      "updatedAt": "2024-09-07T14:32:15.384Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "eccc388e-b5ae-4efc-81ed-95759de7d282",
      "number": 3,
      "content": "Đừng để lòng thương xót và tính chân thật lìa khỏi con;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.386Z",
      "updatedAt": "2024-09-07T14:32:15.386Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "7c631ba4-1d17-43c6-a41a-946b847342ff",
      "number": 3,
      "content": "Hãy buộc chúng quanh cổ con;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.386Z",
      "updatedAt": "2024-09-07T14:32:15.386Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "a87b43a0-0ea9-4f47-82f6-ea8e3472ca3a",
      "number": 3,
      "content": "Hãy ghi tạc chúng vào bia lòng con.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.386Z",
      "updatedAt": "2024-09-07T14:32:15.386Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "8496ce34-7d35-4985-acf7-35520aa11d96",
      "number": 4,
      "content": "Vì như thế con sẽ được ơn và được tiếng tốt,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.389Z",
      "updatedAt": "2024-09-07T14:32:15.389Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "06197d2b-ee91-4d03-b04f-cec1b2494489",
      "number": 4,
      "content": "Trước mặt Đức Chúa Trời và luôn cả người ta.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.389Z",
      "updatedAt": "2024-09-07T14:32:15.389Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "49295e23-1f46-41a1-905a-c00d365a2d7a",
      "number": 5,
      "content": "Hãy hết lòng tin cậy CHÚA;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.391Z",
      "updatedAt": "2024-09-07T14:32:15.391Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "3b0e4a7b-6a7b-4988-9f3a-745d56482d6a",
      "number": 5,
      "content": "Chớ cậy vào sự thông sáng của con;",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.391Z",
      "updatedAt": "2024-09-07T14:32:15.391Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "a4d82087-4053-48ea-85b2-5519d9b96da4",
      "number": 6,
      "content": "Phàm trong các việc làm của con, khá nhận biết Ngài,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.394Z",
      "updatedAt": "2024-09-07T14:32:15.394Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "298d541c-b549-4a94-9853-2556f4338b01",
      "number": 6,
      "content": "Thì Ngài sẽ chỉ dẫn các lối của con.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.394Z",
      "updatedAt": "2024-09-07T14:32:15.394Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "6eca3cec-f31d-4eb5-b724-bd1c25acf2b8",
      "number": 7,
      "content": "Đừng cho mình là khôn ngoan theo mắt mình;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.396Z",
      "updatedAt": "2024-09-07T14:32:15.396Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "c4f2c4ab-8666-47d7-b723-67ac14b3b1d9",
      "number": 7,
      "content": "Hãy kính sợ CHÚA và lánh xa điều ác.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.396Z",
      "updatedAt": "2024-09-07T14:32:15.396Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "8b10adba-26d8-4fc5-9092-0b35d61ae32a",
      "number": 8,
      "content": "Như vậy thân xác con sẽ được mạnh khỏe,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.398Z",
      "updatedAt": "2024-09-07T14:32:15.398Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "3cb6f000-b31d-4892-8130-b0890cc5766b",
      "number": 8,
      "content": "Và xương cốt con sẽ được cứng cáp vững vàng.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.398Z",
      "updatedAt": "2024-09-07T14:32:15.398Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "5e446330-b348-4287-a6bd-01714f49fa35",
      "number": 9,
      "content": "Hãy dùng của cải con để tôn vinh CHÚA,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.400Z",
      "updatedAt": "2024-09-07T14:32:15.400Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "f259cbcc-c8ac-4067-a8db-b1976c4b9e88",
      "number": 9,
      "content": "Hãy dâng các hoa lợi đầu mùa của con để cảm tạ Ngài.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.400Z",
      "updatedAt": "2024-09-07T14:32:15.400Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "ae161aac-aee1-4ee9-b0e1-60723d1741f7",
      "number": 10,
      "content": "Như vậy các vựa lẫm của con sẽ tràn đầy lúa thóc,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.402Z",
      "updatedAt": "2024-09-07T14:32:15.402Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "43a32150-92a5-49f8-9112-1f71d61be1a2",
      "number": 10,
      "content": "Các thùng rượu của con rượu mới sẽ tràn đầy.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.402Z",
      "updatedAt": "2024-09-07T14:32:15.402Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "23c9c8da-5ad0-4d36-a35c-8493afbdb655",
      "number": 11,
      "content": "Con ơi, chớ coi thường sự sửa phạt của CHÚA,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.405Z",
      "updatedAt": "2024-09-07T14:32:15.405Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "23a19941-6db0-4dc6-a101-b50f03bff438",
      "number": 11,
      "content": "Đừng bất mãn trước sự quở trách của Ngài;",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.405Z",
      "updatedAt": "2024-09-07T14:32:15.405Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "7e10795b-8971-4a5b-9dc9-453cdedc57bf",
      "number": 12,
      "content": "Vì ai được CHÚA thương thì Ngài quở trách,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.407Z",
      "updatedAt": "2024-09-07T14:32:15.407Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "49d35a2b-6ddb-4887-98f7-3cf0718cbaa2",
      "number": 12,
      "content": "Như cha thương con mình nên trách phạt khuyên răn.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.407Z",
      "updatedAt": "2024-09-07T14:32:15.407Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "2cda37c9-7723-45a2-98ed-a1a0cc8e3529",
      "number": 13,
      "content": "Phước thay cho người tìm được sự khôn ngoan,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.409Z",
      "updatedAt": "2024-09-07T14:32:15.409Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "bd6bd81f-b584-4d83-9a3c-dfadd2aeadc9",
      "number": 13,
      "content": "Và cho người có được sự thông sáng.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.409Z",
      "updatedAt": "2024-09-07T14:32:15.409Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "dbcd4b5a-f58b-4d6d-bfdf-c1c6a5ab2096",
      "number": 14,
      "content": "Vì lợi ích của khôn ngoan trỗi hơn tiền bạc;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.412Z",
      "updatedAt": "2024-09-07T14:32:15.412Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "4850f1a4-d902-4244-846a-bf4fadecd756",
      "number": 14,
      "content": "Ích lợi của nàng lấn át cả vàng ròng.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.412Z",
      "updatedAt": "2024-09-07T14:32:15.412Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "d1e2afe0-7fe1-4388-9b6b-4cc5eb2e03e2",
      "number": 15,
      "content": "Nàng quý báu hơn châu ngọc;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.414Z",
      "updatedAt": "2024-09-07T14:32:15.414Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "c5ea0f19-e535-45a3-884a-3d80d8686bab",
      "number": 15,
      "content": "Chẳng có vật gì con ưa thích có thể sánh được với nàng.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.414Z",
      "updatedAt": "2024-09-07T14:32:15.414Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "ce051baf-9774-472b-87f9-94c044bd79c6",
      "number": 16,
      "content": "Tay phải nàng phát ban trường thọ;",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.416Z",
      "updatedAt": "2024-09-07T14:32:15.416Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "ae096125-b19c-4c63-adcd-8ed0baca2f5b",
      "number": 16,
      "content": "Tay trái nàng cho giàu có lẫn hiển vinh.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.416Z",
      "updatedAt": "2024-09-07T14:32:15.416Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "b36ae27e-618d-4a95-bc59-0a9f193a210f",
      "number": 17,
      "content": "Con đường nàng là con đường vui thỏa;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.419Z",
      "updatedAt": "2024-09-07T14:32:15.419Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "2c89cc66-96f8-49b6-ab1b-4c87e86ce90b",
      "number": 17,
      "content": "Mọi nẻo nàng là mọi ngả bình an.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.419Z",
      "updatedAt": "2024-09-07T14:32:15.419Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "5335a97d-cdcd-42e3-b54c-3e8e9252b520",
      "number": 18,
      "content": "Khôn ngoan là cây sự sống cho người nào nắm giữ được;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.422Z",
      "updatedAt": "2024-09-07T14:32:15.422Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "21b2a3b1-c35a-4c4b-b5ec-edcf7c4ddef9",
      "number": 18,
      "content": "Ai giữ chặt nàng sẽ được phước lâu dài.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.422Z",
      "updatedAt": "2024-09-07T14:32:15.422Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "a964698e-3f21-43f0-b190-21ba2874a2ab",
      "number": 19,
      "content": "CHÚA dùng sự khôn ngoan để dựng nên trái đất;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.424Z",
      "updatedAt": "2024-09-07T14:32:15.424Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "6675f303-94b5-47e3-9088-492dcc85d0f7",
      "number": 19,
      "content": "Ngài dùng sự thông sáng để tạo lập các tầng trời;",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.424Z",
      "updatedAt": "2024-09-07T14:32:15.424Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "109bffc6-1076-493b-957b-0af5a3c6a6d6",
      "number": 20,
      "content": "Nhờ tri thức của Ngài các vực sâu mở rộng;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.426Z",
      "updatedAt": "2024-09-07T14:32:15.426Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "eb9beff0-d8b2-4e24-9118-932cfbf88e25",
      "number": 20,
      "content": "Những áng mây mù nhỏ những giọt sương đêm.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.426Z",
      "updatedAt": "2024-09-07T14:32:15.426Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "49fce635-37fa-4b20-ac3f-5eedeefd4497",
      "number": 21,
      "content": "Con ơi, đừng để những điều này vuột khỏi tầm mắt con:",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.428Z",
      "updatedAt": "2024-09-07T14:32:15.428Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "55dfd901-ce7e-4e13-8627-643dce896964",
      "number": 21,
      "content": "Hãy giữ lấy sự khôn ngoan và sự cẩn trọng.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.428Z",
      "updatedAt": "2024-09-07T14:32:15.428Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "11b612c4-4669-460e-9310-d9448cf8d8dd",
      "number": 22,
      "content": "Những điều ấy sẽ là sức sống của linh hồn con,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.430Z",
      "updatedAt": "2024-09-07T14:32:15.430Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "01ec9bab-34ca-40c4-b6c6-3572e89865b2",
      "number": 22,
      "content": "Và là những đồ trang sức nơi cổ con.",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.430Z",
      "updatedAt": "2024-09-07T14:32:15.430Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "2f235b1a-2656-4133-88d5-ade3ea169624",
      "number": 23,
      "content": "Nhờ những điều ấy đường con đi sẽ an toàn bảo đảm,",
      "order": 0,
      "parNumber": 49,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.432Z",
      "updatedAt": "2024-09-07T14:32:15.432Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "a6b339c9-2bbb-4563-b7f3-66e717ff9f69",
      "number": 23,
      "content": "Bước chân con sẽ chẳng vấp phạm bao giờ.",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.432Z",
      "updatedAt": "2024-09-07T14:32:15.432Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "7afb34d2-8eac-403f-a491-a706d8f750c9",
      "number": 24,
      "content": "Lúc nằm nghỉ con sẽ không lo sợ;",
      "order": 0,
      "parNumber": 51,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.434Z",
      "updatedAt": "2024-09-07T14:32:15.434Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "7c6fc805-f43e-49a8-9ba3-9444708d7806",
      "number": 24,
      "content": "Khi đặt lưng con sẽ an giấc ngủ ngon.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.434Z",
      "updatedAt": "2024-09-07T14:32:15.434Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "48aeb83a-a60b-450f-9390-1fd95937b29e",
      "number": 25,
      "content": "Đừng sợ hãi nỗi kinh hoàng xảy ra bất chợt;",
      "order": 0,
      "parNumber": 53,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.436Z",
      "updatedAt": "2024-09-07T14:32:15.436Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "a87084c3-947e-476f-a130-b65a01fb4425",
      "number": 25,
      "content": "Đừng âu lo khi đợt phá hoại của kẻ ác đến gần;",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.436Z",
      "updatedAt": "2024-09-07T14:32:15.436Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "cda4c5e1-d63d-4495-9305-03e77acc5427",
      "number": 26,
      "content": "Vì CHÚA sẽ là nguồn trông cậy của con;",
      "order": 0,
      "parNumber": 55,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.439Z",
      "updatedAt": "2024-09-07T14:32:15.439Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "720d014e-dfe9-42c2-b6d9-36dac4596ee5",
      "number": 26,
      "content": "Ngài sẽ giữ chân con khỏi sa vào bẫy.",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.439Z",
      "updatedAt": "2024-09-07T14:32:15.439Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "d87874a5-f86e-4aed-8667-9c19896f38b3",
      "number": 27,
      "content": "Đừng từ chối làm việc thiện cho những người đáng giúp,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.441Z",
      "updatedAt": "2024-09-07T14:32:15.441Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "1dbd2432-aa3e-4a17-b933-5ed21ab3ed18",
      "number": 27,
      "content": "Nếu con có khả năng, hãy giúp đỡ người ta.",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.441Z",
      "updatedAt": "2024-09-07T14:32:15.441Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "5ba2633a-1143-41d4-a411-8d3b5fd5a105",
      "number": 28,
      "content": "Con đừng nói với người lân cận, “Về đi, mai trở lại, tôi sẽ cho,”",
      "order": 0,
      "parNumber": 59,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.443Z",
      "updatedAt": "2024-09-07T14:32:15.443Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "d2133abd-d999-4a12-a651-0d9f1751c05d",
      "number": 28,
      "content": "Trong khi con có sẵn điều người ấy đang cần.",
      "order": 1,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.443Z",
      "updatedAt": "2024-09-07T14:32:15.443Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "f57c54a9-ac90-4dac-9a43-de73fa817a40",
      "number": 29,
      "content": "Con chớ lập mưu hại người lân cận,",
      "order": 0,
      "parNumber": 61,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.446Z",
      "updatedAt": "2024-09-07T14:32:15.446Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "0228ecd3-d433-4fe6-bdd7-87ae1da2e119",
      "number": 29,
      "content": "Người ở gần và tin tưởng nơi con.",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.446Z",
      "updatedAt": "2024-09-07T14:32:15.446Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "69dc86d6-cdab-4505-8048-792110cea22b",
      "number": 30,
      "content": "Con đừng gây sự với ai vô cớ,",
      "order": 0,
      "parNumber": 63,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.448Z",
      "updatedAt": "2024-09-07T14:32:15.448Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "ecdde3b9-ca96-4006-b7e9-b5737c0000ac",
      "number": 30,
      "content": "Khi không ai gây cho con thiệt hại điều gì.",
      "order": 1,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.448Z",
      "updatedAt": "2024-09-07T14:32:15.448Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "c3f0afd4-9005-48ff-8bbd-2eb56dbd71b5",
      "number": 31,
      "content": "Đừng ganh tị với quân tàn bạo;",
      "order": 0,
      "parNumber": 65,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.450Z",
      "updatedAt": "2024-09-07T14:32:15.450Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "2b60042c-0ee4-4f6d-9bc9-e4175cfebf35",
      "number": 31,
      "content": "Chớ chọn đường nào chúng chọn lấy để đi;",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.450Z",
      "updatedAt": "2024-09-07T14:32:15.450Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "03471432-a28b-4ebd-8807-79334c9ed2fb",
      "number": 32,
      "content": "Vì CHÚA ghê tởm kẻ gian tà tội lỗi,",
      "order": 0,
      "parNumber": 67,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.453Z",
      "updatedAt": "2024-09-07T14:32:15.453Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "cea7b941-99cf-4e38-9797-2ebbd3d005da",
      "number": 32,
      "content": "Nhưng Ngài kết bạn tâm giao với người chính trực ngay lành.",
      "order": 1,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.453Z",
      "updatedAt": "2024-09-07T14:32:15.453Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "ae5310cf-cc9d-49f3-8656-d49dc8ac90e2",
      "number": 33,
      "content": "Sự nguyền rủa của CHÚA giáng trên nhà kẻ gian ác,",
      "order": 0,
      "parNumber": 69,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.459Z",
      "updatedAt": "2024-09-07T14:32:15.459Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "06e3d5c5-2bfc-44cc-8f33-28b84f6568b4",
      "number": 33,
      "content": "Nhưng phước hạnh Ngài ban cho chỗ ở người ngay lành.",
      "order": 1,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.459Z",
      "updatedAt": "2024-09-07T14:32:15.459Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "44699e2e-35af-4586-ac6a-7676a75ccc1c",
      "number": 34,
      "content": "Ngài chế giễu phường chê bai nhạo báng,",
      "order": 0,
      "parNumber": 71,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.465Z",
      "updatedAt": "2024-09-07T14:32:15.465Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "4f02a07f-c0d9-47d8-b559-f89fc5e419f3",
      "number": 34,
      "content": "Nhưng Ngài ban ơn cho người thật sự khiêm nhường.",
      "order": 1,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.465Z",
      "updatedAt": "2024-09-07T14:32:15.465Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "2ba22133-1919-457b-9dff-d97d104f7532",
      "number": 35,
      "content": "Người khôn ngoan sẽ thừa hưởng những hiển vinh tôn trọng,",
      "order": 0,
      "parNumber": 73,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.474Z",
      "updatedAt": "2024-09-07T14:32:15.474Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    },
    {
      "id": "8544171f-2d37-4d2f-b80c-efec3896e6fa",
      "number": 35,
      "content": "Còn kẻ điên rồ bị tống cho nhục nhã ê chề.",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:15.474Z",
      "updatedAt": "2024-09-07T14:32:15.474Z",
      "chapterId": "5653303a-ffd8-4ad8-a904-98885e3e6d10"
    }
  ],
