{
  "chapter": {
    "id": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f",
    "number": 14,
    "ref": "https://www.bible.com/bible/19/PRO.14.BD2011",
    "createdAt": "2024-09-07T11:51:11.713Z",
    "updatedAt": "2024-09-07T11:51:11.713Z",
    "bookId": "71c3c74b-28d8-4c20-87d4-7d45fc5c93e2"
  },
  "verses": [
    {
      "id": "7f51e9f6-a292-4165-b977-08780993286b",
      "number": 1,
      "content": "Người nữ khôn ngoan xây dựng gia đình mình hạnh phúc,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.749Z",
      "updatedAt": "2024-09-07T14:34:09.749Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "d2658dda-900a-4816-a27d-c5f22039b02b",
      "number": 1,
      "content": "Còn kẻ dại dột dùng chính tay mình phá hủy nó đi.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.749Z",
      "updatedAt": "2024-09-07T14:34:09.749Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "1172f68e-f5fe-4c85-80b6-cc639be7b9aa",
      "number": 2,
      "content": "Người sống ngay thẳng là người kính sợ CHÚA,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.759Z",
      "updatedAt": "2024-09-07T14:34:09.759Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "8734295e-c86c-4554-948c-5698aa414ac7",
      "number": 2,
      "content": "Kẻ ăn ở gian tà chính là kẻ khinh thường Ngài.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.759Z",
      "updatedAt": "2024-09-07T14:34:09.759Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "33463550-c162-4d66-b7bb-55e30f4fcb63",
      "number": 3,
      "content": "Lời ngạo mạn ra từ miệng kẻ dại trở thành roi đánh nó,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.762Z",
      "updatedAt": "2024-09-07T14:34:09.762Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "14b77296-cb13-4438-834e-8a72be3d3b6c",
      "number": 3,
      "content": "Môi người khôn bảo vệ mạng sống mình.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.762Z",
      "updatedAt": "2024-09-07T14:34:09.762Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "5bdc277e-0649-41b8-94d3-9fd3b28725d0",
      "number": 4,
      "content": "Nơi nào bò không cày, nơi đó không lúa gạo,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.764Z",
      "updatedAt": "2024-09-07T14:34:09.764Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "a739ced9-1f47-4262-a70e-2b19e8cf0a73",
      "number": 4,
      "content": "Hoa lợi nhiều nhờ sức lực của bò đực mà ra.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.764Z",
      "updatedAt": "2024-09-07T14:34:09.764Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "d51a8e04-c940-4bae-9727-9ca84c512f5e",
      "number": 5,
      "content": "Nhân chứng trung tín không bao giờ nói dối,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.766Z",
      "updatedAt": "2024-09-07T14:34:09.766Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "32beceaf-86db-48a2-9390-9d5ab71209e4",
      "number": 5,
      "content": "Kẻ chứng gian vừa thở ra là có hơi hám dối trá rồi.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.766Z",
      "updatedAt": "2024-09-07T14:34:09.766Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "a7f1df9a-3ade-4f9a-9a7b-3988807f3a7f",
      "number": 6,
      "content": "Kẻ nhạo báng tìm khôn ngoan hoài không được,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.769Z",
      "updatedAt": "2024-09-07T14:34:09.769Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "0118e7ed-4f12-4571-86af-a54ad86e4278",
      "number": 6,
      "content": "Còn người thông minh tri thức đến dễ dàng.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.769Z",
      "updatedAt": "2024-09-07T14:34:09.769Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "a5b4ce86-ada1-4ee0-8dc6-7eec09ce7c4f",
      "number": 7,
      "content": "Hãy dang xa con người điên dại,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.770Z",
      "updatedAt": "2024-09-07T14:34:09.770Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "aa20c958-b4aa-4dec-a24a-b9820a2671d4",
      "number": 7,
      "content": "Vì bạn sẽ chẳng thấy tri thức ra từ miệng nó bao giờ.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.770Z",
      "updatedAt": "2024-09-07T14:34:09.770Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "9d1fe6c0-ea78-4058-9805-921d26cef79b",
      "number": 8,
      "content": "Cái khôn của người thận trọng là biết toan tính đường lối mình;",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.772Z",
      "updatedAt": "2024-09-07T14:34:09.772Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "477f4029-a029-49eb-ac9d-d508232898a5",
      "number": 8,
      "content": "Cái dại của kẻ điên dại là mánh khóe dối gian.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.772Z",
      "updatedAt": "2024-09-07T14:34:09.772Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "af5bc509-5a34-4e5e-8d66-5e1510267571",
      "number": 9,
      "content": "Những kẻ điên dại chê cười của lễ chuộc tội;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.774Z",
      "updatedAt": "2024-09-07T14:34:09.774Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "4500de88-b697-4fc6-ba78-ef701f0c3950",
      "number": 9,
      "content": "Nhưng người ngay lành tin rằng nhờ nó mà được đoái thương.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.774Z",
      "updatedAt": "2024-09-07T14:34:09.774Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "a343f40d-46bd-4f13-935c-1b6e34912411",
      "number": 10,
      "content": "Nỗi đắng cay chỉ có lòng mình biết;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.777Z",
      "updatedAt": "2024-09-07T14:34:09.777Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "c1d63b6c-38a0-4e0e-9aea-d2f67e64f975",
      "number": 10,
      "content": "Và niềm vui nào chia sẻ hết được với người dưng.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.777Z",
      "updatedAt": "2024-09-07T14:34:09.777Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "6aa57a26-52f5-4913-b5f7-e243f5290c99",
      "number": 11,
      "content": "Nhà kẻ gian ác trước sau gì cũng bị hủy hoại,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.781Z",
      "updatedAt": "2024-09-07T14:34:09.781Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "c15fec0c-c24f-4e4d-a23f-27b3d9d12018",
      "number": 11,
      "content": "Trại người ngay lành cứ vững chãi lâu dài.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.781Z",
      "updatedAt": "2024-09-07T14:34:09.781Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "3092e845-a5aa-422d-86c0-8b4014aaaf7f",
      "number": 12,
      "content": "Có một con đường dường như chính đáng cho người ta,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.784Z",
      "updatedAt": "2024-09-07T14:34:09.784Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "06d1aebe-ef10-4c6f-8333-5cce41f0147e",
      "number": 12,
      "content": "Nhưng đến cuối đường hóa ra là tử lộ.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.784Z",
      "updatedAt": "2024-09-07T14:34:09.784Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "9211c11a-1c2e-4b8a-ab4f-a9f488f27cc2",
      "number": 13,
      "content": "Dù ngoài mặt cười vang, nhưng trong lòng có thể đang sầu héo;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.788Z",
      "updatedAt": "2024-09-07T14:34:09.788Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "4662376f-0a41-488d-aa87-569829657415",
      "number": 13,
      "content": "Cuối cuộc vui nỗi sầu khổ vẫn kéo lại như thường.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.788Z",
      "updatedAt": "2024-09-07T14:34:09.788Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "46ace125-5dd8-4219-90e5-4ead88cfd7da",
      "number": 14,
      "content": "Kẻ tà tâm sẽ lãnh đủ hậu quả xấu mình gây ra;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.792Z",
      "updatedAt": "2024-09-07T14:34:09.792Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "9d458033-1c1c-4e63-9f07-ac892e608199",
      "number": 14,
      "content": "Người thiện tâm sẽ được thưởng xứng đáng với việc mình làm.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.792Z",
      "updatedAt": "2024-09-07T14:34:09.792Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "2b276aca-3c60-464b-b82d-84ec72aa9a93",
      "number": 15,
      "content": "Kẻ dại dột tin mọi lời người ta nói;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.796Z",
      "updatedAt": "2024-09-07T14:34:09.796Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "c4d5b2fa-7253-49de-911f-6f5a4f48901a",
      "number": 15,
      "content": "Nhưng người khôn ngoan thận trọng từng bước một của mình.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.796Z",
      "updatedAt": "2024-09-07T14:34:09.796Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "33e8ccd1-6a46-4db9-a2eb-23a13b0ed96c",
      "number": 16,
      "content": "Người khôn ngoan ghê sợ và lánh xa điều ác;",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.799Z",
      "updatedAt": "2024-09-07T14:34:09.799Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "7bda7471-e761-491c-a3d8-54b863b1ac9e",
      "number": 16,
      "content": "Còn kẻ dại dột thì hành động khinh suất và bất cẩn.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.799Z",
      "updatedAt": "2024-09-07T14:34:09.799Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "4481cd20-9b19-49e7-975b-ac47537a67d4",
      "number": 17,
      "content": "Kẻ nhạy giận dễ hành động điên rồ;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.803Z",
      "updatedAt": "2024-09-07T14:34:09.803Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "23485405-9785-4199-a952-3c9b06b93845",
      "number": 17,
      "content": "Kẻ toan tính chuyện ác ắt bị người ta ghét.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.803Z",
      "updatedAt": "2024-09-07T14:34:09.803Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "67f59e24-7ba7-4b54-a8e3-e3a647bb734d",
      "number": 18,
      "content": "Kẻ khờ dại thừa hưởng sự điên dại;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.807Z",
      "updatedAt": "2024-09-07T14:34:09.807Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "72f07eaf-1a34-4365-8853-c3e5524dc7d0",
      "number": 18,
      "content": "Người khôn ngoan lấy tri thức làm mão miện của mình.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.807Z",
      "updatedAt": "2024-09-07T14:34:09.807Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "8b055d60-0483-48a8-927e-a8d0f50dcbcf",
      "number": 19,
      "content": "Kẻ xấu phải sấp mình trước mặt người tốt,",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.812Z",
      "updatedAt": "2024-09-07T14:34:09.812Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "465cb2c9-2409-4c16-8474-77cd4bff68d1",
      "number": 19,
      "content": "Kẻ gian ác phải chầu chực trước cổng người ngay lành.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.812Z",
      "updatedAt": "2024-09-07T14:34:09.812Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "6bf09e65-a51b-4d93-bc33-b033dd0cf49d",
      "number": 20,
      "content": "Người nghèo khó ngay cả láng giềng cũng không thích,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.817Z",
      "updatedAt": "2024-09-07T14:34:09.817Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "c0614f77-1fe2-4e01-8871-30c97180cc9d",
      "number": 20,
      "content": "Kẻ giàu sang tính không hết bạn bè.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.817Z",
      "updatedAt": "2024-09-07T14:34:09.817Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "6f7e65d6-ee58-4acd-ae1f-a901fc5b519b",
      "number": 21,
      "content": "Ai khi dể người lân cận là mắc tội,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.821Z",
      "updatedAt": "2024-09-07T14:34:09.821Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "fc4a5aff-face-46f6-9776-554354fa812c",
      "number": 21,
      "content": "Phước cho ai thương xót người nghèo.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.821Z",
      "updatedAt": "2024-09-07T14:34:09.821Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "5494e9de-52fc-4a67-aa1a-d0ce09cd8c69",
      "number": 22,
      "content": "Há chẳng phải kẻ toan mưu ác đang sai lạc sao?",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.826Z",
      "updatedAt": "2024-09-07T14:34:09.826Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "4ace1401-1fc0-4688-bf0f-9f6b698655db",
      "number": 22,
      "content": "Người nỗ lực làm điều lành được xót thương và tin cậy.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.826Z",
      "updatedAt": "2024-09-07T14:34:09.826Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "119edc73-8d08-4b4d-b8a0-bcc8523b8c7c",
      "number": 23,
      "content": "Tất cả các hình thức lao động đều đem lại ích lợi,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.831Z",
      "updatedAt": "2024-09-07T14:34:09.831Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "d6084a57-e6c7-42cc-9642-70ee0b8d9f2e",
      "number": 23,
      "content": "Còn chỉ nói và lười ắt dẫn đến đói nghèo.",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.831Z",
      "updatedAt": "2024-09-07T14:34:09.831Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "074072ee-62ae-4b6b-8069-021a192e875a",
      "number": 24,
      "content": "Giàu có là mão miện của người khôn ngoan,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.836Z",
      "updatedAt": "2024-09-07T14:34:09.836Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "059f991d-3ad6-448b-b4f7-b6db8f57d09c",
      "number": 24,
      "content": "Điên cuồng là vòng hoa của kẻ điên dại.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.836Z",
      "updatedAt": "2024-09-07T14:34:09.836Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "3fdfe518-31b9-44e0-a015-67d38047cb0d",
      "number": 25,
      "content": "Người chứng chân thật cứu biết bao mạng sống,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.841Z",
      "updatedAt": "2024-09-07T14:34:09.841Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "eae65a27-5222-4479-8391-8124644dfbd3",
      "number": 25,
      "content": "Còn kẻ nói dối thở ra toàn hơi hám dối gian.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.841Z",
      "updatedAt": "2024-09-07T14:34:09.841Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "bf723ffe-254c-487e-84e9-1aa9cd3d342a",
      "number": 26,
      "content": "Người kính sợ CHÚA được thành lũy an toàn bảo vệ,",
      "order": 0,
      "parNumber": 50,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.846Z",
      "updatedAt": "2024-09-07T14:34:09.846Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "c85baaa8-0521-4a02-a131-f0caf238e6a3",
      "number": 26,
      "content": "Con cái người ấy có một chỗ để an thân.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.846Z",
      "updatedAt": "2024-09-07T14:34:09.846Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "de3d9267-0a6b-4af7-b593-49890e24b4f3",
      "number": 27,
      "content": "Kính sợ CHÚA là nguồn sự sống,",
      "order": 0,
      "parNumber": 52,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.851Z",
      "updatedAt": "2024-09-07T14:34:09.851Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "91948d18-bd28-4c31-912b-b469de6a1e81",
      "number": 27,
      "content": "Giúp người ta tránh được bẫy tử vong.",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.851Z",
      "updatedAt": "2024-09-07T14:34:09.851Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "07c07961-48d8-4ba3-94a1-3b8cbaf400d6",
      "number": 28,
      "content": "Vua được vinh hiển nhờ có đông dân chúng,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.858Z",
      "updatedAt": "2024-09-07T14:34:09.858Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "ffb17c0d-b902-4779-aeeb-70eeaef61165",
      "number": 28,
      "content": "Không có dân vua lúng túng chịu thua.",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.858Z",
      "updatedAt": "2024-09-07T14:34:09.858Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "3c205dc8-5ac5-4216-beb4-f3f0c8ee2e6c",
      "number": 29,
      "content": "Người chậm nóng giận là người đại trí,",
      "order": 0,
      "parNumber": 56,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.863Z",
      "updatedAt": "2024-09-07T14:34:09.863Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "13661f62-316b-4ea0-b048-433f4788f6f0",
      "number": 29,
      "content": "Kẻ nóng tính tỏ thái độ liền là tự ý phô sự dại dột mình ra.",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.863Z",
      "updatedAt": "2024-09-07T14:34:09.863Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "a99eaeda-cc53-4e75-a732-57ae006e9cdd",
      "number": 30,
      "content": "Lòng bình an ban sinh lực cho thân xác;",
      "order": 0,
      "parNumber": 58,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.868Z",
      "updatedAt": "2024-09-07T14:34:09.868Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "020efdec-d47b-486e-80a6-4090704640c1",
      "number": 30,
      "content": "Tính ganh tị làm mục nát trong xương.",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.868Z",
      "updatedAt": "2024-09-07T14:34:09.868Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "0213e28f-d116-477b-a6e1-7e17fd92fdba",
      "number": 31,
      "content": "Ai áp bức người nghèo là làm nhục Đấng Tạo Hóa mình;",
      "order": 0,
      "parNumber": 60,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.873Z",
      "updatedAt": "2024-09-07T14:34:09.873Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "27d0a959-77dc-45b6-b4bc-b12969bbc1fc",
      "number": 31,
      "content": "Ai thương xót người khốn khổ là tôn kính Ngài.",
      "order": 1,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.873Z",
      "updatedAt": "2024-09-07T14:34:09.873Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "28948786-33c1-4982-b25c-72a4ccaea4ca",
      "number": 32,
      "content": "Kẻ ác bị việc ác của nó đánh đổ;",
      "order": 0,
      "parNumber": 62,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.878Z",
      "updatedAt": "2024-09-07T14:34:09.878Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "7cb775ea-de70-497f-9bf9-7c744466e02a",
      "number": 32,
      "content": "Nhưng người ngay lành khi qua đời vẫn có chỗ nương thân.",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.878Z",
      "updatedAt": "2024-09-07T14:34:09.878Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "d4915dee-e57f-42dc-837a-ecbd006cd313",
      "number": 33,
      "content": "Sự khôn ngoan nghỉ ngơi trong lòng người thông sáng;",
      "order": 0,
      "parNumber": 64,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.883Z",
      "updatedAt": "2024-09-07T14:34:09.883Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "9d52ab0d-a85c-48c3-8806-37e825aa4b2f",
      "number": 33,
      "content": "Còn kẻ dại dột bộc lộ hết lòng dạ nó ra ngoài.",
      "order": 1,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.883Z",
      "updatedAt": "2024-09-07T14:34:09.883Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "f6518818-d605-44c5-8181-a00232fda57b",
      "number": 34,
      "content": "Lẽ công chính làm quốc gia được kính trọng;",
      "order": 0,
      "parNumber": 66,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.888Z",
      "updatedAt": "2024-09-07T14:34:09.888Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "ede2f4ae-3ec3-4fae-a155-1d35eeb8b372",
      "number": 34,
      "content": "Còn tội lỗi làm xấu hổ toàn dân.",
      "order": 1,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.888Z",
      "updatedAt": "2024-09-07T14:34:09.888Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "f286f15b-4ed3-4d6d-8e9b-c3e465498ed0",
      "number": 35,
      "content": "Ân điển vua dành cho bầy tôi tài giỏi,",
      "order": 0,
      "parNumber": 68,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.893Z",
      "updatedAt": "2024-09-07T14:34:09.893Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    },
    {
      "id": "1064c0b8-cf6d-4aee-80fd-ba253baba32c",
      "number": 35,
      "content": "Thịnh nộ vua dọi xuống đầu kẻ gây nhục nhã cho người.",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:09.893Z",
      "updatedAt": "2024-09-07T14:34:09.893Z",
      "chapterId": "dfd5d486-2c1b-4d6c-97a2-ec44c67b5a2f"
    }
  ],
