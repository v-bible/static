{
  "chapter": {
    "id": "3a5879bc-de1b-4901-97ed-acdcc8232fc5",
    "number": 15,
    "ref": "https://www.bible.com/bible/19/PRO.15.BD2011",
    "createdAt": "2024-09-07T11:51:11.722Z",
    "updatedAt": "2024-09-07T11:51:11.722Z",
    "bookId": "71c3c74b-28d8-4c20-87d4-7d45fc5c93e2"
  },
  "verses": [
    {
      "id": "92bb30ea-8781-42e1-9362-f450771b51fe",
      "number": 1,
      "content": "Lời đáp êm dịu làm nguôi cơn giận;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.906Z",
      "updatedAt": "2024-09-07T14:34:20.906Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "5e3833ac-50c1-4f95-9876-45a526c1ad06",
      "number": 1,
      "content": "Còn lời xẳng xớm gây phẫn nộ thêm.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.906Z",
      "updatedAt": "2024-09-07T14:34:20.906Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "c98e5863-750c-4a98-8f48-4609fd322804",
      "number": 2,
      "content": "Lưỡi người khôn ngoan phát ban tri thức;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.909Z",
      "updatedAt": "2024-09-07T14:34:20.909Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "4c1c9d68-d877-4ddb-8314-36b2e6566e21",
      "number": 2,
      "content": "Miệng kẻ dại dột thốt chuyện điên rồ.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.909Z",
      "updatedAt": "2024-09-07T14:34:20.909Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "acb9d92c-2507-41ea-b337-a44acd4c9dd2",
      "number": 3,
      "content": "Mắt CHÚA thấy rõ khắp nơi,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.913Z",
      "updatedAt": "2024-09-07T14:34:20.913Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "64011472-7c9f-44b3-b35f-2aa99acdad45",
      "number": 3,
      "content": "Thấy tất cả kẻ xấu và người tốt.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.913Z",
      "updatedAt": "2024-09-07T14:34:20.913Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "d92dc2f3-071f-47de-ac0e-ed810998f5f6",
      "number": 4,
      "content": "Lưỡi hiền lành là cây sự sống,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.915Z",
      "updatedAt": "2024-09-07T14:34:20.915Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "7d7abc81-66c7-462d-9ceb-dc060cd16e5b",
      "number": 4,
      "content": "Còn lưỡi độc ác làm tan nát tinh thần.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.915Z",
      "updatedAt": "2024-09-07T14:34:20.915Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "86622576-88f4-4e87-a100-610dd8486194",
      "number": 5,
      "content": "Kẻ dại dột khinh lời cha khuyên dạy;",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.918Z",
      "updatedAt": "2024-09-07T14:34:20.918Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "586025a5-2845-4f27-be09-4a051c8052e6",
      "number": 5,
      "content": "Còn người khôn nghe lời quở trách khuyên răn.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.918Z",
      "updatedAt": "2024-09-07T14:34:20.918Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "bba4e6dd-84bf-444a-8941-12ffcf647914",
      "number": 6,
      "content": "Trong nhà người ngay lành có nhiều của cải;",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.920Z",
      "updatedAt": "2024-09-07T14:34:20.920Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "6da1d722-cc71-466d-aed2-f5896d4d68a0",
      "number": 6,
      "content": "Còn trong lợi tức kẻ ác có rắc rối chờ cơ hội xảy ra.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.920Z",
      "updatedAt": "2024-09-07T14:34:20.920Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "e30a0d86-7a56-4db7-a892-61ce83bfea89",
      "number": 7,
      "content": "Môi người khôn ngoan loan truyền tri thức;",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.922Z",
      "updatedAt": "2024-09-07T14:34:20.922Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "2e9a8681-0838-4ad2-946f-76b7caab76b8",
      "number": 7,
      "content": "Còn lòng kẻ dại dột ấm ức nhưng chẳng có kiến thức gì.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.922Z",
      "updatedAt": "2024-09-07T14:34:20.922Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "41d67eb3-d2af-4c3b-899e-aaaa13496de4",
      "number": 8,
      "content": "Lễ vật của kẻ gian ác là gớm ghiếc trước mặt CHÚA,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.924Z",
      "updatedAt": "2024-09-07T14:34:20.924Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "f8f415ce-9e42-44ab-9482-6e605475ebaf",
      "number": 8,
      "content": "Nhưng lời cầu nguyện của người ngay lành thì Ngài lại vui lòng.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.924Z",
      "updatedAt": "2024-09-07T14:34:20.924Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "8672540f-048f-4c3c-a51c-2ca35cae8376",
      "number": 9,
      "content": "Đường lối kẻ gian ác là gớm ghiếc trước mặt CHÚA,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.930Z",
      "updatedAt": "2024-09-07T14:34:20.930Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "e4ee4ba3-d169-433a-886c-77461cdbbf2f",
      "number": 9,
      "content": "Nhưng Ngài yêu thương người đeo đuổi sự ngay lành.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.930Z",
      "updatedAt": "2024-09-07T14:34:20.930Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "7ea897b6-b4f4-4be9-bbe8-b1f6815e2b6d",
      "number": 10,
      "content": "Sự nghiêm phạt dành cho kẻ bỏ đường ngay nẻo chánh,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.936Z",
      "updatedAt": "2024-09-07T14:34:20.936Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "542e10aa-c904-4412-ac5d-3c07937960b3",
      "number": 10,
      "content": "Ai ghét sự khuyên răn chẳng tránh khỏi chết đâu.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.936Z",
      "updatedAt": "2024-09-07T14:34:20.936Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "7887687a-ce7a-465f-8b20-0c403163b082",
      "number": 11,
      "content": "Ngay cả âm phủ và hỏa ngục mà CHÚA còn biết rõ,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.938Z",
      "updatedAt": "2024-09-07T14:34:20.938Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "57fd9379-6d65-4f4c-addf-7dddd5afa583",
      "number": 11,
      "content": "Thì huống chi là lòng dạ loài người.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.938Z",
      "updatedAt": "2024-09-07T14:34:20.938Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "8beb0376-3ac9-45f7-b87d-1e6c734c8498",
      "number": 12,
      "content": "Kẻ nhạo báng không thích bị ai quở trách;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.940Z",
      "updatedAt": "2024-09-07T14:34:20.940Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "4df0dd26-1c2d-49b1-9d9a-c2983da3bc78",
      "number": 12,
      "content": "Nó tránh đến với người khôn để học sự khôn ngoan.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.940Z",
      "updatedAt": "2024-09-07T14:34:20.940Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "765f15e1-a093-4cf2-996d-fa4a6392d012",
      "number": 13,
      "content": "Lòng vui vẻ khiến mặt mày rạng rỡ;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.942Z",
      "updatedAt": "2024-09-07T14:34:20.942Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "aff3a2bc-30e1-416f-bd49-24ea4b6c296b",
      "number": 13,
      "content": "Dạ âu sầu làm tan vỡ tinh thần.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.942Z",
      "updatedAt": "2024-09-07T14:34:20.942Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "fbd96f71-f6d6-4178-8335-af90c981910a",
      "number": 14,
      "content": "Lòng người thông sáng thích kiếm tìm tri thức;",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.944Z",
      "updatedAt": "2024-09-07T14:34:20.944Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "dd41bf38-0e2a-482f-936e-bfdf085cfe93",
      "number": 14,
      "content": "Miệng kẻ dại khờ ưa ẩm thực điên rồ.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.944Z",
      "updatedAt": "2024-09-07T14:34:20.944Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "0066caf9-872b-4b94-abcb-d03c960169b9",
      "number": 15,
      "content": "Kẻ nghèo khó suốt đời chịu khánh kiệt;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.946Z",
      "updatedAt": "2024-09-07T14:34:20.946Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "260f0ece-776c-4947-9169-3409093075af",
      "number": 15,
      "content": "Người vui vẻ trong lòng dự yến tiệc luôn luôn.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.946Z",
      "updatedAt": "2024-09-07T14:34:20.946Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "3181c7ee-06a1-4607-bc39-b94cc8d44174",
      "number": 16,
      "content": "Thà ít của mà kính sợ CHÚA,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.949Z",
      "updatedAt": "2024-09-07T14:34:20.949Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "80be77c3-4460-48de-8fc1-c4749e75e40d",
      "number": 16,
      "content": "Hơn của cải nhiều mà rắc rối ùa theo.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.949Z",
      "updatedAt": "2024-09-07T14:34:20.949Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "a4b5a5d0-c7b9-462d-b5a1-568a4d41bb5d",
      "number": 17,
      "content": "Thà ăn cơm với rau mà thương yêu nhau,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.951Z",
      "updatedAt": "2024-09-07T14:34:20.951Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "0da72411-3482-4791-8a1c-7de02b97d042",
      "number": 17,
      "content": "Hơn là ăn thịt bò mập béo mà ghen ghét cặp theo.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.951Z",
      "updatedAt": "2024-09-07T14:34:20.951Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "5a0ac246-da46-40d7-80d9-6b4055cd39fd",
      "number": 18,
      "content": "Người nóng tính dễ gây xung đột;",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.953Z",
      "updatedAt": "2024-09-07T14:34:20.953Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "d9d9e2d5-48d2-44c3-8eaa-bd86451a994c",
      "number": 18,
      "content": "Còn người chậm nóng giận làm cuộc cãi lộn nguôi đi.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.953Z",
      "updatedAt": "2024-09-07T14:34:20.953Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "1db05823-b9ec-4f97-aa87-989f6f7d8adc",
      "number": 19,
      "content": "Đường kẻ biếng nhác gai góc mọc đầy cản trở;",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.955Z",
      "updatedAt": "2024-09-07T14:34:20.955Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "6457c594-910a-49ab-a967-b118c80f0524",
      "number": 19,
      "content": "Còn lối người ngay lành lại mở rộng thênh thang.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.955Z",
      "updatedAt": "2024-09-07T14:34:20.955Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "8f44d0a7-ad53-4d2f-aeff-84e67d819e92",
      "number": 20,
      "content": "Đứa con khôn ngoan làm cho cha vui vẻ;",
      "order": 0,
      "parNumber": 38,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.958Z",
      "updatedAt": "2024-09-07T14:34:20.958Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "ac241884-60e5-46cc-89ba-b59748240799",
      "number": 20,
      "content": "Còn kẻ dại dột lại khinh rẻ mẹ mình.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.958Z",
      "updatedAt": "2024-09-07T14:34:20.958Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "05b831e0-0ba2-4a5f-81e2-87c9f3c31c2e",
      "number": 21,
      "content": "Kẻ kém hiểu biết cho sự điên dại là vui;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.960Z",
      "updatedAt": "2024-09-07T14:34:20.960Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "ea5349d8-413e-4bfb-9ef7-9ce6bedf6a74",
      "number": 21,
      "content": "Nhưng người có trí tuệ cứ bước theo đường chính đáng.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.960Z",
      "updatedAt": "2024-09-07T14:34:20.960Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "da4fa301-4062-4422-b97b-db336ecdad17",
      "number": 22,
      "content": "Thiếu bàn luận các chương trình bị hỏng,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.962Z",
      "updatedAt": "2024-09-07T14:34:20.962Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "1715cf4d-8587-432a-95bc-c9a91b549cff",
      "number": 22,
      "content": "Nhưng với mưu sĩ nhiều chúng có triển vọng thành công.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.962Z",
      "updatedAt": "2024-09-07T14:34:20.962Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "59ea4927-f53d-41b4-992d-d1ef2b4733b2",
      "number": 23,
      "content": "Lời đáp hay làm mọi người vui thích;",
      "order": 0,
      "parNumber": 44,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.964Z",
      "updatedAt": "2024-09-07T14:34:20.964Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "40c0d36a-2be8-4151-9f8a-204d51b0f199",
      "number": 23,
      "content": "Lời đáp hay đúng lúc thật bổ ích biết bao!",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.964Z",
      "updatedAt": "2024-09-07T14:34:20.964Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "13395cb9-566d-43b9-be83-170c61861c9b",
      "number": 24,
      "content": "Người khôn ngoan bước theo con đường sự sống dẫn đi lên,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.966Z",
      "updatedAt": "2024-09-07T14:34:20.966Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "783a7b49-175c-4227-adb2-db61a3c9b213",
      "number": 24,
      "content": "Để giữ người ấy tránh xa âm phủ ở phía dưới.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.966Z",
      "updatedAt": "2024-09-07T14:34:20.966Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "88da608b-f903-49d7-abc6-3134c3ebe7dc",
      "number": 25,
      "content": "CHÚA phá hủy nhà của kẻ kiêu ngạo;",
      "order": 0,
      "parNumber": 48,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.969Z",
      "updatedAt": "2024-09-07T14:34:20.969Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "48e356d1-f3ba-4d62-86e1-40846dd3a810",
      "number": 25,
      "content": "Nhưng Ngài bảo vệ địa giới của người góa bụa.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.969Z",
      "updatedAt": "2024-09-07T14:34:20.969Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "9f501bc5-b671-4206-92c7-7d368652ece7",
      "number": 26,
      "content": "Ý đồ của kẻ gian ác là gớm ghiếc trước mặt CHÚA;",
      "order": 0,
      "parNumber": 50,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.971Z",
      "updatedAt": "2024-09-07T14:34:20.971Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "f0d1f6f7-3307-49e0-a946-78baf3fb68f6",
      "number": 26,
      "content": "Nhưng lời của người trong sạch được đẹp lòng Ngài.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.971Z",
      "updatedAt": "2024-09-07T14:34:20.971Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "e521e46b-a349-42c6-a8cb-398d8467876f",
      "number": 27,
      "content": "Kẻ tham lợi bất nghĩa chuốc lấy rắc rối cho nhà mình;",
      "order": 0,
      "parNumber": 52,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.973Z",
      "updatedAt": "2024-09-07T14:34:20.973Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "766314fd-e6d2-4915-83ca-be33cc0105e5",
      "number": 27,
      "content": "Nhưng ai ghét của hối lộ sẽ tiếp tục sống còn.",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.973Z",
      "updatedAt": "2024-09-07T14:34:20.973Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "65ff7503-7090-445c-8bb8-b0384456098c",
      "number": 28,
      "content": "Người ngay lành suy nghĩ trong lòng rồi mới đáp;",
      "order": 0,
      "parNumber": 54,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.975Z",
      "updatedAt": "2024-09-07T14:34:20.975Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "8089af1b-194a-4eea-a7fd-8b7f42bdf77d",
      "number": 28,
      "content": "Còn miệng kẻ gian ác vội nói những lời độc ác hại người.",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.975Z",
      "updatedAt": "2024-09-07T14:34:20.975Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "72b91fc8-e6ea-404b-9b05-208159b1b135",
      "number": 29,
      "content": "CHÚA thật cách xa đối với phường gian ác,",
      "order": 0,
      "parNumber": 56,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.978Z",
      "updatedAt": "2024-09-07T14:34:20.978Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "8c426436-4273-42e4-9f7a-c9a4e2b4e474",
      "number": 29,
      "content": "Nhưng Ngài lắng nghe lời cầu nguyện của người ngay lành.",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.978Z",
      "updatedAt": "2024-09-07T14:34:20.978Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "a94d6402-ec75-4d22-bbd6-de6324c58549",
      "number": 30,
      "content": "Ánh mắt hân hoan phản ánh lòng vui vẻ,",
      "order": 0,
      "parNumber": 58,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.980Z",
      "updatedAt": "2024-09-07T14:34:20.980Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "de516741-0d20-4772-9430-4ed388c953b7",
      "number": 30,
      "content": "Tin tức vui mừng làm mạnh khỏe cốt xương.",
      "order": 1,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.980Z",
      "updatedAt": "2024-09-07T14:34:20.980Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "16e70f84-be49-4cbd-9fdb-614b54ffca3a",
      "number": 31,
      "content": "Tai chịu lắng nghe những lời quở trách về cuộc sống,",
      "order": 0,
      "parNumber": 60,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.982Z",
      "updatedAt": "2024-09-07T14:34:20.982Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "56339fb6-ff4e-449d-b0fe-f64f83e21cfb",
      "number": 31,
      "content": "Người ấy sẽ ở giữa các bậc hiền triết khôn ngoan.",
      "order": 1,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.982Z",
      "updatedAt": "2024-09-07T14:34:20.982Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "7be2d18a-4d28-470f-9bcb-7ad2b83489e2",
      "number": 32,
      "content": "Kẻ từ chối nghe lời chỉ giáo khinh bỉ linh hồn mình,",
      "order": 0,
      "parNumber": 62,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.984Z",
      "updatedAt": "2024-09-07T14:34:20.984Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "90da777c-ae2d-4565-a044-3192e9f0e7d2",
      "number": 32,
      "content": "Nhưng ai chịu nghe lời khuyên bảo sẽ được sự thông sáng.",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.984Z",
      "updatedAt": "2024-09-07T14:34:20.984Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "4c6217d1-8a96-451e-9843-cfd3d6f93a8e",
      "number": 33,
      "content": "Kính sợ CHÚA là cách giáo dục khôn ngoan,",
      "order": 0,
      "parNumber": 64,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.986Z",
      "updatedAt": "2024-09-07T14:34:20.986Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    },
    {
      "id": "b39a0890-34ba-4012-9bb9-472f70b880a2",
      "number": 33,
      "content": "Đức khiêm nhường đi trước sự tôn trọng.",
      "order": 1,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:20.986Z",
      "updatedAt": "2024-09-07T14:34:20.986Z",
      "chapterId": "3a5879bc-de1b-4901-97ed-acdcc8232fc5"
    }
  ],
