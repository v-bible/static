{
  "chapter": {
    "id": "8faa5787-6c82-4737-a4b5-a63fad704c87",
    "number": 5,
    "ref": "https://www.bible.com/bible/19/PRO.5.BD2011",
    "createdAt": "2024-09-07T11:51:11.642Z",
    "updatedAt": "2024-09-07T11:51:11.642Z",
    "bookId": "71c3c74b-28d8-4c20-87d4-7d45fc5c93e2"
  },
  "verses": [
    {
      "id": "7cb467da-4f51-4935-a8ba-9ed75b85eff6",
      "number": 1,
      "content": "Con ơi, hãy chăm chú vào sự khôn ngoan của cha,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.934Z",
      "updatedAt": "2024-09-07T14:32:27.934Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "7ae54808-6fa0-46c2-8bb3-ff37a06a623f",
      "number": 1,
      "content": "Hãy nghiêng tai nghe kỹ những lời thông sáng của cha;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.934Z",
      "updatedAt": "2024-09-07T14:32:27.934Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "939bf2c8-957f-46d1-8d59-aa12d8f6b6b2",
      "number": 2,
      "content": "Để con biết đắn đo thận trọng,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.944Z",
      "updatedAt": "2024-09-07T14:32:27.944Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "6ea6499d-378f-488e-ba1a-b0715cd9f540",
      "number": 2,
      "content": "Và môi con gìn giữ tri thức.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.944Z",
      "updatedAt": "2024-09-07T14:32:27.944Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "6748f884-2e4f-48a8-b59c-120accc10ed9",
      "number": 3,
      "content": "Vì môi người phụ nữ dâm đãng sẽ ngọt như mật nhểu;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.946Z",
      "updatedAt": "2024-09-07T14:32:27.946Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "a656d184-16cc-4dbf-8308-2b1074bd4e35",
      "number": 3,
      "content": "Miệng lưỡi nàng nói trơn nhẵn hơn dầu;",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.946Z",
      "updatedAt": "2024-09-07T14:32:27.946Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "0344f61d-93f2-4849-a6c8-fc73fd52917e",
      "number": 4,
      "content": "Nhưng đến cuối cùng nó sẽ đắng như ngải cứu,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.949Z",
      "updatedAt": "2024-09-07T14:32:27.949Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "e6366113-22e4-4ef2-965e-411b481f8fb7",
      "number": 4,
      "content": "Nó bén như gươm hai lưỡi đâm vào.",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.949Z",
      "updatedAt": "2024-09-07T14:32:27.949Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "ff76949c-49e0-47d2-89f9-d4cdda60b610",
      "number": 5,
      "content": "Đôi chân nàng đưa người ta vào chỗ chết;",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.951Z",
      "updatedAt": "2024-09-07T14:32:27.951Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "c17943dd-593d-4f15-b18e-2351f2edc5fb",
      "number": 5,
      "content": "Các bước nàng dẫn họ xuống âm ti.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.951Z",
      "updatedAt": "2024-09-07T14:32:27.951Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "ab412f32-f1b1-4eed-a8e1-73fd80d50c27",
      "number": 6,
      "content": "Nàng chẳng nghĩ đến con đường sự sống;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.953Z",
      "updatedAt": "2024-09-07T14:32:27.953Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "b8cded03-96dd-4f3a-bd6d-bb3935368386",
      "number": 6,
      "content": "Đường lối nàng sai lạc nhưng nàng không màng đúng hay sai.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.953Z",
      "updatedAt": "2024-09-07T14:32:27.953Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "ee4c76e5-cde0-466d-a3d0-7d622a697ba5",
      "number": 7,
      "content": "Vì thế bây giờ, các con ơi, hãy lắng nghe lời cha khuyên bảo,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.956Z",
      "updatedAt": "2024-09-07T14:32:27.956Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "6dafc64b-2e89-4f58-b851-ede8a3221e50",
      "number": 7,
      "content": "Đừng lìa bỏ những lời chỉ dạy của miệng cha.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.956Z",
      "updatedAt": "2024-09-07T14:32:27.956Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "7d19d99a-b533-4caf-9345-598f2413acf6",
      "number": 8,
      "content": "Hãy giữ đường con cách xa người đàn bà dâm đãng,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.958Z",
      "updatedAt": "2024-09-07T14:32:27.958Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "2c27b679-4f81-44a3-bd9b-759be161c550",
      "number": 8,
      "content": "Đừng lảng vảng đến gần nơi cửa nhà nàng;",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.958Z",
      "updatedAt": "2024-09-07T14:32:27.958Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "75531dd7-fe65-42d6-b5f4-d4c610d6b909",
      "number": 9,
      "content": "Kẻo con phải trao cả thanh danh con vào tay kẻ khác,",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.960Z",
      "updatedAt": "2024-09-07T14:32:27.960Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "cd85cac4-37b4-48f8-a533-f8b41e826030",
      "number": 9,
      "content": "Và những năm còn lại của đời con nằm trong tay kẻ bạo tàn;",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.960Z",
      "updatedAt": "2024-09-07T14:32:27.960Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "52c0c77a-aa00-46e4-9c7e-bc1299a2c3e7",
      "number": 10,
      "content": "Kẻo những kẻ xa lạ sẽ no nê với của cải con tạo được,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.962Z",
      "updatedAt": "2024-09-07T14:32:27.962Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "6c247856-6e65-4aee-944f-0c6821c6dcd8",
      "number": 10,
      "content": "Và công lao khó nhọc con sẽ rơi vào tay của người ngoài.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.962Z",
      "updatedAt": "2024-09-07T14:32:27.962Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "f72e0e95-335e-4a31-ae35-aa7e630f892d",
      "number": 11,
      "content": "Rồi đến cuối đời, con sẽ than van rên rỉ,",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.964Z",
      "updatedAt": "2024-09-07T14:32:27.964Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "ade2b2cb-72d3-4599-8820-d487c1c5f024",
      "number": 11,
      "content": "Khi thân xác con và cơ thể con đã kiệt quệ rã rời.",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.964Z",
      "updatedAt": "2024-09-07T14:32:27.964Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "78cb65d2-e943-4860-b40c-83b5c0995028",
      "number": 12,
      "content": "Bấy giờ con sẽ ân hận nói, “Lúc đó tại sao ta lại ghét lời chỉ bảo?",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.966Z",
      "updatedAt": "2024-09-07T14:32:27.966Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "ea5b8723-c2b4-45e9-a39b-25ea400433ea",
      "number": 12,
      "content": "Và tại sao lòng ta khinh thường lời quở trách vậy kìa?",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.966Z",
      "updatedAt": "2024-09-07T14:32:27.966Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "712a0267-f42c-438c-981a-da2bd11872ac",
      "number": 13,
      "content": "Tại sao ta không nghe lời chỉ bảo của những người dạy dỗ?",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.969Z",
      "updatedAt": "2024-09-07T14:32:27.969Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "9ae4fc3f-8c5b-47fb-914e-ab1469c93531",
      "number": 13,
      "content": "Tại sao ta không để tai nghe lời hướng dẫn của các bậc thầy?",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.969Z",
      "updatedAt": "2024-09-07T14:32:27.969Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "81b87825-080a-40db-809a-ecad7e409aed",
      "number": 14,
      "content": "Để giờ đây ta hầu như hoàn toàn bị hủy hoại,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.971Z",
      "updatedAt": "2024-09-07T14:32:27.971Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "f2645d01-9598-4eae-996d-6ef68c658fcc",
      "number": 14,
      "content": "Giữa cộng đồng và hội chúng của dân ta.”",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.971Z",
      "updatedAt": "2024-09-07T14:32:27.971Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "ed25d714-0be9-4de6-9acf-32deebe03d2f",
      "number": 15,
      "content": "Hãy uống nước trong ao của con,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.973Z",
      "updatedAt": "2024-09-07T14:32:27.973Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "4e7c91f0-11d5-4f81-9aa7-4fd56ecac55e",
      "number": 15,
      "content": "Hãy uống nước tươi mát trong giếng của con.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.973Z",
      "updatedAt": "2024-09-07T14:32:27.973Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "4d950696-0f13-40bd-b38c-91d482f0cd6f",
      "number": 16,
      "content": "Các suối nước của con nào phải để tuôn đổ ra ngoài sao?",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.975Z",
      "updatedAt": "2024-09-07T14:32:27.975Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "3a7c474d-c658-4f2a-a426-08bca7c26a25",
      "number": 16,
      "content": "Các dòng nước của con nào phải để đổ ra ngoài đường ư?",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.975Z",
      "updatedAt": "2024-09-07T14:32:27.975Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "ec7f7c38-6060-477b-bd48-c0a86491600b",
      "number": 17,
      "content": "Hãy dành riêng chúng cho một mình con hưởng,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.978Z",
      "updatedAt": "2024-09-07T14:32:27.978Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "ff3a0824-e16a-4c06-ab11-dc8f9d355dba",
      "number": 17,
      "content": "Con không thể đem chúng chia sẻ cho những kẻ lạ xa.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.978Z",
      "updatedAt": "2024-09-07T14:32:27.978Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "454bff8e-59ac-4d98-931c-45bc30272ce5",
      "number": 18,
      "content": "Nguyện nguồn mạch của con được phước;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.980Z",
      "updatedAt": "2024-09-07T14:32:27.980Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "7bcac5f9-ceab-4b37-9b92-dae304d4f62c",
      "number": 18,
      "content": "Con hãy vui thỏa với vợ con cưới lúc thanh xuân.",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.980Z",
      "updatedAt": "2024-09-07T14:32:27.980Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "b2f29aed-cfa3-4fe1-b576-53df86c87253",
      "number": 19,
      "content": "Nguyện nàng như một nai cái đáng yêu, một hươu cái yêu kiều;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.982Z",
      "updatedAt": "2024-09-07T14:32:27.982Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "957c6a4d-98fb-4c82-aeb6-cb821ebfd3c9",
      "number": 19,
      "content": "Nguyện đôi gò bồng đảo nàng làm con luôn thỏa thích,",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.982Z",
      "updatedAt": "2024-09-07T14:32:27.982Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "2cb75e78-6c75-4b27-aa3e-b6af500f7989",
      "number": 19,
      "content": "Nguyện ái tình nàng làm con mãi say mê.",
      "order": 2,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.982Z",
      "updatedAt": "2024-09-07T14:32:27.982Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "b400ef53-c20c-44fd-8193-2e038e34fedf",
      "number": 20,
      "content": "Con ơi, lẽ nào con lại mê đắm một người đàn bà dâm đãng,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.984Z",
      "updatedAt": "2024-09-07T14:32:27.984Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "40fbddc8-2495-498d-8851-6550b673b430",
      "number": 20,
      "content": "Và ôm ấp vào lòng một thân xác lạ xa?",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.984Z",
      "updatedAt": "2024-09-07T14:32:27.984Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "db59eefb-cc6b-4a38-ac36-b92a78860db9",
      "number": 21,
      "content": "Khá biết rằng đường lối của mỗi người luôn ở trước mắt CHÚA;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.986Z",
      "updatedAt": "2024-09-07T14:32:27.986Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "316565ae-b56a-4b74-9354-a9eaf793c631",
      "number": 21,
      "content": "Bất cứ lúc nào Ngài cũng nhìn thấy mọi nẻo của mọi người.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.986Z",
      "updatedAt": "2024-09-07T14:32:27.986Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "519848bf-da97-47a5-85b6-fe91c12e4f64",
      "number": 22,
      "content": "Những tội ác của kẻ ác sẽ vây bắt nó;",
      "order": 0,
      "parNumber": 44,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.988Z",
      "updatedAt": "2024-09-07T14:32:27.988Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "157db4f6-28f9-4858-911c-b268ef3068e0",
      "number": 22,
      "content": "Những tội lỗi của nó sẽ thành các dây trói chặt nó.",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.988Z",
      "updatedAt": "2024-09-07T14:32:27.988Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "d19a680d-891a-410b-bba6-3d5c97915af7",
      "number": 23,
      "content": "Nó sẽ chết vì thiếu lời giáo huấn;",
      "order": 0,
      "parNumber": 46,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.991Z",
      "updatedAt": "2024-09-07T14:32:27.991Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    },
    {
      "id": "aaf60de9-1727-439d-8cbc-23db849fba12",
      "number": 23,
      "content": "Nó sẽ bị lạc đường vì quá dại dột ngông cuồng.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:27.991Z",
      "updatedAt": "2024-09-07T14:32:27.991Z",
      "chapterId": "8faa5787-6c82-4737-a4b5-a63fad704c87"
    }
  ],
