{
  "chapter": {
    "id": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9",
    "number": 4,
    "ref": "https://www.bible.com/bible/19/PRO.4.BD2011",
    "createdAt": "2024-09-23T05:08:40.729Z",
    "updatedAt": "2024-09-23T05:08:40.729Z",
    "bookId": "2cf03725-457c-4fab-86fe-f08be1e90c98"
  },
  "verses": [
    {
      "id": "86109e17-5049-4ead-b686-95f438295303",
      "number": 1,
      "content": "Các con ơi, hãy lắng nghe lời cha dạy bảo;",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:44.960Z",
      "updatedAt": "2024-09-23T05:55:44.960Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "fa3cb9a6-eb91-4da2-ad20-57cbba78d937",
      "number": 1,
      "content": "Hãy chú tâm lãnh hội đến tinh tường.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:44.977Z",
      "updatedAt": "2024-09-23T05:55:44.977Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "91fd050a-82d5-4451-a1d9-9f2e916964b2",
      "number": 2,
      "content": "Vì cha cho các con những lời khuyên dạy tốt;",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:44.982Z",
      "updatedAt": "2024-09-23T05:55:44.982Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "7feb3bb5-6b15-4825-bd96-db2dc9066e7a",
      "number": 2,
      "content": "Mong các con đừng bỏ lời giáo huấn của cha.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:44.987Z",
      "updatedAt": "2024-09-23T05:55:44.987Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "2cedcfe5-071e-4807-aa00-76de003b5b54",
      "number": 3,
      "content": "Khi cha còn là một đứa con thơ dại của ông nội các con,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:44.991Z",
      "updatedAt": "2024-09-23T05:55:44.991Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "4a5ce8bd-d299-4b3d-88b4-4927a6421aac",
      "number": 3,
      "content": "Là con một yêu dấu của bà nội các con.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:44.996Z",
      "updatedAt": "2024-09-23T05:55:44.996Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "d6cd9c4f-b018-4d3e-8c58-9463e485b129",
      "number": 4,
      "content": "Ông nội các con đã dạy cha rằng:",
      "order": 0,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.001Z",
      "updatedAt": "2024-09-23T05:55:45.001Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "5ae63d60-51fa-49bf-b873-c81f7d0142aa",
      "number": 4,
      "content": "Lòng con khá ghi tạc lời cha dạy bảo;",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.006Z",
      "updatedAt": "2024-09-23T05:55:45.006Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "5df0b831-bffd-40f4-b399-c1ddda958082",
      "number": 4,
      "content": "Giữ mệnh lệnh cha để con sống oai hùng:",
      "order": 2,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.011Z",
      "updatedAt": "2024-09-23T05:55:45.011Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "2c8a428b-9782-440c-8e72-5ef96c874502",
      "number": 5,
      "content": "Hãy kiếm khôn ngoan, khá tìm thông sáng,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.016Z",
      "updatedAt": "2024-09-23T05:55:45.016Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "79343646-58b6-4442-a13f-ce4d2650fae6",
      "number": 5,
      "content": "Đừng quên và chớ lìa bỏ lời cha.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.021Z",
      "updatedAt": "2024-09-23T05:55:45.021Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "73847339-35ee-4332-b91c-fe8b17e05c8a",
      "number": 6,
      "content": "Đừng lìa bỏ khôn ngoan, thì nàng sẽ bảo vệ con;",
      "order": 0,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.026Z",
      "updatedAt": "2024-09-23T05:55:45.026Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "7af5b432-1355-49a1-8e72-ea8d2f45b7d5",
      "number": 6,
      "content": "Yêu mến nàng, thì nàng sẽ gìn giữ con.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.030Z",
      "updatedAt": "2024-09-23T05:55:45.030Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "db788b95-6d7a-4963-8d5e-ecae5c1c3951",
      "number": 7,
      "content": "Khôn ngoan là nhu cầu tối thượng, nên con phải đạt sự khôn ngoan;",
      "order": 0,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.036Z",
      "updatedAt": "2024-09-23T05:55:45.036Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "d7eece92-3ce4-4e75-81b0-e18821fccc01",
      "number": 7,
      "content": "Trong mọi thứ con đạt được ở đời, hãy cố đạt sự thông sáng.",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.041Z",
      "updatedAt": "2024-09-23T05:55:45.041Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "1c82ec98-f686-43c4-882d-1ab4a703af2c",
      "number": 8,
      "content": "Hãy coi trọng nàng, nàng sẽ làm con được tôn trọng;",
      "order": 0,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.046Z",
      "updatedAt": "2024-09-23T05:55:45.046Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "df4d1918-5edd-43dc-b644-5841b52b6963",
      "number": 8,
      "content": "Ôm giữ nàng, nàng sẽ mang vinh dự đến cho con.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.051Z",
      "updatedAt": "2024-09-23T05:55:45.051Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "4bbb8024-e40e-4dbf-9811-19bb551c3a35",
      "number": 9,
      "content": "Nàng sẽ đặt trên đầu con một vòng hoa tuyệt mỹ;",
      "order": 0,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.057Z",
      "updatedAt": "2024-09-23T05:55:45.057Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "6eaa969c-14fb-40b5-aa30-50c72840e7a3",
      "number": 9,
      "content": "Nàng sẽ dâng tặng con mão miện đắc thắng huy hoàng.",
      "order": 1,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.062Z",
      "updatedAt": "2024-09-23T05:55:45.062Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "f78c5d04-15e9-4817-bed0-3f3bc923c9ee",
      "number": 10,
      "content": "Con ơi, hãy lắng nghe và tiếp nhận lời cha,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.066Z",
      "updatedAt": "2024-09-23T05:55:45.066Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "20b007e7-3b75-4284-be76-2684b93d1357",
      "number": 10,
      "content": "Thì những năm của đời con sẽ thêm nhiều lên.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.076Z",
      "updatedAt": "2024-09-23T05:55:45.076Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "3039e185-2596-489f-97d6-6d3038a598e4",
      "number": 11,
      "content": "Cha đã dạy dỗ con theo đường lối khôn ngoan;",
      "order": 0,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.081Z",
      "updatedAt": "2024-09-23T05:55:45.081Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "dd9a3771-1f75-48ac-ad0d-43a612386bc8",
      "number": 11,
      "content": "Cha đã hướng dẫn con theo những nẻo đường ngay thẳng.",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.086Z",
      "updatedAt": "2024-09-23T05:55:45.086Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "4a25e727-4df9-41e9-88a5-feb424acc4b4",
      "number": 12,
      "content": "Khi con đi, các bước con sẽ không bị cản trở;",
      "order": 0,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.091Z",
      "updatedAt": "2024-09-23T05:55:45.091Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "9b5cac36-74d5-4767-a242-bb7e9a51f4c3",
      "number": 12,
      "content": "Nếu con chạy, con sẽ không bị vấp ngã bao giờ.",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.096Z",
      "updatedAt": "2024-09-23T05:55:45.096Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "c3c08250-4bd9-48bb-9b9b-a2383b747c16",
      "number": 13,
      "content": "Hãy giữ chặt lời giáo huấn, đừng buông nó ra.",
      "order": 0,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.101Z",
      "updatedAt": "2024-09-23T05:55:45.101Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "899bb418-be16-44c7-86ce-7ea7aca65f6e",
      "number": 13,
      "content": "Hãy canh giữ nàng, vì nàng là sự sống của con.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.106Z",
      "updatedAt": "2024-09-23T05:55:45.106Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "f4d53e60-febc-499f-a079-451af7800d0d",
      "number": 14,
      "content": "Con đừng đi vào nẻo đường của phường gian ác,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.111Z",
      "updatedAt": "2024-09-23T05:55:45.111Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "bb1d4120-3ff3-4999-b3a6-086d42e4bf7d",
      "number": 14,
      "content": "Bước chân con chớ đặt vào lối của bọn gian tà.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.116Z",
      "updatedAt": "2024-09-23T05:55:45.116Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "5d372d2f-62d6-48a0-8b15-6d875a16722f",
      "number": 15,
      "content": "Con hãy tránh xa nó và đừng đi vào đường đó;",
      "order": 0,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.121Z",
      "updatedAt": "2024-09-23T05:55:45.121Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "f17e5d64-f109-4160-a984-0cb00db3cd19",
      "number": 15,
      "content": "Con hãy quay khỏi nó và tiếp tục đường mình.",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.126Z",
      "updatedAt": "2024-09-23T05:55:45.126Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "f7dc5e57-0e42-4fe4-a3cb-9ca4b5b418b7",
      "number": 16,
      "content": "Vì kẻ ác sẽ không ngủ được nếu không làm gì ác đức;",
      "order": 0,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.131Z",
      "updatedAt": "2024-09-23T05:55:45.131Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "fe6f10df-df64-413d-b78d-57a2bf43b569",
      "number": 16,
      "content": "Chúng sẽ bị mất ngủ nếu chưa gây vấp ngã cho ai.",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.136Z",
      "updatedAt": "2024-09-23T05:55:45.136Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "093eed12-213e-45d2-9edf-ba29b6bcc77c",
      "number": 17,
      "content": "Vì chúng cho làm chuyện gian tà thiết yếu như ăn cơm ăn bánh,",
      "order": 0,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.140Z",
      "updatedAt": "2024-09-23T05:55:45.140Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "5e712380-50e9-49d4-a760-bff64c5b6d8f",
      "number": 17,
      "content": "Và cho việc bạo tàn là cần có như uống rượu khi ăn.",
      "order": 1,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.145Z",
      "updatedAt": "2024-09-23T05:55:45.145Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "c14cfee2-ccdb-423b-a892-0af922e6fdd1",
      "number": 18,
      "content": "Đường của người ngay lành giống như ánh bình minh rực rỡ,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.151Z",
      "updatedAt": "2024-09-23T05:55:45.151Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "29dd87eb-7247-4590-b060-dea3f68f8aaf",
      "number": 18,
      "content": "Càng lúc càng rạng ngời cho đến khi chiếu sáng khắp bầu trời;",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.156Z",
      "updatedAt": "2024-09-23T05:55:45.156Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "e05b8a48-93b2-46dd-914d-a06c03dcf605",
      "number": 19,
      "content": "Còn đường kẻ gian ác giống như đêm khuya tăm tối;",
      "order": 0,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.161Z",
      "updatedAt": "2024-09-23T05:55:45.161Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "9f269ea3-a31a-463c-a851-97e25b5e48e3",
      "number": 19,
      "content": "Chúng ngã nhào mà không biết ngã vì sao.",
      "order": 1,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.165Z",
      "updatedAt": "2024-09-23T05:55:45.165Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "1a99f4f6-a7e6-4a09-89c0-ea6d13ae8468",
      "number": 20,
      "content": "Con ơi, hãy chú tâm để ý đến lời cha;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.170Z",
      "updatedAt": "2024-09-23T05:55:45.170Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "049ef373-0bc2-4dc1-bd23-7692e4694e84",
      "number": 20,
      "content": "Hãy nghiêng tai nghe lời cha khuyên bảo.",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.175Z",
      "updatedAt": "2024-09-23T05:55:45.175Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "cd8292c5-c4d6-46d1-9bff-9a59f798d83f",
      "number": 21,
      "content": "Đừng để chúng vuột khỏi tầm mắt con;",
      "order": 0,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.180Z",
      "updatedAt": "2024-09-23T05:55:45.180Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "424b0363-d3f4-4b88-9dec-31f3a49802f7",
      "number": 21,
      "content": "Hãy giữ chúng trong lòng con.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.185Z",
      "updatedAt": "2024-09-23T05:55:45.185Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "3fb1ce3b-df87-4b2b-a806-dfe5c03edf1a",
      "number": 22,
      "content": "Vì chúng là sự sống cho người nào tìm được chúng;",
      "order": 0,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.190Z",
      "updatedAt": "2024-09-23T05:55:45.190Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "eccf21b9-74d1-43f6-b3e2-0f98c0fc2cf2",
      "number": 22,
      "content": "Chúng là sức khỏe cho thân thể ai có chúng trong lòng.",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.195Z",
      "updatedAt": "2024-09-23T05:55:45.195Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "62ab0155-0b0c-44ea-bf54-dc635b4158b4",
      "number": 23,
      "content": "Hãy cẩn thận giữ tấm lòng của con hơn hết,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.200Z",
      "updatedAt": "2024-09-23T05:55:45.200Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "40d30e17-79e5-47c8-b8ac-2a5450158eb0",
      "number": 23,
      "content": "Vì các nguồn sự sống đều từ đó mà ra.",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.206Z",
      "updatedAt": "2024-09-23T05:55:45.206Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "468b0e31-cfda-42a8-a3b4-4ed080825037",
      "number": 24,
      "content": "Hãy bỏ khỏi miệng con những lời điêu ngoa gian dối,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.211Z",
      "updatedAt": "2024-09-23T05:55:45.211Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "02e89557-a0b9-4cfb-a583-e7264c32308c",
      "number": 24,
      "content": "Hãy quăng khỏi môi con những lời dối trá lọc lừa.",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.216Z",
      "updatedAt": "2024-09-23T05:55:45.216Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "c16375d6-bd40-4948-bd89-92110ad9efe8",
      "number": 25,
      "content": "Mắt con hãy nhìn thẳng về phía trước,",
      "order": 0,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.221Z",
      "updatedAt": "2024-09-23T05:55:45.221Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "a53ba982-8335-4a10-9484-668a90c1103e",
      "number": 25,
      "content": "Ánh mắt con khá hướng thẳng về phía trước mặt con.",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.226Z",
      "updatedAt": "2024-09-23T05:55:45.226Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "1e3dc5bc-0a1e-4d25-88ac-d7af9fe463f3",
      "number": 26,
      "content": "Hãy giữ chân con trong đường ngay nẻo chánh,",
      "order": 0,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.231Z",
      "updatedAt": "2024-09-23T05:55:45.231Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "badb1bc4-c659-449e-8c84-d4aa59152198",
      "number": 26,
      "content": "Để mọi đường lối con được thiết lập vững vàng.",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.236Z",
      "updatedAt": "2024-09-23T05:55:45.236Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "2a96e416-3270-4420-a84f-9655aad57535",
      "number": 27,
      "content": "Đừng quay qua bên phải hoặc quay qua bên trái;",
      "order": 0,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.241Z",
      "updatedAt": "2024-09-23T05:55:45.241Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    },
    {
      "id": "0bf2ea96-9783-4827-923f-1399fc4d57e2",
      "number": 27,
      "content": "Hãy quay bước chân con ra khỏi chỗ gian tà.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-23T05:55:45.245Z",
      "updatedAt": "2024-09-23T05:55:45.245Z",
      "chapterId": "f5dc5d00-01e6-4c47-87a7-b5a275a900a9"
    }
  ],
