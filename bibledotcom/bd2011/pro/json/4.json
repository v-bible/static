    },
    {
      "id": "6c946880-8c8f-4f58-9bf9-792e35bceb67",
      "content": "Sống theo Đường Chính Trực",
      "order": 0,
      "createdAt": "2024-09-23T05:55:45.071Z",
      "updatedAt": "2024-09-23T05:55:45.071Z",
