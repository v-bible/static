{
  "chapter": {
    "id": "9d8215d2-43a1-4172-9d4b-0a439797bfd4",
    "number": 7,
    "ref": "https://www.bible.com/bible/19/PRO.7.BD2011",
    "createdAt": "2024-09-07T11:51:11.658Z",
    "updatedAt": "2024-09-07T11:51:11.658Z",
    "bookId": "71c3c74b-28d8-4c20-87d4-7d45fc5c93e2"
  },
  "verses": [
    {
      "id": "c8952196-93fc-4930-9453-63106dce5f9f",
      "number": 1,
      "content": "Con ơi, hãy giữ lấy những lời cha dạy bảo;",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.697Z",
      "updatedAt": "2024-09-07T14:32:39.697Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "12cb67c1-14f9-4dd8-a0b8-e5e92c646956",
      "number": 1,
      "content": "Hãy ghi nhớ những điều răn bảo của cha;",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.697Z",
      "updatedAt": "2024-09-07T14:32:39.697Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "45fc9954-5716-4caf-ae97-6d1b4b0fddea",
      "number": 2,
      "content": "Con hãy giữ những mệnh lệnh của cha mà sống;",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.707Z",
      "updatedAt": "2024-09-07T14:32:39.707Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "dd2f150a-8f89-4bca-9c98-b4f5bbe3eefb",
      "number": 2,
      "content": "Giữ những lời dạy dỗ của cha như con ngươi của mắt con;",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.707Z",
      "updatedAt": "2024-09-07T14:32:39.707Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "1b4f5412-1e5d-422f-a75c-4cd360e061e6",
      "number": 3,
      "content": "Con hãy cột chúng nơi các ngón tay con;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.709Z",
      "updatedAt": "2024-09-07T14:32:39.709Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "8f66f4ca-ccbd-48fd-a242-cf802891e1b0",
      "number": 3,
      "content": "Con hãy ghi khắc chúng vào tâm khảm con.",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.709Z",
      "updatedAt": "2024-09-07T14:32:39.709Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "808bdba9-a14e-41e3-a57f-2806a8dd7f93",
      "number": 4,
      "content": "Con hãy nói với sự khôn ngoan rằng, “Nàng là em gái ta,”",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.712Z",
      "updatedAt": "2024-09-07T14:32:39.712Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "fa103767-7238-4568-b910-873f08c38459",
      "number": 4,
      "content": "Con hãy coi sự thông sáng như bạn thân của con,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.712Z",
      "updatedAt": "2024-09-07T14:32:39.712Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "bc8bb27f-5939-41cc-9b11-b6741366e609",
      "number": 5,
      "content": "Để chúng giữ con khỏi người phụ nữ không đứng đắn,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.714Z",
      "updatedAt": "2024-09-07T14:32:39.714Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "42224924-f673-402f-84d7-9ea59301e556",
      "number": 5,
      "content": "Tức giữ con khỏi người đàn bà dâm đãng có lời dụ dỗ ngọt ngào.",
      "order": 1,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.714Z",
      "updatedAt": "2024-09-07T14:32:39.714Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "1dac4127-b839-4f1f-ad69-e5e78dcaa129",
      "number": 6,
      "content": "Vì từ trong nhà nhìn qua cửa sổ,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.716Z",
      "updatedAt": "2024-09-07T14:32:39.716Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "31e18ff1-dbd8-4cb4-ac64-2aedb06ec140",
      "number": 6,
      "content": "Cha nhìn qua khung cửa sổ của nhà mình,",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.716Z",
      "updatedAt": "2024-09-07T14:32:39.716Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "2bfb2c02-91bd-4e37-b499-905a1a5de1ad",
      "number": 7,
      "content": "Cha thấy trong số những kẻ ngây thơ khờ khạo,",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.719Z",
      "updatedAt": "2024-09-07T14:32:39.719Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "befc37a9-7d16-45e8-93fd-255b4d4ff18c",
      "number": 7,
      "content": "Cha để ý trong số những trai trẻ",
      "order": 1,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.719Z",
      "updatedAt": "2024-09-07T14:32:39.719Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "bc772f17-13c4-441c-87d8-e4da71a5e007",
      "number": 7,
      "content": "Có một chàng trai khờ dại,",
      "order": 2,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.719Z",
      "updatedAt": "2024-09-07T14:32:39.719Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "997afc17-b96d-42ec-ba37-61f41941b9b8",
      "number": 8,
      "content": "Hắn đi dọc theo con đường dẫn đến góc nhà nàng,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.721Z",
      "updatedAt": "2024-09-07T14:32:39.721Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "4235c65e-e04b-4139-bb53-51250badee18",
      "number": 8,
      "content": "Rồi hắn tẻ vào lối dẫn vào nhà nàng,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.721Z",
      "updatedAt": "2024-09-07T14:32:39.721Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "1f9dc7bb-4671-492c-aca9-3e9484d96a11",
      "number": 9,
      "content": "Khi trời chạng vạng, lúc trời sụp tối,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.723Z",
      "updatedAt": "2024-09-07T14:32:39.723Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "67abdbc2-6324-40da-b722-76f1fb586240",
      "number": 9,
      "content": "Khi màn đêm vừa buông xuống, lúc bóng tối bắt đầu bao trùm.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.723Z",
      "updatedAt": "2024-09-07T14:32:39.723Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "94265b10-cb91-4577-a603-0c33764ef0c6",
      "number": 10,
      "content": "Bấy giờ một phụ nữ đi ra đón hắn;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.725Z",
      "updatedAt": "2024-09-07T14:32:39.725Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "b2e48b51-a3d2-44d6-ba49-54fa140be20e",
      "number": 10,
      "content": "Nàng trưng diện như một con điếm với quỷ kế đầy lòng.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.725Z",
      "updatedAt": "2024-09-07T14:32:39.725Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "d0a58db8-2551-4eab-bb4e-79689c762966",
      "number": 11,
      "content": "Miệng nàng nói om sòm và không giữ gìn nết na ý tứ;",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.727Z",
      "updatedAt": "2024-09-07T14:32:39.727Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "40801fc1-2365-4913-b86e-af270eeeda97",
      "number": 11,
      "content": "Chân nàng thường không ở trong nhà,",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.727Z",
      "updatedAt": "2024-09-07T14:32:39.727Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "3ee026d6-2ddc-40eb-a221-207d8d68c6ce",
      "number": 12,
      "content": "Khi thì ở ngoài đường, khi thì ở đầu phố;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.729Z",
      "updatedAt": "2024-09-07T14:32:39.729Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "59247014-19ea-4a9c-8b9a-10434d63a49f",
      "number": 12,
      "content": "Tại các góc đường nàng rình rập đợi chờ.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.729Z",
      "updatedAt": "2024-09-07T14:32:39.729Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "e6895ca7-b179-4bdc-97a6-fa987b9b138c",
      "number": 13,
      "content": "Nàng chụp lấy hắn và ôm hôn hắn,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.731Z",
      "updatedAt": "2024-09-07T14:32:39.731Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "e785b461-38a4-4e11-b71f-f76bd9ec1ee4",
      "number": 13,
      "content": "Rồi với mặt chai mày đá nàng nói với hắn rằng,",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.731Z",
      "updatedAt": "2024-09-07T14:32:39.731Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "fcfb8008-87ef-4428-bebc-ab6ac8b55136",
      "number": 14,
      "content": "“Em phải dâng một của lễ tạ ơn ở nhà;",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.733Z",
      "updatedAt": "2024-09-07T14:32:39.733Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "c9df72ba-a5af-4f9b-aab8-3aaad0be168e",
      "number": 14,
      "content": "Hôm nay em phải trả những gì em đã thề nguyện;",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.733Z",
      "updatedAt": "2024-09-07T14:32:39.733Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "c5c81b40-24a1-4f87-89e9-8426e664958f",
      "number": 15,
      "content": "Vì thế em đi ra kiếm anh;",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.735Z",
      "updatedAt": "2024-09-07T14:32:39.735Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "bd0d7019-aa70-45ff-974b-05e0d73dfbf0",
      "number": 15,
      "content": "Em tìm anh hết sức, và bây giờ em mới tìm được.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.735Z",
      "updatedAt": "2024-09-07T14:32:39.735Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "8fb61ca4-e692-4eb9-b559-90649f47ad87",
      "number": 16,
      "content": "Em đã làm giường sẵn rồi,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.737Z",
      "updatedAt": "2024-09-07T14:32:39.737Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "119eb7c2-71ea-495f-baeb-c0ae3cb98a2b",
      "number": 16,
      "content": "Vải trải giường làm bằng chỉ màu của Ai-cập.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.737Z",
      "updatedAt": "2024-09-07T14:32:39.737Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "fd92670c-cafe-479a-a813-8f369ff3c2c1",
      "number": 17,
      "content": "Em đã tẩm dầu thơm trên giường,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.739Z",
      "updatedAt": "2024-09-07T14:32:39.739Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "179fff5c-7c7a-4738-954c-3449e8f77e3d",
      "number": 17,
      "content": "Đó là dầu thơm của mộc dược, lô hội, và quế hương.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.739Z",
      "updatedAt": "2024-09-07T14:32:39.739Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "3706f145-fcc3-439d-8bea-3b90e5c248e7",
      "number": 18,
      "content": "Hãy đến, chúng ta hãy ân ái với nhau cho đến sáng;",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.742Z",
      "updatedAt": "2024-09-07T14:32:39.742Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "8c17c3ef-ab2c-4bb8-add6-08aeb607f380",
      "number": 18,
      "content": "Chúng ta hãy hưởng khoái lạc của yêu đương.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.742Z",
      "updatedAt": "2024-09-07T14:32:39.742Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "58264fd8-cbd5-4eb3-b181-d66ea4eed0b7",
      "number": 19,
      "content": "Vì chồng em không có ở nhà;",
      "order": 0,
      "parNumber": 38,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.744Z",
      "updatedAt": "2024-09-07T14:32:39.744Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "d324c9fa-d000-402d-8822-aafb74d1b5b3",
      "number": 19,
      "content": "Hắn đã đi xa.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.744Z",
      "updatedAt": "2024-09-07T14:32:39.744Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "5a2da648-fb9c-4ddb-8771-a84482fcc4ca",
      "number": 20,
      "content": "Hắn đã mang túi tiền theo hắn;",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.746Z",
      "updatedAt": "2024-09-07T14:32:39.746Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "4b2efe3f-4505-4d71-8461-c6ecd3331f45",
      "number": 20,
      "content": "Hắn sẽ không về nhà cho đến trăng rằm.”",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.746Z",
      "updatedAt": "2024-09-07T14:32:39.746Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "561eb3fa-f2fd-4669-b7b3-8052720d0a17",
      "number": 21,
      "content": "Nàng dùng những lời quyến dụ như thế để thuyết phục hắn;",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.748Z",
      "updatedAt": "2024-09-07T14:32:39.748Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "3c1c97a7-1290-4a6c-8d57-f58f18ce3858",
      "number": 21,
      "content": "Nàng dụ dỗ hắn bằng những lời lẽ ngọt ngào.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.748Z",
      "updatedAt": "2024-09-07T14:32:39.748Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "c0f8e851-4dd4-42bd-a2c6-b00110673567",
      "number": 22,
      "content": "Hắn bèn đi theo nàng",
      "order": 0,
      "parNumber": 44,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.750Z",
      "updatedAt": "2024-09-07T14:32:39.750Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "6bd6dd0d-0b8d-4ea8-9720-a4e73dbaf28a",
      "number": 22,
      "content": "Như con bò bị dẫn đến lò làm thịt,",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.750Z",
      "updatedAt": "2024-09-07T14:32:39.750Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "50133c29-b893-4ded-8803-ed8873234f15",
      "number": 22,
      "content": "Như con nai tiến lại bẫy đang chờ,",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.750Z",
      "updatedAt": "2024-09-07T14:32:39.750Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "3dd78a52-b1fa-4c78-9496-2ad127286b63",
      "number": 23,
      "content": "Cho đến khi mũi tên ghim xuyên qua gan hắn.",
      "order": 0,
      "parNumber": 47,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.753Z",
      "updatedAt": "2024-09-07T14:32:39.753Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "7707e78d-ec9f-401f-ad2c-c5ee14af9d0a",
      "number": 23,
      "content": "Hắn như con chim bay chúi vào bẫy lưới,",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.753Z",
      "updatedAt": "2024-09-07T14:32:39.753Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "36951117-752b-4991-bbeb-2b52e8aa86d7",
      "number": 23,
      "content": "Mà không biết rằng làm như thế là hại chết đời mình.",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.753Z",
      "updatedAt": "2024-09-07T14:32:39.753Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "4fcf806b-e277-4519-ab2e-955a8eb356fd",
      "number": 24,
      "content": "Vậy con ơi, bây giờ hãy nghe lời cha dạy bảo,",
      "order": 0,
      "parNumber": 50,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.758Z",
      "updatedAt": "2024-09-07T14:32:39.758Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "5e3227a9-87d4-4bd7-925c-ae7b6c9ae5d0",
      "number": 24,
      "content": "Hãy chú tâm để ý những lời nói của miệng cha.",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.758Z",
      "updatedAt": "2024-09-07T14:32:39.758Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "c5f5c88f-2b77-44ef-837e-54377bd4b358",
      "number": 25,
      "content": "Đừng bao giờ để lòng con hướng theo đường lối nàng;",
      "order": 0,
      "parNumber": 52,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.764Z",
      "updatedAt": "2024-09-07T14:32:39.764Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "f7febda0-bb2f-4ae7-9959-9c77c1bea5b8",
      "number": 25,
      "content": "Chớ khi nào để con đi lạc vào các nẻo nàng.",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.764Z",
      "updatedAt": "2024-09-07T14:32:39.764Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "01c98f81-b9ab-4415-a9ec-d6afc5df72d8",
      "number": 26,
      "content": "Vì nàng đã làm cho nhiều người thân bại danh liệt,",
      "order": 0,
      "parNumber": 54,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.769Z",
      "updatedAt": "2024-09-07T14:32:39.769Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "c7690a47-5e84-4b69-bb48-66d62a059b29",
      "number": 26,
      "content": "Các nạn nhân của nàng đông không biết ngần nào.",
      "order": 1,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.769Z",
      "updatedAt": "2024-09-07T14:32:39.769Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "2cddc484-0e78-4bbe-95e2-940d47a49c0d",
      "number": 27,
      "content": "Đường tới nhà nàng là đường dẫn vào âm phủ,",
      "order": 0,
      "parNumber": 56,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.774Z",
      "updatedAt": "2024-09-07T14:32:39.774Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    },
    {
      "id": "2e648205-9266-4ae5-b241-0bc814ce0640",
      "number": 27,
      "content": "Đường đưa xuống các tù ngục của tử thần.",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:32:39.774Z",
      "updatedAt": "2024-09-07T14:32:39.774Z",
      "chapterId": "9d8215d2-43a1-4172-9d4b-0a439797bfd4"
    }
  ],
