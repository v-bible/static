{
  "chapter": {
    "id": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba",
    "number": 13,
    "ref": "https://www.bible.com/bible/19/PRO.13.BD2011",
    "createdAt": "2024-09-07T11:51:11.706Z",
    "updatedAt": "2024-09-07T11:51:11.706Z",
    "bookId": "71c3c74b-28d8-4c20-87d4-7d45fc5c93e2"
  },
  "verses": [
    {
      "id": "8b369ee6-d0e5-4d28-8318-60a527975614",
      "number": 1,
      "content": "Con khôn ngoan nghe lời cha khuyên dạy,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.647Z",
      "updatedAt": "2024-09-07T14:34:00.647Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "b09ac693-1056-4c98-8e0d-ac67e11301e9",
      "number": 1,
      "content": "Còn đứa nhạo báng không nghe lời quở trách của ai.",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.647Z",
      "updatedAt": "2024-09-07T14:34:00.647Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "0f50fb6c-43e9-46a4-9760-1c82f741d941",
      "number": 2,
      "content": "Người tốt sẽ hưởng bông trái lời mình nói,",
      "order": 0,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.656Z",
      "updatedAt": "2024-09-07T14:34:00.656Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "303da4f5-fdce-4107-8edb-94318f4c3dc0",
      "number": 2,
      "content": "Còn kẻ bội bạc phải chuốc lấy bạo tàn.",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.656Z",
      "updatedAt": "2024-09-07T14:34:00.656Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "93bdeb3e-374e-49b6-84e9-ae4e50bcb042",
      "number": 3,
      "content": "Ai giữ miệng mình giữ được mạng sống mình,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.660Z",
      "updatedAt": "2024-09-07T14:34:00.660Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "90fa24e9-d07d-4c8f-90db-7a703c39046a",
      "number": 3,
      "content": "Kẻ nói nhiều ắt bại hoại chẳng sai.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.660Z",
      "updatedAt": "2024-09-07T14:34:00.660Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "91eddf97-998c-4efd-86a6-1286e9e697ef",
      "number": 4,
      "content": "Kẻ biếng nhác đói rũ mà không có gì ăn đỡ bụng,",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.664Z",
      "updatedAt": "2024-09-07T14:34:00.664Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "ef1b5971-143a-46da-961a-09fdec9025c9",
      "number": 4,
      "content": "Còn người siêng năng lúc nào thực phẩm cũng dư thừa.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.664Z",
      "updatedAt": "2024-09-07T14:34:00.664Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "13a7fdac-42e3-40c8-8fd4-64370fa57265",
      "number": 5,
      "content": "Người ngay lành ghét những gì gian dối,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.667Z",
      "updatedAt": "2024-09-07T14:34:00.667Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "4a7239c7-6116-474c-b415-f7e4a117427f",
      "number": 5,
      "content": "Còn kẻ ác gian mang đến xấu hổ và ô nhơ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.667Z",
      "updatedAt": "2024-09-07T14:34:00.667Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "1f3a70d9-db73-4934-b239-ab834de54d87",
      "number": 6,
      "content": "Đức công chính bảo hộ người theo đường chân chính,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.671Z",
      "updatedAt": "2024-09-07T14:34:00.671Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "71bcfc3f-6a1b-4749-809d-405d09dec1fc",
      "number": 6,
      "content": "Còn sự gian tà lật đổ kẻ ác gian.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.671Z",
      "updatedAt": "2024-09-07T14:34:00.671Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "d3f05d3b-aec5-4bc9-8956-ab31950f11a0",
      "number": 7,
      "content": "Có kẻ làm ra vẻ ta đây giàu có mà chẳng có gì cả,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.673Z",
      "updatedAt": "2024-09-07T14:34:00.673Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "273ba9e8-8b56-4f5b-8a92-62443f4a62d8",
      "number": 7,
      "content": "Cũng có người làm ra vẻ nghèo nàn nhưng lại có của cải rất nhiều.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.673Z",
      "updatedAt": "2024-09-07T14:34:00.673Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "a147c460-9449-49eb-b8f2-0c3091b68816",
      "number": 8,
      "content": "Của cải nhiều có thể là tiền để chờ chuộc mạng,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.675Z",
      "updatedAt": "2024-09-07T14:34:00.675Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "0ddadbfb-3089-4918-9523-a1b9b15128de",
      "number": 8,
      "content": "Còn người nghèo nàn chẳng nghe ai hăm dọa tiếng nào.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.675Z",
      "updatedAt": "2024-09-07T14:34:00.675Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "4180686f-579e-42e0-80ba-22c2d17bd10a",
      "number": 9,
      "content": "Ánh sáng người ngay lành luôn tỏa soi rực rỡ,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.678Z",
      "updatedAt": "2024-09-07T14:34:00.678Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "a1d77dbf-cf10-41bf-ab6d-ab22cbd515e6",
      "number": 9,
      "content": "Nhưng đèn kẻ ác gian bị dập tắt mịt mờ.",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.678Z",
      "updatedAt": "2024-09-07T14:34:00.678Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "4695f5cb-9f4f-49df-8849-de753948f636",
      "number": 10,
      "content": "Sự kiêu ngạo chỉ gây nên tranh cãi;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.680Z",
      "updatedAt": "2024-09-07T14:34:00.680Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "ddac71d3-9430-4628-b430-859f55334b57",
      "number": 10,
      "content": "Người khôn ngoan biết ghi nhận lời khuyên.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.680Z",
      "updatedAt": "2024-09-07T14:34:00.680Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "f542a9ff-d11b-4d46-a173-1159a1ecbd55",
      "number": 11,
      "content": "Của cải nhờ lừa dối tinh vi để giàu mau sẽ chóng mất;",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.682Z",
      "updatedAt": "2024-09-07T14:34:00.682Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "29e08c19-bc24-4786-bfb2-261fab434a6f",
      "number": 11,
      "content": "Nhưng người chăm chỉ làm ăn sẽ giàu vững từ từ.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.682Z",
      "updatedAt": "2024-09-07T14:34:00.682Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "765f7e32-86b3-474a-8eca-947cca1b4cf5",
      "number": 12,
      "content": "Trì hoãn niềm hy vọng làm cho lòng đau đớn,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.684Z",
      "updatedAt": "2024-09-07T14:34:00.684Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "01bf5a9d-5be7-44ad-8bcd-9d22559cc49f",
      "number": 12,
      "content": "Ao ước được thành là cây sự sống xanh tươi.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.684Z",
      "updatedAt": "2024-09-07T14:34:00.684Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "6c656eb8-3afd-4c3c-a8a9-f9f1d599af3c",
      "number": 13,
      "content": "Kẻ khinh dể lời hằng sống chuốc cho mình sự hủy diệt,",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.686Z",
      "updatedAt": "2024-09-07T14:34:00.686Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "4af770f2-b98f-416b-b9f6-e8d74db93180",
      "number": 13,
      "content": "Người quý trọng điều răn chắc sẽ được thưởng ban.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.686Z",
      "updatedAt": "2024-09-07T14:34:00.686Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "50d0e96b-9a35-4e92-a3ba-e80566329a54",
      "number": 14,
      "content": "Sự dạy dỗ của người khôn ngoan chính là nguồn sự sống,",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.687Z",
      "updatedAt": "2024-09-07T14:34:00.687Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "b2403f5c-6b27-40dc-b17e-822fb9d18342",
      "number": 14,
      "content": "Kẻ vâng theo sẽ không sa vào bẫy tử vong.",
      "order": 1,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.687Z",
      "updatedAt": "2024-09-07T14:34:00.687Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "4c3aaca8-8afa-45b9-80c6-b903d39d34aa",
      "number": 15,
      "content": "Nhận định sáng suốt luôn mang về ân huệ,",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.690Z",
      "updatedAt": "2024-09-07T14:34:00.690Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "e30b7074-fb20-435c-8a6d-0e4564d90fde",
      "number": 15,
      "content": "Còn đường kẻ bất trung gặp khốn đốn rất nhiều.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.690Z",
      "updatedAt": "2024-09-07T14:34:00.690Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "daeb2434-bf04-43d8-be53-648138e002dd",
      "number": 16,
      "content": "Người khôn ngoan nắm vững tin tức trước khi hành động,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.692Z",
      "updatedAt": "2024-09-07T14:34:00.692Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "93cb00aa-a876-427c-a159-fd6927f6275a",
      "number": 16,
      "content": "Kẻ dại khờ hấp tấp phô bày sự dại dột mình ra.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.692Z",
      "updatedAt": "2024-09-07T14:34:00.692Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "88baceb4-2968-4c2e-8efe-efdaad57028f",
      "number": 17,
      "content": "Sứ giả gian hùng gây thêm điều rắc rối,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.694Z",
      "updatedAt": "2024-09-07T14:34:00.694Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "b8148020-cb52-4da2-baf8-b421e5651010",
      "number": 17,
      "content": "Sứ giả trung thành hóa giải những khó khăn.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.694Z",
      "updatedAt": "2024-09-07T14:34:00.694Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "698da479-5afe-4ab9-a4ae-0b4e5aa7fa21",
      "number": 18,
      "content": "Khinh dể lời khuyên dạy chuốc lấy bần cùng và nhục nhã,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.696Z",
      "updatedAt": "2024-09-07T14:34:00.696Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "a3169679-6599-4dae-9742-d68c329c868a",
      "number": 18,
      "content": "Nhưng ai chịu nghe lời quở trách sẽ được tôn trọng vinh danh.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.696Z",
      "updatedAt": "2024-09-07T14:34:00.696Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "89d3577f-cd4a-4168-aafd-94bd250defb0",
      "number": 19,
      "content": "Mơ ước được thành làm tâm hồn khoan khoái;",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.698Z",
      "updatedAt": "2024-09-07T14:34:00.698Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "79ac055a-6806-4c6e-b9c4-e168486321f5",
      "number": 19,
      "content": "Lìa bỏ gian tà là điều kẻ điên dại gớm ghê.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.698Z",
      "updatedAt": "2024-09-07T14:34:00.698Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "85c64e3b-36b3-4383-a0a5-169a682737d0",
      "number": 20,
      "content": "Ai giao thiệp với người khôn ngoan sẽ học được khôn ngoan,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.700Z",
      "updatedAt": "2024-09-07T14:34:00.700Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "a56bf246-b0e5-4b33-a0eb-a9578b9105fc",
      "number": 20,
      "content": "Ai làm bạn với kẻ điên dại sẽ có ngày mang họa.",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.700Z",
      "updatedAt": "2024-09-07T14:34:00.700Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "9f23f0f1-fd27-4ba1-adb3-ce30b54b7ace",
      "number": 21,
      "content": "Nỗi bất hạnh luôn đuổi theo kẻ tội lỗi,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.705Z",
      "updatedAt": "2024-09-07T14:34:00.705Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "623e8bfe-c117-4fab-8bcb-2aa42bf3b038",
      "number": 21,
      "content": "Sự phồn vinh là phần thưởng của người ngay lành.",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.705Z",
      "updatedAt": "2024-09-07T14:34:00.705Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "9c63bd5a-eafe-456f-bd30-f00bc9b35445",
      "number": 22,
      "content": "Người tốt để gia tài lại cho con cháu,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.711Z",
      "updatedAt": "2024-09-07T14:34:00.711Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "85651c1a-0e07-40cc-80a7-5d708193fd0f",
      "number": 22,
      "content": "Nhưng tội nhân tích trữ của cải để lại cho người ngay lành.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.711Z",
      "updatedAt": "2024-09-07T14:34:00.711Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "cf0a1e70-1024-48b6-a2c2-f854fe467dfc",
      "number": 23,
      "content": "Ruộng của người nghèo trúng mùa và mang lại nhiều hoa lợi,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.716Z",
      "updatedAt": "2024-09-07T14:34:00.716Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "8daa6c1a-6999-4b86-9dba-a245b3018467",
      "number": 23,
      "content": "Nhưng sự bất công cướp sạch những hoa lợi đó đi.",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.716Z",
      "updatedAt": "2024-09-07T14:34:00.716Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "f7e7a8d8-39bd-4d7d-bc49-75cb7c45f04e",
      "number": 24,
      "content": "Ai kiêng cữ roi vọt là không thương con cái,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.721Z",
      "updatedAt": "2024-09-07T14:34:00.721Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "b358164b-7388-4974-8dae-77980090a286",
      "number": 24,
      "content": "Người thương con chăm chỉ sửa trị con mình.",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.721Z",
      "updatedAt": "2024-09-07T14:34:00.721Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "5b18d68e-443c-4f42-9eca-b094754485bf",
      "number": 25,
      "content": "Người ngay lành có đầy đủ thức ăn thức uống,",
      "order": 0,
      "parNumber": 48,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.726Z",
      "updatedAt": "2024-09-07T14:34:00.726Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    },
    {
      "id": "7f970ba1-8e6c-42d8-9915-0a7c2233b513",
      "number": 25,
      "content": "Kẻ ác gian bụng cứ đói triền miên.",
      "order": 1,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:34:00.726Z",
      "updatedAt": "2024-09-07T14:34:00.726Z",
      "chapterId": "d8a6be2a-5c4b-4c5e-bd3a-374f20c1f3ba"
    }
  ],
