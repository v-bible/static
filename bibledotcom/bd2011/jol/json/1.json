{
  "chapter": {
    "id": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a",
    "number": 1,
    "ref": "https://www.bible.com/bible/19/JOL.1.BD2011",
    "createdAt": "2024-09-07T11:51:12.999Z",
    "updatedAt": "2024-09-07T11:51:12.999Z",
    "bookId": "f1919a27-2955-4535-a0af-530364ae9784"
  },
  "verses": [
    {
      "id": "92bb53fb-3947-4edf-b7ed-3de170120859",
      "number": 1,
      "content": "Lời của CHÚA phán với Giô-ên con trai Pê-thu-ên:",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T15:21:09.728Z",
      "updatedAt": "2024-09-07T15:21:09.728Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "0cde4d81-3323-4381-8bfc-42155f880e76",
      "number": 2,
      "content": "Hỡi các trưởng lão, hãy lắng nghe những điều này,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.738Z",
      "updatedAt": "2024-09-07T15:21:09.738Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "3a757e1c-e321-46d3-83e8-3b7a5b5c2e8a",
      "number": 2,
      "content": "Hỡi tất cả dân cư trong xứ, hãy lắng tai nghe.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.738Z",
      "updatedAt": "2024-09-07T15:21:09.738Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "a88bd6c0-d6c2-42ce-ae59-41cc19562bc7",
      "number": 2,
      "content": "Có bao giờ việc như thế này đã xảy ra trong thời của các ngươi,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.738Z",
      "updatedAt": "2024-09-07T15:21:09.738Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "6eff1c7d-5c2e-4f33-b117-fe569a24776c",
      "number": 2,
      "content": "Hoặc trong thời của tổ tiên các ngươi chăng?",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.738Z",
      "updatedAt": "2024-09-07T15:21:09.738Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "a66a2917-2776-46b7-92a0-7765149ca4a6",
      "number": 3,
      "content": "Hãy kể lại cho con cái các ngươi để chúng biết,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.741Z",
      "updatedAt": "2024-09-07T15:21:09.741Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "e69ad491-ac66-4d2a-908a-99d98cb9b064",
      "number": 3,
      "content": "Hầu con cái các ngươi sẽ kể lại cho con cái của chúng,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.741Z",
      "updatedAt": "2024-09-07T15:21:09.741Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "59c3b46f-4b18-49ad-9534-a2768ad140ff",
      "number": 3,
      "content": "Con cái của chúng sẽ kể lại cho thế hệ tiếp theo:",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.741Z",
      "updatedAt": "2024-09-07T15:21:09.741Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "04ae3905-acf9-4048-8334-c8bead7cb681",
      "number": 4,
      "content": "Vật gì cào cào đầu nhỏ bỏ lại,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.743Z",
      "updatedAt": "2024-09-07T15:21:09.743Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "a61bf916-dceb-44c7-84dc-faf1aa56b4ec",
      "number": 4,
      "content": "Cào cào đầu lớn ăn.",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.743Z",
      "updatedAt": "2024-09-07T15:21:09.743Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "b42f2675-034c-48c8-a683-6faf6111c7c7",
      "number": 4,
      "content": "Vật gì cào cào đầu lớn bỏ lại,",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.743Z",
      "updatedAt": "2024-09-07T15:21:09.743Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "d52afe6b-b897-4051-a722-9c52816cc4af",
      "number": 4,
      "content": "Cào cào cánh ngắn ăn.",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.743Z",
      "updatedAt": "2024-09-07T15:21:09.743Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "e142c5d6-add9-4ef8-bc40-8a0c599a911a",
      "number": 4,
      "content": "Vật gì cào cào cánh ngắn bỏ lại,",
      "order": 4,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.743Z",
      "updatedAt": "2024-09-07T15:21:09.743Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "c3b5d52f-2fd4-4c28-a905-cc77a03e6f87",
      "number": 4,
      "content": "Cào cào cánh dài ăn.",
      "order": 5,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.743Z",
      "updatedAt": "2024-09-07T15:21:09.743Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "91f158f8-87c6-46e7-9c5b-44af1add289a",
      "number": 5,
      "content": "Hỡi những kẻ say sưa, hãy tỉnh dậy và khóc lóc;",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.746Z",
      "updatedAt": "2024-09-07T15:21:09.746Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "b1a0918b-a917-40cd-adc5-1bf042479776",
      "number": 5,
      "content": "Hỡi mọi kẻ ghiền rượu, hãy mếu máo than van,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.746Z",
      "updatedAt": "2024-09-07T15:21:09.746Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "bdf25863-130e-4a6d-b86b-074b6747ef53",
      "number": 5,
      "content": "Vì rượu mới chưa kịp uống",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.746Z",
      "updatedAt": "2024-09-07T15:21:09.746Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "9447cc86-d739-4012-9989-bc0d24b40529",
      "number": 5,
      "content": "Đã bị cướp mất;",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.746Z",
      "updatedAt": "2024-09-07T15:21:09.746Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "d3f66b8f-66aa-4c0d-8a06-56b415f98e83",
      "number": 6,
      "content": "Vì một dân hùng mạnh và đông vô số",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.752Z",
      "updatedAt": "2024-09-07T15:21:09.752Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "654b7903-4ad3-47db-84bd-c01f4cd87f32",
      "number": 6,
      "content": "Đã xâm lăng xứ sở chúng ta;",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.752Z",
      "updatedAt": "2024-09-07T15:21:09.752Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "35ac96d7-8eae-41a2-aaa5-56144f3698a8",
      "number": 6,
      "content": "Răng của nó như răng sư tử;",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.752Z",
      "updatedAt": "2024-09-07T15:21:09.752Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "c74fcc81-540a-4a8a-a335-3209bcec500f",
      "number": 6,
      "content": "Nanh của nó như nanh sư tử cái.",
      "order": 3,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.752Z",
      "updatedAt": "2024-09-07T15:21:09.752Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "8dc6646b-2528-42d7-b553-ad821bc12c8c",
      "number": 7,
      "content": "Nó đã phá tan hoang cây nho của chúng ta;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.757Z",
      "updatedAt": "2024-09-07T15:21:09.757Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "e73770d5-5298-459f-bd99-c5781aef12fa",
      "number": 7,
      "content": "Nó đã biến cây vả của chúng ta thành đống củi lộn.",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.757Z",
      "updatedAt": "2024-09-07T15:21:09.757Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "356621ff-35f3-4c34-8269-3d1b46689bc0",
      "number": 7,
      "content": "Nó tước sạch vỏ rồi quăng xuống;",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.757Z",
      "updatedAt": "2024-09-07T15:21:09.757Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "41f904d8-19c5-46d0-a5d6-d59e69c5f86f",
      "number": 7,
      "content": "Nó lột sạch các cành, khiến chúng trắng hếu trụi trơ.",
      "order": 3,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.757Z",
      "updatedAt": "2024-09-07T15:21:09.757Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "1748fb49-7127-4d3e-a25c-c33a227e8ae1",
      "number": 8,
      "content": "Hãy than khóc như người trinh nữ trẻ mặc tang phục",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.762Z",
      "updatedAt": "2024-09-07T15:21:09.762Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "0b51f87c-29a1-4bc6-8587-c29bfe0dfd39",
      "number": 8,
      "content": "Khóc thương cho hôn phu nàng vừa mới mất.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.762Z",
      "updatedAt": "2024-09-07T15:21:09.762Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "bad4db6c-c570-4255-930d-6d2fa9122168",
      "number": 9,
      "content": "Trong nhà CHÚA,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.767Z",
      "updatedAt": "2024-09-07T15:21:09.767Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "cbf9431e-1db8-4596-a78a-3b1dab9b3fe2",
      "number": 9,
      "content": "Của lễ chay và của lễ quán không còn dâng nữa.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.767Z",
      "updatedAt": "2024-09-07T15:21:09.767Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "20d5e4d8-e4cb-4455-968a-481a54e519a5",
      "number": 9,
      "content": "Các tư tế, tức các tôi tớ CHÚA,",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.767Z",
      "updatedAt": "2024-09-07T15:21:09.767Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "120ea100-df5a-4227-b2da-6773e9d520b3",
      "number": 9,
      "content": "Buồn thảm như thể đang thọ tang.",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.767Z",
      "updatedAt": "2024-09-07T15:21:09.767Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "41451022-3e42-4fbc-b029-6012bec5d85e",
      "number": 10,
      "content": "Ruộng đồng bị bỏ hoang,",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.773Z",
      "updatedAt": "2024-09-07T15:21:09.773Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "863fa603-6560-49c2-9e8c-29d323b6ec16",
      "number": 10,
      "content": "Đất đai tiêu điều,",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.773Z",
      "updatedAt": "2024-09-07T15:21:09.773Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "872ace9c-a7e7-4565-8833-c0939fdf43ab",
      "number": 10,
      "content": "Lúa gạo hư hại,",
      "order": 2,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.773Z",
      "updatedAt": "2024-09-07T15:21:09.773Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "40860a6b-b419-4650-873d-f55348159ca9",
      "number": 10,
      "content": "Rượu mới chẳng còn,",
      "order": 3,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.773Z",
      "updatedAt": "2024-09-07T15:21:09.773Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "46a327b4-6045-422c-93a4-a87031cdf2fd",
      "number": 10,
      "content": "Dầu ô-liu khan hiếm.",
      "order": 4,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.773Z",
      "updatedAt": "2024-09-07T15:21:09.773Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "2f33743d-02a8-4756-a59c-c282fa099f76",
      "number": 11,
      "content": "Hỡi các nông phu, hãy thẹn thùng xấu hổ,",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.778Z",
      "updatedAt": "2024-09-07T15:21:09.778Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "0bdf8fc9-6bbb-4b92-821f-a1190dc607da",
      "number": 11,
      "content": "Hỡi những kẻ trồng nho, hãy khóc lóc than van,",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.778Z",
      "updatedAt": "2024-09-07T15:21:09.778Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "0cf2f6c8-d352-468b-970f-f90006f27fe5",
      "number": 11,
      "content": "Vì lúa mì và lúa mạch không còn nữa,",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.778Z",
      "updatedAt": "2024-09-07T15:21:09.778Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "f65759ca-9b55-4f90-88ce-366635a662ce",
      "number": 11,
      "content": "Bởi ruộng nương bị phá hủy cả rồi.",
      "order": 3,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.778Z",
      "updatedAt": "2024-09-07T15:21:09.778Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "656ab500-507b-403d-9134-f01e838c5376",
      "number": 12,
      "content": "Cây nho cằn cỗi,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.783Z",
      "updatedAt": "2024-09-07T15:21:09.783Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "1662622f-c423-483d-ab5d-8ead599c37b6",
      "number": 12,
      "content": "Cây vả héo khô;",
      "order": 1,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.783Z",
      "updatedAt": "2024-09-07T15:21:09.783Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "25d3daeb-090a-49aa-9d9e-6995c4a8cc34",
      "number": 12,
      "content": "Cây lựu, cây chà là, và cây táo cùng chung số phận;",
      "order": 2,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.783Z",
      "updatedAt": "2024-09-07T15:21:09.783Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "5f3dd66d-e42b-4107-a129-a32c2823b898",
      "number": 12,
      "content": "Tất cả cây cối ngoài đồng khô héo,",
      "order": 3,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.783Z",
      "updatedAt": "2024-09-07T15:21:09.783Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "1d7724a5-56f2-42cf-8f95-316dd040df5f",
      "number": 12,
      "content": "Và như thế niềm vui của con cái loài người",
      "order": 4,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.783Z",
      "updatedAt": "2024-09-07T15:21:09.783Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "64e1d87b-3d42-4d6b-8d84-7b2f16e5907b",
      "number": 12,
      "content": "Cũng khô héo theo.",
      "order": 5,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.783Z",
      "updatedAt": "2024-09-07T15:21:09.783Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "c4842372-7e5b-41af-a606-1097a6469b0b",
      "number": 13,
      "content": "Hỡi các tư tế, hãy mặc tang phục và than khóc;",
      "order": 0,
      "parNumber": 50,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.789Z",
      "updatedAt": "2024-09-07T15:21:09.789Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "4c843f36-1098-4ad5-9a1a-f8aaf8cfe3fa",
      "number": 13,
      "content": "Hỡi những người phục vụ nơi bàn thờ, hãy khóc lóc thở than;",
      "order": 1,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.789Z",
      "updatedAt": "2024-09-07T15:21:09.789Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "47530f2b-db70-4166-b2cb-bba36b23f660",
      "number": 13,
      "content": "Hỡi các tôi tớ của Đức Chúa Trời, hãy đến,",
      "order": 2,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.789Z",
      "updatedAt": "2024-09-07T15:21:09.789Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "624ce19a-b623-4fd8-989f-19b2f698e783",
      "number": 13,
      "content": "Hãy mặc vải thô và nằm than thở suốt đêm,",
      "order": 3,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.789Z",
      "updatedAt": "2024-09-07T15:21:09.789Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "ec74325d-e25f-4cf6-b86b-1ddd78a4a9bd",
      "number": 13,
      "content": "Vì nơi nhà Đức Chúa Trời",
      "order": 4,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.789Z",
      "updatedAt": "2024-09-07T15:21:09.789Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "77ed928c-38dc-43d6-abf5-7c46f17bd210",
      "number": 13,
      "content": "Của lễ chay và của lễ quán đã bị cắt.",
      "order": 5,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.789Z",
      "updatedAt": "2024-09-07T15:21:09.789Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "41dbeb64-1220-4ce2-b288-8572e020b14a",
      "number": 14,
      "content": "Hãy biệt riêng ra một cuộc kiêng ăn;",
      "order": 0,
      "parNumber": 56,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.795Z",
      "updatedAt": "2024-09-07T15:21:09.795Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "04cb80f6-5b16-4b1d-81fa-7e032caa2dcb",
      "number": 14,
      "content": "Hãy triệu tập một đại hội trọng thể;",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.795Z",
      "updatedAt": "2024-09-07T15:21:09.795Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "48d6df9a-be74-4ca9-9163-93f894ba8823",
      "number": 14,
      "content": "Hãy mời các trưởng lão",
      "order": 2,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.795Z",
      "updatedAt": "2024-09-07T15:21:09.795Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "0256c000-02b9-467f-877a-344a444b7149",
      "number": 14,
      "content": "Và toàn dân trong xứ",
      "order": 3,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.795Z",
      "updatedAt": "2024-09-07T15:21:09.795Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "035bc56c-8854-4553-a9ec-52d6a65fff12",
      "number": 14,
      "content": "Đến nhà CHÚA, Đức Chúa Trời của anh chị em,",
      "order": 4,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.795Z",
      "updatedAt": "2024-09-07T15:21:09.795Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "51f59b34-1fe3-4af0-b2a8-90067c62694d",
      "number": 14,
      "content": "Rồi hãy cùng nhau kêu cầu CHÚA.",
      "order": 5,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.795Z",
      "updatedAt": "2024-09-07T15:21:09.795Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "fe607238-f346-4342-8b88-886e316529f3",
      "number": 15,
      "content": "Than ôi, ngày ấy đang đến!",
      "order": 0,
      "parNumber": 62,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.806Z",
      "updatedAt": "2024-09-07T15:21:09.806Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "cbcc5c1d-5031-4f36-9343-69c50ac92757",
      "number": 15,
      "content": "Ngày của CHÚA đang đến gần;",
      "order": 1,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.806Z",
      "updatedAt": "2024-09-07T15:21:09.806Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "e108dfbb-a101-4ced-83ce-6acf4fcc1cba",
      "number": 15,
      "content": "Nó đến như một cơn hủy diệt từ Đấng Toàn Năng.",
      "order": 2,
      "parNumber": 64,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.806Z",
      "updatedAt": "2024-09-07T15:21:09.806Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "f6aa5ad7-8f7f-4192-8831-667011210603",
      "number": 16,
      "content": "Há chẳng phải thực phẩm",
      "order": 0,
      "parNumber": 65,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.812Z",
      "updatedAt": "2024-09-07T15:21:09.812Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "d25afb06-40e5-4fce-8dbd-d3f1d758d97f",
      "number": 16,
      "content": "Bị cắt trước mắt chúng ta sao?",
      "order": 1,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.812Z",
      "updatedAt": "2024-09-07T15:21:09.812Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "14b4a4ff-f2cf-4810-a25c-ca0630cfe5fd",
      "number": 16,
      "content": "Sự hân hoan và vui vẻ",
      "order": 2,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.812Z",
      "updatedAt": "2024-09-07T15:21:09.812Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "c527ee51-8b05-4beb-a5b1-c308315eaaf0",
      "number": 16,
      "content": "Cũng chẳng còn thấy nơi nhà Đức Chúa Trời chúng ta sao?",
      "order": 3,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.812Z",
      "updatedAt": "2024-09-07T15:21:09.812Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "394ce176-c0f8-404c-baff-853909bb3d6a",
      "number": 17,
      "content": "Hạt giống chết khô dưới lớp đất,",
      "order": 0,
      "parNumber": 69,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.818Z",
      "updatedAt": "2024-09-07T15:21:09.818Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "e26f278f-abf3-4cff-bcd8-ef062aeb0bb2",
      "number": 17,
      "content": "Các vựa lúa trở thành nhà hoang,",
      "order": 1,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.818Z",
      "updatedAt": "2024-09-07T15:21:09.818Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "4edd3749-9980-4854-a6b7-d3a783eb87cf",
      "number": 17,
      "content": "Các kho gạo trống không đổ nát,",
      "order": 2,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.818Z",
      "updatedAt": "2024-09-07T15:21:09.818Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "12e0bc27-f923-4a08-9da1-479514d06a4b",
      "number": 17,
      "content": "Bởi vì chẳng còn lúa gạo.",
      "order": 3,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.818Z",
      "updatedAt": "2024-09-07T15:21:09.818Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "c893af37-bbcb-42aa-8021-6f6343fd0ff1",
      "number": 18,
      "content": "Súc vật rên rỉ thảm thương,",
      "order": 0,
      "parNumber": 73,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.825Z",
      "updatedAt": "2024-09-07T15:21:09.825Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "e9ad52b4-8d25-47f5-ae4c-40170cea96ee",
      "number": 18,
      "content": "Trâu bò lang thang đói khát,",
      "order": 1,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.825Z",
      "updatedAt": "2024-09-07T15:21:09.825Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "c439fac9-78d8-4106-97ec-b9a6b9d24940",
      "number": 18,
      "content": "Vì chẳng có đồng cỏ cho chúng;",
      "order": 2,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.825Z",
      "updatedAt": "2024-09-07T15:21:09.825Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "26992638-455e-400f-bf9c-bc351091e771",
      "number": 18,
      "content": "Ngay cả các đàn chiên cũng cùng chung số phận.",
      "order": 3,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.825Z",
      "updatedAt": "2024-09-07T15:21:09.825Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "f24379e8-1982-45c0-957d-7fa6b9087333",
      "number": 19,
      "content": "Lạy CHÚA, con cầu xin Ngài,",
      "order": 0,
      "parNumber": 77,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.831Z",
      "updatedAt": "2024-09-07T15:21:09.831Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "5bc9ae3d-a49d-4311-b1d2-4255313375c6",
      "number": 19,
      "content": "Vì lửa đã thiêu rụi",
      "order": 1,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.831Z",
      "updatedAt": "2024-09-07T15:21:09.831Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "ba96d9d5-d30f-42e9-9a2d-b353f790539a",
      "number": 19,
      "content": "Những đồng cỏ trong đồng hoang,",
      "order": 2,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.831Z",
      "updatedAt": "2024-09-07T15:21:09.831Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "363edc14-1334-4016-a5bc-e34b9539d845",
      "number": 19,
      "content": "Và ngọn lửa đã đốt cháy",
      "order": 3,
      "parNumber": 80,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.831Z",
      "updatedAt": "2024-09-07T15:21:09.831Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "8231bf1b-1808-4341-9e62-9b3a34d13cc3",
      "number": 19,
      "content": "Mọi cây cối ngoài đồng.",
      "order": 4,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.831Z",
      "updatedAt": "2024-09-07T15:21:09.831Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "a56995a7-90d5-4bfb-ab15-5b65548fb85d",
      "number": 20,
      "content": "Ngay cả các thú hoang cũng van xin Ngài,",
      "order": 0,
      "parNumber": 82,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.837Z",
      "updatedAt": "2024-09-07T15:21:09.837Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "cc8ba703-b4be-4f09-9bbd-1694064954db",
      "number": 20,
      "content": "Vì các dòng nước đều khô cạn,",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:21:09.837Z",
      "updatedAt": "2024-09-07T15:21:09.837Z",
      "chapterId": "1086bd6d-6b9f-41ac-ac98-bd5bb3ceee7a"
    },
    {
      "id": "f651a2fc-9880-4ad8-b797-add46270ca3e",
      "number": 20,
      "content": "Và lửa đã thiêu rụi",
      "order": 2,
      "parNumber": 84,
      "parIndex": 0,
