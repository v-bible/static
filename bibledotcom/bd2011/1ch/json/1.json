{
  "chapter": {
    "id": "25cb1c40-726a-4116-9385-afd0b4cd3b33",
    "number": 1,
    "ref": "https://www.bible.com/bible/19/1CH.1.BD2011",
    "createdAt": "2024-09-07T11:51:09.489Z",
    "updatedAt": "2024-09-07T11:51:09.489Z",
    "bookId": "96fe28c6-be19-4f1c-8f48-3a0e6d65bfa4"
  },
  "verses": [
    {
      "id": "5045b1e5-4e02-4a29-acba-6e26f9adc5b5",
      "number": 1,
      "content": "A-đam, Sết, Ê-nốt,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.932Z",
      "updatedAt": "2024-09-07T13:02:42.932Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "0bf14784-b6a9-463a-b8a0-66765b7f5d06",
      "number": 2,
      "content": "Kê-nan, Ma-ha-la-lên, Gia-rết,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.942Z",
      "updatedAt": "2024-09-07T13:02:42.942Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "ab40aef5-6272-44b6-b190-d75c4a6b7a82",
      "number": 3,
      "content": "Ê-nóc, Mê-thu-sê-la, La-méc,",
      "order": 0,
      "parNumber": 1,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.944Z",
      "updatedAt": "2024-09-07T13:02:42.944Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "f10fde6d-1e36-4427-8d95-3305db212d59",
      "number": 4,
      "content": "Nô-ê; các con của Nô-ê là Sem, Cham, và Gia-phết.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.946Z",
      "updatedAt": "2024-09-07T13:02:42.946Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "8d319a3d-19a2-4d95-826b-d133c317689c",
      "number": 5,
      "content": "Dòng dõi của Gia-phết: Gô-me, Ma-gót, Ma-đai, Gia-van, Tu-banh, Mê-sếch, và Ti-ra.",
      "order": 0,
      "parNumber": 2,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.948Z",
      "updatedAt": "2024-09-07T13:02:42.948Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "095bc67b-8d03-4d78-9733-b3fad6fde902",
      "number": 6,
      "content": "Dòng dõi của Gô-me: Ách-kê-na, Đi-phát, và Tô-gạc-ma.",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.950Z",
      "updatedAt": "2024-09-07T13:02:42.950Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "7cd234d9-45ec-47f0-9ff0-4cdc9eadfd3d",
      "number": 7,
      "content": "Dòng dõi của Gia-van: Ê-li-sa, Tạt-si, Kít-tim, và Rô-đa-nim.",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.952Z",
      "updatedAt": "2024-09-07T13:02:42.952Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "5c9ba234-bbeb-4e42-b79e-a609709bd14c",
      "number": 8,
      "content": "Dòng dõi của Cham: Cút, Ai-cập, Pút, và Ca-na-an.",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.954Z",
      "updatedAt": "2024-09-07T13:02:42.954Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "b54fb1ed-9963-4d3d-8ba2-0a13dc9c3e8d",
      "number": 9,
      "content": "Dòng dõi của Cút: Sê-ba, Ha-vi-la, Sáp-ta, Ra-a-ma, và Sáp-tê-ca.",
      "order": 0,
      "parNumber": 6,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.956Z",
      "updatedAt": "2024-09-07T13:02:42.956Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "97342007-006c-4382-95a1-51d4e614a70e",
      "number": 9,
      "content": "Dòng dõi của Ra-a-ma: Shê-ba và Đê-đan.",
      "order": 1,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.956Z",
      "updatedAt": "2024-09-07T13:02:42.956Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "c6d0205f-02fb-4b19-8143-6b25e65ea7aa",
      "number": 10,
      "content": "Cút là cha của Nim-rốt; ông ấy là anh hùng đầu tiên trên mặt đất.",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.958Z",
      "updatedAt": "2024-09-07T13:02:42.958Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "8a464aec-2431-4697-b2da-8e1e68ff4753",
      "number": 11,
      "content": "Ai-cập là cha của các dân Lút, A-nam, Lê-háp, Náp-tu,",
      "order": 0,
      "parNumber": 9,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.960Z",
      "updatedAt": "2024-09-07T13:02:42.960Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "86baf8cf-f562-48ab-94f4-b80e7f429cd9",
      "number": 12,
      "content": "Pát-ru, Các-lu (từ dân ấy mà ra dân Phi-li-tin), và dân Cáp-tô.",
      "order": 0,
      "parNumber": 9,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.962Z",
      "updatedAt": "2024-09-07T13:02:42.962Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "20d6cbe1-932d-461f-896e-6e5826b93247",
      "number": 13,
      "content": "Ca-na-an là cha của Si-đôn con đầu lòng ông ấy, và Hết,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.964Z",
      "updatedAt": "2024-09-07T13:02:42.964Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "e832736e-eb20-48d9-8dc0-49b99282729b",
      "number": 14,
      "content": "dân Giê-bu-si, dân A-mô-ri, dân Ghi-ga-si,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.966Z",
      "updatedAt": "2024-09-07T13:02:42.966Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "411a897b-9720-4c76-afe9-0f048103e671",
      "number": 15,
      "content": "dân Hi-vi, dân Ạc-ki, dân Si-ni,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.968Z",
      "updatedAt": "2024-09-07T13:02:42.968Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "86eec5fb-8a84-488b-957c-5f6a40e2fa47",
      "number": 16,
      "content": "dân Ạc-va-đi, dân Xê-ma-ri, và dân Ha-ma-thi.",
      "order": 0,
      "parNumber": 10,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.970Z",
      "updatedAt": "2024-09-07T13:02:42.970Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "8f73b7fc-3015-46b1-bd6a-d573c335cc6e",
      "number": 17,
      "content": "Dòng dõi của Sem: Ê-lam, Át-sua, Ạc-pác-sát, Lút, A-ram, U-xơ, Hun, Ghê-the, và Mê-sếch.",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.973Z",
      "updatedAt": "2024-09-07T13:02:42.973Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "e390e7e4-95f8-45b8-a570-1c3939319929",
      "number": 18,
      "content": "Ạc-pác-sát là cha của Sê-la. Sê-la là cha của Ê-be.",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.977Z",
      "updatedAt": "2024-09-07T13:02:42.977Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "2fa8e01d-ccfa-44cf-af3e-5f2b8ec9763a",
      "number": 19,
      "content": "Ê-be có hai con trai. Tên của người anh là Pê-léc, vì trong thời của ông đất bị chia ra, và tên của người em là Giốc-tan.",
      "order": 0,
      "parNumber": 13,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.982Z",
      "updatedAt": "2024-09-07T13:02:42.982Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "1cd7e43c-ec0c-4551-84cc-312f440e8afc",
      "number": 20,
      "content": "Giốc-tan là cha của Anh-mô-đát, Sê-lép, Ha-xa-ma-vết, Giê-ra,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.986Z",
      "updatedAt": "2024-09-07T13:02:42.986Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "88c65397-a6c1-47cb-8dc8-6e1b88c523d2",
      "number": 21,
      "content": "Ha-đô-ram, U-xanh, Điết-la,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.990Z",
      "updatedAt": "2024-09-07T13:02:42.990Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "2e04df89-7276-48f0-84f5-f355870019fc",
      "number": 22,
      "content": "Ê-banh, A-bi-ma-ên, Sê-ba,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.995Z",
      "updatedAt": "2024-09-07T13:02:42.995Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "98e00559-aa81-4479-bd29-e9cfb709a99b",
      "number": 23,
      "content": "Ô-phia, Ha-vi-la, và Giô-báp. Tất cả những người ấy đều là con cháu của Giốc-tan.",
      "order": 0,
      "parNumber": 14,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:42.999Z",
      "updatedAt": "2024-09-07T13:02:42.999Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "fb4a839a-5dd2-4fbf-b454-fea9a03a649a",
      "number": 24,
      "content": "Sem, Ạc-pác-sát, Sê-la,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.004Z",
      "updatedAt": "2024-09-07T13:02:43.004Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "62d42d05-c850-4b75-b174-d3ff0b511aa5",
      "number": 25,
      "content": "Ê-be, Pê-léc, Rê-u,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.008Z",
      "updatedAt": "2024-09-07T13:02:43.008Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "0021afed-5f98-4a8e-b123-d978de5f839a",
      "number": 26,
      "content": "Sê-rúc, Na-hô, Tê-ra,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.016Z",
      "updatedAt": "2024-09-07T13:02:43.016Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "c2974e89-9be1-47ff-8680-2c8d909974b8",
      "number": 27,
      "content": "Áp-ram, người về sau được đổi tên thành Áp-ra-ham.",
      "order": 0,
      "parNumber": 15,
      "parIndex": 4,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.024Z",
      "updatedAt": "2024-09-07T13:02:43.024Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "527ce83a-43f2-489e-b8ab-596501123a1e",
      "number": 28,
      "content": "Các con trai của Áp-ra-ham là I-sác và Ích-ma-ên.",
      "order": 0,
      "parNumber": 17,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.029Z",
      "updatedAt": "2024-09-07T13:02:43.029Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "dd39ff2f-0806-4fbc-96c3-386250aa6973",
      "number": 29,
      "content": "Đây dòng dõi của họ: Con đầu lòng của Ích-ma-ên là Nê-ba-giốt, kế đến là Kê-đa, Át-bê-ên, Míp-sam,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.034Z",
      "updatedAt": "2024-09-07T13:02:43.034Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "0d05cec4-a2b8-4ad7-be97-01bb49de6954",
      "number": 30,
      "content": "Mích-ma, Đu-ma, Mát-sa, Ha-đát, Tê-ma,",
      "order": 0,
      "parNumber": 18,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.039Z",
      "updatedAt": "2024-09-07T13:02:43.039Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "7390d680-3738-449a-b735-07d736fc1a9d",
      "number": 31,
      "content": "Giê-tua, Na-phích, và Kê-đê-ma. Đó là các con trai của Ích-ma-ên.",
      "order": 0,
      "parNumber": 18,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.044Z",
      "updatedAt": "2024-09-07T13:02:43.044Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "6cf29295-3784-46c9-b91f-37d11a1a1dc4",
      "number": 32,
      "content": "Các con trai do bà Kê-tu-ra, vợ kế của Áp-ra-ham, sinh cho ông là Xim-ran, Giốc-san, Mê-đan, Mi-đi-an, Ích-bách, và Su-a.",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.049Z",
      "updatedAt": "2024-09-07T13:02:43.049Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "7dbbe27c-44bb-48f9-ba5e-df73cc559fdb",
      "number": 32,
      "content": "Các con trai của Giốc-san là Shê-ba và Đê-đan.",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.049Z",
      "updatedAt": "2024-09-07T13:02:43.049Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "6971174a-3d1e-43fd-a176-57e29e35906f",
      "number": 33,
      "content": "Các con trai của Mi-đi-an là Ê-pha, Ê-phe, Ha-nốc, A-bi-đa, và Ên-đa-a. Tất cả những người ấy là con cháu của bà Kê-tu-ra.",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.054Z",
      "updatedAt": "2024-09-07T13:02:43.054Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "941c28a8-0334-4ef8-b882-0493b06366ce",
      "number": 34,
      "content": "Áp-ra-ham sinh I-sác. Các con của I-sác là Ê-sau và I-sơ-ra-ên.",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.059Z",
      "updatedAt": "2024-09-07T13:02:43.059Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "c19ec400-2f5d-48e8-8fd4-c1c25092a770",
      "number": 35,
      "content": "Các con của Ê-sau là Ê-li-pha, Rê-u-ên, Giê-úc, Gia-lam, và Cô-ra.",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.063Z",
      "updatedAt": "2024-09-07T13:02:43.063Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "8cd09605-02af-4468-8c17-a0aa33d883c4",
      "number": 36,
      "content": "Các con của Ê-li-pha là Tê-man, Ô-ma, Xê-phô, Ga-tam, Kê-na, Tim-na, và A-ma-léc.",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.068Z",
      "updatedAt": "2024-09-07T13:02:43.068Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "50246a61-366e-40a1-a974-5fdd6942c967",
      "number": 37,
      "content": "Các con của Rê-u-ên là Na-hát, Xê-ra, Sam-ma, và Mít-xa.",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.073Z",
      "updatedAt": "2024-09-07T13:02:43.073Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "f4285af8-59a9-4cae-8dbb-566f62946c11",
      "number": 38,
      "content": "Các con của Sê-i-rơ là Lô-tan, Sô-banh, Xi-bê-ôn, A-na, Đi-sôn, Ê-xe, và Đi-san.",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.078Z",
      "updatedAt": "2024-09-07T13:02:43.078Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "f4dc84a3-52fa-4531-b8cb-a7ffcde9831f",
      "number": 39,
      "content": "Các con của Lô-tan là Hô-ri và Hô-mam. Em gái của Lô-tan là Tim-na.",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.083Z",
      "updatedAt": "2024-09-07T13:02:43.083Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "45d994a5-d010-4af4-a10e-7ee3d8c68a33",
      "number": 40,
      "content": "Các con của Sô-banh là A-li-an, Ma-na-hát, Ê-banh, Sê-phi, và Ô-nam.",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.088Z",
      "updatedAt": "2024-09-07T13:02:43.088Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "f478be66-1efe-45ec-b090-2c4b1d1f206b",
      "number": 40,
      "content": "Các con của Xi-bê-ôn là Ai-a và A-na.",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.088Z",
      "updatedAt": "2024-09-07T13:02:43.088Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "6e7a09b7-2333-44cf-8a15-f1b138cf4497",
      "number": 41,
      "content": "Con của A-na là Đi-sôn.",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.093Z",
      "updatedAt": "2024-09-07T13:02:43.093Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "8c4c5084-7006-4ddb-85bc-1a194b5ed5ea",
      "number": 41,
      "content": "Các con của Đi-sôn là Ham-ran, Ếch-ban, Ít-ran, và Kê-ran.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.093Z",
      "updatedAt": "2024-09-07T13:02:43.093Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "a28bcb22-00f9-46ff-a12c-c3e71d92ddfa",
      "number": 42,
      "content": "Các con của Ê-xe là Bin-han, Xa-a-van, và Gia-a-can.",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.098Z",
      "updatedAt": "2024-09-07T13:02:43.098Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "ede97083-3085-405a-9cd5-90dac659dc25",
      "number": 42,
      "content": "Các con của Đi-san là U-xơ và A-ran.",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.098Z",
      "updatedAt": "2024-09-07T13:02:43.098Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "74779750-a6b8-4f3e-aed0-e585de3c566f",
      "number": 43,
      "content": "Đây là tên các vua đã trị vì trong đất Ê-đôm trước khi có một vua nào trị vì trên dân I-sơ-ra-ên:",
      "order": 0,
      "parNumber": 34,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.103Z",
      "updatedAt": "2024-09-07T13:02:43.103Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "9e1e3c6d-98ca-4c9f-8f75-2884ca9b92df",
      "number": 43,
      "content": "Bê-la con của Bê-ô; tên của kinh đô vua ấy là Đin-ha-ba.",
      "order": 1,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.103Z",
      "updatedAt": "2024-09-07T13:02:43.103Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "ea6edccb-31d7-4dec-a7a5-87c8d8fd09e0",
      "number": 44,
      "content": "Sau khi Bê-la băng hà, Giô-báp con trai Xê-ra ở Bô-ra thế vị.",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.109Z",
      "updatedAt": "2024-09-07T13:02:43.109Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "e708d258-ba54-4308-9db9-32a3aaff2d70",
      "number": 45,
      "content": "Sau khi Giô-báp băng hà, Hu-sam quê ở vùng Tê-man thế vị.",
      "order": 0,
      "parNumber": 37,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.113Z",
      "updatedAt": "2024-09-07T13:02:43.113Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "72c76d08-9b97-4b96-b71b-ed0281102202",
      "number": 46,
      "content": "Sau khi Hu-sam băng hà, Ha-đát con của Bê-đát, người đã đánh bại dân Mi-đi-an trong đồng bằng Mô-áp, thế vị; tên của kinh đô vua ấy là A-vít.",
      "order": 0,
      "parNumber": 38,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.118Z",
      "updatedAt": "2024-09-07T13:02:43.118Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "7c36717d-c1bf-40b2-b9a4-cb357ef3023a",
      "number": 47,
      "content": "Sau khi Ha-đát băng hà, Sam-la con của Mách-rê-ca thế vị.",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.123Z",
      "updatedAt": "2024-09-07T13:02:43.123Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "5f39f7b9-2426-4389-9137-85ee83293f3e",
      "number": 48,
      "content": "Sau khi Sam-la băng hà, Sa-un quê ở Rê-hô-bốt, một địa danh nằm bên bờ sông Ơ-phơ-rát, thế vị.",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.126Z",
      "updatedAt": "2024-09-07T13:02:43.126Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "8337108b-95af-4271-b564-51fcd3ed333d",
      "number": 49,
      "content": "Sau khi Sa-un băng hà, Ba-anh Ha-nan con của Ách-bơ thế vị.",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.130Z",
      "updatedAt": "2024-09-07T13:02:43.130Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "3c76f2a6-1ccf-4ea0-ad25-6146b706a09a",
      "number": 50,
      "content": "Sau khi Ba-anh Ha-nan băng hà, Ha-đát thế vị; tên của kinh đô vua ấy là Pa-i. Vợ của vua ấy tên là Mê-hê-ta-bên. Bà ấy là ái nữ của Mát-rết, ái nữ của Mê Xa-háp.",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.133Z",
      "updatedAt": "2024-09-07T13:02:43.133Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "3b680ac1-46d9-4ba1-ace4-83a33ffc22a1",
      "number": 51,
      "content": "Sau đó Ha-đát băng hà.",
      "order": 0,
      "parNumber": 42,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.135Z",
      "updatedAt": "2024-09-07T13:02:43.135Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "070ed8e6-16cc-494d-99f7-b9dea940407b",
      "number": 51,
      "content": "Các tộc trưởng của dân Ê-đôm là tộc trưởng Tim-na, tộc trưởng A-li-a, tộc trưởng Giê-thết,",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.135Z",
      "updatedAt": "2024-09-07T13:02:43.135Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "0e54a8b8-cca6-4a22-9767-f21191e8b65d",
      "number": 52,
      "content": "tộc trưởng Ô-hô-li-ba-ma, tộc trưởng Ê-la, tộc trưởng Pi-nôn,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 1,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.138Z",
      "updatedAt": "2024-09-07T13:02:43.138Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "e9430e59-7b32-4f5c-9967-e9e8207e7d7c",
      "number": 53,
      "content": "tộc trưởng Kê-na, tộc trưởng Tê-man, tộc trưởng Míp-xa,",
      "order": 0,
      "parNumber": 43,
      "parIndex": 2,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.141Z",
      "updatedAt": "2024-09-07T13:02:43.141Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    },
    {
      "id": "549a4cd7-e60a-4142-b454-69e0bddb65ac",
      "number": 54,
      "content": "tộc trưởng Mạc-đi-ên, và tộc trưởng I-ram. Đó là các tộc trưởng của dân Ê-đôm.",
      "order": 0,
      "parNumber": 43,
      "parIndex": 3,
      "isPoetry": false,
      "createdAt": "2024-09-07T13:02:43.143Z",
      "updatedAt": "2024-09-07T13:02:43.143Z",
      "chapterId": "25cb1c40-726a-4116-9385-afd0b4cd3b33"
    }
  ],
