{
  "chapter": {
    "id": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4",
    "number": 7,
    "ref": "https://www.bible.com/bible/19/SNG.7.BD2011",
    "createdAt": "2024-09-07T11:51:11.892Z",
    "updatedAt": "2024-09-07T11:51:11.892Z",
    "bookId": "5f3bfdd4-ced2-435c-9aaa-2bf5809b8601"
  },
  "verses": [
    {
      "id": "7963accf-6f30-4921-b9ea-cece8cd9b8da",
      "number": 1,
      "content": "Công nương hỡi,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.080Z",
      "updatedAt": "2024-09-07T14:40:11.080Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "7102499c-0a75-4e2c-9a11-5b2bc7bac506",
      "number": 1,
      "content": "Hai bàn chân em thật xinh đẹp trong đôi hài;",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.080Z",
      "updatedAt": "2024-09-07T14:40:11.080Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "505f2f16-158f-4380-9e53-b4c949e87921",
      "number": 1,
      "content": "Đường cong của cặp đùi em giống như đôi trụ ngọc thạch",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.080Z",
      "updatedAt": "2024-09-07T14:40:11.080Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "ef1378be-8f1a-4ae8-9cf9-d6f248e30ca2",
      "number": 1,
      "content": "Được bàn tay nghệ thuật tài khéo tạc nên.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.080Z",
      "updatedAt": "2024-09-07T14:40:11.080Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "45249852-bcf8-4feb-a5ab-0e82e80d6440",
      "number": 2,
      "content": "Rốn em tròn như chung rượu,",
      "order": 0,
      "parNumber": 4,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.090Z",
      "updatedAt": "2024-09-07T14:40:11.090Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "208cc697-d934-4a6d-9e39-9bf334acccbb",
      "number": 2,
      "content": "Không bao giờ cạn hết rượu pha.",
      "order": 1,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.090Z",
      "updatedAt": "2024-09-07T14:40:11.090Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "3619f2b4-7591-40ab-a0f8-a9efbfaff4af",
      "number": 2,
      "content": "Bụng em như một gò lúa mạch,",
      "order": 2,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.090Z",
      "updatedAt": "2024-09-07T14:40:11.090Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "63cad553-0277-4954-916d-7e4708fea153",
      "number": 2,
      "content": "Bao quanh bằng các hoa huệ.",
      "order": 3,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.090Z",
      "updatedAt": "2024-09-07T14:40:11.090Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "34b2de8d-d5f1-4ac6-af77-c2a39eb50981",
      "number": 3,
      "content": "Đôi gò bồng đảo em như một cặp linh dương con,",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.093Z",
      "updatedAt": "2024-09-07T14:40:11.093Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "4533da91-7364-467f-8948-6251e804276e",
      "number": 3,
      "content": "Cặp con sinh đôi của một linh dương mẹ.",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.093Z",
      "updatedAt": "2024-09-07T14:40:11.093Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "72b49a41-091e-4bb2-8444-279ddc8e2e11",
      "number": 4,
      "content": "Cổ em như một tháp ngà,",
      "order": 0,
      "parNumber": 10,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.095Z",
      "updatedAt": "2024-09-07T14:40:11.095Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "2e8c8090-c235-4c98-b364-08234122fca1",
      "number": 4,
      "content": "Mắt em như các hồ nước ở Hếch-bôn,",
      "order": 1,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.095Z",
      "updatedAt": "2024-09-07T14:40:11.095Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "ddb22142-d79e-45e5-a635-2363f5e32293",
      "number": 4,
      "content": "Bên Cổng Bát Ra-bim.",
      "order": 2,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.095Z",
      "updatedAt": "2024-09-07T14:40:11.095Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "46602b72-b293-491b-86d6-1d999193f5fa",
      "number": 4,
      "content": "Mũi em như Tháp Li-băng,",
      "order": 3,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.095Z",
      "updatedAt": "2024-09-07T14:40:11.095Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "82ed6eb5-38c9-4143-955a-13bce50ee717",
      "number": 4,
      "content": "Hướng nhìn về Đa-mách.",
      "order": 4,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.095Z",
      "updatedAt": "2024-09-07T14:40:11.095Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "dfc445ea-6c85-4696-b268-b3a5be5f311c",
      "number": 5,
      "content": "Đầu em trên thân hình em như Núi Cạt-mên.",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.098Z",
      "updatedAt": "2024-09-07T14:40:11.098Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "cc5480c1-3f34-4681-b921-f44e536c5778",
      "number": 5,
      "content": "Tóc em như những giải lụa hồng",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.098Z",
      "updatedAt": "2024-09-07T14:40:11.098Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "d407933a-073f-43f0-88df-b44c83e1877a",
      "number": 5,
      "content": "Mà nhà vua đã bị trói buộc vào đó.",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.098Z",
      "updatedAt": "2024-09-07T14:40:11.098Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "5c578770-2074-4128-9d56-3044433253d7",
      "number": 6,
      "content": "Em xinh đẹp quá và duyên dáng biết bao;",
      "order": 0,
      "parNumber": 18,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.100Z",
      "updatedAt": "2024-09-07T14:40:11.100Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "2f06c59d-4e2e-4e41-8bc1-92c8b5559605",
      "number": 6,
      "content": "Người yêu hỡi, em quả là tuyệt đẹp.",
      "order": 1,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.100Z",
      "updatedAt": "2024-09-07T14:40:11.100Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "e8d36af1-f986-4866-b889-921b0bf3349f",
      "number": 7,
      "content": "Dáng em như một cây chà là,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.103Z",
      "updatedAt": "2024-09-07T14:40:11.103Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "b58e39c2-752f-4aec-85bc-e06faddc2fdd",
      "number": 7,
      "content": "Đôi gò bồng đảo em như một chùm trái ngọt ngon.",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.103Z",
      "updatedAt": "2024-09-07T14:40:11.103Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "d7cd9487-05a0-48ac-b38b-f2c159be6d9d",
      "number": 8,
      "content": "Anh nói, anh sẽ trèo lên cây chà là đó,",
      "order": 0,
      "parNumber": 22,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.105Z",
      "updatedAt": "2024-09-07T14:40:11.105Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "3babda1f-37c8-4fec-877e-d410028c5097",
      "number": 8,
      "content": "Hái trái nó, và ăn.",
      "order": 1,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.105Z",
      "updatedAt": "2024-09-07T14:40:11.105Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "c9cb2a63-1c44-4e5e-a2b0-533a2f957d70",
      "number": 8,
      "content": "Ôi, ước gì đôi gò bồng đảo em như chùm nho chín mọng,",
      "order": 2,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.105Z",
      "updatedAt": "2024-09-07T14:40:11.105Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "4adf3932-18ad-4c93-9687-13447aa93053",
      "number": 8,
      "content": "Hơi thở em như những trái táo thơm ngon,",
      "order": 3,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.105Z",
      "updatedAt": "2024-09-07T14:40:11.105Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "d3651dc4-5fa3-476e-9a2b-35854037a42e",
      "number": 9,
      "content": "Và miệng em như rượu nồng hảo hạng.",
      "order": 0,
      "parNumber": 26,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.107Z",
      "updatedAt": "2024-09-07T14:40:11.107Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "e0903822-0dfd-4adf-b071-4c100af27fbb",
      "number": 9,
      "content": "Nguyện rượu ấy rót vào miệng người yêu của tôi,",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.107Z",
      "updatedAt": "2024-09-07T14:40:11.107Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "11c41ea8-dc8d-4d22-b786-0ca9a713d678",
      "number": 9,
      "content": "Chảy nhẹ nhàng qua bờ môi thiếp ngủ và lẻn vào làm tê tái chân răng.",
      "order": 2,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.107Z",
      "updatedAt": "2024-09-07T14:40:11.107Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "c8122805-8ff9-49af-b342-51dbe7432793",
      "number": 10,
      "content": "Tôi thuộc về người yêu của tôi,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.109Z",
      "updatedAt": "2024-09-07T14:40:11.109Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "792d15b7-c4ca-455e-b8c8-e68b4ed807c1",
      "number": 10,
      "content": "Sự thèm khát của chàng sẽ do tôi làm thỏa mãn.",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.109Z",
      "updatedAt": "2024-09-07T14:40:11.109Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "2308de14-e20d-44fb-99d0-c26989b84fea",
      "number": 11,
      "content": "Hãy đến, người yêu của em;",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.112Z",
      "updatedAt": "2024-09-07T14:40:11.112Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "60b65aad-4772-4e7b-91ce-d2cbeaa0bb92",
      "number": 11,
      "content": "Chúng ta hãy đi về miền quê,",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.112Z",
      "updatedAt": "2024-09-07T14:40:11.112Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "ae38572a-ea04-4fc2-9e98-9ba5839fe04d",
      "number": 11,
      "content": "Đêm nay chúng mình sẽ ngủ lại trong làng.",
      "order": 2,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.112Z",
      "updatedAt": "2024-09-07T14:40:11.112Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "d6721b44-b9c1-4797-85da-dd615d786cf4",
      "number": 12,
      "content": "Sáng mai chúng ta sẽ dậy sớm đi vào vườn nho,",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.115Z",
      "updatedAt": "2024-09-07T14:40:11.115Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "c2422733-8ef7-4e43-93e6-3cc1319b7bc1",
      "number": 12,
      "content": "Để xem các nhành nho đã đâm chồi nẩy lộc,",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.115Z",
      "updatedAt": "2024-09-07T14:40:11.115Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "6b1a0db2-f30d-4ee7-8e6f-8b7b28918a8f",
      "number": 12,
      "content": "Hoa nho đã nở,",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.115Z",
      "updatedAt": "2024-09-07T14:40:11.115Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "b7c6be72-e1ff-4206-8da1-7db7f5811d30",
      "number": 12,
      "content": "Và thạch lựu đã trổ hoa hay chưa,",
      "order": 3,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.115Z",
      "updatedAt": "2024-09-07T14:40:11.115Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "4722e6f0-ba87-4600-9dbb-0b842eb9205b",
      "number": 12,
      "content": "Rồi ở đó em sẽ dâng tặng anh tình yêu của em.",
      "order": 4,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.115Z",
      "updatedAt": "2024-09-07T14:40:11.115Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "73a27fd5-8703-4b3d-9293-d6f711f63f97",
      "number": 13,
      "content": "Trái phong già đã phảng phất mùi thơm,",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:40:11.118Z",
      "updatedAt": "2024-09-07T14:40:11.118Z",
      "chapterId": "bfbcd317-0f9e-4173-91a6-2cb348cbc4d4"
    },
    {
      "id": "26006f50-71c8-4890-8af9-48bb1bdc92b9",
      "number": 13,
      "content": "Tại các cửa đã có mọi trái ngọt ngon dành sẵn,",
      "order": 1,
      "parNumber": 41,
