{
  "chapter": {
    "id": "eda822eb-c71b-483c-a8c6-db1e07ac26bc",
    "number": 4,
    "ref": "https://www.bible.com/bible/19/SNG.4.BD2011",
    "createdAt": "2024-09-07T11:51:11.884Z",
    "updatedAt": "2024-09-07T11:51:11.884Z",
    "bookId": "5f3bfdd4-ced2-435c-9aaa-2bf5809b8601"
  },
  "verses": [
    {
      "id": "31ee322c-8f7e-4dbe-ae54-2f17474d23a4",
      "number": 1,
      "content": "Em đẹp quá, em yêu!",
      "order": 0,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.681Z",
      "updatedAt": "2024-09-07T14:39:44.681Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "bfd60057-e355-4908-9c06-52c722efdfef",
      "number": 1,
      "content": "Em đẹp quá!",
      "order": 1,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.681Z",
      "updatedAt": "2024-09-07T14:39:44.681Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "54b68b7e-1da1-43cb-867f-85626bf6153f",
      "number": 1,
      "content": "Sau màn the che mặt,",
      "order": 2,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.681Z",
      "updatedAt": "2024-09-07T14:39:44.681Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "2b95b56f-1df8-4e4e-b982-5dce45f5638d",
      "number": 1,
      "content": "Đôi mắt em như một cặp bồ câu;",
      "order": 3,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.681Z",
      "updatedAt": "2024-09-07T14:39:44.681Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "6e5acf5a-c724-4ff0-bc7e-79c334c9da2c",
      "number": 1,
      "content": "Mái tóc em bồng bềnh như một đàn sơn dương",
      "order": 4,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.681Z",
      "updatedAt": "2024-09-07T14:39:44.681Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "9a2595fa-dd90-477a-a70c-8d3ba1bf5967",
      "number": 1,
      "content": "Đang từ triền núi Ghi-lê-át đi xuống.",
      "order": 5,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.681Z",
      "updatedAt": "2024-09-07T14:39:44.681Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "8a40a233-a502-4ac9-8bc7-669290167e6d",
      "number": 2,
      "content": "Răng em trắng đẹp như một đàn chiên",
      "order": 0,
      "parNumber": 8,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.692Z",
      "updatedAt": "2024-09-07T14:39:44.692Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "cf099ec7-6e29-4232-bc4b-692f4cde694f",
      "number": 2,
      "content": "Vừa mới được hớt lông, tắm dưới suối đi lên,",
      "order": 1,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.692Z",
      "updatedAt": "2024-09-07T14:39:44.692Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "99dc9e53-933e-4bab-9d0a-39b834cba0e6",
      "number": 2,
      "content": "Tất cả đều là cặp đôi,",
      "order": 2,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.692Z",
      "updatedAt": "2024-09-07T14:39:44.692Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "63caf0f4-6f61-4367-be88-2e01fa6a7aac",
      "number": 2,
      "content": "Không chiếc nào đơn lẻ.",
      "order": 3,
      "parNumber": 11,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.692Z",
      "updatedAt": "2024-09-07T14:39:44.692Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "d416bd37-9e5a-4f96-b6b7-7e256eec9937",
      "number": 3,
      "content": "Môi em mọng đỏ như tao chỉ hồng,",
      "order": 0,
      "parNumber": 12,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.695Z",
      "updatedAt": "2024-09-07T14:39:44.695Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "74b69e3f-f17a-4d68-9e89-a4fbd86a1b4b",
      "number": 3,
      "content": "Miệng em duyên dáng thật đáng yêu.",
      "order": 1,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.695Z",
      "updatedAt": "2024-09-07T14:39:44.695Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "95caca32-d00e-4ac1-8181-af72e6fef599",
      "number": 3,
      "content": "Má em đỏ hây hây như nửa quả thạch lựu,",
      "order": 2,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.695Z",
      "updatedAt": "2024-09-07T14:39:44.695Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "9801fe8c-4c8d-4d6e-ae37-b2217c962796",
      "number": 3,
      "content": "Thấp thoáng sau màn the che mặt.",
      "order": 3,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.695Z",
      "updatedAt": "2024-09-07T14:39:44.695Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "892b1aa5-6005-4465-a00d-1fafe6df52b0",
      "number": 4,
      "content": "Cổ em xinh đẹp như Tháp Đa-vít,",
      "order": 0,
      "parNumber": 16,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.697Z",
      "updatedAt": "2024-09-07T14:39:44.697Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "cd357e7b-2d6e-499f-a2ed-ff50171496da",
      "number": 4,
      "content": "Được xây dựng để chứa binh khí,",
      "order": 1,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.697Z",
      "updatedAt": "2024-09-07T14:39:44.697Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "e36c18ce-1ad9-451d-888e-29639400b834",
      "number": 4,
      "content": "Trên đó treo một ngàn cái khiên,",
      "order": 2,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.697Z",
      "updatedAt": "2024-09-07T14:39:44.697Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "07219844-0be2-4fcf-9b45-b22a6ee2f5f4",
      "number": 4,
      "content": "Đó là khiên của những dũng sĩ.",
      "order": 3,
      "parNumber": 19,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.697Z",
      "updatedAt": "2024-09-07T14:39:44.697Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "ffb976b1-b31d-49f4-90ba-081775bb0007",
      "number": 5,
      "content": "Đôi gò bồng đảo em như một cặp linh dương con,",
      "order": 0,
      "parNumber": 20,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.700Z",
      "updatedAt": "2024-09-07T14:39:44.700Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "85e4a827-6932-4bac-becc-a4eb55de0a66",
      "number": 5,
      "content": "Cặp con sinh đôi của một linh dương mẹ,",
      "order": 1,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.700Z",
      "updatedAt": "2024-09-07T14:39:44.700Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "98e3b75d-7739-400f-a72b-71b626ce61ad",
      "number": 5,
      "content": "Đang gặm cỏ non giữa thung lũng hoa huệ.",
      "order": 2,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.700Z",
      "updatedAt": "2024-09-07T14:39:44.700Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "c21f7301-f197-4770-a7a4-80fd232252a2",
      "number": 6,
      "content": "Trước khi bình minh đến",
      "order": 0,
      "parNumber": 23,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.703Z",
      "updatedAt": "2024-09-07T14:39:44.703Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "ad4d2997-6669-4695-8fdd-c4c9b6590814",
      "number": 6,
      "content": "Và bóng tối biến đi,",
      "order": 1,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.703Z",
      "updatedAt": "2024-09-07T14:39:44.703Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "bb57cf5b-34cd-4a1e-a753-cbe6cae5be36",
      "number": 6,
      "content": "Anh sẽ đến núi mộc dược",
      "order": 2,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.703Z",
      "updatedAt": "2024-09-07T14:39:44.703Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "66f6fbb2-7b30-422f-af58-7c58590755da",
      "number": 6,
      "content": "Và đến đồi nhũ hương.",
      "order": 3,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.703Z",
      "updatedAt": "2024-09-07T14:39:44.703Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "9d5ad48f-fa7f-4c87-95a2-708f36440503",
      "number": 7,
      "content": "Em yêu ơi, toàn thân em đẹp tuyệt trần,",
      "order": 0,
      "parNumber": 27,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.705Z",
      "updatedAt": "2024-09-07T14:39:44.705Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "f2cfb8e9-3ba0-4d0d-a9c8-0990f39d9bfa",
      "number": 7,
      "content": "Không chỗ nào có thể chê được.",
      "order": 1,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.705Z",
      "updatedAt": "2024-09-07T14:39:44.705Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "31931074-9542-422d-ad44-7aef895da563",
      "number": 8,
      "content": "Hỡi hôn thê của anh, hãy cùng anh rời khỏi Li-băng,",
      "order": 0,
      "parNumber": 29,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.708Z",
      "updatedAt": "2024-09-07T14:39:44.708Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "c9f101cf-135a-468c-9452-df753e308414",
      "number": 8,
      "content": "Hãy cùng anh rời khỏi Li-băng,",
      "order": 1,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.708Z",
      "updatedAt": "2024-09-07T14:39:44.708Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "fccc1675-d0ce-46bb-b0a1-98e58607232e",
      "number": 8,
      "content": "Hãy cùng nhau ngắm cảnh bình minh từ đỉnh A-ma-na,",
      "order": 2,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.708Z",
      "updatedAt": "2024-09-07T14:39:44.708Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "91b3e1f0-afd8-4805-b794-77bf09a8842a",
      "number": 8,
      "content": "Từ đỉnh Sê-nia và từ đỉnh Hẹt-môn,",
      "order": 3,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.708Z",
      "updatedAt": "2024-09-07T14:39:44.708Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "4cc71980-4dd6-4586-a7b4-3fb5c11f1a69",
      "number": 8,
      "content": "Từ các hang sư tử",
      "order": 4,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.708Z",
      "updatedAt": "2024-09-07T14:39:44.708Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "2cbee1e3-f629-41c0-9bfa-db3f90bfe6da",
      "number": 8,
      "content": "Và từ những núi hùm beo.",
      "order": 5,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.708Z",
      "updatedAt": "2024-09-07T14:39:44.708Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "69055814-29f9-4e5e-a0d0-ddcb0ab66340",
      "number": 9,
      "content": "Em đã chiếm đoạt tim anh rồi, hỡi em gái của anh, hôn thê của anh;",
      "order": 0,
      "parNumber": 35,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.710Z",
      "updatedAt": "2024-09-07T14:39:44.710Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "b5bf0510-6166-4171-95e7-822826dd8b01",
      "number": 9,
      "content": "Em đã cướp mất hồn anh bằng một cái liếc mắt của em,",
      "order": 1,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.710Z",
      "updatedAt": "2024-09-07T14:39:44.710Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "963b4ad6-c8ce-4e77-876f-a78535585a54",
      "number": 9,
      "content": "Bằng một dây chuyền đeo cổ của em.",
      "order": 2,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.710Z",
      "updatedAt": "2024-09-07T14:39:44.710Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "0a7b797a-c58f-4d92-afc5-587492ff4ed5",
      "number": 10,
      "content": "Tình yêu của em thật tuyệt vời, hỡi em gái của anh, hôn thê của anh,",
      "order": 0,
      "parNumber": 38,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.713Z",
      "updatedAt": "2024-09-07T14:39:44.713Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "db5eed0d-eb29-4415-af67-2a0b60c9b940",
      "number": 10,
      "content": "Tình yêu của em thật nồng nàn hơn rượu;",
      "order": 1,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.713Z",
      "updatedAt": "2024-09-07T14:39:44.713Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "dc14e4a2-c0a2-443e-8804-d89d6b366b69",
      "number": 10,
      "content": "Mùi nước hoa từ người em làm sảng khoái hơn mọi thứ hương thơm.",
      "order": 2,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.713Z",
      "updatedAt": "2024-09-07T14:39:44.713Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "8650672e-61bf-4ccf-84d9-3e9d1ca822a6",
      "number": 11,
      "content": "Môi em ngọt lịm như mật nhểu, hỡi hôn thê của anh;",
      "order": 0,
      "parNumber": 41,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.715Z",
      "updatedAt": "2024-09-07T14:39:44.715Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "c9f55b75-ce5e-4495-acc0-737050ca5694",
      "number": 11,
      "content": "Dưới lưỡi em là mật ngọt và sữa ngon.",
      "order": 1,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.715Z",
      "updatedAt": "2024-09-07T14:39:44.715Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "6755ef16-4fa9-47e2-98b8-a26a36f60098",
      "number": 11,
      "content": "Mùi thơm từ y phục em tỏa ra như mùi thơm trên Núi Li-băng.",
      "order": 2,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.715Z",
      "updatedAt": "2024-09-07T14:39:44.715Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "03897bdc-d4ef-4f5d-882d-26f317719c03",
      "number": 12,
      "content": "Em là mảnh vườn bị khóa kín, hỡi em gái của anh, hôn thê của anh,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.718Z",
      "updatedAt": "2024-09-07T14:39:44.718Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "4535d0ab-8708-467f-9701-b403e972db46",
      "number": 12,
      "content": "Em là suối nước bị rào ngăn, là mạch nước bị niêm phong, chưa hề có ai uống.",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.718Z",
      "updatedAt": "2024-09-07T14:39:44.718Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "eec7c086-0278-42ff-9ebb-9dbb66884c70",
      "number": 13,
      "content": "Vườn cây của em là một vườn đầy thạch lựu,",
      "order": 0,
      "parNumber": 46,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.720Z",
      "updatedAt": "2024-09-07T14:39:44.720Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "0cba7110-775f-42a9-9170-4adbfec09dc2",
      "number": 13,
      "content": "Đầy những hoa thơm trái ngọt,",
      "order": 1,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.720Z",
      "updatedAt": "2024-09-07T14:39:44.720Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "4e11cb42-739f-49a0-b711-0856116e07b7",
      "number": 13,
      "content": "Nào phụng tiên và cam tùng,",
      "order": 2,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.720Z",
      "updatedAt": "2024-09-07T14:39:44.720Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "23fe7059-a534-44f7-b7ed-6cb8add2e910",
      "number": 14,
      "content": "Cam tùng và hoàng khương, đinh hương và nhục quế,",
      "order": 0,
      "parNumber": 49,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.722Z",
      "updatedAt": "2024-09-07T14:39:44.722Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "2c3ef7f8-5aa8-4593-a345-e6df9e6db493",
      "number": 14,
      "content": "Cùng mọi cây nhũ hương,",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.722Z",
      "updatedAt": "2024-09-07T14:39:44.722Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "88042b30-d764-4746-b471-ac72bf14d5df",
      "number": 14,
      "content": "Mộc dược và trầm hương,",
      "order": 2,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.722Z",
      "updatedAt": "2024-09-07T14:39:44.722Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "92e5a4c2-0ff8-4863-b4a6-21e987d7e0de",
      "number": 14,
      "content": "Với tất cả những cây sản xuất các hương liệu thượng hạng.",
      "order": 3,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.722Z",
      "updatedAt": "2024-09-07T14:39:44.722Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "37424f9d-5941-49a7-b827-e46569fe0558",
      "number": 15,
      "content": "Em là suối nước trong vườn,",
      "order": 0,
      "parNumber": 53,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.725Z",
      "updatedAt": "2024-09-07T14:39:44.725Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "f94798d2-32cf-49a8-b315-cea670626712",
      "number": 15,
      "content": "Là giếng nước trào tuôn, tuôn dòng từ Li-băng chảy xuống.",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.725Z",
      "updatedAt": "2024-09-07T14:39:44.725Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "0ecf6988-6e61-4e2c-a0e0-83b9b95900b0",
      "number": 16,
      "content": "Hãy nổi dậy, hỡi gió từ phương bắc.",
      "order": 0,
      "parNumber": 56,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.727Z",
      "updatedAt": "2024-09-07T14:39:44.727Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "d4c9dd45-4328-49d2-8d71-bccaa76c508e",
      "number": 16,
      "content": "Hãy đến đây, hỡi gió từ phương nam.",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.727Z",
      "updatedAt": "2024-09-07T14:39:44.727Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "ec89ad24-639c-47ce-9d60-831f59795ab1",
      "number": 16,
      "content": "Hãy thổi trên mảnh vườn của ta,",
      "order": 2,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.727Z",
      "updatedAt": "2024-09-07T14:39:44.727Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "cb49ad59-a82c-4803-ad7c-fd4ebf65d2bb",
      "number": 16,
      "content": "Để hương thơm của nó tỏa lan ra ngoài;",
      "order": 3,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.727Z",
      "updatedAt": "2024-09-07T14:39:44.727Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "28da1770-3ebf-437c-ac82-2f776c0ff565",
      "number": 16,
      "content": "Để người yêu của ta đến trong vườn của chàng,",
      "order": 4,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.727Z",
      "updatedAt": "2024-09-07T14:39:44.727Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    },
    {
      "id": "fc6911ed-8ee3-48f2-add1-8f5c715f61e2",
      "number": 16,
      "content": "Hầu thưởng thức những trái ngọt ngon của nó.",
      "order": 5,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T14:39:44.727Z",
      "updatedAt": "2024-09-07T14:39:44.727Z",
      "chapterId": "eda822eb-c71b-483c-a8c6-db1e07ac26bc"
    }
  ],
