{
  "chapter": {
    "id": "7c8f6521-2d01-48f3-94e3-d2f841cb9503",
    "number": 5,
    "ref": "https://www.bible.com/bible/19/MIC.5.BD2011",
    "createdAt": "2024-09-07T11:51:13.099Z",
    "updatedAt": "2024-09-07T11:51:13.099Z",
    "bookId": "fbf55ec8-2a11-46ea-9615-38b4d320953c"
  },
  "verses": [
    {
      "id": "ee24fca6-832b-449b-987d-4c1841aa8304",
      "number": 1,
      "content": "Bây giờ, hỡi Ái Nữ của quân đội, hãy tập họp lại thành đội ngũ,",
      "order": 0,
      "parNumber": 0,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.903Z",
      "updatedAt": "2024-09-07T15:25:01.903Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "6fa13c34-59c3-4b7a-ae46-0eef4617af3c",
      "number": 1,
      "content": "Quân thù đang vây hãm chúng ta,",
      "order": 1,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.903Z",
      "updatedAt": "2024-09-07T15:25:01.903Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "553fd808-51e3-4019-ab9f-d0b426e575dc",
      "number": 1,
      "content": "Chúng sẽ dùng gậy",
      "order": 2,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.903Z",
      "updatedAt": "2024-09-07T15:25:01.903Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "cbbcf59b-d4c8-42b6-9e7b-bc7daec16e58",
      "number": 1,
      "content": "Đánh vào má người lãnh đạo I-sơ-ra-ên.",
      "order": 3,
      "parNumber": 3,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.903Z",
      "updatedAt": "2024-09-07T15:25:01.903Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "d2fb069f-4b00-4cbb-b163-c8c155a10046",
      "number": 2,
      "content": "“Này ngươi, hỡi Bết-lê-hem ở Ép-ra-tha,",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.913Z",
      "updatedAt": "2024-09-07T15:25:01.913Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "379024b5-65d6-43cc-89df-6d1a8ba6710f",
      "number": 2,
      "content": "Dù ngươi nhỏ bé không ra gì giữa các thị tộc của Giu-đa,",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.913Z",
      "updatedAt": "2024-09-07T15:25:01.913Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "fbfb9d69-ce6f-4394-bb70-f7be5b78f231",
      "number": 2,
      "content": "Nhưng từ ngươi sẽ ra cho Ta một người,",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.913Z",
      "updatedAt": "2024-09-07T15:25:01.913Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "1ef63ca8-7040-4d9a-a455-7f2f90d4685f",
      "number": 2,
      "content": "Một người sẽ trị vì trên I-sơ-ra-ên.",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.913Z",
      "updatedAt": "2024-09-07T15:25:01.913Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "7031a7d9-e488-47ff-97ba-cc91096e1532",
      "number": 2,
      "content": "Gốc gác của Người đã có từ ngàn xưa,",
      "order": 4,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.913Z",
      "updatedAt": "2024-09-07T15:25:01.913Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "b07927c1-391d-4b60-b236-8cde3accdd20",
      "number": 2,
      "content": "Từ vô cực trong quá khứ.”",
      "order": 5,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.913Z",
      "updatedAt": "2024-09-07T15:25:01.913Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "83d354aa-2dac-4993-bd19-cb3ce3a61f7f",
      "number": 3,
      "content": "Vì thế Ngài sẽ bỏ mặc họ một thời gian,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.918Z",
      "updatedAt": "2024-09-07T15:25:01.918Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "056e0405-6efc-4b57-b425-14cf406050d0",
      "number": 3,
      "content": "Cho đến khi người đàn bà lâm bồn đã sinh con;",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.918Z",
      "updatedAt": "2024-09-07T15:25:01.918Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "a471c832-1f4f-43db-96ba-4978525ade19",
      "number": 3,
      "content": "Bấy giờ những kẻ cùng huyết thống của người còn sống sót",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.918Z",
      "updatedAt": "2024-09-07T15:25:01.918Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "d2d6872d-de18-4ac4-a1c2-afd41ceec3ab",
      "number": 3,
      "content": "Sẽ trở về quê hương đoàn tụ với dân I-sơ-ra-ên.",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.918Z",
      "updatedAt": "2024-09-07T15:25:01.918Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "7e41b84a-0bc9-4619-9c1e-a5dd8f9859e0",
      "number": 4,
      "content": "Người sẽ đứng dậy chăn bầy của mình bằng quyền lực của CHÚA,",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.923Z",
      "updatedAt": "2024-09-07T15:25:01.923Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "cad35178-d1fb-4aa0-acfd-3153e53b5760",
      "number": 4,
      "content": "Bằng uy danh của CHÚA, Đức Chúa Trời của Người.",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.923Z",
      "updatedAt": "2024-09-07T15:25:01.923Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "686aa34a-569b-4191-9c3e-530e92111617",
      "number": 4,
      "content": "Bấy giờ thiên hạ sẽ được an cư lạc nghiệp,",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.923Z",
      "updatedAt": "2024-09-07T15:25:01.923Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "9bbce6e1-7c4f-416f-a718-19c1f512478a",
      "number": 4,
      "content": "Vì khi ấy quyền lực của Người sẽ trải rộng đến mọi nơi tận cùng trái đất.",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.923Z",
      "updatedAt": "2024-09-07T15:25:01.923Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "2522808c-30c0-4ac6-b3a2-4bb2df7e567c",
      "number": 5,
      "content": "Chính Người sẽ đem lại hòa bình.",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.927Z",
      "updatedAt": "2024-09-07T15:25:01.927Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "e53d213e-278e-4417-b419-474895785554",
      "number": 5,
      "content": "Nếu quân A-sy-ri xâm lăng đất nước chúng ta",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.927Z",
      "updatedAt": "2024-09-07T15:25:01.927Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "f8b6394a-1159-48a5-8e43-d3b9ab5b568e",
      "number": 5,
      "content": "Và bước chân đến các lâu đài của chúng ta,",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.927Z",
      "updatedAt": "2024-09-07T15:25:01.927Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "f5a3a276-0a68-4cc9-bf3e-08f9fa409619",
      "number": 5,
      "content": "Chúng ta sẽ lập bảy người chăn",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.927Z",
      "updatedAt": "2024-09-07T15:25:01.927Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "cf55938b-b025-4c0a-919d-632a61cc2003",
      "number": 5,
      "content": "Và tám người lãnh đạo để đối phó với chúng.",
      "order": 4,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.927Z",
      "updatedAt": "2024-09-07T15:25:01.927Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "0c6e16e4-f815-4274-ac46-d92092632206",
      "number": 6,
      "content": "Họ sẽ đánh bại xứ A-sy-ri bằng gươm;",
      "order": 0,
      "parNumber": 24,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.933Z",
      "updatedAt": "2024-09-07T15:25:01.933Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "66a7a5e6-3c16-4a35-a6bd-2708b88b7bc6",
      "number": 6,
      "content": "Họ sẽ tiến vào đất của Nim-rốt bằng các cổng chính;",
      "order": 1,
      "parNumber": 25,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.933Z",
      "updatedAt": "2024-09-07T15:25:01.933Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "e0d7829a-10c5-46ce-a6f3-1fa8c812279b",
      "number": 6,
      "content": "Người sẽ cứu chúng ta khỏi dân A-sy-ri,",
      "order": 2,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.933Z",
      "updatedAt": "2024-09-07T15:25:01.933Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "b88ecb21-95df-412b-b8ab-74b8d62e4107",
      "number": 6,
      "content": "Nếu chúng đến đất nước chúng ta",
      "order": 3,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.933Z",
      "updatedAt": "2024-09-07T15:25:01.933Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "d359e914-8104-4e90-a5b9-68c5e3c40098",
      "number": 6,
      "content": "Hay bước chân vào biên giới chúng ta.",
      "order": 4,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.933Z",
      "updatedAt": "2024-09-07T15:25:01.933Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "e93cabaa-9aa0-4fd7-a99c-8c9772c9e1a0",
      "number": 7,
      "content": "Bấy giờ những người còn sót lại của Gia-cốp,",
      "order": 0,
      "parNumber": 30,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.937Z",
      "updatedAt": "2024-09-07T15:25:01.937Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "d9b0d4ff-c146-4d0c-9c47-b6804d0bb8e3",
      "number": 7,
      "content": "Những người đang sống giữa các dân,",
      "order": 1,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.937Z",
      "updatedAt": "2024-09-07T15:25:01.937Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "3966add1-ad94-4b12-8a0e-7999f150d6ea",
      "number": 7,
      "content": "Sẽ như sương móc đến từ CHÚA,",
      "order": 2,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.937Z",
      "updatedAt": "2024-09-07T15:25:01.937Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "b32c9f46-a7c3-4bae-a258-164a340573c0",
      "number": 7,
      "content": "Như mưa rào rơi tự do trên cỏ,",
      "order": 3,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.937Z",
      "updatedAt": "2024-09-07T15:25:01.937Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "2ba4f2e6-ba60-44a2-8978-247e75960dcd",
      "number": 7,
      "content": "Không bị lệ thuộc vào người nào,",
      "order": 4,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.937Z",
      "updatedAt": "2024-09-07T15:25:01.937Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "a5a79608-33e1-4f27-8882-4850d5c25ca4",
      "number": 7,
      "content": "Không phải đợi chờ để được phép của ai.",
      "order": 5,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.937Z",
      "updatedAt": "2024-09-07T15:25:01.937Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "f7fc2044-169c-41f7-8234-f613240cd404",
      "number": 8,
      "content": "Lúc ấy những người còn sót lại của Gia-cốp",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.942Z",
      "updatedAt": "2024-09-07T15:25:01.942Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "fc40403b-7fa8-495d-94d6-ad517d2950e7",
      "number": 8,
      "content": "Ở giữa các quốc gia",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.942Z",
      "updatedAt": "2024-09-07T15:25:01.942Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "ed3d2d9b-e36a-4319-9b49-c9fca692e4f7",
      "number": 8,
      "content": "Sẽ như sư tử ở giữa các thú rừng,",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.942Z",
      "updatedAt": "2024-09-07T15:25:01.942Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "89c4834a-a3b9-4ac4-b2b0-13ddc52ec264",
      "number": 8,
      "content": "Như sư tử tơ ở giữa các đàn chiên,",
      "order": 3,
      "parNumber": 39,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.942Z",
      "updatedAt": "2024-09-07T15:25:01.942Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "62c5c478-4f02-4843-b280-9634347af7d8",
      "number": 8,
      "content": "Nếu nó đi qua, nó sẽ giày đạp và xé xác các con mồi ra từng mảnh,",
      "order": 4,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.942Z",
      "updatedAt": "2024-09-07T15:25:01.942Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "82323d57-edea-45cc-8816-cde95526ce14",
      "number": 8,
      "content": "Không ai cứu chúng được.",
      "order": 5,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.942Z",
      "updatedAt": "2024-09-07T15:25:01.942Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "5db96d05-d544-43fc-bedd-d4ad0fab8e1c",
      "number": 9,
      "content": "“Tay của ngươi sẽ đưa lên đè bẹp những kẻ chống lại ngươi,",
      "order": 0,
      "parNumber": 42,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.947Z",
      "updatedAt": "2024-09-07T15:25:01.947Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "d50ab1e0-a9f5-4254-ad07-2da43da915d4",
      "number": 9,
      "content": "Tất cả kẻ thù của ngươi sẽ bị tiêu diệt.",
      "order": 1,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.947Z",
      "updatedAt": "2024-09-07T15:25:01.947Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "4e3d4554-2b6d-4404-b4fc-788804216a87",
      "number": 10,
      "content": "Trong ngày ấy,” CHÚA phán,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.951Z",
      "updatedAt": "2024-09-07T15:25:01.951Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "58057ff7-d287-4c1f-b88d-b120a0afc541",
      "number": 10,
      "content": "“Ta sẽ tiêu diệt các ngựa chiến giữa ngươi,",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.951Z",
      "updatedAt": "2024-09-07T15:25:01.951Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "b382bc41-d8aa-4168-82f8-7d667356c2f9",
      "number": 10,
      "content": "Ta sẽ hủy diệt các xe chiến mã của ngươi.",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.951Z",
      "updatedAt": "2024-09-07T15:25:01.951Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "677023a4-e21f-45e9-bc52-58531ae8b81f",
      "number": 11,
      "content": "Ta sẽ hủy phá các thành trong đất của ngươi,",
      "order": 0,
      "parNumber": 47,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.957Z",
      "updatedAt": "2024-09-07T15:25:01.957Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "315beffb-a54e-455c-9d63-ae08ddd674a9",
      "number": 11,
      "content": "Ta sẽ làm sụp đổ tất cả các lâu đài kiên cố.",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.957Z",
      "updatedAt": "2024-09-07T15:25:01.957Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "023a1756-3e01-4d32-af43-4b897f9c1a59",
      "number": 12,
      "content": "Ta sẽ diệt trừ bọn phù thủy thầy pháp khỏi tay ngươi,",
      "order": 0,
      "parNumber": 49,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.962Z",
      "updatedAt": "2024-09-07T15:25:01.962Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "9bc1f274-da0b-476d-bf19-788954184e23",
      "number": 12,
      "content": "Ngươi sẽ không còn những kẻ đoán mò bói tướng.",
      "order": 1,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.962Z",
      "updatedAt": "2024-09-07T15:25:01.962Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "4c50c43b-18bd-40fe-93e0-c61e6e1f9d47",
      "number": 13,
      "content": "Ta sẽ tiêu diệt các tượng chạm của ngươi",
      "order": 0,
      "parNumber": 51,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.968Z",
      "updatedAt": "2024-09-07T15:25:01.968Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "f25fab63-08de-4d01-b156-e650dbcfe6d0",
      "number": 13,
      "content": "Và các trụ thờ của ngươi khỏi giữa ngươi,",
      "order": 1,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.968Z",
      "updatedAt": "2024-09-07T15:25:01.968Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "a7be85fc-866b-4f96-8d3f-41ae3a6c1b7d",
      "number": 13,
      "content": "Ngươi sẽ không quỳ lạy trước các tác phẩm",
      "order": 2,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.968Z",
      "updatedAt": "2024-09-07T15:25:01.968Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "508bc697-9c07-4aea-b32b-eb9cbfb9ecd7",
      "number": 13,
      "content": "Do tay ngươi làm ra nữa.",
      "order": 3,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.968Z",
      "updatedAt": "2024-09-07T15:25:01.968Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "b66eefcf-9d40-47bf-a9fe-923cff025931",
      "number": 14,
      "content": "Ta sẽ cho nhổ lên tận gốc các trụ thờ Nữ Thần A-sê-ra,",
      "order": 0,
      "parNumber": 55,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.973Z",
      "updatedAt": "2024-09-07T15:25:01.973Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "532e87e1-34f5-4675-98ab-f07ce97688df",
      "number": 14,
      "content": "Và tiêu diệt các thành thờ thần tượng của ngươi.",
      "order": 1,
      "parNumber": 56,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.973Z",
      "updatedAt": "2024-09-07T15:25:01.973Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "52d1a0d9-edcc-4072-a340-b93e74f3d866",
      "number": 15,
      "content": "Trong cơn giận và cơn thịnh nộ,",
      "order": 0,
      "parNumber": 57,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.979Z",
      "updatedAt": "2024-09-07T15:25:01.979Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    },
    {
      "id": "14904031-e485-432a-9e2d-ec6ce89f2d47",
      "number": 15,
      "content": "Ta sẽ thực hiện cuộc báo thù những dân không vâng lời Ta.”",
      "order": 1,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:25:01.979Z",
      "updatedAt": "2024-09-07T15:25:01.979Z",
      "chapterId": "7c8f6521-2d01-48f3-94e3-d2f841cb9503"
    }
  ],
