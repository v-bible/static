    },
    {
      "id": "d0adad69-c22d-428c-aca4-9df869f428e1",
      "content": "Lời Hứa về Sự Phục Hưng Sau Thời Bị Lưu Đày",
      "order": 0,
      "createdAt": "2024-09-07T15:24:54.974Z",
      "updatedAt": "2024-09-07T15:24:54.974Z",
