{
  "chapter": {
    "id": "28637ba7-4eef-4355-9b80-6cf5ab302546",
    "number": 3,
    "ref": "https://www.bible.com/bible/19/ZEP.3.BD2011",
    "createdAt": "2024-09-07T11:51:13.152Z",
    "updatedAt": "2024-09-07T11:51:13.152Z",
    "bookId": "ad844705-f900-4fa4-8011-e1a16653739e"
  },
  "verses": [
    {
      "id": "3414453c-b86a-4e9a-80b6-97a44bfdaec9",
      "number": 1,
      "content": "Khốn cho thành đầy những thứ bẩn thỉu và ô nhơ!",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.157Z",
      "updatedAt": "2024-09-07T15:27:10.157Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "dbcb5088-9dad-49e4-ad4a-2ad823bf5d8b",
      "number": 1,
      "content": "Bạo tàn và áp bức!",
      "order": 1,
      "parNumber": 2,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.157Z",
      "updatedAt": "2024-09-07T15:27:10.157Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "fbb27c7b-d3a6-474b-9019-bbee4c07b516",
      "number": 2,
      "content": "Nó không chịu vâng lời,",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.166Z",
      "updatedAt": "2024-09-07T15:27:10.166Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "a91a577d-e0a5-4358-be4d-238afa7186f2",
      "number": 2,
      "content": "Nó không chịu sự sửa dạy,",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.166Z",
      "updatedAt": "2024-09-07T15:27:10.166Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "da00fff9-e780-4501-a18a-a34197d1902c",
      "number": 2,
      "content": "Nó không tin cậy CHÚA,",
      "order": 2,
      "parNumber": 5,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.166Z",
      "updatedAt": "2024-09-07T15:27:10.166Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "96dcd468-9cd9-45c7-8f98-25d7114b8d7b",
      "number": 2,
      "content": "Nó không chịu đến gần Đức Chúa Trời của nó.",
      "order": 3,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.166Z",
      "updatedAt": "2024-09-07T15:27:10.166Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "d5cfebc2-6bf6-48cb-b84f-97d584a832ab",
      "number": 3,
      "content": "Những kẻ cầm quyền ở giữa nó như bầy sư tử rống,",
      "order": 0,
      "parNumber": 7,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.169Z",
      "updatedAt": "2024-09-07T15:27:10.169Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "cae615ff-0765-496b-a8f1-bb39a39913d9",
      "number": 3,
      "content": "Các thủ lãnh của nó như bọn chó sói ăn đêm,",
      "order": 1,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.169Z",
      "updatedAt": "2024-09-07T15:27:10.169Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "9ecbea7d-e634-48b2-b78b-4412edce92da",
      "number": 3,
      "content": "Chúng chẳng chừa lại một miếng xương",
      "order": 2,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.169Z",
      "updatedAt": "2024-09-07T15:27:10.169Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "c73b761a-0564-4d92-ae7c-36d45158baa5",
      "number": 3,
      "content": "Để gặm sáng hôm sau.",
      "order": 3,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.169Z",
      "updatedAt": "2024-09-07T15:27:10.169Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "c021f356-82fa-4dbb-9438-82ac8fb22742",
      "number": 4,
      "content": "Các tiên tri của nó xấc láo và tráo trở,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.173Z",
      "updatedAt": "2024-09-07T15:27:10.173Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "e46e89c1-3b39-4a02-b72a-02b7cb1306d0",
      "number": 4,
      "content": "Các tư tế của nó làm ô uế nơi thánh",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.173Z",
      "updatedAt": "2024-09-07T15:27:10.173Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "277ab277-c919-4b42-b641-d911b77c8271",
      "number": 4,
      "content": "Và vi phạm trắng trợn luật pháp.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.173Z",
      "updatedAt": "2024-09-07T15:27:10.173Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "a9fd4311-0307-49d0-b2bb-e74879ddcd66",
      "number": 5,
      "content": "CHÚA, Đấng ngự giữa thành, là Đấng công chính,",
      "order": 0,
      "parNumber": 14,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.176Z",
      "updatedAt": "2024-09-07T15:27:10.176Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "1a6188a5-22a7-4477-a218-6f407d0f3d4f",
      "number": 5,
      "content": "Ngài không làm điều tội lỗi.",
      "order": 1,
      "parNumber": 15,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.176Z",
      "updatedAt": "2024-09-07T15:27:10.176Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "cbb22f4d-d254-4fe7-b693-085f11e24ea6",
      "number": 5,
      "content": "Mỗi buổi sáng Ngài đưa công lý của Ngài ra ánh sáng;",
      "order": 2,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.176Z",
      "updatedAt": "2024-09-07T15:27:10.176Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "109b595c-a488-43fb-9951-6c2b116ea814",
      "number": 5,
      "content": "Ngài không bao giờ lỗi hẹn cả;",
      "order": 3,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.176Z",
      "updatedAt": "2024-09-07T15:27:10.176Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "064f5c0d-e273-4d70-91e9-82711cf44a9f",
      "number": 5,
      "content": "Thế mà kẻ bất chính lại không biết hổ thẹn.",
      "order": 4,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.176Z",
      "updatedAt": "2024-09-07T15:27:10.176Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "c7d17731-cc5d-4d97-a707-9fd5f2dba832",
      "number": 6,
      "content": "“Ta đã dẹp sạch các nước,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.178Z",
      "updatedAt": "2024-09-07T15:27:10.178Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "6ede226d-2a0b-42d1-aee7-02b99d6c5657",
      "number": 6,
      "content": "Các tháp phòng thủ nơi các góc thành của chúng đã bị đổ nát,",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.178Z",
      "updatedAt": "2024-09-07T15:27:10.178Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "2e320e83-fb81-42f4-bc55-93e9ce366406",
      "number": 6,
      "content": "Ta đã làm cho các đường phố của chúng ra vắng tanh,",
      "order": 2,
      "parNumber": 21,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.178Z",
      "updatedAt": "2024-09-07T15:27:10.178Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "489103a2-5d3f-4a02-bea3-a3d44cbe48b4",
      "number": 6,
      "content": "Chẳng có một người qua lại.",
      "order": 3,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.178Z",
      "updatedAt": "2024-09-07T15:27:10.178Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "19ee432c-303a-4283-9358-70647823395a",
      "number": 6,
      "content": "Các thành trì của chúng đã hoàn toàn bị phá hủy,",
      "order": 4,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.178Z",
      "updatedAt": "2024-09-07T15:27:10.178Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "fd7e7802-58b5-472d-8d6b-5f3c5fbaf0ef",
      "number": 6,
      "content": "Chẳng còn lại ai, không một người nào sống ở đó.",
      "order": 5,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.178Z",
      "updatedAt": "2024-09-07T15:27:10.178Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "f83b5403-7bf4-40c2-8acc-0dbce67f8992",
      "number": 7,
      "content": "Ta tự nhủ, ‘Khi thấy vậy thế nào ngươi cũng sẽ sợ Ta,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.181Z",
      "updatedAt": "2024-09-07T15:27:10.181Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "65e89aee-bfdc-4740-a569-7403dff723e6",
      "number": 7,
      "content": "Ngươi sẽ chấp nhận lời giáo huấn của Ta.",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.181Z",
      "updatedAt": "2024-09-07T15:27:10.181Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "7ffb2e60-0216-4d2e-8905-d2ca6c4e506a",
      "number": 7,
      "content": "Và như thế nơi ở của ngươi sẽ không bị Ta tiêu diệt;",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.181Z",
      "updatedAt": "2024-09-07T15:27:10.181Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "3f2a1d9e-3112-4407-a587-97673fb7fc40",
      "number": 7,
      "content": "Tất cả các hình phạt Ta định giáng trên ngươi, Ta sẽ không cần thực hiện.’",
      "order": 3,
      "parNumber": 28,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.181Z",
      "updatedAt": "2024-09-07T15:27:10.181Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "439fbf38-95a5-4e3b-b150-a827063cbe3f",
      "number": 7,
      "content": "Thế nhưng chúng càng băng hoại hơn",
      "order": 4,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.181Z",
      "updatedAt": "2024-09-07T15:27:10.181Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "c77744b7-4660-4844-869b-c2c813c9ab56",
      "number": 7,
      "content": "Trong mọi việc chúng làm.",
      "order": 5,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.181Z",
      "updatedAt": "2024-09-07T15:27:10.181Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "3b21ca6f-10e1-48d4-b0b2-58194a745b85",
      "number": 8,
      "content": "Vì thế các ngươi hãy đợi Ta,” CHÚA phán,",
      "order": 0,
      "parNumber": 31,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.184Z",
      "updatedAt": "2024-09-07T15:27:10.184Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "763cb18a-b06b-4e8d-9278-6b01350e4182",
      "number": 8,
      "content": "“Đợi đến ngày Ta đứng lên đoán phạt,",
      "order": 1,
      "parNumber": 32,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.184Z",
      "updatedAt": "2024-09-07T15:27:10.184Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "1d746dae-f5d8-4d8e-aea5-8d818333863b",
      "number": 8,
      "content": "Vì Ta quyết sẽ nhóm hiệp các dân",
      "order": 2,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.184Z",
      "updatedAt": "2024-09-07T15:27:10.184Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "aa4b9255-53f9-45bb-95bc-4126146ddeba",
      "number": 8,
      "content": "Và triệu tập các nước,",
      "order": 3,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.184Z",
      "updatedAt": "2024-09-07T15:27:10.184Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "10088caa-6cbe-42c6-9834-61275028940d",
      "number": 8,
      "content": "Để Ta đổ cơn giận",
      "order": 4,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.184Z",
      "updatedAt": "2024-09-07T15:27:10.184Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "7fe0b1c4-9157-4130-9a06-e0df5350760b",
      "number": 8,
      "content": "Và cơn thịnh nộ của Ta trên chúng;",
      "order": 5,
      "parNumber": 36,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.184Z",
      "updatedAt": "2024-09-07T15:27:10.184Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "cab2af75-6f21-4bc3-b438-216b46d33768",
      "number": 8,
      "content": "Khi ấy khắp đất sẽ bị thiêu rụi",
      "order": 6,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.184Z",
      "updatedAt": "2024-09-07T15:27:10.184Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "580d0fba-36f7-4dd6-8e58-3cd1975d3cce",
      "number": 8,
      "content": "Bởi cơn ghen tức của Ta.”",
      "order": 7,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.184Z",
      "updatedAt": "2024-09-07T15:27:10.184Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "cf4ab591-8ab5-47dc-be55-d3b7ea6250da",
      "number": 9,
      "content": "“Bấy giờ Ta sẽ thanh tẩy",
      "order": 0,
      "parNumber": 40,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.187Z",
      "updatedAt": "2024-09-07T15:27:10.187Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "2397000f-d8c2-43a3-9bb4-a79966f7aa5f",
      "number": 9,
      "content": "Môi miệng của muôn dân,",
      "order": 1,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.187Z",
      "updatedAt": "2024-09-07T15:27:10.187Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "c5e355f2-72ce-4a9e-afc0-ab4149bb7543",
      "number": 9,
      "content": "Để tất cả sẽ kêu cầu danh CHÚA,",
      "order": 2,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.187Z",
      "updatedAt": "2024-09-07T15:27:10.187Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "d1025367-1143-448d-ab18-5b04d792db1c",
      "number": 9,
      "content": "Và chung vai sát cánh với nhau hầu việc Ngài.",
      "order": 3,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.187Z",
      "updatedAt": "2024-09-07T15:27:10.187Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "b139a810-c81f-4bdb-8975-d0a5ba1814e4",
      "number": 10,
      "content": "Từ bên kia các sông của xứ Cút,",
      "order": 0,
      "parNumber": 44,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.191Z",
      "updatedAt": "2024-09-07T15:27:10.191Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "17a30b6e-62dd-4568-932d-06e5449e5624",
      "number": 10,
      "content": "Những kẻ thờ phượng Ta, những kẻ đã bị Ta làm tản lạc",
      "order": 1,
      "parNumber": 45,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.191Z",
      "updatedAt": "2024-09-07T15:27:10.191Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "312fac70-7cb5-475f-882d-e433db31eb8d",
      "number": 10,
      "content": "Sẽ đem những lễ vật về dâng lên Ta.",
      "order": 2,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.191Z",
      "updatedAt": "2024-09-07T15:27:10.191Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "10603ef3-f70b-4a5e-9c59-2c9f1b8fbd5b",
      "number": 11,
      "content": "Trong ngày ấy ngươi sẽ không còn xấu hổ",
      "order": 0,
      "parNumber": 47,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.196Z",
      "updatedAt": "2024-09-07T15:27:10.196Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "419f118a-440b-49e5-a670-66988bd2dbec",
      "number": 11,
      "content": "Về những việc sai quấy ngươi đã làm đối với Ta,",
      "order": 1,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.196Z",
      "updatedAt": "2024-09-07T15:27:10.196Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "d43fac93-321c-4e2f-9b63-8c1fb890aad3",
      "number": 11,
      "content": "Vì Ta sẽ cất khỏi ngươi",
      "order": 2,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.196Z",
      "updatedAt": "2024-09-07T15:27:10.196Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "e094e1f6-6164-4510-86b2-58d4db0673b2",
      "number": 11,
      "content": "Những kẻ cười ngạo nghễ.",
      "order": 3,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.196Z",
      "updatedAt": "2024-09-07T15:27:10.196Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "6aa00cc4-7d1d-467e-945d-0b8177fc719d",
      "number": 11,
      "content": "Ngươi sẽ không bao giờ lập lại sự kiêu ngạo như thế",
      "order": 4,
      "parNumber": 51,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.196Z",
      "updatedAt": "2024-09-07T15:27:10.196Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "40dd61aa-d7b4-4445-8658-09dc34a0cd3a",
      "number": 11,
      "content": "Trên núi thánh của Ta.",
      "order": 5,
      "parNumber": 52,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.196Z",
      "updatedAt": "2024-09-07T15:27:10.196Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "16bb67fd-82bc-44ca-8db0-31b27af08e3c",
      "number": 12,
      "content": "Ta sẽ chừa lại giữa ngươi",
      "order": 0,
      "parNumber": 53,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.202Z",
      "updatedAt": "2024-09-07T15:27:10.202Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "c84421d0-2145-473b-b286-dc15fb02ac07",
      "number": 12,
      "content": "Những người hiền nhu và khiêm tốn,",
      "order": 1,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.202Z",
      "updatedAt": "2024-09-07T15:27:10.202Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "f607bd6d-8753-4908-8537-0fb417903afb",
      "number": 12,
      "content": "Những người tin cậy vào danh CHÚA.",
      "order": 2,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.202Z",
      "updatedAt": "2024-09-07T15:27:10.202Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "1adf0675-3c5e-414a-a196-98e68475d153",
      "number": 13,
      "content": "Những người còn sót lại của I-sơ-ra-ên",
      "order": 0,
      "parNumber": 56,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.208Z",
      "updatedAt": "2024-09-07T15:27:10.208Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "630f7822-798b-464b-a97a-27bd4054944b",
      "number": 13,
      "content": "Sẽ không làm điều gian ác;",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.208Z",
      "updatedAt": "2024-09-07T15:27:10.208Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "6bfba0dd-8ddf-4e1f-ba17-3588b55dc0e2",
      "number": 13,
      "content": "Chúng sẽ không nói dối;",
      "order": 2,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.208Z",
      "updatedAt": "2024-09-07T15:27:10.208Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "f362c410-3a6a-4595-bc70-9dc6b36c8003",
      "number": 13,
      "content": "Lưỡi dối gạt",
      "order": 3,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.208Z",
      "updatedAt": "2024-09-07T15:27:10.208Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "78d06b07-7850-46ac-8a6c-bf4e573a0f59",
      "number": 13,
      "content": "Không có trong miệng chúng.",
      "order": 4,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.208Z",
      "updatedAt": "2024-09-07T15:27:10.208Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "f294b77c-bb4a-430d-94e1-51a962199634",
      "number": 13,
      "content": "Chúng sẽ ăn rồi nằm nghỉ,",
      "order": 5,
      "parNumber": 61,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.208Z",
      "updatedAt": "2024-09-07T15:27:10.208Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "ea9ff20f-decf-41d1-ad10-599224c62957",
      "number": 13,
      "content": "Và không ai sẽ làm chúng phải sợ hãi gì.”",
      "order": 6,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.208Z",
      "updatedAt": "2024-09-07T15:27:10.208Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "7997f548-691c-48e6-8e63-1f4a457e7de6",
      "number": 14,
      "content": "Hãy hát, hỡi Ái Nữ của Si-ôn!",
      "order": 0,
      "parNumber": 64,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.214Z",
      "updatedAt": "2024-09-07T15:27:10.214Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "e1d89637-6ddb-4e57-8984-213babf321a4",
      "number": 14,
      "content": "Hãy reo hò vang dậy, hỡi I-sơ-ra-ên!",
      "order": 1,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.214Z",
      "updatedAt": "2024-09-07T15:27:10.214Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "5993f4f6-f301-4f0d-9e5b-21fee7a3d362",
      "number": 14,
      "content": "Hãy hân hoan và mừng rỡ,",
      "order": 2,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.214Z",
      "updatedAt": "2024-09-07T15:27:10.214Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "c02846ae-a632-4ee4-b9f2-a85fbd9a3f72",
      "number": 14,
      "content": "Hỡi Ái Nữ của Giê-ru-sa-lem!",
      "order": 3,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.214Z",
      "updatedAt": "2024-09-07T15:27:10.214Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "73afb573-d681-47c5-89da-4bfb51edfd43",
      "number": 15,
      "content": "Vì CHÚA đã bỏ án phạt của ngươi,",
      "order": 0,
      "parNumber": 68,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.220Z",
      "updatedAt": "2024-09-07T15:27:10.220Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "55942657-0549-4e51-bba9-167eb5a81697",
      "number": 15,
      "content": "Ngài đã khiến quân thù của ngươi phải rút lui.",
      "order": 1,
      "parNumber": 69,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.220Z",
      "updatedAt": "2024-09-07T15:27:10.220Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "23a88f87-50aa-495e-bde8-7fde45ff176a",
      "number": 15,
      "content": "CHÚA, Vua của I-sơ-ra-ên, đang ngự giữa ngươi;",
      "order": 2,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.220Z",
      "updatedAt": "2024-09-07T15:27:10.220Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "17a1a41d-f844-4b4a-b66a-987989f73f1a",
      "number": 15,
      "content": "Ngươi sẽ không bao giờ sợ tai họa nào nữa.",
      "order": 3,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.220Z",
      "updatedAt": "2024-09-07T15:27:10.220Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "b469b870-f102-4546-857a-dc7c48e63ac9",
      "number": 16,
      "content": "Trong ngày ấy người ta sẽ nói với Giê-ru-sa-lem,",
      "order": 0,
      "parNumber": 72,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.226Z",
      "updatedAt": "2024-09-07T15:27:10.226Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "92bf864f-a005-469c-8190-fca846646c42",
      "number": 16,
      "content": "“Hỡi Si-ôn, đừng lo sợ;",
      "order": 1,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.226Z",
      "updatedAt": "2024-09-07T15:27:10.226Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "df2e5fc5-a1e1-476e-81f8-2a3042ccbae2",
      "number": 16,
      "content": "Tay ngươi chớ bủn rủn.",
      "order": 2,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.226Z",
      "updatedAt": "2024-09-07T15:27:10.226Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "3074cd8a-4c04-4fe0-a3f6-8e9e873c1832",
      "number": 17,
      "content": "Vì CHÚA, Đức Chúa Trời của ngươi, ngự giữa ngươi;",
      "order": 0,
      "parNumber": 75,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.231Z",
      "updatedAt": "2024-09-07T15:27:10.231Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "2c6a2dd2-3b52-4b06-aac1-76162f80ce01",
      "number": 17,
      "content": "Ngài là Đấng quyền năng, Ngài sẽ giải cứu.",
      "order": 1,
      "parNumber": 76,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.231Z",
      "updatedAt": "2024-09-07T15:27:10.231Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "f70867c3-45d1-4ec3-a169-72dc80526fe7",
      "number": 17,
      "content": "Ngài sẽ vui vẻ vì ngươi;",
      "order": 2,
      "parNumber": 77,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.231Z",
      "updatedAt": "2024-09-07T15:27:10.231Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "1e5b2d11-a87f-4b8d-8afd-e3afbd86335f",
      "number": 17,
      "content": "Ngài sẽ làm tâm hồn ngươi yên tĩnh vì tình yêu của Ngài.",
      "order": 3,
      "parNumber": 78,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.231Z",
      "updatedAt": "2024-09-07T15:27:10.231Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "711db2b8-a779-44b7-b566-6644b6ef4c18",
      "number": 17,
      "content": "Ngài sẽ vui vẻ cất tiếng hát vì ngươi.”",
      "order": 4,
      "parNumber": 79,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.231Z",
      "updatedAt": "2024-09-07T15:27:10.231Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "11b859b3-ab29-4268-8d76-7cf94db773ad",
      "number": 18,
      "content": "“Ta sẽ nhóm lại những kẻ từng sầu thảm mỗi khi đến ngày lễ hội,",
      "order": 0,
      "parNumber": 80,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.237Z",
      "updatedAt": "2024-09-07T15:27:10.237Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "6f904341-e606-43d1-ba2a-a465b90fe311",
      "number": 18,
      "content": "Những ngày lễ hội không còn là gánh tủi nhục trên ngươi.",
      "order": 1,
      "parNumber": 81,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.237Z",
      "updatedAt": "2024-09-07T15:27:10.237Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "3450cddf-f497-4af3-aeab-1c839aa5cf45",
      "number": 19,
      "content": "Khi ấy Ta sẽ đối phó",
      "order": 0,
      "parNumber": 82,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.242Z",
      "updatedAt": "2024-09-07T15:27:10.242Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "e270acca-ae42-463b-9cc2-4cde3e876c47",
      "number": 19,
      "content": "Với mọi kẻ áp bức ngươi;",
      "order": 1,
      "parNumber": 83,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.242Z",
      "updatedAt": "2024-09-07T15:27:10.242Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "50d5fff5-e103-4c3c-896b-a106c43fb045",
      "number": 19,
      "content": "Ta sẽ cứu giúp những người bị què quặt;",
      "order": 2,
      "parNumber": 84,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.242Z",
      "updatedAt": "2024-09-07T15:27:10.242Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "df5387bc-33b9-4fe9-af1c-64e37d04a156",
      "number": 19,
      "content": "Ta sẽ nhóm lại những kẻ bị tan lạc;",
      "order": 3,
      "parNumber": 85,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.242Z",
      "updatedAt": "2024-09-07T15:27:10.242Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "1de2f7cf-714d-42bf-a824-ba3ac2984f85",
      "number": 19,
      "content": "Ta sẽ làm chúng được khen ngợi",
      "order": 4,
      "parNumber": 86,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.242Z",
      "updatedAt": "2024-09-07T15:27:10.242Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "257dd92e-5be1-43f7-bad4-b71e26581b12",
      "number": 19,
      "content": "Và nổi danh trong mọi xứ chúng bị sỉ nhục.",
      "order": 5,
      "parNumber": 87,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.242Z",
      "updatedAt": "2024-09-07T15:27:10.242Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "32022f05-ccdd-4887-9601-dc0f73b35b16",
      "number": 20,
      "content": "Khi ấy Ta sẽ tụ họp các ngươi lại;",
      "order": 0,
      "parNumber": 88,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.248Z",
      "updatedAt": "2024-09-07T15:27:10.248Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "668ff294-88de-4f67-bcc1-bbf6f010080e",
      "number": 20,
      "content": "Khi ấy Ta sẽ đem các ngươi trở về.",
      "order": 1,
      "parNumber": 89,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.248Z",
      "updatedAt": "2024-09-07T15:27:10.248Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "01732727-9ec1-4033-ad97-51cf9a6791a5",
      "number": 20,
      "content": "Thật vậy Ta sẽ làm các ngươi được nổi danh",
      "order": 2,
      "parNumber": 90,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.248Z",
      "updatedAt": "2024-09-07T15:27:10.248Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "d7ff6954-8ced-4b4f-aaa8-b678dc32ad90",
      "number": 20,
      "content": "Và khen ngợi giữa mọi dân trên đất,",
      "order": 3,
      "parNumber": 91,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.248Z",
      "updatedAt": "2024-09-07T15:27:10.248Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "a53c8755-4210-4288-ba4d-fa330b4dead2",
      "number": 20,
      "content": "Khi Ta tái lập vận mạng các ngươi",
      "order": 4,
      "parNumber": 92,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.248Z",
      "updatedAt": "2024-09-07T15:27:10.248Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "ae38aa12-4aa6-4428-a049-66d6edbdd4c5",
      "number": 20,
      "content": "Ngay trước mắt các ngươi,”",
      "order": 5,
      "parNumber": 93,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.248Z",
      "updatedAt": "2024-09-07T15:27:10.248Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    },
    {
      "id": "e096e612-126d-4f76-a88e-07a8fa5e5f0d",
      "number": 20,
      "content": "CHÚA phán.",
      "order": 6,
      "parNumber": 94,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:27:10.248Z",
      "updatedAt": "2024-09-07T15:27:10.248Z",
      "chapterId": "28637ba7-4eef-4355-9b80-6cf5ab302546"
    }
  ],
