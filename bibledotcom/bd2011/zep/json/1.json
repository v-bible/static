{
  "chapter": {
    "id": "0da2a87a-73f8-4bdd-a253-251ce6de2505",
    "number": 1,
    "ref": "https://www.bible.com/bible/19/ZEP.1.BD2011",
    "createdAt": "2024-09-07T11:51:13.143Z",
    "updatedAt": "2024-09-07T11:51:13.143Z",
    "bookId": "ad844705-f900-4fa4-8011-e1a16653739e"
  },
  "verses": [
    {
      "id": "7c98ed80-5e3d-4113-9a73-0a4a95a987ef",
      "number": 1,
      "content": "Lời của CHÚA đến với Sô-phô-ni con của Cư-si, con của Ghê-đa-li-a, con của A-ma-ri-a, con của Ê-xê-chia, trong thời Giô-si-a con trai A-môn vua Giu-đa.",
      "order": 0,
      "parNumber": 1,
      "parIndex": 0,
      "isPoetry": false,
      "createdAt": "2024-09-07T15:26:49.768Z",
      "updatedAt": "2024-09-07T15:26:49.768Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "410d6f5d-22fa-4c4e-b047-065d08de4206",
      "number": 2,
      "content": "“Ta sẽ quét sạch",
      "order": 0,
      "parNumber": 3,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.777Z",
      "updatedAt": "2024-09-07T15:26:49.777Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "56139709-4cfe-4d44-a61b-0023485de6f5",
      "number": 2,
      "content": "Mọi sự trên mặt đất,” CHÚA phán.",
      "order": 1,
      "parNumber": 4,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.777Z",
      "updatedAt": "2024-09-07T15:26:49.777Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "e8edc8e2-3e5f-4368-ba6f-5adc01497749",
      "number": 3,
      "content": "“Ta sẽ quét sạch người ta và thú vật;",
      "order": 0,
      "parNumber": 5,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.780Z",
      "updatedAt": "2024-09-07T15:26:49.780Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "d95d868c-b946-4b04-9956-5429a12d6149",
      "number": 3,
      "content": "Ta sẽ quét sạch chim trời",
      "order": 1,
      "parNumber": 6,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.780Z",
      "updatedAt": "2024-09-07T15:26:49.780Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "4d59d9ac-641e-4a58-a399-0a0a415d197c",
      "number": 3,
      "content": "Và cá biển.",
      "order": 2,
      "parNumber": 7,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.780Z",
      "updatedAt": "2024-09-07T15:26:49.780Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "34b6b1f1-9c93-40ff-bec9-a33dcc733d52",
      "number": 3,
      "content": "Những kẻ gian ác sẽ ngã gục,",
      "order": 3,
      "parNumber": 8,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.780Z",
      "updatedAt": "2024-09-07T15:26:49.780Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "759e87a7-b8df-4d17-8259-334ad49b928a",
      "number": 3,
      "content": "Ta sẽ diệt trừ loài người",
      "order": 4,
      "parNumber": 9,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.780Z",
      "updatedAt": "2024-09-07T15:26:49.780Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "feef400b-c4ee-482d-b651-6f9c44140a01",
      "number": 3,
      "content": "Khỏi mặt đất,” CHÚA phán.",
      "order": 5,
      "parNumber": 10,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.780Z",
      "updatedAt": "2024-09-07T15:26:49.780Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "59d4cccb-51b7-494c-80a2-3d99d59146c6",
      "number": 4,
      "content": "“Ta sẽ đưa tay ra chống lại Giu-đa,",
      "order": 0,
      "parNumber": 11,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.783Z",
      "updatedAt": "2024-09-07T15:26:49.783Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "190d65c1-3671-4253-8cc9-cb24f9abe476",
      "number": 4,
      "content": "Và nghịch lại mọi kẻ sống ở Giê-ru-sa-lem.",
      "order": 1,
      "parNumber": 12,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.783Z",
      "updatedAt": "2024-09-07T15:26:49.783Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "d20d21a9-d106-4af4-9ed8-d24232a278ec",
      "number": 4,
      "content": "Ta sẽ diệt trừ khỏi chốn này những kẻ thờ lạy Ba-anh còn sót lại.",
      "order": 2,
      "parNumber": 13,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.783Z",
      "updatedAt": "2024-09-07T15:26:49.783Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "cad498b4-11ae-4df5-89b9-23cb728b09ab",
      "number": 4,
      "content": "Ta sẽ diệt trừ các tư tế thờ lạy hình tượng, để tên tuổi chúng không còn nữa.",
      "order": 3,
      "parNumber": 14,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.783Z",
      "updatedAt": "2024-09-07T15:26:49.783Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "ae5ba690-baee-444a-8bf2-3c7942a30ee4",
      "number": 5,
      "content": "Ta sẽ diệt trừ những kẻ lên sân thượng sụp lạy",
      "order": 0,
      "parNumber": 15,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.789Z",
      "updatedAt": "2024-09-07T15:26:49.789Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "05b3eef2-5af4-4471-8a8a-f300fceae764",
      "number": 5,
      "content": "Và thờ phượng các thần linh trên không trung,",
      "order": 1,
      "parNumber": 16,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.789Z",
      "updatedAt": "2024-09-07T15:26:49.789Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "ce93099b-2720-477a-a4e0-480c3dba90c1",
      "number": 5,
      "content": "Và những kẻ sấp mình xuống nhân danh CHÚA mà thề,",
      "order": 2,
      "parNumber": 17,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.789Z",
      "updatedAt": "2024-09-07T15:26:49.789Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "27901a5f-4c88-4720-b64b-0edcf101eb0d",
      "number": 5,
      "content": "Rồi lại nhân danh Minh-côm mà thề,",
      "order": 3,
      "parNumber": 18,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.789Z",
      "updatedAt": "2024-09-07T15:26:49.789Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "9d8c017e-9e4a-4ff8-acce-49632747832d",
      "number": 6,
      "content": "Cùng những kẻ xây lưng chối bỏ CHÚA,",
      "order": 0,
      "parNumber": 19,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.794Z",
      "updatedAt": "2024-09-07T15:26:49.794Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "9272b61b-2747-4f9c-a66d-499cc9b7cdc2",
      "number": 6,
      "content": "Không tìm kiếm CHÚA, và không cầu vấn Ngài.”",
      "order": 1,
      "parNumber": 20,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.794Z",
      "updatedAt": "2024-09-07T15:26:49.794Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "e4f19349-6bd4-4a30-9e14-df9234a24a2b",
      "number": 7,
      "content": "Hãy im lặng trước mặt CHÚA Hằng Hữu!",
      "order": 0,
      "parNumber": 21,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.796Z",
      "updatedAt": "2024-09-07T15:26:49.796Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "1818b4c2-6348-4b73-8a61-4debcf90d5d6",
      "number": 7,
      "content": "Vì ngày của CHÚA đến gần.",
      "order": 1,
      "parNumber": 22,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.796Z",
      "updatedAt": "2024-09-07T15:26:49.796Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "a9f6d205-4f13-4acb-9fa5-627bdf74e387",
      "number": 7,
      "content": "CHÚA đã chuẩn bị một cuộc hiến tế.",
      "order": 2,
      "parNumber": 23,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.796Z",
      "updatedAt": "2024-09-07T15:26:49.796Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "af2de1ea-8dca-48c7-bb13-0aabd7efde13",
      "number": 7,
      "content": "Ngài đã biệt riêng ra thánh những người khách của Ngài.",
      "order": 3,
      "parNumber": 24,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.796Z",
      "updatedAt": "2024-09-07T15:26:49.796Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "3ee57b94-7c0a-4513-b4da-73117c2ccba0",
      "number": 8,
      "content": "“Trong ngày hiến tế của CHÚA,",
      "order": 0,
      "parNumber": 25,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.799Z",
      "updatedAt": "2024-09-07T15:26:49.799Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "5fb1d9dc-8ccc-4ad1-890b-566c35e48692",
      "number": 8,
      "content": "Ta sẽ phạt các thủ lãnh, các hoàng tử,",
      "order": 1,
      "parNumber": 26,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.799Z",
      "updatedAt": "2024-09-07T15:26:49.799Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "8efa6c8c-7803-49b0-81f2-06eebb30b6a9",
      "number": 8,
      "content": "Và tất cả những kẻ mặc y phục ngoại bang.",
      "order": 2,
      "parNumber": 27,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.799Z",
      "updatedAt": "2024-09-07T15:26:49.799Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "e01c686f-cb5c-4d6e-a46b-ea6ad64e07a7",
      "number": 9,
      "content": "Trong ngày ấy Ta sẽ phạt",
      "order": 0,
      "parNumber": 28,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.801Z",
      "updatedAt": "2024-09-07T15:26:49.801Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "c501c6f7-f326-4351-b7b3-94166c55596e",
      "number": 9,
      "content": "Tất cả những kẻ nhảy qua ngạch cửa để thờ lạy hình tượng,",
      "order": 1,
      "parNumber": 29,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.801Z",
      "updatedAt": "2024-09-07T15:26:49.801Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "a96d825e-813b-4f56-abca-d509a433f876",
      "number": 9,
      "content": "Những kẻ chất đầy nhà chủ chúng",
      "order": 2,
      "parNumber": 30,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.801Z",
      "updatedAt": "2024-09-07T15:26:49.801Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "c1fc85cd-efdf-44f4-b614-079587bc6f6f",
      "number": 9,
      "content": "Những của cải chiếm được nhờ bạo lực và dối gian.",
      "order": 3,
      "parNumber": 31,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.801Z",
      "updatedAt": "2024-09-07T15:26:49.801Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "ab0148e5-1bb6-46e3-934a-f1608df2736a",
      "number": 10,
      "content": "Trong ngày ấy,” CHÚA phán,",
      "order": 0,
      "parNumber": 32,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.803Z",
      "updatedAt": "2024-09-07T15:26:49.803Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "67a8668f-883c-409d-b55f-b9bea4108ac7",
      "number": 10,
      "content": "“Người ta sẽ nghe tiếng khóc to ở Cổng Cá,",
      "order": 1,
      "parNumber": 33,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.803Z",
      "updatedAt": "2024-09-07T15:26:49.803Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "e7359035-427f-456c-a2f6-7ccd209e2143",
      "number": 10,
      "content": "Tiếng rên rỉ thở than ở Quận Nhì,",
      "order": 2,
      "parNumber": 34,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.803Z",
      "updatedAt": "2024-09-07T15:26:49.803Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "2384b343-c1d9-4f3b-a3c1-33414bb10c76",
      "number": 10,
      "content": "Và một tiếng đổ ầm từ miền đồi núi.",
      "order": 3,
      "parNumber": 35,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.803Z",
      "updatedAt": "2024-09-07T15:26:49.803Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "178abfaa-2289-4c03-bc7e-c125b118827b",
      "number": 11,
      "content": "Hãy khóc đi, hỡi dân cư ở khu thương mại!",
      "order": 0,
      "parNumber": 36,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.806Z",
      "updatedAt": "2024-09-07T15:26:49.806Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "8446ec30-b971-41ca-9bb0-7a269f8c072d",
      "number": 11,
      "content": "Vì tất cả các thương gia bị diệt mất.",
      "order": 1,
      "parNumber": 37,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.806Z",
      "updatedAt": "2024-09-07T15:26:49.806Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "d496714d-48ef-445c-93da-cc2a65f265f5",
      "number": 11,
      "content": "Tất cả những kẻ đổi bạc đều bị giết.",
      "order": 2,
      "parNumber": 38,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.806Z",
      "updatedAt": "2024-09-07T15:26:49.806Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "23ce271c-b854-4988-8cc4-d88eed4699bf",
      "number": 12,
      "content": "Khi ấy Ta sẽ dùng lồng đèn để lục soát Giê-ru-sa-lem;",
      "order": 0,
      "parNumber": 39,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.808Z",
      "updatedAt": "2024-09-07T15:26:49.808Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "954927ae-5df8-4aaa-ba7c-007f935f8014",
      "number": 12,
      "content": "Ta sẽ phạt những kẻ tự mãn,",
      "order": 1,
      "parNumber": 40,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.808Z",
      "updatedAt": "2024-09-07T15:26:49.808Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "9fe8d83e-22fa-42b2-b49b-3fc5e642e4e2",
      "number": 12,
      "content": "Những kẻ thản nhiên trước sự băng hoại xã hội,",
      "order": 2,
      "parNumber": 41,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.808Z",
      "updatedAt": "2024-09-07T15:26:49.808Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "d261ac82-0cbc-4272-a814-1ca486abe1e1",
      "number": 12,
      "content": "Những kẻ nói trong lòng,",
      "order": 3,
      "parNumber": 42,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.808Z",
      "updatedAt": "2024-09-07T15:26:49.808Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "df4bcce9-61f1-4800-9d34-74d35e4ee47c",
      "number": 12,
      "content": "‘CHÚA sẽ không ban phước",
      "order": 4,
      "parNumber": 43,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.808Z",
      "updatedAt": "2024-09-07T15:26:49.808Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "09ea7762-71e2-4f72-8a48-f13b1e804b8c",
      "number": 12,
      "content": "Hoặc giáng họa cho ai.’",
      "order": 5,
      "parNumber": 44,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.808Z",
      "updatedAt": "2024-09-07T15:26:49.808Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "94d67647-0754-43c6-8482-25633e2dc9f5",
      "number": 13,
      "content": "Vì thế các tài sản của chúng sẽ bị cướp đi,",
      "order": 0,
      "parNumber": 45,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.811Z",
      "updatedAt": "2024-09-07T15:26:49.811Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "edc2387f-ac93-4d86-8619-ba011cb7316e",
      "number": 13,
      "content": "Nhà cửa của chúng sẽ ra hoang phế;",
      "order": 1,
      "parNumber": 46,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.811Z",
      "updatedAt": "2024-09-07T15:26:49.811Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "f2311b35-7c54-4a59-9765-8a3e81f7901c",
      "number": 13,
      "content": "Chúng xây nhà",
      "order": 2,
      "parNumber": 47,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.811Z",
      "updatedAt": "2024-09-07T15:26:49.811Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "83737029-c188-4b03-9038-fc4297032107",
      "number": 13,
      "content": "Nhưng sẽ không được ở trong đó;",
      "order": 3,
      "parNumber": 48,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.811Z",
      "updatedAt": "2024-09-07T15:26:49.811Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "f848bc69-402d-47ee-9c64-aaddbc2bf820",
      "number": 13,
      "content": "Chúng trồng vườn nho",
      "order": 4,
      "parNumber": 49,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.811Z",
      "updatedAt": "2024-09-07T15:26:49.811Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "d917b6f7-a6c3-4262-aaee-62832722b739",
      "number": 13,
      "content": "Nhưng sẽ không được uống rượu của vườn nho.”",
      "order": 5,
      "parNumber": 50,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.811Z",
      "updatedAt": "2024-09-07T15:26:49.811Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "2a324e7a-372e-4de7-8146-a101b06aaeb5",
      "number": 14,
      "content": "Ngày lớn của CHÚA đã gần,",
      "order": 0,
      "parNumber": 52,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.814Z",
      "updatedAt": "2024-09-07T15:26:49.814Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "73cc2da8-4ea6-4cbb-9a14-12712a969334",
      "number": 14,
      "content": "Gần lắm rồi và ngày ấy đến nhanh chóng.",
      "order": 1,
      "parNumber": 53,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.814Z",
      "updatedAt": "2024-09-07T15:26:49.814Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "5ddd278f-db16-44d9-936e-bcaa93bdd94e",
      "number": 14,
      "content": "Những tiếng kêu gào trong ngày của CHÚA thật thê thảm,",
      "order": 2,
      "parNumber": 54,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.814Z",
      "updatedAt": "2024-09-07T15:26:49.814Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "747d5ec5-d1b7-45e8-b5f5-29160cce7cb9",
      "number": 14,
      "content": "Những chiến sĩ dũng mãnh cũng phải khóc to trong ngày ấy.",
      "order": 3,
      "parNumber": 55,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.814Z",
      "updatedAt": "2024-09-07T15:26:49.814Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "6507a811-90f8-4003-a52b-0f1210613050",
      "number": 15,
      "content": "Ngày ấy là ngày của thịnh nộ,",
      "order": 0,
      "parNumber": 56,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.817Z",
      "updatedAt": "2024-09-07T15:26:49.817Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "cba26516-4a6a-46b3-8253-c32e2a22722d",
      "number": 15,
      "content": "Ngày của khốn quẫn và bi thương,",
      "order": 1,
      "parNumber": 57,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.817Z",
      "updatedAt": "2024-09-07T15:26:49.817Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "7cbe6eec-f497-447d-8216-4bba894a7ff7",
      "number": 15,
      "content": "Ngày của hủy diệt và điêu tàn,",
      "order": 2,
      "parNumber": 58,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.817Z",
      "updatedAt": "2024-09-07T15:26:49.817Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "74d6e926-d164-4dc3-9e68-b3809777226e",
      "number": 15,
      "content": "Ngày của tối tăm và mù mịt,",
      "order": 3,
      "parNumber": 59,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.817Z",
      "updatedAt": "2024-09-07T15:26:49.817Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "f8f684f3-2b7c-435a-b3f8-f4ec6d52a83b",
      "number": 15,
      "content": "Ngày của âm u và đen tối.",
      "order": 4,
      "parNumber": 60,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.817Z",
      "updatedAt": "2024-09-07T15:26:49.817Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "6c1e9d00-35d2-498a-bdbf-27e56b3fed12",
      "number": 16,
      "content": "Ngày của tiếng kèn trận và tiếng hò hét",
      "order": 0,
      "parNumber": 61,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.823Z",
      "updatedAt": "2024-09-07T15:26:49.823Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "629ec081-b5aa-471e-a338-53df2e3193e2",
      "number": 16,
      "content": "Xung phong vào các thành kiên cố,",
      "order": 1,
      "parNumber": 62,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.823Z",
      "updatedAt": "2024-09-07T15:26:49.823Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "0d35fb8a-bb0e-4b4e-bb8f-3efbb47f7309",
      "number": 16,
      "content": "Và vào những tháp cao ở các góc thành.",
      "order": 2,
      "parNumber": 63,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.823Z",
      "updatedAt": "2024-09-07T15:26:49.823Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "b08df8ff-f3dc-4c53-8040-8343eae4bd85",
      "number": 17,
      "content": "“Ta sẽ đem cảnh khốn quẫn đến cho dân trong thành,",
      "order": 0,
      "parNumber": 64,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.828Z",
      "updatedAt": "2024-09-07T15:26:49.828Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "ad1bf7b6-47a0-4ce8-a131-d461c343c098",
      "number": 17,
      "content": "Chúng sẽ bước đi như những người mù,",
      "order": 1,
      "parNumber": 65,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.828Z",
      "updatedAt": "2024-09-07T15:26:49.828Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "9843910a-5ad1-4c86-9cf1-2f9416aa7a93",
      "number": 17,
      "content": "Vì chúng đã phạm tội chống lại CHÚA,",
      "order": 2,
      "parNumber": 66,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.828Z",
      "updatedAt": "2024-09-07T15:26:49.828Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "11cf43d9-e0da-455a-9969-eb4e3d2be70c",
      "number": 17,
      "content": "Máu của chúng sẽ đổ ra như bụi,",
      "order": 3,
      "parNumber": 67,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.828Z",
      "updatedAt": "2024-09-07T15:26:49.828Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "48eada15-4403-4afd-87a9-a359941df3c7",
      "number": 17,
      "content": "Thân thể chúng sẽ rữa nát như phân.”",
      "order": 4,
      "parNumber": 68,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.828Z",
      "updatedAt": "2024-09-07T15:26:49.828Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "ebaf3804-b473-47c0-9fc7-2204c3d0ec61",
      "number": 18,
      "content": "Bạc hoặc vàng của chúng",
      "order": 0,
      "parNumber": 69,
      "parIndex": 1,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.834Z",
      "updatedAt": "2024-09-07T15:26:49.834Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "ba3367ef-3259-4ccb-975d-0c5c7f28b98c",
      "number": 18,
      "content": "Đều không thể cứu chúng",
      "order": 1,
      "parNumber": 70,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.834Z",
      "updatedAt": "2024-09-07T15:26:49.834Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "59a408cd-aec9-4fa6-9d17-3c05ab6fc247",
      "number": 18,
      "content": "Trong ngày thịnh nộ của CHÚA.",
      "order": 2,
      "parNumber": 71,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.834Z",
      "updatedAt": "2024-09-07T15:26:49.834Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "5e9dfc59-bfe2-45a7-866d-708979b09bef",
      "number": 18,
      "content": "Trong lửa giận của Ngài,",
      "order": 3,
      "parNumber": 72,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.834Z",
      "updatedAt": "2024-09-07T15:26:49.834Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "a4ec11ad-2dc3-4e91-a6a8-54e9ff04f52c",
      "number": 18,
      "content": "Cả xứ sẽ bị thiêu rụi;",
      "order": 4,
      "parNumber": 73,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.834Z",
      "updatedAt": "2024-09-07T15:26:49.834Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "07a381c8-b99a-4d8f-b0a9-b2a655ee42b9",
      "number": 18,
      "content": "Thật vậy Ngài sẽ tuyệt diệt đồng loạt cách khủng khiếp",
      "order": 5,
      "parNumber": 74,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.834Z",
      "updatedAt": "2024-09-07T15:26:49.834Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    },
    {
      "id": "1afe51be-a578-403d-a94b-c6b8f26010a5",
      "number": 18,
      "content": "Mọi kẻ sống trong xứ.",
      "order": 6,
      "parNumber": 75,
      "parIndex": 0,
      "isPoetry": true,
      "createdAt": "2024-09-07T15:26:49.834Z",
      "updatedAt": "2024-09-07T15:26:49.834Z",
      "chapterId": "0da2a87a-73f8-4bdd-a253-251ce6de2505"
    }
  ],
